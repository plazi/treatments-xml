<collectingCountry id="F36A763EC477CD4BFCA7F987152FF993" box="[786,854,1593,1618]" name="Brazil" pageId="10" pageNumber="11">Brazil</collectingCountry>
(
<collectingRegion id="49B9F84CC477CD4BFCD3F98415D3F993" box="[870,938,1594,1618]" country="Brazil" name="Ceara" pageId="10" pageNumber="11">Ceará</collectingRegion>
