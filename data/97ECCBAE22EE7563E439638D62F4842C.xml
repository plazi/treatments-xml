<taxonomicName id="B6BF4936B84A3A421FCA7DA674D6AD7C" class="Insecta" family="Notodontidae" genus="Disphragis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Disphragis bifurcata" order="Lepidoptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="species" species="bifurcata">Disphragis bifurcata</taxonomicName>
a male (Costa Rica) b female (Costa Rica) 11 
<taxonomicName id="F1D877DEE85056CBF3F9DA9998BCE1A7" class="Insecta" family="Notodontidae" genus="Disphragis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Disphragis hemicera" order="Lepidoptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="species" species="hemicera">Disphragis hemicera</taxonomicName>
a male (Costa Rica) b female (Costa Rica) 12 
<taxonomicName id="44E0101E10288DC9D8E1D71DAC1EF714" class="Insecta" family="Notodontidae" genus="Disphragis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Disphragis sobolis" order="Lepidoptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="species" species="sobolis">Disphragis sobolis</taxonomicName>
a male (Ecuador) b female (Peru) 13 
<taxonomicName id="46710A8230E09085C26FEF948C47390D" class="Insecta" family="Notodontidae" genus="Disphragis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Disphragis notabilis" order="Lepidoptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="species" species="notabilis">Disphragis notabilis</taxonomicName>
