<taxonomicName id="3C4475B388B05EAF7D6AB8CFBCDADA0C" authorityName="Tian &amp; Li" authorityYear="2020" class="Insecta" family="Carabidae" genus="Wanoblemus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Wanoblemus huangshanicus" order="Coleoptera" pageId="0" pageNumber="33" phylum="Arthropoda" rank="species" species="huangshanicus">
