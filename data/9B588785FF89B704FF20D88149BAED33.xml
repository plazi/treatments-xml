<quantity id="D4099B76FF89B704FAC0DD3149BDEC0E" box="[1399,1436,1060,1082]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="8" pageNumber="299" unit="in" value="1.0">1 in</quantity>
