<quantity id="D4099B76FF89B704FEFADDE44DBBED33" box="[333,410,1265,1287]" metricMagnitude="-3" metricUnit="m" metricValue="8.9" pageId="8" pageNumber="299" unit="mm" value="8.9">8.9 mm</quantity>
.......................................................................... 
