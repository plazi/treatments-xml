<taxonomicName id="33F24D0CFF885A236CC080B5FC4D0AE7" authority="(Phil.) I.M.Johnst. ex Cabrera" authorityName="(Phil.) I.M.Johnst. ex Cabrera" box="[461,965,1239,1264]" class="Magnoliopsida" family="Anacardiaceae" genus="Schinus" kingdom="Plantae" order="Sapindales" pageId="28" pageNumber="563" phylum="Tracheophyta" rank="species" species="patagonica">
