<taxonomicName id="5398A7902EF546E1B7B837EB340759B9" authority="Spinola, 1806" authorityName="Spinola" authorityYear="1806" class="Insecta" family="Chrysididae" genus="Elampus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Elampus Gayi" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="Gayi">Elampus Gayi</taxonomicName>
