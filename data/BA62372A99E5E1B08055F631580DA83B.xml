<taxonomicName id="C8D4E5570D767775206DD57AB8BBF40D" authority="Spinola, 1806" authorityName="Spinola" authorityYear="1806" class="Insecta" family="Chrysididae" genus="Elampus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Elampus Gayi" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="Gayi">Elampus Gayi</taxonomicName>
