<taxonomicName id="D6B86EC27209FFB8FEF1798185AAF912" box="[346,691,1746,1768]" class="Insecta" family="Cicadellidae" genus="Empoasca" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="paragucia" status="sp. nov." subGenus="Empoasca">
