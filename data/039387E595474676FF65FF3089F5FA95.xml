</taxonomicName>
, 
<taxonomicName id="4C3A4D7095474674FD4BFE3F8962FE2E" box="[697,820,408,434]" class="Arachnida" family="Gonyleptidae" genus="Eusarcus" kingdom="Animalia" order="Opiliones" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="pusillus">
