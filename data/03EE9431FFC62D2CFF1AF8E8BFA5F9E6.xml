<taxonomicName id="4C475EA4FFC12D2CFEF6FF23BF98F9CB" box="[379,496,188,213]" class="Magnoliopsida" family="Amaranthaceae" genus="Haloxylon" kingdom="Plantae" order="Caryophyllales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="genus">
