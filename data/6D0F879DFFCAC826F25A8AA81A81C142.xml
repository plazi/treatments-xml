: 
<collectingRegion id="2762F869FFCAC826F5C888801C38C0D2" box="[1215,1332,695,719]" country="India" name="Karnataka" pageId="7" pageNumber="368">Karnataka</collectingRegion>
, 
<collectingRegion id="2762F869FFCAC826F43288801A77C0EF" country="India" name="Maharashtra" pageId="7" pageNumber="368">Maharashtra</collectingRegion>
, and 
<collectingRegion id="2762F869FFCAC826F2CC88ED1D33C0EF" box="[955,1087,730,754]" country="India" name="Tamil Nadu" pageId="7" pageNumber="368">Tamil Nadu</collectingRegion>
(
