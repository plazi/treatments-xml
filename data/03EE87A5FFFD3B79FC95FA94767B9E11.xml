<emphasis id="B933EAA1FFFE3B79FE65FA7E76AC9DDE" box="[422,508,1440,1464]" italics="true" pageId="30" pageNumber="63">P. setifer</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C474D30FFFE3B79FDC8FA7E752B9DDE" authorityName="ASSING" authorityYear="2017" box="[523,635,1440,1464]" class="Insecta" family="Staphylinidae" genus="Pheidologitonetes" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="63" phylum="Arthropoda" rank="species" species="bursatus">
<emphasis id="B933EAA1FFFE3B79FDC8FA7E752B9DDE" box="[523,635,1440,1464]" italics="true" pageId="30" pageNumber="63">P. bursatus</emphasis>
</taxonomicName>
, and 
<taxonomicName id="4C474D30FFFE3B79FD78FA7E74579DDE" baseAuthorityName="PACE" baseAuthorityYear="1998" box="[699,775,1440,1464]" class="Insecta" family="Staphylinidae" genus="Pheidologitonetes" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="63" phylum="Arthropoda" rank="species" species="adesi">
