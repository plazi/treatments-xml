<collectingCountry id="F3507623FFFE3B79FEA9FA9E76BB9D3E" box="[362,491,1344,1368]" name="Hong Kong" pageId="30" pageNumber="63">Hong Kong</collectingCountry>
), and 
<taxonomicName id="4C474D30FFFE3B79FDE8FA9E74669D3E" authority="ASSING, 2017" authorityName="ASSING" authorityYear="2017" box="[555,822,1344,1368]" class="Insecta" family="Staphylinidae" genus="Pheidologitonetes" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="63" phylum="Arthropoda" rank="species" species="bursatus">
<emphasis id="B933EAA1FFFE3B79FDE8FA9E75CC9D3E" box="[555,668,1344,1368]" italics="true" pageId="30" pageNumber="63">P. bursatus</emphasis>
