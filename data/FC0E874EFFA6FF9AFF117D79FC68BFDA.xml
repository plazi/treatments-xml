<taxonomicName id="B3A74DDBFFA6FF9AFD0878BDFCB3BFC5" authorityName="du Bus" authorityYear="1868" box="[720,848,426,450]" class="Mammalia" family="Hyperoodontidae" genus="Ziphirostrum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="5" pageNumber="110" phylum="Chordata" rank="genus">Ziphirostrum</taxonomicName>
