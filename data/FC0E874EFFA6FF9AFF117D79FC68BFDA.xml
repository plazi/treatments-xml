, except 
<emphasis id="46D3EA4AFFA6FF9AFC617887FB51BFAF" box="[953,1202,400,424]" italics="true" pageId="5" pageNumber="110">
<taxonomicName id="B3A74DDBFFA6FF9AFC617887FBCCBFAF" authorityName="Lambert" authorityYear="2005" box="[953,1071,400,424]" class="Mammalia" family="Hyperoodontidae" genus="Beneziphius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="5" pageNumber="110" phylum="Chordata" rank="genus">Beneziphius</taxonomicName>
, 
