<taxonomicName class="Insecta" family="Noctuidae" genus="Mamestra" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mamestra doira" order="Lepidoptera" pageId="6" pageNumber="109" phylum="Arthropoda" rank="species" species="doira">Mamestra doira</taxonomicName>
