long, 
<number box="[372,421,2008,2032]" pageId="10" pageNumber="67" value="0.5">0.50</number>
wide. Clypeus 
<number box="[595,644,2008,2032]" pageId="10" pageNumber="67" value="0.08">0.08</number>
high. Leg measurements: I 
<number box="[957,1020,2008,2032]" pageId="10" pageNumber="67" value="10.7">10.70</number>
(
<number box="[1035,1088,2008,2032]" pageId="10" pageNumber="67" value="2.75">2.75</number>
, 
<number box="[1099,1151,2008,2032]" pageId="10" pageNumber="67" value="0.25">0.25</number>
, 
<number box="[1162,1214,2008,2032]" pageId="10" pageNumber="67" value="3.65">3.65</number>
, 
<number box="[1225,1277,2008,2032]" pageId="10" pageNumber="67" value="2.65">2.65</number>
, 
<number box="[1288,1341,2008,2032]" pageId="10" pageNumber="67" value="1.4">1.40</number>
); II 
<number box="[1387,1436,2008,2032]" pageId="10" pageNumber="67" value="6.85">6.85</number>
(2.00, 
<number box="[224,275,151,175]" pageId="11" pageNumber="68" value="0.25">0.25</number>
, 2.00, 
<number box="[352,404,151,175]" pageId="11" pageNumber="68" value="1.7">1.70</number>
, 
<number box="[416,470,151,175]" pageId="11" pageNumber="68" value="0.9">0.90</number>
); III 
<number box="[526,575,151,175]" pageId="11" pageNumber="68" value="5.12">5.12</number>
(
<number box="[592,644,151,175]" pageId="11" pageNumber="68" value="1.75">1.75</number>
, 
<number box="[657,709,151,175]" pageId="11" pageNumber="68" value="0.22">0.22</number>
, 
<number box="[721,773,151,175]" pageId="11" pageNumber="68" value="1.5">1.50</number>
, 
<number box="[785,836,151,175]" pageId="11" pageNumber="68" value="1.05">1.05</number>
, 
<number box="[849,888,151,175]" pageId="11" pageNumber="68" value="0.6">0.6</number>
); IV 
<number box="[947,996,151,175]" pageId="11" pageNumber="68" value="7.92">7.92</number>
(
<number box="[1012,1065,151,175]" pageId="11" pageNumber="68" value="2.37">2.37</number>
, 
<number box="[1077,1129,151,175]" pageId="11" pageNumber="68" value="0.25">0.25</number>
