<taxonomicName authorityName="Ponomarenko" authorityYear="1966" box="[490,697,1426,1450]" class="Insecta" family="Ommatidae" genus="Notocupes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="504" phylum="Arthropoda" rank="species" species="excellens">
<emphasis box="[490,697,1426,1450]" italics="true" pageId="5" pageNumber="504">Notocupes excellens</emphasis>
