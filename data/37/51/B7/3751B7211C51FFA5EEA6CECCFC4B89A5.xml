<paragraph blockId="2.[151,1437,512,972]" box="[199,1008,944,972]" pageId="2" pageNumber="491">
<emphasis bold="true" box="[199,353,944,971]" pageId="2" pageNumber="491">Distribution.</emphasis>
<collectingCountry box="[359,429,945,971]" name="China" pageId="2" pageNumber="491">China</collectingCountry>
, 
<collectingCountry box="[440,504,945,971]" name="Japan" pageId="2" pageNumber="491">Japan</collectingCountry>
(
<typeStatus box="[519,569,945,971]" pageId="2" pageNumber="491">type</typeStatus>
