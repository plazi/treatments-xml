<collectingRegion box="[181,275,1783,1808]" country="Ukraine" name="Ternopil'" pageId="147" pageNumber="148">Ternopil</collectingRegion>
(
<collectingCountry box="[289,383,1783,1808]" name="Ukraine" pageId="147" pageNumber="148">Ukraine</collectingCountry>
