<paragraph blockId="73.[151,621,836,862]" box="[151,621,836,862]" pageId="73" pageNumber="74">
<heading bold="true" box="[151,621,836,862]" fontSize="11" level="1" pageId="73" pageNumber="74" reason="1">
<emphasis bold="true" box="[151,621,836,862]" pageId="73" pageNumber="74">
Genus 
<taxonomicName authority="Monterosato, 1923" authorityName="Monterosato" authorityYear="1923" box="[237,621,836,862]" class="Gastropoda" family="Conidae" genus="Lautoconus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="73" pageNumber="74" phylum="Mollusca" rank="genus">
<emphasis bold="true" box="[237,380,836,862]" italics="true" pageId="73" pageNumber="74">Lautoconus</emphasis>
<bibRefCitation author="Monterosato" box="[388,621,836,862]" pageId="73" pageNumber="74" refString="Monterosato, M. di (1923) Molluschi delle Coste Cirenaiche Raccolti dall'Ing. Camillo Crema. Memoria Reale Comitato Talassografico Italiano, 107, 1 - 14." type="journal article" year="1923">Monterosato, 1923</bibRefCitation>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
