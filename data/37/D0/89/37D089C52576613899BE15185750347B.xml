<paragraph pageId="23" pageNumber="102">
<normalizedToken originalValue="Chûjô">Chujo</normalizedToken>
described 
<taxonomicName class="Insecta" family="Erotylidae" genus="Microsternus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microsternus taiwanus" order="Coleoptera" pageId="23" pageNumber="102" phylum="Arthropoda" rank="species" species="taiwanus">Microsternus taiwanus</taxonomicName>
