<th box="[631,718,1544,1571]" gridcol="3" gridrow="0" pageId="106" pageNumber="107">Exopod</th>
<th box="[823,924,1544,1571]" gridcol="4" gridrow="0" pageId="106" pageNumber="107">Endopod</th>
</tr>
<tr box="[199,924,1580,1607]" gridrow="1" pageId="106" pageNumber="107">
<th box="[199,324,1580,1607]" gridcol="0" gridrow="1" pageId="106" pageNumber="107">Leg 1</th>
<td box="[439,498,1580,1607]" gridcol="1" gridrow="1" pageId="106" pageNumber="107">0-0</td>
<td box="[535,596,1580,1607]" gridcol="2" gridrow="1" pageId="106" pageNumber="107">0-0</td>
<td box="[631,718,1580,1607]" gridcol="3" gridrow="1" pageId="106" pageNumber="107">VI, 1</td>
<td box="[823,924,1580,1607]" gridcol="4" gridrow="1" pageId="106" pageNumber="107">1, 2, 5</td>
</tr>
<tr box="[199,924,1616,1643]" gridrow="2" pageId="106" pageNumber="107">
