<subSubSection pageId="23" pageNumber="24" type="etymology">
<paragraph blockId="23.[151,1437,1190,2009]" pageId="23" pageNumber="24">
<emphasis bold="true" box="[199,334,1767,1793]" pageId="23" pageNumber="24">Etymology.</emphasis>
