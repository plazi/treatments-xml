<td box="[439,498,331,357]" gridcol="1" gridrow="1" pageId="111" pageNumber="112">0-0</td>
<td box="[535,596,331,357]" gridcol="2" gridrow="1" pageId="111" pageNumber="112">1-0</td>
<td box="[631,718,331,357]" gridcol="3" gridrow="1" pageId="111" pageNumber="112">VI</td>
<td box="[823,1028,331,357]" gridcol="4" gridrow="1" pageId="111" pageNumber="112">II</td>
</tr>
