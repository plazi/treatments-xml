<paragraph blockId="230.[151,1320,1355,1379]" box="[151,1320,1355,1379]" pageId="230" pageNumber="231">
<emphasis bold="true" box="[151,253,1355,1379]" pageId="230" pageNumber="231">FIG. 151.</emphasis>
<emphasis box="[260,461,1355,1379]" italics="true" pageId="230" pageNumber="231">bnterçcçla sulcatus</emphasis>
<emphasis bold="true" box="[467,547,1355,1379]" pageId="230" pageNumber="231">
