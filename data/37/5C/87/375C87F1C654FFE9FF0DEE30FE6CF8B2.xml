<collectingCountry box="[361,440,1699,1724]" name="Russia" pageId="7" pageNumber="324">Russia</collectingCountry>
: 
<collectingRegion box="[451,511,1700,1723]" country="Russia" name="Tyva" pageId="7" pageNumber="324">Tuva</collectingRegion>
