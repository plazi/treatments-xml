. 
<taxonomicName authority="Li &amp; Rahayu &amp; Ng, 2018" authorityName="Li &amp; Rahayu &amp; Ng" authorityYear="2018" box="[295,538,1654,1676]" class="Malacostraca" family="Sesarmidae" genus="Parasesarma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="19" pageNumber="470" phylum="Arthropoda" rank="species" species="gracilipes" status="n. sp.">
<emphasis box="[295,538,1654,1676]" italics="true" pageId="19" pageNumber="470">Parasesarma gracilipes</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[547,605,1654,1675]" pageId="19" pageNumber="470" rank="species">
