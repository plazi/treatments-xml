<subSubSection pageId="1" pageNumber="183" type="nomenclature">
<paragraph blockId="1.[151,1436,1906,2004]" box="[151,240,1906,1932]" pageId="1" pageNumber="183">
<heading bold="true" box="[151,240,1906,1932]" fontSize="11" level="1" pageId="1" pageNumber="183" reason="1">
<taxonomicName authorityName="Saussure" authorityYear="1862" box="[151,240,1906,1932]" class="Insecta" family="Blaberidae" genus="Thorax" kingdom="Animalia" order="Blattodea" pageId="1" pageNumber="183" phylum="Arthropoda" rank="genus">
