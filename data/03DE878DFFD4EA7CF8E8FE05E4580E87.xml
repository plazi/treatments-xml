<taxonomicName id="4C774D18FFD4EA7CF8E8FE05E3A00DE2" authority="Schmarda, 1859" authorityName="Schmarda" authorityYear="1859" box="[151,666,404,430]" class="Rhabditophora" family="Pseudocerotidae" genus="Pseudobiceros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="3" pageNumber="104" phylum="Platyhelminthes" rank="species" species="miniatus">
