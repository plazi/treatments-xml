<taxonomicName id="AD5BF03885B2A085C1AA56C9CA73981B" class="Magnoliopsida" family="Malvaceae" genus="Theobroma" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Theobroma grandiflorum" order="Malvales" pageId="2" pageNumber="101" phylum="Tracheophyta" rank="species" species="grandiflorum">Theobroma grandiflorum</taxonomicName>
