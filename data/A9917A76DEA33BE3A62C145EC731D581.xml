<taxonomicName id="858DD5AC3CE054375C2A2C5F9F01215E" class="Magnoliopsida" family="Caricaceae" genus="Carica" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carica papaya" order="Brassicales" pageId="2" pageNumber="101" phylum="Tracheophyta" rank="species" species="papaya">Carica papaya</taxonomicName>
