<location id="EC1E6074FF81FFA6FE57FDB1FD51FD60" LSID="urn:lsid:plazi:treatment:616887B9FF86FFA6FF3CF808FF77FCDD:EC1E6074FF81FFA6FE57FDB1FD51FD60" box="[508,735,620,645]" country="Grenada" county="Aldeadavila de la Ribera" municipality="Salamanca" name="Salamanca province" pageId="28" pageNumber="69" stateProvince="Madrid">Salamanca province</location>
