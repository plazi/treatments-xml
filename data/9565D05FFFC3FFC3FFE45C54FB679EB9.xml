<taxonomicName id="DACC1ACAFFC3FFC3FCFF5F74FC359E19" authorityName="Jochum, Slapnik &amp; Pall-Gergely" authorityYear="2014" box="[869,1003,1373,1397]" class="Gastropoda" family="Gastrocoptidae" genus="Angustopila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="14" pageNumber="364" phylum="Mollusca" rank="genus">
<emphasis id="2FB8BD5BFFC3FFC3FCFF5F74FC359E19" box="[869,1003,1373,1397]" italics="true" pageId="14" pageNumber="364">Angustopila</emphasis>
</taxonomicName>
species (unpublished information) and 
<taxonomicName id="DACC1ACAFFC3FFC3FCB75F54FA889EF9" authority="Pall-Gergely &amp; Hunyadi" authorityName="Pall-Gergely &amp; Hunyadi" box="[813,1366,1405,1429]" class="Gastropoda" family="Gastrocoptidae" genus="Tonkinospira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="14" pageNumber="364" phylum="Mollusca" rank="species" species="danangensis">
<emphasis id="2FB8BD5BFFC3FFC3FCB75F54FB999EF9" box="[813,1095,1405,1429]" italics="true" pageId="14" pageNumber="364">Tonkinospira danangensis</emphasis>
Páll-Gergely &amp; Hunyadi
</taxonomicName>
in Páll- Gergely, Grego, 
