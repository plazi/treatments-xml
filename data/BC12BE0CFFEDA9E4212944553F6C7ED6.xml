<taxonomicName id="14B12DAC2E2FF317EEFA2A03E886D81A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:33873" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole minuscula Bernard" order="Hymenoptera" pageNumber="226" phylum="Arthropoda" rank="species" species="minuscula">Pheidole minuscula</taxonomicName>
