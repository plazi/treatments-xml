<emphasis id="B9435F7CFFBBFFF4DC7923860BA697C1" box="[308,766,1725,1748]" italics="true" pageId="24" pageNumber="524">
<taxonomicName id="4C37F8EDFFBBFFF4DC7923860B7F97C6" box="[308,551,1725,1748]" class="Insecta" family="Baetidae" genus="Callibaetis" kingdom="Animalia" order="Ephemeroptera" pageId="24" pageNumber="524" phylum="Arthropoda" rank="species" species="stictogaster">Callibaetis stictogaster</taxonomicName>
