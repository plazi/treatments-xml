<figureCitation id="D79A2A3EC20EFF9832E18735FF7F6B3B" box="[159,270,1840,1866]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-2="FIGURE 3" captionStartId-0="2.[151,255,1036,1060]" captionStartId-1="3.[151,255,578,602]" captionStartId-2="3.[151,255,1975,1999]" captionTargetBox-0="[220,1370,371,1010]" captionTargetBox-2="[360,1206,663,1951]" captionTargetId-0="figure@2.[151,1436,317,1010]" captionTargetId-2="figure@3.[340,1259,636,1951]" captionTargetPageId-0="2" captionTargetPageId-2="3" captionText-0="FIGURE 1. Fibuloides khaonanensis, adult male." captionText-1="FIGURE 2. Fibuloides khaonanensis, male genitalia. Scale bar: 0.5 mm." captionText-2="FIGURE 3. Fibuloides khaonanensis, dorsal view of male abdominal segment with scale patches. Scale bar: 0.5 mm." httpUri-0="https://zenodo.org/record/274085/files/figure.png" httpUri-2="https://zenodo.org/record/274086/files/figure.png" pageId="1" pageNumber="62">
