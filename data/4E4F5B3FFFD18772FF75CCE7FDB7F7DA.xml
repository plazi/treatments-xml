<taxonomicName id="01E691AAFFD18772FC3CCCE7FB91F18A" authorityName="Jeannel" authorityYear="1962" box="[990,1094,979,1005]" class="Insecta" family="Leiodidae" genus="Eupelates" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="brunneus">brunneus</taxonomicName>
<taxonomicName id="01E691AAFFD18772FBAFCCE7FB62F18A" authorityName="Jeannel" authorityYear="1962" box="[1101,1205,979,1005]" class="Insecta" family="Leiodidae" genus="Eupelates" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="brunneus">brunneus</taxonomicName>
