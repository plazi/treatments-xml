<emphasis id="F492363BFFD18772FE4BCCC2FDBBF677" box="[425,620,1014,1040]" italics="true" pageId="42" pageNumber="43">
<taxonomicName id="01E691AAFFD18772FE4BCCC2FDD1F677" authorityName="Jeannel" authorityYear="1936" box="[425,518,1014,1040]" class="Insecta" family="Leiodidae" genus="Adelopsis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="brunnea">brunnea</taxonomicName>
