<taxonomicName id="4C714D228186FF2AFD8EFB3FFD0BFB7A" box="[514,696,1166,1192]" class="Liliopsida" family="Orchidaceae" genus="Pogonia" kingdom="Plantae" order="Asparagales" pageId="169" pageNumber="170" phylum="Tracheophyta" rank="species" species="juliana">
