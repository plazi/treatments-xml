<taxonomicName id="4C1192EF3D06FB0C5CFDF9F4FBCBF994" baseAuthorityName="Rothschild" baseAuthorityYear="1912" box="[941,1067,1618,1645]" class="Insecta" family="Erebidae" genus="Cyana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="flavalba">
<emphasis id="B965357E3D06FB0C5CFDF9F4FBCBF994" box="[941,1067,1618,1645]" italics="true" pageId="26" pageNumber="27">C. flavalba</emphasis>
