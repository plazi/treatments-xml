<location id="956F8FDAFFFBFFC9FE32F900FD82F937" LSID="urn:lsid:plazi:treatment:18196817FFFBFFC9FF64FA0DFD72F8A3:956F8FDAFFFBFFC9FE32F900FD82F937" box="[449,525,1766,1790]" country="Philippines" county="Luzon" latitude="14.35" longLatPrecision="773" longitude="121.32" municipality="Jalajala" name="Luzon" pageId="5" pageNumber="6" stateProvince="Manila">Luzon</location>
). 
<location id="956F8FDAFFFBFFC9FDD1F900FDB6F934" LSID="urn:lsid:plazi:treatment:18196817FFFBFFC9FF64FA0DFD72F8A3:956F8FDAFFFBFFC9FDD1F900FDB6F934" box="[546,569,1766,1789]" country="Philippines" county="Luzon" latitude="14.35" longLatPrecision="773" longitude="121.32" municipality="Jalajala" name="In" pageId="5" pageNumber="6" stateProvince="Manila">In</location>
his travel report, 
<bibRefCitation id="F421A4F0FFFBFFC9FCF9F900FC68F937" author="Meyen" box="[778,999,1765,1790]" pageId="5" pageNumber="6" refString="Meyen, F. J. F. (1835) Reise um die Erde Ausgefuhrt auf dem Koniglich Preussischen Seehandlungs-Schiffe Prinzess Louise, Commandirt von Capitain W. Wendt, in den Jahren 1830, 1831 und 1832. Vol. 2. Historischer Bericht. Sander'sche Buchhandlung, Berlin, vii + 413 pp." type="book" year="1835">
<location id="956F8FDAFFFBFFC9FCF9F900FCD7F937" LSID="urn:lsid:plazi:treatment:18196817FFFBFFC9FF64FA0DFD72F8A3:956F8FDAFFFBFFC9FCF9F900FCD7F937" box="[778,856,1766,1790]" country="Philippines" county="Luzon" latitude="14.35" longLatPrecision="773" longitude="121.32" municipality="Jalajala" name="Meyen" pageId="5" pageNumber="6" stateProvince="Manila">Meyen</location>
(1835: 254)
</bibRefCitation>
