<bibRefCitation id="A0A9171EB4ADA95493DDD04433BA35E5" author="Buysson, R du" journalOrPublisher="Bullettin de la Societe Entomologique de France" pageId="24" pageNumber="133" pagination="29 - 30" title="Sur la Chrysisshangaiensis." volume="1901" year="1901">1901</bibRefCitation>
; 
<bibRefCitation id="4824964357120F8C340645775AD69E80" author="Piel, O" journalOrPublisher="Lingnan Science Journal" pageId="25" pageNumber="134" pagination="173 - 201" title="Monemaflavescens Wkr. and its parasites (Lepidoptera, Heterogeneidae)." volume="12" year="1933">Piel 1933</bibRefCitation>
; 
<bibRefCitation id="9C23C22E92B1BCD5DD36C6AAB6EC665E" author="Parker, DE" journalOrPublisher="Journal of Agricoltural Research" pageId="25" pageNumber="134" pagination="449 - 458" title="Chrysisshanghaiensis Smith, a parasite of the oriental moth Monemaflavescens." volume="52" year="1936">Parker 1936</bibRefCitation>
