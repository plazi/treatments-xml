16. 
<taxonomicName id="21C2332D20A255CEECD392A0097406E9" LSID="http://zoobank.org/426B2B2A-6F48-4ABA-8601-EC74A69FB77E" authority="Liebherr, 2016" authorityName="Liebherr" authorityYear="2016" class="Insecta" family="Carabidae" genus="Cyphocoleus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyphocoleus cordatus" order="Coleoptera" pageId="36" pageNumber="247" phylum="Arthropoda" rank="species" species="cordatus" status="sp. n.">Cyphocoleus cordatus</taxonomicName>
