</taxonomicName>
, 
<taxonomicName id="4C328BDA195CFFDBFF1CFB644B86FB19" authorityName="Cameron" authorityYear="1945" box="[251,418,1181,1203]" class="Insecta" family="Staphylinidae" genus="Hadrotes" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="755" phylum="Arthropoda" rank="species" species="wakefieldi">
