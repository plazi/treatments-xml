<taxonomicName id="4C328BDA195CFFDBFE9BFB844BD3FB38" authorityName="Leach" authorityYear="1819" box="[380,503,1150,1171]" class="Insecta" family="Staphylinidae" genus="Creophilus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="755" phylum="Arthropoda" rank="genus">
