<paragraph id="354B36B45601FFB4D157FDE5FE036764" blockId="29.[108,436,519,539]" box="[108,436,519,539]" pageId="29" pageNumber="29">
<tableNote id="C812373A5601FFB4D157FDE5FE036764" box="[108,436,519,539]" pageId="29" pageNumber="29" targetBox="[258,1011,329,495]" targetPageId="29">
<superScript id="C2819BFC5601FFB4D157FDE5FFC5676C" attach="left" box="[108,114,519,531]" fontSize="5" pageId="29" pageNumber="29">a</superScript>
