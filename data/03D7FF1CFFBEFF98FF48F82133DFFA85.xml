<emphasis id="B90A9218FFBEFF98FA83F8AB34DCFC2D" bold="true" box="[1291,1440,965,989]" pageId="117" pageNumber="116">
<collectingCountry id="F3690E9AFFBEFF98FA83F8AB34E7FC2D" box="[1291,1435,965,989]" name="Venezuela" pageId="117" pageNumber="116">Venezuela</collectingCountry>
