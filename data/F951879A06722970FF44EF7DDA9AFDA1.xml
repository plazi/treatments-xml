<taxonomicName id="B6F84D0F06722971FF44EF7DDA26FD30" box="[151,311,620,646]" class="Insecta" family="Lauxaniidae" genus="Sciasmomyia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="417" phylum="Arthropoda" rank="genus">Sciasmomyia</taxonomicName>
