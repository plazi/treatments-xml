<taxonomicName id="C5D4A8B5A819D70BB7B0FAC8396476D7" authority="Seguy, 1961" authorityName="Seguy" authorityYear="1961" box="[200,599,1398,1425]" class="Insecta" family="Syrphidae" genus="Eumerus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="elaverensis">
