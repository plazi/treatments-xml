<geoCoordinate id="1088F8D7FF9FCA3C1DEEFE2DFCB3C3DE" box="[763,864,477,497]" degrees="20" direction="east" minutes="06" orientation="longitude" pageId="12" pageNumber="547" precision="15" seconds="16" value="20.104445">20°06′16″E</geoCoordinate>
