</taxonomicName>
, 
<taxonomicName id="4C67FFB6FFDEFFD9FC7FF9BFFB3EDF1C" box="[973,1235,1622,1647]" class="Rhabditophora" family="Pleioplanidae" genus="Melloplana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="3" pageNumber="529" phylum="Platyhelminthes" rank="species" species="ferruginea">
<emphasis id="B9135827FFDEFFD9FC7FF9BFFB3EDF1C" box="[973,1235,1622,1647]" italics="true" pageId="3" pageNumber="529">Melloplana ferruginea</emphasis>
</taxonomicName>
, and 
