<collectingCounty id="62B9FCB9FFDEFFD9FF4AF9BFFEC6DF1C" box="[248,299,1622,1647]" pageId="3" pageNumber="529">Such</collectingCounty>
key taxa 
<collectingCountry id="F370C4A5FFDEFFD9FE02F9BEFE2ADF1C" box="[432,455,1623,1647]" name="American Samoa" pageId="3" pageNumber="529">aS</collectingCountry>
<taxonomicName id="4C67FFB6FFDEFFD9FE64F9BFFD31DF1D" box="[470,732,1622,1646]" class="Rhabditophora" family="Notoplanidae" genus="Notocomplana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="3" pageNumber="529" phylum="Platyhelminthes" rank="species" species="humilis">
<emphasis id="B9135827FFDEFFD9FE64F9BFFD31DF1D" box="[470,732,1622,1646]" italics="true" pageId="3" pageNumber="529">Notocomplana humilis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C67FFB6FFDEFFD9FD40F9BFFC5ADF1D" box="[754,951,1622,1646]" class="Rhabditophora" family="Leptoplanidae" genus="Notoplana" kingdom="Animalia" order="Polycladida" pageId="3" pageNumber="529" phylum="Platyhelminthes" rank="species" species="dubia">
