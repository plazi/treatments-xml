<taxonomicName id="4C727CE3D315DF58FA525211FDE0F8F4" box="[294,522,1878,1904]" class="Polypodiopsida" family="Dryopteridaceae" genus="Arachniodes" kingdom="Plantae" order="Polypodiales" pageId="3" pageNumber="259" phylum="Tracheophyta" rank="species" species="libingii">
