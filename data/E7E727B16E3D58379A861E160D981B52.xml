<subSubSection id="376398C0964A070C32523B16D39518DE" pageId="0" pageNumber="73" type="materials_examined">
<paragraph id="1EA153EB394F821E766F88FA4FE178F2" pageId="0" pageNumber="73">Material examined.</paragraph>
<paragraph id="7AD483F2A1B567C794E1E3A5F226E0D3" pageId="0" pageNumber="73">
