<quantity id="FCE3E541776F23985A3E2D1048E80334" metricMagnitude="-2" metricUnit="m" metricValue="2.0" metricValueMax="2.5" metricValueMin="1.5" unit="mm" value="20.0" valueMax="25.0" valueMin="15.0">15-25 mm</quantity>
, LB; one specimen 
<quantity id="BBF4B6961A3C0261E02EEB56BD756C9B" metricMagnitude="-2" metricUnit="m" metricValue="3.0" unit="mm" value="30.0">30 mm</quantity>
