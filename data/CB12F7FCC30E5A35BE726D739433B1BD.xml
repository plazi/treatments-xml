<taxonomicName id="D9E0293C447013E991CB85B00C56FF22" authorityName="Schrenk, Enum. Pl. Nov. [F. E. L. Fischer &amp; C. A. Meyer] 1: 19. 1841. (Fischer and Meyer" authorityYear="1841" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus subsericeus" order="Solanales" pageId="144" pageNumber="145" phylum="Tracheophyta" rank="species" species="subsericeus">
