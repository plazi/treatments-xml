<td id="9F7884CA1A21AA3ED5D90D41E11936F9" align="left" pageId="null" pageNumber="43" valign="top">
2. 
<normalizedToken id="832EC22C8D85AEDF6FEFED95957533FE" originalValue="Blätter" pageId="null" pageNumber="43">Blaetter</normalizedToken>
<normalizedToken id="2EB422D09C61F63F3DC34FD3C1F64FB3" originalValue="über" pageId="null" pageNumber="43">ueber</normalizedToken>
