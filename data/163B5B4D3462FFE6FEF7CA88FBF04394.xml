<collectingCountry id="E685AACB3461FFE6FCA1CD0EFC6D423C" box="[862,940,1095,1121]" name="Peru" pageId="6" pageNumber="55">PERU</collectingCountry>
, Avispas, Madre de Dios, 
<date id="EA2CCC9B3461FFE6FB00CD0EFE4942D4" pageId="6" pageNumber="55" value="1962-09-10" valueMax="1962-09-20" valueMin="1962-09-10">
<number id="285AF87C3461FFE6FB00CD0EFEE942D4" pageId="6" pageNumber="55" value="15.0" valueMax="20.0" valueMin="10.0">10– 20</number>
.ix. 
<number id="285AF87C3461FFE6FEB6CD26FE4942D4" box="[329,392,1135,1161]" pageId="6" pageNumber="55" value="1962.0">1962</number>
</date>
, L. Pena, 
<quantity id="596A47BE3461FFE6FE03CD26FD8742D4" box="[508,582,1135,1161]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="6" pageNumber="55" unit="m" value="400.0">400 m</quantity>
