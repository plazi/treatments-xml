<collectionCode id="F883729E3461FFE6FD7ECD26FD0342D4" box="[641,706,1135,1161]" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="6" pageNumber="55">CNC</collectionCode>
