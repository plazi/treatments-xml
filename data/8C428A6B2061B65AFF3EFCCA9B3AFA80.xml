<taxonomicName id="C3EB40FE2060B65CFD2FF8DB9D57F869" box="[646,870,1910,1933]" class="Bivalvia" family="Thyasiridae" genus="Adontorhina" kingdom="Animalia" order="Lucinoida" pageId="3" pageNumber="40" phylum="Mollusca" rank="species" species="keegani">
<emphasis id="369FE76F2060B65CFD2FF8DB9D57F869" box="[646,870,1910,1933]" italics="true" pageId="3" pageNumber="40">Adontorhina keegani</emphasis>
</taxonomicName>
