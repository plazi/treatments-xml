<taxonomicName id="B5E54D633C5C0B0FFEE0AD68FC99F8FD" authority="Holthuis, 1950" authorityName="Holthuis" authorityYear="1950" box="[293,769,1843,1866]" class="Malacostraca" family="Palaemonidae" genus="Macrobrachium" kingdom="Animalia" order="Decapoda" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="crenulatum">
