<number id="3DBCED53FF86725A16A1FA96E500FA9B" box="[681,797,1298,1323]" pageId="84" pageNumber="85" value="1.65" valueMax="1.84" valueMin="1.46">1.46−1.84</number>
, 
<number id="3DBCED53FF86725A1722FA96E526FA9B" box="[810,827,1298,1323]" pageId="84" pageNumber="85" value="5.0">5</number>
♀ 
<typeStatus id="54CF41D6FF86725A175DFA97E5D8FA9B" box="[853,965,1299,1323]" pageId="84" pageNumber="85">paratypes</typeStatus>
: 
<number id="3DBCED53FF86725A17D9FA97E259FA9B" box="[977,1092,1299,1323]" pageId="84" pageNumber="85" value="2.06" valueMax="2.28" valueMin="1.84">1.84−2.28</number>
) mm, ThL = 
