4. 
<taxonomicName id="2E4758DF29BDD525B08EDB66BDACB80A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180124" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster ranavalonae Forel" order="Hymenoptera" pageNumber="184" phylum="Arthropoda" rank="species" species="ranavalonae">CREMASTOGASTER RANAVALONAE</taxonomicName>
