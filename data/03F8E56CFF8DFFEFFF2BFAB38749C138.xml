<paragraph id="8BEE547AFF8DFFEFFF2BF9EC8593C010" blockId="7.[151,1436,1565,1911]" pageId="7" pageNumber="8">
<emphasis id="B9258868FF8DFFEFFF2BF9EC85B4C078" bold="true" box="[151,219,1565,1591]" pageId="7" pageNumber="8">Note:</emphasis>
<taxonomicName id="4C512FF9FF8DFFEFFF5FF9EC84AAC079" box="[227,453,1565,1590]" class="Arachnida" family="Sparassidae" genus="Sinopoda" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="longshan">
