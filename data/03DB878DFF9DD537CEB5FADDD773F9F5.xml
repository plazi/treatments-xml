<collectingCountry id="F365760BFF9AD537CFF5FB2FD01AFB61" box="[471,560,1197,1220]" name="Panama" pageId="12" pageNumber="463">Panama</collectingCountry>
(
<collectingRegion id="49B6F879FF9AD537CC1CFB2ED0B5FB60" box="[574,671,1196,1221]" country="Panama" name="Chiriqui" pageId="12" pageNumber="463">Chiriqui</collectingRegion>
) (
<figureCitation id="13492A1EFF9AD537CC94FB2ED132FB61" box="[694,792,1196,1221]" captionStart="FIGURE 114" captionStartId="31.[151,250,1701,1723]" captionTargetBox="[190,1375,215,1655]" captionTargetId="figure@31.[150,1437,153,1679]" captionTargetPageId="31" captionText="FIGURE 114. Geographical records of Hypotelus. Hypotelus andinus, Colombia, Ecuador and Bolivia; H. insulanus, Caribbean islands; H. marginatus, Colombia, Venezuela and Guatemala; H. testaceus, Panama; H. brevitarsus sp. nov., Panama. Countries without dots but colored represent the occurrence but no locality given." httpUri="https://zenodo.org/record/803674/files/figure.png" pageId="12" pageNumber="463">Fig. 114</figureCitation>
