<taxonomicName id="BA9E4D19C47A376EFED4BDAEFF51FBA4" authority="(Duncan, 1880)" baseAuthorityName="Duncan" baseAuthorityYear="1880" class="Anthozoa" family="Felixaraeidae" genus="Paracycloseris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="11" pageNumber="758" phylum="Cnidaria" rank="species" species="nariensis">
