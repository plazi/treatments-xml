<taxonomicName id="BA9E4D19C478376CFCAABCB2FB5EFC8A" authority="Wells, 1934" authorityName="Wells" authorityYear="1934" box="[850,1210,748,772]" class="Anthozoa" family="Felixaraeidae" genus="Paracycloseris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="9" pageNumber="756" phylum="Cnidaria" rank="species" species="elizabethae">
