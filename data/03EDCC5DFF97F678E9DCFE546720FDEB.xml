<taxonomicName id="4C4406C8FF97F67BE825F8146703F78B" authority="(Fabricius)" baseAuthorityName="Fabricius" box="[352,575,2042,2067]" class="Insecta" family="Dryophthoridae" genus="Dynamis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="borassi">
