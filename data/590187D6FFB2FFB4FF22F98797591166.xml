<taxonomicName id="16A84D43FFB2FFB4FEF1F9349461112E" baseAuthorityName="Wollaston" baseAuthorityYear="1867" box="[324,555,1774,1800]" class="Insecta" family="Coccinellidae" genus="Scymnus" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="nigropictus">
