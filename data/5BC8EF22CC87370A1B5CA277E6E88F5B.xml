<taxonomicName id="42DC7A569CB2F0D408DDB944138F1DD4" class="Insecta" family="Cerambycidae" genus="Clytomelegena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Clytomelegena postaurata" order="Coleoptera" pageId="2" pageNumber="15" phylum="Arthropoda" rank="species" species="postaurata">Clytomelegena postaurata</taxonomicName>
