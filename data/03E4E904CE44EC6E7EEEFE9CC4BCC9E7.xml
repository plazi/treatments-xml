<collectingCountry id="F35A1882CE44EC6E7F46FEB1C6A3C97C" box="[319,418,298,324]" name="Somalia" pageId="43" pageNumber="44">Somalia</collectingCountry>
, northern region, 
<location id="8E920EC9CE44EC6E7C0BFEB1C4A5C97C" LSID="urn:lsid:plazi:treatment:03E4E904CE44EC6E7EEEFE9CC4BCC9E7:8E920EC9CE44EC6E7C0BFEB1C4A5C97C" box="[626,932,298,324]" country="Somalia" name="Mait-Erigavo Escarpment" pageId="43" pageNumber="44">Mait-Erigavo Escarpment</location>
