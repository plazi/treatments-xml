and 
<taxonomicName id="0A154669A9587AD4FFA7DF1CB9B7BF25" authority="Martinez, 2020" authorityName="Martinez" authorityYear="2020" class="Insecta" family="Noctuidae" genus="Gaujoptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gaujoptera" order="Lepidoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="genus">
