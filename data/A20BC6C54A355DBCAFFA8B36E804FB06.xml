</taxonomicName>
species 
<emphasis id="1D086408297CC8E220D0F86A0B8D7D1B" bold="true" pageId="0" pageNumber="71">64</emphasis>
<taxonomicName id="744CBA276254C247369013B4ECB73C2F" authorityName="Martinez" authorityYear="2020" class="Insecta" family="Pantheidae" genus="Gaujonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gaujonia bichu" order="Lepidoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="species" species="bichu">
<emphasis id="765F52FBE9757326A07CF5CA1E0FA708" italics="true" pageId="0" pageNumber="71">Gaujonia bichu</emphasis>
</taxonomicName>
, holotype, FSU, Zamora-Chinchipe, Ecuador 
