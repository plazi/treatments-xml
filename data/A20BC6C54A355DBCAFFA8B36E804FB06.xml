, 
<geoCoordinate id="69997B13B6D7C3EE88231DCB5E82231D" degrees="076" direction="west" minutes="50" orientation="longitude" precision="15" seconds="30" value="-76.84167">076°50'30&quot;W</geoCoordinate>
