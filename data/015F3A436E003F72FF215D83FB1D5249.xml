</taxonomicName>
[= 
<taxonomicName id="4EF6F0D66E003F73FD5B5C31FB575400" authority="Scopoli, 1763" authorityName="Scopoli" authorityYear="1763" box="[749,1268,1853,1877]" class="Insecta" family="Tenthredinidae" genus="Eurhadinoceraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="fulviventris">
