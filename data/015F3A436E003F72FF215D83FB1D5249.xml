(Scopoli, 
<number id="3F3E99726E003F73FB1B5C31FB485400" box="[1197,1259,1853,1877]" pageId="14" pageNumber="215" value="1763.0">1763</number>
)
</taxonomicName>
]. 
<typeStatus id="564D35F76E003F73FABE5C32FADB5403" box="[1288,1400,1854,1878]" pageId="14" pageNumber="215">Lectotype</typeStatus>
