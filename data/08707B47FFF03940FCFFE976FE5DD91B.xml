<taxonomicName id="47D9B1D2FFF03947FC03E976FACEDF75" authority="FAUVEL, 1905" authorityName="FAUVEL" authorityYear="1905" box="[963,1281,1839,1871]" class="Insecta" family="Staphylinidae" genus="Amaurodera" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="90" phylum="Arthropoda" rank="genus">
