<taxonomicName id="4C4F4D0CFF82FFF4F187FA392D3D71E3" authorityName="Zelazny" authorityYear="1981" box="[722,821,1476,1500]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="97" pageNumber="98" phylum="Arthropoda" rank="species" species="concolor">
