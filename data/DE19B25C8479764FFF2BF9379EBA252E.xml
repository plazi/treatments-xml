<taxonomicName id="91B078C98478764CFDC1F82E9C4823AC" box="[637,823,1925,1950]" class="Insecta" family="Braconidae" genus="Cremnops" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="wileycoyotius">
