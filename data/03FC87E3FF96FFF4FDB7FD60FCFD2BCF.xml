<quantity id="4CAD9B10FF96FFF7FBFCFACEFB9E2C70" box="[1057,1116,1318,1342]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="8" pageNumber="1862" unit="m" value="100.0">
<elevation id="0078D1C6FF96FFF7FBFCFACEFB9E2C70" box="[1057,1116,1318,1342]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="8" pageNumber="1862" unit="m" value="100.0">
