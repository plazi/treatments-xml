</taxonomicName>
, 
<taxonomicName id="4C744D69FF8D964F9A89FA1648C6FA5A" authority="Bate, 1881" authorityName="Bate" authorityYear="1881" box="[395,681,1501,1525]" class="Malacostraca" family="Solenoceridae" genus="Hymenopenaeus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="neptunus">
<emphasis id="B900EAF8FF8D964F9A89FA164BF0FA5B" box="[395,415,1501,1524]" italics="true" pageId="5" pageNumber="478">H</emphasis>
. 
<emphasis id="B900EAF8FF8D964F9AAFFA16487DFA5B" box="[429,530,1501,1524]" italics="true" pageId="5" pageNumber="478">neptunus</emphasis>
(
<bibRefCitation id="EFE54B1BFF8D964F9920FA1648CEFA5A" author="Bate" box="[546,673,1501,1525]" pageId="5" pageNumber="478" refString="Bate, C. S. (1881) On the Penaeidae. The Annals and Magazine of Natural History, Series 5, 8 (51), 169 - 196. https: // doi. org / 10.1080 / 00222938109487441" type="journal article" year="1881">Bate, 1881</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="4C744D69FF8D964F99B7FA164E68FA5A" authority="Bate, 1881" authorityName="Bate" authorityYear="1881" box="[693,1031,1500,1525]" class="Malacostraca" family="Solenoceridae" genus="Hymenopenaeus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="obliquirostris">
<emphasis id="B900EAF8FF8D964F99B7FA16491FFA5B" box="[693,880,1500,1524]" italics="true" pageId="5" pageNumber="478">H. obliquirostris</emphasis>
(
<bibRefCitation id="EFE54B1BFF8D964F9882FA164991FA5A" author="Bate" box="[896,1022,1501,1525]" pageId="5" pageNumber="478" refString="Bate, C. S. (1881) On the Penaeidae. The Annals and Magazine of Natural History, Series 5, 8 (51), 169 - 196. https: // doi. org / 10.1080 / 00222938109487441" type="journal article" year="1881">Bate, 1881</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="4C744D69FF8D964F9F11FA164F55FA5A" authority="Ramadan, 1938" authorityName="Ramadan" authorityYear="1938" box="[1043,1338,1500,1525]" class="Malacostraca" family="Solenoceridae" genus="Hymenopenaeus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="fattahi">
<emphasis id="B900EAF8FF8D964F9F11FA164E48FA5B" box="[1043,1063,1501,1524]" italics="true" pageId="5" pageNumber="478">H</emphasis>
. 
<emphasis id="B900EAF8FF8D964F9F34FA174E10FA5B" box="[1078,1151,1500,1524]" italics="true" pageId="5" pageNumber="478">fattahi</emphasis>
<bibRefCitation id="EFE54B1BFF8D964F9F84FA174F55FA5A" author="Ramadan" box="[1158,1338,1500,1525]" pageId="5" pageNumber="478" refString="Ramadan, M. M. (1938) Crustacea: Penaeidae. Scientific Reports of the John Murray Expedition, 5, 35 - 76." type="journal article" year="1938">Ramadan, 1938</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C744D69FF8D964F9E47FA164B4AF9B6" authority="Bruce, 1966" authorityName="Bruce" authorityYear="1966" class="Malacostraca" family="Solenoceridae" genus="Hymenopenaeus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="halli">
<emphasis id="B900EAF8FF8D964F9E47FA164F36FA5B" box="[1349,1369,1501,1524]" italics="true" pageId="5" pageNumber="478">H</emphasis>
. 
<emphasis id="B900EAF8FF8D964F9E6AFA174FF4FA5B" box="[1384,1435,1500,1524]" italics="true" pageId="5" pageNumber="478">halli</emphasis>
<bibRefCitation id="EFE54B1BFF8D964F9B95F9CA4B4AF9B6" author="Bruce" box="[151,293,1536,1561]" pageId="5" pageNumber="478" refString="Bruce, A. J. (1966) Hymenopenaeus halli sp. nov., a new species of penaeid prawn from the South China Sea (Decapoda, Penaeidae). Crustaceana, 11, 216 - 224." type="journal article" year="1966">Bruce, 1966</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C744D69FF8D964F9A31F9CA4BCCF9B7" box="[307,419,1536,1560]" class="Malacostraca" family="Solenoceridae" genus="Hymenopenaeus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="debilis">
<emphasis id="B900EAF8FF8D964F9A31F9CA4B28F9B7" box="[307,327,1537,1560]" italics="true" pageId="5" pageNumber="478">H</emphasis>
