and 
<taxonomicName id="B99F4D0F9A01D305FE4DFE63AE07FE6D" box="[449,562,441,464]" class="Insecta" family="Brachodidae" genus="Nigilgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="257" phylum="Arthropoda" rank="species" species="anactis">
