and 
<taxonomicName id="B99F4D0F9A01D305FC85FE4FAF4BFE11" box="[777,894,405,428]" class="Insecta" family="Brachodidae" genus="Nigilgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="257" phylum="Arthropoda" rank="species" species="anactis">
<emphasis id="4CEBEA9E9A01D305FC85FE4FAF4BFE11" box="[777,894,405,428]" italics="true" pageId="16" pageNumber="257">N. anactis</emphasis>
</taxonomicName>
; signum very wide in 
<taxonomicName id="B99F4D0F9A01D305FB07FE4FA918FE11" box="[1163,1325,404,428]" class="Insecta" family="Brachodidae" genus="Nigilgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="257" phylum="Arthropoda" rank="species" species="atribractea" status="sp. nov.">
<emphasis id="4CEBEA9E9A01D305FB07FE4FA918FE11" box="[1163,1325,404,428]" italics="true" pageId="16" pageNumber="257">N. atribractea</emphasis>
</taxonomicName>
<emphasis id="4CEBEA9E9A01D305FABBFE4EA9A1FE11" bold="true" box="[1335,1428,404,428]" pageId="16" pageNumber="257">
<taxonomicNameLabel id="57D857E59A01D305FABBFE4EA9A1FE11" box="[1335,1428,404,428]" pageId="16" pageNumber="257" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; narrower in 
