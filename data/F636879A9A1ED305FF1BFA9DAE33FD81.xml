<taxonomicName id="B99F4D0F9A01D305FEABFDFFADA0FD81" box="[295,405,548,572]" class="Insecta" family="Brachodidae" genus="Nigilgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="257" phylum="Arthropoda" rank="species" species="browni" status="sp. nov.">
