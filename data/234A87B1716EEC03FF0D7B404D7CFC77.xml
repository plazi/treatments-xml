<collectingCountry id="D3F47637716DEC03FD7D7F674F28FCBF" box="[743,814,808,833]" name="China" pageId="2" pageNumber="203">China</collectingCountry>
, 
<collectingCountry id="D3F47637716DEC03FCA17F664F78FCBE" box="[827,894,809,832]" name="Japan" pageId="2" pageNumber="203">Japan</collectingCountry>
