<collectingCountry id="D3F47637716DEC03FB347F674916FCBF" box="[1198,1296,808,833]" name="Vietnam" pageId="2" pageNumber="203">Vietnam</collectingCountry>
, 
<collectingCountry id="D3F47637716DEC03FA867F674996FCBF" box="[1308,1424,808,833]" name="Sri Lanka" pageId="2" pageNumber="203">
