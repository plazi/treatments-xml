<paragraph id="1593CFEC96185428A35B1C4608B16267" pageId="0" pageNumber="1">
<emphasis id="6229080E5A70AE062CE80D90EF567ACC" bold="true" pageId="0" pageNumber="1">Figures 278-288.</emphasis>
<taxonomicName id="E5919D71CCF373B0818232EC3E2D37F3" baseAuthorityName="Szepligeti" baseAuthorityYear="1903" class="Insecta" family="Braconidae" genus="Aleiodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleiodes diversus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="diversus">
<emphasis id="0929AA7EDE0349BF3DE5F85A045ED387" italics="true" pageId="0" pageNumber="1">Aleiodes diversus</emphasis>
