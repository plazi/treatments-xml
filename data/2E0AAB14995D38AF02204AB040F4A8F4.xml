<taxonomicName id="D976FC0F0F0E9B7C07CE2A5C1C1EEF4B" baseAuthorityName="Rehder" baseAuthorityYear="1945" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="105" pageNumber="106" phylum="Mollusca" rank="species" species="pumilio">
