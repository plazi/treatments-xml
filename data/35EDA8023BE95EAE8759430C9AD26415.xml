<paragraph id="FC3F2D7F831C4C6901B4212BE59DD300" pageId="36" pageNumber="87">
<taxonomicName id="258499F2AF1BFBF1C16B901874C22EAE" authorityName="Champion" authorityYear="1906" class="Insecta" family="Curculionidae" genus="Arachnomorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arachnomorpha circumlineata" order="Coleoptera" pageId="36" pageNumber="87" phylum="Arthropoda" rank="species" species="circumlineata">
