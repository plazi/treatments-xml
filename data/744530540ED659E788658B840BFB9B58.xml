<paragraph id="A6B343719106E2BD8CC909DE511AD363" pageId="0" pageNumber="301">
<taxonomicName id="48C866A0437FFA8742E8AAB921B3213A" authorityName="Yang, van Achterberg &amp; Chen" authorityYear="2008" class="Insecta" family="Braconidae" genus="Sculptolobus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sculptolobus" order="Hymenoptera" pageId="0" pageNumber="301" phylum="Arthropoda" rank="genus">Sculptolobus</taxonomicName>
