<taxonomicName id="CE92F81116187AFB384EDFA9BF784819" authorityName="Yang, van Achterberg &amp; Chen" authorityYear="2008" class="Insecta" family="Braconidae" genus="Sculptolobus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sculptolobus sulcifer" order="Hymenoptera" pageId="0" pageNumber="301" phylum="Arthropoda" rank="species" species="sulcifer">Sculptolobus sulcifer</taxonomicName>
