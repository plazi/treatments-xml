<taxonomicName id="E6F64D09FFB0AA3DFACEFC52AA5CFBD0" baseAuthorityName="Ewer" baseAuthorityYear="1965" class="Reptilia" family="Euparkeriidae" genus="Euparkeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prolacertiformes" pageId="31" pageNumber="870" phylum="Chordata" rank="genus">
