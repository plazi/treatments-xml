<quantity id="4CC89B356548FF9676CAC01AFBCDF9D7" box="[1011,1118,1625,1651]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" pageId="15" pageNumber="16" unit="mm" value="1.15">1.15 mm</quantity>
