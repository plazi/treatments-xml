</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="02F54D76EE3CFFB1FE49FA22FBBEFA26" authority="Brzeski &amp; Winiszewska-Slipinska, 1993" authorityName="Brzeski &amp; Winiszewska-Slipinska" authorityYear="1993" box="[473,1087,1410,1443]" class="Adenophorea" family="Tripylidae" genus="Tripylella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="3" pageNumber="4" phylum="Nematoda" rank="genus">
