<taxonomicName id="02F54D76EE3CFFB1FB54F9E0FAE5F9E1" box="[1220,1380,1611,1636]" class="Adenophorea" family="Tripylidae" genus="Trischistoma" kingdom="Animalia" order="Enoplida" pageId="3" pageNumber="4" phylum="Nematoda" rank="genus">
