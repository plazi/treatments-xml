<taxonomicName id="02F54D76EE3CFFB1FDBCF9E0FD05F9E1" box="[556,644,1611,1636]" class="Adenophorea" family="Tripylidae" genus="Tripyla" kingdom="Animalia" order="Enoplida" pageId="3" pageNumber="4" phylum="Nematoda" rank="genus">
