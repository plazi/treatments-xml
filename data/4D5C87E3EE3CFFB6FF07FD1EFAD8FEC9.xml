and 
<taxonomicName id="02F54D76EE3CFFB1FD54F9E0FCBCF9E1" box="[708,829,1611,1636]" class="Adenophorea" family="Tripylidae" genus="Tripylella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="3" pageNumber="4" phylum="Nematoda" rank="genus">
