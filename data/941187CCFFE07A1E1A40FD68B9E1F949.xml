<collectionCode id="7AA9AE1FFFE07A1C1AF5F926BF1DF925" box="[445,515,1762,1788]" httpUri="http://grbio.org/cool/j51y-avsb" name="Naking Institute of Geology and Palaeontology" pageId="5" pageNumber="64">NIGP</collectionCode>
