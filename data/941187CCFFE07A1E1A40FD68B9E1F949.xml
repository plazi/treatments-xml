<collectionCode id="7AA9AE1FFFE07A1C19D3F926BFFCF925" box="[667,738,1762,1788]" httpUri="http://grbio.org/cool/j51y-avsb" name="Naking Institute of Geology and Palaeontology" pageId="5" pageNumber="64">NIGP</collectionCode>
