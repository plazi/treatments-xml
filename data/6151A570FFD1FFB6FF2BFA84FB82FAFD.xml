<subSubSection id="A1E247EDFFD1FFB5FF2BFA84FE4CFABE" box="[151,429,1335,1361]" pageId="1" pageNumber="86" type="nomenclature">
<paragraph id="E9471466FFD1FFB5FF2BFA84FE4CFABE" blockId="1.[151,429,1335,1361]" box="[151,429,1335,1361]" pageId="1" pageNumber="86">
<heading id="B20FA30AFFD1FFB5FF2BFA84FE4CFABE" bold="true" box="[151,429,1335,1361]" fontSize="11" level="1" pageId="1" pageNumber="86" reason="1">
<emphasis id="DB8CC874FFD1FFB5FF2BFA84FE4CFABE" bold="true" box="[151,429,1335,1361]" pageId="1" pageNumber="86">
<taxonomicName id="2EF86FE5FFD1FFB5FF2BFA84FED9FABE" authority="Okuno &amp; Bruce, 2010" authorityName="Okuno &amp; Bruce" authorityYear="2010" box="[151,312,1335,1361]" class="Malacostraca" family="Palaemonidae" genus="Ancylomenes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="86" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="DB8CC874FFD1FFB5FF2BFA84FED9FABE" bold="true" box="[151,312,1335,1361]" italics="true" pageId="1" pageNumber="86">Ancylomenes</emphasis>
</taxonomicName>
