<taxonomicName id="51A62DFBFFF9FFB433CEFD7EFE2B53E7" authority="Watts, 1997" authorityName="Watts" authorityYear="1997" box="[151,448,727,754]" class="Insecta" family="Dytiscidae" genus="Sekaliporus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="108" phylum="Arthropoda" rank="genus">
<emphasis id="A4D28A6AFFF9FFB433CEFD7EFE2B53E7" bold="true" box="[151,448,727,754]" pageId="1" pageNumber="108">
<emphasis id="A4D28A6AFFF9FFB433CEFD7EFECD53E4" bold="true" box="[151,294,727,753]" italics="true" pageId="1" pageNumber="108">Sekaliporus</emphasis>
<bibRefCitation id="F2372B89FFF9FFB43274FD71FE2B53E7" author="Watts" box="[301,448,728,754]" pageId="1" pageNumber="108" refString="Watts, C. H. S. (1997) A new genus and species of Australian Dytiscidae (Coleoptera). Records of the South Australian Museum, 29 (2), 121 - 123." type="journal article" year="1997">
Watts, 
<number id="206E445FFFF9FFB432DCFD71FE2B53E7" box="[389,448,728,754]" pageId="1" pageNumber="108" value="1997.0">1997</number>
</bibRefCitation>
