<typeStatus id="54CE886FFEBCFE8F70B5FAD0FF6AFF38" box="[151,203,1353,1374]" pageId="304" pageNumber="305">Type</typeStatus>
species: 
<taxonomicName id="4C754D4EFEBCFE8F7104FAD0FD18FF39" authority="Heller, 1880" authorityName="Heller" authorityYear="1880" box="[294,697,1353,1375]" class="Arachnida" family="Syringophilidae" genus="Syringophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="304" pageNumber="305" phylum="Arthropoda" rank="species" species="bipectinatus">
<emphasis id="B901EADFFEBCFE8F7104FAD0FD94FF39" box="[294,565,1353,1375]" italics="true" pageId="304" pageNumber="305">Syringophilus bipectinatus</emphasis>
Heller, 1880
</taxonomicName>
