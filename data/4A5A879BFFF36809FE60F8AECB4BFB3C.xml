<quantity id="050B9B68FFF06809FC66FD03CCCCFC8C" box="[913,987,747,773]" metricMagnitude="2" metricUnit="m" metricValue="2.84" pageId="118" pageNumber="119" unit="m" value="284.0">284 m</quantity>
