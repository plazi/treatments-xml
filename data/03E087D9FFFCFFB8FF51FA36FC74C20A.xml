<taxonomicName id="4C494D4CFFFCFFB8FA84FA7FFEFAC22E" class="Hydrozoa" family="Sertulariidae" genus="Sertularia" kingdom="Animalia" order="Leptothecata" pageId="9" pageNumber="210" phylum="Cnidaria" rank="species" species="stolonifera">
