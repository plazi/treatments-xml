<quantity id="4CB19B2AFFFCFFB8FB55FA7FFB44C1C2" box="[1171,1241,1483,1509]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="9" pageNumber="210" unit="m" value="400.0">400 m</quantity>
