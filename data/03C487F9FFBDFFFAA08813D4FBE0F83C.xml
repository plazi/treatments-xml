<taxonomicName id="4C6D4D6CFFBDFFFAA4F81601FA45F8E5" box="[1255,1431,1888,1913]" class="Polychaeta" family="Dorvilleidae" genus="Ophryotrocha" kingdom="Animalia" order="Eunicida" pageId="5" pageNumber="48" phylum="Annelida" rank="species" species="craigsmithi">
