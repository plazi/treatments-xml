<taxonomicName id="27CA4D35FF72FF145A9CFB0AFC3A65D8" box="[642,905,1159,1185]" class="Insecta" family="Sarcophagidae" genus="Brachicoma" kingdom="Animalia" order="Diptera" pageId="191" pageNumber="192" phylum="Arthropoda" rank="species" species="metopiella">
