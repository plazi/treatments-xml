<emphasis id="D2BEEAA4FF72FF1459A9FB26FD1165BC" box="[439,674,1195,1221]" italics="true" pageId="191" pageNumber="192">
<taxonomicName id="27CA4D35FF72FF1459A9FB26FDF365BD" box="[439,576,1195,1221]" class="Insecta" family="Conopidae" genus="Semel" kingdom="Animalia" order="Diptera" pageId="191" pageNumber="192" phylum="Arthropoda" rank="species" species="capta">Semel capta</taxonomicName>
