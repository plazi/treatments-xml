<taxonomicName id="E3B64D6BFFFCFF9EFF61FC1DFDF2B33C" authority="(Schenck, 1863)" baseAuthorityName="Schenck" baseAuthorityYear="1863" box="[151,584,1013,1039]" class="Insecta" family="Cynipidae" genus="Neuroterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="475" phylum="Arthropoda" rank="species" species="albipes">
