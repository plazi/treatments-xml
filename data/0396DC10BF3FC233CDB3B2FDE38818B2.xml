, 
<collectingRegion id="49FBA3E4BF3FC233CDD9B05FE78F1AD3" box="[129,247,1642,1666]" country="Sweden" name="Stockholm" pageId="48" pageNumber="201">Stockholm</collectingRegion>
, 
