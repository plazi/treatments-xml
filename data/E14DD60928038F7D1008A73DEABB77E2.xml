<paragraph id="8F2F0D786959DCF728F063C1FF72E7B2" pageId="4" pageNumber="81">
<taxonomicName id="91B79F1A781C2208F1E216CD8290BD65" LSID="MB829583" authority="Meng Zhou &amp; Y. C. Dai" class="Agaricomycetes" family="Haploporidae" genus="Haploporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Haploporus angustisporus" order="Plagiorchiida" pageId="4" pageNumber="81" phylum="Basidiomycota" rank="species" species="angustisporus">Haploporus angustisporus Meng Zhou&amp;Y.C.Dai</taxonomicName>
