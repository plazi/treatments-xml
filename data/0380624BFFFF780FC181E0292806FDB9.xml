<materialsCitation id="3B41D900FFF9780FC412E2A22BF7FE54" ID-GBIF-Occurrence="1638540705" collectionCode="ZISP" country="China" location="Same" pageId="12" pageNumber="566" specimenCount="1" specimenCount-male="1" stateProvince="Gansu" typeStatus="lectotype">
