<quantity id="8D6B9B35FFFEFF95FD6EFB3E1F22FB3A" box="[744,804,1198,1222]" metricMagnitude="1" metricUnit="m" metricValue="1.8" pageId="77" pageNumber="78" unit="m" value="18.0">18 m</quantity>
