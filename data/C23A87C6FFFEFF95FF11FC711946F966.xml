<quantity id="8D6B9B35FFFEFF95FCB6FB1A1F97FB5E" box="[816,913,1162,1186]" metricMagnitude="1" metricUnit="m" metricValue="2.4" metricValueMax="2.8" metricValueMin="2.0" pageId="77" pageNumber="78" unit="m" value="24.0" valueMax="28.0" valueMin="20.0">20-28 m</quantity>
