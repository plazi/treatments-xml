<collectionCode id="2C82AE15FFFEFF95FB5DFB1A192AFB5D" LSID="urn:lsid:biocol.org:col:34871" box="[1243,1324,1162,1186]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="77" pageNumber="78">USNM</collectionCode>
