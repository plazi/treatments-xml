<heading id="D0E08E76FF98F612FF7F0AE31FC7F88E" bold="true" box="[151,676,1778,1805]" fontSize="11" level="1" pageId="17" pageNumber="118" reason="1">
<taxonomicName id="4C174299FF98F612FF7F0AE31FC7F88E" authority="(Van Duzee, 1907)" authorityName="Van Duzee" authorityYear="1907" baseAuthorityName="Van Duzee" baseAuthorityYear="1907" box="[151,676,1778,1805]" class="Insecta" family="Cicadellidae" genus="Apogonalia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="120" phylum="Arthropoda" rank="species" species="sanguinipes">
<emphasis id="B963E508FF98F612FF7F0AE31FC7F88E" bold="true" box="[151,676,1778,1805]" pageId="17" pageNumber="118">
<emphasis id="B963E508FF98F612FF7F0AE31CD8F88F" bold="true" box="[151,443,1778,1804]" italics="true" pageId="17" pageNumber="118">Apogonalia SanguinipeS</emphasis>
