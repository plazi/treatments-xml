</taxonomicName>
<taxonomicNameLabel id="A237844EA2268E75FE854CBCFE9F7DDA" box="[292,356,402,427]" pageId="22" pageNumber="23" rank="species">
<emphasis id="B9043935A2268E75FE854CBCFE9F7DDA" bold="true" box="[292,356,402,427]" pageId="22" pageNumber="23">n. sp.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="4C709EA4A2268E75FED54CBAFE1C7DDA" authority="Passanha &amp; Brescovit, 2018" authorityName="Passanha &amp; Brescovit" authorityYear="2018" box="[372,487,404,427]" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="mutum" status="n. sp.">
