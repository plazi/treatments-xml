<taxonomicName id="4C709EA4A2268E75FE624C41FDA87DF6" authority="Passanha &amp; Brescovit, 2018" authorityName="Passanha &amp; Brescovit" authorityYear="2018" box="[451,595,366,391]" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="yacambu" status="n. sp.">
