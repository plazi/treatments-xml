<taxonomicName id="8DEC4D30FF534FE4FF50FF9D463FFD0E" box="[151,326,717,742]" class="Insecta" family="Pamphagidae" genus="Nocaracris" kingdom="Animalia" order="Orthoptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="species" species="niethammeri">
<emphasis id="7898EAA1FF534FE4FF50FF9D463FFD0E" bold="true" box="[151,326,717,742]" italics="true" pageId="181" pageNumber="182">P. niethammeri</emphasis>
