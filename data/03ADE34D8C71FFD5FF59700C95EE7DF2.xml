<taxonomicName id="4C0429D88C71FFD3FE17709F96CF78B6" box="[473,767,1939,1963]" class="Insecta" family="Chironomidae" genus="Micropsectra" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="30" phylum="Arthropoda" rank="species" species="insignilobus">
