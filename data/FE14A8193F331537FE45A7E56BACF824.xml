<quantity id="B145B4EA3F331537FACFA08F6DCCFB7A" box="[1374,1457,1195,1224]" metricMagnitude="2" metricUnit="m" metricValue="2.85" pageId="4" pageNumber="17" unit="m" value="285.0">285 m</quantity>
