<paragraph id="8B87E76C666CFFDCFF0B06306A8D9928" blockId="10.[151,723,288,313]" box="[151,723,288,313]" pageId="10" pageNumber="291">
Synonyms: 
<taxonomicName id="4C389CEF666CFFDCFE8C06316A8D9928" authority="Bigelow and Schroeder, 1954" authorityName="Bigelow and Schroeder" authorityYear="1954" box="[272,723,288,313]" class="Chondrichthyes" family="Rajidae" genus="Raja" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="10" pageNumber="291" phylum="Chordata" rank="species" species="fuliginea">
