<subSubSection id="C378657DFFAC561AEE86FA36FAD7FA7B" box="[151,1384,1449,1475]" pageId="16" pageNumber="17" type="etymology">
<paragraph id="8BDD36F6FFAC561AEE86FA36FAD7FA7B" blockId="16.[151,1437,1449,2016]" box="[151,1384,1449,1475]" pageId="16" pageNumber="17">
<emphasis id="B916EAE4FFAC561AEE86FA36FE97FA7B" bold="true" box="[151,296,1449,1475]" pageId="16" pageNumber="17">Etymology:</emphasis>
