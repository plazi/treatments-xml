) of 
<taxonomicName id="4C624D75FFAC561AEDFDFA36FB18FA7A" box="[1004,1191,1449,1474]" class="Cestoda" family="Anoplocephalidae" genus="Sciurotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="16" pageNumber="17" phylum="Platyhelminthes" rank="species" species="transversaria">
<emphasis id="B916EAE4FFAC561AEDFDFA36FB18FA7A" box="[1004,1191,1449,1474]" italics="true" pageId="16" pageNumber="17">S. transversaria</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C624D75FFAC561AEAF1FA36FADFFA7A" box="[1248,1376,1449,1474]" class="Cestoda" family="Anoplocephalidae" genus="Sciurotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="16" pageNumber="17" phylum="Platyhelminthes" rank="species" species="wigginsi">
<emphasis id="B916EAE4FFAC561AEAF1FA36FADFFA7A" box="[1248,1376,1449,1474]" italics="true" pageId="16" pageNumber="17">S. wigginsi</emphasis>
