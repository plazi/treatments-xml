<collectionCode id="ED0AE78AFF92FFFBFE69FD502617CDC7" box="[499,573,711,735]" country="USA" httpUri="http://grbio.org/cool/asxa-80fx" name="University of Maine" pageId="26" pageNumber="2026">UMO</collectionCode>
