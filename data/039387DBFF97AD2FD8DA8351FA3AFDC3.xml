<taxonomicName id="4C3A4D4EFF97AD2ED9D78350FD93FF20" authorityName="Boucomont" authorityYear="1911" box="[410,565,1554,1581]" class="Insecta" family="Bolboceratidae" genus="Bolbelasmus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="392" phylum="Arthropoda" rank="genus">
