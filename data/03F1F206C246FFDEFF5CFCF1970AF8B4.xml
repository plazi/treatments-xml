<subSubSection id="C342109BC246FFDEFF0CF8E0970AF8B4" pageId="11" pageNumber="112" type="etymology">
<paragraph id="8BE74310C246FFDEFF0CF8E0970AF8B4" blockId="11.[151,1436,906,1867]" pageId="11" pageNumber="112">
<emphasis id="B92C9F02C246FFDEFF0CF8E0971DF8D8" bold="true" box="[199,334,1806,1831]" pageId="11" pageNumber="112">Etymology.</emphasis>
