<taxonomicName id="4C024D685867F337FEF79421FE3BFBE8" authorityName="Sauer" authorityYear="1968" baseAuthorityName="Andrassy" baseAuthorityYear="1961" box="[337,454,1120,1146]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="pileatus">
