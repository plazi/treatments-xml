<taxonomicName id="4C024D685867F337FB46947DFA8AFBC4" authorityName="Goodey" authorityYear="1963" baseAuthorityName="Andrassy" baseAuthorityYear="1958" box="[1248,1399,1084,1110]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="bulgaricus">
<emphasis id="B976EAF95867F337FB46947DFA8AFBC4" box="[1248,1399,1084,1110]" italics="true" pageId="12" pageNumber="334">T. bulgaricus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C024D685867F337FA2E947DFEEEFBE8" authorityName="Goodey" authorityYear="1963" baseAuthorityName="Cobb" baseAuthorityYear="1920" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="cylindricus">
<emphasis id="B976EAF95867F337FA2E947DFEEEFBE8" italics="true" pageId="12" pageNumber="334">T. cylindricus</emphasis>
</taxonomicName>
