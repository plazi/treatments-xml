<taxonomicName id="4C024D685867F337FDA59421FD64FBE8" authorityName="Sauer" authorityYear="1968" box="[515,665,1121,1146]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="constrictus">
