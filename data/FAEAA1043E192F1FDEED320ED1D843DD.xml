<taxonomicName id="B8CD2BD4A0C8E6531385DF749B70776D" class="Gastropoda" family="Lymnaeidae" genus="Lymnaea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lymnaea (Peregriana) intermedia" pageId="3" pageNumber="134" phylum="Mollusca" rank="species" species="intermedia" subGenus="Peregriana">Lymnaea (Peregriana) intermedia</taxonomicName>
