= 
<taxonomicName id="35894D47FF91FFDDFF7F0F575C3C616D" authority="Giglio-Tos, 1919" authorityName="Giglio-Tos" authorityYear="1919" box="[219,550,297,319]" class="Insecta" family="Hymenopodidae" genus="Parhymenopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mantodea" pageId="15" pageNumber="145" phylum="Arthropoda" rank="genus">
<emphasis id="C0FDEAD6FF91FFDDFF7F0F575F68616D" box="[219,370,297,319]" italics="true" pageId="15" pageNumber="145">Parhymenopus</emphasis>
<bibRefCitation id="96184B35FF91FFDDFEDD0F575C3C616D" author="Giglio-Tos" box="[377,550,297,319]" pageId="15" pageNumber="145" refString="Giglio-Tos, E. (1919) Saggio di una nuova classificazione dei mantidi. Bullettino della Societa Entomologica Italiana, 49, 50 - 87." type="journal article" year="1919">
