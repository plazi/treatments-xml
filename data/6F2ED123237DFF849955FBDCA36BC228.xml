<taxonomicName id="20871BB6237DFF859EDDF931A47CCCFC" authority="Irmler, 2015" authorityName="Irmler" authorityYear="2015" box="[1261,1320,1757,1781]" class="Insecta" family="Staphylinidae" genus="Paratorchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="185" phylum="Arthropoda" rank="species" species="tilli" status="spec. nov.">
