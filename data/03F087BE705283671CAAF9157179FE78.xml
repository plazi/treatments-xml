<paragraph id="8BE636A8705283611CAAF9737333F906" blockId="3.[151,803,1723,1782]" box="[151,280,1757,1782]" pageId="3" pageNumber="494">
(Figs 
<number id="3D91248F705283611CEAF973733AF906" box="[215,273,1757,1782]" pageId="3" pageNumber="494" value="12.0" valueMax="15.0" valueMin="9.0">9–15</number>
)
</paragraph>
