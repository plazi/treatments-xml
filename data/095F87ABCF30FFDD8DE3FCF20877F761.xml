male, 
<emphasis id="B382EAAFCF30FFDC8C9BF80F09A5F1EF" bold="true" box="[495,653,1986,2012]" pageId="4" pageNumber="63">
<collectingCountry id="F9E1762DCF30FFDC8C9BF80F09A0F1EF" box="[495,648,1986,2012]" name="Ecuador" pageId="4" pageNumber="63">ECUADOR</collectingCountry>
