<taxonomicName id="439367CAFF61F603FEE6D9F7FE5CFD14" authorityName="Caballero" authorityYear="1944" box="[332,491,663,689]" class="Chromadorea" family="Onchocercidae" genus="Ochoterenella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="14" pageNumber="15" phylum="Nematoda" rank="genus">
<emphasis id="B6E7C05BFF61F603FEE6D9F7FE5CFD14" box="[332,491,663,689]" italics="true" pageId="14" pageNumber="15">Ochoterenella</emphasis>
