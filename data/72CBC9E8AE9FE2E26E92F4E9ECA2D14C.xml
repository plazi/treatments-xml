<paragraph id="475FA852680FC0ADF1C7C8E0F6D05DC7" pageId="0" pageNumber="1">
<taxonomicName id="87164746F8841E4A94152A741CDDCFA1" class="Diplopoda" family="Iulomorphidae" genus="Talomius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Talomius weldensis" order="Spirostreptida" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="weldensis">Talomius weldensis</taxonomicName>
