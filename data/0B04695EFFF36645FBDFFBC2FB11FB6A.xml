<taxonomicName id="44ADA3CBFFF36645FBF6FB52FB4EFB0F" authorityName="Linnaeus" authorityYear="1758" box="[1073,1155,1183,1204]" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="137" phylum="Arthropoda" rank="genus">Formica</taxonomicName>
</emphasis>
