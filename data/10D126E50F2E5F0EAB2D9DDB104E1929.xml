<taxonomicName id="564DA1F288AFADE013C4111666F5D69B" class="Magnoliopsida" family="Lamiaceae" genus="Plectranthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Plectranthus scutellarioides" order="Lamiales" pageId="0" pageNumber="93" phylum="Tracheophyta" rank="species" species="scutellarioides">Plectranthus scutellarioides</taxonomicName>
