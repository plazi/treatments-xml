<taxonomicName id="19389A5152B8D3BB08B249D708BDB8B6" family="Lamiaceae" genus="Ocymum" higherTaxonomySource="treatment-meta" kingdom="Plantae" lsidName="Ocymum" order="Lamiales" pageId="0" pageNumber="93" rank="genus">Ocymum</taxonomicName>
