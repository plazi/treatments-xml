<taxonomicName id="F4167669709B8AEB67D6D65F36DECE8A" class="Magnoliopsida" family="Lamiaceae" genus="Ocimum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ocimum scutellarioides" order="Lamiales" pageId="0" pageNumber="93" phylum="Tracheophyta" rank="species" species="scutellarioides">Ocimum scutellarioides</taxonomicName>
