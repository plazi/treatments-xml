<taxonomicName id="FF33EB0DCA23889A91DF2AA350ED3C28" authority="Thonn." authorityName="Thonn." class="Magnoliopsida" family="Lamiaceae" genus="Solenostemon" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Solenostemon scutellarioides" order="Lamiales" pageId="0" pageNumber="93" phylum="Tracheophyta" rank="species" species="scutellarioides">Solenostemon scutellarioides</taxonomicName>
