<subSubSection id="C366656FFFDD4C16F5273917F45BF352" pageId="18" pageNumber="367" type="discussion">
<paragraph id="8BC336E4FFDD4C16F5273917F45BF352" blockId="18.[2690,3490,2957,3523]" pageId="18" pageNumber="367">
<taxonomicName id="4C7C4D67FFDD4C16F5273917F44AF36E" box="[2690,2994,3168,3207]" class="Mammalia" family="Emballonuridae" genus="Diclidurus" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="367" phylum="Chordata" rank="species" species="ingens">
