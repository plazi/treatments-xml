<taxonomicName id="4C7C4D67FFDD4C16FCB52422FBD8EE99" authorityName="Peters" authorityYear="1869" box="[784,1056,4437,4464]" class="Mammalia" family="Emballonuridae" genus="Diclidurus" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="367" phylum="Chordata" rank="species" species="scutatus">Diclidurus scutatus</taxonomicName>
