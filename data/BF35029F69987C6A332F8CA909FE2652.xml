<taxonomicName id="E5F58ED3250F586C594028BC8940EBCB" class="Insecta" family="Bolboceratidae" genus="Bolbochromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bolbochromus malayensis" order="Coleoptera" pageId="6" pageNumber="45" phylum="Arthropoda" rank="species" species="malayensis">Bolbochromus malayensis</taxonomicName>
