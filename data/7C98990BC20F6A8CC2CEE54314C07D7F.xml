<paragraph id="C79B6F5B48BD9C8A2C8464D011D22A49">
<taxonomicName id="D3FC3BBD18DB5538CC7738F5CFCC157B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145082" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium (Xiphomyrmex) marginatum Forel" lsidName-HNS="Tetramorium (Xiphomyrmex) marginatum Forel" order="Hymenoptera" phylum="Arthropoda" rank="species" species="marginatum" subGenus="Xiphomyrmex">Tetramorium {Xiphomyrmex) marginatum Forel</taxonomicName>
