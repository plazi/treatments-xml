<taxonomicName id="59A30D86C66EFF9F29859802377A7E07" authority="Huber, 2018" authorityName="Huber" authorityYear="2018" box="[910,1128,1872,1894]" class="Arachnida" family="Pholcidae" genus="Mesabolivar" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="macushi" status="sp. n.">
