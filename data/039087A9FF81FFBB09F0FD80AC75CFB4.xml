<taxonomicName id="4C394D3CFF81FFBB0D75FD32ABD1C964" authorityName="Oppell" authorityYear="1811" box="[1042,1172,757,781]" class="Reptilia" family="Colubridae" kingdom="Animalia" order="Squamata" pageId="46" pageNumber="47" phylum="Chordata" rank="family">Colubridae</taxonomicName>
