host), 
<taxonomicName id="4C394D3CFF81FFBB0B03FD31AC5BC964" box="[612,798,757,781]" class="Chromadorea" family="Rhabdiasidae" genus="Entomelas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="46" pageNumber="47" phylum="Nematoda" rank="species" species="climacophora">
