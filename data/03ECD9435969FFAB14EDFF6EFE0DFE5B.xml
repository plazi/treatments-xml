<taxonomicNameLabel id="A202093C5968FFB415A5F840FD84F87F" box="[479,610,1977,2000]" pageId="15" pageNumber="16" rank="species">new species</taxonomicNameLabel>
. 
<emphasis id="B931B4475968FFB41617F840FD9CF87E" bold="true" box="[621,634,1977,2001]" pageId="15" pageNumber="16">a</emphasis>
epygine, ventral view; 
<emphasis id="B931B4475968FFB4170CF840FC63F87E" bold="true" box="[886,901,1977,2001]" pageId="15" pageNumber="16">b</emphasis>
vulva; 
<emphasis id="B931B4475968FFB417ACF840FC04F87E" bold="true" box="[982,994,1977,2001]" pageId="15" pageNumber="16">c</emphasis>
