from 
<collectingCountry id="F35228C55969FFB5161AFEFBFD46FEB5" box="[608,672,258,282]" name="Chile" pageId="14" pageNumber="15">Chile</collectingCountry>
, Región V de Valparaíso, Petorca province, Zapallar, 
<date id="FFFB4E955969FFB5116FFEFBFA71FEB5" box="[1301,1431,258,282]" pageId="14" pageNumber="15" value="1950-11-27">
