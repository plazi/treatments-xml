<collectingCountry id="F331F5BAFFD7915CAC60FEBE75F7FE8C" box="[364,474,268,294]" name="Bahamas" pageId="9" pageNumber="10">Bahamas</collectingCountry>
, Grand 
<collectingCountry id="F331F5BAFFD7915CAF3BFEBE76BAFE8C" box="[567,663,268,294]" name="Bahamas" pageId="9" pageNumber="10">Bahama</collectingCountry>
