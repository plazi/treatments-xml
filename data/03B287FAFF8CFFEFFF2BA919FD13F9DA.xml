<collectionCode id="ED0AAE29FF8CFFEFFABDAB81FACAFC39" box="[1281,1340,981,1007]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="11" pageNumber="533" type="Museum">ZSM</collectionCode>
