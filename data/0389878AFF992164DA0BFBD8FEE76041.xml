</taxonomicName>
<taxonomicNameLabel id="A26757F5FF982164DB80FE59FF066061" pageId="21" pageNumber="2803" rank="species">comb. nov.</taxonomicNameLabel>
; 
<taxonomicName id="4C204D1FFF982164D983FE79FE376060" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" box="[180,408,495,519]" class="Bacilli" family="Lactobacillaceae" genus="Companilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="21" pageNumber="2803" phylum="Firmicutes" rank="genus">
