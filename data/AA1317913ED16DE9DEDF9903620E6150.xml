6. 
<taxonomicName id="7A9326B9D1E531418F1F6A72CA243622" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28530" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex laevigatus Weber" order="Hymenoptera" pageNumber="178" phylum="Arthropoda" rank="species" species="laevigatus">Cyphomyrmex laevigatus Weber</taxonomicName>
