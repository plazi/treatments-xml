<taxonomicName id="60894D1EFFF5D614A927FA8CFDABFABF" baseAuthorityName="Moore" baseAuthorityYear="1867" box="[294,525,1350,1374]" class="Insecta" family="Crambidae" genus="Neadeloides" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="290" phylum="Arthropoda" rank="species" species="cinerealis">
<emphasis id="95FDEA8FFFF5D614A927FA8CFDABFABF" box="[294,525,1350,1374]" italics="true" pageId="5" pageNumber="290">
Neadeloides 
<taxonomicName id="60894D1EFFF5D614A9ABFA8CFDABFABF" box="[426,525,1350,1374]" pageId="5" pageNumber="290" rank="species" species="cinerealis">cinerealis</taxonomicName>
