<taxonomicName id="6AD247ADFFDEFFE3FEBCFBF3E8EFFBBB" baseAuthorityName="Germar" baseAuthorityYear="1839" box="[277,500,1087,1113]" class="Insecta" family="Pentatomidae" genus="Cyrtocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="trigonus">
