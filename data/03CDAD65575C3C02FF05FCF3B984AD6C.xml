<emphasis id="B910C061575D3C01FD65F940B800AE19" bold="true" box="[759,830,1778,1800]" pageId="37" pageNumber="138">n. gen.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="4C6467F0575D3C01FCD9F940B890AE19" box="[843,942,1778,1800]" class="Gastropoda" family="Clenchiellidae" genus="Colenuda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="37" pageNumber="138" phylum="Mollusca" rank="genus" status="n. gen.">
