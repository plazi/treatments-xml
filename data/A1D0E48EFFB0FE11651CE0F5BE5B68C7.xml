<normalizedToken id="842A60326F09237399D25D0742A4120C" originalValue="Südlicher" pageId="null" pageNumber="701">Suedlicher</normalizedToken>
<normalizedToken id="9A7B46C22EE451626D64DB4280C7F3B1" originalValue="Zürgelbaum" pageId="null" pageNumber="701">Zuergelbaum</normalizedToken>
</paragraph>
