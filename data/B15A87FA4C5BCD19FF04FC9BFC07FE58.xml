<taxonomicName id="FEF34D6F4C5BCD16FADAFAE0FA20FADC" authorityName="Yao &amp; Li" authorityYear="2019" box="[1353,1436,1342,1367]" class="Arachnida" family="Pholcidae" genus="Khorata" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="162" phylum="Arthropoda" rank="species" species="qian">
