<taxonomicName id="A97C80582D193E99C2B0264A25D22098" class="Insecta" family="Cerambycidae" genus="Obrium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Obrium" order="Coleoptera" pageId="207" pageNumber="208" phylum="Arthropoda" rank="genus">Obrium</taxonomicName>
