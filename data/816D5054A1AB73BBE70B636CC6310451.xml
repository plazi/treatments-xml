<subSubSection id="6B3E96F95450A0D73ACA4C4ED864F0CA" type="nomenclature">
<paragraph id="BD21F8E86F6E2D09E7A05670E05DF0E5" pageNumber="329">
<taxonomicName id="80C93C68FA27412322348EEE0C5351EA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181809" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole nubicola Wilson" order="Hymenoptera" pageNumber="329" phylum="Arthropoda" rank="species" species="nubicola">Pheidole nubicola</taxonomicName>
