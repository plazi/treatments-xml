<taxonomicName id="75F04D3DFFC0FFBEFDFF5E1BFD5C994B" authorityName="Mound &amp; Masumoto" authorityYear="2005" box="[580,679,1795,1816]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="darwini">
