<taxonomicName id="75F04D3DFFC0FFBEFED05E3DFE419969" authorityName="Mound &amp; Masumoto" authorityYear="2005" box="[363,442,1829,1850]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="diana">
