<taxonomicName id="4C2F4D7A7E6AFFAFF82AF9AC2025F9F6" box="[348,614,1604,1627]" class="Insecta" family="Leuctridae" genus="Rhopalopsole" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="species" species="ningxiana" status="sp. nov.">
