<taxonomicName id="0F974D5DFFECFFB068E11808FEF8B50B" authority="Gnezdilov, 2017" authorityName="Gnezdilov" authorityYear="2017" box="[218,381,1537,1556]" class="Insecta" family="Caliscelidae" genus="Raunolina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="16" phylum="Arthropoda" rank="species" species="remanei" status="sp. nov.">
<emphasis id="FAE3EACCFFECFFB068E11808FEF8B50B" box="[218,381,1537,1556]" italics="true" pageId="5" pageNumber="16">Raunolina remanei</emphasis>
