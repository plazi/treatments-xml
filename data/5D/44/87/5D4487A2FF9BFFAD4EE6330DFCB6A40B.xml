<taxonomicName authorityName="Fieber" authorityYear="1858" box="[573,699,849,875]" class="Insecta" family="Miridae" genus="Macrolophus" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="costalis">
