<emphasis box="[1264,1334,849,875]" italics="true" pageId="21" pageNumber="322">Salvia</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1351,1437,849,875]" class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" pageId="21" pageNumber="322" phylum="Tracheophyta" rank="genus">
<emphasis box="[1351,1437,849,875]" italics="true" pageId="21" pageNumber="322">Stachys</emphasis>
