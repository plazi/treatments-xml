. 
<emphasis bold="true" box="[286,334,1721,1743]" pageId="8" pageNumber="521">A–G</emphasis>
, 
<taxonomicName box="[349,559,1721,1743]" class="Insecta" family="Heleomyzidae" genus="Rhinotora" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="521" phylum="Arthropoda" rank="species" species="paschoali" status="sp. nov.">
<emphasis box="[349,559,1721,1743]" italics="true" pageId="8" pageNumber="521">Rhinotora paschoali</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[575,657,1721,1742]" pageId="8" pageNumber="521" rank="species">
