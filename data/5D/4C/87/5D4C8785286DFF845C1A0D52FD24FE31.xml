<emphasis bold="true" box="[403,494,1832,1856]" pageId="42" pageNumber="43">A, B, C.</emphasis>
<taxonomicName authority="Hippa, 2006" authorityName="Hippa" authorityYear="2006" box="[502,673,1833,1856]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="horrida" status="sp. n.">
<emphasis box="[502,673,1833,1856]" italics="true" pageId="42" pageNumber="43">Manota horrida</emphasis>
</taxonomicName>
<emphasis bold="true" box="[682,742,1832,1856]" pageId="42" pageNumber="43">
<taxonomicNameLabel box="[682,742,1832,1856]" pageId="42" pageNumber="43" rank="species">sp. n.</taxonomicNameLabel>
