<taxonomicName authorityName="J. L. Barnard" authorityYear="1962" box="[857,986,1645,1670]" class="Malacostraca" family="Eriopisidae" genus="Netamelita" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="714" phylum="Arthropoda" rank="genus">
<emphasis box="[857,986,1645,1670]" italics="true" pageId="1" pageNumber="714">Netamelita</emphasis>
</taxonomicName>
: 
<taxonomicName authority="Thomas &amp; Barnard, 1991" authorityName="Thomas &amp; Barnard" authorityYear="1991" box="[1002,1431,1645,1671]" class="Malacostraca" family="Eriopisidae" genus="Netamelita" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="714" phylum="Arthropoda" rank="species" species="brocha">
<emphasis box="[1002,1119,1645,1670]" italics="true" pageId="1" pageNumber="714">N. brocha</emphasis>
