<subScript attach="left" box="[1393,1401,170,184]" fontSize="6" pageId="8" pageNumber="268">1</subScript>
<emphasis box="[1402,1428,154,179]" italics="true" pageId="8" pageNumber="268">–v</emphasis>
<subScript attach="left" box="[1428,1436,170,184]" fontSize="6" pageId="8" pageNumber="268">1</subScript>
48 (46–52), 
<emphasis box="[292,304,190,215]" italics="true" pageId="8" pageNumber="268">v</emphasis>
<subScript attach="right" box="[305,313,206,220]" fontSize="6" pageId="8" pageNumber="268">2</subScript>
<emphasis box="[313,339,190,215]" italics="true" pageId="8" pageNumber="268">–v</emphasis>
<subScript attach="left" box="[339,347,206,220]" fontSize="6" pageId="8" pageNumber="268">2</subScript>
78 (61–80), 
<emphasis box="[497,520,190,215]" italics="true" pageId="8" pageNumber="268">sc</emphasis>
<subScript attach="left" box="[520,528,206,220]" fontSize="6" pageId="8" pageNumber="268">2</subScript>
