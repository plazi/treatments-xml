<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Solidago" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Solidago" order="Asterales" pageId="6" pageNumber="101" phylum="Tracheophyta" rank="genus">Solidago</taxonomicName>
