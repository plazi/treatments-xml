<taxonomicName baseAuthorityName="Thomson" baseAuthorityYear="1878" class="Insecta" family="Pteromalidae" genus="Apelioma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apelioma pteromalinum" order="Coleoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" species="pteromalinum">
