<bibRefCitation author="Hassanzadeh" box="[1329,1345,1214,1239]" pageId="5" pageNumber="456" refString="Hassanzadeh Awal, M. &amp; Modarres Awal, M. (2010 b) Species of the flower bugs genus Anthocoris Fallen, 1814 from Mashhad region, North-East of Iran (Heteroptera: Anthocoridae). Munis Entomology &amp; Zoology, 5 (2), 658 - 660." type="journal article" year="2010" yearSuffix="b">b</bibRefCitation>
), cotton field, 
<taxonomicName box="[218,440,1251,1275]" class="Magnoliopsida" family="Oleaceae" genus="Jasminum" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="456" phylum="Tracheophyta" rank="species" species="fruticans">
<emphasis box="[218,440,1251,1275]" italics="true" pageId="5" pageNumber="456">Jasminum fruticans</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Hoffmanns. &amp; Link" authorityYear="1809" box="[456,562,1250,1275]" class="Magnoliopsida" family="Oleaceae" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="456" phylum="Tracheophyta" rank="family">Oleaceae</taxonomicName>
) (
<bibRefCitation author="Ghahari" box="[585,811,1250,1275]" pageId="5" pageNumber="456" refString="Ghahari, H., Cherot, F., Moulet, P., Carpintero, D. L., Linnavuori, R. E., Sakenin, H. &amp; Ostovan, H. (2011) Heteroptera (Insecta) fauna of Iranian cotton fields and surrounding grasslands. Entomologie faunistique, 64, 3 - 13." type="journal article" year="2011">
Ghahari 
<emphasis box="[686,737,1250,1275]" italics="true" pageId="5" pageNumber="456">et al</emphasis>
. 2011
</bibRefCitation>
), rape (
<bibRefCitation author="Khaghaninia" box="[896,1176,1250,1275]" pageId="5" pageNumber="456" refString="Khaghaninia, S., Farshbaf-Pourabad, R. &amp; Askari, O. (2011) A contribution to the Heteroptera fauna of Zunuz region, northwest of Iran. North - Western Journal of Zoology, 7 (1), 35 - 38." type="journal article" year="2011">
