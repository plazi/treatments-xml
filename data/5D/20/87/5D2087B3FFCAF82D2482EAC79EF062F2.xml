(
<bibRefCitation box="[1226,1416,1938,1963]" pageId="35" pageNumber="236" refId="ref40145" refString="Holmgren, A. E. (1872) Insekter fran Nordgronland, samlade af Prof. A. E. Nordenskiold ar 1870. Ofversigt af Kungliga Vetenskaps-Akademiens Forhandlingar, Stockholm, 29, 97 - 105." type="journal article">Hоlmgren, 1872</bibRefCitation>
)
</taxonomicName>
