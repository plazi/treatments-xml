<taxonomicName authority="(Stein, 1907)" authorityName="" authorityYear="1907" baseAuthorityName="Stein" baseAuthorityYear="1907" box="[364,716,1772,1797]" class="Insecta" family="Muscidae" genus="Spilogona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="236" phylum="Arthropoda" rank="species" species="impar">
