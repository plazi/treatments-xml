<paragraph blockId="41.[92,1048,132,890]" lastBlockId="42.[92,1048,132,477]" lastPageId="42" lastPageNumber="1213" pageId="41" pageNumber="1212">
<emphasis box="[132,334,610,634]" italics="true" pageId="41" pageNumber="1212">
<typeStatus box="[132,225,610,634]" pageId="41" pageNumber="1212">Allotype</typeStatus>
