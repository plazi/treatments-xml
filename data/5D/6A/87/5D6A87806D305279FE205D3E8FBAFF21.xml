. 
<quantity box="[872,1027,132,156]" metricMagnitude="3" metricUnit="m" metricValue="2.875" metricValueMax="2.95" metricValueMin="2.8" pageId="33" pageNumber="1204" unit="m" value="2875.0" valueMax="2950.0" valueMin="2800.0">2800 ±2950 m</quantity>
