<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:196298" class="Insecta" family="Proctotrupidae" genus="Exallonyx" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Exallonyx fuliginis He &amp; Fan" order="Hymenoptera" pageNumber="41" phylum="Arthropoda" rank="species" species="fuliginis">E. fuliginis He and Fan</taxonomicName>
