<emphasis bold="true" box="[700,770,1851,1875]" pageId="5" pageNumber="514">
<emphasis bold="true" box="[700,725,1851,1875]" italics="true" pageId="5" pageNumber="514">b1</emphasis>
, 
<emphasis bold="true" box="[739,764,1851,1875]" italics="true" pageId="5" pageNumber="514">b5</emphasis>
,
</emphasis>
first and fifth 
<emphasis box="[922,935,1851,1875]" italics="true" pageId="5" pageNumber="514">b</emphasis>
cells; 
<emphasis bold="true" box="[1003,1042,1851,1875]" pageId="5" pageNumber="514">
<emphasis bold="true" box="[1003,1036,1851,1875]" italics="true" pageId="5" pageNumber="514">b’1</emphasis>
,
