<typeStatus box="[312,426,323,349]" pageId="9" pageNumber="40">Holotype</typeStatus>
</emphasis>
, female, from 
<taxonomicName authority="Mast." authorityName="Mast." box="[611,869,324,350]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="9" pageNumber="40" phylum="Tracheophyta" rank="species" species="asperata">
<emphasis box="[611,792,324,349]" italics="true" pageId="9" pageNumber="40">Picea asperata</emphasis>
Mast.
</taxonomicName>
(
<taxonomicName box="[888,999,324,350]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="9" pageNumber="40" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
), Guan’e Gou, Dangchang County, 
<collectingRegion box="[363,552,364,390]" country="China" name="Gansu" pageId="9" pageNumber="40">Gansu Province</collectingRegion>
, 
<collectingCountry box="[564,637,364,390]" name="China" pageId="9" pageNumber="40">China</collectingCountry>
, 
<geoCoordinate box="[649,795,364,390]" degrees="33" direction="north" minutes="57" orientation="latitude" pageId="9" pageNumber="40" precision="15" seconds="39" value="33.960835">33°57’39”N</geoCoordinate>
, 
<geoCoordinate box="[807,965,364,390]" degrees="104" direction="east" minutes="19" orientation="longitude" pageId="9" pageNumber="40" precision="15" seconds="45" value="104.32916">104°19’45”E</geoCoordinate>
, 
<date box="[977,1206,364,390]" pageId="9" pageNumber="40" value="2005-09-11">11 September 2005</date>
, collected by Z.­W. Song and X.­F. Xue. 
<emphasis bold="true" box="[623,748,403,429]" pageId="9" pageNumber="40">
<typeStatus box="[623,748,403,429]" pageId="9" pageNumber="40">Paratypes</typeStatus>
</emphasis>
, 
<specimenCount box="[763,879,404,430]" pageId="9" pageNumber="40" type="female">4 females</specimenCount>
, with the same date as 
<typeStatus box="[1149,1255,404,430]" pageId="9" pageNumber="40">holotype</typeStatus>
