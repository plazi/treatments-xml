<paragraph blockId="17.[125,1172,1173,1294]" pageId="17" pageNumber="2114">
<emphasis box="[125,191,1173,1197]" italics="true" pageId="17" pageNumber="2114">
<collectingRegion box="[125,186,1173,1197]" country="Maldives" name="Maale" pageId="17" pageNumber="2114">Male</collectingRegion>
