<taxonomicName id="4C1C4D13FFCFFFC04B9CFC31FB73D5FB" authorityName="Marsh" authorityYear="1885" box="[994,1263,925,963]" class="Reptilia" family="Not" genus="Camptosaurus" higherTaxonomySource="CoL" kingdom="Animalia" order="Dinosauria" pageId="2" pageNumber="173" phylum="Chordata" rank="genus">Camptosaurus</taxonomicName>
