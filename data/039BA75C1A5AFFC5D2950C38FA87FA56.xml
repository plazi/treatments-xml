<taxonomicName id="4C326DC91A5AFFC5D0B90CD0FC1EFA56" authorityName="Brunner von Wattenwyl" authorityYear="1874" box="[736,909,1465,1488]" class="Insecta" family="Pyrgomorphidae" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="315" phylum="Arthropoda" rank="subFamily" subFamily="Pyrgomorphinae">Pyrgomorphinae</taxonomicName>
