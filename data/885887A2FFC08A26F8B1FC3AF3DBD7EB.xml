<taxonomicName id="C7F14D37FFC08A26F57DFA24F3B6DCCF" authorityName="Koopman" authorityYear="1989" box="[2737,3123,1413,1452]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="294" phylum="Chordata" rank="species" species="hillorum">
