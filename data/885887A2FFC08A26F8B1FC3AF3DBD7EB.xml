<taxonomicName id="C7F14D37FFC08A26F3EAFA4EF36CDF75" authorityName="Cretzschmar" authorityYear="1826" box="[3110,3305,1519,1558]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="294" phylum="Chordata" rank="species" species="clivosus">
