<taxonomicName id="4C5C4D17E12AFFBDDE9BF96EFEA413B5" class="Magnoliopsida" family="Myrtaceae" genus="Metrosideros" higherTaxonomySource="CoL" kingdom="Plantae" order="Myrtales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="collina">
<emphasis id="B928EA86E12AFFBDDE9BF96EFEA413B5" italics="true" pageId="20" pageNumber="21">Metrosideros collina</emphasis>
</taxonomicName>
(
<taxonomicName id="4C5C4D17E12AFFBDDB62F945FE1613B5" box="[361,491,1709,1735]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
), 
<emphasis id="B928EA86E12AFFBDDBF4F946FD5F13B5" box="[511,674,1710,1735]" italics="true" pageId="20" pageNumber="21">
<taxonomicName id="4C5C4D17E12AFFBDDBF4F946FD6013B5" box="[511,669,1710,1735]" class="Magnoliopsida" family="Primulaceae" genus="Myrsine" kingdom="Plantae" order="Ericales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="spec">Myrsine spec</taxonomicName>
