<taxonomicName id="4C5C4D17E12AFFBDD8BAF945FCB313B5" box="[689,846,1709,1735]" class="Magnoliopsida" family="Myrsinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ericales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Myrsinaceae</taxonomicName>
