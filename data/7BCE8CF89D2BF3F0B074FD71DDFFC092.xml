<paragraph id="1AF8BA59F7351713675ED12591E7926E" pageId="0" pageNumber="1">
<taxonomicName id="3C1D71323B20C8D43AF38EA89B0A5FD6" authorityName="Kuntze" authorityYear="1891" class="Magnoliopsida" family="Phyllanthaceae" genus="Diasperus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Diasperus anamiticus" order="Malpighiales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="anamiticus">
<emphasis id="4BAAF4CC6AF3F97F870DFCDB4DE4435D" italics="true" pageId="0" pageNumber="1">Diasperus anamiticus</emphasis>
