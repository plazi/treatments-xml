<taxonomicName id="4C054D77FFAFFFD9FFA2FD8F3858FD91" authority="Mayr and Gilliard, 1951: 14" authorityName="Mayr and Gilliard" authorityPageNumber="14" authorityYear="1951" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="20" pageNumber="20" phylum="Chordata" rank="subSpecies" species="novaeguineae" subSpecies="shaw-mayeri">
