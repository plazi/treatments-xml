<taxonomicName id="4C3ADC80EC7EFFFFFD76F8E4FCA0F89B" box="[673,818,1820,1845]" class="Malacostraca" family="Phronimidae" genus="Phronima" kingdom="Animalia" order="Amphipoda" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="stebbingii">
