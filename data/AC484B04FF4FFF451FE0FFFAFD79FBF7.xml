<quantity id="E31957F7FF4CFF451D1FFC34FD37FD27" box="[616,702,616,642]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="166" pageNumber="167" unit="mm" value="3.1">3,1mm</quantity>
