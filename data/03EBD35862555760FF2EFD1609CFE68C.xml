<collectingCountry id="F35522DE62555760FE76FA290A7DE6A8" box="[463,534,1456,1482]" name="China" pageId="1" pageNumber="290">China</collectingCountry>
, 
<collectingCountry id="F35522DE62555760FD99FA280A08E6A9" box="[544,611,1457,1483]" name="Japan" pageId="1" pageNumber="290">Japan</collectingCountry>
, 
<collectingCountry id="F35522DE62555760FDD5FA280AD8E6A9" box="[620,691,1457,1483]" name="South Korea" pageId="1" pageNumber="290">Korea</collectingCountry>
, 
<collectingCountry id="F35522DE62555760FD04FA280A9DE6A9" box="[701,758,1457,1483]" name="Laos" pageId="1" pageNumber="290">Laos</collectingCountry>
