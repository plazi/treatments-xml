<taxonomicName id="9EF9F4DCFFB3FFDFFC91FED1FC04FE35" authority="(H. Turner) Callm., Buerki, Munzinger &amp; Lowry" authorityName="Callm., Buerki, Munzinger &amp; Lowry" authorityYear="2020" baseAuthorityName="Callm., Buerki, Munzinger &amp; Lowry" baseAuthorityYear="2020" class="Magnoliopsida" family="Sapindaceae" genus="Neoarytera" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="281" phylum="Tracheophyta" rank="species" species="nekorensis" status="comb. nov.">
