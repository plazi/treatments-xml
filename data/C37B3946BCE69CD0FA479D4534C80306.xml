<paragraph id="CF93662EB4C77605EE01725A017FD1AA" pageId="39" pageNumber="40">
<taxonomicName id="39B466EFAFBDD93CB0BC9B84E47FE57E" authority="Ewing, 1917" authorityName="Ewing" authorityYear="1917" class="Arachnida" family="Phthiracaridae" genus="Tropacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="carinatus">Tropacarus carinatus, Sellnick</taxonomicName>
