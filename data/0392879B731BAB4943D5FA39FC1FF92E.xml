<taxonomicName id="4C3B4D0E731BAB4942F5F94DFC13F92E" authority="(Medler &amp; Lussenhop 1968)" authorityName="Medler &amp; Lussenhop" authorityYear="1968" baseAuthorityName="Medler &amp; Lussenhop" baseAuthorityYear="1968" box="[439,903,1692,1717]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="110" pageNumber="111" phylum="Arthropoda" rank="species" species="addenda">
<emphasis id="B94FEA9F731BAB4942F5F94DFE5AF92F" box="[439,462,1693,1716]" italics="true" pageId="110" pageNumber="111">M</emphasis>
. 
<emphasis id="B94FEA9F731BAB49429EF94CFDA9F92E" box="[476,573,1692,1717]" italics="true" pageId="110" pageNumber="111">addenda</emphasis>
(
<bibRefCitation id="EFAA4B7C731BAB49410EF94CFCEAF92E" author="Medler &amp; Lussenhop" box="[588,894,1692,1717]" firstAuthor="Medler" pageId="110" pageNumber="111" pagination="1 - 80" refId="ref94805" refString="Medler, J. T. &amp; Lussenhop, J. F. (1968) Leafcutter bees of Wisconsin. University of Wisconsin Research Bulletin, 274, 1 - 80." type="journal article" year="1968">Medler &amp; Lussenhop 1968</bibRefCitation>
