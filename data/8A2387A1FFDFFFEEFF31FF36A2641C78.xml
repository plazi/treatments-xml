to 
<taxonomicName id="C58A4D34FFDFFFEEFD27FDAFA3EA1C1C" authorityName="Lee" authorityYear="2016" box="[641,730,526,552]" class="Insecta" family="Cicadidae" genus="Changa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="530" phylum="Arthropoda" rank="genus">
