<taxonomicName id="C58A4D34FFDFFFEEFD50FD93A27D1C78" authorityName="Distant" authorityYear="1905" box="[758,845,562,588]" class="Insecta" family="Cicadidae" genus="Haphsa" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="530" phylum="Arthropoda" rank="genus">
<emphasis id="30FEEAA5FFDFFFEEFD50FD93A27D1C78" box="[758,845,562,588]" italics="true" pageId="7" pageNumber="530">Haphsa</emphasis>
