<taxonomicName id="C58A4D34FFDFFFEEFBF1FDAFA59E1C1C" authorityName="Distant" authorityYear="1905" box="[1111,1198,526,552]" class="Insecta" family="Cicadidae" genus="Haphsa" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="530" phylum="Arthropoda" rank="genus">
