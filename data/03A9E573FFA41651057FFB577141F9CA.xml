<taxonomicName id="4C002FE6FFA41651057FFB5771D7FB35" authority="Boulenger, 1901" authorityName="Boulenger" authorityYear="1901" box="[264,744,1204,1230]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="ansorgii">
