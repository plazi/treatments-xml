<collectingCountry id="BBE858F2FFA07752FE85F0A3FE9F46CA" box="[278,377,912,936]" name="Brazil" pageId="10" pageNumber="299">BRAZIL</collectingCountry>
– 
<emphasis id="F18BC470FFA07752FE0BF0BCFDFF46CA" bold="true" box="[408,537,911,936]" pageId="10" pageNumber="299">
<collectingRegion id="013BD680FFA07752FE0BF0BCFDE146C5" box="[408,519,911,935]" country="Brazil" name="Rondonia" pageId="10" pageNumber="299">Rondônia</collectingRegion>
