<taxonomicName id="2B8862CCFFDB4F37E03D0F9AB7D1FE55" box="[283,476,407,433]" class="Anthozoa" family="Caryophylliidae" kingdom="Animalia" order="Scleractinia" pageId="52" pageNumber="69" phylum="Cnidaria" rank="family">CARYOPHYLLIIDAE</taxonomicName>
