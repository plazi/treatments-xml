<subSubSection id="1DDE6500FFCDFFE2CAEDFC548A70FC67" box="[160,845,944,1005]" pageId="13" pageNumber="1261" type="etymology">
<paragraph id="557B368BFFCDFFE2CAEDFC548820FC40" blockId="13.[160,845,944,1005]" box="[160,285,944,970]" pageId="13" pageNumber="1261">
<emphasis id="67B0EA99FFCDFFE2CAEDFC548820FC40" bold="true" box="[160,285,944,970]" italics="true" pageId="13" pageNumber="1261">Etymology</emphasis>
</paragraph>
