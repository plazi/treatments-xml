<collectionCode id="86DBAE73FEFAFE9C5C04FB7EFBDB6474" box="[1050,1128,1267,1293]" country="Italy" httpUri="http://grbio.org/cool/mvk4-k796" name="Museo Zoologico La Specola, Universita di Firenze" pageId="311" pageNumber="312">MZUF</collectionCode>
and 
<specimenCount id="F6CCFD3FFEFAFE9C5CBEFB7EFA896474" box="[1184,1338,1267,1293]" pageId="311" pageNumber="312" type="generic">one specimen</specimenCount>
