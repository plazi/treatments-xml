<collectionCode id="86DBAE73FEFAFE9C5AB2FA9AFD496448" box="[684,762,1303,1329]" country="Italy" httpUri="http://grbio.org/cool/mvk4-k796" name="Museo Zoologico La Specola, Universita di Firenze" pageId="311" pageNumber="312">MZUF</collectionCode>
