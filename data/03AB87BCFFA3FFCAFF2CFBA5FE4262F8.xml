<emphasis id="B976EAB8FFA3FFC8FF2CFBA5FDE96697" bold="true" box="[151,627,1149,1175]" pageId="1" pageNumber="82">
<taxonomicName id="4C024D29FFA3FFC8FF2CFBA5FD936697" authority="Kuhlmann" authorityName="Kuhlmann" box="[151,521,1149,1175]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="82" phylum="Arthropoda" rank="species" species="comaticus" status="sp. nov.">
<emphasis id="B976EAB8FFA3FFC8FF2CFBA5FEE26697" bold="true" box="[151,376,1149,1175]" italics="true" pageId="1" pageNumber="82">Colletes comaticus</emphasis>
Kuhlmann
</taxonomicName>
, 
<taxonomicNameLabel id="A24557C3FFA3FFC8FDADFBA5FDE96697" box="[534,627,1149,1175]" pageId="1" pageNumber="82" rank="species">sp. nov.</taxonomicNameLabel>
