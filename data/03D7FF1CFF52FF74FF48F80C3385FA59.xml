<taxonomicName id="4C7E3589FF52FF74FB18FE5F34DCFAB9" authorityName="Ohaus" authorityYear="1925" box="[1168,1440,1329,1353]" class="Insecta" family="Rutelidae" genus="Ptenomela" kingdom="Animalia" order="Coleoptera" pageId="153" pageNumber="152" phylum="Arthropoda" rank="species" species="epipleurica">
