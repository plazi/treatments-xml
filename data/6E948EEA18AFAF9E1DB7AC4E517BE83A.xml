<paragraph id="E43FA9944A3C6FF75029C987667D3577" pageNumber="153">
<taxonomicName id="DB1799C7EE65E8539265EE88C3ACB6EF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34983" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName="Polyrhachis nigrita Mayr" lsidName-HNS="Polyrhachis nigrita Mayr" order="Hymenoptera" pageNumber="153" phylum="Arthropoda" rank="species" species="nigrita">P. nigrita</taxonomicName>
<taxonomicNameLabel id="21647A9E19E889E1CCD1B8156B34E24A" rank="species">nov. spec.</taxonomicNameLabel>
</paragraph>
