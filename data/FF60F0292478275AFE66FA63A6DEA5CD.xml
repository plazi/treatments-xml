<taxonomicName id="B0C93ABC24782755FF6AF9C3A6DDA107" authority="C. L. Koch" authorityName="C. L. Koch" box="[143,400,1602,1626]" class="Arachnida" family="Sarcoptidae" kingdom="Animalia" order="Astigmata" pageId="7" pageNumber="1278" phylum="Arthropoda" rank="family">Sarcoptidae C.L. Koch</taxonomicName>
