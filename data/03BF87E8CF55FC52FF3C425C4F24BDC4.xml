<taxonomicName id="4C164D7DCF57FC52FB22417C4C17BD54" authority="Hippa &amp; Kurina, 2013" authorityName="Hippa &amp; Kurina" authorityYear="2013" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="96" phylum="Arthropoda" rank="species" species="appendiculata">
