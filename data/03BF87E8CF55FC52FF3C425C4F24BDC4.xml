<taxonomicName id="4C164D7DCF57FC52FAB440E84CBCBCC0" authority="Jaschhof &amp; Hippa, 2005" authorityName="Jaschhof &amp; Hippa" authorityYear="2005" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="96" phylum="Arthropoda" rank="species" species="rectolobata">
