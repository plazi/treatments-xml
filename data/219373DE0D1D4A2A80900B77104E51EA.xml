<subSubSection id="B5EAFFC2602514534E3BA830BC350C1D" type="nomenclature">
<paragraph id="BF03805FF22876A4DE6B7F74B888B4BB" pageNumber="102">
<taxonomicName id="8E6EB76A20C9594F9BC0E1F59BDDE823" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:158882" class="Insecta" family="Formicidae" genus="Odontomachus" kingdom="Animalia" lsidName-HNS="Odontomachus haematoda var. stanleyi Wheeler" order="Hymenoptera" pageNumber="102" phylum="Arthropoda" rank="variety" species="haematoda" variety="stanleyi">Odontomachus haematoda variety stanleyi</taxonomicName>
