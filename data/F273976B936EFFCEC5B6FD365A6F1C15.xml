(
<collectingRegion id="B81EE89F936EFFCEC42DFC0659261BBC" box="[297,344,931,952]" country="Indonesia" name="Jawa Timur" pageId="6" pageNumber="1261">Java</collectingRegion>
and 
<collectingRegion id="B81EE89F936EFFCEC492FC06598B1BBD" box="[406,501,931,953]" country="Indonesia" name="Sulawesi Utara" pageId="6" pageNumber="1261">Sulawesi</collectingRegion>
), 
<collectingCountry id="02CD66ED936EFFCEC70AFC065A161BBD" box="[526,616,931,953]" name="Vietnam" pageId="6" pageNumber="1261">Vietnam</collectingCountry>
, 
<collectingCountry id="02CD66ED936EFFCEC77DFC065A9A1BBD" box="[633,740,931,953]" name="Cambodia" pageId="6" pageNumber="1261">Cambodia</collectingCountry>
, 
<collectingCountry id="02CD66ED936EFFCEC7F1FC065B571BBC" box="[757,809,931,952]" name="Laos" pageId="6" pageNumber="1261">Laos</collectingCountry>
, 
<collectingCountry id="02CD66ED936EFFCEC63DFC065BED1BBD" box="[825,915,931,953]" name="Thailand" pageId="6" pageNumber="1261">Thailand</collectingCountry>
and 
<collectingCountry id="02CD66ED936EFFCEC6D7FC065C431BBD" box="[979,1085,931,953]" name="Sri Lanka" pageId="6" pageNumber="1261">Sri Lanka</collectingCountry>
(
<bibRefCitation id="1E4B5B8C936EFFCEC5BEFC6559C61BD2" author="JAEGER B" box="[186,440,960,982]" pageId="6" pageNumber="1261" pagination="291 - 346" refId="ref36531" refString="JAEGER B. (2013): Revision of the East Palaearctic and Oriental species of the Acupalpus subgenus Stenolophidius JEANNEL, 1948 (Coleoptera, Carabidae, Harpalini, Stenolophina). - Vernate 32: 291 - 346." type="journal article" year="2013">JAEGER 2013: 301-302</bibRefCitation>
