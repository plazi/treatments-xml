<taxonomicName id="F9DB4D277C1DFF89FC53AC6A9114FECD" authority="R. Br." authorityName="R. Br." box="[970,1137,311,338]" class="Magnoliopsida" family="Apocynaceae" genus="Alstonia" kingdom="Plantae" order="Gentianales" pageId="40" pageNumber="41" phylum="Tracheophyta" rank="genus">
