<paragraph id="DE059A1E1021FFE5FF15D93E4487924E" blockId="2.[151,1437,1264,1902]" pageId="2" pageNumber="559">
<keyLead id="654B392B1021FFE5FF15D93E4487924E" pageId="2" pageNumber="559">
- 
<materialsCitation id="6ED290431021FFE5FF60D93E4487924E" ID-GBIF-Occurrence="3014908340" country="Mexico" location="Oaxaca" pageId="2" pageNumber="559" specimenCount="1" stateProvince="Oaxaca">
