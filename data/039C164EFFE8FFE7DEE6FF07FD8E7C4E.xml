<paragraph id="8B8AA758FFE8FFE4DEE6FF07FBC5783B" blockId="105.[724,1119,528,551]" box="[724,1119,528,551]" pageId="105" pageNumber="106">
<heading id="D0C21034FFE8FFE4DEE6FF07FBC5783B" box="[724,1119,528,551]" centered="true" fontSize="9" level="2" pageId="105" pageNumber="106" reason="2">
<taxonomicName id="4C35DCDBFFE8FFE4DEE6FF07FC52783A" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[724,968,528,551]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="105" pageNumber="106" phylum="Arthropoda" rank="species" species="weiae" status="new species">
<emphasis id="B9417B4AFFE8FFE4DEE6FF07FC52783A" bold="true" box="[724,968,528,551]" italics="true" pageId="105" pageNumber="106">Paramblynotus weiae</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFE8FFE4DFEAFF05FBC5783B" box="[984,1119,529,551]" pageId="105" pageNumber="106" rank="species">new species</taxonomicNameLabel>
