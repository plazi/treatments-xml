<taxonomicName id="73DA4D3AFFA2FF9B19E5F96220C957F4" authority="Fernicola 2001" authorityName="Fernicola" authorityYear="2001" box="[151,671,1701,1728]" class="Amphibia" family="Ceratophryidae" genus="Ceratophrys" kingdom="Animalia" order="Anura" pageId="8" pageNumber="45" phylum="Chordata" rank="species" species="ameghinorum">
