<quantity id="4CC843E0404EFFC8E1F707B734C4FA75" box="[946,1006,1468,1492]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="21" pageNumber="172" unit="m" value="10.0">10 m</quantity>
