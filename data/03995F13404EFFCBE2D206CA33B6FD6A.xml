<date id="FF8EC8C5404EFFC8E3DF07B03531FA72" box="[410,539,1467,1492]" pageId="21" pageNumber="172" value="2006-09-26">
<number id="3DF8FC22404EFFC8E3DF07B036F6FA72" box="[410,476,1467,1492]" pageId="21" pageNumber="172" value="26.09">26.09</number>
. 
<quantity id="4CC843E0404EFFC8E3A507B03531FA72" box="[480,539,1467,1492]" metricMagnitude="6" metricUnit="m" metricValue="2.0061" pageId="21" pageNumber="172" unit="m" value="2006100.0">2006</quantity>
