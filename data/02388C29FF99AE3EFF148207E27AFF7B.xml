<collectingCountry id="F2867DAFFF98AE3EFB5785A2E541FF33" box="[1236,1318,151,177]" name="France" pageId="49" pageNumber="50">France</collectingCountry>
: 
<collectingRegion id="4855F3DDFF98AE3EFAAD85A2E5FBFF33" box="[1326,1436,151,177]" country="France" name="Aquitaine" pageId="49" pageNumber="50">Aquitaine</collectingRegion>
