<quantity id="D5489B42F20F2438D0085813B15EFA87" box="[803,892,1301,1325]" metricMagnitude="-3" metricUnit="m" metricValue="9.7" pageId="2" pageNumber="261" unit="mm" value="9.7">9.7 mm</quantity>
