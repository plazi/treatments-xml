<taxonomicName id="AAD88588B75BA0699ADFFA29FDE14DAE" authority="Bagnall, 1916: 401" authorityName="Bagnall" authorityPageNumber="401" authorityYear="1916" box="[151,632,1505,1530]" class="Insecta" family="Thripidae" genus="Dendrothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="246" phylum="Arthropoda" rank="species" species="sexmaculatus">
<emphasis id="5FAC2219B75BA0699ADFFA29FE344DAE" box="[151,429,1506,1530]" italics="true" pageId="15" pageNumber="246">Dendrothrips sexmaculatus</emphasis>
<treatmentCitation id="EC79D81AB75BA0699BFCFA2AFDE14DAE" author="Bagnall, R. S." box="[436,632,1505,1530]" page="401" pageId="15" pageNumber="246" year="1916">
