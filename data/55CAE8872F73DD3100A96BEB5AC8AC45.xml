<paragraph id="E9CF71AB61CBCE4A76F21C1CF3F33B5C" pageId="0" pageNumber="9042">
species of 
<taxonomicName id="61F9746B1D50DDFCE42EE92CD14B41A3" class="Insecta" family="Ichneumonidae" genus="Thaumatogelis" kingdom="Animalia" lsidName="Thaumatogelis" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Thaumatogelis</taxonomicName>
