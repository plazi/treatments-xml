<taxonomicName id="DB79AA20CDDCD4570D90529B83C0BDBD" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis burmanica" order="Hymenoptera" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="burmanica">Chrysis burmanica</taxonomicName>
