<taxonomicName id="4C434D27FF9BFFCEFF26FB78FEB59F4F" box="[151,286,1262,1287]" class="Entognatha" family="Cyphoderidae" genus="Cyphoderus" kingdom="Animalia" order="Collembola" pageId="37" pageNumber="338" phylum="Arthropoda" rank="genus">
