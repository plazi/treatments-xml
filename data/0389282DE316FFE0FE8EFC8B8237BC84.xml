<taxonomicName id="4C20E2B8E315FFE1FD0FFB158427B82D" authority="Xu &amp; Olmi &amp; He, 2006" authorityName="Xu &amp; Olmi &amp; He" authorityYear="2006" box="[649,826,1264,1287]" class="Insecta" family="Dryinidae" genus="Anteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="species" species="amabilis" status="new species">
<emphasis id="B9544529E315FFE1FD0FFB158427B82D" box="[649,826,1264,1287]" italics="true" pageId="2" pageNumber="59">Anteon amabilis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A267F852E315FFE1FCCCFB1584D0B82D" box="[842,973,1264,1287]" pageId="2" pageNumber="59" rank="species">new species</taxonomicNameLabel>
