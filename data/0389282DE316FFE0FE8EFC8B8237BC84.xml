<taxonomicName id="4C20E2B8E315FFE1FE8EFAF286C8B804" authority="Olmi" authorityName="Olmi. Additions" authorityYear="1989" box="[264,469,1303,1326]" class="Insecta" family="Dryinidae" genus="Anteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="species" species="fidum">
