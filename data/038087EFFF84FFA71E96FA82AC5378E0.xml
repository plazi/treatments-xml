<taxonomicName id="4C294D7AFF85FFA71AA5FD57AC5C78E3" box="[1188,1380,712,737]" class="Insecta" family="Gelechiidae" genus="Megacraspedus" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="multispinella">
