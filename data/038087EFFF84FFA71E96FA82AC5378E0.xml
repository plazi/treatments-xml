<taxonomicName id="4C294D7AFF85FFA71FF9FD57ABFC78E3" box="[504,708,712,737]" class="Insecta" family="Gelechiidae" genus="Megacraspedus" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="orenburgensis">
