<geoCoordinate id="32F03325094E7460768DD48C60660E7C" degrees="37.8065" direction="north" orientation="latitude" precision="5" value="37.8065">37.8065°N</geoCoordinate>
, -
<geoCoordinate id="BBF45B94171E5963762BB80E12010282" degrees="79.6152" direction="west" orientation="longitude" precision="5" value="-79.6152">79.6152°W</geoCoordinate>
