<taxonomicName id="4C208FBCFFC0FF9BFD65FCC2FC469C0B" box="[719,924,804,829]" class="Aves" family="Corvidae" genus="Podoces" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="22" phylum="Chordata" rank="species" species="panderi">
