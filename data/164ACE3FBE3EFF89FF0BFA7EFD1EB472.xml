</emphasis>
<taxonomicName id="59E304AABE3EFF8AFEB7FA31FDADB34F" authority="Walker &amp; Scott, 1903" authorityName="Walker &amp; Scott" authorityYear="1903" box="[299,618,1514,1539]" class="Malacostraca" family="Kuriidae" genus="Kuria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
