<taxonomicName id="59E304AABE3DFF89FD7DFED9FBADB451" authority="Krapp-Schickel, 1976" authorityName="Krapp-Schickel" authorityYear="1976" box="[737,1130,260,284]" class="Malacostraca" family="Hyalidae" genus="Micropythia" kingdom="Animalia" order="Amphipoda" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
<emphasis id="AC97A33BBE3DFF89FD7DFED9FCACB451" box="[737,875,260,284]" italics="true" pageId="26" pageNumber="27">Micropythia</emphasis>
Krapp-Schickel, 
<number id="282B6D0EBE3DFF89FBB3FED9FBADB451" box="[1071,1130,260,284]" pageId="26" pageNumber="27" value="1976.0">1976</number>
