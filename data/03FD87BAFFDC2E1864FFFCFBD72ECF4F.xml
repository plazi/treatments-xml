<taxonomicName id="4C544D2FFFDF2E1864FFFBA5D36BCE02" authorityName="Stebbing" authorityYear="1912" box="[151,292,1052,1077]" class="Malacostraca" family="Lampropidae" genus="Platysympus" kingdom="Animalia" order="Cumacea" pageId="51" pageNumber="52" phylum="Arthropoda" rank="genus">
