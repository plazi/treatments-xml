<taxonomicName id="4C544D2FFFDF2E186533FBA5D3BACE02" authorityName="Bacescu" authorityYear="1972" box="[347,501,1052,1077]" class="Malacostraca" family="Lampropidae" genus="Archaeocuma" kingdom="Animalia" order="Cumacea" pageId="51" pageNumber="52" phylum="Arthropoda" rank="genus">
<emphasis id="B920EABEFFDF2E186533FBA5D3BACE02" box="[347,501,1052,1077]" italics="true" pageId="51" pageNumber="52">Archaeocuma</emphasis>
