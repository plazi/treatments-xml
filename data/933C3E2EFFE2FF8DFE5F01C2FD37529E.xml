<emphasis id="29E1532AFFE2FF8DFE34035EFE3353CB" bold="true" box="[482,488,628,652]" italics="true" pageId="7" pageNumber="1738">′</emphasis>
S
</geoCoordinate>
, 
<geoCoordinate id="7EA1E9FFFFE2FF8DFDC2035EFD5B53CB" box="[532,640,628,652]" degrees="70" direction="west" minutes="59" orientation="longitude" pageId="7" pageNumber="1738" precision="925" value="-70.98333">
70°59 
<emphasis id="29E1532AFFE2FF8DFD82035EFD8153CB" bold="true" box="[596,602,628,652]" italics="true" pageId="7" pageNumber="1738">′</emphasis>
W
</geoCoordinate>
) and in the Estero Consuelo (
<geoCoordinate id="7EA1E9FFFFE2FF8DFC3E035EFB9153CB" box="[1000,1098,628,652]" degrees="31" direction="south" minutes="46" orientation="latitude" pageId="7" pageNumber="1738" precision="925" value="-31.766666">
