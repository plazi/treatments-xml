<collectingRegion id="498BF85BFFF3D751FD65F96EA5859DE5" box="[759,924,1765,1790]" country="Brazil" name="Santa Catarina" pageId="9" pageNumber="444">Santa Catarina</collectingRegion>
<collectingCountry id="F3587629FFF3D751FC30F96DA5F09DE5" box="[930,1001,1765,1790]" name="Iceland" pageId="9" pageNumber="444">Island</collectingCountry>
, Florianópolis, 
<collectingRegion id="498BF85BFFF3D751FB0AF96EA3269DE5" box="[1176,1343,1765,1790]" country="Brazil" name="Santa Catarina" pageId="9" pageNumber="444">Santa Catarina</collectingRegion>
