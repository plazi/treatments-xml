<collectionCode id="ED67D3E3F7629B6EFF4BB1E3FEB8E9EC" box="[200,291,386,410]" country="Switzerland" httpUri="http://grbio.org/cool/js1h-4156" name="Naturhistorisches Museum, Basel" pageId="10" pageNumber="9">NMBA</collectionCode>
