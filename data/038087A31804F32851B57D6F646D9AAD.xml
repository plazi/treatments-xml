<paragraph id="8B9636B51804F32851E57D82646D9AAD" blockId="19.[151,1436,1126,1223]" pageId="19" pageNumber="400">
<taxonomicName id="4C294D361804F32851E57D82603C9AC8" box="[199,367,1163,1187]" class="Malacostraca" family="Galatheidae" genus="Munida" kingdom="Animalia" order="Decapoda" pageId="19" pageNumber="400" phylum="Arthropoda" rank="species" species="striata">
