<collectingCountry id="F31D760DFB79FFC1FE80F4E617E6F8B1" box="[359,426,1770,1795]" name="China" pageId="6" pageNumber="596">China</collectingCountry>
(
<collectingRegion id="49CEF87FFB79FFC1FE5EF4E6145CF8B1" box="[441,528,1770,1795]" country="China" name="Hainan" pageId="6" pageNumber="596">Hainan</collectingRegion>
