syn. 
<taxonomicName id="4C524D441005FFA042150C1086BF8005" authority="HOLMGREN 1864" authorityName="HOLMGREN" authorityYear="1864" box="[289,760,845,870]" class="Insecta" family="Ichneumonidae" genus="Ichneumon" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="1498" phylum="Arthropoda" rank="species" species="anthracinus">
<emphasis id="B926EAD51005FFA042150C10866E8005" box="[289,553,845,870]" italics="true" pageId="21" pageNumber="1498">Ichneumon anthracinus</emphasis>
<bibRefCitation id="EFC34B361005FFA041040C1386BF8005" author="HOLMGREN A" box="[560,760,845,870]" pageId="21" pageNumber="1498" pagination="1 - 214" refId="ref76574" refString="HOLMGREN A. E. (1864): Ichneumonologia Suecica. Tomus primus. Ichneumonides oxypygi. - Holm, 1 - 214." type="book chapter" year="1864">HOLMGREN 1864</bibRefCitation>
</taxonomicName>
