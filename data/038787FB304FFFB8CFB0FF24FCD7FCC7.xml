<taxonomicName id="4C2E4D6E304AFFB9CDDDF988FC66F959" authority="Jiang &amp; Wang, 2021" authorityName="Jiang &amp; Wang" authorityYear="2021" box="[762,965,1651,1675]" class="Insecta" family="Elmidae" genus="Zaitzevia" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="595" phylum="Arthropoda" rank="species" species="tangliangi" status="sp. nov.">
