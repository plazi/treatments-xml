<taxonomicName id="4C2E4D6E304DFFBECED8F874FD69F874" authority="Jiang &amp; Wang, 2021" authorityName="Jiang &amp; Wang" authorityYear="2021" box="[511,714,1934,1958]" class="Insecta" family="Elmidae" genus="Zaitzevia" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="594" phylum="Arthropoda" rank="species" species="tangliangi" status="sp. nov.">
