<geoCoordinate id="EE022B9C5958FFC7C0D1C0317E2F703E" box="[896,1058,799,826]" degrees="38.270694" direction="north" orientation="latitude" pageId="3" pageNumber="22" precision="1" value="38.270695">38.270694° N</geoCoordinate>
