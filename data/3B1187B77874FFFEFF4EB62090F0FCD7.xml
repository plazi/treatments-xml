. OOL = 2.0 
<collectionCode id="D5A9AE647877FFFFFACEB3DD954AF939" LSID="urn:lsid:biocol.org:col:14382" box="[1303,1372,1690,1714]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="10" pageNumber="158">MOD</collectionCode>
