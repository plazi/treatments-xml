<quantity id="4CEF9B4FFFDAFFF3A365FA26FC2EF989" box="[812,898,1529,1553]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="2" pageNumber="107" unit="mm" value="4.5">4,5 mm</quantity>
