<caption id="C5B05094172F4C7FFF06A2AAFD6F32F6" ID-Table-UUID="C5B05094172F4C7FFF06A2AAFD6F32F6" box="[151,767,1204,1229]" httpUri="http://table.plazi.org/id/C5B05094172F4C7FFF06A2AAFD6F32F6" pageId="12" pageNumber="223" targetBox="[166,1418,1245,2029]" targetIsTable="true" targetPageId="12">
<paragraph id="9170001C172F4C7FFF06A2AAFD6F32F6" blockId="12.[151,767,1204,1229]" box="[151,767,1204,1229]" pageId="12" pageNumber="223">
<emphasis id="A3BBDC0E172F4C7FFF06A2AAFE9A32F7" bold="true" box="[151,266,1204,1228]" pageId="12" pageNumber="223">TABLE 2.</emphasis>
