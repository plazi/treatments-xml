, 
<taxonomicName id="05F8B3B01BECC05846E0A181EC801CA5" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="47" pageNumber="48" phylum="Mollusca" rank="species" species="dentritis">
