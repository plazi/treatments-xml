<subSubSection id="E68C18DDFF87A70F34049D7F9E1AFE73" lastPageId="7" lastPageNumber="557" pageId="3" pageNumber="553" type="diagnosis">
<paragraph id="AE294B56FF87A70F34049D7F9FAAFFCD" blockId="3.[491,1552,1492,2031]" lastBlockId="7.[491,1538,229,392]" lastPageId="7" lastPageNumber="557" pageId="3" pageNumber="553">
<emphasis id="9CE29744FF87A70B34049D7F98F5F8B8" bold="true" box="[531,648,1904,1928]" pageId="3" pageNumber="553">Diagnosis</emphasis>
— Length of idiosoma: 936– 
<specimenCount id="B89080DFFF87A70B35DB9D609E43F8B7" box="[972,1086,1903,1927]" pageId="3" pageNumber="553" type="female">940 female</specimenCount>
