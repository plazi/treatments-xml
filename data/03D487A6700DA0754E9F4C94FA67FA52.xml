<taxonomicName id="4C7D4D33700BA0754F0E49C1FE35FA70" box="[262,458,1473,1495]" class="Insecta" family="Cicadellidae" genus="Doraturopsis" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="28" phylum="Arthropoda" rank="species" species="heros">
