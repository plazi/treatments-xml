<collectingDate id="EFC0E9B8FFE6FFD7D30F1F7AFC13FAAB" box="[885,1012,1317,1342]" pageId="1" pageNumber="410" value="2015-04-29">
<date id="FF841050FFE6FFD7D30F1F7AFC13FAAB" box="[885,1012,1317,1342]" pageId="1" pageNumber="410" value="2015-04-29">29.IV.2015</date>
</collectingDate>
, 
<collectorName id="26CF5346FFE6FFD7D3851F7AFB40FAAB" box="[1023,1191,1317,1342]" pageId="1" pageNumber="410">Haruki Karube</collectorName>
