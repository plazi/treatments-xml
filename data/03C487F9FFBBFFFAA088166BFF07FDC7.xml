<quantity id="4C959B0AFFBCFFFBA26511DFFD16FF44" box="[634,708,190,216]" metricMagnitude="2" metricUnit="m" metricValue="1.04" pageId="4" pageNumber="47" unit="m" value="104.0">104 m</quantity>
