<taxonomicName id="70F4CB83FFBFFFE9FDB3F837FD14F82F" authorityName="Richmond" authorityYear="1908" box="[611,741,2013,2038]" class="Aves" family="Prunellidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="31" pageNumber="482" phylum="Chordata" rank="family">Prunellidae</taxonomicName>
