<taxonomicName id="70F4CB83FFBFFFE9FF47F837FF0EF82F" authorityName="Bonaparte" authorityYear="1853" box="[151,255,2013,2038]" class="Aves" family="Mimidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="31" pageNumber="482" phylum="Chordata" rank="family">Mimidae</taxonomicName>
