<heading id="D0938185FFD24D663835FE96FD428D3F" bold="true" box="[264,691,283,309]" fontSize="11" level="1" pageId="7" pageNumber="8" reason="1">
<emphasis id="B910EAFBFFD24D663835FE96FD428D3F" bold="true" box="[264,691,283,309]" pageId="7" pageNumber="8">
<taxonomicName id="4C644D6AFFD24D663835FE96FDB68D3F" authority="Thompson" authorityName="Thompson" box="[264,583,283,309]" class="Insecta" family="Syrphidae" genus="Austalis" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="rhina" status="sp. nov.">Austalis rhina Thompson</taxonomicName>
, 
<taxonomicNameLabel id="A2235780FFD24D663B69FE96FD428D3F" box="[596,691,283,309]" pageId="7" pageNumber="8" rank="species">sp. nov.</taxonomicNameLabel>
