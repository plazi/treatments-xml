Schenkling, 1899a: 136 (
<taxonomicName id="734A06C5719BDEDC14862BFDF0FDC2DA" authority="Gorham, 1876" authorityName="Gorham" authorityYear="1876" class="Insecta" family="Cleridae" genus="Callimerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Callimerus" order="Coleoptera" pageId="18" pageNumber="27" phylum="Arthropoda" rank="genus">Callimerus</taxonomicName>
; type locality: &quot;Neu Guinea (Fly River)&quot;); - 
<bibRefCitation id="4430ECF8AE1C75E80C713BB634F79254" author="Chapin, EA" journalOrPublisher="The Philippine Journal of Science" pageId="23" pageNumber="32" pagination="159 - 286" title="Classification of the Philippine components of the Coleopterous family Cleridae." volume="25" year="1924">Chapin 1924</bibRefCitation>
: 190 (&quot; 
<taxonomicName id="A021D63FA7CF917BBA63F164E2C6A818" authority="Chapin, 1924" authorityName="Chapin" authorityYear="1924" class="Insecta" family="Cleridae" genus="Brachycallimerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Brachycallimerus" order="Coleoptera" pageId="18" pageNumber="27" phylum="Arthropoda" rank="genus">Brachycallimerus</taxonomicName>
