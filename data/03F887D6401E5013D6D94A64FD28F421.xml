<taxonomicName id="4C514D43401E5013D5354DCCFADCF5C5" authority="(DeLong, 1944)" baseAuthorityName="DeLong" baseAuthorityYear="1944" box="[891,1339,1231,1257]" class="Insecta" family="Cicadellidae" genus="Scaphytopius" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="36" phylum="Arthropoda" rank="species" species="abruptus">
