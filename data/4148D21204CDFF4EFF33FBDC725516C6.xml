<geoCoordinate id="ACD505C304CDFF4EFC3AF9427217169E" box="[926,1029,1679,1705]" degrees="29" direction="west" minutes="16" orientation="longitude" pageId="157" pageNumber="158" precision="925" value="-29.266666">29°16’W</geoCoordinate>
, 
<quantity id="0E19CEE104CDFF4EFBB4F94372AF169D" box="[1040,1213,1678,1706]" metricMagnitude="0" metricUnit="m" metricValue="3.0275" metricValueMax="3.05" metricValueMin="3.005" pageId="157" pageNumber="158" unit="m" value="3.0275" valueMax="3.05" valueMin="3.005">
<geoCoordinate id="ACD505C304CDFF4EFBB4F943725C169E" box="[1040,1102,1678,1705]" degrees="3,005" direction="north" orientation="latitude" pageId="157" pageNumber="158" precision="55" value="3.005">3,005</geoCoordinate>
<geoCoordinate id="ACD505C304CDFF4EFBEAF943728E169E" box="[1102,1180,1678,1705]" degrees="3,050" direction="west" orientation="longitude" pageId="157" pageNumber="158" precision="55" value="-3.05">–3,050</geoCoordinate>
