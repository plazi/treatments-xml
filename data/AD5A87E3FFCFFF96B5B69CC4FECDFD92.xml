<collectionCode id="43E2AE30FFCFFF90B6A59E9EFC7BFC11" box="[900,997,968,994]" country="Austria" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="2" pageNumber="588" type="Museum">NHMW</collectionCode>
