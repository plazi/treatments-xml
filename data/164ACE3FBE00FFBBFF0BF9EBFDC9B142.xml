</emphasis>
<taxonomicName id="59E304AABE00FFB4FEB7F9A3FD97B3D8" authority="Boeck, 1871" authorityName="Boeck" authorityYear="1871" box="[299,592,1661,1685]" class="Malacostraca" family="Eusiridae" genus="Pontogeneia" kingdom="Animalia" order="Amphipoda" pageId="39" pageNumber="40" phylum="Arthropoda" rank="genus">
