<taxonomicName id="C79B8EE3FF8EFFB0648AFC48292F4297" authorityName="Regan, 1913:" authorityYear="1003" box="[409,599,911,934]" class="Actinopterygii" family="Poeciliidae" genus="Pamphorichthys" kingdom="Animalia" order="Cyprinodontiformes" pageId="43" pageNumber="44" phylum="Chordata" rank="genus">
