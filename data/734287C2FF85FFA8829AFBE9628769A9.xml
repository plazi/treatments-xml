<taxonomicName id="3CEB4D57FF85FFA88074F9B363466911" authority="Gaedike, 2018" authorityName="Gaedike" authorityYear="2018" box="[633,774,1572,1597]" class="Insecta" family="Epermeniidae" genus="Gnathifera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="robinsoni" status="sp. nov.">
