<taxonomicName id="3CEB4D57FF85FFA8833EF9B3619A6910" authority="Gaedike, 2018" authorityName="Gaedike" authorityYear="2018" box="[307,474,1572,1596]" class="Insecta" family="Epermeniidae" genus="Gnathifera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="fuscostriata" status="sp. nov.">G. fuscostriata</taxonomicName>
