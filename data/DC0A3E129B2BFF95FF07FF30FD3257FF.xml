<subSubSection id="1CB9DC8F9B2BFF95FF07FEC9FDE55338" box="[151,638,291,314]" pageId="2" pageNumber="59" type="materials_examined">
<paragraph id="541C8F049B2BFF95FF07FEC9FDE55338" blockId="2.[151,638,291,314]" box="[151,638,291,314]" pageId="2" pageNumber="59">
<emphasis id="66D753169B2BFF95FF07FEC9FE80533B" bold="true" box="[151,283,291,314]" pageId="2" pageNumber="59">
<typeStatus id="8B1831A69B2BFF95FF07FEC9FF515338" box="[151,202,292,313]" pageId="2" pageNumber="59">Type</typeStatus>
