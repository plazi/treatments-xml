<taxonomicName id="13914D5BFFC0BB562FF32DC90BAEFE5F" authority="(Ledoyer, 1973)" baseAuthorityName="Ledoyer" baseAuthorityYear="1973" box="[193,676,438,465]" class="Malacostraca" family="Aoridae" genus="Autonoe" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="389" phylum="Arthropoda" rank="species" species="rubromaculatus">
