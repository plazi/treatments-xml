<taxonomicName id="4C37D68BFFAEFF9BFF62B7F6FD183484" authority="(Lundblad, 1935)" baseAuthorityName="Lundblad" baseAuthorityYear="1935" box="[151,695,1698,1725]" class="Arachnida" family="Unionicolidae" genus="Unionicola" kingdom="Animalia" order="Trombidiformes" pageId="15" pageNumber="116" phylum="Arthropoda" rank="species" species="tenuis" subGenus="Hexatax">
