<paragraph id="8B93285CFF92FFCC9BFDF930FCBBBD42" blockId="44.[151,1436,1697,1794]" pageId="44" pageNumber="45">
<materialsCitation id="3B442201FF92FFCC9BFDF930FCBFBD42" collectingDate="Apr and Jun 1994, Dec 1994" collectingDateMax="Jan 1995 and Jun 1997" collectingDateMin="Apr and Jun 1994, Dec 1994" collectionCode="TCEC" country="Uganda" county="This" location="Minziro forest" municipality="Mpanda" pageId="44" pageNumber="45" specimenCount="1" stateProvince="Kigoma">
<collectingCounty id="62F250D0FF92FFCC9BFDF930FF1CBCFA" box="[151,201,1697,1722]" pageId="44" pageNumber="45">This</collectingCounty>
species is restricted to 
<collectingCountry id="F33B68CCFF92FFCC9A8BF930FDEDBCFA" box="[481,568,1697,1722]" name="Uganda" pageId="44" pageNumber="45">Uganda</collectingCountry>
and 
<collectingCountry id="F33B68CCFF92FFCC991DF930FD0BBCFA" box="[631,734,1697,1722]" name="Tanzania" pageId="44" pageNumber="45">Tanzania</collectingCountry>
, where it is rare in the forests of 
<collectingMunicipality id="6BF7B226FF92FFCC9F18F930FB1BBCFA" box="[1138,1230,1697,1722]" pageId="44" pageNumber="45">Mpanda</collectingMunicipality>
and 
<collectingRegion id="49E8E6BEFF92FFCC9E66F930FABCBCFA" box="[1292,1385,1697,1722]" country="Tanzania" name="Kigoma" pageId="44" pageNumber="45">Kigoma</collectingRegion>
