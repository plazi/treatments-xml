<taxonomicName id="C51599E12F10D2E45C9C105DE227B222" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia stricta" order="Zingiberales" pageId="189" pageNumber="190" phylum="Tracheophyta" rank="species" species="stricta">Heliconia stricta</taxonomicName>
