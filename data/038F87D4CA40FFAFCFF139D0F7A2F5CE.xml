<caption id="DF59664ACA40FFAFCD9D3B4DF661F840" ID-DOI="http://doi.org/10.5281/zenodo.5714181" ID-Zenodo-Dep="5714181" httpUri="https://zenodo.org/record/5714181/files/figure.png" inLine="true" pageId="16" pageNumber="635" targetBox="[1425,2014,1635,2048]" targetPageId="16">
