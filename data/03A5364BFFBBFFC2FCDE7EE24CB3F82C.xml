<taxonomicName id="4C0CFCDEFFBEFFC2FEBC7F454CFEF80E" box="[336,434,1915,1935]" class="Insecta" family="Formicidae" genus="Strumigenys" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="daithma">
