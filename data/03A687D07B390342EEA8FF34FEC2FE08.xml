<taxonomicName id="4C0F4D457B3C0342ECE2FE38FC9EFE51" box="[733,856,404,428]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecaceae" pageId="68" pageNumber="69" phylum="Tracheophyta" rank="order">Arecaceae</taxonomicName>
