<taxonomicName id="7F624BBC922C14FFD92159F8E9686944" LSID="3E4FF6AE-5008-563B-8F16-82377F8FC0BC" authority="(Tschitscherine, 1897)" baseAuthorityName="Tschitscherine" baseAuthorityYear="1897" class="Insecta" family="Carabidae" genus="Chydaeus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Chydaeus bedeli" order="Coleoptera" pageId="31" pageNumber="70" phylum="Arthropoda" rank="species" species="bedeli">
