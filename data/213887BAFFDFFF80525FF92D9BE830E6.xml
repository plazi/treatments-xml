<collectionCode id="CF80AE69FFD8FF80506CFE1F9A8832EE" box="[827,904,444,470]" pageId="12" pageNumber="13">AWIV</collectionCode>
, 
<number id="1F59248BFFD8FF8050C0FE1F9D1032EE" box="[919,1040,444,470]" pageId="12" pageNumber="13" value="0.5700000000000001" valueMax="0.58" valueMin="0.56">0.56–0.58</number>
; ANW, 
<number id="1F59248BFFD8FF805721FE1F9DEE32EE" box="[1142,1262,444,470]" pageId="12" pageNumber="13" value="0.20500000000000002" valueMax="0.21" valueMin="0.2">0.20–0.21</number>
