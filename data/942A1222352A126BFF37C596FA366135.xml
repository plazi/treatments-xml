<collectingCountry id="6494E3A4352A126BFCACC343FCE16266" box="[780,844,1357,1380]" name="Nepal" pageId="0" pageNumber="61">Nepal</collectingCountry>
(nr Birganj); 
<collectingCountry id="6494E3A4352A126BFC7FC343FBEC6266" box="[991,1089,1357,1380]" name="Malaysia" pageId="0" pageNumber="61">Malaysia</collectingCountry>
