<taxonomicName id="13AD7F9DFFA79E714C61F963BDE2A082" box="[336,501,1735,1757]" class="Insecta" family="Eucharitidae" genus="Pseudochalcura" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="244" phylum="Arthropoda" rank="genus">
<emphasis id="E6D9D80CFFA79E714C61F963BDE2A082" box="[336,501,1735,1757]" italics="true" pageId="5" pageNumber="244">Pseudochalcura</emphasis>
</taxonomicName>
spp. 9–11, 
<taxonomicName id="13AD7F9DFFA79E714F5FF963BF4FA081" box="[622,856,1735,1758]" class="Insecta" family="Eucharitidae" genus="Pseudochalcura" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="244" phylum="Arthropoda" rank="species" species="pauca">
