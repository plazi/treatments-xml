<paragraph id="0025A777FF83E2F483905EF0B3BB502A" pageId="783" pageNumber="784">
Genus 
<taxonomicName id="9FBDD290E409862076C834AF4556466A" LSID="51DA660F-5841-8AB8-2AE6-797D0403B74F" authority="Laporte, 1834" authorityName="Laporte" authorityYear="1834" class="Hexapoda" family="Carabidae" genus="Calybe" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Calybe" order="Coleoptera" pageId="783" pageNumber="784" phylum="Arthropoda" rank="genus">Calybe Laporte, 1834</taxonomicName>
