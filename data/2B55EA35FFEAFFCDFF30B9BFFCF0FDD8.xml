<treatmentCitationGroup id="83EC7C0DFFEAFFC2FF30B9DCFE84F9D8" pageId="7" pageNumber="6">
<treatmentCitation id="225D7D32FFEAFFC2FF30B9DCFD18FA38" author="Peringuey, L." box="[193,753,1506,1530]" page=")" pageId="7" pageNumber="6" year="1898">
<taxonomicName id="64FC20A0FFEAFFC2FF30B9DCFD18FA38" authority="Peringuey (1898: 340)" authorityName="Peringuey" authorityPageNumber="340" authorityYear="1898" box="[193,753,1506,1530]" class="Insecta" family="Carabidae" genus="Graphipterus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="distinctus">
<emphasis id="91888731FFEAFFC2FF30B9DCFE0AFA38" box="[193,483,1506,1530]" italics="true" pageId="7" pageNumber="6">Graphipterus distinctus</emphasis>
<bibRefCitation id="C76D26D2FFEAFFC2FE19B9DCFD18FA38" author="Peringuey, L." box="[488,753,1506,1530]" pageId="7" pageNumber="6" pagination="315 - 378" refId="ref8077" refString="Peringuey, L. 1898. Descriptive catalogue of the Coleoptera of South Africa, Family Carabidae, First supplement. Transactions of the South African Philosophical Society 10: 315 - 378." type="journal article" year="1898">Péringuey (1898: 340)</bibRefCitation>
