<taxonomicName id="35CF9B8FFFB2A974FC822BD1FC58F88B" authorityName="Waterhouse" authorityYear="1878" box="[781,897,1788,1813]" class="Insecta" family="Lycidae" genus="Lyropaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="439" phylum="Arthropoda" rank="genus" sensu="stricto">
