<taxonomicName id="93DE4D7B252FFF92FF0BE959254AFBB1" authorityName="Dumortier" authorityYear="1829" box="[238,353,1077,1103]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="50" pageNumber="51" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
), Wuhan City, 
<collectingRegion id="961AF81A252FFF92FDE8E95926EDFBB1" box="[525,710,1077,1103]" country="China" name="Hubei" pageId="50" pageNumber="51">Hubei Province</collectingRegion>
, P. 
<collectionCode id="32CFAE3D252FFF92FD0DE95926D5FBB1" box="[744,766,1077,1103]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="50" pageNumber="51">R</collectionCode>
. 
<collectingCountry id="2CC97668252FFF92FCEDE959277AFBB1" box="[776,849,1077,1103]" name="China" pageId="50" pageNumber="51">China</collectingCountry>
, 
<geoCoordinate id="31EA503F252FFF92FCB8E95927DBFBB1" box="[861,1008,1077,1103]" degrees="30" direction="north" minutes="58" orientation="latitude" pageId="50" pageNumber="51" precision="15" seconds="07" value="30.96861">30°58'07&quot; N</geoCoordinate>
, 
<geoCoordinate id="31EA503F252FFF92FC19E95920B2FBB1" box="[1020,1177,1077,1103]" degrees="114" direction="east" minutes="41" orientation="longitude" pageId="50" pageNumber="51" precision="15" seconds="20" value="114.68889">114°41'20&quot; E</geoCoordinate>
, 
<date id="20601038252FFF92FB40E9592176FBB1" box="[1189,1373,1077,1103]" pageId="50" pageNumber="51" value="2006-10-05">5 October 2006</date>
, coll. Zi-Wei Song. 
<emphasis id="66AAEAEA252FFF92FEA1E93225EAFB86" bold="true" box="[324,449,1118,1144]" pageId="50" pageNumber="51">
<typeStatus id="8B65885A252FFF92FEA1E93225EAFB86" box="[324,449,1118,1144]" pageId="50" pageNumber="51">Paratypes</typeStatus>
