<quantity id="4CEF9B25FFF6FFD29B7DFEF1F904311B" box="[768,837,259,286]" metricMagnitude="2" metricUnit="m" metricValue="1.319784" pageId="103" pageNumber="104" unit="ft" value="433.0">433 ft</quantity>
