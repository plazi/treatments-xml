<taxonomicName id="4C1234DC0B048779FE901B9DB46AFE79" box="[358,576,1003,1024]" class="Insecta" family="Hybosoridae" genus="Aegidinus" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="33" phylum="Arthropoda" rank="species" species="oreibates">
