(
<figureCitation id="133B2A7EFFB2FFF1BDBFEA405A22FBC8" box="[158,324,1042,1067]" captionStart="FIGURES 74 – 82. 74 – 76" captionStartId="31.[151,264,1952,1975]" captionTargetBox="[171,1415,496,1916]" captionTargetId="figure@31.[151,1435,477,1933]" captionTargetPageId="31" captionText="FIGURES 74 – 82. 74 – 76: S. maolanensis sp. nov. (74) dorsal habitus; (75) lateral habitus; (76) frontal habitus. 77 – 79: S. tenuis sp. nov. (77) dorsal habitus; (78) lateral habitus; (79) frontal habitus. 80 – 82: S. brachyloba sp. nov. (80) dorsal habitus; (81) lateral habitus; (82) frontal habitus. Scale bars: 0.5 mm." httpUri="https://zenodo.org/record/999051/files/figure.png" pageId="30" pageNumber="31">Figures 77–79</figureCitation>
, 
<figureCitation id="133B2A7EFFB2FFF1BC6EEA415AD2FBC8" box="[335,436,1043,1067]" captionStart="FIGURES 227 – 236. 227 – 231" captionStartId="56.[151,264,1900,1923]" captionTargetBox="[160,1419,253,1806]" captionTargetId="figure@56.[151,1435,245,1824]" captionTargetPageId="56" captionText="FIGURES 227 – 236. 227 – 231: S. tenuis sp. nov. (227) abdomen; (228 – 230) male genitalia: (228) penis; (229) tegmen, lateral view; (230) tegmen, ventral view; (231) ovipositor. 232 – 236: S. brachyloba sp. nov. (232) abdomen; (233 – 235) male genitalia: (233) penis; (234) tegmen, lateral view; (235) tegmen, ventral view; (236) ovipositor. Scale bars: 0.1 mm." httpUri="https://zenodo.org/record/999083/files/figure.png" pageId="30" pageNumber="31">227–231</figureCitation>
, 
<figureCitation id="133B2A7EFFB2FFF1BC9EEA415A8CFBC8" box="[447,490,1043,1067]" captionStart="FIGURE 300" captionStartId="65.[151,250,1972,1994]" captionTargetBox="[205,1370,1152,1936]" captionTargetId="figure@65.[193,1377,1132,1955]" captionTargetPageId="65" captionText="FIGURE 300. Distribution map. S. latissima sp. nov. (▲); S. maolanensis sp. nov. (■); S. tenuis sp. nov. (●); S. brachyloba sp. nov. ()." httpUri="https://zenodo.org/record/999107/files/figure.png" pageId="30" pageNumber="31">300</figureCitation>
