<number id="3DF06B71FFE399738314FCA4FE9ACECE" box="[347,376,884,910]" pageId="10" pageNumber="11" value="13.0">13</number>
females
</specimenCount>
, 
<number id="3DF06B71FFE3997383A6FCA4FE1ACECE" box="[489,504,884,910]" pageId="10" pageNumber="11" value="1.0">1</number>
second instar larva, 
<date id="FF865F96FFE3997380A5FCA4FCBCCECE" box="[746,862,884,910]" pageId="10" pageNumber="11" value="1999-11-02">
<number id="3DF06B71FFE3997380A5FCA4FD1ACECE" box="[746,760,884,910]" pageId="10" pageNumber="11" value="2.0">2</number>
­xi­ 
