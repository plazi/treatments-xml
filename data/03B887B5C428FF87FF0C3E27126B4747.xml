, 
<date id="FFAF1063C42FFF87FED53E2216FF41F7" box="[334,452,620,645]" pageId="12" pageNumber="581" value="2002-04-13">
<number id="3DD92484C42FFF87FED53E22165341F7" box="[334,360,620,645]" pageId="12" pageNumber="581" value="13.0">13</number>
