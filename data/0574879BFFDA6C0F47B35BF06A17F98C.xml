<taxonomicName id="4ADD4D0EFFD86C0E45295B4C6868FBF9" box="[525,805,1085,1108]" class="Insecta" family="Nymphidae" genus="Pronymphes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="hoffeinsorum" status="sp. nov.">
<emphasis id="BFA9EA9FFFD86C0E45295B4C6868FBF9" box="[525,805,1085,1108]" italics="true" pageId="6" pageNumber="65">Pronymphes hoffeinsorum</emphasis>
</taxonomicName>
