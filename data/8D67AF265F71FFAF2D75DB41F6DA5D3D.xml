<typeStatus id="DA75A0925F73FFAF2E6ADBA9F4FB5ED9" box="[286,385,453,477]" pageId="7" pageNumber="36">Holotype</typeStatus>
male, 
<collectingCountry id="7DD95EA05F73FFAF2EBBDBAAF7525EDA" box="[463,552,454,478]" name="Norway" pageId="7" pageNumber="36">Norway</collectingCountry>
, 
