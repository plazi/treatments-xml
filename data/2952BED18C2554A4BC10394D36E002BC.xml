<taxonomicName id="8E9320BBBE5DF0A2731ABB2378666BC2" LSID="2952bed1-8c25-54a4-bc10-394d36e002bc" authority="Jabeen, I. Ahmad, Kiran, J. Khan &amp; Khalid" class="Agaricomycetes" family="Amanitaceae" genus="Amanita" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Amanita ahmadii" order="Agaricales" pageId="0" pageNumber="81" phylum="Basidiomycota" rank="species" species="ahmadii">Amanita ahmadii Jabeen, I. Ahmad, Kiran, J. Khan &amp; Khalid</taxonomicName>
