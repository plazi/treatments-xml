(
<figureCitation id="43DC2A39E63F1552FC31FC77FC17FC0F" box="[902,973,1001,1022]" captionStart="Figs" captionStartId="1.[175,215,1570,1589]" captionTargetBox="[222,1165,184,1547]" captionTargetId="figure-57@1.[151,1171,183,1574]" captionTargetPageId="1" captionText="Figs. 1–4. Chrysobothris species, dorsal habitus. 1) C. sabae; 2) C. hispaniolae; 3) C. thoracica; 4) C. marskeae." figureDoi="http://doi.org/10.5281/zenodo.4893208" httpUri="https://zenodo.org/record/4893208/files/figure.png" pageId="4" pageNumber="85">Figs. 4</figureCitation>
, 
<figureCitation id="43DC2A39E63F1552FC6EFC77FC3FFC0F" box="[985,997,1001,1022]" captionStart="Figs" captionStartId="3.[175,215,1710,1729]" captionTargetBox="[159,1166,1288,1687]" captionTargetId="figure-499@3.[151,1171,1288,1687]" captionTargetPageId="3" captionText="Figs. 5–7. Chrysobothris species, male genitalia. 5) C. sabae; 6) C. marskeae; 7) C. thoracica." figureDoi="http://doi.org/10.5281/zenodo.4893212" httpUri="https://zenodo.org/record/4893212/files/figure.png" pageId="4" pageNumber="85">6</figureCitation>
