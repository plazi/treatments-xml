(
<typeStatus id="6322A783567CD551FC4EC0A00028FB7F" box="[995,1098,1181,1205]" pageId="14" pageNumber="15">paratype</typeStatus>
); wing length 
<number id="0A510B06567CD551FB45C0A0017BFB7F" box="[1256,1305,1181,1205]" pageId="14" pageNumber="15" value="3.01">3.01</number>
(
<typeStatus id="6322A783567CD551FA85C0A001F2FB7F" box="[1320,1424,1181,1205]" pageId="14" pageNumber="15">holotype</typeStatus>
