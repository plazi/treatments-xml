<paragraph id="8B824A7EFFC80D13408FFE7C4F48FDF0" blockId="19.[151,827,258,526]" box="[151,558,504,526]" pageId="19" pageNumber="330">
<emphasis id="B949966CFFC80D13408FFE7C4C5BFDF0" box="[151,317,504,526]" italics="true" pageId="19" pageNumber="330">
&quot; 
<taxonomicName id="4C3D31FDFFC80D1340B8FE7C4D85FDF0" authorityName="Wagner" authorityYear="1927" box="[160,227,504,526]" class="Gastropoda" family="Hydrobiidae" genus="Iglica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="19" pageNumber="330" phylum="Mollusca" rank="genus">Iglica</taxonomicName>
“ 
