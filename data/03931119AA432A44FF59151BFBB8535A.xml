<taxonomicName id="4C3ADB8CAA402A45FCC212DEFC6350EA" authority="Borgmeier" authorityName="Borgmeier" authorityYear="1962" box="[780,995,441,467]" class="Insecta" family="Phoridae" genus="Epicnemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="105" phylum="Arthropoda" rank="species" species="testis">
