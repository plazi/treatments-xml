, 
<taxonomicName id="4C3ADB8CAA402A45FF2712BAFD8E50CE" authority="Mostovski." authorityName="Mostovski." box="[233,526,477,503]" class="Insecta" family="Phoridae" genus="Epicnemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="105" phylum="Arthropoda" rank="species" species="namkyensis">
<emphasis id="B94E7C1DAA402A45FF2712BAFF7A50CE" box="[233,250,478,503]" italics="true" pageId="2" pageNumber="105">E</emphasis>
