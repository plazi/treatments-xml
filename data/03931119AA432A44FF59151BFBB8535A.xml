<paragraph id="8B85A00FAA432A46FF591444FCE35601" blockId="1.[151,867,1823,1848]" box="[151,867,1823,1848]" pageId="1" pageNumber="104">
<typeStatus id="54811EADAA432A46FF591444FF495601" box="[151,201,1824,1848]" pageId="1" pageNumber="104">Type</typeStatus>
species: 
<taxonomicName id="4C3ADB8CAA432A46FEE81444FDE75601" authority="Brues 1915" authorityName="Brues" authorityYear="1915" box="[294,615,1823,1848]" class="Insecta" family="Phoridae" genus="Hypocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="104" phylum="Arthropoda" rank="species" species="flavidula">
