<paragraph id="59601142FFCEBD75FF4E2C31FD1EF8B2" blockId="37.[151,1437,1375,2012]" pageId="37" pageNumber="38">
<materialsCitation id="E9B71B1FFFCEBD75FF4E2C31FD1AF8B2" collectionCode="MAB" pageId="37" pageNumber="38" specimenCount="2" typeStatus="holotype">
<typeStatus id="8664AFE0FFCEBD75FF4E2C31FED5F8FA" box="[199,303,1807,1831]" pageId="37" pageNumber="38">Holotype</typeStatus>
and 
<typeStatus id="8664AFE0FFCEBD75FEE12C31FE38F8FA" box="[360,450,1807,1831]" pageId="37" pageNumber="38">allotype</typeStatus>
of 
<taxonomicName id="9EDF6AC1FFCEBD75FE622C2EFD43F8F5" authorityName="Muche" authorityYear="1963" box="[491,697,1808,1832]" class="Insecta" family="Meloidae" genus="Cerocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="pseudogloriosa">
<emphasis id="6BABCD50FFCEBD75FE622C2EFD43F8F5" box="[491,697,1808,1832]" italics="true" pageId="37" pageNumber="38">C. pseudogloriosa</emphasis>
</taxonomicName>
