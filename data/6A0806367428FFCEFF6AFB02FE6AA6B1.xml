species: 
<taxonomicName id="25A1CCA37428FFCEFD05FAC3FB2EA60C" authority="Nonaka &amp; Jangvitaya, 1994" authorityName="Nonaka &amp; Jangvitaya" authorityYear="1994" box="[760,1188,1331,1354]" class="Insecta" family="Thripidae" genus="Takethrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="bambusae">
