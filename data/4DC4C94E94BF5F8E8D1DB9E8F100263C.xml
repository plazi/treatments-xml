<taxonomicName id="AB47D1BACD40757A262D5A0A50C8FA33" authority="Han, 2009" authorityName="Swinhoe" authorityYear="1893" class="Insecta" family="Geometridae" genus="Pseudepisothalma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pseudepisothalma ocellata" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="species" species="ocellata">Pseudepisothalma ocellata</taxonomicName>
