<collectingCountry id="F4A3766F972DFF86FCF85C59FC99FCFF" box="[787,858,829,853]" name="Belize" pageId="58" pageNumber="59">Belize</collectingCountry>
[Carrie Bow Cay], 
<collectingCountry id="F4A3766F972DFF86FBB65C5AFB76FCFF" box="[1117,1205,830,853]" name="Panama" pageId="58" pageNumber="59">Panama</collectingCountry>
