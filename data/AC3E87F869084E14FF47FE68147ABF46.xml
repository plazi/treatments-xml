<taxonomicName id="E3974D6D69094E14FD1AFF64147FBCE2" box="[714,823,152,176]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="7" pageNumber="47" phylum="Arthropoda" rank="species" species="majori">
