<location id="48139C9FFFF4B37B4001F878CD110CE5" LSID="urn:lsid:plazi:treatment:C5657B52FFFBB37B44D1FA7DCB050CC1:48139C9FFFF4B37B4001F878CD110CE5" box="[1095,1303,1969,1993]" country="Mexico" municipality="Arena Bank" name="Gulf of California" pageId="104" pageNumber="105" stateProvince="Baja California">Gulf of California</location>
