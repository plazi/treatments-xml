<collectingCountry id="35DB8AD4FFF4B37B477AF87BCB740CE5" box="[828,882,1970,1993]" name="Peru" pageId="104" pageNumber="105">Peru</collectingCountry>
, 
<materialsCitation id="FDA4C019FFF4B37B47C6F878CAF90CC1" country="Mexico" location="Gulf of California" municipality="Arena Bank" pageId="104" pageNumber="105" specimenCount="1" stateProvince="Baja California">
