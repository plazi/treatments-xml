<collectingCountry id="C78B4EB0A0045D78FC45960476B0FCCC" box="[916,976,835,861]" name="India" pageId="12" pageNumber="223">India</collectingCountry>
, 
<collectingCountry id="C78B4EB0A0045D78FC0D9604717FFCCC" box="[988,1055,835,861]" name="Nepal" pageId="12" pageNumber="223">Nepal</collectingCountry>
