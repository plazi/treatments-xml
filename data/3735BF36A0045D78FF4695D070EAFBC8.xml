<collectingCountry id="C78B4EB0A0045D78FF0496047475FCCC" box="[213,277,835,861]" name="Chile" pageId="12" pageNumber="223">Chile</collectingCountry>
; 
<taxonomicName id="789C75A3A0045D78FEF3960477E5FCCF" authority="Kurosawa (1985)" authorityName="Kurosawa" authorityYear="1985" box="[290,645,835,862]" class="Insecta" family="Lymexylidae" genus="Hymaloxylon" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="223" phylum="Arthropoda" rank="genus">
<emphasis id="8DE8D232A0045D78FEF3960474D7FCCC" box="[290,439,835,861]" italics="true" pageId="12" pageNumber="223">Hymaloxylon</emphasis>
<bibRefCitation id="DB0D73D1A0045D78FE6E960377E5FCCF" author="Kurosawa, Y." box="[447,645,835,862]" pageId="12" pageNumber="223" pagination="109 - 119" refId="ref28184" refString="Kurosawa, Y. (1985) Revisional notes on the family Lymexylonidae (Coleoptera) in eastern and southeastern Asia. Bulletin of the National Science Museum, Tokyo, (A), 11, 109 - 119." type="journal article" year="1985">Kurosawa (1985)</bibRefCitation>
</taxonomicName>
