(introduced with 
<taxonomicName id="789C75A3A0045D78FE8696CC74B3FC34" box="[343,467,907,933]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="12" pageNumber="223" phylum="Tracheophyta" rank="genus">
