<collectingCountry id="C78B4EB0A0045D78FDA2962077CFFC10" box="[627,687,871,897]" name="India" pageId="12" pageNumber="223">India</collectingCountry>
, the 
<collectingCountry id="C78B4EB0A0045D78FD3996207609FC10" box="[744,873,871,897]" name="Philippines" pageId="12" pageNumber="223">Philippines</collectingCountry>
