<taxonomicName id="C7C899BE88481839FF100852FEEEC654" authorityName="Erichson" authorityYear="1848" box="[241,333,261,287]" class="Insecta" family="Scarabaeidae" genus="Phalops" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
