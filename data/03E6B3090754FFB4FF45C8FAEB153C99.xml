<taxonomicName id="4C4F799C0754FFB2FB93C9D4EAF03CC7" authority="Soares &amp; Soares. I" authorityName="Soares &amp; Soares. I" class="Arachnida" family="Stygnidae" genus="Protimesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="286" phylum="Arthropoda" rank="species" species="evelinae">
