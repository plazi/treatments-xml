<taxonomicName id="4C4F799C0754FFB2FCE2C9AAE9AE3CEA" box="[816,930,959,983]" class="Arachnida" family="Stygnidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="286" phylum="Arthropoda" rank="family">Stygnidae</taxonomicName>
