<quantity id="BF053886BB45FFDEFCD0F96F6A90F93B" box="[879,936,1760,1784]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="19" pageNumber="420" unit="m" value="20.0">20 m</quantity>
