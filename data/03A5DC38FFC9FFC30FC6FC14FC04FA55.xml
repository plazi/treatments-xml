, 
<collectingRegion id="49C8A3CCFFC9FFC30D5EFBFEFD56FBBC" box="[527,680,1100,1124]" country="Liberia" name="Grand Gedeh" pageId="8" pageNumber="255">Grand Gedeh</collectingRegion>
, 
<collectingMunicipality id="6BD7F754FFC9FFC30FC6FBD7FF21FBA5" box="[151,223,1125,1149]" pageId="8" pageNumber="255">Tchien</collectingMunicipality>
, along road from 
<location id="8ED33BF5FFC9FFC30EC4FBD7FE1CFBA5" LSID="urn:lsid:plazi:treatment:03A5DC38FFC9FFC30FC6FC14FC04FA55:8ED33BF5FFC9FFC30EC4FBD7FE1CFBA5" box="[405,482,1125,1149]" country="Liberia" county="Blyden" municipality="Tchien" name="Zwedru" pageId="8" pageNumber="255" stateProvince="Grand Gedeh">Zwedru</location>
