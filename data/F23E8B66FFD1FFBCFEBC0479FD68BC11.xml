<emphasis id="48E3E662FFD1FFBCFBAA0742FB3DBC78" box="[1054,1214,1559,1584]" italics="true" pageId="20" pageNumber="21">
<taxonomicName id="BD9741F3FFD1FFBCFBAA0742FB39BC78" box="[1054,1210,1559,1584]" class="Insecta" family="Therevidae" genus="Psilocephala" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="angustata">W. angustata</taxonomicName>
