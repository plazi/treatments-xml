<typeStatus id="F02A883EFFF4AD1EFE68972CFE6FFA8B" box="[386,495,1290,1316]" pageId="10" pageNumber="67">Holotype</typeStatus>
male, Yunnan: Mengyang, Yexianggu (
<quantity id="E8699B79FFF4AD1EFC2A972CFB8FFA8B" box="[960,1039,1290,1316]" metricMagnitude="2" metricUnit="m" metricValue="7.6" pageId="10" pageNumber="67" unit="m" value="760.0">760 m</quantity>
), 
<date id="5B2F105CFFF4AD1EFBCE972CFB38FA8B" box="[1060,1208,1290,1316]" pageId="10" pageNumber="67" value="2007-04-17">
