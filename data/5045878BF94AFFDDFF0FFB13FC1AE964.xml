<taxonomicName id="1FEC4D1EF94AFFDDFE42FB22FD1BE8FD" authority="Bluten" authorityName="Bluten" box="[511,721,1209,1231]" class="Magnoliopsida" family="Verbenaceae" genus="Lantana" kingdom="Plantae" order="Lamiales" pageId="6" pageNumber="945" phylum="Tracheophyta" rank="species" species="camara">
