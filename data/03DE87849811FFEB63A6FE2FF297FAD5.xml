<paragraph id="8BC836929811FFE86295FDA2F5B3FDDE" blockId="1.[160,935,560,621]" box="[160,935,596,621]" pageId="1" pageNumber="970">
<taxonomicName id="4C774D119811FFE86295FDA2F486FDDE" authority="Jiao &amp; Shi, 2013" authorityName="Jiao &amp; Shi" authorityYear="2013" box="[160,658,596,621]" class="Insecta" family="Tettigoniidae" genus="Xizicus" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="970" phylum="Arthropoda" rank="species" species="xizangensis" subGenus="Axizicus">
<emphasis id="B903EA809811FFE86295FDA2F6FCFDDE" box="[160,232,596,621]" italics="true" pageId="1" pageNumber="970">Xizicus</emphasis>
(
<emphasis id="B903EA809811FFE862C2FDA2F758FDDE" box="[247,332,596,621]" italics="true" pageId="1" pageNumber="970">Axizicus</emphasis>
) 
<emphasis id="B903EA809811FFE86369FDA2F7C3FDDE" box="[348,471,596,621]" italics="true" pageId="1" pageNumber="970">xizangensis</emphasis>
<bibRefCitation id="EFE64B639811FFE863EAFDA2F486FDDE" author="Jiao J &amp; Shi FM &amp; Gao JG" box="[479,658,596,621]" pageId="1" pageNumber="970" pagination="296 - 300" refId="ref3093" refString="Jiao J, Shi FM, Gao JG. 2013. Two new species of the genus Xizicus, 1993 (Orthoptera: Tettigoniidae: Meconematinae) from Xizang, China. Zootaxa. 3694 (3): 296 - 300. doi: 10.11646 / zootaxa. 3694.3.10." type="journal article" year="2013">Jiao &amp; Shi, 2013</bibRefCitation>
