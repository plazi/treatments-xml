<taxonomicName id="4C76F7B9FFD3FFF913EDFF7FDFA53298" box="[1005,1162,151,175]" class="Branchiopoda" family="Limnadiidae" genus="Eulimnadia" kingdom="Animalia" order="Diplostraca" pageId="3" pageNumber="150" phylum="Arthropoda" rank="species" species="khoratensis">
