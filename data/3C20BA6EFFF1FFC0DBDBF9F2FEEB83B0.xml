<taxonomicName id="738970FBFFF1FFC0D87CF8F5FC78834C" box="[816,955,1841,1866]" class="Secernentea" family="Ascarididae" genus="Pseudoterranova" kingdom="Animalia" order="Ascaridida" pageId="10" pageNumber="311" phylum="Nematoda" rank="species" species="decipiens">
