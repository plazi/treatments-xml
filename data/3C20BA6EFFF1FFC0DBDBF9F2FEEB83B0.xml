<taxonomicName id="738970FBFFF1FFC0D9F9F893FC2B8368" box="[693,1000,1877,1902]" class="Secernentea" family="Ascarididae" genus="Pseudoterranova" kingdom="Animalia" order="Ascaridida" pageId="10" pageNumber="311" phylum="Nematoda" rank="species" sensu="stricto" species="decipiens">
