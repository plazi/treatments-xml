<taxonomicName id="5B67D52BBC90E1831CE93A38945AF79F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:137846" class="Insecta" family="Formicidae" genus="Dorylus" kingdom="Animalia" lsidName="Dorylus (Anomma) emeryi subsp. opaca Forel" lsidName-HNS="Dorylus (Anomma) emeryi subsp. opaca Forel" order="Hymenoptera" pageNumber="58" phylum="Arthropoda" rank="subSpecies" species="emeryi" species.completionEvidence="missing" species.completionRound="1" subGenus="Anomma" subSpecies="opaca">Dorylus (Anomma) Emeryi Mayr, subsp. opaca</taxonomicName>
, 
<taxonomicNameLabel id="AF0029EB83B10FADB729A8A18CD2A284" rank="subSpecies">n. subsp.</taxonomicNameLabel>
</paragraph>
