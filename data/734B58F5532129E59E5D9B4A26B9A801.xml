<taxonomicName id="5B99312267EA7E91F629781BF74D8DC6" authorityName="Sturany" authorityYear="1900" class="Gastropoda" family="Fasciolariidae" form="paucicostata" genus="Fusus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="subsp. form" order="Neogastropoda" pageId="14" pageNumber="59" phylum="Mollusca" rank="form" species="bifrons" subSpecies="form">form paucicostata</taxonomicName>
