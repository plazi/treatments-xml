<taxonomicName id="4AC454C4886BECC451424D953CCFEB19" class="Actinopterygii" family="Chaetodontidae" genus="Chaetodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chaetodon auriga" order="Perciformes" pageId="0" pageNumber="85" phylum="Chordata" rank="species" species="auriga">Chaetodon auriga</taxonomicName>
