<bibRefCitation id="BF494B3F862BFFA9FDD4F8EEBC55F84D" author="Kurbatov" box="[556,610,1897,1920]" pageId="3" pageNumber="496" refString="Kurbatov, S. A. (1991) New data for the fauna of beetles tribe Euthiini (Coleoptera, Scydmaenidae) from the far east of the USSR. Zoologicheskii Zhurnal, 70, 153 - 155." type="journal article" year="1991">1991</bibRefCitation>
) and 
<collectingCountry id="A3CF765E862BFFA9FD65F8EEBCEDF84D" box="[669,730,1897,1920]" name="Japan" pageId="3" pageNumber="496">Japan</collectingCountry>
(
<bibRefCitation id="BF494B3F862BFFA9FD1FF8EEBA2BF84D" author="Jaloszynski" box="[743,1052,1897,1920]" pageId="3" pageNumber="496" refString="Jaloszynski, P., Hoshina, H. (2004) Revision of Japanese species of Paraneseuthia Franz (Coleoptera, Scydmaenidae). Japanese Journal of Systematic Entomology, 10, 133 - 143." type="journal article" year="2004">
