<collectingCountry id="F3416ABAFFC5FFFF0C0FFB23FEE0FAE4" box="[325,380,1264,1285]" name="Peru" pageId="10" pageNumber="743">Peru</collectingCountry>
(
<collectingRegion id="4992E4C8FFC5FFFF0CC6FB3CFE67FAE4" box="[396,507,1263,1285]" country="Peru" name="Ayacucho" pageId="10" pageNumber="743">Ayacucho</collectingRegion>
