, 
<collectingRegion id="4992E4C8FFC5FFFF0F35FB3CFD6EFAE4" box="[639,754,1263,1285]" country="Peru" name="Apurimac" pageId="10" pageNumber="743">Apurimac</collectingRegion>
, 
<collectingRegion id="4992E4C8FFC5FFFF0DDAFADDFF4AFAC2" box="[144,214,1294,1315]" country="Peru" name="Cusco" pageId="10" pageNumber="743">Cusco</collectingRegion>
