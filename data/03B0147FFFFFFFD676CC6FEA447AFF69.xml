<emphasis id="B96D797BFFFFFFD576CC6FEA4513F982" bold="true" box="[151,545,1525,1552]" pageId="17" pageNumber="18">
Genus 
<taxonomicName id="4C19DEEAFFFFFFD576B56FEA4697F99D" authority="Cairns, 2018" authorityName="Cairns" authorityYear="2018" box="[238,421,1525,1551]" class="Anthozoa" family="Primnoidae" genus="Macroprimnoa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="17" pageNumber="18" phylum="Cnidaria" rank="genus" status="gen. nov.">
<emphasis id="B96D797BFFFFFFD576B56FEA4697F99D" bold="true" box="[238,421,1525,1551]" italics="true" pageId="17" pageNumber="18">Macroprimnoa</emphasis>
</taxonomicName>
