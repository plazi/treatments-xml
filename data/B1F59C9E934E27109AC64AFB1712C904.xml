<taxonomicName id="8332341D0D2D5009BB747A86781E69E0" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Fabaceae" genus="Lathyrus" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="species" species="sylvestris">Lathyrus sylvestris</taxonomicName>
