<location id="B31F6017FF88C85EFC40D722FB597614" LSID="urn:lsid:plazi:treatment:3E6987DAFF88C85EFE29D46EFB6076FE:B31F6017FF88C85EFC40D722FB597614" box="[1022,1170,1568,1594]" latitude="-13.216666" longLatPrecision="1291" longitude="-64.8" name="Rio Madeira" pageId="21" stateProvince="Amazonas and Rondonia">Rio Madeira</location>
