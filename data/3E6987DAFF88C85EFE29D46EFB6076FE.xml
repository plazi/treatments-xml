<materialsCitation id="06A83C91FF88C85EFF7ED722FB6C76FE" collectorName="Voss &amp; da Silva" latitude="-13.216666" location="Rio Madeira" longLatPrecision="1291" longitude="-64.8" pageId="21" specimenCount="1" stateProvince="Amazonas and Rondonia">
DISTRIBUTION: 
<taxonomicName id="71C04D4FFF88C85EFEC5D722FD547614" authorityName="Voss and da Silva" authorityYear="2001" box="[379,671,1568,1594]" class="Mammalia" family="Erethizontidae" genus="Coendou" kingdom="Animalia" order="Rodentia" pageId="21" phylum="Chordata" rank="species" species="roosmalenorum">
