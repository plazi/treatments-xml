<geoCoordinate id="D3F4500BFF88C85EFF2ED7B4FF3C76FE" box="[144,247,1717,1744]" degrees="64" direction="west" minutes="48" orientation="longitude" pageId="21" precision="925" value="-64.8">64°48′W</geoCoordinate>
), about 
<quantity id="71389B29FF88C85EFEE9D7B7FE7A76FE" box="[343,433,1717,1744]" metricMagnitude="5" metricUnit="m" metricValue="5.0" pageId="21" unit="km" value="500.0">500 km</quantity>
