<materialsCitation id="BBE72E38FFD9EC7BECBAA74DFB8BE540" box="[143,1126,832,859]" collectorName="von Mitte Juli" location="Zurich" pageId="1" pageNumber="170" specimenCount="1" stateProvince="Zuerich">
