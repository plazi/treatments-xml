<taxonomicName id="B9D24D7D3034FFE46A38F8E3FD796822" authority="A. Adams, 1854" authorityName="A. Adams" authorityYear="1854" box="[289,658,1820,1842]" class="Gastropoda" family="Philinidae" genus="Philine" kingdom="Animalia" order="Cephalaspidea" pageId="9" pageNumber="510" phylum="Mollusca" rank="species" species="orientalis">
