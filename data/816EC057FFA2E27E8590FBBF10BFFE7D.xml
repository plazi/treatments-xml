<taxonomicName id="CEC70AC2FFA0E279841AF93313FDF97A" authority="Noodt, 1962" authorityName="Noodt" authorityYear="1962" box="[285,683,1684,1707]" class="Maxillopoda" family="Parastenocarididae" genus="Remaneicaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="11" pageNumber="510" phylum="Arthropoda" rank="species" species="palaciosi">
