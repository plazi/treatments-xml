<geoCoordinate id="EE41500AFEFAFEC9713EF94CFEFAFC8B" box="[284,347,1749,1773]" degrees="2" direction="east" minutes="10" orientation="longitude" pageId="374" pageNumber="375" precision="925" value="2.1666667">
<emphasis id="B901EADFFEFAFEC9713EF94CFE97FC8B" box="[284,310,1749,1773]" italics="true" pageId="374" pageNumber="375">e2</emphasis>
