<paragraph id="8BCA36CDFEFAFEC970B5FD1DFBA9F8FF" blockId="374.[151,1032,613,666]" box="[151,1032,643,666]" pageId="374" pageNumber="375">
<typeStatus id="54CE886FFEFAFEC970B5FD1DFF68F8FF" box="[151,201,644,665]" pageId="374" pageNumber="375">Type</typeStatus>
host: 
<taxonomicName id="4C754D4EFEFAFEC97125FD1DFDA4F8FC" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[263,517,644,666]" class="Aves" family="Phasianidae" genus="Gallus" kingdom="Animalia" order="Galliformes" pageId="374" pageNumber="375" phylum="Chordata" rank="subSpecies" species="gallus" subSpecies="domesticus">
