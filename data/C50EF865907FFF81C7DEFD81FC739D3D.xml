<taxonomicName id="8AA732F0907FFF81C7A6FBC5FC949D7C" authorityName="Paperna &amp; Thurston" authorityYear="1969" box="[675,815,1028,1055]" class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="21" pageNumber="870" phylum="Platyhelminthes" rank="species" species="brevicirrus">
