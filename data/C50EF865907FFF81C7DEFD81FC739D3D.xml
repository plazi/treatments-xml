<collectingCountry id="35B009E3907FFF81C6E9FBE4FBFB9D5D" box="[1004,1088,1060,1087]" name="Uganda" pageId="21" pageNumber="870">Uganda</collectingCountry>
on 
<taxonomicName id="8AA732F0907FFF81C168FBE4FC789D3D" authority="(Boulenger, 1897)" baseAuthorityName="Boulenger" baseAuthorityYear="1897" class="Actinopterygii" family="Cichlidae" genus="Tilapia" kingdom="Animalia" order="Perciformes" pageId="21" pageNumber="870" phylum="Chordata" rank="species" species="rendalli">
