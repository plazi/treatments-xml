7. 
<taxonomicName id="F2AF450BCAA1BD967C796F389CBB6F32" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138343" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica congerens Nylander" lsidName-HNS="Formica congerens Nylander" order="Hymenoptera" pageNumber="906" phylum="Arthropoda" rank="species" species="congerens">F. congerens</taxonomicName>
