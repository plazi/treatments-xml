<taxonomicName id="4C01CA8577E7FF1CFEB3A5579218F9C8" authority="Schmidt" authorityName="Schmidt" box="[304,613,1596,1618]" class="Demospongiae" family="Tethyidae" genus="Columnitis" kingdom="Animalia" order="Hadromerida" pageId="176" pageNumber="177" phylum="Porifera" rank="species" species="squamata">
<taxonomicName id="4C01CA8577E7FF1CFEB3A5579275F9C8" box="[304,520,1596,1618]" class="Demospongiae" family="Tethyidae" genus="Columnitis" kingdom="Animalia" order="Hadromerida" pageId="176" pageNumber="177" phylum="Porifera" rank="species" species="squamata">
<taxonomicName id="4C01CA8577E7FF1CFEB3A55791E2F9C8" box="[304,415,1596,1618]" class="Demospongiae" family="Tethyidae" genus="Columnitis" kingdom="Animalia" order="Hadromerida" pageId="176" pageNumber="177" phylum="Porifera" rank="genus">
<emphasis id="B9756D1477E7FF1CFEB3A55791E2F9C8" box="[304,415,1596,1618]" italics="true" pageId="176" pageNumber="177">Columnitis</emphasis>
</taxonomicName>
<taxonomicName id="4C01CA8577E7FF1CFE26A5569275F9C8" authority="Schmidt" authorityName="Schmidt" authorityYear="1870" box="[421,520,1597,1618]" pageId="176" pageNumber="177" rank="species" species="squamata">
