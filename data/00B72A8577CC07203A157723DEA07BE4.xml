<taxonomicName id="0CBF5E306779BFFD4A7B43D25C6ED33E" class="Insecta" family="Staphylinidae" genus="Coryphium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coryphium nigrum" order="Coleoptera" pageId="5" pageNumber="12" phylum="Arthropoda" rank="species" species="nigrum">Coryphium nigrum</taxonomicName>
