<number id="BB2924BBF4731A01049D7939CFB1F8B9" box="[1229,1244,1802,1828]" pageId="8" pageNumber="62" value="2.0">2</number>
V
</date>
; I 0; II 
<date id="795F105CF4731A01051D7939CE1CF8B9" box="[1357,1393,1802,1828]" pageId="8" pageNumber="62">
<number id="BB2924BBF4731A01051D7939CE31F8B9" box="[1357,1372,1802,1828]" pageId="8" pageNumber="62" value="2.0">2</number>
V
</date>
; III 
<date id="795F105CF4731A0100C77901CBD4F8D1" box="[151,185,1842,1868]" pageId="8" pageNumber="62">
<number id="BB2924BBF4731A0100C77901CBC9F8D1" box="[151,164,1842,1868]" pageId="8" pageNumber="62" value="3.0">3</number>
