<quantity id="8D02BB9A5F71FF93FD8415D32ED1FE0F" box="[635,717,434,460]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" pageId="3" pageNumber="401" unit="mm" value="4.6">4.6mm</quantity>
