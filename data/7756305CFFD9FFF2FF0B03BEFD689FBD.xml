(
<figureCitation id="67C49DCFFFD9FFF6FF030393FF7C9F96" box="[159,238,186,211]" captionStart="FIGURES 1 – 4" captionStartId="2.[151,264,1914,1937]" captionTargetBox="[179,1417,310,1859]" captionTargetId="figure@2.[167,1419,303,1890]" captionTargetPageId="2" captionText="FIGURES 1 – 4. Dorsal view photograph of Narnia spp. 1, N. coachellae Bliven. 2, N. femorata Stål. 3, N. inornata Distant. 4, N. anaticula sp. nov." httpUri="https://zenodo.org/record/217899/files/figure.png" pageId="1" pageNumber="286">Figs. 4</figureCitation>
, 
<figureCitation id="67C49DCFFFD9FFF6FF660392FE9A9F96" box="[250,264,187,211]" captionStart="FIGURE 8" captionStartId="4.[151,250,836,858]" captionText="FIGURE 8. Male genital capsule of Narnia anaticula sp. nov." pageId="1" pageNumber="286">8</figureCitation>
