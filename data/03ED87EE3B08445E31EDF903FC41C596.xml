<taxonomicName id="4C444D7B3B04445E3045FC3BFE60C596" authorityName="Kieffer" authorityYear="1904" box="[319,429,916,942]" class="Insecta" family="Bethylidae" genus="Holepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">
<emphasis id="B930EAEA3B04445E3045FC3BFE60C596" box="[319,429,916,942]" italics="true" pageId="56" pageNumber="57">Holepyris</emphasis>
</taxonomicName>
