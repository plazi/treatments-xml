<taxonomicName id="4C444D7B3B04445E32A2FCDFFAB6C5B3" authority="(Kurian, 1955)" baseAuthorityName="Kurian" baseAuthorityYear="1955" box="[984,1403,880,907]" class="Insecta" family="Bethylidae" genus="Sclerodermus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="raonis">
<emphasis id="B930EAEA3B04445E32A2FCDFFB09C5B2" box="[984,1220,880,906]" italics="true" pageId="56" pageNumber="57">Sclerodermus raonis</emphasis>
(
<bibRefCitation id="EFD54B093B04445E35ADFCDFFABFC5B3" author="Kurian, C." box="[1239,1394,880,907]" pageId="56" pageNumber="57" pagination="67 - 155" refId="ref41065" refString="Kurian, C. (1955) Bethyloidea (Hymenoptera) from Indian. Agra University Journal of Research, 4, 67 - 155." type="journal article" year="1955">Kurian, 1955</bibRefCitation>
)
</taxonomicName>
to 
<taxonomicName id="4C444D7B3B04445E31EDFC3BFEC8C596" authorityName="Kieffer" authorityYear="1904" box="[151,261,916,942]" class="Insecta" family="Bethylidae" genus="Holepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">
<emphasis id="B930EAEA3B04445E31EDFC3BFEC8C596" box="[151,261,916,942]" italics="true" pageId="56" pageNumber="57">Holepyris</emphasis>
</taxonomicName>
