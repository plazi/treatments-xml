<taxonomicName id="4C444D7B3B04445E339BFE3CFC85C795" box="[737,840,403,429]" class="Insecta" family="Bethylidae" genus="Odontepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="slides">
