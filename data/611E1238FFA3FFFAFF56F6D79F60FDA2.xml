<taxonomicName id="2EB7D8ADFFA3FFFAFE96F6D79866FF54" box="[343,539,220,246]" class="Clitellata" family="Enchytraeidae" genus="Cernosvitoviella" kingdom="Animalia" order="Enchytraeida" pageId="2" pageNumber="324" phylum="Annelida" rank="genus">
