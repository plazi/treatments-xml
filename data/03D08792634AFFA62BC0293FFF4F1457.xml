<subSubSection id="C363650F634AFFA42A692970FD57174B" box="[113,704,347,367]" pageId="1" pageNumber="472" type="type_taxon">
<paragraph id="8BC63684634AFFA42A692970FD57174B" blockId="1.[113,704,347,367]" box="[113,704,347,367]" pageId="1" pageNumber="472">
