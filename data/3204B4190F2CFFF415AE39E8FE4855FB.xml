<taxonomicName id="7DAD7E8C0F2CFFF4140C39B0FD6A5718" authority="McIntosh, 1874" authorityName="McIntosh" authorityYear="1874" box="[309,717,1269,1292]" class="Polychaeta" family="Polynoidae" genus="Hermadion" kingdom="Animalia" order="Phyllodocida" pageId="8" pageNumber="53" phylum="Annelida" rank="species" species="assimile">
<emphasis id="88D9D91D0F2CFFF4140C39B0FDB65718" box="[309,529,1269,1292]" italics="true" pageId="8" pageNumber="53">Hermadion assimile</emphasis>
<bibRefCitation id="DE3C78FE0F2CFFF4172539B0FD6A5718" author="McIntosh" box="[540,717,1269,1292]" pageId="8" pageNumber="60" refString="McIntosh, W. C. (1874) XXVIII. On the invertebrate fauna and fishes of St. Andrews. Annals and Magazine of Natural History, series 4, 14, 192 - 207." type="journal article" year="1874">
McIntosh, 
<number id="0C6517280F2CFFF417AC39B0FD6A5718" box="[661,717,1269,1292]" pageId="8" pageNumber="53" value="1874.0">1874</number>
</bibRefCitation>
</taxonomicName>
