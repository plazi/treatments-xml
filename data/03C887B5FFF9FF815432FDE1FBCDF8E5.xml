<emphasis id="B915EAB1FFF9FF8151D7FCA5FCF5FCF4" italics="true" pageId="14" pageNumber="15">
<taxonomicName id="4C614D20FFF9FF8151D7FCA5FE21FCF4" authority="Mycetis" authorityName="Mycetis" class="Insecta" family="Hesperiidae" genus="Viola" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="riviniana">Viola riviniana, Mycetis</taxonomicName>
