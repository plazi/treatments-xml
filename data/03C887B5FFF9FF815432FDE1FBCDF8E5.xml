</emphasis>
and 
<taxonomicName id="4C614D20FFF9FF815400FC2DFDFAFC24" box="[314,517,919,944]" class="Magnoliopsida" family="Oxalidaceae" genus="Oxalis" kingdom="Plantae" order="Oxalidales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="acetosella">
