<taxonomicName id="4C790D9FFF10D3CECD836771C93499E0" class="Arachnida" family="Ascidae" genus="Lasioseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="169" pageNumber="170" phylum="Arthropoda" rank="species" species="furcisetus">
