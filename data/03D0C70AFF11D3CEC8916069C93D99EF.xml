<taxonomicName id="4C790D9FFF10D3CEC92A6771CB5A990D" authority="Hirschmann" authorityName="Hirschmann" box="[300,877,332,357]" class="Arachnida" family="Ascidae" genus="Lasioseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="169" pageNumber="170" phylum="Arthropoda" rank="species" species="ometisimilis" subGenus="Lasioseius">
