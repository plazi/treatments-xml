<taxonomicName id="4C790D9FFF10D3CEC9A66739CA0B9975" box="[416,572,260,285]" class="Arachnida" family="Ascidae" genus="Lasioseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="169" pageNumber="170" phylum="Arthropoda" rank="species" species="penicilliger">
