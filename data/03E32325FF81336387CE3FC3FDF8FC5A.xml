, 
<collectingCountry id="F35DD2A3FF813363865F3F99FE7CFD53" box="[316,404,721,742]" name="Tanzania" pageId="5" pageNumber="437">Tanzania</collectingCountry>
, 
<collectingCountry id="F35DD2A3FF81336386C03F99FE07FD53" box="[419,495,721,742]" name="Uganda" pageId="5" pageNumber="437">Uganda</collectingCountry>
, and 
<collectingCountry id="F35DD2A3FF813363854D3F99FD7BFD53" box="[558,659,721,742]" name="Zimbabwe" pageId="5" pageNumber="437">Zimbabwe</collectingCountry>
(
<bibRefCitation id="EFDBEFC2FF81336387D13FA3FEC7FCB5" author="Ferreira, M. C." box="[178,303,747,768]" pageId="5" pageNumber="437" pagination="5 - 1088" refId="ref3778" refString="Ferreira, M. C. 1972. Os escarab'ideos de Africa (sul do Saara). I. Revista de Entomologia de Mocambique 11: 5 - 1088." type="journal article" year="1972">Ferreira 1972</bibRefCitation>
