<paragraph id="EA6B36F7FF87F763FF7EF94CFC937BF9" blockId="121.[151,835,1751,1818]" box="[151,835,1751,1777]" pageId="121" pageNumber="122">
<heading id="B123819BFF87F763FF7EF94CFC937BF9" bold="true" box="[151,835,1751,1777]" fontSize="11" level="1" pageId="121" pageNumber="122" reason="1">
<emphasis id="D8A0EAE5FF87F763FF7EF94CFC937BF9" bold="true" box="[151,835,1751,1777]" pageId="121" pageNumber="122">
<taxonomicName id="2DD44D74FF87F763FF7EF94CFD767BF9" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,678,1751,1777]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="121" pageNumber="122" phylum="Arthropoda" rank="species" species="joariva" status="new species">
<emphasis id="D8A0EAE5FF87F763FF7EF94CFE127BF9" bold="true" box="[151,450,1751,1777]" italics="true" pageId="121" pageNumber="122">Cheumatopsyche joariva</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFF87F763FD5AF94CFC937BF9" box="[691,835,1751,1777]" pageId="121" pageNumber="122" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
