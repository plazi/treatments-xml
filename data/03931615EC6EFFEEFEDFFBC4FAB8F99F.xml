with pereonites 1–2 fused (except 
<taxonomicName id="4C3ADC80EC6FFFEEFB54FEE4FAB3FE98" authorityName="Claus" authorityYear="1879" box="[1155,1313,284,310]" class="Malacostraca" family="Lestrigonidae" genus="Phronimopsis" kingdom="Animalia" order="Amphipoda" pageId="31" pageNumber="32" phylum="Arthropoda" rank="genus">
