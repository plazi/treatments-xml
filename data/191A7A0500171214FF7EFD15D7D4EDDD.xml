<quantity id="564B66F600171213FE29F860D297EAFD" box="[448,509,1985,2008]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="91" pageNumber="92" unit="mm" value="7.0">7mm</quantity>
