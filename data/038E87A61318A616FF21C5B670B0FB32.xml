and 
<taxonomicName id="4C274D331319A616FC3BC3BF7523FBE4" authorityName="E. Y. Watson" authorityYear="1893" box="[986,1067,1004,1025]" class="Insecta" family="Hesperiidae" genus="Nascus" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="genus">
