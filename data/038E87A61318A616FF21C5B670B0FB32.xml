<emphasis id="B953EAA21319A616FE69C45B70B3FBFB" bold="true" box="[392,443,1032,1054]" pageId="31" pageNumber="30">121)</emphasis>
<taxonomicName id="4C274D331319A616FE23C45A7369FBF8" authority="salon" box="[450,609,1032,1054]" class="Insecta" family="Hesperiidae" genus="Nascus" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="subSpecies" species="salon" subSpecies="salon">
