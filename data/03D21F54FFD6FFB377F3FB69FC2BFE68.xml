<taxonomicName id="4C7BD5C1FFD6FFBC767CFE03FCCBF887" authorityName="Freitag &amp; Kodada" authorityYear="2017" box="[280,456,1837,1858]" class="Insecta" family="Elmidae" genus="Ancyronyx" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="128" phylum="Arthropoda" rank="species" species="toraja">
