<emphasis id="B96FEADEFFC88342919865DFD457F87D" bold="true" box="[189,295,1962,1988]" pageId="3" pageNumber="4">Fig. 2. A</emphasis>
. 
<taxonomicName id="4C1B4D4FFFC88342901365DED48FF87D" authority="Enghoff, 2012" authorityName="Enghoff" authorityYear="2012" box="[310,511,1962,1988]" class="Diplopoda" family="Julidae" genus="Dolichoiulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Julida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="typhlocanaria" status="sp. nov.">
