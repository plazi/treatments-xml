<taxonomicName id="4BA558BBFF857913FEEFDFDBA8A76999" authorityName="Andre" authorityYear="1896" box="[318,503,1966,1989]" class="Insecta" family="Mutillidae" genus="Sinotilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="162" phylum="Arthropoda" rank="species" species="cyaneiventris">
