<taxonomicName id="4BA558BBFF857913FA56DFDBA86969B5" authorityName="Yamane" authorityYear="1999" class="Insecta" family="Mutillidae" genus="Sinotilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="162" phylum="Arthropoda" rank="species" species="yakushimensis">
