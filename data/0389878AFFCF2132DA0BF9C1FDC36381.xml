<taxonomicName id="4C204D1FFFCF2133DA9CF8A6FACC652F" authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[939,1379,1840,1865]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="66" pageNumber="2848" phylum="Firmicutes" rank="species" species="angrenensis">
