<subSubSection id="C360651EFF97FF92E6B369B1FEF366E7" lastPageId="20" lastPageNumber="71" pageId="19" pageNumber="70" type="description">
<paragraph id="8BC53695FF97FF92E6B369B1FEF366E7" blockId="19.[151,796,2012,2034]" lastBlockId="20.[151,1437,151,752]" lastPageId="20" lastPageNumber="71" pageId="19" pageNumber="70">
<taxonomicName id="4C7A4D16FF97FF95E6B369B1FE6461C1" box="[151,429,2012,2034]" class="Arachnida" family="Zorocratidae" genus="Campostichomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="70" phylum="Arthropoda" rank="species" species="seriatum">
