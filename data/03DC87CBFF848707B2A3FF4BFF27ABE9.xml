<taxonomicName id="4C754D5EFF848707B0CEFE29F92FABC9" box="[1023,1126,440,463]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="26" pageNumber="395" phylum="Tracheophyta" rank="genus">
