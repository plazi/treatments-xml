<taxonomicName id="F0FF4D3F6C03E6509777F958FBB95D6E" authority="Camras, 1957 a: 13" authorityName="Camras" authorityPageNumber="13" authorityYear="1957" class="Insecta" family="Conopidae" genus="Scatoccemyia" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="67" phylum="Arthropoda" rank="genus">
<emphasis id="058BEAAE6C03E6509777F958FCDA5D6E" italics="true" pageId="24" pageNumber="67">Scatoccemyia</emphasis>
<bibRefCitation id="536E4B4D6C03E6509137F8B9FBB95D6E" author="Camras S" box="[870,1084,1819,1841]" pageId="24" pageNumber="67" pagination="9 - 16" refId="ref25112" refString="Camras S. 1957 a. Descriptions and records of Neotropical Conopidae (Diptera). Psyche 64: 9 - 16." type="journal article" year="1957">Camras, 1957a: 13</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="F0FF4D3F6C03E6509619F8B9FB485D6E" authorityName="Rondani" authorityYear="1845" box="[1096,1229,1820,1841]" class="Insecta" family="Conopidae" genus="Thecophora" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="67" phylum="Arthropoda" rank="genus">
