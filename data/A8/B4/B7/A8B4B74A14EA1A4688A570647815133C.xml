<taxonomicName baseAuthorityName="Letzner" baseAuthorityYear="1851" class="Hexapoda" family="Carabidae" genus="Platynus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="772" pageNumber="773" phylum="Arthropoda" rank="species" species="bicolor">
