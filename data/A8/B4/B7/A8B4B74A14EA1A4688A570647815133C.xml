<taxonomicName class="Hexapoda" family="Carabidae" genus="Anchomenus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="772" pageNumber="773" phylum="Arthropoda" rank="species" species="ovipennis">
