<quantity box="[903,1057,1337,1364]" metricMagnitude="-2" metricUnit="m" metricValue="1.02" metricValueMax="1.09" metricValueMin="0.95" pageId="52" pageNumber="53" unit="mm" value="10.2" valueMax="10.9" valueMin="9.5">9.5–10.9 mm</quantity>
, AA = 24–28°, SL/ 
<collectionCode box="[1285,1332,1337,1363]" country="Germany" name="Museum Donaueschingen" pageId="52" pageNumber="53">MD</collectionCode>
