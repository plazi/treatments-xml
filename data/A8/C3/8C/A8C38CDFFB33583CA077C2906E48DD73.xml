<taxonomicName class="Insecta" family="Lyonetiidae" genus="Lyonetia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lyonetia clerckella" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="clerckella">
<emphasis italics="true" pageId="0" pageNumber="1">Lyonetia clerckella</emphasis>
</taxonomicName>
(Linnaeus) (
<taxonomicName class="Insecta" genus="Lyonetia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
, 
<taxonomicName authorityName="Stainton" authorityYear="1854" class="Hexapoda" family="Lyonetiidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="family">Lyonetiidae</taxonomicName>
) (
<bibRefCitation DOI="https://doi.org/10.1303/aez.33.299" author="Adachi, I" journalOrPublisher="Applied Entomology and Zoology" pageId="0" pageNumber="1" pagination="299 - 304" refId="B1" refString="Adachi, I, 1998. Hymenopterous parasitoids of the peach leafminer, Lyonetia clerckella (Linnaeus) (Lepidoptera: Lyonetiidae). Applied Entomology and Zoology 33 (2): 299 - 304, DOI: https://doi.org/10.1303/aez.33.299" title="Hymenopterous parasitoids of the peach leafminer, Lyonetia clerckella (Linnaeus) (Lepidoptera: Lyonetiidae)." url="https://doi.org/10.1303/aez.33.299" volume="33" year="1998">Adachi 1998</bibRefCitation>
) and the citrus leafminer 
<taxonomicName authorityName="Stainton" authorityYear="1856" class="Hexapoda" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phyllocnistis citrella" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="citrella">
<emphasis italics="true" pageId="0" pageNumber="1">Phyllocnistis citrella</emphasis>
</taxonomicName>
