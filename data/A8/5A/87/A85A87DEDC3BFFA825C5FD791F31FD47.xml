<taxonomicName authority="zetes (Linnaeus, 1758)" authorityName="Linnaeus" authorityYear="1758" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[860,1381,1231,1256]" class="Insecta" family="Nymphalidae" genus="Acraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="75" phylum="Arthropoda" rank="subSpecies" species="zetes" subGenus="Acraea" subSpecies="zetes">
