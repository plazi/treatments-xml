<paragraph blockId="5.[151,1436,1012,1360]" pageId="5" pageNumber="62">
<emphasis bold="true" box="[151,265,1156,1181]" pageId="5" pageNumber="62">
<typeStatus box="[151,260,1156,1181]" pageId="5" pageNumber="62">Syntypes</typeStatus>
,
</emphasis>
<specimenCount box="[272,383,1156,1181]" pageId="5" pageNumber="62" type="female">2 females</specimenCount>
