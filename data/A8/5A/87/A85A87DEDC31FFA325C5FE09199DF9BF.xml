<taxonomicName authorityName="Godart" authorityYear="1823" box="[306,462,727,752]" class="Insecta" family="Nymphalidae" genus="Calisto" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="81" phylum="Arthropoda" rank="species" species="hysius">
