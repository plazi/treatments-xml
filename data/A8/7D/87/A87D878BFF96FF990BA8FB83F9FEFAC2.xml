<paragraph blockId="96.[151,362,1138,1197]" box="[151,360,1138,1164]" pageId="96" pageNumber="97">
<heading bold="true" box="[151,360,1138,1164]" fontSize="11" level="1" pageId="96" pageNumber="97" reason="1">
<emphasis bold="true" box="[151,360,1138,1164]" pageId="96" pageNumber="97">
<taxonomicName authorityName="Ulrich &amp; Cooper" authorityYear="1938" box="[151,281,1138,1164]" class="Rhynchonellata" family="Archaeorthidae" genus="Nothorthis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthida" pageId="96" pageNumber="97" phylum="Brachiopoda" rank="genus">
