</taxonomicName>
<taxonomicNameLabel box="[436,504,1384,1405]" pageId="2" pageNumber="123" rank="species">nov.sp.</taxonomicNameLabel>
, Habitus, dorsal; (
<emphasis bold="true" box="[666,677,1383,1404]" pageId="2" pageNumber="123">2</emphasis>
) 
<taxonomicName authority="Klausnitzer, 2019" authorityName="Klausnitzer" authorityYear="2019" box="[689,819,1385,1405]" class="Insecta" family="Scirtidae" genus="Scirtes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="123" phylum="Arthropoda" rank="species" species="geberti" status="nov.sp.">
<emphasis box="[689,819,1385,1405]" italics="true" pageId="2" pageNumber="123">Scirtes geberti</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[824,892,1384,1405]" pageId="2" pageNumber="123" rank="species">nov.sp.</taxonomicNameLabel>
