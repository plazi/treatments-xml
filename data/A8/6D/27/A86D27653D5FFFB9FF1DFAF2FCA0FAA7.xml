<collectingRegion box="[313,479,1303,1329]" country="Colombia" name="Cundinamarca" pageId="45" pageNumber="46">Cundinamarca</collectingRegion>
, and 
<collectingRegion box="[538,596,1303,1329]" country="Colombia" name="Meta" pageId="45" pageNumber="46">Meta</collectingRegion>
(
<collectingCountry box="[610,724,1303,1330]" name="Colombia" pageId="45" pageNumber="46">Colombia</collectingCountry>
