<taxonomicName authority="Franz" authorityName="Franz" box="[647,850,1990,2011]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="49" phylum="Arthropoda" rank="species" species="comarapae">
