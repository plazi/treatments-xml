<collectingRegion box="[359,441,1196,1221]" country="Paraguay" name="Central" pageId="26" pageNumber="67">Central</collectingRegion>
<collectingCountry box="[448,531,1196,1221]" name="Bolivia" pageId="26" pageNumber="67">Bolivia</collectingCountry>
(
<figureCitation box="[546,644,1196,1221]" captionStart="FIGURE 101" captionStartId="40.[151,250,1770,1792]" captionTargetBox="[151,1414,635,1749]" captionTargetId="figure@40.[151,1436,625,1749]" captionTargetPageId="40" captionText="FIGURE 101. Distribution of nominal Protoconnus species." httpUri="https://zenodo.org/record/1195000/files/figure.png" pageId="26" pageNumber="67">Fig. 101</figureCitation>
