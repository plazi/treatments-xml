<quantity box="[654,743,654,679]" metricMagnitude="3" metricUnit="m" metricValue="2.125" pageId="22" pageNumber="63" unit="m" value="2125.0">2125 m</quantity>
, S 00°36'18.4'' / W 77°53'09.0'', 
<date box="[1117,1243,655,679]" pageId="22" pageNumber="63" value="2009-12-09">9 XII 2009</date>
/ leg. R. RUTA&quot; [white, printed], &quot; 
<taxonomicName authorityName="Franz" authorityYear="1967" box="[354,554,691,714]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="63" phylum="Arthropoda" rank="genus">
<emphasis box="[354,554,691,714]" italics="true" pageId="22" pageNumber="63">PROTOCONNUS</emphasis>
</taxonomicName>
/ 
<taxonomicName authorityName="Jałoszyński" authorityYear="2018" box="[580,724,690,715]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="63" phylum="Arthropoda" rank="species" species="ecuadoranus">
