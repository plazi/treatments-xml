/ Nebelwald,Peru&quot; with &quot;Sa 84&quot; on the reverse side [white, printed], &quot;♀&quot; [white, handwritten in blue], &quot; 
<taxonomicName authorityName="Franz" authorityYear="1967" box="[266,409,763,787]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="82" phylum="Arthropoda" rank="genus">Protoconnus</taxonomicName>
/ 
<taxonomicName box="[437,543,762,787]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="62" phylum="Arthropoda" rank="species" species="peruensis">peruensis</taxonomicName>
/ m. / PARATYPUS&quot; [yellow, handwritten and printed], &quot; 
<taxonomicName authorityName="Franz" authorityYear="1967" box="[1218,1418,763,786]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="62" phylum="Arthropoda" rank="genus">
