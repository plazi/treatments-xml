<number box="[290,328,906,928]" pageId="17" pageNumber="268" value="244.0">244</number>
(originally in 
<taxonomicName box="[473,552,906,928]" class="Insecta" family="Staphylinidae" genus="Velleius" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="268" phylum="Arthropoda" rank="genus">
