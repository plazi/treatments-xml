</materialsCitation>
. 
<materialsCitation ID-GBIF-Occurrence="1323865282" collectingDate="1967" collectingDateMax="1980" collectingDateMin="1967" collectionCode="NMPC" collectorName="J. Soukup &amp; J. Jelinek" country="Peru" county="National University" location="Specimens" municipality="San Marcos" pageId="14" pageNumber="315" specimenCount="1" stateProvince="Lima" typeStatus="holotype">
