<quantity box="[1056,1160,296,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="8" pageNumber="309" unit="mm" value="1.7">1.70 mm</quantity>
, EL 
<quantity box="[1217,1321,296,320]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="8" pageNumber="309" unit="mm" value="4.1">4.10 mm</quantity>
