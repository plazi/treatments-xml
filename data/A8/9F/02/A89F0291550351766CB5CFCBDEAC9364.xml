<taxonomicName class="Arachnida" family="Nanhermanniidae" genus="Nanhermannia" genus.bestMatchDistance="1772" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="74" phylum="Arthropoda" rank="species" species="areolata">N. areolata</taxonomicName>
