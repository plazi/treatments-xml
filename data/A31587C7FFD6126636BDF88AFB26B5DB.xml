<taxonomicName id="ECBC4D52FFD712663476FF46FCCBB5BE" authorityName="Tee-Van" authorityYear="1940" box="[604,894,151,176]" class="Actinopterygii" family="Cirrhitidae" genus="Cirrhitichthys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="49" pageNumber="50" phylum="Chordata" rank="species" species="corallicola">
