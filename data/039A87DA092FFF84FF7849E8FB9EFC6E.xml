</taxonomicName>
. 12. 
<taxonomicName id="4C334D4F0928FF86FB4E4AE2FAB4F87B" baseAuthorityName="Tams" baseAuthorityYear="1929" box="[1185,1391,1944,1968]" class="Insecta" family="Lasiocampidae" genus="Weberolegra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="421" phylum="Arthropoda" rank="species" species="weberi">
