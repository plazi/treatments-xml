<collectingCountry id="F34D7651FFE5FFBCFDA898ACFD7385D6" box="[596,673,925,949]" name="Canada" pageId="19" pageNumber="510">Canada</collectingCountry>
; Guwayza Formation, 
<collectingCountry id="F34D7651FFE5FFBCFF0B9889FEEB85B3" box="[247,313,952,976]" name="Oman" pageId="19" pageNumber="510">Oman</collectingCountry>
; 
<collectingCountry id="F34D7651FFE5FFBCFEBD9889FEA885AC" box="[321,378,952,975]" name="Japan" pageId="19" pageNumber="510">Japan</collectingCountry>
;? 
<collectingRegion id="499EF823FFE5FFBCFE769889FE0685B3" box="[394,468,952,976]" country="Turkey" name="Antalya" pageId="19" pageNumber="510">Antalya</collectingRegion>
