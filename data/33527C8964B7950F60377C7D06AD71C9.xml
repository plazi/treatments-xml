<paragraph id="13AA78BA2CCF4FB05731AAEE9A2199A5" pageId="16" pageNumber="17">
<taxonomicName id="EBC21B7E45542AB7DF45E874E4BB6069" class="Arachnida" family="Linyphiidae" genus="Glebala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glebala aspera" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="aspera">Glebala aspera</taxonomicName>
