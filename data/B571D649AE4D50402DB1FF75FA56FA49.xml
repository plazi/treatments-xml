<keyLead id="8629C46AAE4D50402DB1FB82FA56FB49" pageId="30" pageNumber="31">
<number id="8B107578AE4D50402DB1FB82FF6EFB89" box="[151,164,1120,1143]" pageId="30" pageNumber="31" value="7.0">7</number>
