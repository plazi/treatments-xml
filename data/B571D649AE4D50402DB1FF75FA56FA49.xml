<quantity id="FA20CABAAE4D50402E8EFB62FB8DFB69" box="[936,1095,1152,1175]" metricMagnitude="-3" metricUnit="m" metricValue="6.25" metricValueMax="7.15" metricValueMin="5.35" pageId="30" pageNumber="31" unit="mm" value="6.25" valueMax="7.15" valueMin="5.35">5.35–7.15 mm</quantity>
