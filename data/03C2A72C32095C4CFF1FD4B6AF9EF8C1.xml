<taxonomicName id="4C6B6DB932095C4CFDC8D677AF9EF8C1" authority="(Molina) Standl." authorityName="Standl." baseAuthorityName="Molina" box="[535,982,1858,1882]" class="Magnoliopsida" family="Cucurbitaceae" genus="Lagenaria" kingdom="Plantae" order="Cucurbitales" pageId="17" pageNumber="16" phylum="Tracheophyta" rank="species" species="siceraria">
