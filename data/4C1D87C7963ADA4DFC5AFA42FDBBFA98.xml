<taxonomicName id="03B44D529639DA4DFBC2FF57FB43FF72" baseAuthorityName="Pallas" baseAuthorityYear="1769" box="[1057,1172,169,192]" class="Aves" family="Diomedeidae" genus="Phoebastria" kingdom="Animalia" order="Procellariiformes" pageId="6" pageNumber="163" phylum="Chordata" rank="species" species="albatrus">
