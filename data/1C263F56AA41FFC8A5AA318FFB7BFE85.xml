<taxonomicName id="538FF5C3AA42FFC8A1193649FB64FEAE" authorityName="Motschulsky" authorityYear="1860" box="[1060,1209,303,327]" class="Insecta" family="Staphylinidae" genus="Lesteva" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="cordicollis">
