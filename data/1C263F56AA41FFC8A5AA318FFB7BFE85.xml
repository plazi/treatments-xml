<taxonomicName id="538FF5C3AA42FFC8A62C3791FC76FF16" authorityName="Motschulsky" authorityYear="1860" box="[785,939,231,255]" class="Insecta" family="Staphylinidae" genus="Lesteva" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="cordicollis">
