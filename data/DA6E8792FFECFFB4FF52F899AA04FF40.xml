<taxonomicName id="95C74D07FFEDFFB4FEB4FF6FA90BFF24" box="[369,552,151,176]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="49" phylum="Arthropoda" rank="species" species="ruficorne" subGenus="Nevermannia">
<emphasis id="60B3EA96FFEDFFB4FEB4FF6FAA5CFF3B" box="[369,383,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
. (
<emphasis id="60B3EA96FFEDFFB4FE53FF6EAA8AFF24" box="[406,425,153,176]" italics="true" pageId="3" pageNumber="49">N</emphasis>
.) 
<emphasis id="60B3EA96FFEDFFB4FE05FF6FA90BFF24" box="[448,552,152,176]" italics="true" pageId="3" pageNumber="49">ruficorne</emphasis>
</taxonomicName>
and 
<taxonomicName id="95C74D07FFEDFFB4FD9AFF6FA820FF24" box="[607,771,151,176]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="49" phylum="Arthropoda" rank="species" species="vernum" subGenus="Nevermannia">
<emphasis id="60B3EA96FFEDFFB4FD9AFF6FA94EFF3B" box="[607,621,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
. (
<emphasis id="60B3EA96FFEDFFB4FD40FF6EA9BBFF24" box="[645,664,153,176]" italics="true" pageId="3" pageNumber="49">N</emphasis>
.) 
<emphasis id="60B3EA96FFEDFFB4FD6AFF6EA820FF24" box="[687,771,153,176]" italics="true" pageId="3" pageNumber="49">vernum</emphasis>
</taxonomicName>
