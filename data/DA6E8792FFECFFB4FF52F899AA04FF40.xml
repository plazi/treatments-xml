In 
<collectingCountry id="2AD07614FFECFFB5FF7DF841AA03F859" box="[184,288,1974,1997]" name="Morocco" pageId="2" pageNumber="48">Morocco</collectingCountry>
, the genus 
<taxonomicName id="95C74D07FFECFFB5FE60F842A92CF859" box="[421,527,1973,1997]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FE60F842A92CF859" box="[421,527,1973,1997]" italics="true" pageId="2" pageNumber="48">Simulium</emphasis>
</taxonomicName>
is represented by seven subgenera: 
<emphasis id="60B3EA96FFECFFB5FC76F842AF75F859" box="[947,1110,1973,1997]" italics="true" pageId="2" pageNumber="48">Crosskeyellum</emphasis>
, 
<taxonomicName id="95C74D07FFECFFB5FBA2F842AFCEF859" authority="Roubaud, 1906" authorityName="Roubaud" authorityYear="1906" box="[1127,1261,1973,1997]" class="Insecta" family="Simuliidae" genus="Eusimulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FBA2F842AFCEF859" box="[1127,1261,1973,1997]" italics="true" pageId="2" pageNumber="48">Eusimulium</emphasis>
</taxonomicName>
, 
<taxonomicName id="95C74D07FFECFFB5FB3BF841AEB6F859" authority="Enderlein, 1921" authorityName="Enderlein" authorityYear="1921" box="[1278,1429,1974,1997]" class="Insecta" family="Simuliidae" genus="Nevermannia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FB3BF841AEB6F859" box="[1278,1429,1974,1997]" italics="true" pageId="2" pageNumber="48">Nevermannia</emphasis>
</taxonomicName>
, 
<taxonomicName id="95C74D07FFECFFB5FF52F82EAA31F865" authority="Rubtzov, 1951" authorityName="Rubtzov" authorityYear="1951" box="[151,274,2009,2033]" class="Insecta" family="Simuliidae" genus="Obuchovia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FF52F82EAA31F865" box="[151,274,2009,2033]" italics="true" pageId="2" pageNumber="48">Obuchovia</emphasis>
</taxonomicName>
, 
<taxonomicName id="95C74D07FFECFFB5FEE6F82EAAA8F865" box="[291,395,2009,2033]" class="Insecta" family="Simuliidae" genus="Rubzovia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FEE6F82EAAA8F865" box="[291,395,2009,2033]" italics="true" pageId="2" pageNumber="48">Rubzovia</emphasis>
</taxonomicName>
, 
<taxonomicName id="95C74D07FFECFFB5FE59F82EA925F865" box="[412,518,2009,2033]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FE59F82EA925F865" box="[412,518,2009,2033]" italics="true" pageId="2" pageNumber="48">Simulium</emphasis>
</taxonomicName>
, and 
<taxonomicName id="95C74D07FFECFFB5FD8DF82EA99BF865" authority="Enderlein, 1921" authorityName="Enderlein" authorityYear="1921" box="[584,696,2009,2033]" class="Insecta" family="Simuliidae" genus="Wilhelmia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
<emphasis id="60B3EA96FFECFFB5FD8DF82EA99BF865" box="[584,696,2009,2033]" italics="true" pageId="2" pageNumber="48">Wilhelmia</emphasis>
</taxonomicName>
. 
<taxonomicName id="95C74D07FFECFFB5FD0EF82DA841F865" authority="Enderlein, 1921" authorityName="Enderlein" authorityYear="1921" box="[715,866,2010,2033]" class="Insecta" family="Simuliidae" genus="Nevermannia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
