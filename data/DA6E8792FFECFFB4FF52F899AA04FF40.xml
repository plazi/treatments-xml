<taxonomicName id="95C74D07FFEDFFB4FBB2FF6FAE01FF24" box="[1143,1314,151,176]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="49" phylum="Arthropoda" rank="species" species="ornatum" subGenus="Simulium">
<emphasis id="60B3EA96FFEDFFB4FBB2FF6FAFA6FF3B" box="[1143,1157,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
