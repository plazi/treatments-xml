</taxonomicName>
and 
<taxonomicName id="95C74D07FFECFFB5FC5BF82EAF24F865" box="[926,1031,2009,2033]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="48" phylum="Arthropoda" rank="genus">
