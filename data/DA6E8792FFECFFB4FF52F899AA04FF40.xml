<emphasis id="60B3EA96FFEDFFB4FB58FF6FAF88FF3B" box="[1181,1195,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
.) 
<emphasis id="60B3EA96FFEDFFB4FB06FF6EAE01FF24" box="[1219,1314,153,176]" italics="true" pageId="3" pageNumber="49">ornatum</emphasis>
</taxonomicName>
and 
<taxonomicName id="95C74D07FFEDFFB4FA9DFF6FAA34FF40" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="49" phylum="Arthropoda" rank="species" species="variegatum" subGenus="Simulium">
<emphasis id="60B3EA96FFEDFFB4FA9DFF6FAE45FF3B" box="[1368,1382,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
. (
<emphasis id="60B3EA96FFEDFFB4FAB8FF6FAEA8FF3B" box="[1405,1419,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
.) 
<emphasis id="60B3EA96FFEDFFB4FF52FF4AAA34FF40" box="[151,279,189,212]" italics="true" pageId="3" pageNumber="49">variegatum</emphasis>
</taxonomicName>
).
