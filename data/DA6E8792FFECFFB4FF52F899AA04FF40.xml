<taxonomicName id="95C74D07FFEDFFB4FC1AFF6FAF49FF24" box="[991,1130,151,176]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="49" phylum="Arthropoda" rank="species" species="bezzii" subGenus="Simulium">
<emphasis id="60B3EA96FFEDFFB4FC1AFF6FA8CEFF3B" box="[991,1005,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
. (
<emphasis id="60B3EA96FFEDFFB4FBC1FF6FAF31FF3B" box="[1028,1042,152,175]" italics="true" pageId="3" pageNumber="49">S</emphasis>
.) 
<emphasis id="60B3EA96FFEDFFB4FBEFFF6FAF49FF24" box="[1066,1130,152,176]" italics="true" pageId="3" pageNumber="49">bezzii</emphasis>
</taxonomicName>
