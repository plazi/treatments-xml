<taxonomicName id="92A3338109A5A7521FA254494EA98DC2" authorityName="Wang &amp; Wang" authorityYear="2021" class="Insecta" family="Blaberidae" genus="Cyrtonotula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyrtonotula maculosa" order="Blattodea" pageId="0" pageNumber="127" phylum="Arthropoda" rank="species" species="maculosa">
<emphasis id="46ECA9ADC68F05F01645839A1345BD87" italics="true" pageId="0" pageNumber="127">Cyrtonotula maculosa</emphasis>
