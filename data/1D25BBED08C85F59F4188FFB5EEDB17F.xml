<taxonomicName id="8159557DF3A756AAA3298299B81EF660" LSID="http://zoobank.org/5E10353D-E093-4A12-886F-C92596DEE86E" authority="Kang" authorityName="Förster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lytopylus sigifredomarini" order="Hymenoptera" pageId="47" pageNumber="140" phylum="Arthropoda" rank="species" species="sigifredomarini">Lytopylus sigifredomarini Kang</taxonomicName>
