3. 
<taxonomicName id="374F3DCFE93633EB3F163497654443E0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179402" authority="Agassiz, 1846" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster grevei Forel" order="Hymenoptera" pageNumber="183" phylum="Arthropoda" rank="species" species="grevei" status="nov. sp.">CREMASTOGASTER GREVEI</taxonomicName>
