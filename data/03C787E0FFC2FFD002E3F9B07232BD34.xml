<paragraph id="8BD136F6FFCCFFD00245FF287232BD34" blockId="25.[809,1425,197,557]" pageId="25">
<taxonomicName id="4C6E4D75FFCCFFD00245FF2874BDBFC3" authorityName="Redtenbacher" authorityYear="1906" box="[809,958,197,218]" class="Insecta" family="Pseudophasmatidae" genus="Paraphasma" kingdom="Animalia" order="Phasmida" pageId="25" phylum="Arthropoda" rank="genus">
<emphasis id="B91AEAE4FFCCFFD00245FF2874BDBFC3" box="[809,958,197,218]" italics="true" pageId="25">Paraphasma</emphasis>
</taxonomicName>
