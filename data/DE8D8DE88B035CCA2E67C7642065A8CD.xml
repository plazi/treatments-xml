<taxonomicName id="CC80184114FBD0E79F5C5DDDB35EE078" class="Arachnida" family="Galumnidae" genus="Galumna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Galumna granalata" order="Sarcoptiformes" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="granalata">Galumna granalata</taxonomicName>
