<taxonomicName id="4FC4E690387EAE06FAE46D81FA75FE10" authorityName="Jones, Cheng &amp; Gillespie" authorityYear="1958" box="[1296,1426,441,467]" class="Cestoda" family="Proteocephalidae" genus="Ophiotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Onchoproteocephalidea" pageId="21" pageNumber="22" phylum="Platyhelminthes" rank="species" species="gracilis">
<emphasis id="BAB04101387EAE06FAE46D81FAC2FE10" box="[1296,1317,441,466]" italics="true" pageId="21" pageNumber="22">O</emphasis>
