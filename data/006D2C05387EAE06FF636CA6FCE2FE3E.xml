<taxonomicName id="4FC4E690387EAE06FD7F6DA9FC9CFE68" authorityName="Hannum" authorityYear="1925" box="[651,891,401,426]" class="Cestoda" family="Proteocephalidae" genus="Ophiotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Onchoproteocephalidea" pageId="21" pageNumber="22" phylum="Platyhelminthes" rank="species" species="magna">
