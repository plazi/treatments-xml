<taxonomicName id="4FC4E690387EAE06FC266DA9FB15FE68" box="[978,1266,401,426]" class="Amphibia" family="Ranidae" genus="Lithobates" kingdom="Animalia" order="Anura" pageId="21" pageNumber="22" phylum="Chordata" rank="species" species="catesbeianus">
