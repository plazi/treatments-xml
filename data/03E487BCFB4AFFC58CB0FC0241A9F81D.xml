<taxonomicName id="4C4D4D29FB4BFFC58E20F8C147CEF8C2" box="[519,642,1846,1869]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="14" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
