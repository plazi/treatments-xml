<taxonomicName id="27CA4D35FF5BFF3D5A96FCCFFC206225" authorityName="Rondani" authorityYear="1862" box="[648,915,834,860]" class="Insecta" family="Muscidae" genus="Mesembrina" kingdom="Animalia" order="Diptera" pageId="150" pageNumber="151" phylum="Arthropoda" rank="species" species="vespertina">
