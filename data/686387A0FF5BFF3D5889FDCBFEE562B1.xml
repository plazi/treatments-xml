<taxonomicName id="27CA4D35FF5BFF3D5C45FCEBFB2262F9" box="[1115,1169,870,896]" class="Insecta" family="Lauxaniidae" genus="Rarissimo" kingdom="Animalia" order="Diptera" pageId="150" pageNumber="151" phylum="Arthropoda" rank="species" species="lecta">lecta</taxonomicName>
