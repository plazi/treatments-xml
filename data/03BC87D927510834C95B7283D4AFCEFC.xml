<taxonomicName id="4C154D4C27520835CEC2749FD7AACE77" authority="Dommergues &amp; Goolaerts, 2012" authorityName="Dommergues &amp; Goolaerts" authorityYear="2012" class="Cephalopoda" genus="Oxyarietites" kingdom="Animalia" order="Ammonoidea" pageId="6" pageNumber="523" phylum="Mollusca" rank="species" species="boletzkyi" status="n. gen., n. sp.">
