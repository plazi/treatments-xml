<taxonomicName id="44A84D58635CFFBAFE61F9B6FD61FC54" baseAuthorityName="Bigot" baseAuthorityYear="1880" box="[413,724,906,933]" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="55" phylum="Arthropoda" rank="species" species="mira" subGenus="Enoplempis">
