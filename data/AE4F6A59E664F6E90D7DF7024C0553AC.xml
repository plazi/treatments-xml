<taxonomicName id="AC63027C6F0F1CAEC098D53CB86C39B3" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="subFamily" subFamily="Mimosoideae">Mimosoideae</taxonomicName>
