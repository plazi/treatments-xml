<quantity id="4CDC9B75FFCFFFE2FCB0EE8AFC058F83" box="[854,964,1761,1785]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.5" metricValueMin="0.5" pageId="48" pageNumber="49" unit="mm" value="10.0" valueMax="15.0" valueMin="5.0">5–15 mm</quantity>
