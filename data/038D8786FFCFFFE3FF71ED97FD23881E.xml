<paragraph id="8B9B3690FFCFFFE2FF2FEF6EFCCB8E3B" blockId="48.[151,1436,1761,2037]" pageId="48" pageNumber="49">
<emphasis id="B950EA82FFCFFFE2FF2FEF6EFE788E64" bold="true" box="[201,441,1797,1822]" inLineHeading="true" pageId="48" pageNumber="49" reason="1">Material examined.</emphasis>
