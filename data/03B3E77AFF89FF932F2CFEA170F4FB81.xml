<collectingCountry id="F30D16FCFF89FF932EA0FB9D737DFBE1" box="[213,299,1038,1062]" name="Indonesia" pageId="6" pageNumber="733">Ambon</collectingCountry>
(
<bibRefCitation id="EF8B2B9DFF89FF932F4FFB9D704CFBE1" author="Van der Horst CJ" box="[314,538,1038,1062]" pageId="6" pageNumber="733" pagination="45 - 75" refId="ref8128" refString="Van der Horst CJ. 1922. The Madreporaria of the Siboga Expedition, Part 3: Eupsammidae. Siboga-Expeditie 16: 45 - 75." type="journal article" year="1922">van der Horst 1922</bibRefCitation>
), 
<collectingCountry id="F30D16FCFF89FF932C58FB9D7027FBE1" box="[557,625,1038,1062]" name="Japan" pageId="6" pageNumber="733">Japan</collectingCountry>
(
<bibRefCitation id="EF8B2B9DFF89FF932CF5FB9D71E9FBE1" author="Ogawa K &amp; Takahashi K" box="[640,959,1038,1062]" pageId="6" pageNumber="733" pagination="15 - 33" refId="ref7806" refString="Ogawa K, Takahashi K. 1995. A revision of Japanese ahermatypic corals around the coastal region with a guide to identification- II. Genus Dendrophyllia. Nankiseibutu 37: 15 - 33." type="journal article" year="1995">Ogawa and Takahashi 1995</bibRefCitation>
) and Conic Island marine cave in 
<collectingCountry id="F30D16FCFF89FF932FF3FBBD7059FB81" box="[390,527,1070,1094]" name="Hong Kong" pageId="6" pageNumber="733">Hong Kong</collectingCountry>
