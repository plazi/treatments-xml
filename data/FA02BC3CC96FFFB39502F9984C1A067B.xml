<taxonomicName id="B5AB76A9C96FFFB296FDF9464C0206E0" baseAuthorityName="Bezzi" baseAuthorityYear="1928" box="[439,599,1754,1781]" class="Insecta" family="Syrphidae" genus="Allograpta" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="amphotera">
