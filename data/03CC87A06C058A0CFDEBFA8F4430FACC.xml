<taxonomicName id="4C654D356C068A0DFDE7F84B459FF80D" baseAuthorityName="Gude" baseAuthorityYear="1897" box="[543,662,1931,1957]" class="Gastropoda" family="Plectopylidae" genus="Plectopylis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="magna">
<emphasis id="B911EAA46C068A0DFDE7F84B459FF80D" box="[543,662,1931,1957]" italics="true" pageId="42" pageNumber="43">C. magna</emphasis>
