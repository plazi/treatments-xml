<taxonomicName id="E4A497246FB49F5564C5A0EC646931EF" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina cattaroensis" order="Stylommatophora" pageId="12" pageNumber="13" phylum="Mollusca" rank="subSpecies" species="cattaroensis" subSpecies="antivaricostata">Montenegrina cattaroensis antivaricostata</taxonomicName>
