<subSubSection id="6FB61FCB6F28FFE33281F9C87A28F906" pageId="3" pageNumber="40" type="etymology">
<paragraph id="27134C406F28FFE33281F9C87A28F906" blockId="3.[151,1436,1526,1992]" pageId="3" pageNumber="40">
<emphasis id="15D890526F28FFE33281F9C87A6FF9EE" bold="true" box="[197,331,1606,1632]" pageId="3" pageNumber="40">
<emphasis id="15D890526F28FFE33281F9C87A60F9EE" bold="true" box="[197,324,1606,1632]" italics="true" pageId="3" pageNumber="40">Etymology</emphasis>
