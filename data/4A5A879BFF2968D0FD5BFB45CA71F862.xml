<collectingRegion id="0037F86FFF2968D0FDB7F9CBCD91F9B4" box="[576,646,1571,1597]" country="New Zealand" name="West Coast" pageId="175" pageNumber="176">Coast</collectingRegion>
, 
<geoCoordinate id="A7C7504AFF2968D0FD64F9C9CDECF9B4" box="[659,763,1569,1597]" degrees="66" direction="south" minutes="38" orientation="latitude" pageId="175" pageNumber="176" precision="925" value="-66.63333">66°38ʹ S</geoCoordinate>
, 
<geoCoordinate id="A7C7504AFF2968D0FCFEF9C9CC97F9B7" box="[777,896,1569,1598]" degrees="140" direction="east" minutes="42" orientation="longitude" pageId="175" pageNumber="176" precision="925" value="140.7">140°42ʹ E</geoCoordinate>
to 
<geoCoordinate id="A7C7504AFF2968D0FC5FF9C9CB07F9B4" box="[936,1040,1569,1597]" degrees="66" direction="south" minutes="38" orientation="latitude" pageId="175" pageNumber="176" precision="925" value="-66.63333">66°38ʹ S</geoCoordinate>
, 
<geoCoordinate id="A7C7504AFF2968D0FBE9F9C9CB81F9B7" box="[1054,1174,1569,1598]" degrees="140" direction="east" minutes="40" orientation="longitude" pageId="175" pageNumber="176" precision="925" value="140.66667">140°40ʹ E</geoCoordinate>
, 
<quantity id="050B9B68FF2968D0FB53F9CBCA3AF9B7" box="[1188,1325,1571,1598]" metricMagnitude="2" metricUnit="m" metricValue="7.234999999999999" metricValueMax="7.29" metricValueMin="7.18" pageId="175" pageNumber="176" unit="m" value="723.5" valueMax="729.0" valueMin="718.0">718–729 m</quantity>
, mud, beam trawl, 
<date id="B64D104DFF2968D0FEB8F9A2CEF0F9ED" box="[335,487,1610,1636]" pageId="175" pageNumber="176" value="2012-01-20">20 Jan. 2012</date>
, CE-000002100 (MNHN-IU- 
<date id="B64D104DFF2968D0FCB0F9A2CCDFF9EC" box="[839,968,1610,1637]" pageId="175" pageNumber="176" value="2009" valueMax="2540">2009-2540</date>
