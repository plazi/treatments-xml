<taxonomicName id="4C2354C8EC64FFD9FDF4FE75FD04FDBD" box="[561,659,506,532]" class="Amphibia" family="Mantellidae" genus="Boophis" kingdom="Animalia" order="Anura" pageId="11" pageNumber="34" phylum="Chordata" rank="species" species="tadpoles">tadpoles</taxonomicName>
