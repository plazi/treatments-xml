<taxonomicName id="4C2B4D69FFDDC234576AFB6DFCA307BB" authority="(Loew, 1869)" baseAuthorityName="Loew" baseAuthorityYear="1869" box="[586,805,1185,1209]" class="Insecta" family="Tephritidae" genus="Tephritomyia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="624" phylum="Arthropoda" rank="species" species="lauta">
