<number id="78435C01FFD4B83DFE1DF9CDFD9F375A" box="[488,539,1592,1616]" pageId="5" pageNumber="246" value="82.1">82.1</number>
, female 
<number id="78435C01FFD4B83DFD8EF9CDFD34375A" box="[635,688,1592,1616]" pageId="5" pageNumber="246" value="87.2">87.2</number>
