<number id="78435C01FFD4B83DFC4CFA39FC6834EE" box="[953,1004,1484,1508]" pageId="5" pageNumber="246" value="18.7">18.7</number>
, female 
<number id="78435C01FFD4B83DFBB8FA39FB0534EE" box="[1101,1153,1484,1508]" pageId="5" pageNumber="246" value="17.8">17.8</number>
