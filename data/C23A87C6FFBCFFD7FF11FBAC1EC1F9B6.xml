<quantity id="8D6B9B35FFBCFFD7FC90FAA41F5EFAB3" box="[790,856,1332,1357]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="15" pageNumber="16" unit="mm" value="5.0">5 mm</quantity>
