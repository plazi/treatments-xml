<quantity id="4CE99B5B5632D01DFC89EAF8FCF8E8F9" box="[811,849,912,942]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" pageId="3" pageNumber="60" unit="in" value="3.0">
