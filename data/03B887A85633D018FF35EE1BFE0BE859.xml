</quantity>
postanal, setae 
<emphasis id="B965EAAC5632D01DFE79EAC8FE51E8FA" box="[475,504,912,937]" italics="true" pageId="3" pageNumber="60">ad</emphasis>
<quantity id="4CE99B5B5632D01DFE5AEAF8FDB7E8F9" box="[504,542,912,942]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="3" pageNumber="60" unit="in" value="2.0">
<emphasis id="B965EAAC5632D01DFE5AEAF8FDA9E8FD" box="[504,512,928,942]" italics="true" pageId="3" pageNumber="60">2</emphasis>
