<emphasis id="B9667641FFEAFFA7FF658E3AFE2DFAB3" box="[189,393,1369,1396]" italics="true" pageId="30" pageNumber="31">
<taxonomicName id="4C12D1D0FFEAFFA7FF658E3AFEA0FAB4" box="[189,260,1370,1396]" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
