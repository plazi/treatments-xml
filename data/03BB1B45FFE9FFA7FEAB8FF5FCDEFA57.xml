<quantity id="4CEA07B6FFEAFFA7FBAF8E39FB7EFAB4" box="[1143,1242,1369,1396]" metricMagnitude="3" metricUnit="m" metricValue="3.8339999999999996" pageId="30" pageNumber="31" unit="m" value="3834.0">3834 m</quantity>
