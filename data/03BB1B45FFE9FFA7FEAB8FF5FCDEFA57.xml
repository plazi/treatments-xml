<paragraph id="8BADAA53FFEAFFA7FF658E56FCDEFA57" blockId="30.[189,1398,1334,1431]" pageId="30" pageNumber="31">
<taxonomicName id="4C12D1D0FFEAFFA7FF658E56FE7AFA90" authorityName="Sinev &amp; Dumont" authorityYear="2016" baseAuthorityName="Van Damme &amp; Eggermont" baseAuthorityYear="2011" box="[189,478,1334,1360]" class="Branchiopoda" family="Chydoridae" genus="Flavalona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anomopoda" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="sphagnophila" status="comb. nov.">
<emphasis id="B9667641FFEAFFA7FF658E56FE7AFA90" box="[189,478,1334,1360]" italics="true" pageId="30" pageNumber="31">Flavalona sphagnophila</emphasis>
</taxonomicName>
