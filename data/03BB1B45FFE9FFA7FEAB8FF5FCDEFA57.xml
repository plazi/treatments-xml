<quantity id="4CEA07B6FFEAFFA7FCC58E39FC24FAB4" box="[797,896,1369,1396]" metricMagnitude="3" metricUnit="m" metricValue="4.017" pageId="30" pageNumber="31" unit="m" value="4017.0">4017 m</quantity>
