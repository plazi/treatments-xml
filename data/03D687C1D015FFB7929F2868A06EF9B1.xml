<collectingCountry id="F3687647D015FFB790BE2A58A1BEF9B1" box="[659,736,1584,1605]" name="Oman" pageId="21" pageNumber="410">Muscat</collectingCountry>
, 
<collectingCountry id="F3687647D015FFB790C72A58A075F9B1" box="[746,811,1584,1605]" name="Oman" pageId="21" pageNumber="410">Oman</collectingCountry>
