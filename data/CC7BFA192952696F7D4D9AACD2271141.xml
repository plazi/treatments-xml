<taxonomicName id="2B1F63C1E1F8DD3BDE7622C4D682DE46" genus="A." lsidName="A. octospinata" pageId="0" pageNumber="137" rank="species" species="octospinata">A. octospinata</taxonomicName>
, mature female (QM W2119) and 
<taxonomicName id="A8CD28F23D75370ED9DDA6460D078E37" genus="A." lsidName="A. montana" pageId="0" pageNumber="137" rank="species" species="montana">A. montana</taxonomicName>
