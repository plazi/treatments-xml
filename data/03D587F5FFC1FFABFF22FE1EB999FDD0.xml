<collectingCountry id="F36B7673FFCEFFABFBD8FEC6BECDFE89" box="[1133,1194,330,356]" name="India" pageId="8" pageNumber="64">India</collectingCountry>
; and 
<taxonomicName id="4C7C4D60FFCEFFABFB59FEC6BFF2FE89" authorityName="Webster" authorityYear="1879" box="[1260,1429,330,356]" class="Polychaeta" family="Pilargidae" genus="Cabira" kingdom="Animalia" order="Phyllodocida" pageId="8" pageNumber="64" phylum="Annelida" rank="species" species="incerta">
<emphasis id="B908EAF1FFCEFFABFB59FEC6BFF2FE89" box="[1260,1429,330,356]" italics="true" pageId="8" pageNumber="64">Cabira incerta</emphasis>
</taxonomicName>
, from the Gulf of Mexico, 
<collectingCountry id="F36B7673FFCEFFABFE7DFEE2B863FE65" box="[456,516,366,392]" name="United States of America" pageId="8" pageNumber="64">USA</collectingCountry>
