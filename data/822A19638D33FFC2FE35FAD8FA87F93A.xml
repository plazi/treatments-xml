<taxonomicName id="CD83D3F68D33FFC2FD29FADFFC9AFAD5" box="[733,822,1356,1384]" class="Mammalia" family="Hominidae" genus="Homo" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Primates" pageId="26" pageNumber="27" phylum="Chordata" rank="genus">
