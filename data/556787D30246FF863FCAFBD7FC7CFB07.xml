<collectionCode id="BBDFAE000245FF863DDCFE14FCD7FE67" box="[726,808,482,506]" country="New Zealand" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" lsid="urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="26" pageNumber="25" type="Other institutional type">NZAC</collectionCode>
