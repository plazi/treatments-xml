<taxonomicNameLabel id="A22657CAFFF8FF8057BBFEDDFD40FE15" box="[641,703,359,385]" pageId="15" pageNumber="16" rank="species">sp. n.</taxonomicNameLabel>
and 
<taxonomicName id="4C614D20FFF8FF8057C2FEDDFC59FE14" box="[760,934,359,384]" class="Arachnida" family="Stigmaeidae" genus="Eustigmaeus" kingdom="Animalia" order="Prostigmata" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="microsegnis">
