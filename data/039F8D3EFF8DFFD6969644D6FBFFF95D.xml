<subSubSection id="C32C6FA3FF8DFFD69696433CFB88FB2D" pageId="48" pageNumber="349" type="materials_examined">
<paragraph id="8B893C28FF8DFFD69696433CFF73FBB1" blockId="48.[151,1436,1086,1687]" pageId="48" pageNumber="349">
<emphasis id="B942E03AFF8DFFD69696433CFE3FFB9D" bold="true" box="[151,441,1086,1111]" pageId="48" pageNumber="349">
<typeStatus id="548D828AFF8DFFD69696433CFF54FB9D" box="[151,210,1086,1111]" pageId="48" pageNumber="349">Type</typeStatus>
