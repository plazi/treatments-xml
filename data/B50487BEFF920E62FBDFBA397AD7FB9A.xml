<taxonomicName id="FAAD4D2BFF9F0E62FFD4BF827D45FBEA" box="[1180,1359,1014,1039]" class="Insecta" family="Dolichopodidae" genus="Chrysotus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="57" phylum="Arthropoda" rank="species" species="angustifrons">
