<number id="8B65248FFF900E6DFF03BBCA7C78F83D" box="[1099,1138,1982,2008]" pageId="7" pageNumber="56" value="0.3">0.3</number>
, 
<number id="8B65248FFF900E6DFF36BBCA7CACF83D" box="[1150,1190,1982,2008]" pageId="7" pageNumber="56" value="0.2">0.2</number>
. II: 
<number id="8B65248FFF900E6DFF9DBBCA7CF7F83D" box="[1237,1277,1982,2008]" pageId="7" pageNumber="56" value="2.9">2.9</number>
, 
<number id="8B65248FFF900E6DFE41BBCA7D3BF83D" box="[1289,1329,1982,2008]" pageId="7" pageNumber="56" value="3.2">3.2</number>
, 2.0, 1.0, 
<number id="8B65248FFF9F0E62FBDFBCEC78CAFF57" box="[151,192,152,178]" pageId="8" pageNumber="57" value="0.7">0.7</number>
, 
<number id="8B65248FFF9F0E62FB85BCEC78FCFF57" box="[205,246,152,178]" pageId="8" pageNumber="57" value="0.3">0.3</number>
