<collectingCountry id="F358767E2923FFD6781316539EA7FEAB" box="[600,696,268,294]" name="Panama" pageId="100" pageNumber="101">Panamá</collectingCountry>
; 
<collectingCountry id="F358767E2923FFD6788D16539F38FEAB" box="[710,807,268,294]" name="Jamaica" pageId="100" pageNumber="101">Jamaica</collectingCountry>
, 
<collectingCountry id="F358767E2923FFD6797E16539F61FEAB" box="[821,894,268,294]" name="Aruba" pageId="100" pageNumber="101">Aruba</collectingCountry>
and 
<collectingCountry id="F358767E2923FFD679F116539804FEAB" box="[954,1051,268,294]" name="Curaçao" pageId="100" pageNumber="101">Curaçao</collectingCountry>
(Stock 1975), 
<collectingCountry id="F358767E2923FFD67E8516539959FEAB" box="[1230,1350,268,294]" name="Venezuela" pageId="100" pageNumber="101">Venezuela</collectingCountry>
(
<bibRefCitation id="EFDE4B1F2923FFD67F1316539CF7FEC1" author="Stock, J. H." pageId="100" pageNumber="101" pagination="1 - 32" refId="ref77628" refString="Stock, J. H. (1979) Pycnogonida from the mediolittoral and infralittoral zones in the tropical Western Atlantic. Studies in the Fauna of Curacao and other Caribbean Islands, 59 (184), 1 - 32." type="journal article" year="1979">Stock (1979)</bibRefCitation>
