<taxonomicName id="44ADA3CBFFF36645FB4CF8EFFC4FF8EE" authority="Dlussky, Wappler" authorityName="Dlussky, Wappler" class="Insecta" family="Formicidae" genus="Gesomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="137" phylum="Arthropoda" rank="species" species="germanicus">
