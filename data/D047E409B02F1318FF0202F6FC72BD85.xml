XVIII 
<number id="EE264738B02F1318FDCB0165FD80BA9D" box="[606,634,620,645]" pageId="3" pageNumber="274" value="36.0">36</number>
(
<number id="EE264738B02F1318FD180165FD63BA9C" box="[653,665,620,644]" pageId="3" pageNumber="274" value="1.0">1</number>
spcm.), 
