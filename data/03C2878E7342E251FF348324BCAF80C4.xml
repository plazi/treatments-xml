<taxonomicName id="4C6B4D1B7342E251FF3483EDBB91838A" authority="Priesner, 1939 a: 130" authorityName="Priesner" authorityPageNumber="130" authorityYear="1939" box="[151,702,1194,1216]" class="Insecta" family="Thripidae" genus="Anaphothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="440" phylum="Arthropoda" rank="subGenus" subGenus="Hyalopterothrips">
<emphasis id="B91FEA8A7342E251FF3483EDB80E838A" box="[151,289,1194,1216]" italics="true" pageId="3" pageNumber="440">Anaphothrips</emphasis>
(
<emphasis id="B91FEA8A7342E251FE8C83EDB8F3838A" box="[303,476,1194,1216]" italics="true" pageId="3" pageNumber="440">Hyalopterothrips</emphasis>
) 
<treatmentCitation id="0ACA10897342E251FE4F83EDBB91838A" author="" box="[492,702,1194,1216]" page="130" pageId="3" pageNumber="440">
<bibRefCitation id="EFFA4B697342E251FE4F83EDBBA3838A" box="[492,652,1194,1216]" pageId="3" pageNumber="440" refId="ref4381" refString="Priesner, H. (1939 a) Contributions towards a knowledge of the Thysanoptera of Egypt, XII. Bulletin de la Societe Royal Entomologique d'Egypte, 22, 123 - 132." type="journal article">Priesner, 1939a</bibRefCitation>
