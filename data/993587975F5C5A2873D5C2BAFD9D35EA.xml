<taxonomicName id="D69C4D025F5E5A2F713FC57BFCB9336F" authority="Hemp, 2020" authorityName="Hemp" authorityYear="2020" box="[637,802,1880,1907]" class="Insecta" family="Phaneropteridae" genus="Apteroscirtus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="141" phylum="Arthropoda" rank="species" species="densissimus" status="n. sp.">
