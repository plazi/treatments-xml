<taxonomicName id="D69C4D025F5E5A2F760BC57BFEF8338B" authority="Hemp, 2013" authorityName="Hemp" authorityYear="2013" class="Insecta" family="Phaneropteridae" genus="Apteroscirtus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="141" phylum="Arthropoda" rank="species" species="cristatus">
<emphasis id="23E8EA935F5E5A2F760BC57BFF56338B" italics="true" pageId="3" pageNumber="141">A. cristatus</emphasis>
<bibRefCitation id="750D4B705F5E5A2F7395C55FFEF8338B" author="Hemp, C." box="[215,355,1916,1943]" pageId="3" pageNumber="141" pagination="301 - 342" refId="ref4372" refString="Hemp, C. (2013) Annotated list of Ensifera (Orthoptera) and further records on Caelifera (Orthoptera) of Mt Kilimanjaro, Tanzania. Zootaxa, 3613 (4), 301 - 342. https: // doi. org / 10.11646 / zootaxa. 3613.4.1" type="journal article" year="2013">Hemp, 2013</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="D69C4D025F5E5A2F72DCC55FFD75338B" authority="Hemp, 2013" authorityName="Hemp" authorityYear="2013" box="[414,750,1916,1943]" class="Insecta" family="Phaneropteridae" genus="Apteroscirtus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="141" phylum="Arthropoda" rank="species" species="planidorsatus">
