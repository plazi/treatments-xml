<taxonomicName id="D69C4D025F5E5A2F7678C583FF6C33C3" class="Insecta" family="Phaneropteridae" genus="Apteroscirtus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="141" phylum="Arthropoda" rank="species" species="planidorsatus">
