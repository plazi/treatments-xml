<quantity id="4C9C9B30FFBB1749FDDBF9EAEB9B9605" box="[543,645,1564,1588]" metricMagnitude="-3" metricUnit="m" metricValue="1.42" pageId="16" pageNumber="2004" unit="mm" value="1.42">1.42 mm</quantity>
