<taxonomicName id="0B8A61C7218F02BF04B97BE5B524EA0F" class="Insecta" family="Curculionidae" genus="Pyrodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyrodes" order="Coleoptera" pageId="199" pageNumber="200" phylum="Arthropoda" rank="genus">Pyrodes</taxonomicName>
