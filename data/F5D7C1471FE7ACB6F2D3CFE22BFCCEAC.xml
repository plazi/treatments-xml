<taxonomicName id="0068F5A250497CC8FB6503F9626F8CAF" genus="Aprionomastix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aprionomastix fasciatipennis" pageId="0" pageNumber="7326" rank="species" species="fasciatipennis">Aprionomastix fasciatipennis</taxonomicName>
