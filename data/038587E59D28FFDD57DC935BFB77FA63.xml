<subSubSection id="C33665789D2FFFDD578C9034FC9AFAB7" box="[199,844,1305,1331]" pageId="52" pageNumber="53" type="etymology">
<paragraph id="8B9336F39D2FFFDD578C9034FC9AFAB7" blockId="52.[151,1437,153,1511]" box="[199,844,1305,1331]" pageId="52" pageNumber="53">
<emphasis id="B958EAE19D2FFFDD578C9034FE98FAB7" bold="true" box="[199,334,1305,1331]" pageId="52" pageNumber="53">Etymology.</emphasis>
