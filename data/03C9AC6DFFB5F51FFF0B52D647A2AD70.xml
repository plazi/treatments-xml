<quantity id="4C98B09EFFB5F51CFC18534D47ABAF7E" box="[900,992,1831,1855]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="28" pageNumber="379" unit="mm" value="1.9">1.9 mm</quantity>
