<taxonomicName id="270BF2A296886365FE35550ACB1C67EF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181992" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole mamore Mann" lsidName-HNS="Pheidole mamore Mann" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="mamore">mamore</taxonomicName>
, 
<taxonomicName id="2651A108B292DCFEB6682A2EF1D8B6A4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182067" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole praeses Wilson" lsidName-HNS="Pheidole praeses Wilson" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="praeses">praeses</taxonomicName>
