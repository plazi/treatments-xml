, 
<taxonomicName id="F77850A16F9BF4AF31B90F2A50AED3A9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182018" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole bucculenta Forel" lsidName-HNS="Pheidole bucculenta Forel" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="bucculenta">bucculenta</taxonomicName>
