<taxonomicName id="5E618147481C55A62B5EB2BCA0E082F9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182045" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hortonae Wilson" lsidName-HNS="Pheidole hortonae Wilson" order="Hymenoptera" pageNumber="703" phylum="Arthropoda" rank="species" species="hortonae">hortonae</taxonomicName>
