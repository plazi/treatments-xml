<quantity id="4CEF9B25FF5BFF7F9C0DF9DEFEFE3642" box="[1136,1215,1580,1607]" metricMagnitude="-1" metricUnit="m" metricValue="3.5052" pageId="202" pageNumber="203" unit="in" value="13.8">13.8 in</quantity>
