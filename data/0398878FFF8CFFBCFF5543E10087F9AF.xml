<paragraph id="8B8E3699FF8CFFBCFF5543E100A3F8B6" blockId="4.[151,464,1173,1200]" box="[151,464,1173,1200]" pageId="4">
<heading id="D0C681F5FF8CFFBCFF5543E100A3F8B6" bold="true" box="[151,464,1173,1200]" fontSize="11" level="1" pageId="4" reason="1">
<taxonomicName id="4C314D1AFF8CFFBCFF5543E100A3F8B6" authority="Rondani, 1856" authorityName="Rondani" authorityYear="1856" box="[151,464,1173,1200]" class="Insecta" family="Carnidae" genus="Meoneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" phylum="Arthropoda" rank="genus">
