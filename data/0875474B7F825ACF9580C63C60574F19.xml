<taxonomicName id="559266F52ECCB6DE91CBDC9C18EA622B" genus="Z." lsidName="Z. ephialtes subsp. athamanthae" pageId="0" pageNumber="39" rank="subSpecies" species="ephialtes" subSpecies="athamanthae">
<emphasis id="97D3958D44F3FF7400A4EE654EFE1D8E" italics="true" pageId="0" pageNumber="39">Z. ephialtes athamanthae</emphasis>
</taxonomicName>
