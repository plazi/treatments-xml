<paragraph id="8BA9BC0A8007FFEC8616FCC7FD23DF5F" blockId="4.[264,733,780,803]" box="[264,733,780,803]" pageId="4" pageNumber="5">
<taxonomicName id="4C16C7898007FFEC8616FCC7FD27DF5F" authority="Lehr, 1989 Lehr (1989:233)" authorityName="Lehr, 1989 Lehr" authorityPageNumber="233" authorityYear="1989" box="[264,729,780,803]" class="Insecta" family="Asilidae" genus="Asilella" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="londti">
