<materialsCitation id="3B105D7F986D7071C5A7F8D5FBD6F8C2" box="[226,1028,1879,1906]" collectingDate="1992-03" collectionCode="V" collectorName="de Mucubaji" country="Venezuela" elevation="3500" location="Paramo de Mucubaji" municipality="Santo Domingo" pageId="62" pageNumber="63" specimenCount="1" stateProvince="Merida">
