<subSubSection id="14936530FF8E8878286AE036C0A5FCA7" box="[837,1161,792,814]" pageId="8" pageNumber="9" type="type_taxon">
<paragraph id="5C3636BBFF8E8878286AE036C0A5FCA7" blockId="8.[837,1161,792,814]" box="[837,1161,792,814]" pageId="8" pageNumber="9">
<emphasis id="6EFDEAA9FF8E8878286AE036C789FCA4" box="[837,933,793,814]" italics="true" pageId="8" pageNumber="9">
<typeStatus id="83328819FF8E8878286AE036C741FCA4" box="[837,877,793,814]" pageId="8" pageNumber="9">Type</typeStatus>
genus:
</emphasis>
<treatmentCitationGroup id="7C991195FF8E88782884E037C0A5FCA7" box="[939,1161,792,814]" pageId="8" pageNumber="9">
<treatmentCitation id="DD2810AAFF8E88782884E037C0A5FCA7" author="Bruguière" box="[939,1161,792,814]" pageId="8" pageNumber="9" year="1789">
