<taxonomicName id="4C724D79FF94A574FF44FE29B038E5B7" authority="Holmberg" authorityName="Holmberg" box="[151,375,422,448]" class="Insecta" family="Apidae" genus="Svastra" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="25" phylum="Arthropoda" rank="genus">
<emphasis id="B906EAE8FF94A574FF44FE29B038E5B7" bold="true" box="[151,375,422,448]" pageId="9" pageNumber="25">
