<paragraph id="8A5B5602D24BAE5907C688DAFD32FB1D" blockId="5.[151,695,1181,1208]" box="[151,695,1181,1208]" pageId="5" pageNumber="65">
<heading id="D113E16ED24BAE5907C688DAFD32FB1D" bold="true" box="[151,695,1181,1208]" fontSize="11" level="1" pageId="5" pageNumber="65" reason="1">
<taxonomicName id="4DE42D81D24BAE5907C688DAFD32FB1D" authority="Otte, 2006" authorityName="Otte" authorityYear="2006" box="[151,695,1181,1208]" class="Insecta" family="Phalangopsidae" genus="Paragryllus" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="65" phylum="Arthropoda" rank="species" species="eclogos" subGenus="Paragryllus">
<emphasis id="B8908A10D24BAE5907C688DAFD32FB1D" bold="true" box="[151,695,1181,1208]" pageId="5" pageNumber="65">
<emphasis id="B8908A10D24BAE5907C688DAFEA0FB12" bold="true" box="[151,293,1181,1208]" italics="true" pageId="5" pageNumber="65">Paragryllus</emphasis>
(
<emphasis id="B8908A10D24BAE59066788DAFE41FB12" bold="true" box="[310,452,1181,1208]" italics="true" pageId="5" pageNumber="65">Paragryllus</emphasis>
) 
<emphasis id="B8908A10D24BAE59068488DAFDAAFB12" bold="true" box="[469,559,1181,1208]" italics="true" pageId="5" pageNumber="65">eclogos</emphasis>
<bibRefCitation id="EE752BF3D24BAE59056788DAFD32FB1D" author="Otte, D." box="[566,695,1181,1207]" pageId="5" pageNumber="65" pagination="299 - 418" refId="ref11175" refString="Otte, D. (2006) Eighty-four new cricket species (Orthoptera: Grylloidea) from La Selva, Costa Rica. Transactions of the American Entomological Society, 132 (3 - 4), 299 - 418. https: // doi. org / 10.3157 / 0002 - 8320 (2006) 132 [299: ENCSOG] 2.0. CO; 2" type="journal article" year="2006">Otte, 2006</bibRefCitation>
