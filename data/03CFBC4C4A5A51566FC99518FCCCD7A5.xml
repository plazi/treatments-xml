<number id="3DAE1F7D4A5A51556EB69678FDC8D05F" box="[488,531,1814,1839]" pageId="9" pageNumber="307" value="600.0">600</number>
bp; GenBank accession numbers 
<accessionNumber id="943590B94A5A51556CC59678FBC3D05F" box="[923,1048,1814,1839]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/KT948642" pageId="9" pageNumber="307">
KT 
<number id="3DAE1F7D4A5A51556C9F9678FBC3D05F" box="[961,1048,1814,1839]" pageId="9" pageNumber="307" value="948642.0">948642</number>
</accessionNumber>
, 
<accessionNumber id="943590B94A5A51556B7B9678FB7AD05F" box="[1061,1185,1814,1839]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/KT948643" pageId="9" pageNumber="307">
KT 
<number id="3DAE1F7D4A5A51556B159678FB7AD05F" box="[1099,1185,1814,1839]" pageId="9" pageNumber="307" value="948643.0">948643</number>
</accessionNumber>
, 
<accessionNumber id="943590B94A5A51556BF09678FAF1D05F" box="[1198,1322,1814,1839]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/KT948644" pageId="9" pageNumber="307">
KT 
<number id="3DAE1F7D4A5A51556B8A9678FAF1D05F" box="[1236,1322,1814,1839]" pageId="9" pageNumber="307" value="948644.0">948644</number>
</accessionNumber>
) from the three 
<collectingCountry id="F3714DCA4A5A51556F879655FEFCD022" box="[217,295,1851,1874]" name="Wallis and Futuna" pageId="9" pageNumber="307">Futuna</collectingCountry>
