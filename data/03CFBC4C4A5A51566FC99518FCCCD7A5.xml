AB 
<number id="3DAE1F7D4A5A51556A1F96EDFA43D0EB" box="[1345,1432,1923,1947]" pageId="9" pageNumber="307" value="772281.0">772281</number>
</accessionNumber>
, 
<accessionNumber id="943590B94A5A51556FC996C9FECDD0CF" box="[151,278,1959,1983]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AB772283" pageId="9" pageNumber="307">
