<number id="3DAE1F7D4A5A51556C1B96C8FC41D0CF" box="[837,922,1958,1983]" pageId="9" pageNumber="307" value="2.6500000000000004" valueMax="3.7" valueMin="1.6">1.6–3.7</number>
