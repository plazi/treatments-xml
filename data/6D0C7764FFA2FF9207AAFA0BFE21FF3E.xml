<taxonomicName id="22A5BDF1FFA2FF9106FBF9D3FD87F995" box="[454,554,1591,1617]" class="Arachnida" family="Theraphosidae" genus="Euathlus" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
