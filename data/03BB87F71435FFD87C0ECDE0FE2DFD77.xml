<taxonomicName id="4C124D621435FFDB7C9CCB22FC09F9CB" authority="(Bruneau de Mire, 1952)" baseAuthorityName="Bruneau de Mire" baseAuthorityYear="1952" box="[502,1005,1559,1583]" class="Insecta" family="Carabidae" genus="Dyschiriodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="572" phylum="Arthropoda" rank="species" species="mortchaensis" subGenus="Dyschiriodes">
<emphasis id="B966EAF31435FFDB7C9CCB22FDF4F9CB" box="[502,528,1560,1583]" italics="true" pageId="13" pageNumber="572">D.</emphasis>
(
<emphasis id="B966EAF31435FFDB7F4ACB22FDDEF9CB" box="[544,570,1560,1583]" italics="true" pageId="13" pageNumber="572">D.</emphasis>
