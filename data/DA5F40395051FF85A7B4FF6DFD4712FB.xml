<geoCoordinate id="37C297E85051FF85A3BAFE0EFAAF12BB" box="[1230,1352,418,442]" degrees="64" direction="west" minutes="38" orientation="longitude" pageId="4" pageNumber="3" precision="15" seconds="4" value="-64.634445">64º38’4”O</geoCoordinate>
