<quantity id="7F6CD8A9FFC83325FE106A83382DFB12" box="[476,540,1155,1176]" metricMagnitude="3" metricUnit="m" metricValue="2.2" pageId="1" pageNumber="1608" unit="m" value="2200.0">2200m</quantity>
