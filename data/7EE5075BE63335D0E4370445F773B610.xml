<taxonomicName id="1760E689A7209DF14A1B7D680AB94D19" class="Insecta" family="Carabidae" genus="Tachys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tachys ampicollis" order="Coleoptera" pageId="22" pageNumber="109" phylum="Arthropoda" rank="species" species="ampicollis">Tachys ampicollis</taxonomicName>
