: 
<taxonomicName id="4C364D1DFFF8FFD0A821F8BE10E3473E" box="[309,470,1868,1894]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="sibirica">
<emphasis id="B942EA8CFFF8FFD0A821F8BE10E3473E" box="[309,470,1868,1894]" italics="true" pageId="24" pageNumber="25">
