<heading id="DB49819EFF8CFF85FF4EFBB2FE1B5E6F" bold="true" box="[151,430,1086,1113]" fontSize="11" level="1" pageId="4" pageNumber="543" reason="1">
<emphasis id="B2CAEAE0FF8CFF85FF4EFBB2FE1B5E6F" bold="true" box="[151,430,1086,1113]" pageId="4" pageNumber="543">
<taxonomicName id="47BE4D71FF8CFF85FF4EFBB2FE8D5E6F" authority="Watts &amp; Bradford &amp; Cooper, 2021" authorityName="Watts &amp; Bradford &amp; Cooper" authorityYear="2021" box="[151,312,1086,1113]" class="Insecta" family="Scirtidae" genus="Perplexacara" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="543" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B2CAEAE0FF8CFF85FF4EFBB2FE8D5E6F" bold="true" box="[151,312,1086,1113]" italics="true" pageId="4" pageNumber="543">Perplexacara</emphasis>
</taxonomicName>
