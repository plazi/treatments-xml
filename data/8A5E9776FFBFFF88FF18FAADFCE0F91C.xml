<collectingCountry id="7AE066F0FFBFFF88FC41FA76FBBEFA40" box="[974,1059,1501,1526]" name="Angola" pageId="5" pageNumber="240">Angola</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBFFF88FBBCFA76FB0EFA40" box="[1075,1171,1501,1526]" name="Djibouti" pageId="5" pageNumber="240">Djibouti</collectingCountry>
