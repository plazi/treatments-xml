<collectingCountry id="7AE066F0FFBFFF88FF18F9AAFF72F9AC" box="[151,239,1537,1562]" name="Malawi" pageId="5" pageNumber="240">Malawi</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBFFF88FF72F9AAFE09F9AC" box="[253,404,1537,1562]" name="Mozambique" pageId="5" pageNumber="240">Mozambique</collectingCountry>
