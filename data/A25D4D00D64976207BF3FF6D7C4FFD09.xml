<number id="9C3CEE31D649762078E3FEF67D93FEFD" box="[903,932,259,283]" pageId="5" pageNumber="206" value="12.0">12</number>
; SC 
<number id="9C3CEE31D649762078B1FEF67DC6FEFD" box="[981,1009,259,283]" pageId="5" pageNumber="206" value="12.0">12</number>
; SC 
<number id="9C3CEE31D64976207F47FEF67A09FEFD" box="[1059,1086,259,283]" pageId="5" pageNumber="206" value="13.0">13</number>
; SD 
