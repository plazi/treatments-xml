<number id="9C3CEE31D64976207A97FEF67C27FEFD" box="[499,528,259,283]" pageId="5" pageNumber="206" value="23.0">23</number>
; SD 
<number id="9C3CEE31D64976207927FEF67C67FEFD" box="[579,592,259,283]" pageId="5" pageNumber="206" value="8.0">8</number>
