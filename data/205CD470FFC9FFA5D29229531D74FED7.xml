<quantity id="6F0DC883FFC9FFA6D119296C1F75FAD4" box="[825,888,1335,1355]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="57" pageNumber="1734" unit="m" value="400.0">400 m</quantity>
