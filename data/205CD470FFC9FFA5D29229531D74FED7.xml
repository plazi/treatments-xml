<paragraph id="A84A6566FFC9FFA6D292296C1DC1FAFC" blockId="57.[178,1085,1335,1379]" pageId="57" pageNumber="1734">
<collectingRegion id="6A31AB84FFC9FFA6D292296C1D14FAD4" box="[178,281,1335,1355]" country="Austria" name="Vorarlberg" pageId="57" pageNumber="1734">Vorarlberg</collectingRegion>
