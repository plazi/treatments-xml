<subSubSection id="C36DDFF8F471C61191C8A7A24B4A3A23" pageId="45" pageNumber="1932" type="materials_examined">
<paragraph id="8BC88C73F471C61191C8A7A24A903D64" blockId="45.[92,1047,246,1545]" pageId="45" pageNumber="1932">
<emphasis id="B9035061F471C61191C8A7A2493A3D24" box="[132,289,246,270]" italics="true" pageId="45" pageNumber="1932">Type material</emphasis>
