<quantity id="0D039B3F223BFFEAFAB9FE4C8A3C27F2" box="[1317,1406,404,429]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="7" pageNumber="114" unit="m" value="2500.0">2500 m</quantity>
