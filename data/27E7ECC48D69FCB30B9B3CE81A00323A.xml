<taxonomicName id="ACD4F13E7ACF8AF0EEE9ACD6620D8CA3" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus mellipes" order="Hymenoptera" pageId="109" pageNumber="110" phylum="Arthropoda" rank="species" species="mellipes">
