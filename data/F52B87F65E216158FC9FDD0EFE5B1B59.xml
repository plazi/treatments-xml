(MA) and 
<treatmentCitation id="FC2310F15E2E6159FE44D8D0FC951B98" box="[397,772,657,678]" pageId="24" pageNumber="157">
<taxonomicName id="BA824D635E2E6159FE44D8D0FC951B98" authority="KARAVAJEV, 1929" authorityName="KARAVAJEV" authorityYear="1929" box="[397,772,657,678]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="157" phylum="Arthropoda" rank="species" species="constricta">
