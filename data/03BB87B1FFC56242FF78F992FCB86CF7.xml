<taxonomicName id="4C124D24FFC56243FF78F94BFD956AAC" authority="Andre, 1896: 253" authorityName="Andre" authorityPageNumber="253" authorityYear="1896" box="[151,526,1686,1708]" class="Insecta" family="Formicidae" genus="Myrmecorhynchus" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="283" phylum="Arthropoda" rank="genus">
<emphasis id="B966EAB5FFC56243FF78F94BFEC86AAC" box="[151,339,1686,1708]" italics="true" pageId="0" pageNumber="283">Myrmecorhynchus</emphasis>
<treatmentCitation id="0AB310B6FFC56243FEB6F94BFD956AAC" author="Andre" box="[345,526,1686,1708]" page="253" pageId="0" pageNumber="283" year="1896">
<bibRefCitation id="EF834B56FFC56243FEB6F94BFE476AAC" author="Andre" box="[345,476,1686,1708]" pageId="0" pageNumber="283" refString="Andre, E. (1896) Fourmis nouvelles d'Asie et d'Australie. Revue d'Entomologie (Caen), 15, 251 - 265." type="journal article" year="1896">
André, 
<number id="3DDA2480FFC56243FE4AF94BFE476AAC" box="[421,476,1686,1708]" pageId="0" pageNumber="283" value="1896.0">1896</number>
</bibRefCitation>
