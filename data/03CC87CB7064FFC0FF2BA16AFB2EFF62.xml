<quantity id="4C9D9B387064FFC7FE56A1D0FDB7F81C" box="[490,633,1938,1963]" metricMagnitude="-1" metricUnit="m" metricValue="3.85" metricValueMax="4.1" metricValueMin="3.6" pageId="9" pageNumber="302" unit="mm" value="385.0" valueMax="410.0" valueMin="360.0">360–410mm</quantity>
