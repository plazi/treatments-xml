<collectingCountry id="7198767BD74D392CFAD7F8EDFA5B9F37" box="[1354,1432,1820,1844]" name="Russia" pageId="43" pageNumber="44">Russia</collectingCountry>
, 
<collectingCountry id="7198767BD74D392CFF0AF8CEFECB9F54" box="[151,264,1855,1879]" name="Tajikistan" pageId="43" pageNumber="44">Tajikistan</collectingCountry>
, 
<collectingCountry id="7198767BD74D392CFE89F8CEFEA79F54" box="[276,356,1855,1879]" name="Turkey" pageId="43" pageNumber="44">Turkey</collectingCountry>
