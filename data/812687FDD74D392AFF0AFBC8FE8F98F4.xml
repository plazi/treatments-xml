<collectingCountry id="7198767BD74D392CFC4EF8EDFBEC9F37" box="[979,1071,1820,1844]" name="Georgia" pageId="43" pageNumber="44">Georgia</collectingCountry>
, 
<collectingCountry id="7198767BD74D392CFBA1F8ECFBAF9F37" box="[1084,1132,1821,1844]" name="Iran" pageId="43" pageNumber="44">Iran</collectingCountry>
