<quantity id="5651439CFFACFFBAFC21FA46FBE1FA06" box="[953,1045,1416,1442]" metricMagnitude="3" metricUnit="m" metricValue="2.4" pageId="3" pageNumber="33" unit="m" value="2400.0">
