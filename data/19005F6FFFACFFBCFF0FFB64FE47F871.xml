<materialsCitation id="21C1E424FFACFFBAFF69FA46FD66FA6C" collectingDate="2009-10-25" collectionCode="BJFC" country="China" elevation="2400" location="Gaoligong Mountains" municipality="Baoshan" pageId="3" pageNumber="33" specimenCount="1" stateProvince="Yunnan" typeStatus="holotype">
