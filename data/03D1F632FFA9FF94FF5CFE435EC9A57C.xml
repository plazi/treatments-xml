<taxonomicName id="4C783CA7FFA9FF94FE0DFB775D26A498" authorityName="Velenovsky" authorityYear="1889" box="[473,596,1238,1264]" class="Magnoliopsida" family="Lamiaceae" genus="Teucrium" kingdom="Plantae" order="Lamiales" pageId="4" pageNumber="202" phylum="Tracheophyta" rank="species" species="skorpilii">
