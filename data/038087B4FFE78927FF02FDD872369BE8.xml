, 
<taxonomicName id="4C294D21FFE78938FCA3F8E377979E03" authority="(Lamarck, 1816)" authorityName="Lamarck" authorityYear="1816" baseAuthorityName="Lamarck" baseAuthorityYear="1816" box="[822,1155,1878,1903]" class="Echinoidea" family="Brissidae" genus="Metalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spatangoida" pageId="77" pageNumber="78" phylum="Echinodermata" rank="species" species="sternalis">
