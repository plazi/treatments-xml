<taxonomicName id="4C0D4D43BA3C704F8DA36A7AFDCFCB64" authority="Xavier &amp; Bonaldo, 2021" authorityName="Xavier &amp; Bonaldo" authorityYear="2021" box="[474,599,1861,1887]" class="Arachnida" family="Corinnidae" genus="Tupirinna" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="216" phylum="Arthropoda" rank="species" species="platnicki" status="sp. nov.">
