of 
<taxonomicName id="4C4C62A7FFB40530A4CB81473512900D" authorityName="Kraepelin" authorityYear="1929" box="[747,888,1563,1589]" class="Arachnida" family="Buthidae" genus="Uroplectes" kingdom="Animalia" order="Scorpiones" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="schubotzi">
