<taxonomicName id="7FC44D44FF8A672501002439FCA2FEA8" authority="Hempel 1937: 5" authorityName="Hempel" authorityPageNumber="5" authorityYear="1937" box="[307,783,258,281]" class="Insecta" family="Eriococcidae" genus="Neotectococcus" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="lenticularis">
