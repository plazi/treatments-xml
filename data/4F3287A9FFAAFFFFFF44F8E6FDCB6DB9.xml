locality.
</emphasis>
<collectingMunicipality id="2740ACC5FFA9FFFFFEEFFF6EFE1D6CFC" box="[316,468,151,176]" pageId="18">Waikau Cave</collectingMunicipality>
, 
<location id="C2446064FFA9FFFFFE0CFF6EFDD46CFC" LSID="urn:lsid:plazi:treatment:4F3287A9FFAAFFFFFF44F8E6FDCB6DB9:C2446064FFA9FFFFFE0CFF6EFDD46CFC" box="[479,541,151,176]" municipality="Waikau Cave" name="Maui" pageId="18" stateProvince="Hawaii">Maui</location>
, 
<collectingRegion id="055FF85DFFA9FFFFFDFBFF6EFDB46CFC" box="[552,637,151,176]" country="United States of America" name="Hawaii" pageId="18">Hawaii</collectingRegion>
