</emphasis>
<location id="8EF660619049FFD9FE8DAD68FE12B5CB" LSID="urn:lsid:plazi:treatment:038087AC904AFFDFFF76AB38FC47B086:8EF660619049FFD9FE8DAD68FE12B5CB" box="[364,479,367,392]" country="Russia" municipality="Yantarny" name="Baltic Sea" pageId="2" pageNumber="255" stateProvince="Kaliningrad">Baltic Sea</location>
coast, 
<collectingMunicipality id="6BF2ACC09049FFD9FDCCAD77FD5FB5CB" box="[557,658,368,392]" pageId="2" pageNumber="255">Yantarny</collectingMunicipality>
