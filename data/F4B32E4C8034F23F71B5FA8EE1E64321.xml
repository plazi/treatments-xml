<taxonomicName id="A1B0D8BC1F1A8569DECAC2835F4892D8" LSID="http://zoobank.org/45CD9418-019C-4790-940B-B095762D4E38" authority="Herczek, Gorczyca &amp; Taszakowski, 2018" authorityName="Herczek, Gorczyca &amp; Taszakowski" authorityYear="2018" class="Insecta" family="Miridae" genus="Sulawesimetopus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sulawesimetopus henryi" order="Hemiptera" pageId="4" pageNumber="151" phylum="Arthropoda" rank="species" species="henryi">Sulawesimetopus henryi</taxonomicName>
