<quantity id="4CEF9B25FF89FFAD9A69FCD2F8E9333E" box="[532,680,800,827]" metricMagnitude="3" metricUnit="m" metricValue="1.06" metricValueMax="2.0" metricValueMin="0.12" pageId="24" pageNumber="25" unit="m" value="1060.0" valueMax="2000.0" valueMin="120.0">
