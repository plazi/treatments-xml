<quantity id="4CDC9B72FF92901AFF00FDB1BED8FDAE" box="[601,699,514,537]" metricMagnitude="-3" metricUnit="m" metricValue="1.55" pageId="4" pageNumber="311" unit="mm" value="1.55">1.55 mm</quantity>
