<taxonomicName id="4C6D4D1CFFC60218508F22E5FE70235E" authority="Rousse" authorityName="Rousse" box="[151,498,1262,1288]" class="Insecta" family="Ichneumonidae" genus="Lycorina" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="230" phylum="Arthropoda" rank="species" species="jacksonfive">
<emphasis id="B919EA8DFFC60218508F22E5FE11235E" bold="true" box="[151,403,1262,1288]" italics="true" pageId="8" pageNumber="230">Lycorina jacksonfive</emphasis>
