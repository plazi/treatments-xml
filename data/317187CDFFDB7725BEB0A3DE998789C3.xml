<number id="0F1024FCFFDB7725BD46A13E9AF7891F" box="[865,921,798,822]" pageId="55" pageNumber="56" value="1922.0">1922</number>
</bibRefCitation>
</taxonomicName>
(= 
<taxonomicName id="7ED84D58FFDB7725BDF5A13D9DFA891F" box="[978,1172,797,822]" class="Trematoda" family="Cyclocoelidae" genus="Harrahium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="55" pageNumber="56" phylum="Platyhelminthes" rank="species" species="halli">
