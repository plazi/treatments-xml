<taxonomicName id="7ED84D58FFDB7725BEB0A1619B2D8973" authority="Zeliff, 1946" authorityName="Zeliff" authorityYear="1946" box="[151,579,833,858]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="55" pageNumber="56" phylum="Platyhelminthes" rank="species" species="nittanyense">
