<quantity id="4CEF9B25FFB0FF949CB5FA8BFF433591" box="[1224,1282,1401,1428]" metricMagnitude="1" metricUnit="m" metricValue="2.7" pageId="33" pageNumber="34" unit="m" value="27.0">27 m</quantity>
(
<quantity id="4CEF9B25FFB0FF949D6FFA8BFF083591" box="[1298,1353,1401,1428]" metricMagnitude="1" metricUnit="m" metricValue="2.71272" pageId="33" pageNumber="34" unit="ft" value="89.0">89 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FFB0FF9498BBFA6CFB1A35BD" author="Weaver, P. L." box="[198,347,1438,1464]" pageId="33" pageNumber="34" pagination="165 - 185" refId="ref332541" refString="Weaver, P. L. (1970) Species diversity and ecology of tide pool fishes in three Pacific coastal areas of Costa Rica. Revista de Biologia Tropical, 17, 165 - 185." type="journal article" year="1970">Weaver 1970</bibRefCitation>
