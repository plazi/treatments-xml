<taxonomicName id="B5AB76A9C97DFFA097F7F80F4F8707B8" box="[189,466,1938,1964]" class="Magnoliopsida" family="Verbenaceae" genus="Citharexylum" kingdom="Plantae" order="Lamiales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="spinosum">
