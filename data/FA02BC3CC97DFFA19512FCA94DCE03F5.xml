<paragraph id="72140D2AC97DFFA097F7F8D04CFC07B9" blockId="24.[189,1398,1868,1965]" pageId="24" pageNumber="25">
<taxonomicName id="B5AB76A9C97DFFA097F7F8D04F290772" authorityName="Macquart" authorityYear="1855" box="[189,380,1869,1895]" class="Insecta" family="Syrphidae" genus="Syritta" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="oceanica">
