(
<taxonomicName id="B5AB76A9C97DFFA0957AF8F24C83079E" box="[560,726,1903,1930]" class="Magnoliopsida" family="Boraginaceae" kingdom="Plantae" order="Boraginales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="family">Boraginaceae</taxonomicName>
