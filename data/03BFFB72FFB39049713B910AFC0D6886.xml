<taxonomicName id="4C1631E7FFB9904871AC94CDFD516BC2" box="[415,644,1763,1786]" class="Malacostraca" family="Ischnomesidae" genus="Haplomesus" kingdom="Animalia" order="Isopoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="hanseni">
