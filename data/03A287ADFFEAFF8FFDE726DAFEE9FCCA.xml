<taxonomicName id="4C0B4D38FFEBFF8CFEE7238BFD12F821" authority="Kuhlmann" authorityName="Kuhlmann" box="[278,698,1932,1959]" class="Insecta" family="Colletidae" genus="Scrapter" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="bokkeveldensis" status="sp. nov.">
<emphasis id="B97FEAA9FFEBFF8CFEE7238BFD9DF821" box="[278,565,1932,1959]" italics="true" pageId="5" pageNumber="6">Scrapter bokkeveldensis</emphasis>
