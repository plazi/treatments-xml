<quantity id="4CCB9B5EF83B3666FDEC28BFFDF1F94E" box="[558,607,1700,1720]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="34" pageNumber="147" unit="mm" value="1.0">1 mm</quantity>
