), 
<collectingRegion id="612BF8549E32FFFAFCE776A1FBF2149C" box="[852,1028,699,721]" country="Brazil" name="Rio de Janeiro" pageId="54" pageNumber="54">Rio de Janeiro</collectingRegion>
