<taxonomicName id="72D2349FFF86FFE9FCEEF8D2FB51F8C4" authority="Forel, 1900" authorityName="Forel" authorityYear="1900" box="[885,1210,1885,1911]" class="Insecta" family="Formicidae" genus="Rhopalomastix" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="121" phylum="Arthropoda" rank="genus">
