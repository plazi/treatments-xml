<paragraph blockId="92.[151,1436,1831,2037]" box="[151,710,1831,1856]" pageId="92" pageNumber="93">
<emphasis bold="true" box="[151,306,1831,1856]" pageId="92" pageNumber="93">
<typeStatus box="[151,210,1831,1856]" pageId="92" pageNumber="93">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Gravenhorst 1829" authorityName="Gravenhorst" authorityYear="1829" box="[313,710,1831,1856]" class="Insecta" family="Braconidae" genus="Bassus" kingdom="Animalia" order="Hymenoptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="exsultans">
<emphasis box="[313,501,1831,1856]" italics="true" pageId="92" pageNumber="93">Bassus exsultans</emphasis>
<bibRefCitation author="Gravenhorst" box="[509,710,1831,1856]" pageId="92" pageNumber="93" refString="Gravenhorst, J. L. C. (1829) Ichneumonologia Europaea. Pars III. Sumtibus auctoris, Vratislaviae, 1097 pp." type="book" year="1829">
