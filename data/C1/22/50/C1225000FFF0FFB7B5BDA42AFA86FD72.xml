</paragraph>
</subSubSection>
<subSubSection pageId="82" pageNumber="83" type="distribution">
<paragraph blockId="82.[151,1437,151,717]" box="[199,594,620,645]" pageId="82" pageNumber="83">
<emphasis bold="true" box="[199,352,620,645]" pageId="82" pageNumber="83">Distribution.</emphasis>
<collectingCountry box="[359,443,620,645]" name="Canada" pageId="82" pageNumber="83">Canada</collectingCountry>
and 
<collectingCountry box="[497,589,620,645]" name="Sweden" pageId="82" pageNumber="83">Sweden</collectingCountry>
