–8.0, ♀ 
<number box="[1195,1233,622,647]" pageId="12" pageNumber="526" value="5.5">5.5</number>
–7.0; fore femora Ƌ 
<number box="[168,273,658,683]" pageId="12" pageNumber="526" value="11.5" valueMax="15.5" valueMin="7.5">7.5–15.5</number>
, ♀10.0– 
<number box="[373,425,658,683]" pageId="12" pageNumber="526" value="15.5">15.5</number>
; hind femora Ƌ 
<number box="[609,729,658,683]" pageId="12" pageNumber="526" value="27.5" valueMax="30.5" valueMin="24.5">24.5–30.5</number>
