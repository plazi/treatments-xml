<quantity box="[358,458,879,903]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="35" pageNumber="2459" unit="mm" value="3.8">3.80 mm</quantity>
; wing length 
<quantity box="[608,709,879,903]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="35" pageNumber="2459" unit="mm" value="3.8">3.80 mm</quantity>
