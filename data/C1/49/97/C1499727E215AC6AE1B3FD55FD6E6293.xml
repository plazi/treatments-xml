<quantity box="[668,831,839,863]" metricMagnitude="-3" metricUnit="m" metricValue="4.075" metricValueMax="4.3" metricValueMin="3.85" pageId="29" pageNumber="2453" unit="mm" value="4.075" valueMax="4.3" valueMin="3.85">3.85–4.30 mm</quantity>
