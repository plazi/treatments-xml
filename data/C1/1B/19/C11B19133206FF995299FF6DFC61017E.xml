<subSubSection pageId="44" pageNumber="538" type="discussion">
<paragraph blockId="44.[151,1436,151,367]" pageId="44" pageNumber="538">
<emphasis bold="true" box="[198,319,151,177]" pageId="44" pageNumber="538">Remarks.</emphasis>
<taxonomicName authorityName="White &amp; Thomas" authorityYear="2009" box="[333,547,152,177]" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="44" pageNumber="538" phylum="Tracheophyta" rank="species" species="sparsa" status="sp. nov.">
<emphasis box="[333,547,152,177]" italics="true" pageId="44" pageNumber="538">Leucothoe sparsa</emphasis>
</taxonomicName>
<emphasis bold="true" box="[560,660,151,177]" pageId="44" pageNumber="538">
<taxonomicNameLabel box="[560,660,151,177]" pageId="44" pageNumber="538" rank="species">sp. nov.</taxonomicNameLabel>
