<taxonomicName authorityName="Thomas &amp; Klebba" authorityYear="2006" box="[1294,1431,1818,1844]" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="51" pageNumber="545" phylum="Tracheophyta" rank="species" species="ashleyae">
<emphasis box="[1294,1310,1818,1843]" italics="true" pageId="51" pageNumber="545">L</emphasis>
. ashleyae
</taxonomicName>
<emphasis box="[1431,1435,1818,1843]" italics="true" pageId="51" pageNumber="545">,</emphasis>
females of 
<taxonomicName authorityName="J. L. Barnard" authorityYear="1974" box="[292,435,1856,1882]" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="51" pageNumber="545" phylum="Tracheophyta" rank="species" species="assimilis">
<emphasis box="[292,308,1856,1881]" italics="true" pageId="51" pageNumber="545">L</emphasis>
. 
<emphasis box="[326,435,1856,1881]" italics="true" pageId="51" pageNumber="545">assimilis</emphasis>
</taxonomicName>
