</taxonomicName>
<emphasis box="[772,777,1440,1465]" italics="true" pageId="27" pageNumber="521">,</emphasis>
females of 
<taxonomicName authorityName="J. L. Barnard" authorityYear="1974" box="[926,1072,1439,1465]" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="27" pageNumber="521" phylum="Tracheophyta" rank="species" species="assimilis">
<emphasis box="[926,942,1440,1465]" italics="true" pageId="27" pageNumber="521">L</emphasis>
. assimilis
</taxonomicName>
<emphasis box="[1071,1076,1440,1465]" italics="true" pageId="27" pageNumber="521">,</emphasis>
females of 
<taxonomicName authorityName="Schellenberg" authorityYear="1928" box="[1225,1394,1440,1465]" class="Malacostraca" family="Leucothoidae" genus="Leucothoella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="27" pageNumber="521" phylum="Arthropoda" rank="species" species="banwarthii">
<emphasis box="[1225,1394,1440,1465]" italics="true" pageId="27" pageNumber="521">L. banwarthii</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Thomas &amp; Klebba" authorityYear="2007" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="27" pageNumber="521" phylum="Tracheophyta" rank="species" species="barana">
<emphasis box="[1411,1427,1440,1465]" italics="true" pageId="27" pageNumber="521">L</emphasis>
