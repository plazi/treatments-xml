<subSubSection pageId="21" pageNumber="433" type="distribution">
<paragraph blockId="21.[816,1336,1029,1089]" pageId="21" pageNumber="433">
<emphasis box="[816,953,1029,1055]" italics="true" pageId="21" pageNumber="433">New record</emphasis>
