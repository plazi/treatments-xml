<collectingRegion box="[316,422,1078,1104]" country="Indonesia" name="Sumatera Utara" pageId="23" pageNumber="435">Sumatra</collectingRegion>
; 
<collectingRegion box="[445,695,1078,1104]" country="Indonesia" name="Lampung" pageId="23" pageNumber="435">Lampung Province</collectingRegion>
