<specimenCount box="[1314,1350,838,864]" pageId="19" pageNumber="431" type="female">1♀</specimenCount>
(
<collectionCode box="[1364,1428,838,864]" country="Indonesia" httpUri="http://grbio.org/cool/b9gz-izvg" name="Museum Zoologicum Bogoriense" pageId="19" pageNumber="431" type="Museum">MZB</collectionCode>
