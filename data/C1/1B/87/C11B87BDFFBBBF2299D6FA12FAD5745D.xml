: Kuyung Arang Village, 
<specimenCount box="[682,720,1824,1850]" pageId="25" pageNumber="437" type="male">1♂</specimenCount>
, 
<specimenCount box="[734,771,1824,1850]" pageId="25" pageNumber="437" type="female">1♀</specimenCount>
(
<collectionCode box="[167,232,1858,1884]" country="Indonesia" httpUri="http://grbio.org/cool/b9gz-izvg" name="Museum Zoologicum Bogoriense" pageId="25" pageNumber="437" type="Museum">MZB</collectionCode>
