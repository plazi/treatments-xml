7 
<taxonomicName authority="Shiraki, 1913" authorityName="Shiraki" authorityYear="1913" box="[909,1170,1109,1130]" class="Insecta" family="Tridactylidae" genus="Xya" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="187" phylum="Arthropoda" rank="species" species="nitobei">
