<taxonomicName authorityName="Nicholls" authorityYear="2006" box="[490,583,1889,1911]" class="Lobosa" family="Cryptodifflugiidae" genus="Cryptodifflugia" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Arcellinida" pageId="8" pageNumber="300" phylum="Amoebozoa" rank="species" species="leachi">
