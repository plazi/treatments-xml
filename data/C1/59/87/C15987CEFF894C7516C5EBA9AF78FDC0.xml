Ƥ: K 
<number box="[634,681,453,479]" pageId="4" pageNumber="36" value="659.0">659</number>
, 
<emphasis bold="true" box="[692,801,453,479]" pageId="4" pageNumber="36">
<collectingCountry box="[692,801,453,479]" name="Bulgaria" pageId="4" pageNumber="36">Bulgaria</collectingCountry>
</emphasis>
, 
<taxonomicName box="[816,931,453,479]" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="36" phylum="Arthropoda" rank="species" species="alibotush">Alibotush</taxonomicName>
Mts., above hut Izvora, about 
<quantity box="[1289,1381,453,479]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="4" pageNumber="36" unit="m" value="1000.0">1000 m</quantity>
. alt., on roots of 
<taxonomicName box="[285,394,491,516]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="4" pageNumber="36" phylum="Tracheophyta" rank="genus">
<emphasis box="[285,394,491,516]" italics="true" pageId="4" pageNumber="36">Trifolium</emphasis>
</taxonomicName>
sp. 
<number box="[442,500,490,516]" pageId="4" pageNumber="36" value="1.19">
1, 
<date box="[471,500,490,516]" pageId="4" pageNumber="36" value="2009-06-19">19</date>
</number>
.VI. 
<number box="[542,608,490,516]" pageId="4" pageNumber="36" value="2009.0">2009</number>
; 
