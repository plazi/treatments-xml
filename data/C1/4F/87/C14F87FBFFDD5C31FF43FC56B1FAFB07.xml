species: 
<taxonomicName authority="Pace, 2006" authorityName="Pace" authorityYear="2006" box="[294,698,1117,1144]" class="Insecta" family="Staphylinidae" genus="Himalusa" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="annapurnensis">
