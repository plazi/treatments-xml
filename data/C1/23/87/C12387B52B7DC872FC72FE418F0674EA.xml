<collectingCountry box="[1233,1322,1391,1414]" name="Georgia" pageId="5" pageNumber="634">Georgia</collectingCountry>
, 
<collectingCountry box="[1337,1383,1391,1414]" name="Iran" pageId="5" pageNumber="634">Iran</collectingCountry>
