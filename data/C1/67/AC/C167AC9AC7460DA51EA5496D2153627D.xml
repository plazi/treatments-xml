<taxonomicName class="Dicotyledoneae" family="Scrophulariaceae" genus="Verbascum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Tubiflorae" pageId="9" pageNumber="22" phylum="Angiospermae" rank="genus">
<emphasis italics="true" pageId="9" pageNumber="22">Verbascum</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="de Jussieu" authorityYear="1789" class="Magnoliopsida" family="Scrophulariaceae" genus="Scrophulariaceae" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Lamiales" pageId="9" pageNumber="22" phylum="Magnoliophyta" rank="genus">Scrophulariaceae</taxonomicName>
