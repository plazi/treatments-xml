<paragraph blockId="44.[151,1127,513,1592]" box="[151,721,1569,1592]" pageId="44" pageNumber="389">
<emphasis bold="true" box="[151,298,1569,1592]" pageId="44" pageNumber="389">Distribution.</emphasis>
<collectingCountry box="[304,369,1569,1592]" name="Japan" pageId="44" pageNumber="389">Japan</collectingCountry>
