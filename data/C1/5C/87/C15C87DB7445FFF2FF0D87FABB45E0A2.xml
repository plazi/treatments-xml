Reported from 
<collectingCountry box="[529,596,333,356]" name="Japan" pageId="20" pageNumber="271">Japan</collectingCountry>
, 
<collectingCountry box="[606,677,333,356]" name="South Korea" pageId="20" pageNumber="271">Korea</collectingCountry>
, and 
<collectingCountry box="[735,847,332,357]" name="Indonesia" pageId="20" pageNumber="271">Indonesia</collectingCountry>
