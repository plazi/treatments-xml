<taxonomicName authority="Mulsant, 1850" authorityName="Mulsant" authorityYear="1850" box="[326,606,1426,1445]" class="Insecta" family="Coccinellidae" genus="Tiphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="277" phylum="Arthropoda" rank="species" species="plumbea">
