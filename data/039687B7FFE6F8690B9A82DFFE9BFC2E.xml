<subSubSection id="C325652AFFE6F8690B9A82F8FBE9FE8D" lastPageId="4" lastPageNumber="43" pageId="2" pageNumber="41" type="materials_examined">
<paragraph id="8B8036A1FFE6F86F0B9A82F8FC26FA5D" blockId="2.[140,1055,1433,1566]" box="[140,907,1469,1496]" pageId="2" pageNumber="41">
<typeStatus id="54848803FFE6F86F0B9A82F8FF6EFA52" box="[140,195,1470,1496]" pageId="2" pageNumber="41">Type</typeStatus>
