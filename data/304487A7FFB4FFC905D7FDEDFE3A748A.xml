<materialsCitation id="08853CECFFB4FFCE048EFD5EFC847654" ID-GBIF-Occurrence="3073880312" collectionCode="NHMW" country="China" determinerName="H. Franz" location="CHINA" pageId="3" pageNumber="357" specimenCount="1" specimenCount-male="1" typeStatus="holotype">
