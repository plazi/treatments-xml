<typeStatus id="54F3881FFFA97B11609AFC0387F9FC33" box="[151,204,965,989]" pageId="1" pageNumber="542">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="4C484D3EFFA97B116127FC03847FFC33" authority="Brusca and Iverson, 1985" authorityName="Brusca and Iverson" authorityYear="1985" box="[298,842,965,989]" class="Malacostraca" family="Cirolanidae" genus="Metacirolana" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="542" phylum="Arthropoda" rank="species" species="costaricensis">
