<taxonomicName id="4C2901BE047A1529FB7B484FFAF6F974" authorityName="Pease" authorityYear="1871" box="[1112,1309,1638,1664]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="115" pageNumber="116" phylum="Mollusca" rank="species" species="minima">
