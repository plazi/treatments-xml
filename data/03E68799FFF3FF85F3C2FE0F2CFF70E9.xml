<taxonomicName id="4C4F4D0CFFF3FF85F21CFB662FD4708C" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[329,476,1179,1203]" class="Insecta" family="Derbidae" genus="Alara" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="anulus">
