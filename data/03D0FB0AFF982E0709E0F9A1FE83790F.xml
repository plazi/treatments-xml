<taxonomicName id="4C79319FFF982E030999F9A1FDD67C45" authority="Bowerbank, 1864" authorityName="Bowerbank" authorityYear="1864" box="[238,597,1576,1602]" class="Demospongiae" family="Polymastiidae" genus="Polymastia" kingdom="Animalia" order="Hadromerida" pageId="17" pageNumber="18" phylum="Porifera" rank="genus">
<emphasis id="B90D960EFF982E030999F9A1FEF77C45" bold="true" box="[238,372,1576,1602]" italics="true" pageId="17" pageNumber="18">Polymastia</emphasis>
