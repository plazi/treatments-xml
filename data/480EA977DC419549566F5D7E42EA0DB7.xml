<paragraph id="9E28C962365768AAC0AD369770C05DD6" pageId="11" pageNumber="48">
<taxonomicName id="936F360C935E65A435250F8A2DD00D57" baseAuthorityName="de Saussure" baseAuthorityYear="1854" class="Hexapoda" family="Vespidae" genus="Vespula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="11" pageNumber="48" phylum="Arthropoda" rank="species" species="vidua">
<emphasis id="BAE441E8E72CE7DFFF54D4D1F4005A1A" italics="true" pageId="11" pageNumber="48">Vespula vidua</emphasis>
</taxonomicName>
