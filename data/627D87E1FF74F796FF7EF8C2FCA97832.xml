<emphasis id="D8A0EAE5FF74F790FF7EF8C2FC857A7A" bold="true" box="[151,853,1880,1907]" pageId="138" pageNumber="139">
<taxonomicName id="2DD44D74FF74F790FF7EF8C2FD677A7A" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,695,1880,1907]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="138" pageNumber="139" phylum="Arthropoda" rank="species" species="songbua" status="new species">
<emphasis id="D8A0EAE5FF74F790FF7EF8C2FE047A7B" bold="true" box="[151,468,1881,1907]" italics="true" pageId="138" pageNumber="139">Cheumatopsyche songbua</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFF74F790FD2CF8C3FC857A7A" box="[709,853,1880,1906]" pageId="138" pageNumber="139" rank="species">new species</taxonomicNameLabel>
