<geoCoordinate id="EE1C5055FF906305BBF0FD27C308FD40" box="[438,528,691,713]" degrees="27" direction="north" minutes="35" orientation="latitude" pageId="7" pageNumber="1460" precision="925" value="27.583334">27°35‘N</geoCoordinate>
