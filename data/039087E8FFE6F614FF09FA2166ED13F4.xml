<number id="3DF124D9FFE6F617FD8AF98C65361458" box="[532,561,1615,1638]" pageId="13" pageNumber="14" value="59.0">59</number>
</treatmentCitation>
</taxonomicName>
[
<typeStatus id="5482885CFFE6F617FDDAF98C657B1458" box="[580,636,1615,1638]" pageId="13" pageNumber="14">Type</typeStatus>
locality: 
<collectingCountry id="F32E766EFFE6F617FD71F98C645C1458" box="[751,859,1615,1638]" name="Colombia" pageId="13" pageNumber="14">Colombia</collectingCountry>
, Dpto. Antioquia, Quebrada la Jimenez, Sopetran; 
<collectionCode id="ED28AE3BFFE6F617FF59F9AC661B14B8" box="[199,284,1647,1670]" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="13" pageNumber="14">NMNH</collectionCode>
