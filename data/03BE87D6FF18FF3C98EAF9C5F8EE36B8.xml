. To 
<quantity id="4CEF9B25FF18FF3C9C7AF9C5FE223654" box="[1031,1123,1591,1617]" metricMagnitude="-1" metricUnit="m" metricValue="1.67" pageId="137" pageNumber="138" unit="cm" value="16.7">16.7 cm</quantity>
(
<quantity id="4CEF9B25FF18FF3C9C09F9C5FEF93657" box="[1140,1208,1591,1618]" metricMagnitude="-1" metricUnit="m" metricValue="1.6764" pageId="137" pageNumber="138" unit="in" value="6.6">6.6 in</quantity>
) SL. 
<collectingRegion id="49D3F822FF18FF3C9C84F9C5FF3F3654" box="[1273,1406,1591,1617]" country="United States of America" name="Washington" pageId="137" pageNumber="138">Washington</collectingRegion>
