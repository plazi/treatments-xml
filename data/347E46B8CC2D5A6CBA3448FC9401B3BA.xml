<subSubSection id="4DF2AD521E95F188498576BBA4B6A3B6" pageId="0" pageNumber="49" type="etymology">
<paragraph id="BCCEA239FDFBEFCEBD88725D56B0E598" pageId="0" pageNumber="49">Etymology.</paragraph>
<paragraph id="20546FD111FA3C1F51D916A9FA685534" pageId="0" pageNumber="49">
<emphasis id="8956CEA960DCF23901C76C659A6F0BB9" italics="true" pageId="0" pageNumber="49">Tenuisa</emphasis>
