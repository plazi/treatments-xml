<taxonomicName id="884F3574F256DEC94AAB49DCBFBA7DBE" authorityName="Lesson" authorityYear="1828" class="Aves" family="Chionidae" kingdom="Animalia" lsidName="" order="Charadriiformes" pageId="0" pageNumber="109" phylum="Chordata" rank="family">Chionidae</taxonomicName>
