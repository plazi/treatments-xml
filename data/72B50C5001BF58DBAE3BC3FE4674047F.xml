<taxonomicName id="258E094A7CB89F4D92E919975BCEDB8B" authorityName="J.R.Forster" authorityYear="1788" class="Aves" genus="Chionis" kingdom="Animalia" lsidName="" order="Charadriiformes" pageId="0" pageNumber="109" phylum="Chordata" rank="order">Charadriiformes</taxonomicName>
