<taxonomicName id="A2863AA8FFE5FFE9FE26FBE297603600" baseAuthorityName="Dobson" baseAuthorityYear="1880" box="[459,601,1069,1095]" class="Mammalia" family="Natalidae" genus="Chilonatalus" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="13" phylum="Chordata" rank="species" species="micropus">
<emphasis id="57F29D39FFE5FFE9FE26FBE297603600" box="[459,601,1069,1095]" italics="true" pageId="12" pageNumber="13">C. micropus</emphasis>
</taxonomicName>
