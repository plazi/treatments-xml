<number id="DA3D954208377256B661F4F30617E9C1" box="[835,879,1936,1962]" pageId="1" pageNumber="60" value="244.0">244</number>
(
<number id="DA3D954208377256B6A2F4F306CAE9C1" box="[896,946,1936,1962]" pageId="1" pageNumber="60" value="248.0">248</number>
