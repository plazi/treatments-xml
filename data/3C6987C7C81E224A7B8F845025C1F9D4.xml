<typeStatus id="6B7B8873C81E224A7A0184C424F5FC03" box="[868,970,981,1003]" pageId="125" pageNumber="126">holotype</typeStatus>
from ‘‘La Cumbre de Valencia,’’ 
<collectingRegion id="7604F833C81E224A7A0784E324E4FBE0" box="[866,987,1010,1032]" country="Venezuela" name="Carabobo" pageId="125" pageNumber="126">Carabobo</collectingRegion>
, 
<collectingCountry id="CCD77641C81E224A7A9284E32352FBE0" box="[1015,1133,1010,1032]" name="Venezuela" pageId="125" pageNumber="126">Venezuela</collectingCountry>
(E. Simon), deposited in 
<collectionCode id="D2D1AE14C81E224A7A848301237FFBCD" box="[993,1088,1040,1061]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="125" pageNumber="126">MNHN</collectionCode>
(5695, 
<collectionCode id="D2D1AE14C81E224A7BFE833C25F7FBAA" box="[667,712,1069,1090]" pageId="125" pageNumber="126">PBI</collectionCode>
_ 
<accessionNumber id="AB93AB32C81E224A7BB2833C2456FBAB" box="[727,873,1069,1091]" httpUri="http://www.ncbi.nlm.nih.gov/protein/OON29542" pageId="125" pageNumber="126">OON 29542</accessionNumber>
