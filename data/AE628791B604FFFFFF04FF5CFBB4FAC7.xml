<number id="900324A0B602FFFFFCE0FD81FC3BFD8A" box="[883,912,512,536]" pageId="4" pageNumber="87" value="29.0">29</number>
, 
<number id="900324A0B602FFFFFC08FE7EFC64FD8A" box="[923,975,511,536]" pageId="4" pageNumber="87" value="1.16">1.16</number>
; P-4, 59/ 
<number id="900324A0B602FFFFFBA5FE7EFBF8FD8A" box="[1078,1107,511,536]" pageId="4" pageNumber="87" value="20.0">20</number>
, 
<number id="900324A0B602FFFFFBCCFD81FB2EFD8A" box="[1119,1157,512,536]" pageId="4" pageNumber="87" value="2.9">2.9</number>
; P-5, 34/ 
<number id="900324A0B602FFFFFB7EFD81FAA2FD8A" box="[1261,1289,512,536]" pageId="4" pageNumber="87" value="12.0">12</number>
, 3.0; dL P- 
<number id="900324A0B602FFFFFA15FD81FA38FD8A" box="[1414,1427,512,536]" pageId="4" pageNumber="87" value="2.0">2</number>
