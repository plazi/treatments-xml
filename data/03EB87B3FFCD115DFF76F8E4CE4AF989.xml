<taxonomicName id="4C424D26FFCF115DFC1CFA72CBE8FA62" box="[1021,1159,1504,1527]" class="Insecta" family="Heteroceridae" genus="Heterocerus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="264" phylum="Arthropoda" rank="genus">
