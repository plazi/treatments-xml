<collectingCountry id="F3761D80314FFFAAFE79F8E6FE6DF889" box="[401,476,1800,1826]" name="China" pageId="2" pageNumber="39">China</collectingCountry>
: 
<typeStatus id="54DAE3B2314FFFAAFE0DF8E6FDE3F889" box="[485,594,1800,1826]" pageId="2" pageNumber="39">Holotype</typeStatus>
ɗ, Guangxi; 
<taxonomicName id="4C612693314FFFAAFD06F8E6FC71F889" authority="Defu, 1350" authorityName="Defu" authorityYear="1350" box="[750,960,1800,1826]" class="Insecta" family="Cicadellidae" genus="Napo" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="39" phylum="Arthropoda" rank="genus">
Napo, Defu, 
<quantity id="4C99F0F5314FFFAAFC6CF8E6FC71F889" box="[900,960,1800,1826]" metricMagnitude="3" metricUnit="m" metricValue="1.35" pageId="2" pageNumber="39" unit="m" value="1350.0">1350</quantity>
</taxonomicName>
m, 
<date id="FFDF7BD0314FFFAAFC0BF8E6FB31F889" box="[995,1152,1800,1826]" pageId="2" pageNumber="39" value="2000-06-18">
<number id="3DA94F37314FFFAAFC0BF8E6FBB1F889" box="[995,1024,1800,1826]" pageId="2" pageNumber="39" value="18.0">18</number>
