<taxonomicName id="A3364904D10B2D9BCC51AF7EABBB779F" class="Magnoliopsida" family="Elaeagnaceae" genus="Hippophae" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Hippophae rhamnoides subsp. sinensis" order="Rosales" pageId="0" pageNumber="209" phylum="Tracheophyta" rank="subSpecies" species="rhamnoides" subSpecies="sinensis">Hippophae rhamnoides Linnaeus ssp. sinensis</taxonomicName>
