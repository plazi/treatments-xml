<collectionCode id="ED35AE50FFB33264FE29FECEFD83FE08" LSID="urn:lsid:biocol.org:col:34988" box="[451,541,367,391]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="38" pageNumber="489">MNHN</collectionCode>
