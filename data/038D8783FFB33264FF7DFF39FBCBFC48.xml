<taxonomicName id="4C244D16FFB33264FDFAFEEBFC68FEEC" authority="Desutter-Grandcolas" authorityName="Desutter-Grandcolas" box="[528,1014,330,355]" class="Insecta" family="Haglotettigoniidae" genus="Paragryllodes" kingdom="Animalia" order="Orthoptera" pageId="38" pageNumber="489" phylum="Arthropoda" rank="species" species="amani" status="n. sp.">
