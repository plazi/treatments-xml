<taxonomicName id="01C069AA9930F6D8E89F730112F290CC" class="Insecta" family="Aphididae" genus="Essigella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Essigella domenechi" order="Hemiptera" pageId="1" pageNumber="104" phylum="Arthropoda" rank="species" species="domenechi">Essigella domenechi</taxonomicName>
