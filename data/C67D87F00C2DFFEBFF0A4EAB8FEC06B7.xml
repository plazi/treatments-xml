<taxonomicName id="89D44D650C2DFFE8FEB74F6D8973022F" authority="Audouin, 1826" authorityName="Audouin" authorityYear="1826" box="[298,574,1829,1853]" class="Insecta" family="Sphingidae" genus="Enyo" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="94" phylum="Arthropoda" rank="species" species="nitida">
<emphasis id="7CA0EAF40C2DFFE8FEB74F6D8AD2022F" box="[298,415,1829,1853]" italics="true" pageId="1" pageNumber="94">Enyo nitida</emphasis>
