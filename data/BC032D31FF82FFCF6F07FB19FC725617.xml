<emphasis id="06DE4035FF83FFCF6E11FF3FFF0153D7" bold="true" box="[113,222,221,244]" pageId="17" pageNumber="368">Remarks.</emphasis>
<bibRefCitation id="503BE1D6FF83FFCF6E83FF3FFEB153D7" author="WEISE J." box="[227,366,221,244]" pageId="17" pageNumber="368" pagination="67 - 127" refId="ref20925" refString="WEISE J. 1910: Beitrag zur Kenntnis der amerikanischen Hispinen. Archiv fur Naturgeschichte 76: 67 - 127." type="journal article" year="1910">WEISE (1910)</bibRefCitation>
transferred 
<taxonomicName id="F3AAE7A4FF83FFCF6F8AFF3FFDB753D7" authorityName="Blanchard" authorityYear="1843" box="[490,616,221,244]" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="368" phylum="Arthropoda" rank="species" species="miniacea">
<emphasis id="06DE4035FF83FFCF6F8AFF3FFDB753D7" box="[490,616,221,244]" italics="true" pageId="17" pageNumber="368">C. miniacea</emphasis>
</taxonomicName>
to the genus 
<taxonomicName id="F3AAE7A4FF83FFCF6C90FF3FFCBC53D7" authorityName="Baly" authorityYear="1859" box="[752,867,221,244]" class="Insecta" family="Chrysomelidae" genus="Homalispa" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="368" phylum="Arthropoda" rank="genus">
