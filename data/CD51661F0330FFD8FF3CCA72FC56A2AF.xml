<quantity id="82007AEC0331FFD8FCE4CBD4FC56A23E" box="[847,999,1772,1796]" metricMagnitude="2" metricUnit="m" metricValue="4.05" metricValueMax="4.4" metricValueMin="3.7" pageId="15" pageNumber="216" unit="m" value="405.0" valueMax="440.0" valueMin="370.0">
