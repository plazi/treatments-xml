<quantity id="4CE0C2A8195CFFE225578F70FE167333" box="[366,440,1825,1849]" metricMagnitude="-2" metricUnit="m" metricValue="3.3" pageId="8" pageNumber="42" unit="mm" value="33.0">33mm</quantity>
