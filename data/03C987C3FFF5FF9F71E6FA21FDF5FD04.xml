male: 
<collectingCountry id="F3777645FFF3FF9C7327FA53FD5BF989" box="[598,766,1520,1544]" name="Argentina" pageId="5" pageNumber="6">ARGENTINA</collectingCountry>
: 
<collectingRegion id="49A4F837FFF3FF9C727AFA53FC2DF986" box="[779,904,1520,1543]" country="Argentina" name="Catamarca" pageId="5" pageNumber="6">
