<taxonomicName id="4C604D56FFF3FF9C75FBF9B6FB44F9AD" box="[1162,1249,1557,1580]" class="Magnoliopsida" family="Asteraceae" genus="Senecio" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="genus">
