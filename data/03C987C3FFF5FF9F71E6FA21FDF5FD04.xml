<typeStatus id="54DB8877FFF3FF9C7386F9FFFCF9F9F4" box="[759,860,1628,1653]" pageId="5" pageNumber="6">holotype</typeStatus>
, but 
<date id="FFDE1015FFF3FF9C72E6F9FFFBAEF9F4" box="[919,1035,1628,1653]" pageId="5" pageNumber="6" value="1973-02-18">18.ii.1973</date>
, 
<quantity id="4C989B30FFF3FF9C756BF9FFFBCEF9F5" box="[1050,1131,1628,1652]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="5" pageNumber="6" unit="m" value="3000.0">3000m</quantity>
, collected from flowers of 
<taxonomicName id="4C604D56FFF3FF9C71E6F923FEB2F919" authority="Cav." authorityName="Cav." box="[151,279,1664,1688]" class="Magnoliopsida" family="Asteraceae" genus="Stevia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="genus">
<emphasis id="B914EAC7FFF3FF9C71E6F923FF7EF916" box="[151,219,1664,1687]" italics="true" pageId="5" pageNumber="6">Stevia</emphasis>
Cav.
</taxonomicName>
(
<taxonomicName id="4C604D56FFF3FF9C7056F923FE00F919" box="[295,421,1664,1688]" class="Magnoliopsida" family="Asteraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
) “#378” and bearing labels numbered 79659 and 79660; same locality, but with date of 
<date id="FFDE1015FFF3FF9C71E6F907FEA5F93C" box="[151,256,1700,1725]" pageId="5" pageNumber="6" value="1973-01-28">28.i.1973</date>
and label numbered 78557, three females [
<mcCollectionCode id="5E4D535AFFF3FF9C7399F907FC96F93D" box="[744,819,1700,1724]" pageId="5" pageNumber="6">CTMI</mcCollectionCode>
