</collectingRegion>
, Cuesta Minas Capillitas, 
<quantity id="4C989B30FFF3FF9C75C6FA53FAADF989" box="[1207,1288,1520,1544]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="5" pageNumber="6" unit="m" value="3000.0">3000m</quantity>
, 
<date id="FFDE1015FFF3FF9C7467FA4CFA32F989" box="[1302,1431,1519,1544]" pageId="5" pageNumber="6" value="1973-12-30">30.xii.1973</date>
