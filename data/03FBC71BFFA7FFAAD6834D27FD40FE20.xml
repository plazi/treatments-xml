<taxonomicName id="4C520D8EFFA7FFABD5B64DB5FA15F877" authority="H. Milne Edwards, 1853" authorityName="H. Milne Edwards" authorityYear="1853" box="[930,1436,1963,1989]" class="Malacostraca" family="Varunidae" genus="Eriocheir" kingdom="Animalia" order="Decapoda" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="sinensis">
