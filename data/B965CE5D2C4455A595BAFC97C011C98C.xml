<taxonomicName id="B3C5F41002E8564570034BB33717DBF8" LSID="B965CE5D-2C44-55A5-95BA-FC97C011C98C" authority="Y. H. Tan &amp; H. B. Ding" authorityName="Y. H. Tan &amp; H. B. Ding" authorityYear="2020" class="Liliopsida" family="Zingiberaceae" genus="Amomum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Amomum schistocalyx" order="Zingiberales" pageId="0" pageNumber="139" phylum="Tracheophyta" rank="species" species="schistocalyx" status="sp. nov.">Amomum schistocalyx Y.H.Tan &amp; H.B.Ding</taxonomicName>
