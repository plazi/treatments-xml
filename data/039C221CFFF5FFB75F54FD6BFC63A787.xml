<collectionCode id="ED240BCFFFF5FFB75EC9FCC1FEBCA73C" box="[136,283,771,794]" pageId="3" pageNumber="742">TYPE MATERIAL</collectionCode>
. — 
<typeStatus id="548E2DA8FFF5FFB75F09FCC1FE0FA73E" box="[328,424,771,795]" pageId="3" pageNumber="742">Holotype</typeStatus>
: 
<accessionNumber id="94660EE9FFF5FFB75FF3FCC1FD96A73F" box="[434,561,770,794]" httpUri="http://www.ncbi.nlm.nih.gov/protein/MZB45691" pageId="3" pageNumber="742">
