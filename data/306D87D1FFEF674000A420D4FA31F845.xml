<taxonomicName id="7FC44D44FFEF674004DB22A6FA31F805" authorityName="Cockerell" authorityYear="1899" box="[1256,1436,1949,1972]" class="Insecta" family="Eriococcidae" genus="Carpochloroides" kingdom="Animalia" order="Hemiptera" pageId="91" pageNumber="92" phylum="Arthropoda" rank="genus">
