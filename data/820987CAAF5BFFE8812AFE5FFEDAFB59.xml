<taxonomicName id="CDA04D5FAF5BFFE8812AFE5FFEDAFE58" authority="Cope ED, 1876" authorityName="Cope ED" authorityYear="1876" baseAuthorityName="Cope" box="[56,268,436,455]" class="Amphibia" family="Scapherpetontidae" genus="Scapherpeton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Caudata" pageId="17" pageNumber="357" phylum="Chordata" rank="species" species="excisum" status="sp . nov .">
<emphasis id="38D4EACEAF5BFFE8812AFE5FFF61FE5B" bold="true" box="[56,183,436,455]" pageId="17" pageNumber="357">Scapherpeton</emphasis>
excisum
</taxonomicName>
, 
<taxonomicNameLabel id="23E757B5AF5BFFE88008FE52FE89FE5F" box="[282,351,441,455]" pageId="17" pageNumber="357" rank="species">sp. nov.</taxonomicNameLabel>
