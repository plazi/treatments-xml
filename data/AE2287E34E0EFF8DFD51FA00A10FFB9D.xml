<taxonomicName id="E18B4D764E0FFF8DFF16FBDAA0B9FBDE" box="[159,254,1040,1065]" class="Branchiopoda" family="Limnadiidae" genus="Eulimnadia" kingdom="Animalia" order="Diplostraca" pageId="9" pageNumber="382" phylum="Arthropoda" rank="species" species="garreti">
