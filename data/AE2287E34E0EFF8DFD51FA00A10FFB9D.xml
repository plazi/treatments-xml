<taxonomicName id="E18B4D764E0FFF8DFE34FC3BA254FBFD" authorityName="Sars" authorityYear="1896" box="[445,531,1008,1034]" class="Branchiopoda" family="Limnadiidae" genus="Eulimnadia" kingdom="Animalia" order="Diplostraca" pageId="9" pageNumber="382" phylum="Arthropoda" rank="species" species="dahli">
<emphasis id="14FFEAE74E0FFF8DFE34FC3BA254FBFD" box="[445,531,1008,1034]" italics="true" pageId="9" pageNumber="382">E. dahli</emphasis>
