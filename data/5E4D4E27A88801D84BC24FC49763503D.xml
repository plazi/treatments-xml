<subSubSection id="A08A8998289B69B68ADF5509F40A4E14" pageId="0" pageNumber="1" type="nomenclature">
<paragraph id="CB50CECF3AC69D17A9000B98C9CBCC63" pageId="0" pageNumber="1">
<taxonomicName id="16AF1484FAE4C9B79370C1D847E60EDC" LSID="828701" authority="Xiao L. He &amp; E. Horak" class="Agaricomycetes" family="Entolomataceae" genus="Entoloma" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Entoloma griseocarpum" order="Agaricales" pageId="0" pageNumber="1" phylum="Basidiomycota" rank="species" species="griseocarpum">Entoloma griseocarpum Xiao L. He &amp; E. Horak</taxonomicName>
<taxonomicNameLabel id="7D5FCABF712F81AA987AEF91ECD6D90C" pageId="0" pageNumber="1">sp. nov.</taxonomicNameLabel>
