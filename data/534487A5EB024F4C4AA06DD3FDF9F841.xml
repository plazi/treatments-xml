<taxonomicName id="1CED4D30EB034F4F4B526CE2FDE6F8C8" authority="Jałoszyński, 2016" authorityName="Jałoszyński" authorityYear="2016" box="[357,639,1247,1269]" class="Insecta" family="Staphylinidae" genus="Cephennodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="420" phylum="Arthropoda" rank="species" species="setifer" status="sp. n.">
<emphasis id="E999EAA1EB034F4F4B526CE2FE77F8C8" box="[357,494,1247,1269]" italics="true" pageId="5" pageNumber="420">Cephennodes</emphasis>
(s. str.) 
<emphasis id="E999EAA1EB034F4F48776CE2FDE6F8C8" box="[576,639,1247,1269]" italics="true" pageId="5" pageNumber="420">setifer</emphasis>
</taxonomicName>
<taxonomicNameLabel id="F2AA57DAEB034F4F48B16CE2FD26F8C9" box="[646,703,1247,1268]" pageId="5" pageNumber="420" rank="species">
