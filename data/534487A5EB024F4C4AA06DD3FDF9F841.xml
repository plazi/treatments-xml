<taxonomicName id="1CED4D30EB034F4F4EF56CFDFF45F8C8" authority="Jałoszyński, 2016" authorityName="Jałoszyński" authorityYear="2016" class="Insecta" family="Staphylinidae" genus="Cephennodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="420" phylum="Arthropoda" rank="species" species="hystrix" status="sp. n.">
<emphasis id="E999EAA1EB034F4F4EF56CFDFAD3F8EB" box="[1218,1354,1216,1238]" italics="true" pageId="5" pageNumber="420">Cephennodes</emphasis>
(s. str.) 
<emphasis id="E999EAA1EB034F4F4AA06CE2FF45F8C8" box="[151,220,1247,1269]" italics="true" pageId="5" pageNumber="420">hystrix</emphasis>
</taxonomicName>
<taxonomicNameLabel id="F2AA57DAEB034F4F4AD56CE2FE82F8C9" box="[226,283,1247,1268]" pageId="5" pageNumber="420" rank="species">
<emphasis id="E999EAA1EB034F4F4AD56CE2FE82F8C9" bold="true" box="[226,283,1247,1268]" pageId="5" pageNumber="420">sp. n.</emphasis>
