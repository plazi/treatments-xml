<taxonomicName id="4C294D15FF99FF8AFD30FE19F451FE2B" authorityName="" authorityYear="1895" box="[716,817,438,464]" class="Liliopsida" family="Poaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="2" pageNumber="392" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
