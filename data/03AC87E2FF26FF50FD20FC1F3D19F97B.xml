<specimenCode id="DBA39E8FFF26FF50FCA0F9093D56F907" box="[863,1073,1678,1699]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="157" pageNumber="157">AMNH 697326</specimenCode>
