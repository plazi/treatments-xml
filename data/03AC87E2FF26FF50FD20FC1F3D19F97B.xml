<location id="8EDA602FFF26FF50FCE8F9F73AD7F922" LSID="urn:lsid:plazi:treatment:03AC87E2FF26FF50FD20FC1F3D19F97B:8EDA602FFF26FF50FCE8F9F73AD7F922" box="[791,944,1648,1670]" municipality="Schraeder" name="Tweed River" pageId="157" pageNumber="157">Tweed River</location>
on 
<date id="FFBB1034FF26FF50FC1AF9F73DC4F921" box="[997,1187,1648,1670]" pageId="157" pageNumber="157" value="1912-08-16">
<collectingDate id="EFFFE9DCFF26FF50FC1AF9F73DC4F921" box="[997,1187,1648,1670]" pageId="157" pageNumber="157" value="1912-08-16">16 August 1912</collectingDate>
</date>
