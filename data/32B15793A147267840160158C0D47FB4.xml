<taxonomicName id="0AF5315A47A41C04A1B001508C0F68C8" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis comparata" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="comparata">Chrysis comparata</taxonomicName>
