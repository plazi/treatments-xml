<taxonomicName id="4591C5386D10B1317D028830B58BC7A5" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis distinguenda" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="distinguenda">Chrysis distinguenda</taxonomicName>
