<taxonomicName id="4C237C9CFFB10C1CFE13E21DFDC4F8D9" authority="Navas" authorityName="Navas" box="[474,591,1872,1896]" class="Insecta" family="Myrmeleontidae" genus="Neuroleon" kingdom="Animalia" order="Neuroptera" pageId="9" pageNumber="41" phylum="Arthropoda" rank="genus">
