<taxonomicName id="F2F44D375606FFB3D292F9CFFB32633B" baseAuthorityName="Schreber" baseAuthorityYear="1774" box="[937,1157,1580,1604]" class="Mammalia" family="Emballonuridae" genus="Saccopteryx" kingdom="Animalia" order="Chiroptera" pageId="26" pageNumber="26" phylum="Chordata" rank="species" species="leptura">
