<taxonomicName id="4C774D0F8155FF89C9B63737FEF5FA71" authority="Haliday" authorityName="Haliday" box="[151,361,1384,1411]" class="Insecta" family="Mymaridae" genus="Anagrus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
