<taxonomicName id="EB41EFB52030F8E0BB5882F9BB70A4FA" class="Magnoliopsida" family="Fabaceae" genus="Phaseolus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Phaseolus" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="genus">
<emphasis id="95369C86BD04E623A63FF33735FBCC60" italics="true" pageId="0" pageNumber="69">Phaseolus</emphasis>
</taxonomicName>
, namely 
<taxonomicName id="91FE7FD17F273BEE669E894209F00639" class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. albida" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="albida">
<emphasis id="1793ED41150BADA9BD547058DD62F636" italics="true" pageId="0" pageNumber="69">C. albida</emphasis>
</taxonomicName>
Matta &amp; Belliard, 
<taxonomicName id="56D431388208D46DAA773AE6796EFA25" class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. apii" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="apii">
<emphasis id="F8933ED5135F5BED362C2C8F8FA7A403" italics="true" pageId="0" pageNumber="69">C. apii</emphasis>
</taxonomicName>
, 
<taxonomicName id="86C4B68134BFDF34B708C946A731BE93" class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. canescens" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="canescens">
<emphasis id="26E0CFDBFE3A886FBC9BA07897C0FDAC" italics="true" pageId="0" pageNumber="69">C. canescens</emphasis>
</taxonomicName>
, 
<taxonomicName id="7EF85F04078DED89752A7DA340C0F814" class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. canscorina" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="canscorina">
<emphasis id="895631EA429986C1C277C49A677C5A40" italics="true" pageId="0" pageNumber="69">C. canscorina</emphasis>
</taxonomicName>
, 
<taxonomicName id="CF7A7A448771DB91742D6F998127EB24" class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. caracallae" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="caracallae">
<emphasis id="63A432284BB61F4B01FF7FCEC4648D11" italics="true" pageId="0" pageNumber="69">C. caracallae</emphasis>
</taxonomicName>
, 
