<taxonomicName id="4C344D515C9DFF3948F6FD24FD6DFCEE" authority="Santos &amp; Azevedo, 2000" authorityName="Santos &amp; Azevedo" authorityYear="2000" box="[151,675,762,787]" class="Insecta" family="Bethylidae" genus="Anisepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="130" pageNumber="131" phylum="Arthropoda" rank="species" species="punctatus">
