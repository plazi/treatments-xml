<quantity id="4C98D5BEFFDAFF8210DFA17CFCC4F89D" box="[762,850,1885,1912]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="3" pageNumber="334" unit="mm" value="5.3">5.3 mm</quantity>
