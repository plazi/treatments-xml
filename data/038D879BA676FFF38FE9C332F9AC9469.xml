<taxonomicName id="4C244D0EA676FFF38E37C1E1FEB595D8" box="[329,645,1687,1712]" class="Insecta" family="Staphylinidae" genus="Scaphidium" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="60" phylum="Arthropoda" rank="species" species="stigmatinotum">
