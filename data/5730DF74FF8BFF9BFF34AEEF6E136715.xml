<taxonomicName id="189915E1FF8BFF98FE82AF346F2F615B" authority="Muesebeck 1955" authorityName="Muesebeck" authorityYear="1955" box="[289,638,1813,1835]" class="Insecta" family="Braconidae" genus="Perilitus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="250" phylum="Arthropoda" rank="species" species="eximius">
