<taxonomicName id="57BE87D4FF51FF7BFE359C484FCA0CA1" box="[499,641,892,918]" class="Actinopterygii" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="212" pageNumber="213" phylum="Chordata" rank="order">Perciformes</taxonomicName>
