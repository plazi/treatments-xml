<emphasis id="B9398400CE6AEC407F8AF8D8C530CF64" box="[499,561,1859,1884]" italics="true" pageId="5" pageNumber="6">Pupa</emphasis>
<taxonomicName id="4C4D2391CE6AEC407C41F8D8C470CF65" authority="Draparnaud, 1801" authorityName="Draparnaud" authorityYear="1801" box="[568,881,1859,1885]" class="Gastropoda" family="Chondrinidae" genus="Granopupa" kingdom="Animalia" order="Stylommatophora" pageId="5" pageNumber="6" phylum="Mollusca" rank="species" species="granum">
