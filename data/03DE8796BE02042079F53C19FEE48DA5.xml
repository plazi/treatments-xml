<taxonomicName id="4C774D03BE05042278CE3FA4FDBF8AF5" box="[391,619,1678,1699]" class="Aves" family="Alaudidae" genus="Eremophila" kingdom="Animalia" order="Passeriformes" pageId="60" pageNumber="60" phylum="Chordata" rank="species" species="duttonii">
