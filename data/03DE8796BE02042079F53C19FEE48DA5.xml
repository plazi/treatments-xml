(
<taxonomicName id="4C774D03BE0504227BEB3DE1FCF688B7" box="[674,802,1227,1249]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="60" pageNumber="60" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
