(pl. 38A, B), 
<emphasis id="B903EA92BE05042279B03F86FD948A96" box="[249,576,1707,1729]" italics="true" pageId="60" pageNumber="60">
<taxonomicName id="4C774D03BE05042279B03F86FE578A96" box="[249,387,1707,1729]" class="Aves" family="Alaudidae" genus="Eremophila" kingdom="Animalia" order="Passeriformes" pageId="60" pageNumber="60" phylum="Chordata" rank="species" species="freelingii">E. freelingii</taxonomicName>
, 
<taxonomicName id="4C774D03BE05042278D93F86FD948A96" box="[400,576,1707,1729]" class="Aves" family="Alaudidae" genus="Eremophila" kingdom="Animalia" order="Passeriformes" pageId="60" pageNumber="60" phylum="Chordata" rank="species" species="macdonnellii">E. macdonnellii</taxonomicName>
