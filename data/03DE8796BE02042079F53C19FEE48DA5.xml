<taxonomicName id="4C774D03BE0504227A393DE1FBFE88B6" box="[880,1066,1227,1248]" class="Magnoliopsida" family="Loranthaceae" genus="Amyema" kingdom="Plantae" order="Santalales" pageId="60" pageNumber="60" phylum="Tracheophyta" rank="species" species="lucasii">
