<collectingCounty id="4A314E3A9E08FFC0FCB17249FC5F1025" box="[770,937,1619,1640]" pageId="12" pageNumber="12">upper Sumaco</collectingCounty>
</materialsCitation>
; 
<materialsCitation id="13873CEB9E08FFC1FC017248FE3C17D4" ID-GBIF-Occurrence="3059738924" collectingDate="1922-09" collectingDateMax="1928-07" collectingDateMin="1922-09" collectionCode="MCZ, ANSP" county="Baeza" lastPageId="13" lastPageNumber="13" location="The" municipality="Baeza" pageId="12" pageNumber="12" specimenCode="AMNH 183493-183496, 183500, AMNH 183494, AMNH 183500, AMNH 176261, AMNH 173844, 176263-176265" specimenCount="3" typeStatus="paratype">
