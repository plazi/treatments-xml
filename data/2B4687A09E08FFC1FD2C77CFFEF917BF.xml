<location id="A630606D9E08FFC0FCE071E0FC081042" LSID="urn:lsid:plazi:treatment:2B4687A09E08FFC1FD2C77CFFEF917BF:A630606D9E08FFC0FCE071E0FC081042" box="[851,1022,1530,1551]" county="upper Sumaco" municipality="Baeza" name="upper Sumaco" pageId="12" pageNumber="12">upper Sumaco</location>
