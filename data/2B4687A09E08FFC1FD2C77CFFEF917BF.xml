<collectionCode id="C5FEAE739E08FFC0FBFF71BBFB5C13FB" box="[1100,1194,1441,1462]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="12" pageNumber="12">AMNH</collectionCode>
