<geoCoordinate id="38EC504DD659FFE7FC4CFADFFC1AFAFC" box="[954,1004,1374,1401]" degrees="4" direction="south" minutes="9" orientation="latitude" pageId="28" pageNumber="429" precision="925" value="-4.15">
