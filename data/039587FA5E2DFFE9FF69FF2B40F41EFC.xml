<taxonomicName id="4C3C4D6F5E2DFFE9FD85FEF044341E35" authority="Linnavuori, 1965" authorityName="Linnavuori" authorityYear="1965" box="[635,1064,332,357]" class="Insecta" family="Miridae" genus="Phytocoris" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="species" species="irroratus">
<emphasis id="B948EAFE5E2DFFE9FD85FEF043431E34" box="[635,863,332,357]" italics="true" pageId="14" pageNumber="465">Phytocoris irroratus</emphasis>
<bibRefCitation id="EFAD4B1D5E2DFFE9FC98FEF044341E35" author="Linnavuori" box="[870,1064,332,357]" pageId="14" pageNumber="465" refString="Linnavuori, R. (1965) Studies on the South- and Eastmediterranean Hemipterous Fauna. Acta Entomologica Fennica, 21, 1 - 70." type="journal article" year="1965">Linnavuori, 1965</bibRefCitation>
</taxonomicName>
was junior primary homonym of 
<taxonomicName id="4C3C4D6F5E2DFFE9FF69FED3424C1ED8" authority="Blanchard, 1852" authorityName="Blanchard" authorityYear="1852" box="[151,592,367,392]" class="Insecta" family="Miridae" genus="Phytocoris" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="species" species="irroratus">
