<emphasis id="08FBEAA27E34FFE9FF136D308127FB59" bold="true" box="[151,306,1140,1165]" pageId="31" pageNumber="122">
<typeStatus id="E53488127E34FFE9FF136D3080C7FB59" box="[151,210,1140,1165]" pageId="31" pageNumber="122">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="FD8F4D337E34FFE9FEBD6D3082CBFB58" authority="Holmes, 1908" authorityName="Holmes" authorityYear="1908" box="[313,734,1140,1165]" class="Malacostraca" family="Scopelocheiridae" genus="Scopelocheirus" kingdom="Animalia" order="Amphipoda" pageId="31" pageNumber="122" phylum="Arthropoda" rank="species" species="coecus">
