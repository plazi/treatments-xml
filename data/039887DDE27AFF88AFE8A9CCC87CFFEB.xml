<figureCitation id="130A2A4EE27BFF88AA4FAC2FC9E3FF85" box="[1328,1432,891,916]" captionStart="FIGURES 219 – 222" captionStartId="74.[151,269,1963,1987]" captionTargetBox="[200,1387,201,1930]" captionTargetId="figure@74.[187,1407,194,1954]" captionTargetPageId="74" captionText="FIGURES 219 – 222. Insula squamata sp. nov., male holotype and female paratype. 219, palp, ventral view; 220, palp, retrolateral view; 221, epigyne; 222, vulva. Scale bars: 0.1 mm." httpUri="https://zenodo.org/record/284794/files/figure.png" pageId="17" pageNumber="18">
Figs 
<number id="3DF924ECE27BFF88AA14AC2FC9E3FF85" box="[1387,1432,891,915]" pageId="17" pageNumber="18" value="222228.0">222</number>
</figureCitation>
, 
<figureCitation id="130A2A4EE27BFF88AFE8ACCBCCBDFFA1" box="[151,198,927,951]" captionStart="FIGURES 227 – 231" captionStartId="76.[151,269,1971,1995]" captionTargetBox="[192,1389,194,1950]" captionTargetId="figure@76.[192,1389,194,1954]" captionTargetPageId="76" captionText="FIGURES 227 – 231. Insula squamata sp. nov., male holotype and female paratype. 227, epigyne; 228, vulva; 229, male habitus, dorsal view; 230, female habitus, dorsal view; 231, female habitus, ventral view." httpUri="https://zenodo.org/record/284796/files/figure.png" pageId="17" pageNumber="18">228</figureCitation>
