<subSubSection id="C3368A82E61CFA72FF55FE5F083EC966" pageId="10" pageNumber="487" type="materials_examined">
<paragraph id="8B93D909E61CFA72FF55FE5F083EC966" blockId="10.[151,1437,151,1149]" pageId="10" pageNumber="487">
<emphasis id="B958051BE61CFA72FF55FE5F0C37C91E" bold="true" box="[199,374,511,536]" pageId="10" pageNumber="487">
