<paragraph id="50C620D44E102F4B416BE220A931BE26" pageId="0" pageNumber="77">
<taxonomicName id="710C027A052D6BB074EA99AB3DE504D8" class="Gastropoda" family="Bignoniaceae" genus="Martinella" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Martinella martini" order="Lamiales" pageId="0" pageNumber="77" phylum="Mollusca" rank="species" species="martini">
