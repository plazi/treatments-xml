<taxonomicName id="4C014D2CFFE4FFA278666E57FE339843" box="[350,440,1891,1913]" class="Insecta" family="Haglotettigoniidae" genus="Phaloria" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="29" phylum="Arthropoda" rank="genus">
