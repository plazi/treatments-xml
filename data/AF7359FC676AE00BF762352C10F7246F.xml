<taxonomicName id="1FEDA5CBA04A92D559E3229E6AA446A4" LSID="http://zoobank.org/18F14A57-CA9C-4329-BA1F-F885461CCCB6" class="Insecta" family="Osmylidae" genus="Heterosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterosmylus processus" order="Neuroptera" pageId="4" pageNumber="111" phylum="Arthropoda" rank="species" species="processus">Heterosmylus processus</taxonomicName>
