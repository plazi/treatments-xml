<taxonomicName id="014D14E0B3559C18A9402A9A58BDF59E" class="Insecta" family="Carabidae" genus="Rhysodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhysodes" order="Coleoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="genus">Rhysodes</taxonomicName>
