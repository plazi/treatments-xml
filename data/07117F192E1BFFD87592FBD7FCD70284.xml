<typeStatus id="500370AD2E1AFFD874A7FF24FF3F02A5" box="[132,188,133,157]" pageId="5" pageNumber="988">Type</typeStatus>
material of 
<taxonomicName id="48B8B58C2E1AFFD87541FF24FD9302A4" authorityName="Moore" authorityYear="1992" box="[354,528,132,156]" class="Malacostraca" family="Stegocephalidae" genus="Phippsiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="988" phylum="Arthropoda" rank="species" species="cascadiensis">
