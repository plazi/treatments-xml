<emphasis id="25E3EAD9563BFFC3E89AFE10FD86FE74" bold="true" box="[151,525,403,430]" pageId="21" pageNumber="22">
<taxonomicName id="D0974D48563BFFC3E89AFE10FEE5FE77" box="[151,366,404,430]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="braueri" status="new species">
<emphasis id="25E3EAD9563BFFC3E89AFE10FEE5FE77" bold="true" box="[151,366,404,430]" italics="true" pageId="21" pageNumber="22">Chimarra braueri</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="3ED057A2563BFFC3E970FE17FD86FE74" box="[381,525,403,429]" pageId="21" pageNumber="22" rank="species">new species</taxonomicNameLabel>
</emphasis>
