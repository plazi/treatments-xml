<collectingCountry id="F33C3ACECB5AC26BCF23FCF9FE1EC20D" box="[322,406,786,809]" name="Mexico" pageId="70" pageNumber="71">Mexico</collectingCountry>
, 
<collectingRegion id="49EFB4BCCB5AC26BCFC0FCF9FD8CC20D" box="[417,516,786,809]" country="Mexico" name="Guerrero" pageId="70" pageNumber="71">Guerrero</collectingRegion>
, 
<collectorName id="26DE1F88CB5AC26BCC6FFCF9FD70C20D" box="[526,760,786,809]" pageId="70" pageNumber="71">Pie de la Cuesta, D. S</collectorName>
