<materialsCitation id="3B437003CB5AC26BCEF6FCF9FC17C20D" box="[151,927,786,809]" collectionCode="CISC" collectorName="Pie de la Cuesta, D. S" country="Mexico" location="Guerrero" pageId="70" pageNumber="71" specimenCount="1" stateProvince="Guerrero" typeStatus="holotype">
