species: 
<taxonomicName id="29DFE08754175353FECCF9FE8F63C98E" authority="Simon, 1910" authorityName="Simon" authorityYear="1910" box="[294,598,1654,1677]" class="Arachnida" family="Salticidae" genus="Belippo" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="548" phylum="Arthropoda" rank="species" species="anguina">
<emphasis id="DCAB471654175353FECCF9FE8CFBC98F" box="[294,462,1654,1677]" italics="true" pageId="1" pageNumber="548">Belippo anguina</emphasis>
<bibRefCitation id="8A4EE6F554175353FE3EF9FE8F63C98E" author="Simon" box="[468,598,1654,1676]" pageId="1" pageNumber="548" refString="Simon, E. (1910) Arachnides recueillis par L. Fea sur la cote occidentale d'Afrique. 2 e partie. Annali del Museo Civico di Storia Naturale di Genova, 44, 335 - 449." type="journal article" year="1910">
