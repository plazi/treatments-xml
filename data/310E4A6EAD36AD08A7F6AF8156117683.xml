<quantity id="7E5F569DAD36AD08A582A9E5570B7613" box="[739,869,1779,1804]" metricMagnitude="2" metricUnit="m" metricValue="6.5" metricValueMax="7.5" metricValueMin="5.5" pageId="9" pageNumber="274" unit="m" value="650.0" valueMax="750.0" valueMin="550.0">550–750 m</quantity>
