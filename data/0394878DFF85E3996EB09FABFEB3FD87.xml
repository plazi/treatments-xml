, HW = 
<quantity id="4CC59B7EFF86E39F6DED9F1EFBF8F9CB" box="[970,1058,1606,1630]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="130" pageNumber="131" unit="mm" value="0.7">0.7 mm</quantity>
, PL = 
