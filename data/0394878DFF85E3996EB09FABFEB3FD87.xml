<paragraph id="8B82369BFF86E39F6EE09F1DFD5AF917" blockId="130.[151,1437,1533,2029]" pageId="130" pageNumber="131">
<emphasis id="B949EA89FF86E39F6EE09F1DFEA7F9C8" bold="true" box="[199,381,1605,1630]" pageId="130" pageNumber="131">Measurements.</emphasis>
BL = 
<quantity id="4CC59B7EFF86E39F6FE29F1EFDC7F9CB" box="[453,541,1606,1630]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="130" pageNumber="131" unit="mm" value="1.8">1.8 mm</quantity>
