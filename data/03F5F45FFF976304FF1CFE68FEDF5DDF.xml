<taxonomicName id="4C5C3ECAFF976305FEE2F886FD975C35" authority="Muona, 2022" authorityName="Muona" authorityYear="2022" box="[318,570,1866,1893]" class="Insecta" family="Eucnemidae" genus="Asiocnemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="species" species="colombicus" status="n. sp.">
<emphasis id="B928995BFF976305FEE2F886FD975C35" box="[318,570,1866,1893]" italics="true" pageId="10" pageNumber="9">Asiocnemis colombicus</emphasis>
</taxonomicName>
<emphasis id="B928995BFF976305FD9EF886FDD25C34" bold="true" box="[578,639,1867,1893]" pageId="10" pageNumber="9">
