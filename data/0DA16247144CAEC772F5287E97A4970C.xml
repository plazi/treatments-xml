<taxonomicName id="1F5E643478903AB755166CCF654D76F0" LSID="http://zoobank.org/9DAA0C1A-15FE-40C1-9C9F-19C9A6676F8F" authority="Olmi, Xu &amp; Guglielmino" authorityName="Jurine" authorityYear="1807" class="Insecta" family="Dryinidae" genus="Anteon" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Anteon huettingeri" order="Hymenoptera" pageId="2" pageNumber="143" phylum="Arthropoda" rank="species" species="huettingeri">Anteon huettingeri Olmi, Xu &amp; Guglielmino</taxonomicName>
<taxonomicNameLabel id="5A7998B94D9D56A8B6018D0DB23A1367" pageId="2" pageNumber="143">sp. n.</taxonomicNameLabel>
