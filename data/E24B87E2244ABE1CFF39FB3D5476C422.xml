<taxonomicName id="ADE24D77244BBE1CFEAAFF795788C5EA" box="[260,393,188,212]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="5" pageNumber="48" phylum="Tracheophyta" rank="species" species="sylvestris">
<emphasis id="5896EAE6244BBE1CFEAAFF795788C5EA" box="[260,393,188,212]" italics="true" pageId="5" pageNumber="48">P. sylvestris</emphasis>
