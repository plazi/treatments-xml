<taxonomicName id="ADE24D77244ABE1DFBAAF8615231C282" box="[1028,1072,1957,1980]" class="Arachnida" family="Phytoptidae" genus="Trisetacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="pini">
<emphasis id="5896EAE6244ABE1DFBAAF8615231C282" box="[1028,1072,1957,1980]" italics="true" pageId="4" pageNumber="47">pini</emphasis>
</taxonomicName>
'. 
<bibRefCitation id="0E734B05244ABE1DFBE9F86052FDC282" author="Connold" box="[1095,1276,1956,1980]" pageId="4" pageNumber="47" refString="Connold, E. J. (1909) Plant Galls of Great Britain. Adlard &amp; Son, London, 292 pp." type="book" year="1909">
Connold (
<number id="DC2A24D3244ABE1DFB14F86052F4C282" box="[1210,1269,1956,1980]" pageId="4" pageNumber="47" value="1909.0">1909</number>
)
</bibRefCitation>
and 
<bibRefCitation id="0E734B05244ABE1DFA94F86056E0C2DE" author="Swanton" pageId="4" pageNumber="47" refString="Swanton, E. W. (1912) British Plant Galls. Methuen &amp; Co. Ltd., London, 287 pp." type="book" year="1912">
