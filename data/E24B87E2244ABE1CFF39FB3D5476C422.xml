<bibRefCitation id="0E734B05244BBE1CFE3AFF5354D8C591" author="Bagnall" box="[404,729,151,175]" pageId="5" pageNumber="48" refString="Bagnall, R. S. &amp; Harrison, J. W. H. (1928) A Catalogue of the British Eriophyidae. Annals and Magazine of Natural History, 10 (2), 427 - 445." type="journal article" year="1928">
Bagnall and Harrison (
<number id="DC2A24D3244BBE1CFD3AFF5354D1C591" box="[660,720,151,175]" pageId="5" pageNumber="48" value="1928.0">1928</number>
)
</bibRefCitation>
, 
<bibRefCitation id="0E734B05244BBE1CFD4BFF535587C591" author="Burkill" box="[741,902,151,175]" pageId="5" pageNumber="48" refString="Burkill, H. J. (1930) British Gall Mites. London Naturalist, 58 - 68." type="book chapter" year="1930">
Burkill (
<number id="DC2A24D3244BBE1CFCEAFF53557FC591" box="[836,894,151,175]" pageId="5" pageNumber="48" value="1930.0">1930</number>
)
</bibRefCitation>
and 
<bibRefCitation id="0E734B05244BBE1CFC10FF535246C591" author="Turk" box="[958,1095,151,175]" pageId="5" pageNumber="48" refString="Turk, F. A. (1953) A synonymic Catalogue of British Acari: Part II. Annals and" type="book" year="1953">
<collectingCountry id="12F57664244BBE1CFC10FF5355F2C591" box="[958,1011,151,175]" name="Turks and Caicos Islands" pageId="5" pageNumber="48">Turk</collectingCountry>
(
