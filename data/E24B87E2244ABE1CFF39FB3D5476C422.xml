(
<bibRefCitation id="0E734B05244ABE1DFEFCF86057E6C282" author="Nalepa" box="[338,487,1956,1980]" pageId="4" pageNumber="47" refString="Nalepa, A. (1887) Die Anatomie der Phytopten. Sitzungsberichte der kaiserlichen Akademie der Wissenschaften. Matchematische-naturwissenschaftliche Klasse, Wien. Abtheilung 1, 96 (4), 115 - 165 + 2 pls." type="journal article" year="1887">
Nalepa 
<number id="DC2A24D3244ABE1DFE00F86057E6C282" box="[430,487,1956,1980]" pageId="4" pageNumber="47" value="1887.0">1887</number>
</bibRefCitation>
)
</taxonomicName>
and 
<taxonomicName id="ADE24D77244ABE1DFD83F86154ACC282" box="[557,685,1956,1980]" class="Arachnida" family="Phytoptidae" genus="Trisetacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="ehmanni">
