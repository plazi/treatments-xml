<taxonomicName id="ADE24D77244ABE1DFF39F86157EEC282" authority="Nalepa 1887" authorityName="Nalepa" authorityYear="1887" box="[151,495,1956,1980]" class="Arachnida" family="Phytoptidae" genus="Trisetacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="pini">
