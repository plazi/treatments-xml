<collectingCountry id="E9AC76288910FFF3FC94FD9319EB04BB" box="[834,901,1421,1446]" name="China" pageId="9" pageNumber="540">China</collectingCountry>
(Hainan), 
<collectingCountry id="E9AC76288910FFF3FBD7FD931E5304BB" box="[1025,1085,1421,1446]" name="India" pageId="9" pageNumber="540">India</collectingCountry>
, 
<collectingCountry id="E9AC76288910FFF3FB9CFD931EF204BB" box="[1098,1180,1421,1446]" name="Taiwan" pageId="9" pageNumber="540">Taiwan</collectingCountry>
