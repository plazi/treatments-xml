<paragraph id="8BC34F687047BF22FF4DFBA7FEFFFBDA" blockId="12.[189,324,1047,1073]" box="[189,324,1047,1073]" pageId="12" pageNumber="13">
<heading id="D08BF8047047BF22FF4DFBA7FEFFFBDA" bold="true" box="[189,324,1047,1073]" fontSize="11" level="3" pageId="12" pageNumber="13" reason="3">
