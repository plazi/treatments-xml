<taxonomicName id="9F6855880763BC33DE8703FA0575C2A5" LSID="http://zoobank.org/5B16A804-AF56-4C34-A1F5-B66A42CA6A44" authority="Sasakawa &amp; Ito" class="Insecta" family="Carabidae" genus="Trephionus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trephionus bifidilobatus" order="Coleoptera" pageId="8" pageNumber="99" phylum="Arthropoda" rank="species" species="bifidilobatus">
