<collectionCode id="ED15AE636A70FF86D2CCFE09FA9B0B5C" box="[1238,1319,490,513]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="9" pageNumber="202" type="Museum">FMNH</collectionCode>
