<taxonomicName id="FC9A14DA8E219B7FFD3BFE0F7888FDDA" box="[651,727,488,514]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="cauky">cauky</taxonomicName>
