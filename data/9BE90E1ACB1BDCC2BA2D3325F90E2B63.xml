<taxonomicName id="EB97CC333DFCDB1E0F315516A0A61B26" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136149" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica stricta Jerdon" lsidName-HNS="Formica stricta Jerdon" order="Hymenoptera" pageNumber="123" phylum="Arthropoda" rank="species" species="stricta">Formica stricta</taxonomicName>
