<paragraph id="8BD88A4A533DFFA5FF52FB58FD06FEFD" blockId="8.[212,1172,1148,1689]" lastBlockId="9.[144,1104,168,352]" lastPageId="9" lastPageNumber="882" pageId="8" pageNumber="881">
<taxonomicName id="4C67F1C9533DFFA4FF52FB58FE29FB19" authorityName="Mello-Leitao" authorityYear="1923" box="[212,498,1187,1211]" class="Arachnida" family="Theraphosidae" genus="Hapalotremus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="881" phylum="Arthropoda" rank="species" species="cyclothorax">
