(
<collectingRegion id="49E0F86E7A0CFFBCFE46AB7C0D63B1BF" box="[441,531,368,392]" country="China" name="Yunnan" pageId="4" pageNumber="331">Yunnan</collectingRegion>
, 
<collectingRegion id="49E0F86E7A0CFFBCFDE1AB630DF3B1BF" box="[542,643,367,392]" country="China" name="Guangxi" pageId="4" pageNumber="331">Guangxi</collectingRegion>
