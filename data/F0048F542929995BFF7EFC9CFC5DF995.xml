<geoCoordinate id="1D9958852929995BFD41FB53FCFBFB61" box="[680,779,1180,1206]" degrees="31" direction="east" minutes="09" orientation="longitude" pageId="13" pageNumber="501" precision="925" value="31.15">31°09’ E</geoCoordinate>
