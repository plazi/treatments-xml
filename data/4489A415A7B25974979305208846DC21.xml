<taxonomicName id="F973A3C8678DBAE4B826F23760F50585" authorityName="Fairmaire" authorityYear="1891" class="Insecta" family="Melolonthidae" genus="Serica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Serica opacifrons" order="Coleoptera" pageId="0" pageNumber="35" phylum="Arthropoda" rank="species" species="opacifrons">
