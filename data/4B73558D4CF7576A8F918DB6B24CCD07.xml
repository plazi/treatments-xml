<emphasis id="094459D432C96C15AAF32607A4F61C11" italics="true" pageId="6" pageNumber="53">Clematis guniuensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="286D3FBDB7F2E987A8E3145BFA076A92" genus="C." lsidName="C. huchouensis" pageId="6" pageNumber="53" rank="species" species="huchouensis">
<emphasis id="30B957DE6B75599C9A9628051E79D094" italics="true" pageId="6" pageNumber="53">C. huchouensis</emphasis>
</taxonomicName>
and 
<taxonomicName id="A9425F1D2A0AF4CDE4BE05146FD6C78E" genus="C." lsidName="C. florida" pageId="6" pageNumber="53" rank="species" species="florida">
<emphasis id="F66CF4C8F2B95D58BCC3B4749F5AD229" italics="true" pageId="6" pageNumber="53">C. florida</emphasis>
</taxonomicName>
.
</paragraph>
</caption>
