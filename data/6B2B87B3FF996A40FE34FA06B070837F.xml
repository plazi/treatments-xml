<collectingCountry id="9B957635FF986A40FECAFD4FB3CA837F" box="[323,387,747,772]" name="China" pageId="5" pageNumber="194">China</collectingCountry>
(
<collectingRegion id="2146F847FF986A40FE19FD4FB3A8837F" box="[400,481,747,772]" country="China" name="Hainan" pageId="5" pageNumber="194">Hainan</collectingRegion>
Island).
