<heading id="D0B33A1FAC116143FF28F5E0B4B2F97E" bold="true" box="[151,662,1689,1715]" fontSize="11" level="1" pageId="8" pageNumber="41" reason="1">
<taxonomicName id="4C44F6F0AC116143FF28F5E0B4B2F97E" authority="Doreste, 1981" authorityName="Doreste" authorityYear="1981" box="[151,662,1689,1715]" class="Arachnida" family="Tetranychidae" genus="Mononychellus" kingdom="Animalia" order="Prostigmata" pageId="8" pageNumber="41" phylum="Arthropoda" rank="species" species="progresivus">
