<taxonomicName id="AFA64D61FFD3297F7029E3FD3AE5FB02" box="[628,726,1157,1181]" class="Insecta" family="Thespidae" genus="Oligonyx" kingdom="Animalia" order="Mantodea" pageId="4" pageNumber="265" phylum="Arthropoda" rank="genus">
