<taxonomicName id="4C326DC91A41FFDED28A0B28FD8AFDD3" authority="Karsch, 1893: 91" authorityName="Karsch" authorityPageNumber="91" authorityYear="1893" box="[211,537,577,597]" class="Insecta" family="Acrididae" genus="Serpusia" kingdom="Animalia" order="Orthoptera" pageId="31" pageNumber="342" phylum="Arthropoda" rank="species" species="catamita">
