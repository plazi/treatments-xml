<paragraph id="8BAA36860E10FFCFFF14C11ACEFD7439" blockId="14.[151,1232,1138,1625]" box="[199,739,1570,1593]" pageId="14" pageNumber="15">
<treatmentCitationGroup id="AB0511A80E10FFCFFF14C11ACEFD7439" box="[199,739,1570,1593]" pageId="14" pageNumber="15">
<taxonomicName id="4C154D050E10FFCFFF14C11ACEC17439" authority="Townes, 1945: 55" authorityName="Townes" authorityPageNumber="55" authorityYear="1945" box="[199,735,1570,1593]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="subSpecies" species="pedatum" subSpecies="excelsius">
<emphasis id="B961EA940E10FFCFFF14C11ACE097439" box="[199,535,1570,1593]" italics="true" pageId="14" pageNumber="15">Polypedilum pedatum excelsius</emphasis>
<treatmentCitation id="0AB410970E10FFCFFDCEC11ACEC17439" author="Townes" box="[541,735,1570,1593]" page="55" pageId="14" pageNumber="15" year="1945">
