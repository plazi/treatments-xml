<taxonomicName id="29CB4D7AE80BFF84FF4DE62DFE80FC4D" authorityName="Hancock" authorityYear="1909" box="[151,283,871,897]" class="Insecta" family="Tetrigidae" genus="Halmatettix" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
