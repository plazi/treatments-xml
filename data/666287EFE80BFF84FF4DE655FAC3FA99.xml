<taxonomicName id="29CB4D7AE80BFF84FAC2E62DFEEEFC6A" authority="Grant, 1955" authorityName="Grant" authorityYear="1955" class="Insecta" family="Tetrigidae" genus="Halmatettix" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="allardi">
