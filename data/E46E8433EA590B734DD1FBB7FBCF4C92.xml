<taxonomicName id="ABC74EA6EA590B734C7AFB68FCC94EB4" authority="Medikus (1787: 28)" authorityName="Medikus" authorityPageNumber="28" authorityYear="1787" box="[316,801,1154,1180]" class="Magnoliopsida" family="Malvaceae" genus="Abutilon" kingdom="Plantae" order="Malvales" pageId="1" pageNumber="40" phylum="Tracheophyta" rank="species" species="theophrasti">
