(as 
<taxonomicName id="4C1E4D2D1D3BFFB5FDEFE258B604FA1E" authority="Champion" authorityName="Champion" authorityYear="1886" class="Insecta" family="Tenebrionidae" genus="Cynaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="434" phylum="Arthropoda" rank="species" species="opacus">
