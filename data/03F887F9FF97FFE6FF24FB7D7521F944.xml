<taxonomicName id="4C514D6CFF97FFE6FBDEF94A7167F97D" box="[1133,1285,1717,1742]" class="Insecta" family="Buprestidae" genus="Julodimorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="47" phylum="Arthropoda" rank="species" species="saundersii">
