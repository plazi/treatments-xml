<taxonomicName id="E3E18191FF4AFF431ED8F86BFDD8F9EA" baseAuthorityName="Benoist" baseAuthorityYear="1944" box="[431,593,1591,1615]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="corallina" subGenus="Chaetalictus">
