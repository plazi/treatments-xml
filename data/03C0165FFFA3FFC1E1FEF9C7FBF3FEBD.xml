<number id="3DA1B56EFFA2FFC7E319FE93FD9DFED8" box="[624,638,269,293]" pageId="3" value="2.0">2</number>
<emphasis id="B91D7B5BFFA2FFC7E317FE92FD6FFED8" box="[638,652,268,293]" italics="true" pageId="3">b</emphasis>
