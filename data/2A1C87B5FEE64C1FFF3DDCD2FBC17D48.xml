<paragraph id="A20A36A3FEE74C1CFEC8D9C5FB447C1C" blockId="369.[313,1348,1631,1818]" pageId="369" pageNumber="370">
<taxonomicName id="65B54D20FEE74C1CFEC8D9C5FDF37DCC" authority="Schnabel, 2009" authorityName="Schnabel" authorityYear="2009" box="[313,572,1718,1738]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="369" pageNumber="370" phylum="Arthropoda" rank="species" species="paku">
