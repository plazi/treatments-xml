<quantity id="B1579B30FFA94E1CE146FA4AFB45FA56" box="[1075,1182,1416,1442]" metricMagnitude="-4" metricUnit="m" metricValue="7.1" pageId="3" pageNumber="47" unit="mm" value="0.71">0.71 mm</quantity>
, Ediam 
<quantity id="B1579B30FFA94E1CE18BFA4AFAB2FA56" box="[1278,1385,1416,1442]" metricMagnitude="-4" metricUnit="m" metricValue="2.1" pageId="3" pageNumber="47" unit="mm" value="0.21">0.21 mm</quantity>
