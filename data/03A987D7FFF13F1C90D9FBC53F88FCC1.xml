<location id="8EDF601AFFEE3F1C94EBFEAF3E0FFED9" LSID="urn:lsid:plazi:treatment:03A987D7FFF13F1C90D9FBC53F88FCC1:8EDF601AFFEE3F1C94EBFEAF3E0FFED9" box="[1189,1358,272,298]" country="Mexico" county="State" latitude="19.103611" longLatPrecision="21" longitude="-99.755554" municipality="Nevado de Toluca" name="Lake La Luna" pageId="17" pageNumber="18">Lake La Luna</location>
in the crater of the volcano 
<collectingMunicipality id="6BDBACBBFFEE3F1C91DBFE87392EFEA1" box="[405,623,312,338]" pageId="17" pageNumber="18">Nevado de Toluca</collectingMunicipality>
, 
<collectingCounty id="62DE4E4DFFEE3F1C9232FE8739F7FEA1" box="[636,694,312,338]" pageId="17" pageNumber="18">State</collectingCounty>
of 
<collectingCountry id="F3177651FFEE3F1C92AEFE87387CFEA1" box="[736,829,312,338]" name="Mexico" pageId="17" pageNumber="18">Mexico</collectingCountry>
, 
