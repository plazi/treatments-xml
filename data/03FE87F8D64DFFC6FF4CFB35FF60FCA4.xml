<heading id="D0A08182D64DFFC5FF4CFB35FE00FA96" bold="true" box="[151,402,1259,1286]" fontSize="11" level="1" pageId="1" pageNumber="40" reason="1">
<emphasis id="B923EAFCD64DFFC5FF4CFB35FE00FA96" bold="true" box="[151,402,1259,1286]" pageId="1" pageNumber="40">
<taxonomicName id="4C574D6DD64DFFC5FF4CFB35FEDAFA95" box="[151,328,1260,1286]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="aquila" status="sp. n.">
<emphasis id="B923EAFCD64DFFC5FF4CFB35FEDAFA95" bold="true" box="[151,328,1260,1286]" italics="true" pageId="1" pageNumber="40">Manota aquila</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2105787D64DFFC5FE8BFB32FE00FA96" box="[336,402,1259,1285]" pageId="1" pageNumber="40" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
