</taxonomicName>
, resembling 
<taxonomicName id="4C011396D222026CFDE8F9FA53C9353B" box="[577,715,1549,1573]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="45" pageNumber="2827" phylum="Firmicutes" rank="species" species="plantarum">
