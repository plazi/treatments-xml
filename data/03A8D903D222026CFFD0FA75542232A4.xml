<taxonomicName id="4C011396D222026CFFD0FA2450D636F6" box="[121,468,1488,1512]" class="Bacilli" family="Lactobacillaceae" genus="Lactiplantibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="45" pageNumber="2827" phylum="Firmicutes" rank="species" species="paraplantarum">
