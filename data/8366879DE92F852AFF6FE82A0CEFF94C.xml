<number id="BD0724ACE92E852AFAC3E8CE0B7CFE44" box="[1339,1398,404,429]" pageId="63" pageNumber="64" value="1957.0">1957</number>
</date>
] L. E. Peña./ 
<typeStatus id="D4748829E92E852AFF01E8E20F85FE39" box="[249,399,440,464]" pageId="63" pageNumber="64">HOLOTYPE</typeStatus>
<taxonomicName id="CCCF4D08E92E852AFE60E8ED0CB1FE39" authority="Mathis" authorityName="Mathis" box="[408,699,439,464]" class="Insecta" family="Ephydridae" genus="Neoephydra" kingdom="Animalia" order="Diptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="penai">Neoephydra penai Mathis</taxonomicName>
