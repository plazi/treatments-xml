<collectingCountry id="73D8761BE92E852AFC26E8CE0A3EFE45" box="[990,1076,404,428]" name="Chile" pageId="63" pageNumber="64">CHILE</collectingCountry>
. 
<date id="7F71104BE92E852AFBC7E8CE0AC4FE44" box="[1087,1230,404,429]" pageId="63" pageNumber="64" value="1957-11-29">
<number id="BD0724ACE92E852AFBC7E8CE0A54FE45" box="[1087,1118,404,428]" pageId="63" pageNumber="64" value="29.0">29</number>
