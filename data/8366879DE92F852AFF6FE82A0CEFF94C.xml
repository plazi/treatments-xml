<taxonomicName id="CCCF4D08E92E852AFC6BE8CE0DD8FE44" box="[915,978,404,429]" class="Insecta" family="Geometridae" genus="Talca" kingdom="Animalia" order="Lepidoptera" pageId="63" pageNumber="49" phylum="Arthropoda" rank="genus">Talca</taxonomicName>
