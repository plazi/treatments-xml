, 
<collectingCountry id="F31A7637FFB8FF39FBE8F86312A1F83C" box="[1065,1161,1998,2019]" name="Indonesia" pageId="5" pageNumber="132">Indonesia</collectingCountry>
, 
<collectingCountry id="F31A7637FFB8FF39FB55F863137BF83C" box="[1172,1363,1998,2019]" name="Papua New Guinea" pageId="5" pageNumber="132">Papua New Guinea</collectingCountry>
)
</keyLead>
