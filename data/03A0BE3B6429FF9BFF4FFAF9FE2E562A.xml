<taxonomicName id="4C0974AE6429FF9CFF2BF8BBFE0B502E" authorityName=": Ognev" authorityYear="1913" box="[243,458,1835,1860]" class="Mammalia" family="Vespertilionidae" genus="Murina" kingdom="Animalia" order="Chiroptera" pageId="35" pageNumber="36" phylum="Chordata" rank="species" species="hilgendorfi">
