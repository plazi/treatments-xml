<taxonomicName id="4C6F2AEE1320FFC6BF146EED6E43FF78" authority="Moret, 1926" authorityName="Moret" authorityYear="1926" box="[300,536,205,232]" class="Demospongiae" family="Theonellidae" genus="Lerouxia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lithistida" pageId="7" pageNumber="292" phylum="Porifera" rank="genus">
