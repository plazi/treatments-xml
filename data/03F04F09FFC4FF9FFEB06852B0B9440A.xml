<taxonomicName id="4C59859CFFC7FF9EFE7C6E2AB202456F" authority="Hu 1994" authorityName="Hu" authorityYear="1994" box="[452,820,1332,1355]" class="Arachnida" family="Atypidae" genus="Atypus" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="baotianmanensis">
