<taxonomicName id="EFC973E5AFCAF056748D9B8A3AAC9E96" LSID="452AE17D-31B0-519B-865A-2AA900DAAF76" authority="Shimizu" authorityName="Shimizu" authorityYear="2020" class="Insecta" family="Ichneumonidae" genus="Enicospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enicospilus limnophilus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="limnophilus" status="sp. nov.">Enicospilus limnophilus Shimizu</taxonomicName>
