<number id="ED6A2489FFB97414FC7FFB67F8B1FA9A" box="[977,1019,1263,1288]" pageId="1" pageNumber="440" value="295.0">295</number>
(
<number id="ED6A2489FFB97414FBA4FB67FF38FA9A" box="[1034,1138,1263,1288]" pageId="1" pageNumber="440" value="296.5" valueMax="305.0" valueMin="288.0">288–305</number>
); leg II 
<number id="ED6A2489FFB97414FB68FB67FFBAFA9A" box="[1222,1264,1263,1288]" pageId="1" pageNumber="440" value="235.0">235</number>
(
<number id="ED6A2489FFB97414FB51FB67FE2DFA9A" box="[1279,1383,1263,1288]" pageId="1" pageNumber="440" value="235.0" valueMax="245.0" valueMin="225.0">225–245</number>
); leg III 
<number id="ED6A2489FFB97414FF14FA9CFBAEFABE" box="[186,228,1300,1324]" pageId="1" pageNumber="440" value="221.0">221</number>
(
<number id="ED6A2489FFB97414FF5CFA9CFA10FABE" box="[242,346,1300,1324]" pageId="1" pageNumber="440" value="221.5" valueMax="230.0" valueMin="213.0">213–230</number>
