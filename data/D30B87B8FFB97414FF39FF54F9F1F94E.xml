<number id="ED6A2489FFB97414FE14FA9CFAAEFABE" box="[442,484,1300,1324]" pageId="1" pageNumber="440" value="334.0">334</number>
(
<number id="ED6A2489FFB97414FE5DFA9CF911FABF" box="[499,603,1300,1325]" pageId="1" pageNumber="440" value="337.5" valueMax="350.0" valueMin="325.0">325–350</number>
