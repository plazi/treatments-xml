<taxonomicName id="8921112ABC01F4A91D140F489238C47B" authority="L." class="Polypodiopsida" family="Pteridaceae" genus="Pteris" kingdom="Plantae" order="Polypodiales" pageId="null" pageNumber="103" phylum="Tracheophyta" rank="species" species="cretica">
