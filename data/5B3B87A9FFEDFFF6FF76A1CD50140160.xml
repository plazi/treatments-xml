<collectionCode id="B583AE7AFFEEFFF6FEEFA29753EA0380" box="[369,450,415,438]" collectionName="United Kingdom, London, The Natural History Museum [formerly British Museum (Natural History)]" pageId="26" pageNumber="191">BMNH</collectionCode>
