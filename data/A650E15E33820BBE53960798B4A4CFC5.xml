<paragraph id="639AE1A85F2D02009A1EA8D88C2DA705" pageId="10" pageNumber="11">
<taxonomicName id="1A645AB13357443BA3AFBD035767BD5E" LSID="urn:lsid:zoobank.org:act:D441247B-3568-4AE5-ACDF-562E75F8A8FC" class="Insecta" family="Blaberidae" genus="Pseudophoraspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudophoraspis clavellata" order="Blattodea" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="clavellata">Pseudophoraspis clavellata</taxonomicName>
<taxonomicNameLabel id="51C9E65BE8C20E32DFEF6994469ED4F4" pageId="10" pageNumber="11">sp. n.</taxonomicNameLabel>
Figs 9
