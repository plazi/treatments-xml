<taxonomicName id="4C004D2147436E65FEB51DD3FD47F97E" authority="Zetterstedt, 1838" authorityName="Zetterstedt" authorityYear="1838" box="[296,743,1728,1750]" class="Insecta" family="Ceratopogonidae" genus="Ceratopogon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="559" phylum="Arthropoda" rank="species" species="lacteipennis">
<emphasis id="B974EAB047436E65FEB51DD3FD8FF97E" box="[296,559,1728,1750]" italics="true" pageId="8" pageNumber="559">Ceratopogon lacteipennis</emphasis>
Zetterstedt, 1838
</taxonomicName>
, 
<emphasis id="B974EAB047436E65FD6E1DD3FC8BF97E" box="[755,811,1729,1750]" italics="true" pageId="8" pageNumber="559">sensu</emphasis>
Kieffer (misidentified = 
<taxonomicName id="4C004D2147436E65FBB21DD2FA31F97E" authority="Goetghebuer" authorityName="Goetghebuer" authorityYear="1920" box="[1071,1425,1728,1750]" class="Insecta" family="Ceratopogonidae" genus="Psilohelea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="559" phylum="Arthropoda" rank="species" species="sociabilis">
<emphasis id="B974EAB047436E65FBB21DD2FAA2F97E" box="[1071,1282,1728,1750]" italics="true" pageId="8" pageNumber="559">Psilohelea sociabilis</emphasis>
