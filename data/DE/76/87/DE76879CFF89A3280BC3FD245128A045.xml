<specimenCode box="[693,862,454,473]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="14" pageNumber="14" type="Museum">USNM 219750</specimenCode>
