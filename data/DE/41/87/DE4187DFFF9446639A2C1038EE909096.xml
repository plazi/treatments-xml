</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Kurosa, 1984" authorityName="Kurosa" authorityYear="1984" box="[931,1274,1817,1843]" class="Arachnida" family="Scutacaridae" genus="Archidispus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="pseudomega">
