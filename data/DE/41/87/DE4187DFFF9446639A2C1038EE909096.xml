<taxonomicName authority="Kurosa, 1976" authorityName="Kurosa" authorityYear="1976" box="[642,919,1817,1843]" class="Arachnida" family="Scutacaridae" genus="Archidispus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="omega">
