<taxonomicName authority="Kurosa, 1990" authorityName="Kurosa" authorityYear="1990" box="[264,595,1855,1881]" class="Arachnida" family="Scutacaridae" genus="Archidispus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="stenolophi">
