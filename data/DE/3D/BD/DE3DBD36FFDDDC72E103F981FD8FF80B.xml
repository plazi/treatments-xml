<taxonomicName authorityName="Borner" authorityYear="1903" box="[238,349,1970,1995]" class="Entognatha" family="Isotomidae" genus="Isotomurus" kingdom="Animalia" order="Collembola" pageId="17" pageNumber="404" phylum="Arthropoda" rank="genus">
