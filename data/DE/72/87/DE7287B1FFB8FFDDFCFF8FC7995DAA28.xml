(
<figureCitation box="[874,924,1049,1071]" captionStart="Figs 1–12" captionStartId="8.[189,229,1882,1904]" captionTargetBox="[194,1426,231,1859]" captionTargetId="figure-42@8.[189,1437,231,1874]" captionTargetPageId="8" captionText="Figs 1–12: Habitus. 1: Brachida linsaymontis spec. nov.; 2: Sternotropa linsaymontis spec. nov.; 3: Sternotropa tambourinensis spec. nov.; 4: Sternotropa brisbanensis spec. nov.; 5: Austraepiskia barrinensis gen. nov., spec. nov.; 6: Austracoenonica laminaris gen. nov., spec. nov.; 7: Tryphopsichara australiana gen. nov., spec. nov.;8: Leptostiba tambourinensis spec. nov.; 9: Atheta (Acrotona) wachteli spec. nov.; 10: Atheta (Traumoecia) tambourinensis spec. nov.; 11: Gastropaga brisbanensis spec. nov.; 12: Gastropaga barrinensis spec. nov." figureDoi="http://doi.org/10.5281/zenodo.4754017" httpUri="https://zenodo.org/record/4754017/files/figure.png" pageId="2" pageNumber="329">Figs 5</figureCitation>
, 
<figureCitation box="[936,991,1049,1071]" captionStart="Figs 24–34" captionStartId="10.[189,229,1948,1970]" captionTargetBox="[190,1431,281,1922]" captionTargetId="figure-85@10.[189,1437,281,1924]" captionTargetPageId="10" captionText="Figs 24–34: Spermatheca, labium with labial palpus, mentum, maxilla with maxillary palpus, aedeagus in lateral and ventral view. 24–27: Austraepiskiabarrinensis gen. nov., spec. nov.;28–32: Austracoenonica laminaris gen. nov., spec. nov.;33–34: Tryphopsichara australiana gen. nov., spec. nov." figureDoi="http://doi.org/10.5281/zenodo.4754023" httpUri="https://zenodo.org/record/4754023/files/figure.png" pageId="2" pageNumber="329">24–27</figureCitation>
