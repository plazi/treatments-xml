<taxonomicName box="[823,1005,506,529]" class="Trematoda" family="Macrovalvitrematidae" genus="Neomacrovalvitrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="34" pageNumber="35" phylum="Platyhelminthes" rank="species" species="argentinensis">
