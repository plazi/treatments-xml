<taxonomicName authorityName="Chopard" authorityYear="1925" box="[532,654,1765,1789]" class="Insecta" family="Gryllidae" genus="Paranisitra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="84" phylum="Arthropoda" rank="species" species="longipes">
