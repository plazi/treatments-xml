<taxonomicName authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="46" pageNumber="47" phylum="Angiospermae" rank="species" species="stricta">
