<taxonomicName class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="nummularia">
