<taxonomicName authority="(Ivanov)" baseAuthorityName="Ivanov" baseAuthorityYear="1896" box="[281,590,1851,1875]" class="Insecta" family="Braconidae" genus="Cyanopterus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="112" phylum="Arthropoda" rank="species" species="tricolor">
