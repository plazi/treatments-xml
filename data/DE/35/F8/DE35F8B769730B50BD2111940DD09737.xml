<subSubSection pageId="66" pageNumber="67" type="nomenclature">
<paragraph pageId="66" pageNumber="67">
75. 
<taxonomicName LSID="urn:lsid:zoobank.org:act:5E783DAA-30CE-41D6-A8DE-0FEE8E94B143" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus scissops" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="scissops">Trigonopterus scissops Riedel</taxonomicName>
