<taxonomicName authority="Calman, 1907" authorityName="Calman" authorityYear="1907" class="Malacostraca" family="Euryrhynchidae" genus="Euryrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="10" pageNumber="31" phylum="Arthropoda" rank="species" species="burchelli">
