<taxonomicName authority="(Franz)" authorityName="Jałoszyński" authorityYear="2021" baseAuthorityName="Jałoszyński" baseAuthorityYear="2021" box="[1243,1432,1235,1260]" class="Insecta" family="Staphylinidae" genus="Heterotetramelus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="133" phylum="Arthropoda" rank="species" species="baloghi">
