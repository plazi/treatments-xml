<taxonomicName authority="Walsm." authorityName="Walsm." box="[238,450,840,865]" class="Insecta" family="Gracillariidae" genus="Didactica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
