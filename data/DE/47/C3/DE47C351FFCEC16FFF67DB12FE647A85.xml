<collectingCountry box="[682,793,295,321]" name="Argentina" pageId="85" pageNumber="86">Argentina</collectingCountry>
(
<collectingRegion box="[810,939,295,321]" country="Argentina" name="Salta" pageId="85" pageNumber="86">Salta Prov.</collectingRegion>
