<paragraph blockId="6.[350,928,1008,1032]" box="[350,928,1008,1032]" pageId="6" pageNumber="49">
<heading bold="true" box="[350,928,1008,1032]" centered="true" fontSize="8" level="2" pageId="6" pageNumber="49" reason="3">
<emphasis bold="true" box="[350,928,1008,1032]" pageId="6" pageNumber="49">
[? 
<taxonomicName authority="(MacLeay, 1825)" baseAuthorityName="MacLeay" baseAuthorityYear="1825" box="[372,921,1008,1032]" class="Insecta" family="Curculionidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="fabricii" subGenus="Guignotites">
