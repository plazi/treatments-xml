<taxonomicName authority="Borovec, 2019" authorityName="Borovec" authorityYear="2019" box="[709,862,1834,1856]" class="Insecta" family="Curculionidae" genus="Heisonyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="262" phylum="Arthropoda" rank="species" species="oberprieleri" status="sp. n.">
