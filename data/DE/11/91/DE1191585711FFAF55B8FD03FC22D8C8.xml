<subSubSection box="[199,1132,1015,1042]" pageId="4" pageNumber="336" type="distribution">
<paragraph blockId="4.[151,1436,835,1258]" box="[199,1132,1015,1042]" pageId="4" pageNumber="336">
<emphasis bold="true" box="[199,345,1015,1041]" pageId="4" pageNumber="336">Distribution</emphasis>
. 
<collectingCountry box="[349,411,1015,1041]" name="United States of America" pageId="4" pageNumber="336">USA</collectingCountry>
