<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2329" class="Insecta" family="Formicidae" genus="Aenictus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Aenictus Shuckard" order="Hymenoptera" pageNumber="16" phylum="Arthropoda" rank="genus">Aenictus</taxonomicName>
