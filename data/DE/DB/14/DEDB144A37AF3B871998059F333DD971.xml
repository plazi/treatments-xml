<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25036" class="Insecta" family="Formicidae" genus="Aenictus" genus.bestMatchDistance="11" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Aenictus ceylonicus (Mayr)" order="Hymenoptera" pageNumber="16" phylum="Arthropoda" rank="species" species="ceylonicus">A. ceylonicus</taxonomicName>
