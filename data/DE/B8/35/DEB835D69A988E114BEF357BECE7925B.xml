<subSubSection pageId="null" pageNumber="580" type="nomenclature">
<paragraph pageId="null" pageNumber="580">
<taxonomicName authority="L." class="Liliopsida" family="Amaryllidaceae" genus="Narcissus" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="580" phylum="Tracheophyta" rank="species" species="poeticus">
Narcissus 
<normalizedToken originalValue="poéticus" pageId="null" pageNumber="580">poeticus</normalizedToken>
<authorityName pageId="null" pageNumber="580">L.</authorityName>
</taxonomicName>
</paragraph>
