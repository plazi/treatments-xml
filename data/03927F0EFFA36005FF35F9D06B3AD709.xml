<typeStatus id="548070BAFFA36005FDD8F9896DE4D561" box="[607,663,1607,1628]" pageId="76" pageNumber="611">Type</typeStatus>
species: 
<taxonomicName id="4C3BB59BFFA36005FF20F9AB6DE7D546" authority="Collier &amp; Farmer, 1964" authorityName="Collier &amp; Farmer" authorityYear="1964" box="[167,660,1637,1659]" class="Gastropoda" family="Dorididae" genus="Conualevia" kingdom="Animalia" order="Nudibranchia" pageId="76" pageNumber="611" phylum="Mollusca" rank="species" species="marcusi">
