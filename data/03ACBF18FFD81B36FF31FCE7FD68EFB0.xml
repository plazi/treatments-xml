<taxonomicName id="4C05758DFFD81B38FE6BFB67FDD1EFF3" box="[461,527,1220,1244]" class="Malacostraca" family="Munididae" genus="Sadayoshia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="36" phylum="Arthropoda" rank="species" species="adaro">
