<emphasis id="B939EABFFFCB2B17FF3CF82738331895" bold="true" box="[151,356,1980,2004]" pageId="24" pageNumber="25">FIGURES 47–52.</emphasis>
<taxonomicName id="4C4D4D2EFFCB2B17FEC7F8273BFA1892" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[364,685,1980,2003]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="urhpirdana" status="new species">
<emphasis id="B939EABFFFCB2B17FEC7F8273BFA1892" box="[364,685,1980,2003]" italics="true" pageId="24" pageNumber="25">Cheumatopsyche urhpirdana</emphasis>
</taxonomicName>
, 
<emphasis id="B939EABFFFCB2B17FD16F8273A121895" bold="true" box="[701,837,1980,2004]" pageId="24" pageNumber="25">
<taxonomicNameLabel id="A20A57C4FFCB2B17FD16F8273A121895" box="[701,837,1980,2004]" pageId="24" pageNumber="25" rank="species">new species</taxonomicNameLabel>
</emphasis>
