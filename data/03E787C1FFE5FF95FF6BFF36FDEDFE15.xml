<subSubSection id="C354655CFFE5FF97FF3AF9F0FC3EF9C9" box="[198,1015,1617,1644]" pageId="12" pageNumber="13" type="etymology">
<paragraph id="8BF136D7FFE5FF97FF3AF9F0FC3EF9C9" blockId="12.[151,1436,1617,2008]" box="[198,1015,1617,1644]" pageId="12" pageNumber="13">
