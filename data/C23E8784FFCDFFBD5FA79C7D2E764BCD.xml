<taxonomicName id="8D974D11FFCDFFBD5B2A9C852E9E4CB8" box="[1101,1220,941,967]" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="28" pageNumber="27" phylum="Tracheophyta" rank="genus">
