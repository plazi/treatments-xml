<taxonomicName id="8D974D11FFCDFFBD5E039CD428384B6A" baseAuthorityName="Maskell" baseAuthorityYear="1890" box="[356,610,1019,1046]" class="Insecta" family="Aphalaridae" genus="Ctenarytaina" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="eucalypti">
