<taxonomicName id="4C374D63FFF0FFCD60BE166BFED1FEA3" authorityName="Churchill &amp; Ellis &amp; Pique &amp; Oakley" authorityYear="2014" box="[202,368,294,318]" class="Ostracoda" family="Sarsiellidae" genus="Eusarsiella" kingdom="Animalia" order="Myodocopida" pageId="12" pageNumber="456" phylum="Arthropoda" rank="species" species="eli">
