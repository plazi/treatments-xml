<taxonomicName id="ACFC4D67E265FFFA45708C57F1892761" authority="De Geer, 1775" authorityName="De Geer" authorityYear="1775" box="[269,718,794,819]" class="Insecta" family="Cerambycidae" genus="Cerambyx" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="pulverulentus">
