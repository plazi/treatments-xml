<collectionCode id="ED7D57DFA762ED3AB148F928FED4A205" LSID="urn:lsid:biocol.org:col:34871" box="[274,354,1783,1807]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="30" pageNumber="31">USNM</collectionCode>
