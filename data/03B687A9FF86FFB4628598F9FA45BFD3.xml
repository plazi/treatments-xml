</taxonomicName>
</keyLead>
</paragraph>
</keyStep>
<keyStep id="30EB2E1AFF86FFB462859A34FA45BFD3" pageId="2" pageNumber="19">
<paragraph id="8BA036BFFF86FFB462859A34FA4ABF83" blockId="2.[151,1436,506,1012]" pageId="2" pageNumber="19">
<keyLead id="30EE958AFF86FFB462859A34FA4ABF83" pageId="2" pageNumber="19">
<number id="3DD72498FF86FFB462859A34FF7BBF5B" box="[151,168,866,892]" pageId="2" pageNumber="19" value="5.0">5</number>
