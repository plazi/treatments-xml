<taxonomicName id="4C384D35FFC71654FDE9FC4455B489C6" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[583,767,961,984]" class="Actinopterygii" family="Cyprinidae" genus="Carassius" kingdom="Animalia" order="Cypriniformes" pageId="7" pageNumber="248" phylum="Chordata" rank="species" species="auratus">
