<taxonomicName id="5600178E34A10587921CE607E8994A9C" authorityName="Swederus" authorityYear="1795" class="Insecta" family="Euphorbiaceae" genus="Pteromalus" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="family">Euphorbiaceae</taxonomicName>
