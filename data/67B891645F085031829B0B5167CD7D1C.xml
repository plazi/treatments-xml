<taxonomicName id="A057DFC0C0B12213F5F06C4AC2CBE204" class="Liliopsida" family="Poaceae" genus="Euphorbia" kingdom="Plantae" lsidName="Euphorbia amygdaloides" order="Poales" pageId="0" pageNumber="77092" phylum="Tracheophyta" rank="species" species="amygdaloides">
