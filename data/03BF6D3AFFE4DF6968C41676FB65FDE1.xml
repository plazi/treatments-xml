<taxonomicName id="4C16A7AFFFE7DF696CD715FCFB0CFF10" box="[1156,1226,189,214]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
