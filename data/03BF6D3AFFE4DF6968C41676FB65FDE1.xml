</taxonomicName>
, 
<taxonomicName id="4C16A7AFFFE7DF696C8915FCFAECFF10" box="[1242,1322,189,214]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
<emphasis id="B962003EFFE7DF696C8915FCFAECFF10" box="[1242,1322,189,214]" italics="true" pageId="18" pageNumber="19">Betula</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C16A7AFFFE7DF696D6815FCFA53FF10" box="[1339,1429,189,214]" class="Magnoliopsida" family="Cornaceae" genus="Cornus" kingdom="Plantae" order="Caryophyllales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
<emphasis id="B962003EFFE7DF696D6815FCFA53FF10" box="[1339,1429,189,214]" italics="true" pageId="18" pageNumber="19">Cornus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C16A7AFFFE7DF6968C415A3FF3FFF3D" box="[151,249,226,251]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
<emphasis id="B962003EFFE7DF6968C415A3FF3FFF3D" box="[151,249,226,251]" italics="true" pageId="18" pageNumber="19">Populus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C16A7AFFFE7DF69695B15A3FE8CFF3D" box="[264,330,226,251]" class="Magnoliopsida" family="Grossulariaceae" genus="Ribes" kingdom="Plantae" order="Saxifragales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
