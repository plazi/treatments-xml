</taxonomicName>
(as 
<emphasis id="B90A0173FFA1BB25FBA5E22E15A5FAE5" italics="true" pageId="16" pageNumber="513">
<taxonomicName id="4C7EA6E2FFA1BB25FBA5E22E117DFA89" box="[1088,1289,1309,1334]" class="Insecta" family="Tettigoniidae" genus="Platycleis" kingdom="Animalia" order="Orthoptera" pageId="16" pageNumber="513" phylum="Arthropoda" rank="species" species="kraussi">Platycleis kraussi</taxonomicName>
, 
<taxonomicName id="4C7EA6E2FFA1BB25FAF0E22D149FFAE5" class="Insecta" family="Tettigoniidae" genus="Metrioptera" kingdom="Animalia" order="Orthoptera" pageId="16" pageNumber="513" phylum="Arthropoda" rank="species" species="kraussi">Metrioptera kraussi</taxonomicName>
