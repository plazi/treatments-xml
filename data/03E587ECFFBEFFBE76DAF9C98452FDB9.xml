<emphasis id="B938EAE8FFBFFFB877C8FAD08468FA96" bold="true" box="[1009,1031,1330,1354]" pageId="11">B,</emphasis>
AMNH 268228); 
<taxonomicName id="4C4C4D79FFBFFFB87454FAAD812DFABB" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[109,322,1359,1383]" class="Mammalia" family="Dasypodidae" genus="Cabassous" kingdom="Animalia" order="Cingulata" pageId="11" phylum="Chordata" rank="species" species="unicinctus">
<emphasis id="B938EAE8FFBFFFB87454FAAD812DFABB" box="[109,322,1359,1383]" italics="true" pageId="11">Cabassous unicinctus</emphasis>
</taxonomicName>
(
<emphasis id="B938EAE8FFBFFFB8756AFAAD8105FABB" bold="true" box="[339,362,1359,1383]" pageId="11">C,</emphasis>
AMNH 74113); 
<taxonomicName id="4C4C4D79FFBFFFB87625FAAD8286FABB" baseAuthorityName="Kerr" baseAuthorityYear="1792" box="[540,745,1359,1383]" class="Mammalia" family="Dasypodidae" genus="Priodontes" kingdom="Animalia" order="Cingulata" pageId="11" phylum="Chordata" rank="species" species="maximus">
