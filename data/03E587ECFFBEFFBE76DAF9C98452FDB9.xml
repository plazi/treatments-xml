(
<emphasis id="B938EAE8FFBFFFB876AEFAD382C0FA95" bold="true" box="[663,687,1329,1353]" pageId="11">A,</emphasis>
AMNH 268229); 
<taxonomicName id="4C4C4D79FFBFFFB87754FAD0838FFA95" baseAuthorityName="Thomas" baseAuthorityYear="1901" box="[877,992,1330,1353]" class="Mammalia" family="Dasypodidae" genus="Dasypus" kingdom="Animalia" order="Cingulata" pageId="11" phylum="Chordata" rank="species" species="pastasae">
