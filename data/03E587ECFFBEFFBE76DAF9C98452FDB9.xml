(Lund, 1843; 
<typeStatus id="54F78858FFBFFFB876D6F98D8371F95B" box="[751,798,1647,1671]" pageId="11">type</typeStatus>
locality 
<collectingRegion id="4988F818FFBFFFB87747F98D847CF95B" box="[894,1043,1647,1671]" country="Brazil" name="Minas Gerais" pageId="11">Minas Gerais</collectingRegion>
, 
<collectingCountry id="F35B766AFFBFFFB87019F98D840EF95B" box="[1056,1121,1647,1671]" name="Brazil" pageId="11">Brazil</collectingCountry>
) in “the Amazon-Solimões River area in 
<collectingCountry id="F35B766AFFBFFFB87030F9728428F974" box="[1033,1095,1680,1704]" name="Brazil" pageId="11">Brazil</collectingCountry>
and 
<collectingCountry id="F35B766AFFBFFFB87044F97384C0F975" box="[1149,1199,1681,1705]" name="Peru" pageId="11">Peru</collectingCountry>
.” 
