<treatmentCitationGroup id="2CC1981C3332FFF489B7B766FDF2BF0D" box="[151,621,576,598]" pageId="10" pageNumber="317">
<taxonomicName id="CBD1C4B13332FFF489B7B766FDF6BF0D" authority="Mostovski, 2000:313" authorityName="Mostovski" authorityPageNumber="313" authorityYear="2000" box="[151,617,576,598]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="317" phylum="Arthropoda" rank="species" species="belyaevae">
