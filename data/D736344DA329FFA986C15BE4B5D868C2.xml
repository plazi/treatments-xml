<taxonomicName id="989FFED8A329FFA9874258E0B09268AE" box="[276,465,1691,1718]" class="Agaricomycetes" family="Pterulaceae" genus="Radulomyces" kingdom="Fungi" order="Agaricales" pageId="5" pageNumber="322" phylum="Basidiomycota" rank="species" species="undetermined">
<emphasis id="6DEB5949A329FFA9874258E0B0EA68AD" box="[276,425,1691,1717]" italics="true" pageId="5" pageNumber="322">Radulomyces</emphasis>
sp.
</taxonomicName>
, occasionally singly in the basidiomes of other fungi from deciduous trees: 
<taxonomicName id="989FFED8A329FFA9834358E7B37C68C1" authority="(P. Karst.) Fiasson &amp; Niemela" authorityName="Fiasson &amp; Niemela" authorityYear="1984" baseAuthorityName="P. Karst." class="Agaricomycetes" family="Hymenochaetaceae" genus="Fomitiporia" kingdom="Fungi" order="Hymenochaetales" pageId="5" pageNumber="322" phylum="Basidiomycota" rank="species" species="robusta">
<emphasis id="6DEB5949A329FFA9834358E7B1AF68C1" italics="true" pageId="5" pageNumber="322">Fomitiporia robusta</emphasis>
(P.Karst.) Fiasson &amp; Niemelä
</taxonomicName>
, 
<taxonomicName id="989FFED8A329FFA9841C58BBB24F68C1" box="[586,780,1727,1754]" class="Agaricomycetes" family="Polyporaceae" genus="Cerrena" kingdom="Fungi" order="Polyporales" pageId="5" pageNumber="322" phylum="Basidiomycota" rank="species" species="unicolor">
