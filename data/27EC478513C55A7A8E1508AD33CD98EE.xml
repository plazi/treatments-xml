<paragraph id="E26F478706921609A986D52A717E1F16" pageId="0" pageNumber="60630">
<taxonomicName id="0CD77163C7EE83444DE5355F661717D2" baseAuthorityName="Buckley" baseAuthorityYear="1866" class="Hexapoda" family="Formicidae" genus="Dorymyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dorymyrmex insanus" order="Hymenoptera" pageId="0" pageNumber="60630" phylum="Arthropoda" rank="species" species="insanus">
