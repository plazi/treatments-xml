<emphasis id="B979EAD695770862E101FD92E9ECFDA4" box="[359,440,583,608]" italics="true" pageId="36" pageNumber="37">
<collectingCountry id="F31A765495770862E101FD92E9ECFDA4" box="[359,440,583,608]" name="Taiwan" pageId="36" pageNumber="37">Taiwan</collectingCountry>
