<taxonomicName id="F6B02A88F012DF7709536A7B90CAF189" baseAuthorityName="Reibisch" baseAuthorityYear="1893" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="173" pageNumber="174" phylum="Mollusca" rank="species" species="wolfi">
