<emphasis id="B920EAD0FF983D52FF6EC24BFB14FAD3" bold="true" box="[151,289,1306,1328]" pageId="47" pageNumber="548">FIGURE 37.</emphasis>
<taxonomicName id="4C544D41FF983D52FEDEC24AFBAAFACC" box="[295,415,1306,1328]" class="Arachnida" family="Salticidae" genus="Myrmarachne" kingdom="Animalia" order="Araneae" pageId="47" pageNumber="548" phylum="Arthropoda" rank="species" species="sabahna" status="sp. nov.">
<emphasis id="B920EAD0FF983D52FEDEC24AFB09FACC" box="[295,316,1307,1328]" italics="true" pageId="47" pageNumber="548">M</emphasis>
. 
<taxonomicName id="4C544D41FF983D52FEB0C24BFBAAFACC" box="[329,415,1306,1328]" pageId="47" pageNumber="548" rank="species" species="sabahna" status="sp. nov.">
<emphasis id="B920EAD0FF983D52FEB0C24BFBAAFACC" box="[329,415,1306,1328]" italics="true" pageId="47" pageNumber="548">sabahna</emphasis>
</taxonomicName>
</taxonomicName>
