<collectingCountry id="7198767BD75D393CFD1AFD23FD749AEA" box="[647,695,722,745]" name="Iran" pageId="59" pageNumber="60">Iran</collectingCountry>
, 
<emphasis id="3BFBEAF9D75D393CFD5FFD23FC809AEA" box="[706,835,721,745]" italics="true" pageId="59" pageNumber="60">P. s a lv i ac u s</emphasis>
