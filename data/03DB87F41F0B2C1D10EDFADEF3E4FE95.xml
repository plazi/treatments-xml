<taxonomicName id="4C724D611F0B2C0211C6FADEF432FAB2" box="[444,587,1289,1315]" class="Ostracoda" family="Halocyprididae" genus="Conchoecia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Halocyprida" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus">
