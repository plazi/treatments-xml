<location id="8EE231A8BE6FFFFFFAE6525327B0EB04" LSID="urn:lsid:plazi:treatment:0394D665BE6FFFF8FF3C543626CEEA42:8EE231A8BE6FFFFFFAE6525327B0EB04" box="[1357,1431,1306,1332]" country="Palau" latitude="-7.0" longLatPrecision="782" longitude="134.0" name="Island" pageId="67" pageNumber="68" stateProvince="Angaur">Island</location>
, 
<date id="FF8341B3BE6FFFFFFF3C520B2332EB6C" box="[151,277,1346,1372]" pageId="67" pageNumber="68" value="1936-04-16">
<collectingDate id="EFC7B85BBE6FFFFFFF3C520B2332EB6C" box="[151,277,1346,1372]" pageId="67" pageNumber="68" value="1936-04-16">16.iv.1936</collectingDate>
</date>
, 
<collectorName id="26C802A5BE6FFFFFFE8A520B23B3EB6C" box="[289,404,1346,1372]" pageId="67" pageNumber="68">Y. Kondo</collectorName>
, 
<specimenCount id="9D3BACFABE6FFFFFFE34520B23DEEB6C" box="[415,505,1346,1372]" pageId="67" pageNumber="68" type="male">3 males</specimenCount>
(I male det 
<collectorName id="26C802A5BE6FFFFFFD2C520B212CEB6C" box="[647,779,1346,1372]" pageId="67" pageNumber="68">K. G. Blair</collectorName>
). (
<collectionCode id="ED2CFFB6BE6FFFFFFC8D520B215CEB6C" box="[806,891,1346,1372]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="67" pageNumber="68" type="Museum">BPBM</collectionCode>
)
