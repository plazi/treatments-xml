Known only from 
<collectingRegion id="95102FB88C48FFD65E2BB017FD1D5B0E" box="[567,656,872,895]" country="China" name="Yunnan" pageId="7" pageNumber="386">Yunnan</collectingRegion>
, 
<collectingCountry id="2FC3A1CA8C48FFD65E86B018FD505BF1" box="[666,733,871,896]" name="China" pageId="7" pageNumber="386">China</collectingCountry>
(
<figureCitation id="CFEFFDDF8C48FFD65EF1B018FCB05BF1" box="[749,829,871,896]" captionStart="FIGURE 11" captionStartId="13.[151,250,1482,1504]" captionTargetBox="[151,1436,483,1460]" captionTargetId="figure@13.[151,1436,483,1460]" captionTargetPageId="13" captionText="FIGURE 11. Distribution records. A ▲ S. forcipiformis sp. nov.; B ▲ S. serratus sp. nov., • S. pinniformis sp. nov.; C ▲ S. tengchongensis sp. nov.; D ▲ S. triapiculus sp. nov." httpUri="https://zenodo.org/record/841251/files/figure.png" pageId="7" pageNumber="386">Fig. 11</figureCitation>
