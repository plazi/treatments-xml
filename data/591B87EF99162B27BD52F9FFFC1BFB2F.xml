<quantity id="164A9B1C99162B27BDC3F934FE27F97E" box="[384,494,1738,1762]" metricMagnitude="-1" metricUnit="m" metricValue="3.5" metricValueMax="5.0" metricValueMin="2.0" pageId="17" pageNumber="409" unit="m" value="0.35" valueMax="0.5" valueMin="0.2">
