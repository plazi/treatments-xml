<collectingRegion id="1376F81B99162B27BD41F97AFEA5F903" box="[258,364,1668,1692]" country="Vietnam" name="Can Tho" pageId="17" pageNumber="409">Can Tho</collectingRegion>
, 
<location id="D46D602299162B27BD3AF97AFDEDF903" LSID="urn:lsid:plazi:treatment:591B87EF99162B27BD52F9FFFC1BFB2F:D46D602299162B27BD3AF97AFDEDF903" box="[377,548,1668,1692]" country="Vietnam" county="Material" latitude="10.061945" longLatPrecision="21" longitude="105.78389" municipality="Mekong" name="Mekong River" pageId="17" pageNumber="409" stateProvince="Can Tho">Mekong River</location>
