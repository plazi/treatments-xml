<paragraph id="EC7B36AD8E30FE2A2C9FFC8642879522" blockId="16.[189,457,780,806]" box="[189,457,780,806]" pageId="16" pageNumber="17">
<heading id="B73381C18E30FE2A2C9FFC8642879522" bold="true" box="[189,457,780,806]" fontSize="11" level="3" pageId="16" pageNumber="17" reason="3">
