<taxonomicName id="4C265D93FFC15454BC1233C23D4310AA" box="[880,1068,685,709]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="585" phylum="Arthropoda" rank="species" species="undetermined">
