<paragraph id="8B8AA758FFB5FFB9DED7FF3CFB09785E" blockId="52.[741,1171,555,578]" box="[741,1171,555,578]" pageId="52" pageNumber="53">
<heading id="D0C21034FFB5FFB9DED7FF3CFB09785E" box="[741,1171,555,578]" centered="true" fontSize="9" level="2" pageId="52" pageNumber="53" reason="2">
<taxonomicName id="4C35DCDBFFB5FFB9DED7FF3CFC61785E" box="[741,1019,555,578]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="prinslooi" status="new species">
<emphasis id="B9417B4AFFB5FFB9DED7FF3CFC61785E" bold="true" box="[741,1019,555,578]" italics="true" pageId="52" pageNumber="53">Paramblynotus prinslooi</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFB5FFB9D83EFF3AFB09785E" box="[1036,1171,556,578]" pageId="52" pageNumber="53" rank="species">new species</taxonomicNameLabel>
