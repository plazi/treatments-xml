<taxonomicName id="FDB948E1B06B9B3DFE996391FDBB65F7" authority="BISCHOFF" authorityName="BISCHOFF" authorityYear="1910" box="[339,624,1165,1187]" class="Insecta" family="Chrysididae" genus="Cephaloparnops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="80" pageNumber="85" phylum="Arthropoda" rank="genus">
