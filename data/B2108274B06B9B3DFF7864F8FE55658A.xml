<taxonomicName id="FDB948E1B06B9B3DFDE1636CFC5E65D2" authority="(WALKER)" baseAuthorityName="WALKER" baseAuthorityYear="1871" box="[555,917,1136,1158]" class="Insecta" family="Chrysididae" genus="Euchroeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="80" pageNumber="85" phylum="Arthropoda" rank="species" species="pallispinosus">
