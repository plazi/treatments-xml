<taxonomicName id="B295FD1C8676C757F242895CB0955D36" authority="L." class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Aplousobranchia" pageId="null" pageNumber="666" phylum="Chordata" rank="species" species="pentandra">
Salix 
<normalizedToken id="8990C1BD0903C84B3D84867364C6FE36" originalValue="pentándra" pageId="null" pageNumber="666">pentandra</normalizedToken>
<authorityName id="6B027CAC2ECDE86B4FAE45778EBF9DF5" pageId="null" pageNumber="666">L.</authorityName>
