<taxonomicName id="D9955C9E48FB27826175454C7D44CFA4" authorityName="Haliday" authorityYear="1833" class="Insecta" family="Scelionidae" genus="Gryon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gryon" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="genus">
<emphasis id="DCAC5A9F194C8E5DD3C3C0B5E255C221" italics="true" pageId="0" pageNumber="323">Gryon</emphasis>
</taxonomicName>
based on its placement in the 
<taxonomicName id="C26E5DAF523D4EA21E398AF9883342A5" authorityName="Talamas &amp; Bremer &amp; Moore &amp; Bon &amp; Lahey &amp; Roberts &amp; Combee &amp; McGathey &amp; van Noort &amp; Timokhov &amp; Hougardy &amp; Hogg" authorityYear="2021" baseAuthorityName="Talamas &amp; Bremer &amp; Moore &amp; Bon &amp; Lahey &amp; Roberts &amp; Combee &amp; McGathey &amp; van Noort &amp; Timokhov &amp; Hougardy &amp; Hogg" baseAuthorityYear="2021" class="Insecta" family="Scelionidae" genus="Gryon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gryon myrmecophilum" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="species" species="myrmecophilum">
<emphasis id="B4620FB02812D4504334BE54A6B28CAA" italics="true" pageId="0" pageNumber="323">Gryon myrmecophilum</emphasis>
</taxonomicName>
