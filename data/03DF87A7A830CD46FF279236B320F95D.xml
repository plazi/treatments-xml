<taxonomicName id="4C764D32A830CD46FD0B9279B4B6F92D" authority="Walker, 1869" authorityName="Walker" authorityYear="1869" box="[699,952,1645,1671]" class="Insecta" family="Phaneropteridae" genus="Itarissa" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="56" phylum="Arthropoda" rank="genus">
