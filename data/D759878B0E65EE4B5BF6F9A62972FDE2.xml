<paragraph id="5F4F369D0E65EE4B5BF6F8FE29DDFE0F" blockId="26.[151,1437,1828,2040]" lastBlockId="27.[151,1437,151,573]" lastPageId="27" lastPageNumber="328" pageId="26" pageNumber="327">
<materialsCitation id="EF983CC00E65EE4A5BF6F8FE2FACF8E3" ID-GBIF-Occurrence="1416805642" box="[151,1433,1828,1853]" collectionCode="ANIC" county="December" location="Elizabeth Bay" municipality="Beginning" pageId="26" pageNumber="327" specimenCount="1" typeStatus="syntype">
