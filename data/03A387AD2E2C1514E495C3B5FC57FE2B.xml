(
<figureCitation id="13312A3E2E281514E59CC748FE1DFE0F" box="[414,495,367,392]" captionStart="FIGURE 3" captionStartId="5.[151,250,1340,1362]" captionTargetBox="[116,1471,201,1318]" captionTargetId="figure@5.[116,1471,193,1318]" captionTargetPageId="5" captionText="FIGURE 3. A. Damage by Rhabdotalebra albinoi n. sp. (Circle indicating the damage in the form of rings and horseshoes on leaves of “ Guarán-Guarán ”). Scale bar = 1 mm." httpUri="https://zenodo.org/record/816083/files/figure.png" pageId="6" pageNumber="231">Fig.3A</figureCitation>
) 
<taxonomicName id="4C0A4D382E281514E607C756FD8BFE0F" authority="Paradell &amp; Catalano &amp; Petrulevičius, 2017" authorityName="Paradell &amp; Catalano &amp; Petrulevičius" authorityYear="2017" box="[517,633,367,392]" class="Insecta" family="Cicadellidae" genus="Rhabdotalebra" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="231" phylum="Arthropoda" rank="species" species="albinoi" status="n.sp.">
<emphasis id="B97EEAA92E281514E607C756FD8BFE0F" box="[517,633,367,392]" italics="true" pageId="6" pageNumber="231">R. albinoi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24D57D22E281514E685C748FD4DFE0F" box="[647,703,367,392]" pageId="6" pageNumber="231" rank="species">
