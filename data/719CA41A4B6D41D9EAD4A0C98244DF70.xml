<taxonomicName id="6F35034357D2D1636E49F9D6024968BB" authority="Schaeffer, 1777" authorityName="Schaeffer" authorityYear="1777" class="Insecta" family="Cleridae" genus="Cleroides" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cleroides" order="Coleoptera" pageId="2" pageNumber="77" phylum="Arthropoda" rank="genus">Cleroides</taxonomicName>
