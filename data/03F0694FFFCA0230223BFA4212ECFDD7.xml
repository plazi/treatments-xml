<taxonomicName id="4C59A3DAFFC8023623E1FA35104DF9CB" box="[440,629,1517,1538]" class="Polypodiopsida" family="Nephrolepidaceae" genus="Nephrolepis" kingdom="Plantae" order="Polypodiales" pageId="19" pageNumber="304" phylum="Tracheophyta" rank="species" species="hirsulata">
