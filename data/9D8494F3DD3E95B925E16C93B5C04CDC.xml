<taxonomicName id="72A202784A3AC62BDCD75D554ABEC181" authorityName="Wood &amp; Bright" authorityYear="1992" class="Insecta" family="Curculionidae" genus="Cnestus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cnestus bicornis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="bicornis">
