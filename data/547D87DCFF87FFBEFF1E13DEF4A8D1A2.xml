<number id="6A1C24EDFF86FFB9FA051735F3DFD2E9" box="[1420,1431,367,391]" pageId="11" pageNumber="47" value="1.0">1</number>
; toes are 
<number id="6A1C24EDFF86FFB9FF7F17CEF757D2C2" box="[246,287,404,428]" pageId="11" pageNumber="47" value="3.5" valueMax="4.0" valueMin="3.0">3-4</number>
- 
<number id="6A1C24EDFF86FFB9FEA117CEF719D2C2" box="[296,337,404,428]" pageId="11" pageNumber="47" value="3.5" valueMax="5.0" valueMin="2.0">2-5</number>
- 
<number id="6A1C24EDFF86FFB9FED317CEF72DD2C2" box="[346,357,404,428]" pageId="11" pageNumber="47" value="1.0">1</number>
