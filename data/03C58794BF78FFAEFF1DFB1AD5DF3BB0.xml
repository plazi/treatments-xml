<number id="3DA424A5BF78FFAEFD45FA6BD6D33B94" box="[719,733,1410,1435]" pageId="11" pageNumber="114" value="5.0">5</number>
<number id="3DA424A5BF78FFAEFD6EFA6AD7003B94" box="[740,782,1411,1435]" pageId="11" pageNumber="114" value="3.0" valueMax="4.0" valueMin="2.0">2–4</number>
