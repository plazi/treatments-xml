<quantity box="[821,904,1388,1413]" metricMagnitude="-2" metricUnit="m" metricValue="4.1" pageId="37" pageNumber="304" unit="mm" value="41.0">41 mm</quantity>
, 
<typeStatus box="[917,1005,1388,1413]" pageId="37" pageNumber="304">allotype</typeStatus>
, 
