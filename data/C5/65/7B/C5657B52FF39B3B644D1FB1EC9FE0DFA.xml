<materialsCitation box="[522,830,1654,1678]" collectorName="Santa Rosa I." location="California" pageId="165" pageNumber="166" specimenCount="1" stateProvince="California">
to 
<collectorName box="[551,701,1654,1678]" pageId="165" pageNumber="166">Santa Rosa I.</collectorName>
