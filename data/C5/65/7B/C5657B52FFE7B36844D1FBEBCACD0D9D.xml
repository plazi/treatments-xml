and 
<location LSID="urn:lsid:plazi:treatment:C5657B52FFE7B36844D1FBEBCACD0D9D:48139C9FFFE7B36846EDF9C3CB1B0D0D" box="[683,797,1545,1569]" county="Fall River Mills" municipality="Fall River" name="Hat Creek" pageId="123" pageNumber="124" stateProvince="California">Hat Creek</location>
near 
