<taxonomicName authorityName="Bate" authorityYear="1888" box="[644,768,991,1014]" class="Malacostraca" family="Axiidae" genus="Eiconaxius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="136" pageNumber="137" phylum="Arthropoda" rank="genus">
