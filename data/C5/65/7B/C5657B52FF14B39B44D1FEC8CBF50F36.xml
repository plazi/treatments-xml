<quantity box="[1157,1302,810,834]" metricMagnitude="3" metricUnit="m" metricValue="1.4525" metricValueMax="2.31" metricValueMin="0.595" pageId="136" pageNumber="137" unit="m" value="1452.5" valueMax="2310.0" valueMin="595.0">
