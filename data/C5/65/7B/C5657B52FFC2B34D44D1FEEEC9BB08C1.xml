<elevation box="[972,1087,909,933]" metricMagnitude="1" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="94" pageNumber="95" unit="m" value="75.0" valueMax="100.0" valueMin="50.0">50–100 m</elevation>
