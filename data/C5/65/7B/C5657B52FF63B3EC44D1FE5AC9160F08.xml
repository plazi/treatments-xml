<taxonomicName baseAuthorityName="Renier" baseAuthorityYear="1804" box="[876,1180,928,952]" class="Polychaeta" family="Chaetopteridae" genus="Chaetopterus" kingdom="Animalia" pageId="255" pageNumber="256" phylum="Annelida" rank="species" species="variopedatus">
<emphasis box="[876,1180,928,952]" italics="true" pageId="255" pageNumber="256">Chaetopterus variopedatus</emphasis>
</taxonomicName>
; intertidal to 
<specimenCount box="[1332,1396,927,951]" pageId="255" pageNumber="256" type="generic">
<quantity box="[1332,1392,927,951]" metricMagnitude="1" metricUnit="m" metricValue="2.1" pageId="255" pageNumber="256" unit="m" value="21.0">21 m</quantity>
