<collectingCounty box="[296,521,1338,1362]" pageId="205" pageNumber="206">Mendocino County</collectingCounty>
, 
<collectorName box="[541,654,1338,1362]" pageId="205" pageNumber="206">California</collectorName>
to 
<location LSID="urn:lsid:plazi:treatment:C5657B52FF51B3DE44D1FD45CD540D06:48139C9FFF51B3DE4687FAF3CB7C0E7F" box="[705,890,1338,1363]" country="Iceland" county="Mendocino County" municipality="northern Gulf of California" name="Magdalena Bay" pageId="205" pageNumber="206">Magdalena Bay</location>
, 
<collectorName box="[908,1091,1338,1362]" pageId="205" pageNumber="206">Baja California</collectorName>
; 
<collectingMunicipality box="[1109,1435,1338,1362]" pageId="205" pageNumber="206">northern Gulf of California</collectingMunicipality>
between 
<collectingCountry box="[253,293,1374,1398]" name="Iceland" pageId="205" pageNumber="206">Isla</collectingCountry>
<collectorName box="[302,535,1374,1398]" pageId="205" pageNumber="206">Angel de la Guardia</collectorName>
, 
<collectorName box="[548,702,1374,1398]" pageId="205" pageNumber="206">Point Willard</collectorName>
, 
<collectorName box="[715,829,1374,1398]" pageId="205" pageNumber="206">Tiburon I.</collectorName>
