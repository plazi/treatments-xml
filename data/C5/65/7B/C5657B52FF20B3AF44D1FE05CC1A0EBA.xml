, 
<collectingRegion box="[634,765,1190,1214]" country="Russia" name="Kamchatka" pageId="188" pageNumber="189">Kamchatka</collectingRegion>
; 
<collectorName box="[781,906,1190,1214]" pageId="188" pageNumber="189">Pribilof Is.</collectorName>
to 
<collectorName box="[951,1061,1191,1215]" pageId="188" pageNumber="189">Monterey</collectorName>
, 
<collectingRegion box="[1077,1194,1190,1214]" country="United States of America" name="California" pageId="188" pageNumber="189">California</collectingRegion>
