<taxonomicName authority="(A. Agassiz, 1863)" baseAuthorityName="A. Agassiz" baseAuthorityYear="1863" box="[531,923,1992,2017]" class="Malacostraca" family="Hippolytidae" genus="Heptacarpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="franciscanus">
<emphasis box="[531,545,1993,2017]" italics="true" pageId="66" pageNumber="67">S</emphasis>
. 
<emphasis box="[558,701,1993,2017]" italics="true" pageId="66" pageNumber="67">franciscanus</emphasis>
(A. Agassiz, 1863)
</taxonomicName>
; intertidal zones to 
<specimenCount box="[1142,1206,1992,2016]" pageId="66" pageNumber="67" type="generic">
