<emphasis bold="true" box="[199,282,1413,1437]" inLineHeading="true" pageId="236" pageNumber="237" reason="1">Range.</emphasis>
<materialsCitation collectorName="Santa Cruz I." country="Mexico" county="San Ignacio Bay" location="South of Santa Cruz" municipality="California" pageId="236" pageNumber="237" specimenCount="1" stateProvince="Baja California">
