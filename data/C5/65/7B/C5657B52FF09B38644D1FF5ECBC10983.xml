<quantity box="[916,1060,591,615]" metricMagnitude="2" metricUnit="m" metricValue="7.2" metricValueMax="12.55" metricValueMin="1.85" pageId="149" pageNumber="150" unit="m" value="720.0" valueMax="1255.0" valueMin="185.0">185–1255 m</quantity>
