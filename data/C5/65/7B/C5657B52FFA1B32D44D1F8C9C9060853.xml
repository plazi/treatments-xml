<specimenCount box="[844,908,583,607]" pageId="62" pageNumber="63" type="generic">
<quantity box="[844,904,583,607]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="62" pageNumber="63" unit="m" value="20.0">20 m</quantity>
.
</specimenCount>
</paragraph>
