<taxonomicName authority="Holthuis, 1947" authorityName="Holthuis" authorityYear="1947" box="[530,926,1725,1747]" class="Malacostraca" family="Hippolytidae" genus="Spirontocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="holmesi">
