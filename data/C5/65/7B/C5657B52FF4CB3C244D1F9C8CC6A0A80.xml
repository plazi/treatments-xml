<specimenCount box="[1205,1283,295,319]" pageId="209" pageNumber="210" type="generic">
<quantity box="[1205,1279,295,319]" metricMagnitude="2" metricUnit="m" metricValue="1.4" pageId="209" pageNumber="210" unit="m" value="140.0">140 m</quantity>
.
</specimenCount>
</paragraph>
