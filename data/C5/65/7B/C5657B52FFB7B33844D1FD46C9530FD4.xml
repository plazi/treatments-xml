<elevation metricMagnitude="3" metricUnit="m" metricValue="3.8049999999999997" metricValueMax="5.61" metricValueMin="2.0" pageId="43" pageNumber="44" unit="m" value="3805.0" valueMax="5610.0" valueMin="2000.0">2000–5610 m</elevation>
