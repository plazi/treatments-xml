, 
<location LSID="urn:lsid:plazi:treatment:C5657B52FFE2B36D44D1FF5ECC880869:48139C9FFFE2B36D44D1FDB0C97409BD" box="[151,370,633,657]" county="Alameda County" municipality="Unalaska" name="Santa Clara County" pageId="126" pageNumber="127" stateProvince="Alaska">Santa Clara County</location>
