, 
<collectingRegion box="[693,829,597,621]" country="United States of America" name="Washington" pageId="126" pageNumber="127">Washington</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:C5657B52FFE2B36D44D1FF5ECC880869:48139C9FFFE2B36D470EFD9CCBFC0941" box="[840,1018,597,621]" county="Alameda County" municipality="Unalaska" name="Alameda Creek" pageId="126" pageNumber="127" stateProvince="Alaska">Alameda Creek</location>
