</taxonomicName>
or 
<taxonomicName authority="(Dall, 1889)" baseAuthorityName="Dall" baseAuthorityYear="1889" box="[955,1337,1029,1053]" class="Gastropoda" family="Eucyclidae" genus="Bathybembix" kingdom="Animalia" order="Seguenziida" pageId="193" pageNumber="194" phylum="Mollusca" rank="species" species="bairdi">
