<typeStatus box="[990,1045,1276,1300]" pageId="209" pageNumber="210">Type</typeStatus>
locality &quot; 
<location LSID="urn:lsid:plazi:treatment:C5657B52FF4DB3C244D1FE36CA990E53:48139C9FFF4DB3C240C6FB35CCCF0E38" box="[1152,1225,1276,1300]" name="Upper" pageId="209" pageNumber="210" stateProvince="California">Upper</location>
<collectingRegion box="[1232,1351,1276,1300]" country="United States of America" name="California" pageId="209" pageNumber="210">California</collectingRegion>
.”
