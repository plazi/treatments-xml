<materialsCitation box="[655,1093,1551,1575]" collectorName="San Geronimo I." location="Baja California" pageId="111" pageNumber="112" specimenCount="1" stateProvince="Baja California">
to 
<collectorName box="[691,892,1551,1575]" pageId="111" pageNumber="112">San Geronimo I.</collectorName>
, 
