<location LSID="urn:lsid:plazi:treatment:C55987A1811FFFD43391F9300AC2F960:482F606C811FFFD5372FFDB00F93FD8C" box="[1101,1235,584,606]" latitude="-40.544422" longLatPrecision="1" longitude="-73.7213" name="Pucatrihue" pageId="6" pageNumber="309" stateProvince="Valparaiso">Pucatrihue</location>
(
<geoCoordinate box="[1252,1419,584,605]" degrees="40" direction="south" minutes="32" orientation="latitude" pageId="6" pageNumber="309" precision="1" seconds="39.92" value="-40.544422">40°32’39.92’’S</geoCoordinate>
, 
<geoCoordinate box="[809,974,614,636]" degrees="73" direction="west" minutes="43" orientation="longitude" pageId="6" pageNumber="309" precision="1" seconds="16.68" value="-73.7213">73°43’16.68’’W</geoCoordinate>
)
