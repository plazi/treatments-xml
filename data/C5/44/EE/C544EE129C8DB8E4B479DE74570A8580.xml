<paragraph pageId="null" pageNumber="395">
<taxonomicName authority="L." class="Liliopsida" family="Cyperaceae" genus="Schoenus" kingdom="Plantae" order="Poales" pageId="null" pageNumber="395" phylum="Tracheophyta" rank="species" species="nigricans">
