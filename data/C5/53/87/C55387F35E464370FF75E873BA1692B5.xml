<quantity box="[1063,1191,654,681]" metricMagnitude="2" metricUnit="m" metricValue="6.515" metricValueMax="7.75" metricValueMin="5.279999999999999" pageId="7" pageNumber="430" unit="m" value="651.5" valueMax="775.0" valueMin="528.0">528–775 m</quantity>
