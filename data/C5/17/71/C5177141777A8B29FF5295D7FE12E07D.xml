<quantity box="[728,803,450,476]" metricMagnitude="-2" metricUnit="m" metricValue="8.89" metricValueMax="10.16" metricValueMin="7.62" pageId="3" pageNumber="4" unit="in" value="3.5" valueMax="4.0" valueMin="3.0">3–4 in</quantity>
each aster, 
<number box="[944,1016,450,476]" pageId="3" pageNumber="4" value="12.0" valueMax="13.0" valueMin="11.0">11–13</number>
marginal between asters, 
<number box="[1325,1369,450,476]" pageId="3" pageNumber="4" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
anterior; III, 
<number box="[252,329,488,514]" pageId="3" pageNumber="4" value="21.0" valueMax="22.0" valueMin="20.0">20–22</number>
; IV–V, 
<number box="[419,496,488,514]" pageId="3" pageNumber="4" value="28.5" valueMax="31.0" valueMin="26.0">26–31</number>
; VI, 
<number box="[553,630,488,514]" pageId="3" pageNumber="4" value="25.5" valueMax="27.0" valueMin="24.0">24–27</number>
