marginal between asters, 
<number box="[603,632,1053,1079]" pageId="9" pageNumber="10" value="14.0">14</number>
anterior (
<figureCitation box="[749,838,1053,1079]" captionStart="FIGURES 26 – 36. 26 – 30" captionStartId="10.[151,269,1839,1863]" captionTargetBox="[305,1317,358,1774]" captionTargetId="figure@10.[248,1343,354,1823]" captionTargetPageId="10" captionText="FIGURES 26 – 36. 26 – 30. Myrsidea brunneinuchi. 26. Male ventral terminalia. 27. Male metasternum and sternites I – II. 28. Female sternite II. 29. Male genital sac sclerite. 30. Female metanotum and dorsal abdomen. 31 – 33. M. gularis. 31. Male dorsoventral terminalia. 32. Male sternite II. 33. Male genital sac sclerite. 34 – 36. M. conirostris. 34. Female metanotum and dorsal abdomen. 35. Male genital sac sclerite. 36. Male ventral terminalia." httpUri="https://zenodo.org/record/176600/files/figure.png" pageId="9" pageNumber="10">
Fig. 
<number box="[805,838,1053,1079]" pageId="9" pageNumber="10" value="32.0">32</number>
</figureCitation>
); III, 
<number box="[905,983,1053,1079]" pageId="9" pageNumber="10" value="30.5" valueMax="31.0" valueMin="30.0">30–31</number>
; IV, 
<number box="[1038,1115,1053,1079]" pageId="9" pageNumber="10" value="44.0" valueMax="47.0" valueMin="41.0">41–47</number>
; V, 
<number box="[1161,1239,1053,1079]" pageId="9" pageNumber="10" value="45.5" valueMax="52.0" valueMin="39.0">39–52</number>
; VI, 
<number box="[1298,1375,1053,1079]" pageId="9" pageNumber="10" value="41.0" valueMax="45.0" valueMin="37.0">37–45</number>
; VII, 
<number box="[151,227,1093,1119]" pageId="9" pageNumber="10" value="30.5" valueMax="32.0" valueMin="29.0">29–32</number>
