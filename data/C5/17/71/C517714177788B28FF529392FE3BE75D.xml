, 
<number box="[437,558,264,290]" pageId="2" pageNumber="3" value="0.55" valueMax="0.59" valueMin="0.51">0.51–0.59</number>
; ANW, 
<number box="[652,773,264,290]" pageId="2" pageNumber="3" value="0.195" valueMax="0.2" valueMin="0.19">0.19–0.20</number>
; TL, 
