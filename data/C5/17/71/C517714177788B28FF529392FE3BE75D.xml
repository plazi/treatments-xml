<number box="[1310,1431,226,252]" pageId="2" pageNumber="3" value="0.27" valueMax="0.28" valueMin="0.26">0.26–0.28</number>
; MW, 
<number box="[216,337,264,290]" pageId="2" pageNumber="3" value="0.43" valueMax="0.45" valueMin="0.41">0.41–0.45</number>
; 
