<number box="[151,228,893,919]" pageId="7" pageNumber="8" value="28.0" valueMax="32.0" valueMin="24.0">24–32</number>
; II, 
<number box="[281,358,893,919]" pageId="7" pageNumber="8" value="31.0" valueMax="36.0" valueMin="26.0">26–36</number>
