<quantity box="[1231,1306,418,444]" metricMagnitude="-2" metricUnit="m" metricValue="8.89" metricValueMax="10.16" metricValueMin="7.62" pageId="6" pageNumber="7" unit="in" value="3.5" valueMax="4.0" valueMin="3.0">3–4 in</quantity>
each aster, 
<number box="[151,180,458,484]" pageId="6" pageNumber="7" value="16.0">16</number>
marginal between asters, 
<number box="[485,514,458,484]" pageId="6" pageNumber="7" value="14.0">14</number>
anterior; III, 
<number box="[672,704,458,484]" pageId="6" pageNumber="7" value="25.0">25</number>
; IV–V, 
<number box="[795,872,458,484]" pageId="6" pageNumber="7" value="33.5" valueMax="34.0" valueMin="33.0">33–34</number>
; VI, 
<number box="[930,962,458,484]" pageId="6" pageNumber="7" value="27.0">27</number>
; VII, 
<number box="[1030,1062,458,484]" pageId="6" pageNumber="7" value="16.0">16</number>
