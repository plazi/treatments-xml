; MW, 
<number box="[738,858,1509,1535]" pageId="3" pageNumber="4" value="0.42" valueMax="0.43" valueMin="0.41">0.41–0.43</number>
; 
<collectionCode box="[873,950,1509,1535]" pageId="3" pageNumber="4">AWIV</collectionCode>
, 
<number box="[963,1084,1509,1535]" pageId="3" pageNumber="4" value="0.54" valueMax="0.55" valueMin="0.53">0.53–0.55</number>
; ANW, 
<number box="[1182,1303,1509,1535]" pageId="3" pageNumber="4" value="0.2" valueMax="0.21" valueMin="0.19">0.19–0.21</number>
; TL, 
<number pageId="3" pageNumber="4" value="1.405" valueMax="1.45" valueMin="1.36">1.36– 1.45</number>
