<taxonomicName box="[569,728,1977,2001]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="51" pageNumber="352" phylum="Arthropoda" rank="species" species="deresyensis">
<emphasis box="[569,728,1977,2001]" italics="true" pageId="51" pageNumber="352">E. deresyensis</emphasis>
</taxonomicName>
and 
<taxonomicName box="[783,942,1977,2001]" class="Insecta" family="Oecophoridae" genus="Oecophora" kingdom="Animalia" order="Lepidoptera" pageId="51" pageNumber="352" phylum="Arthropoda" rank="species" species="galactitella">
<emphasis box="[783,942,1977,2001]" italics="true" pageId="51" pageNumber="352">E. galactitella</emphasis>
