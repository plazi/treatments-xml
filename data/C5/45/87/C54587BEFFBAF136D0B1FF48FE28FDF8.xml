<taxonomicName authority="Wang, Jing, Liu, Li" authorityName="Wang, Jing, Liu, Li" authorityYear="2014" box="[294,871,217,239]" class="Insecta" family="Tettigoniidae" genus="Xizicus" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="quadrifascipes" status="sp. nov." subGenus="Zangxizicus">
<emphasis box="[294,367,218,239]" italics="true" pageId="8" pageNumber="309">Xizicus</emphasis>
(
<emphasis box="[382,503,218,239]" italics="true" pageId="8" pageNumber="309">Zangxizicus</emphasis>
) 
<emphasis box="[517,666,217,239]" italics="true" pageId="8" pageNumber="309">quadrifascipes</emphasis>
Wang, Jing, Liu, Li
</taxonomicName>
, 
<emphasis bold="true" box="[882,962,217,238]" pageId="8" pageNumber="309">
<taxonomicNameLabel box="[882,962,217,238]" pageId="8" pageNumber="309" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
