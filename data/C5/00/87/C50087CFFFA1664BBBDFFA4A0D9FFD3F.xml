<taxonomicName authorityName="Clarke" authorityYear="2015" baseAuthorityName="Giesbert" baseAuthorityYear="1991" box="[377,493,1891,1915]" class="Insecta" family="Cerambycidae" genus="Ameriphoderes" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="44" phylum="Arthropoda" rank="species" species="magna">
