<typeStatus box="[1176,1279,414,440]" pageId="8" pageNumber="9">holotype</typeStatus>
specimen off Motobu, 
<collectingRegion box="[338,448,450,476]" country="Japan" name="Okinawa" pageId="8" pageNumber="9">Okinawa</collectingRegion>
