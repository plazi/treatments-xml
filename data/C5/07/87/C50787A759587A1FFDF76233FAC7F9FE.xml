<subSubSection box="[189,1057,1335,1408]" pageId="19" pageNumber="20" type="distribution">
<paragraph blockId="19.[189,342,1335,1361]" box="[189,342,1335,1361]" pageId="19" pageNumber="20">
<heading bold="true" box="[189,342,1335,1361]" fontSize="11" level="3" pageId="19" pageNumber="20" reason="2">
