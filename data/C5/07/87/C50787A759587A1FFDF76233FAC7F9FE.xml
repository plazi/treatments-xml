<paragraph blockId="17.[189,426,1090,1116]" box="[189,426,1090,1116]" pageId="17" pageNumber="18">
<heading bold="true" box="[189,426,1090,1116]" fontSize="11" level="3" pageId="17" pageNumber="18" reason="2">
<emphasis bold="true" box="[189,426,1090,1116]" pageId="17" pageNumber="18">Material examined</emphasis>
</heading>
</paragraph>
<paragraph blockId="17.[189,1398,1137,1234]" box="[234,348,1137,1163]" pageId="17" pageNumber="18">
<heading bold="true" box="[234,348,1137,1163]" fontSize="11" level="3" pageId="17" pageNumber="18" reason="6">
