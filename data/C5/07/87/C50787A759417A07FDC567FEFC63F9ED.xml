<taxonomicName authority="Motschulsky, 1851" authorityName="Motschulsky" authorityYear="1851" class="Insecta" family="Tenebrionidae" genus="Blaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="appendiculata">
