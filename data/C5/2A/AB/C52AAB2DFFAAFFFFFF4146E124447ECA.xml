<paragraph blockId="14.[151,1436,222,391]" pageId="14" pageNumber="501">
<taxonomicName authority="Voigt, 1903" authorityName="Voigt" authorityYear="1903" box="[199,507,294,319]" family="Chaetonotidae" genus="Aspidiophorus" kingdom="Animalia" order="Chaetonotida" pageId="14" pageNumber="501" phylum="Gastrotricha" rank="genus">
