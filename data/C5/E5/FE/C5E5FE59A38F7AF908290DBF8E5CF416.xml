<taxonomicName class="Insecta" family="Ectobiidae" genus="Muzoa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Muzoa" order="Blattodea" pageId="2" pageNumber="67" phylum="Arthropoda" rank="genus">Muzoa</taxonomicName>
