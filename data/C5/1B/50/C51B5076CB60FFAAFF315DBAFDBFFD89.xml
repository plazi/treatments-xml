<subSubSection box="[199,1206,404,429]" pageId="2" pageNumber="389" type="description">
<paragraph blockId="2.[151,1437,151,573]" box="[199,1206,404,429]" pageId="2" pageNumber="389">
<emphasis bold="true" box="[199,453,404,429]" pageId="2" pageNumber="389">Measurements (mm).</emphasis>
