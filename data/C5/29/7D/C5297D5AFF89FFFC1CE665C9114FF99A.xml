<typeStatus box="[862,960,1605,1630]" pageId="4" pageNumber="91">holotype</typeStatus>
origin, 
<collectingCountry box="[1048,1109,1605,1630]" name="India" pageId="4" pageNumber="91">India</collectingCountry>
