(
<figureCitation id="13452A166A7D0E51FF13FC1AFF3EFBA7" box="[159,226,1014,1039]" captionStart="FIGURE 1" captionStartId="5.[151,250,1807,1829]" captionTargetBox="[176,1419,204,1756]" captionTargetId="figure@5.[151,1436,195,1783]" captionTargetPageId="5" captionText="FIGURE 1. Antillipeltis species, habitus, dorsal: A, A. darlingtoni sp. nov., length = 5.1 mm; B, A. maculata sp. nov., length = 4.4 mm; C, A. minuta sp. nov., length = 2.8 mm; D, A. nitida sp. nov., length = 3.1 mm; E, A. portoricensis sp. nov., length = 4.3 mm; F, A. pubescens sp. nov., length = 5.5 mm." httpUri="https://zenodo.org/record/231125/files/figure.png" pageId="8" pageNumber="443">
Figs 
<number id="3DB624B46A7D0E51FF5BFC1BFF3EFBA7" box="[215,226,1015,1039]" pageId="8" pageNumber="443" value="1.0">1</number>
</figureCitation>
C, 
<number id="3DB624B46A7D0E51FE8AFC1BFECFFBA7" box="[262,275,1015,1039]" pageId="8" pageNumber="443" value="3.0">3</number>
