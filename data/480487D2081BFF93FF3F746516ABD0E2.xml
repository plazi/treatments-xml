<subSubSection id="88B7654F0819FF93FF6F701513DFD03A" pageId="7" pageNumber="34" type="etymology">
<paragraph id="C01236C40819FF93FF6F701513DFD03A" blockId="7.[151,1439,151,898]" pageId="7" pageNumber="34">
<emphasis id="F2D9EAD60819FF93FF6F7015130CD051" bold="true" box="[199,339,791,817]" pageId="7" pageNumber="34">Etymology.</emphasis>
