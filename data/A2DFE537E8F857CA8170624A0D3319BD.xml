<subSubSection id="6CDE56DDBB30F58C09A3B5D0B9B9C8C5" pageId="0" pageNumber="27" type="specimen condition">
<paragraph id="CFBA3DAB440A0161B7BAE41D9BB3D841" pageId="0" pageNumber="27">Specimen condition.</paragraph>
<paragraph id="78FAFCEAFE851118284DC803A672FD38" pageId="0" pageNumber="27">Face obscured by antenna. Propodeum and metanotum obscured by air bubble. Setae on mesoscutellum obscured by debris. Nematoceran fly present as a separate inclusion.</paragraph>
<caption id="693C18F362FA790D38119358CB645C68" doi="10.3897/jhr.79.55899.figure5" httpUri="https://binary.pensoft.net/fig/469573" pageId="0" pageNumber="27" start="Figure 5" startId="F5">
<paragraph id="AB8A528EEDE8CADAFB565E35AFC68134" pageId="0" pageNumber="27">
<emphasis id="C6CF4274947A36F20E3C479CCC27A03A" bold="true" pageId="0" pageNumber="27">Figure 5.</emphasis>
<emphasis id="E69368E21622C86D4F8D740AFF4534F9" bold="true" pageId="0" pageNumber="27">A-D. A</emphasis>
<taxonomicName id="E017C9A773CF06D887E09F4788E3236C" baseAuthorityName="Provancher" baseAuthorityYear="1887" class="Insecta" family="Eulophidae" genus="Sympiesis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sympiesis conica" order="Hymenoptera" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="conica">
<emphasis id="19D1712B16574C15D82973F83126B8F3" italics="true" pageId="0" pageNumber="27">Sympiesis cf. conica</emphasis>
</taxonomicName>
, head, tfs = transfacial sulcus, scs = scrobal sulcus 
<emphasis id="9B610B36C55AB1C03EE8A5CB785D026C" bold="true" pageId="0" pageNumber="27">B</emphasis>
<taxonomicName id="DA29C8E24D5626C4512C527774EAE90C" baseAuthorityName="Walker" baseAuthorityYear="1839" class="Insecta" family="Eulophidae" genus="Crataepus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crataepus marbis" order="Hymenoptera" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="marbis">
<emphasis id="8D0BBBCF59AFA9AE2DD0CD8719DA98D4" italics="true" pageId="0" pageNumber="27">Crataepus marbis</emphasis>
</taxonomicName>
, head, oos = occellar ocular sulcus, ocs = occellar triangle sulcus, uos = upper ocular sulcus 
<emphasis id="5DBE56BA290D7CEC72BB1128930E3D1C" bold="true" pageId="0" pageNumber="27">C</emphasis>
<taxonomicName id="11F863C32BCF7C613252030FCF9477F9" authorityName="Girault" authorityYear="1916" class="Insecta" family="Eulophidae" genus="Zagrammosoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zagrammosoma americanum" order="Hymenoptera" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="americanum">
