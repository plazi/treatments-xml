<taxonomicName id="DD6F70B8B6A2D528590E85A30F8A1124" authorityName="Boucek" authorityYear="1958" class="Insecta" family="Tetracampidae" genus="Foersterella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Foersterella erdoesi" order="Hymenoptera" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="erdoesi">
<emphasis id="EC99B351875C00408CC4498834A18F62" italics="true" pageId="0" pageNumber="27">Foersterella erdoesi</emphasis>
