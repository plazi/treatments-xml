<taxonomicName id="A4316D2C0293F7810DB35A774947B803" class="Gastropoda" family="Polyceridae" genus="Gymnodoris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnodoris" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="genus">
