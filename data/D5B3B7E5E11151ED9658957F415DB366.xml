<taxonomicName id="614502F458EF4557AFE924BB549B5D81" class="Gastropoda" family="Polyceridae" genus="Gymnodoris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnodoris" order="Nudibranchia" pageId="0" pageNumber="73" phylum="Mollusca" rank="genus">
<emphasis id="2053690B687C44CA009BA241D3E20193" italics="true" pageId="0" pageNumber="73">Gymnodoris</emphasis>
</taxonomicName>
