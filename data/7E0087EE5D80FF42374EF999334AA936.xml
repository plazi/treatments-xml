<taxonomicName id="31A94D7B5D8EFF4337EAF9E933C4AD4D" authorityName="Webb" authorityYear="2006" box="[428,642,1564,1589]" class="Insecta" family="Therevidae" genus="Lysilinga" kingdom="Animalia" order="Diptera" pageId="216" pageNumber="217" phylum="Arthropoda" rank="species" species="chamela">
