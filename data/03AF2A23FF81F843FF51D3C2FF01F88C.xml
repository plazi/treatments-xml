<emphasis id="B9724727FF81F843FDCDD40FFDC8FA84" box="[523,553,1300,1326]" italics="true" pageId="123" pageNumber="124">p”</emphasis>
, 
<emphasis id="B9724727FF81F843FDF1D40FFDAFFA84" box="[567,590,1300,1326]" italics="true" pageId="123" pageNumber="124">a’</emphasis>
, 
<emphasis id="B9724727FF81F843FD9AD40FFD9BFA84" box="[604,634,1300,1326]" italics="true" pageId="123" pageNumber="124">a”</emphasis>
, 
<emphasis id="B9724727FF81F843FD4ED40FFD7EFA84" box="[648,671,1300,1326]" italics="true" pageId="123" pageNumber="124">u’</emphasis>
, 
<emphasis id="B9724727FF81F843FD6BD40FFD2AFA84" box="[685,715,1300,1326]" italics="true" pageId="123" pageNumber="124">u”</emphasis>
, 
<emphasis id="B9724727FF81F843FD1FD40FFD1BFA84" box="[729,762,1300,1326]" italics="true" pageId="123" pageNumber="124">vs’</emphasis>
) and 
<emphasis id="B9724727FF81F843FCFFD40FFCBAFA84" box="[825,859,1300,1326]" italics="true" pageId="123" pageNumber="124">ω1</emphasis>
; III and IV with 8 setae each (
