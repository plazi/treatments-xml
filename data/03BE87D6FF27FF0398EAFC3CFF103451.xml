<quantity id="4CEF9B25FF27FF039A23FBE7F8B03434" box="[606,753,1045,1073]" metricMagnitude="2" metricUnit="m" metricValue="6.708648" metricValueMax="12.877799999999999" metricValueMin="0.539496" pageId="182" pageNumber="183" unit="ft" value="2201.0" valueMax="4225.0" valueMin="177.0">177–4,225 ft</quantity>
