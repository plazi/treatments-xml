. To 
<quantity id="4CEF9B25FF27FF039B03FC3CF99933EC" box="[894,984,974,1001]" metricMagnitude="-1" metricUnit="m" metricValue="4.45" pageId="182" pageNumber="183" unit="cm" value="44.5">44.5 cm</quantity>
