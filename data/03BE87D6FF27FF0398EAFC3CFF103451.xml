<quantity id="4CEF9B25FF27FF0399B4FBE7F80E3435" box="[457,591,1045,1072]" metricMagnitude="2" metricUnit="m" metricValue="6.709999999999999" metricValueMax="12.88" metricValueMin="0.54" pageId="182" pageNumber="183" unit="m" value="671.0" valueMax="1288.0" valueMin="54.0">54–1,288 m</quantity>
