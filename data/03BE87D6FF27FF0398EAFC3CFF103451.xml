<quantity id="4CEF9B25FF27FF039D4AFBE7FF313435" box="[1335,1392,1045,1072]" metricMagnitude="1" metricUnit="m" metricValue="2.5" pageId="182" pageNumber="183" unit="m" value="25.0">25 m</quantity>
