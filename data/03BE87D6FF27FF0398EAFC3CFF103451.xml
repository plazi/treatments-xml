<quantity id="4CEF9B25FF27FF039B95FC3CFE7933EC" box="[1000,1080,974,1001]" metricMagnitude="-1" metricUnit="m" metricValue="4.445" pageId="182" pageNumber="183" unit="in" value="17.5">17.5 in</quantity>
