<td id="768C5DE3EFB7C5CAFF2DFD8BFEE6FDA1" box="[232,291,581,608]" gridcol="1" gridrow="2" pageId="230" pageNumber="231">0-0</td>
<td id="768C5DE3EFB7C5CAFE8DFD8BFE40FDA1" box="[328,389,581,608]" gridcol="2" gridrow="2" pageId="230" pageNumber="231">1-0</td>
