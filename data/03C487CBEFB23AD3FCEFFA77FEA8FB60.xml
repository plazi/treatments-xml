<taxonomicName id="4C6D4D5EEFB43AD0FEF1FAFCFD95FA8A" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[308,592,1329,1355]" class="Maxillopoda" family="Notodelphyidae" genus="Periproctia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="hexachaetata" status="sp. nov.">
