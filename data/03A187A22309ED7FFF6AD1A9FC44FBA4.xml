<taxonomicName id="4C084D372309ED7FFE85D306FDF9FC83" box="[376,548,842,867]" class="Insecta" family="Cantharidae" genus="Silis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="baolacensis">
