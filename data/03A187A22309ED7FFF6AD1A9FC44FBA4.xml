<taxonomicName id="4C084D372309ED7FFDA2D306FD38FC83" box="[607,741,842,867]" class="Insecta" family="Cantharidae" genus="Silis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="tonkinea">
