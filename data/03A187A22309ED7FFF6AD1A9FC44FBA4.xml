<taxonomicName id="4C084D372309ED7FFAD8D306FA49FC83" box="[1317,1428,842,867]" class="Insecta" family="Cantharidae" genus="Podosilis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="60" phylum="Arthropoda" rank="genus">
