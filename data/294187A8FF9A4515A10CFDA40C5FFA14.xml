<paragraph id="A15736BEFF984512A112FDD00C0CFC5E" pageId="3" pageNumber="4">
<table id="D3E8C41EFF98BAEEA112FDD00D4CFBB1" box="[278,1295,632,1049]" gridcols="3" gridrows="4" pageId="3" pageNumber="4">
<tr id="1FD834FCFF98BAEEA112FDD00D4CFD7D" box="[278,1295,632,725]" gridrow="0" pageId="3" pageNumber="4">
<th id="5C095D80FF98BAEEA112FDD009C1FD7D" box="[278,386,632,725]" gridcol="0" gridrow="0" pageId="3" pageNumber="4">Pronotum</th>
<th id="5C095D80FF98BAEEA1ADFDD00B19FD7D" box="[425,858,632,725]" gridcol="1" gridrow="0" pageId="3" pageNumber="4">blue-green-golden, very shiny, glabrous; prehumeral-ridge fine</th>
