<taxonomicName id="4C6903CFFFE4CE0DFCFB311CFC65F8ED" authorityName="Evans" authorityYear="1956" box="[802,965,1818,1844]" class="Arachnida" family="Parholaspididae" genus="Neparholaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="427" phylum="Arthropoda" rank="genus">
