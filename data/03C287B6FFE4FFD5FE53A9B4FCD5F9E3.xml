<typeStatus id="54D08802FFE4FFD5FDFFA88BFD63FA63" box="[610,708,1448,1472]" pageId="2" pageNumber="631">lectotype</typeStatus>
(male) and 
<specimenCount id="9D6DFD29FFE4FFD5FCDAA88AFBA4FA63" box="[839,1027,1448,1473]" pageId="2" pageNumber="631" type="generic" typeStatus="paralectotypes">22 paralectotypes</specimenCount>
