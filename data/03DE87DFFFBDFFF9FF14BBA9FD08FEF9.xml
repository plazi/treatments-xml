<materialsCitation id="3B1F3C94FFBCFFF9FCBBB9A1FCD2FF12" collectionCode="CSIRO" country="Australia" location="Australian National Insect Collection" municipality="Canberra" pageId="3" pageNumber="44" specimenCount="2" specimenCount-female="1" specimenCount-male="1" stateProvince="Tasmania" typeStatus="paratype">
