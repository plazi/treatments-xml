<quantity id="4CB79B18FFDB85567FBCF851FBF8F802" box="[1016,1088,1942,1966]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="13" pageNumber="336" unit="mm" value="11.0">11mm</quantity>
