<taxonomicName id="4C4F4D7EFFDB85567FE1F87EFBB1F87E" box="[933,1033,1977,2002]" class="Insecta" family="Lycaenidae" genus="Rhamma" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="336" phylum="Arthropoda" rank="genus">
<emphasis id="B93BEAEFFFDB85567FE1F87EFBB1F87E" box="[933,1033,1977,2002]" italics="true" pageId="13" pageNumber="336">Rhamma</emphasis>
</taxonomicName>
