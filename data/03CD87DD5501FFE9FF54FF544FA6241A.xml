<number id="3DAC24EC5501FFE9FC76FD214DC420C4" box="[949,980,738,762]" pageId="4" pageNumber="53" value="92.0">92</number>
, ctt 
<number id="3DAC24EC5501FFE9FBC0FD214A1D20C4" box="[1027,1037,738,762]" pageId="4" pageNumber="53" value="1.0">1</number>
: 
<number id="3DAC24EC5501FFE9FBDAFD214A2720C4" box="[1049,1079,738,762]" pageId="4" pageNumber="53" value="16.0">16</number>
, Mx 
