, 
<collectingMunicipality id="6BDAACCBFFE27D6328D1FDC366ACFDAD" box="[711,919,525,551]" pageId="44" pageNumber="45">northern Florida</collectingMunicipality>
to 
<collectingCountry id="F3167621FFE27D6329D5FDC36133FDAD" box="[963,1032,525,551]" name="Cuba" pageId="44" pageNumber="45">Cuba</collectingCountry>
, 
<quantity id="4CF99B54FFE27D632E00FDC361BEFDAD" box="[1046,1157,525,551]" metricMagnitude="1" metricUnit="m" metricValue="4.8" metricValueMax="7.7" metricValueMin="1.9" pageId="44" pageNumber="45" unit="m" value="48.0" valueMax="77.0" valueMin="19.0">
