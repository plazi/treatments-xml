<materialsCitation id="3B693CECFFE27D632BF9FDC36418FDC6" collectionCode="NMNH" country="Cuba" elevation="48" location="Gulf of Mexico" municipality="northern Florida" pageId="44" pageNumber="45" specimenCount="1">
