<taxonomicName id="3C8B4D09FF98FFB2FB3F6319A93C485D" box="[1209,1381,1571,1595]" class="Actinopterygii" family="Pseudochromidae" genus="Pseudochromis" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="59" phylum="Chordata" rank="genus">
