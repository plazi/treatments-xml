<subSubSection id="9C48FABD64C16A305FEADE9B519840DD" lastPageId="12" lastPageNumber="33" pageId="11" pageNumber="32" type="other material studied">
<paragraph id="58AA6B2EDDC30BB121A015419228D148" pageId="11" pageNumber="32">Other material studied.</paragraph>
<paragraph id="B35FA75DFB75662805D13DB4B42DFB98" lastPageId="12" lastPageNumber="33" pageId="11" pageNumber="32">
