<taxonomicName id="4C714D303F27F061FB7FFB9DFEC03B32" authority="Chamberlin 1918" authorityName="Chamberlin" authorityYear="1918" class="Diplopoda" family="Xystodesmidae" genus="Boraria" kingdom="Animalia" order="Polydesmida" pageId="10" pageNumber="26" phylum="Arthropoda" rank="species" species="infesta">
