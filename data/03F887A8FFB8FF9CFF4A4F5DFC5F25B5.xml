<taxonomicName id="4C514D3DFFB8FF9CFF4A4F5DFDD6240E" authority="Beljaev &amp; Mironov, 1982" authorityName="Beljaev &amp; Mironov" authorityYear="1982" box="[151,628,1587,1614]" class="Holothuroidea" family="Myriotrochidae" genus="Prototrochus" kingdom="Animalia" order="Apodida" pageId="1" pageNumber="265" phylum="Echinodermata" rank="genus">
