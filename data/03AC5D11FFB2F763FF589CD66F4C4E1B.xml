<quantity id="4CFD41E2FFB2F76CFE12927C6CF04014" box="[414,511,2048,2072]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" pageId="7" pageNumber="158" unit="mm" value="11.5">11,5 mm</quantity>
