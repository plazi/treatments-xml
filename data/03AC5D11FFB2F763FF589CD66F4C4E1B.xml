<quantity id="4CFD41E2FFB2F76CFD3F927C6E084014" box="[691,775,2048,2072]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" pageId="7" pageNumber="158" unit="mm" value="6.5">6,5 mm</quantity>
