<paragraph id="8BF0368FFFB2FFC4F3C2FEBC2C0C7565" blockId="81.[151,516,320,347]" box="[151,516,320,347]" pageId="81" pageNumber="82">
<heading id="D0B881E3FFB2FFC4F3C2FEBC2C0C7565" bold="true" box="[151,516,320,347]" fontSize="11" level="1" pageId="81" pageNumber="82" reason="1">
<emphasis id="B93BEA9DFFB2FFC4F3C2FEBC2C0C7565" bold="true" box="[151,516,320,347]" pageId="81" pageNumber="82">
<taxonomicName id="4C4F4D0CFFB2FFC4F3C2FEBC2F5D7564" authorityName="Zelazny" authorityYear="1981" box="[151,341,321,347]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="ornata">
<emphasis id="B93BEA9DFFB2FFC4F3C2FEBC2F5D7564" bold="true" box="[151,341,321,347]" italics="true" pageId="81" pageNumber="82">Rhotana ornata</emphasis>
</taxonomicName>
