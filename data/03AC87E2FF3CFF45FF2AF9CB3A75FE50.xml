<paragraph id="8BBA36F4FF3CFF4AFD25FEB33A5AFE01" blockId="135.[701,1228,308,1759]" pageId="135" pageNumber="135">
<typeStatus id="54BE8856FF3CFF4AFD25FEB33A27FEE8" box="[730,832,308,332]" pageId="135" pageNumber="135">Syntype</typeStatus>
