<taxonomicName id="4C034D4D7619FF806931FAE990E2FADD" authority="Benham, 1921" authorityName="Benham" authorityYear="1921" box="[293,653,1307,1330]" class="Polychaeta" family="Polynoidae" genus="Hololepidella" kingdom="Animalia" order="Phyllodocida" pageId="6" pageNumber="31" phylum="Annelida" rank="species" species="flynni">
