<taxonomicName id="4C054D38ED7E867765D1FDCEFCD8A63F" box="[724,818,564,589]" class="Insecta" family="Leptoceridae" genus="Notalina" kingdom="Animalia" order="Trichoptera" pageId="8" pageNumber="41" phylum="Arthropoda" rank="species" species="paulista">
