<taxonomicName id="8DEC4D30FF8A4F3DFD2DFC4A44B1FEC7" authority="Ramme" authorityName="Ramme" authorityYear="1951" box="[746,968,280,304]" class="Insecta" family="Pamphagidae" genus="Nocaropsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="genus">
