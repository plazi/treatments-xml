<paragraph id="B568C5FC3C112BEDF94A56496B5B4C2C" pageId="33" pageNumber="84">
<taxonomicName id="4A362035CE5508B947F71576315F14D6" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pholcus ethagala" order="Araneae" pageId="33" pageNumber="84" phylum="Arthropoda" rank="species" species="ethagala">Pholcus ethagala</taxonomicName>
