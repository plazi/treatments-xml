(
<figureCitation id="CE891894FFDF9937CB8BFB87FEDBDC7B" box="[159,260,1112,1138]" captionStart="FIGURES 1–3" captionStartId="2.[151,264,1654,1678]" captionTargetBox="[157,1430,1035,1625]" captionTargetId="figure-380@2.[151,1436,1029,1631]" captionTargetPageId="2" captionText="FIGURES 1–3. Phaonia sunqiia Xue &amp; Du sp. nov. Male: 1. Sternite 5 in ventral view, scale bar = 0.5 mm; 2. Cerci in posterior view, scale bar = 0.2 mm; 3. Surstylus, cerci and epandrium in profile, scale bar = 0.2 mm." figureDoi="http://doi.org/10.5281/zenodo.4358918" httpUri="https://zenodo.org/record/4358918/files/figure.png" pageId="1" pageNumber="560">Figs 1–3</figureCitation>
, 
<figureCitation id="CE891894FFDF9937CA04FB87FE87DC7A" box="[272,344,1112,1139]" captionStart="FIGURES 24–32" captionStartId="10.[151,264,1769,1793]" captionTargetBox="[176,1418,209,1717]" captionTargetId="figure-31@10.[151,1436,181,1745]" captionTargetPageId="10" captionText="FIGURES 24–32. Phaonia sunqiia, Xue &amp; Du sp. nov. Male: 24. Body in profile, scale bar = 2.0 mm; 25. Head in anterior view, scale bar = 1.0 mm. Phaonia zhangjunyui, Xue &amp; Du sp. nov. Male: 26. Head in anterior view, scale bar = 1.0 mm. Phaonia liujiayui, Xue sp. nov. Male: 27. Head in anterior view, scale bar = 1.0 mm; 28. Scutellum in dorsal view, scale bar = 500 μm. Phaonia wangpingga Xue sp. nov. Male: 29. Body in profile, scale bar = 2.0 mm; 30. Scutellum in dorsal view, scale bar = 1.0 mm. Phaonia tianmushanensis Xue sp. nov. Male: 31. Head in anterior view, scale bar = 1.0 mm; 32. Scutellum in dorsal view, scale bar = 1.0 mm." figureDoi="http://doi.org/10.5281/zenodo.4358936" httpUri="https://zenodo.org/record/4358936/files/figure.png" pageId="1" pageNumber="560">24–25</figureCitation>
, 
<figureCitation id="CE891894FFDF9937CA70FB87FE5EDC7B" box="[356,385,1112,1138]" captionStart="FIGURE 33" captionStartId="11.[151,250,1908,1932]" captionTargetBox="[151,1436,1029,1885]" captionTargetId="figure-422@11.[151,1436,1029,1885]" captionTargetPageId="11" captionText="FIGURE 33. Distribution map of the type localities of the new species: 1. Phaonia sunqiia, Xue &amp; Du sp. nov.; 2. Phaonia zhangjunyui, Xue &amp; Du sp. nov.; 3. Phaonia liujiayui, Xue sp. nov.; 4. Phaonia liujiayui, Xue sp. nov.; 5. Phaonia tianmushanensis Xue sp. nov." figureDoi="http://doi.org/10.5281/zenodo.4358940" httpUri="https://zenodo.org/record/4358940/files/figure.png" pageId="1" pageNumber="560">33</figureCitation>
