<subSubSection id="1AD06528C73D0D22FF14F828FDF1F875" pageId="21" pageNumber="222" type="distribution">
<paragraph id="527536A3C73D0D22FF14F828FDF1F875" blockId="21.[151,1436,1979,2042]" pageId="21" pageNumber="222">
<emphasis id="60BEEAB1C73D0D22FF14F828FE85F85A" bold="true" box="[151,297,1979,2005]" pageId="21" pageNumber="222">Distribution</emphasis>
. 
<collectingCountry id="2ADD7633C73D0D22FEB5F828FEDEF85A" box="[310,370,1979,2005]" name="United States of America" pageId="21" pageNumber="222">USA</collectingCountry>
; widespread in eastern 
<collectingCountry id="2ADD7633C73D0D22FDF1F828FD04F85A" box="[626,680,1979,2005]" name="United States of America" pageId="21" pageNumber="222">USA</collectingCountry>
from 
<collectingRegion id="900EF841C73D0D22FD68F828FC90F85A" box="[747,828,1979,2005]" country="United States of America" name="Florida" pageId="21" pageNumber="222">Florida</collectingRegion>
to 
<collectingRegion id="900EF841C73D0D22FCDEF828FC5EF85A" box="[861,1010,1979,2005]" country="United States of America" name="Pennsylvania" pageId="21" pageNumber="222">Pennsylvania</collectingRegion>
and west to 
<collectingRegion id="900EF841C73D0D22FBFFF828FB4FF85A" box="[1148,1251,1979,2005]" country="United States of America" name="Missouri" pageId="21" pageNumber="222">Missouri</collectingRegion>
; in western 
<collectingCountry id="2ADD7633C73D0D22FAE5F828FA30F85A" box="[1382,1436,1979,2005]" name="United States of America" pageId="21" pageNumber="222">USA</collectingCountry>
