<paragraph id="8BCD0962FFA6A84CFF09FCF91DE23E0E" blockId="23.[144,948,778,841]" box="[144,948,817,841]" pageId="23" pageNumber="1096">
<emphasis id="B906D570FFA6A84CFF09FCF91EB33E0F" box="[144,229,817,841]" italics="true" pageId="23" pageNumber="1096">Length.</emphasis>
<quantity id="4C8AA487FFA6A84CFF77FCF91F203E0E" box="[238,374,817,841]" metricMagnitude="-3" metricUnit="m" metricValue="9.45" metricValueMax="9.5" metricValueMin="9.4" pageId="23" pageNumber="1096" unit="mm" value="9.45" valueMax="9.5" valueMin="9.4">9.4–9.5 mm</quantity>
, length of elytra: 
<quantity id="4C8AA487FFA6A84CFDD8FCF91C9C3E0E" box="[577,714,817,841]" metricMagnitude="-3" metricUnit="m" metricValue="6.949999999999999" metricValueMax="7.1" metricValueMin="6.8" pageId="23" pageNumber="1096" unit="mm" value="6.949999999999999" valueMax="7.1" valueMin="6.8">6.8–7.1 mm</quantity>
