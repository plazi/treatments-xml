</taxonomicName>
, 
<taxonomicName id="89FBF496F344FFDCFF62FC9AFD6FFCAA" authority="Linnaeus (1753: 120)" authorityName="Linnaeus" authorityPageNumber="120" authorityYear="1753" box="[203,696,806,833]" class="Magnoliopsida" family="Lythraceae" genus="Ammannia" kingdom="Plantae" order="Myrtales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="species" species="baccifera">
