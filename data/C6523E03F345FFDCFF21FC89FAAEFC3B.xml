</taxonomicName>
, 
<taxonomicName id="89FBF496F344FFDCFD6FFC9BFB7BFCAB" authority="Linnaeus (1753: 463 - 464)" authorityName="Linnaeus" authorityPageNumber="463 - 464" authorityYear="1753" box="[710,1196,806,833]" class="Magnoliopsida" family="Molluginaceae" genus="Glinus" kingdom="Plantae" order="Caryophyllales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="species" species="lotoides">
