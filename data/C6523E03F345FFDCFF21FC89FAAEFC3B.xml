(
<bibRefCitation id="2A6AF2E4F344FFDCFBB6FCF7FB25FC8F" author="Linnaeus, C." box="[1055,1266,842,869]" pageId="5" pageNumber="26" pagination="560 - 1200" refId="ref3396" refString="Linnaeus, C. (1753) Species Plantarum I. Imprensis Laurentii Salvii, pp. 560 - 1200." type="book chapter" year="1753">Linnaeus 1753: 58</bibRefCitation>
) 
