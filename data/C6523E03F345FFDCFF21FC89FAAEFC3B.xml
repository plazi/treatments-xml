<taxonomicName id="89FBF496F344FFDCFF4DFCD2FD46FC63" authority="Linnaeus (1753: 226)" authorityName="Linnaeus" authorityPageNumber="226" authorityYear="1753" box="[228,657,878,905]" class="Magnoliopsida" family="Polygonaceae" genus="Rumex" kingdom="Plantae" order="Caryophyllales" pageId="5" pageNumber="26" phylum="Tracheophyta" rank="species" species="dentatus">
