<paragraph id="3735368BD129FFC0FD66AB154B92D5B0" blockId="122.[710,1220,1394,1447]" box="[710,1220,1394,1417]" pageId="122" pageNumber="123">
<heading id="6C7D81E7D129FFC0FD66AB154B92D5B0" box="[710,1220,1394,1417]" centered="true" fontSize="9" level="2" pageId="122" pageNumber="123" reason="2">
<taxonomicName id="F08A4D08D129FFC0FD66AB154CCED5B1" box="[710,920,1394,1417]" class="Reptilia" family="Squamata (awaiting allocation)" genus="Aiolosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="122" pageNumber="123" phylum="Chordata" rank="species" species="oriens">
<emphasis id="05FEEA99D129FFC0FD66AB154CCED5B1" box="[710,920,1394,1417]" italics="true" pageId="122" pageNumber="123">Aiolosaurus oriens</emphasis>
</taxonomicName>
+ 
<taxonomicName id="F08A4D08D129FFC0FC19AB154B92D5B0" authorityName="Borsuk-Bialynicka" authorityYear="1984" box="[953,1220,1394,1417]" class="Reptilia" family="Varanidae" genus="Cherminotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="122" pageNumber="123" phylum="Chordata" rank="species" species="longifrons">
<emphasis id="05FEEA99D129FFC0FC19AB154B92D5B0" box="[953,1220,1394,1417]" italics="true" pageId="122" pageNumber="123">Cherminotus longifrons</emphasis>
</taxonomicName>
</heading>
</paragraph>
<paragraph id="3735368BD129FFC0FCE8ABF64B17D59E" blockId="122.[710,1220,1394,1447]" box="[840,1089,1425,1447]" pageId="122" pageNumber="123">
