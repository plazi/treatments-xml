<taxonomicName id="4C014D32FFD37D522B24F854672FF83B" baseAuthorityName="Deichmann" baseAuthorityYear="1936" box="[306,532,1946,1969]" class="Anthozoa" family="Alcyoniidae" genus="Bellonella" kingdom="Animalia" order="Alcyonacea" pageId="29" pageNumber="30" phylum="Cnidaria" rank="species" species="rubistella">
