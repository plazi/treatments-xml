</taxonomicName>
(
<taxonomicName id="95964D0E9C130020CBFFDD29FA55FEA2" box="[1265,1429,296,320]" class="Elasmobranchii" kingdom="Animalia" pageId="5" pageNumber="122" phylum="Chordata" rank="class">Elasmobranchii</taxonomicName>
