<quantity id="5B719B78137B5401FC2FFB007587E5E0" box="[907,993,1154,1179]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="10" pageNumber="11" unit="mm" value="1.5">1.5 mm</quantity>
