Male 
<quantity id="5B719B78137B5401FE30FB00778FE5E0" box="[404,489,1154,1179]" metricMagnitude="-3" metricUnit="m" metricValue="5.8" pageId="10" pageNumber="11" unit="mm" value="5.8">5.8 mm</quantity>
long, 
<quantity id="5B719B78137B5401FD8BFB0074F5E5E0" box="[559,659,1154,1179]" metricMagnitude="-3" metricUnit="m" metricValue="1.35" pageId="10" pageNumber="11" unit="mm" value="1.35">1.35 mm</quantity>
