runs to couplet 
<number id="231824F9FF9A670DFF70F9AFFF42AB73" box="[151,195,1556,1580]" pageId="1" pageNumber="59" value="106.0">106</number>
, subfamily 
<taxonomicName id="52D04D5DFF9A670DFEA0F9AFFE57AB73" box="[327,470,1556,1580]" class="Insecta" family="Eulophidae" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="59" phylum="Arthropoda" rank="subFamily" subFamily="Entedoninae">Entedoninae</taxonomicName>
