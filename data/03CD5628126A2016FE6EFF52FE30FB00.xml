<taxonomicName id="4C649CBD126A2016FC4FFBA9FEBEFB87" authority="Wittmer, 1988" authorityName="Wittmer" authorityYear="1988" class="Insecta" family="Cantharidae" genus="Micropodabrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="81" phylum="Arthropoda" rank="species" species="yunnanus">
