<treatmentCitationGroup id="F7E6E3633805C20161A1FB63ED8D1A82" box="[151,974,1218,1243]" pageId="3" pageNumber="523">
<taxonomicName id="10F6BFCE3805C20161A1FB63EC281A82" authority="Lacordaire, 1868: 125" authorityName="Lacordaire" authorityPageNumber="125" authorityYear="1868" box="[151,619,1218,1243]" class="Insecta" family="Cerambycidae" genus="Nothopleurus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="523" phylum="Arthropoda" rank="species" species="ebeninus">
<emphasis id="E582185F3805C20161A1FB63EFC21A83" box="[151,385,1218,1242]" italics="true" pageId="3" pageNumber="523">Nothopleurus ebeninus</emphasis>
<treatmentCitation id="5657E25C3805C20160BEFB63EC281A82" author="Lacordaire, J. T." box="[392,619,1218,1243]" page="125" pageId="3" pageNumber="523" year="1868">
<bibRefCitation id="B367B9BC3805C20160BEFB63EC7B1A82" author="Lacordaire, J. T." box="[392,568,1218,1243]" pageId="3" pageNumber="523" refId="ref6345" refString="Lacordaire, J. T. (1868) Histoire naturelle des insectes. Genera des Coleopteres ou expose methodique et critique de tous les genres proposes jusqu'ici dans cet ordre d'insectes. Vol. 8. Librairie Encyclopedique de Roret, Paris, 552 pp." type="book" year="1868">Lacordaire, 1868</bibRefCitation>
