<taxonomicName id="4C244D12FFD6FFBDFED138DACC80FB5C" authorityName="Poinar" authorityYear="2006" box="[333,409,1207,1231]" class="Insecta" family="Mantispidae" genus="Feroseta" kingdom="Animalia" order="Neuroptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="priscus">
