</subSubSection>
<subSubSection id="C33E651AFFD6FFBDFFC43909CDD4FA06" pageId="31" pageNumber="32" type="reference_group">
<paragraph id="8B9B3691FFD6FFBDFFC43909CF7FFAE9" blockId="31.[88,614,1380,1754]" box="[88,614,1380,1402]" pageId="31" pageNumber="32">
<treatmentCitationGroup id="AB3411BFFFD6FFBDFFC43909CF7FFAE9" box="[88,614,1380,1402]" pageId="31" pageNumber="32">
<taxonomicName id="4C244D12FFD6FFBDFFC43909CDC0FAE9" box="[88,217,1380,1402]" class="Insecta" family="Chrysopidae" kingdom="Animalia" order="Neuroptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="family">Chrysopidae</taxonomicName>
larva: 
<treatmentCitation id="0A851080FFD6FFBDFEBE3909CF7BFAE9" author="Grimaldi, D. &amp; M. S. Engel" box="[290,610,1380,1402]" page="352" pageId="31" pageNumber="32" year="2005">
<bibRefCitation id="EFB54B60FFD6FFBDFEBE3909CF7BFAE9" author="Grimaldi, D. &amp; M. S. Engel" box="[290,610,1380,1402]" pageId="31" pageNumber="32" refId="ref19602" refString="Grimaldi, D., and M. S. Engel. 2005. Evolution of the insects. Cambridge: Cambridge University Press, xv + 755 pp." type="book" year="2005">Grimaldi and Engel, 2005: 352</bibRefCitation>
