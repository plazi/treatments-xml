<taxonomicName id="B8EF4D5DFFB2FFB8E413FA3FFD7B8FC2" authorityName="Assing" authorityYear="2019" box="[571,660,1508,1528]" class="Insecta" family="Staphylinidae" genus="Quedius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="197" phylum="Arthropoda" rank="species" species="limans">
