<taxonomicName id="4C6CA1CF6729B551C40E495439022331" class="Magnoliopsida" family="Asteraceae" genus="Pulicaria" kingdom="Plantae" order="Asterales" pageId="380" pageNumber="381" phylum="Tracheophyta" rank="species" species="dysenterica">
