<taxonomicName id="FDE0848EFFB4B246F706FA4FFB88A096" authority="Risso, 1826" authorityName="Risso" authorityYear="1826" box="[847,1132,1411,1436]" class="Calcarea" family="Sycettidae" genus="Sycon" kingdom="Animalia" order="Leucosolenida" pageId="15" pageNumber="218" phylum="Porifera" rank="species" species="humboldtii">
