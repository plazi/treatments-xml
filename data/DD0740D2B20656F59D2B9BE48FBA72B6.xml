<taxonomicName id="E6B3FFFF4422478FBED940A0C2639BC5" class="Arachnida" family="Salticidae" genus="Vinnius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Vinnius calcarifer" order="Araneae" pageId="0" pageNumber="73" phylum="Arthropoda" rank="species" species="calcarifer">
