<collectionCode id="458AAE0BFF960427FB0E66928549D2E4" LSID="urn:lsid:biocol.org:col:34700" box="[1227,1290,953,979]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="11" pageNumber="58">CAS</collectionCode>
). 
<number id="955324E9FF960427FADA6692856DD2E4" box="[1311,1326,953,979]" pageId="11" pageNumber="58" value="4.0">4</number>
ɗ, 
<number id="955324E9FF960427FA9E6692853BD2E4" box="[1371,1400,953,979]" pageId="11" pageNumber="58" value="10.0">10</number>
