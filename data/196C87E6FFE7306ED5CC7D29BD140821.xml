(
<quantity id="563D9B15FFE6306ED1A27E67BC130951" box="[1273,1382,1123,1149]" metricMagnitude="-3" metricUnit="m" metricValue="1.66" pageId="13" pageNumber="514" unit="mm" value="1.66">1.66 mm</quantity>
), PL 
<quantity id="563D9B15FFE6306ED5CC7E83B84F098D" box="[151,314,1159,1185]" metricMagnitude="-4" metricUnit="m" metricValue="3.65" metricValueMax="4.0" metricValueMin="3.3" pageId="13" pageNumber="514" unit="mm" value="0.365" valueMax="0.4" valueMin="0.33">0.33–0.40 mm</quantity>
(
<quantity id="563D9B15FFE6306ED4137E83B8C4098D" box="[328,433,1159,1185]" metricMagnitude="-4" metricUnit="m" metricValue="3.7" pageId="13" pageNumber="514" unit="mm" value="0.37">0.37 mm</quantity>
), EW 
