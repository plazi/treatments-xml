(
<quantity id="563D9B15FFE6306ED6C97E67BA8A0951" box="[914,1023,1123,1149]" metricMagnitude="-3" metricUnit="m" metricValue="1.97" pageId="13" pageNumber="514" unit="mm" value="1.97">1.97 mm</quantity>
), EL 
