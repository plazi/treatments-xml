<subSubSection id="CD444C98AF255AB0038946DB0DB14079" pageId="18" pageNumber="19" type="reference_group">
<paragraph id="980EA737CAE45E42370DF9B25105D3C3" pageId="18" pageNumber="19">
<taxonomicName id="E616735537DB4EF978E2E8A7AAADDB5E" authority="Schmidt, 1862" authorityName="Schmidt" authorityYear="1862" class="Demospongiae" family="Dysideidae" genus="Spongelia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Spongelia spinifera" order="Dictyoceratida" pageId="18" pageNumber="19" phylum="Porifera" rank="species" species="spinifera">Spongelia spinifera</taxonomicName>
