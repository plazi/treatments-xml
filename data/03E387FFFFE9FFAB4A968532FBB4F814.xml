<taxonomicName id="4C4A4D6AFFE9FFA548A8858EFCF2F812" box="[681,784,1978,2001]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="genus">
