<taxonomicName id="4C4A4D6AFFE9FFA54E878546FAE3F84A" box="[1158,1281,1906,1929]" class="Aves" family="Thraupidae" genus="Hemispingus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="goeringi">
