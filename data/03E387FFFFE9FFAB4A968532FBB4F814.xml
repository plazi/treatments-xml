<taxonomicName id="4C4A4D6AFFE9FFA54922858EFB4CF811" authority="Bonaparte, 1850" authorityName="Bonaparte" authorityYear="1850" box="[803,1198,1977,2002]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="hispaniolensis">
<emphasis id="B93EEAFBFFE9FFA54922858EFC01F811" box="[803,995,1977,2002]" italics="true" pageId="7" pageNumber="336">P. hispaniolensis</emphasis>
Bonaparte, 1850
</taxonomicName>
, 
<taxonomicName id="4C4A4D6AFFE9FFA54EBF858EFF33F835" authority="Salvin, 1895" authorityName="Salvin" authorityYear="1895" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="rubecula">
<emphasis id="B93EEAFBFFE9FFA54EBF858EFAA0F811" box="[1214,1346,1977,2002]" italics="true" pageId="7" pageNumber="336">P. rubecula</emphasis>
Salvin, 1895
</taxonomicName>
, 
<taxonomicName id="4C4A4D6AFFE9FFA54ADC85EAFE80F835" box="[221,354,2013,2038]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="nigrorufa">
<emphasis id="B93EEAFBFFE9FFA54ADC85EAFE80F835" box="[221,354,2013,2038]" italics="true" pageId="7" pageNumber="336">P. nigrorufa</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C4A4D6AFFE9FFA54B7085EAFD71F835" authority="Sharpe, 1888" authorityName="Sharpe" authorityYear="1888" box="[369,659,2013,2038]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="boliviana">
<emphasis id="B93EEAFBFFE9FFA54B7085EAFE14F835" box="[369,502,2013,2038]" italics="true" pageId="7" pageNumber="336">P. boliviana</emphasis>
Sharpe, 1888
</taxonomicName>
, and 
<taxonomicName id="4C4A4D6AFFE9FFA548CF85EAFBE4F835" authority="Landbeck, 1865" authorityName="Landbeck" authorityYear="1865" box="[718,1030,2013,2038]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="ornata">
<emphasis id="B93EEAFBFFE9FFA548CF85EAFCD7F836" box="[718,821,2014,2037]" italics="true" pageId="7" pageNumber="336">P. ornata</emphasis>
(Landbeck, 1865)
