<taxonomicName id="4C4A4D6AFFE9FFA54F0E8546FA87F849" box="[1295,1381,1905,1930]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="baeri">
