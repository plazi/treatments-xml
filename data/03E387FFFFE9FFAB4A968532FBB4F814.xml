<taxonomicName id="4C4A4D6AFFE9FFA54B028546FE39F849" box="[259,475,1905,1930]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="nigrorufa">
