<taxonomicName id="4C4A4D6AFFE9FFA549F285A2FBB8F86E" box="[1011,1114,1942,1965]" class="Aves" family="Emberizidae" genus="Poospiza" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="genus">
