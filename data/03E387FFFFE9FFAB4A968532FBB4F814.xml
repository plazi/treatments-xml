<taxonomicName id="4C4A4D6AFFE9FFA548DB851EFBD1F881" authority="Berlepsch, 1893" authorityName="Berlepsch" authorityYear="1893" box="[730,1075,1833,1858]" class="Aves" family="Thraupidae" genus="Compsospiza" higherTaxonomySource="CoL" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="genus">
