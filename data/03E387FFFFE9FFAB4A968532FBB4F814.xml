Berlepsch, 1893
</taxonomicName>
(type = 
<taxonomicName id="4C4A4D6AFFE9FFA54E9C851EFEB0F8A5" authority="Berlepsch, 1893" authorityName="Berlepsch" authorityYear="1893" class="Aves" family="Thraupidae" genus="Compsospiza" higherTaxonomySource="CoL" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="336" phylum="Chordata" rank="species" species="garleppi">
