<taxonomicName id="7F940ECFFFCE3323FE226D9A38A2FC3A" authorityName="Walker" authorityYear="1852" box="[494,659,922,944]" class="Insecta" family="Psychomyiidae" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="1614" phylum="Arthropoda" rank="family">Psychomyiidae</taxonomicName>
