<taxonomicName id="C33F7C2E0E20D1DDB4EB6CF1C40F168F" LSID="http://zoobank.org/F812CDE3-0E4F-4240-AC75-5771ACAD770A" class="Gastropoda" family="Diplommatinidae" genus="Diancta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diancta pulchella" order="Architaenioglossa" pageId="16" pageNumber="17" phylum="Mollusca" rank="species" species="pulchella">Diancta pulchella</taxonomicName>
