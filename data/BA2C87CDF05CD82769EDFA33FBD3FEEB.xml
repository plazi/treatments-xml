<collectingCountry id="4A92764BF05ED8276A7BFE99FC49FEAB" box="[821,924,270,292]" name="Thailand" pageId="71" pageNumber="454">Thailand</collectingCountry>
, 
<collectingCountry id="4A92764BF05ED8276AEEFE99FBD1FEAB" box="[928,1028,270,292]" name="Vietnam" pageId="71" pageNumber="454">Vietnam</collectingCountry>
