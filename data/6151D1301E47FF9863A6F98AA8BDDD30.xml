<number id="5F3072011E47FF9B66BFF8A4A8BAD80B" box="[1422,1435,1885,1908]" pageId="5" pageNumber="6" value="4.0">4</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep id="520C78831E47FF9B63A6F884A8BAD88B" pageId="5" pageNumber="6">
<paragraph id="E94760261E47FF9B63A6F884A8BDD8CB" blockId="5.[151,1436,1725,2036]" pageId="5" pageNumber="6">
<keyLead id="5209C3131E47FF9B63A6F884A8BDD8CB" pageId="5" pageNumber="6">
<number id="5F3072011E47FF9B63A6F884AD86D8EB" box="[151,167,1917,1940]" pageId="5" pageNumber="6" value="3.0">3</number>
