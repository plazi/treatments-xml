<quantity id="2E00CDC31E44FF986034FE61AE5BDED0" box="[773,890,408,431]" metricMagnitude="-2" metricUnit="m" metricValue="1.65" metricValueMax="1.9" metricValueMin="1.4" pageId="6" pageNumber="7" unit="mm" value="16.5" valueMax="19.0" valueMin="14.0">14–19 mm</quantity>
) species occurring in 
<collectingCountry id="91EF20B61E44FF986752FE61A9CEDED0" box="[1123,1263,408,431]" name="South Africa" pageId="6" pageNumber="7">South Africa</collectingCountry>
, 
<collectingCountry id="91EF20B61E44FF9867C8FE61A84ADED0" box="[1273,1387,408,431]" name="Swaziland" pageId="6" pageNumber="7">Swaziland</collectingCountry>
