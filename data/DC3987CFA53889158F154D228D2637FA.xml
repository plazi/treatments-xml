<subSubSection id="1C8A6552A53889158F4F4C988D9B375A" pageId="38" pageNumber="311" type="discussion">
<paragraph id="542F36D9A53889158F4F4C988D9B375A" blockId="38.[831,1437,1449,1601]" pageId="38" pageNumber="311">
<emphasis id="66E4EACBA53889158F4F4C988BAB34DA" bold="true" box="[831,935,1449,1473]" pageId="38" pageNumber="311">Remarks:</emphasis>
<taxonomicName id="93904D5AA53889158FDC4C9B8C3F34DA" authorityName="MORAWITZ" authorityYear="1874" box="[940,1075,1449,1473]" class="Insecta" family="Colletidae" genus="Hylaeus" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="311" phylum="Arthropoda" rank="species" species="scutellaris">
