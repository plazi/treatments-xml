<typeStatus id="317F2BF73E45FFB9074FFC3EB1CBCDAB" box="[113,161,1002,1025]" pageId="23" pageNumber="828">TYPE</typeStatus>
SPECIES. — 
<taxonomicName id="29C4EED63E45FFB9062EFC3EB371CA55" authority="Stimpson, 1851" authorityName="Stimpson" authorityYear="1851" box="[272,539,1001,1025]" class="Bivalvia" family="Lucinidae" genus="Lucina" kingdom="Animalia" order="Lucinoida" pageId="23" pageNumber="828" phylum="Mollusca" rank="species" species="filosa">
