<taxonomicName id="4C0EEA921E62FFB2FE8E4A0BFC5CFD5A" authority="Martinez and Zaldivar-Riveron" authorityName="Martinez and Zaldivar-Riveron" authorityYear="2008" box="[329,953,1345,1369]" class="Insecta" family="Braconidae" genus="Allorhogas" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="2697" phylum="Arthropoda" rank="species" species="joergenseni" status="sp. nov.">
