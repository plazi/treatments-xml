<taxonomicName id="A4D3AF996873FFE4FE246497FCE0F931" authority="DeLeon, 1967" authorityName="DeLeon" authorityYear="1967" box="[396,887,1753,1779]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromips" kingdom="Animalia" order="Mesostigmata" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="mangleae">
<emphasis id="51A708086873FFE4FE246497FD2DF930" box="[396,698,1753,1778]" italics="true" pageId="3" pageNumber="4">Typhlodromips mangleae</emphasis>
DeLeon, 
<number id="D51BC63D6873FFE4FC946497FCE0F931" box="[828,887,1753,1779]" pageId="3" pageNumber="4" value="1967.0">1967</number>
</taxonomicName>
