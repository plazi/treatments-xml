<taxonomicName id="ACB2DED9FFF0FFB1FCBFFB339BA3DEDB" box="[799,948,1187,1211]" class="Magnoliopsida" family="Onagraceae" kingdom="Plantae" order="Myrtales" pageId="29" pageNumber="28" phylum="Tracheophyta" rank="family">Onagraceae</taxonomicName>
