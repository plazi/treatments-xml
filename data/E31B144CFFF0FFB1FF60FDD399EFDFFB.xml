<taxonomicName id="ACB2DED9FFF0FFB1FB3CFB339D3FDEDB" box="[1180,1320,1187,1211]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="29" pageNumber="28" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
