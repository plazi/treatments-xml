<taxonomicName id="C9A5D8CD195EADDF32E5B0B2CCFA618E" authority="(Lour.) Spreng. However" authorityName="Spreng. However" baseAuthorityName="Lour." class="Magnoliopsida" family="Cucurbitaceae" genus="Momordica" kingdom="Plantae" order="Cucurbitales" phylum="Tracheophyta" rank="species" species="cochinchinensis">
