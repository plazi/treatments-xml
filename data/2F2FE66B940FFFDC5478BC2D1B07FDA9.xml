<taxonomicName id="60862CFE9400FFDE54F8BCA01AFBF95C" authority="Carpenter 1881" authorityName="Carpenter" authorityYear="1881" box="[460,944,1667,1692]" class="Crinoidea" family="Himerometridae" genus="Himerometra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="8" pageNumber="9" phylum="Echinodermata" rank="species" species="robustipinna">
