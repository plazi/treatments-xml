<taxonomicName id="60862CFE9402FFDC5594B8751803FDA9" authority="AH Clark, 1907" authorityName="AH Clark" authorityYear="1907" box="[160,328,593,617]" class="Crinoidea" family="Himerometridae" genus="Himerometra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="10" pageNumber="11" phylum="Echinodermata" rank="species" species="robustipinna">
<emphasis id="95F28B6F9402FFDC5594B8751803FDA9" box="[160,328,593,617]" italics="true" pageId="10" pageNumber="11">H. robustipinna</emphasis>
