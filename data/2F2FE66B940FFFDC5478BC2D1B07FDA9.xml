(
<bibRefCitation id="C3172A8C9400FFDE57CEBCA01AE2F95C" author="Carpenter P." box="[762,937,1668,1692]" journalOrPublisher="Notes from the Leyden Museum" pageId="8" pageNumber="9" pagination="173 - 217" part="3" refString="Carpenter P. 1881. Comatulae of the Leyden Museum. Notes from the Leyden Museum. 3: 173 - 217." title="Comatulae of the Leyden Museum" type="journal article" year="1881">Carpenter 1881</bibRefCitation>
)
</taxonomicName>
(
<taxonomicName id="60862CFE9400FFDE568BBCA01D32F95C" box="[959,1145,1668,1692]" class="Crinoidea" family="Himerometridae" kingdom="Animalia" order="Comatulida" pageId="8" pageNumber="9" phylum="Echinodermata" rank="family">Himerometridae</taxonomicName>
