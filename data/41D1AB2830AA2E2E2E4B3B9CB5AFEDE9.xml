<taxonomicName id="31E1A4E7D5B16F30BF50C666A7EECE7B" LSID="http://bioguid.osu.edu/osuc_concepts:7822" authority="Kozlov" class="Insecta" family="Platygastridae" genus="Anirama" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anirama" order="Hymenoptera" pageId="2" pageNumber="101" phylum="Arthropoda" rank="genus">Anirama Kozlov</taxonomicName>
