<paragraph id="F23636C4FF95FFD9FF330CD05ABB62B9" blockId="11.[151,1436,583,747]" pageId="11" pageNumber="141">
Related to 
<taxonomicName id="35894D47FF95FFD9FEB30CD05FAC6295" box="[279,438,686,711]" class="Insecta" family="Hymenopodidae" genus="Odontomantis" kingdom="Animalia" order="Mantodea" pageId="11" pageNumber="141" phylum="Arthropoda" rank="genus">
