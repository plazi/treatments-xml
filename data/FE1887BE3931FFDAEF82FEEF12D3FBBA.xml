<caption id="22CE66203931FFDAECA4FBD410D2FBE8" ID-DOI="http://doi.org/10.5281/zenodo.5743800" ID-Zenodo-Dep="5743800" httpUri="https://zenodo.org/record/5743800/files/figure.png" pageId="6" pageNumber="320" startId="6.[151,186,1082,1103]" targetBox="[151,755,257,1031]" targetPageId="6">
