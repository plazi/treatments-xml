Described from 
<specimenCount id="9D1C06950247FFEF26CE1ADDFDCE5A87" box="[454,534,948,974]" pageId="19" pageNumber="20" type="adult">
<number id="3DD2DF3B0247FFEF26CE1ADDFE0D5A87" box="[454,469,948,974]" pageId="19" pageNumber="20" value="3.0">3</number>
adult
</specimenCount>
