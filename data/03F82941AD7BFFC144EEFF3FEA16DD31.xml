<number id="3D998A70AD7BFFC04669FEFEEB11DE7E" box="[528,588,345,370]" pageId="54" pageNumber="55" value="1957.0">1957</number>
</date>
Cook (Coll. 
<number id="3D998A70AD7BFFC04693FEFEEA57DE7D" box="[746,778,345,369]" pageId="54" pageNumber="55" value="79.0">79</number>
); 
<typeStatus id="54EA26F5AD7BFFC0475BFEFDEADEDE7E" box="[802,899,346,370]" pageId="54" pageNumber="55">paratype</typeStatus>
