<quantity id="4CC363FDFFA36005FB9DF8F36B05D46E" box="[1050,1142,1853,1875]" metricMagnitude="-3" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="76" pageNumber="611" unit="mm" value="8.5" valueMax="9.0" valueMin="8.0">8–9 mm</quantity>
