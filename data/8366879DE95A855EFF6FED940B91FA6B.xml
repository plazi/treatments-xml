<number id="BD0724ACE95A855EFF6FEC4F0EAEFAC3" box="[151,164,1301,1322]" pageId="75" pageNumber="76" value="1.0">1</number>
. Dorsocentral setae 
<number id="BD0724ACE95A855EFE62EC4F0FACFAC3" box="[410,422,1301,1322]" pageId="75" pageNumber="76" value="6.0">6</number>
(
<number id="BD0724ACE95A855EFE4CEC4F0FCBFAC3" box="[436,449,1301,1322]" pageId="75" pageNumber="76" value="2.0">2</number>
+ 
<number id="BD0724ACE95A855EFE28EC4F0FD7FAC3" box="[464,477,1301,1322]" pageId="75" pageNumber="76" value="4.0">4</number>
