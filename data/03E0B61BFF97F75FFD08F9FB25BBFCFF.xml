<collectionCode id="ED589FC8FF96F75FFC99FEA9246DFEEC" box="[796,892,315,341]" pageId="13" pageNumber="14">MUSM</collectionCode>
), 
<collectingRegion id="498DC9EFFF96F75FFC15FEA924C0FEEC" box="[912,977,315,341]" country="Peru" name="Lima" pageId="13" pageNumber="14">Lima</collectingRegion>
, 
<collectingCountry id="F35E479DFF96F75FFC59FEAE2307FEEF" box="[988,1046,316,342]" name="Peru" pageId="13" pageNumber="14">Peru</collectingCountry>
