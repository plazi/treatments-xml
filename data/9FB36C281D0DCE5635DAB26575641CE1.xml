<taxonomicName id="0A87943E6F3089C46709B1B973D07C28" authority="" authorityName="" class="Insecta" family="Liviidae" genus="Pyraloidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pyraloidea" order="Hemiptera" pageId="7" pageNumber="202" phylum="Arthropoda" rank="superfamily" superfamily="Pyraloidea">Pyraloidea</taxonomicName>
