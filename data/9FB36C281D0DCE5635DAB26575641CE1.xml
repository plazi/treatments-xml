<taxonomicName id="8F3C64F9B4FA3DBBF344C8F5085C4E03" class="Insecta" family="Crambidae" genus="Glaucocharis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glaucocharis reniell" order="Lepidoptera" pageId="7" pageNumber="202" phylum="Arthropoda" rank="species" species="reniell">Glaucocharis reniell</taxonomicName>
