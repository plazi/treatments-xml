<taxonomicName id="23E326948BC1ED43F9B6B315652A9B1C" class="Insecta" family="Crambidae" genus="Scirpophaga" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scirpophaga incertulas" order="Lepidoptera" pageId="7" pageNumber="202" phylum="Arthropoda" rank="species" species="incertulas">Scirpophaga incertulas</taxonomicName>
