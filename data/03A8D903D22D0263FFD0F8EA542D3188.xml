<taxonomicName id="4C011396D22D0263FDD6F89A53D9349B" box="[639,731,1902,1925]" class="Bacilli" family="Lactobacillaceae" genus="Latilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="34" pageNumber="2816" phylum="Firmicutes" rank="species" species="graminis">
