<taxonomicName id="4C011396D22D0263FCDDFE47553732D4" box="[884,1077,435,458]" class="Polycystina" family="Corythoecidae" genus="Cornum" higherTaxonomySource="GBIF" kingdom="Chromista" order="Albaillellaria" pageId="34" pageNumber="2816" phylum="Radiozoa" rank="species" species="aspersum">
