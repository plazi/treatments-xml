<taxonomicName id="4C0D4D6C1E4AFF818DD6098EFDB0FE85" box="[483,596,1179,1201]" class="Insecta" family="Chorotypidae" genus="Hydrolutos" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="438" phylum="Arthropoda" rank="genus">Hydrolutos</taxonomicName>
