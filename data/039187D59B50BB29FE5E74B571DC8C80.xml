<heading id="D0CF81AF9B50BB29FE5E74B577A58920" box="[458,767,248,272]" fontSize="10" level="7" pageId="126" pageNumber="129" reason="1">
<taxonomicName id="4C384D409B50BB29FE5E74B577178920" box="[458,589,248,272]" class="Actinopterygii" family="Cobitidae" genus="Cobitis" kingdom="Animalia" order="Cypriniformes" pageId="126" pageNumber="129" phylum="Chordata" rank="genus">
<emphasis id="B94CEAD19B50BB29FE5E74B577468920" box="[458,540,248,272]" italics="true" pageId="126" pageNumber="129">Cobitis</emphasis>
spp
</taxonomicName>
