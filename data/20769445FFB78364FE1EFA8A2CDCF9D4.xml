<taxonomicName id="6FDF5ED0FFB78364FED9F9AA2F25F9B4" authorityName="Bily" authorityYear="2000" box="[358,595,1544,1568]" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="78" phylum="Arthropoda" rank="species" species="aurohumeralis" subGenus="Haplanthaxia">
<emphasis id="9AABF941FFB78364FED9F9AA2C00F98B" box="[358,374,1544,1567]" italics="true" pageId="1" pageNumber="78">A</emphasis>
