<collectionCode id="ED2CAD85FFBAFFF8233D56D7D8B518DD" LSID="urn:lsid:biocol.org:col:34697" box="[1347,1432,158,184]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="5" pageNumber="6">BPBM</collectionCode>
