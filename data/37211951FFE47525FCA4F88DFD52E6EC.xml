<specimenCode id="EF2E003CFFE57525FEC8FCCDFE3FE639" box="[306,439,791,815]" collectionCode="G" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="21" pageNumber="108" type="Herbarium">G00659788</specimenCode>
