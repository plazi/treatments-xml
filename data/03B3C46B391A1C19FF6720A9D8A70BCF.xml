<number id="3DD2675A391A1C1AFEAB2791D8510FFC" box="[347,357,1493,1519]" pageId="21" pageNumber="22" value="1.0">1</number>
Rs cell 
<number id="3DD2675A391A1C1AFE352791D8D80FFC" box="[453,492,1493,1519]" pageId="21" pageNumber="22" value="2.6">2.6</number>
