(
<bibRefCitation id="813A4B28FFEAFFB7C8C5FDD8512CFD5F" author="Watson RB" box="[482,620,630,649]" pageId="89" pageNumber="867" pagination="1 - 756" refId="ref41839" refString="Watson RB. 1886. Report on the Scahopoda and Gasteropoda collected by H. M. S. Challenger during the years 1873 - 1876. Reports of the scientific results of the voyage of H. M. S. '' Challenger' '. Zoology 15: 1 - 756." type="journal article" year="1886">Watson, 1886</bibRefCitation>
)
</taxonomicName>
. (A) Shell from Ampère bank, 
<taxonomicName id="22AB4D5AFFEAFFB7CAB8FDD8575AFD5F" authority="Hensen" authorityName="Hensen" box="[927,1050,630,649]" class="Actinopterygii" family="Gerreidae" genus="Victor" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="89" pageNumber="867" phylum="Chordata" rank="genus">
