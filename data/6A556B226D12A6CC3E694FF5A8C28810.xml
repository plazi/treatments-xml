<taxonomicName id="383F57282B958B77DD7DAD0CA59F1F8E" class="Insecta" family="Muscidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Diptera" pageId="18" pageNumber="71" phylum="Arthropoda" rank="family">Muscidae</taxonomicName>
