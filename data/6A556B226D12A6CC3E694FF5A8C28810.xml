<taxonomicName id="0574489E0C40BB6836ED6A79A6E2AED4" class="Magnoliopsida" family="Malvaceae" genus="Diptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Malvales" pageId="18" pageNumber="71" phylum="Tracheophyta" rank="genus">Diptera</taxonomicName>
