<taxonomicName id="A8A4F2FFE39A4BAF6613BB09D333589B" class="Hexapoda" family="Sarcophagidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="18" pageNumber="71" phylum="Arthropoda" rank="family">Sarcophagidae</taxonomicName>
, 
<taxonomicName id="003DE85CE96A32452D341F2C79E2031E" class="Insecta" family="Syrphidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Diptera" pageId="18" pageNumber="71" phylum="Arthropoda" rank="family">Syrphidae</taxonomicName>
, 
<taxonomicName id="8CB3AE6845F50481917FDF2807B68202" class="Hexapoda" family="Calliphoridae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="18" pageNumber="71" phylum="Arthropoda" rank="family">Calliphoridae</taxonomicName>
