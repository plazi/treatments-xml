species: 
<taxonomicName id="56E04D245D5FFD47D89A95488705FAD9" authority="Amaoka, 1969" authorityName="Amaoka" authorityYear="1969" box="[646,1037,1283,1308]" class="Actinopterygii" family="Bothidae" genus="Japonolaeops" kingdom="Animalia" order="Pleuronectiformes" pageId="35" pageNumber="190" phylum="Chordata" rank="species" species="dentatus">
