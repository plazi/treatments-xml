<paragraph id="8BAE36ABFFED94796BEE8F9FFCE9F862" blockId="12.[151,1436,1968,2034]" box="[151,866,1968,1995]" pageId="12" pageNumber="13">
<typeStatus id="54AA8809FFED94796BEE8F9FFE88F862" box="[151,259,1969,1995]" pageId="12" pageNumber="13">Syntypes</typeStatus>
‘Ƥ(Ƥ)’ = 
<number id="3DD9248CFFED94796A058F9EFE19F863" box="[380,402,1968,1994]" pageId="12" pageNumber="13" value="3.0">3</number>
(
<number id="3DD9248CFFED94796AE58F9EFE39F863" box="[412,434,1968,1994]" pageId="12" pageNumber="13" value="3.0">3</number>
); 
<number id="3DD9248CFFED94796AB28F9EFE6AF863" box="[459,481,1968,1994]" pageId="12" pageNumber="13" value="3.0">3</number>
(
<number id="3DD9248CFFED94796A938F9EFD8BF863" box="[490,512,1968,1994]" pageId="12" pageNumber="13" value="3.0">3</number>
), 
<collectingCountry id="F306763BFFED947969608F9FFDE7F862" box="[537,620,1969,1995]" name="France" pageId="12" pageNumber="13">France</collectingCountry>
