<specimenCount id="9D4BFD44FF8DC026E213FD1B0C59E849" box="[317,334,765,783]" pageId="33" pageNumber="34" type="male">♂</specimenCount>
, 
<collectingCountry id="F35A765DFF8DC026E24EFD1D0CC7E853" box="[352,464,763,789]" name="Australia" pageId="33" pageNumber="34">Australia</collectingCountry>
: north 
<collectingRegion id="4989F82FFF8DC026E10BFD1D0FA5E853" box="[549,690,763,789]" country="Australia" name="Queensland" pageId="33" pageNumber="34">Queensland</collectingRegion>
, 
<collectingMunicipality id="6B96ACB7FF8DC026E1EEFD1D0E73E853" box="[704,868,763,789]" pageId="33" pageNumber="34">Burton Ridge</collectingMunicipality>
, 
