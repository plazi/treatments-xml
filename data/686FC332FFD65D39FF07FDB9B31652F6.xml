, 
<number id="560E6003FFD65D39FE5FF9A4B336533E" box="[463,517,1536,1562]" pageId="1" pageNumber="64" value="1.2">1.20</number>
, 
<number id="560E6003FFD65D39FD81F9A4B374533E" box="[529,583,1536,1562]" pageId="1" pageNumber="64" value="0.75">0.75</number>
