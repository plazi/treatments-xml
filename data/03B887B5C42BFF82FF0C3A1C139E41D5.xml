<number id="3DD92484C42AFF82FF7F3D66162A4232" box="[228,273,296,320]" pageId="9" pageNumber="578" value="800.0">800</number>
; apical seta length 
<number id="3DD92484C42AFF82FE7D3D69153E4232" box="[486,517,295,320]" pageId="9" pageNumber="578" value="75.0">75</number>
