<taxonomicName id="C8E84D5B0275FFFCA03CFD5BFD2E523E" authority="Hübner, 1819" authorityName="Hübner" authorityYear="1819" box="[625,755,763,787]" class="Insecta" family="Nymphalidae" genus="Epinephele" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="343" phylum="Arthropoda" rank="species" species="imbrialis" status="n. comb.">
<emphasis id="3D9CEACA0275FFFCA03CFD5BFD2E523E" box="[625,755,763,787]" italics="true" pageId="12" pageNumber="343">S. imbrialis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="26AF57B10275FFFCA0B7FD5BFCBC523E" box="[762,865,763,787]" pageId="12" pageNumber="343">
