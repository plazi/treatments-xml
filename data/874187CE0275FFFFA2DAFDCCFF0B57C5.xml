<subSubSection id="47F265530275FFFCA28AFD5AFEF0521B" pageId="12" pageNumber="343" type="diagnosis">
<paragraph id="0F5736D80275FFFCA28AFD5AFEF0521B" blockId="12.[151,1435,726,822]" pageId="12" pageNumber="343">
