<number id="3DC62490FFC2FFABFA59FE78EAC1199A" box="[1424,1436,461,482]" pageId="6" pageNumber="7" value="4.0">4</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep id="30FA2E12FFC2FFABFF5EFE5FEAC11A2F" pageId="6" pageNumber="7">
<paragraph id="8BB136B7FFC2FFABFF5EFE5FEAC61A41" blockId="6.[151,1437,223,1015]" pageId="6" pageNumber="7">
<keyLead id="30FF9582FFC2FFABFF5EFE5FEAC61A41" pageId="6" pageNumber="7">
<number id="3DC62490FFC2FFABFF5EFE5FEFFB1987" box="[151,166,490,511]" pageId="6" pageNumber="7" value="4.0">4</number>
