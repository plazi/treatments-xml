<taxonomicName id="4C0E4D34FFC2FFABFB05FD90EA141A42" box="[1228,1353,549,570]" class="Insecta" family="Clusiidae" genus="Sobarocephala" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="megastylis" status="sp. nov.">
