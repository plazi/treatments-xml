<collectingCountry id="F3197627FFC2FFABFBCDFDC8EB2C1AEA" box="[1028,1137,637,658]" name="Philippines" pageId="6" pageNumber="7">Philippines</collectingCountry>
)..................................................................................................... 
<taxonomicName id="4C0E4D34FFC2FFABFBB6FD2FEAC11AD7" authority="Sasakawa, 1993" authorityName="Sasakawa" authorityYear="1993" box="[1151,1436,666,687]" class="Insecta" family="Clusiidae" genus="Sobarocephala" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="vockerothi">
