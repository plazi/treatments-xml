<paragraph id="8BB136B7FFC2FFABFF5EFE27EAC619BC" blockId="6.[151,1437,223,1015]" pageId="6" pageNumber="7">
<keyLead id="30FF9582FFC2FFABFF5EFE27EAC619BC" pageId="6" pageNumber="7">
<number id="3DC62490FFC2FFABFF5EFE27EFF819DF" box="[151,165,402,423]" pageId="6" pageNumber="7" value="3.0">3</number>
