<paragraph id="7CD7AB8C2D0EEB20712683F24A4D4929" pageId="2" pageNumber="23">
<taxonomicName id="9219653F14BF629ED3B43B00306A3D2A" LSID="http://zoobank.org/F6030468-65D6-48A2-A5B7-3B36477D9DB9" genus="Bambusimukaria" lsidName="Bambusimukaria" pageId="2" pageNumber="23" rank="genus">Bambusimukaria</taxonomicName>
