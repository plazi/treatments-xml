<taxonomicName id="03C10170D7E394CE24A0491ED0428A34" authorityName="Yang" authorityYear="1974" class="Insecta" family="Coniopterygidae" genus="Conwentzia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Conwentzia sinica" order="Neuroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="sinica">
<emphasis id="5A3E9B3F65B311F3E4730F349B1E6AC2" italics="true" pageId="0" pageNumber="1">Conwentzia sinica</emphasis>
