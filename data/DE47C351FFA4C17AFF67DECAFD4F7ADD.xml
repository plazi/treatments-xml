<taxonomicName id="91EE09C4FFDAC17AFE9BDECAFE1D7BE5" authorityName="Zahniser" authorityYear="2021" box="[363,491,151,177]" class="Insecta" family="Cicadellidae" genus="Faltala" kingdom="Animalia" order="Hemiptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="viscacha">
<emphasis id="649AAE55FFDAC17AFE9BDECAFE1D7BE5" box="[363,491,151,177]" italics="true" pageId="64" pageNumber="65">F. viscacha</emphasis>
</taxonomicName>
