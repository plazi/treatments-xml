<collectingCountry id="2EF932D7FFDAC17AFD58DECAFCE17BE5" box="[680,791,151,177]" name="Argentina" pageId="64" pageNumber="65">Argentina</collectingCountry>
(
<collectingRegion id="942ABCA5FFDAC17AFCD9DECAFC537BE5" box="[809,933,151,177]" country="Argentina" name="Entre Rios" pageId="64" pageNumber="65">Entre Ríos</collectingRegion>
&amp; 
