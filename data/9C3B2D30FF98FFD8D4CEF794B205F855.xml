<collectionCode id="728304E3FF98FFD8D616F7FEB4B4FEA2" LSID="urn:lsid:biocol.org:col:34991" box="[591,682,253,277]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="12" pageNumber="13">HNHM</collectionCode>
