<caption id="DF2407EE67444730D9C6FBC2FBB4C84D" ID-DOI="http://doi.org/10.5281/zenodo.4920303" ID-Zenodo-Dep="4920303" box="[151,1024,1100,1125]" httpUri="https://zenodo.org/record/4920303/files/figure.png" pageId="11" pageNumber="252" startId="11.[151,250,1101,1125]" targetBox="[300,1240,281,1077]" targetPageId="11">
<paragraph id="8BE4576667444730D9C6FBC2FBB4C84D" blockId="11.[151,1024,1100,1125]" box="[151,1024,1100,1125]" pageId="11" pageNumber="252">
