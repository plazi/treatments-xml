<heading id="D0841732FFD11812FF46F9C5FD6BA8EF" bold="true" box="[151,652,1639,1665]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<keyLead id="3082036BFFD11812FF46F9C5FD6BA8EF" box="[151,652,1639,1665]" pageId="19" pageNumber="20">
<keyStep id="3087B8FBFFD11812FF46F9C5FD6BA8EF" box="[151,652,1639,1665]" pageId="19" pageNumber="20">
<key id="FFE2F76FFFD11812FF46F9C5FD6BA8EF" box="[151,652,1639,1665]" pageId="19" pageNumber="20">
<emphasis id="B9077C4CFFD11812FF46F9C5FD6BA8EF" bold="true" box="[151,652,1639,1665]" pageId="19" pageNumber="20">
<number id="3DBBB279FFD11812FF46F9C5FF51A8EF" box="[151,182,1639,1665]" pageId="19" pageNumber="20" value="29.0">29</number>
) 
<taxonomicName id="4C73DBDDFFD11812FF14F9C5FD6BA8EF" authority="Pener, Broza et Ayal, 1971" authorityName="Pener, Broza et Ayal" authorityYear="1971" box="[197,652,1639,1665]" class="Insecta" family="Tettigoniidae" genus="Raggeana" kingdom="Animalia" order="Orthoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
<emphasis id="B9077C4CFFD11812FF14F9C5FED8A8EF" bold="true" box="[197,319,1639,1665]" italics="true" pageId="19" pageNumber="20">Raggeana</emphasis>
<bibRefCitation id="EFE2DDAFFFD11812FE97F9C5FD6BA8EF" author="Pener" box="[326,652,1639,1665]" pageId="19" pageNumber="20" refString="Pener, M. P., Broza, M. &amp; Ayal, Y. (1971) A new genus of Decticinae (Orthoptera: Tettigoniidae) from the Middle East. Journal of Entomology, 40, 179 - 183." type="journal article" year="1971">
Pener, Broza et Ayal, 
<number id="3DBBB279FFD11812FD83F9C5FD6BA8EF" box="[594,652,1639,1665]" pageId="19" pageNumber="20" value="1971.0">1971</number>
</bibRefCitation>
