<taxonomicName id="4C717EBEF233FF9FFC8BFC32CD1BFC34" authorityName="Martynov" authorityYear="1928" box="[870,1032,970,995]" class="Insecta" family="Cladochoristidae" genus="Kamopanorpa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="genus">
