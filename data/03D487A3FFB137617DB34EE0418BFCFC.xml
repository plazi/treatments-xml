(
<figureCitation id="13462A30FFB137667DBB4E9D43A8FA42" box="[159,243,1490,1516]" captionStart="FIGURES 1–12" captionStartId="4.[151,271,1941,1965]" captionTargetBox="[315,1248,197,1922]" captionTargetId="figure-90@4.[311,1253,183,1922]" captionTargetPageId="4" captionText="FIGURES 1–12. Rhynchortalia spp., dorsal habitus: 1. R. australis sp. n.; 2. R. viridis sp. n.; 3. R. wau sp. n.; 4. R. papuana sp. n.; 5. R. iongai sp. n.; 6. R. moresby sp. n.; 7. R. purpurea sp. n.; 8. R.similis sp. n.; 9. R. anupama sp. n.; 10. R. riedeli sp. n.; 11. R. insueta Crotch; 12. R. iba sp. n." figureDoi="http://doi.org/10.5281/zenodo.5309135" httpUri="https://zenodo.org/record/5309135/files/figure.png" pageId="11" pageNumber="36">Figs. 7</figureCitation>
, 
<figureCitation id="13462A30FFB137667DDB4E9D4244FA42" box="[255,287,1490,1516]" captionStart="FIGURES 18–29" captionStartId="7.[151,269,1353,1377]" captionTargetBox="[159,1436,200,1330]" captionTargetId="figure-295@7.[151,1436,197,1330]" captionTargetPageId="7" captionText="FIGURES 18–29. Male terminal abdominal ventrites: 18. R. australis; 19. R. viridis; 20. R. wau; 21. R. papuana; 22. R. iongai; 23. R. iba; 24. R. similis; 25. R. purpurea; 26. R. riedeli; 27. R. insueta; 28. R. anupama; 29. R. moresby." figureDoi="http://doi.org/10.5281/zenodo.5309139" httpUri="https://zenodo.org/record/5309139/files/figure.png" pageId="11" pageNumber="36">25</figureCitation>
, 
<figureCitation id="13462A30FFB137667C0F4E9D422DFA42" box="[299,374,1490,1516]" captionStart="FIGURES 30–56" captionStartId="10.[151,270,1845,1869]" captionTargetBox="[164,1416,189,1801]" captionTargetId="figure-138@10.[163,1424,179,1809]" captionTargetPageId="10" captionText="FIGURES 30–56. Male genitalia: 30–33. R. australis: 30. Tegmen, lateral view; 31. Tegmen, ventral view; 32. Penis; 33. Penis apex; 34–37. R. viridis: 34. Tegmen, lateral view; 35. Tegmen, ventral view; 36. Penis; 37. Penis apex; 38–41. R. wau: 38. Tegmen, lateral view; 39. Tegmen, ventral view; 40. Penis; 41. Penis apex; 42–45. R. papuana: 42. Tegmen, lateral view; 43. Tegmen, ventral view; 44. Penis; 45. Penis apex; 46–49. R. iongai: 46. Tegmen, lateral view; 47. Tegmen, ventral view; 48. Penis; 49. Penis apex; 50–52. R. similis: 50. Tegmen, lateral view; 51. Tegmen, ventral view; 52. Penis; 53–56. R. purpurea: 53. Tegmen, lateral view; 54. Tegmen, ventral view; 55. Penis; 56. Penis apex." figureDoi="http://doi.org/10.5281/zenodo.5309143" httpUri="https://zenodo.org/record/5309143/files/figure.png" pageId="11" pageNumber="36">53–56</figureCitation>
