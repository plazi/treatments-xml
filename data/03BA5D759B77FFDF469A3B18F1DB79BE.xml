<subSubSection id="C309BFE89B76FFDF46CA39CEF19C7AF5" pageId="85" pageNumber="86" type="materials_examined">
<paragraph id="8BACEC639B76FFDF46CA39CEF6847B95" blockId="85.[151,1436,151,644]" box="[199,1426,151,176]" pageId="85" pageNumber="86">
<emphasis id="B96730719B76FFDF46CA39CEF1117B8A" bold="true" box="[199,519,151,176]" inLineHeading="true" pageId="85" pageNumber="86" reason="1">Other specimens recorded.</emphasis>
