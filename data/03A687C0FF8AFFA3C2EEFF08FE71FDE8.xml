<quantity id="4CF79B33FF8AFFA3C0E9FEC4FD50FEDD" box="[656,710,285,305]" metricMagnitude="2" metricUnit="m" metricValue="4.45" pageId="101" pageNumber="256" unit="m" value="445.0">445m</quantity>
