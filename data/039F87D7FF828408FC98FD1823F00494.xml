<taxonomicName id="4C364D42FF82840EFB74FC3827700781" baseAuthorityName="Rubinoff &amp; Schmitz" baseAuthorityYear="2010" box="[1239,1404,986,1007]" class="Insecta" family="Cosmopterigidae" genus="Hyposmocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="42" phylum="Arthropoda" rank="genus">Hyposmocoma</taxonomicName>
