<caption id="DF6FAAE5FFEE3839FF49140C0F3EF8BD" httpUri="https://zenodo.org/record/569137/files/figure.png" pageId="3" targetBox="[161,1424,1266,1831]" targetPageId="3">
<paragraph id="8BAFFA6DFFEE3839FF49140C0F3EF8BD" blockId="3.[151,1436,1853,1911]" pageId="3">
<emphasis id="B964267FFFEE3839FF49140C0A59F899" bold="true" box="[151,276,1853,1875]" pageId="3">FIGURE 3.</emphasis>
<taxonomicName id="4C1081EEFFEE3839FEC2140C0AEAF89E" box="[284,423,1853,1876]" class="Malacostraca" family="Hippolytidae" genus="Hippolyte" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="ngi" status="spec. nov.">
