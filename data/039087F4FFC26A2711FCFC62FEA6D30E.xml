<quantity id="4CC19B07FFC26A211050FA1EFC26D586" box="[845,907,1485,1508]" metricMagnitude="-1" metricUnit="m" metricValue="5.0" pageId="3" pageNumber="112" unit="cm" value="50.0">50 cm</quantity>
