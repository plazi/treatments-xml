<caption id="DF7766372917FFDFFF03C2769EA1F804" httpUri="https://zenodo.org/record/262441/files/figure.png" pageId="46" pageNumber="47" targetBox="[151,1436,193,1862]" targetPageId="46">
<paragraph id="8BB736BF2917FFDFFF03C2769EA1F804" blockId="46.[151,1437,1883,2005]" pageId="46" pageNumber="47">
