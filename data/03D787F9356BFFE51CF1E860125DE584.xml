<taxonomicName id="4C7E4D6C356BFFE51E6BE92411A1E55D" authority="O.F.Müller, 1773" authorityName="Kozhov" authorityYear="1936" box="[525,664,1876,1901]" class="Gastropoda" family="Valvatidae" genus="Valvata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ectobranchia" pageId="8" pageNumber="53" phylum="Mollusca" rank="species" species="brevicula">
