<taxonomicName id="573CA18A2F276B3B1EC20E1AF2AE0C63" authorityName="Shimbori &amp; Shaw" authorityYear="2020" class="Insecta" family="Braconidae" genus="Aleiodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleiodes ovatus" order="Hymenoptera" pageId="0" pageNumber="41" phylum="Arthropoda" rank="species" species="ovatus">
<emphasis id="612891DE0B6F8AC11A7BABE8A6F51C4C" italics="true" pageId="0" pageNumber="41">Aleiodes ovatus</emphasis>
</taxonomicName>
sp. nov. 
<emphasis id="1FDFFB000DE9F13D929A7332CFFF418E" bold="true" pageId="0" pageNumber="41">73</emphasis>
lateral habitus 
<emphasis id="3F1A29879764F280B2F96F682E670D91" bold="true" pageId="0" pageNumber="41">74</emphasis>
