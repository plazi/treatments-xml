<emphasis id="B97FEACE7830FFAAFCE3FD069F64FD62" box="[775,848,644,669]" italics="true" pageId="37" pageNumber="38">
<taxonomicName id="4C0B4D5F7830FFAAFCE3FD069F7FFD62" box="[775,843,644,669]" class="Insecta" family="Carabidae" genus="Agra" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="dable">dable</taxonomicName>
