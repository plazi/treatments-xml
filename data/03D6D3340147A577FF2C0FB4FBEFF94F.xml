<taxonomicName id="4C7F19A10147A577FEFD0C42FDD5F989" box="[326,574,1601,1625]" class="Insecta" family="Staphylinidae" genus="Pseudolathra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="599" phylum="Arthropoda" rank="species" species="regularis">
