<taxonomicName id="4C7F19A10147A577FDA20C8BFD7FF94F" box="[537,660,1671,1695]" class="Insecta" family="Staphylinidae" genus="Pseudolathra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="599" phylum="Arthropoda" rank="species" species="regularis">
