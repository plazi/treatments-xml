<taxonomicName id="09BB4D2DFFA5FFAFFD2F1D03FCB86B21" authorityName="Alibert" authorityYear="1947" box="[686,855,1296,1318]" class="Insecta" family="Homotomidae" genus="Phytolyma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="302" phylum="Arthropoda" rank="species" species="fusca">
<emphasis id="FCCFEABCFFA5FFAFFD2F1D03FCB86B21" box="[686,855,1296,1318]" italics="true" pageId="3" pageNumber="302">Phytolyma fusca</emphasis>
