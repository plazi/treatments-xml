<heading id="D0AB81A4FFD2FFC5FF5E5A45FD466237" bold="true" box="[151,694,1796,1821]" fontSize="10" level="2" pageId="6" pageNumber="405" reason="2">
<taxonomicName id="4C5C4D4BFFD2FFC5FF5E5A45FD466237" authority="Denis et Schiffermuller" authorityName="Denis et Schiffermuller" box="[151,694,1796,1821]" class="Insecta" family="Geometridae" genus="Ecliptopera" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="405" phylum="Arthropoda" rank="species" species="silaceata">
