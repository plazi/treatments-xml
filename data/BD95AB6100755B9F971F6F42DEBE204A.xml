<geoCoordinate id="0CF35E1C6E78B4057CBEE7D0BEB9D11D" degrees="102" direction="east" minutes="01" orientation="longitude" precision="15" seconds="33" value="102.02583">102°01'33&quot;E</geoCoordinate>
