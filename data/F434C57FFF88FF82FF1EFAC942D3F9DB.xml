<taxonomicName id="BB9D0FEAFF88FF82FEE9FAC942C5FA4E" authority="PACE" box="[326,503,1400,1422]" class="Insecta" family="Staphylinidae" genus="Leptusa" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="432" phylum="Arthropoda" rank="subGenus" subGenus="Dysleptusa">
<emphasis id="4EE9A87BFF88FF82FEE9FAC9428AFA4D" bold="true" box="[326,440,1400,1422]" italics="true" pageId="5" pageNumber="432">Dysleptusa</emphasis>
<emphasis id="4EE9A87BFF88FF82FE11FAC942C5FA4E" bold="true" box="[446,503,1400,1421]" pageId="5" pageNumber="432">PACE</emphasis>
</taxonomicName>
