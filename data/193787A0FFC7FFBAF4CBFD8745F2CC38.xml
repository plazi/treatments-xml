<taxonomicName id="569E4D35FFC7FFBAF4CBFB4C451ACEB4" baseAuthorityName="Lamarck" baseAuthorityYear="1816" box="[151,382,1162,1187]" class="Asteroidea" family="Goniasteridae" genus="Fromia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Valvatida" pageId="37" pageNumber="38" phylum="Echinodermata" rank="species" species="milleporella">
