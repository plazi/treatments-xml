<quantity id="56669B53FFC7FFBAF69BFB4B4741CEB5" box="[711,805,1163,1187]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="37" pageNumber="38" unit="m" value="15.0" valueMax="30.0" valueMin="0.0">
