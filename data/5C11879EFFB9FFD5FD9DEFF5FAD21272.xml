<quantity id="13409B6DFFB9FFD5FBF4EB74FB101273" box="[1077,1239,1588,1615]" metricMagnitude="3" metricUnit="m" metricValue="1.75" metricValueMax="1.85" metricValueMin="1.65" pageId="15" pageNumber="16" unit="m" value="1750.0" valueMax="1850.0" valueMin="1650.0">1650–1850 m</quantity>
