<typeStatus id="54CD98F6FFB1FF9E439E5903FF56FC1A" box="[151,207,1875,1901]" pageId="16" pageNumber="312">Type</typeStatus>
locality: probably 
<collectingRegion id="49B2E8B6FFB1FF9E42AA5902FDDEFC1B" box="[419,583,1874,1900]" country="Brazil" name="Santa Catarina" pageId="16" pageNumber="312">Santa Catarina</collectingRegion>
(
