</taxonomicName>
from Wahoo, 
<collectingCountry box="[649,870,326,353]" name="Papua New Guinea" pageId="8" pageNumber="269">Papua New Guinea</collectingCountry>
; 
<taxonomicName authority="Haeckel, 1872" authorityName="Haeckel" authorityYear="1872" box="[883,1151,326,353]" class="Calcarea" family="Leucaltidae" genus="Ascandra" kingdom="Animalia" order="Clathrinida" pageId="8" pageNumber="269" phylum="Porifera" rank="species" species="densa">
