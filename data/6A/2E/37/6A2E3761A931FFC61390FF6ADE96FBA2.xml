<taxonomicName authorityName="Savigny" authorityYear="1816" box="[199,310,350,374]" class="Ascidiacea" family="Didemnidae" genus="Didemnum" kingdom="Animalia" order="Enterogona" pageId="3" pageNumber="404" phylum="Chordata" rank="genus">
