</taxonomicName>
and 
<taxonomicName authorityName="Huntsman" authorityYear="1912" box="[1165,1398,422,446]" class="Ascidiacea" family="Styelidae" genus="Metandrocarpa" kingdom="Animalia" order="Pleurogona" pageId="20" pageNumber="421" phylum="Chordata" rank="species" species="taylori">
