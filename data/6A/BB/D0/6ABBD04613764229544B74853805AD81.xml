<taxonomicName class="Actinopterygii" family="Sparidae" genus="Archosargus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Archosargus rhomboidalis" order="Perciformes" pageId="0" pageNumber="125" phylum="Chordata" rank="species" species="rhomboidalis">Archosargus rhomboidalis</taxonomicName>
