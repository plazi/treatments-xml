<subSubSection pageId="3" pageNumber="4" type="discussion">
<paragraph blockId="3.[151,1437,615,2027]" pageId="3" pageNumber="4">
<emphasis bold="true" box="[201,330,1933,1958]" pageId="3" pageNumber="4">Comments</emphasis>
