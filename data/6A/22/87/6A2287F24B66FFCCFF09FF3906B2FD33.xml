</subSubSection>
<subSubSection box="[199,633,553,578]" pageId="4" pageNumber="553" type="distribution">
<paragraph blockId="4.[151,1437,151,758]" box="[199,633,553,578]" pageId="4" pageNumber="553">
<emphasis bold="true" box="[199,345,553,578]" pageId="4" pageNumber="553">Distribution</emphasis>
. 
<emphasis bold="true" box="[359,460,553,578]" pageId="4" pageNumber="553">Oriental</emphasis>
: 
<collectingCountry box="[475,542,553,578]" name="China" pageId="4" pageNumber="553">China</collectingCountry>
