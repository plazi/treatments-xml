<subSubSection pageId="7" pageNumber="556" type="diagnosis">
<paragraph blockId="7.[151,1436,1964,2024]" pageId="7" pageNumber="556">
<emphasis bold="true" box="[151,266,1964,1989]" pageId="7" pageNumber="556">Diagnosis</emphasis>
