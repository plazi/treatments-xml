is distinguished from 
<taxonomicName baseAuthorityName="Frey" baseAuthorityYear="1972" box="[722,860,1298,1324]" class="Insecta" family="Melolonthidae" genus="Compsodactylus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="species" species="martinezi">
