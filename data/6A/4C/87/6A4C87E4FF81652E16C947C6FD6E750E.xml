<paragraph blockId="4.[151,1436,1298,1577]" pageId="4" pageNumber="391">
<taxonomicName authorityName="Fuhrmann &amp; Smith" authorityYear="2021" box="[199,477,1298,1324]" class="Insecta" family="Melolonthidae" genus="Compsodactylus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="species" species="mendax">
