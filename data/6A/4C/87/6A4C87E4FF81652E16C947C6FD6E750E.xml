and 
<taxonomicName authorityName="Figueroa &amp; Neita-Moreno" authorityYear="2019" box="[911,1026,1298,1324]" class="Insecta" family="Melolonthidae" genus="Compsodactylus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="species" species="vallejoi">
