<number box="[159,230,440,464]" pageId="17" pageNumber="405" value="30.0" valueMax="32.0" valueMin="28.0">28–32</number>
vs. 
<number box="[280,349,440,464]" pageId="17" pageNumber="405" value="32.0" valueMax="34.0" valueMin="30.0">30–34</number>
[
<taxonomicName box="[368,523,439,464]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="405" phylum="Chordata" rank="species" species="leucospilos">
<emphasis box="[368,523,439,464]" italics="true" pageId="17" pageNumber="405">P. leucospilos</emphasis>
</taxonomicName>
], 
<number box="[548,618,440,464]" pageId="17" pageNumber="405" value="32.5" valueMax="33.0" valueMin="32.0">32–33</number>
[
<taxonomicName box="[636,845,439,464]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="405" phylum="Chordata" rank="species" species="manananggalae">
