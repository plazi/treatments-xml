and 
<taxonomicName box="[584,793,404,429]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="405" phylum="Chordata" rank="species" species="manananggalae">
