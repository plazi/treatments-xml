<taxonomicName genus="Flacourtiaceae" lsidName="Flacourtiaceae" pageId="36" pageNumber="37" rank="genus">Flacourtiaceae</taxonomicName>
) (
<bibRefCitation author="Bryant, GE" journalOrPublisher="Proceedings of the Royal Entomological Society of London (series B)" pageId="49" pageNumber="50" title="Two injurious species of Phytophaga (Halticinae) from Ivory Coast. Bull. Entomol. Res. 35: 141 − 142." url="10.1017/S0007485300017363" year="1944 b">Bryant 1944b</bibRefCitation>
, as 
<taxonomicName class="Aves" family="Estrildidae" genus="Poephila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Poephila" order="Passeriformes" pageId="36" pageNumber="37" phylum="Chordata" rank="genus">Poephila</taxonomicName>
