<paragraph pageId="36" pageNumber="37">Ecology.</paragraph>
<paragraph pageId="36" pageNumber="37">
<taxonomicName authority="Baly, 1876" authorityName="Baly" authorityYear="1876" class="Insecta" family="Chrysomelidae" genus="Lypnea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lypnea flaveola" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="flaveola">Lypnea flaveola</taxonomicName>
