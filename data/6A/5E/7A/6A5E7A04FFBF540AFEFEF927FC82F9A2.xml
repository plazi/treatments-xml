<taxonomicName authorityName="Fabricius" authorityYear="1787" box="[209,365,1952,1975]" class="Insecta" family="Mantidae" genus="Mantis" kingdom="Animalia" order="Mantodea" pageId="8" pageNumber="65" phylum="Arthropoda" rank="species" species="perspicua">
