<taxonomicName box="[808,975,1348,1389]" class="Insecta" family="Caeciliusidae" genus="Caecilius" kingdom="Animalia" order="Psocodea" pageId="4" pageNumber="29" phylum="Arthropoda" rank="species" species="basalis">
