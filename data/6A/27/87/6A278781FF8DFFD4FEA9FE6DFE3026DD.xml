<paragraph blockId="8.[264,1323,1017,1654]" pageId="8" pageNumber="9">
Total length 
<quantity box="[467,579,1057,1083]" metricMagnitude="-3" metricUnit="m" metricValue="3.81" pageId="8" pageNumber="9" unit="mm" value="3.81">3.81 mm</quantity>
