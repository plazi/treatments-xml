<taxonomicName authority="Fairmaire, 1864" authorityName="Fairmaire" authorityYear="1864" class="Insecta" family="Cerambycidae" genus="Hoplosia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hoplosia" order="Coleoptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="genus">Hoplosia</taxonomicName>
