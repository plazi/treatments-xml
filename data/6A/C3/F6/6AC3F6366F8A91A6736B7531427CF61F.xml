<taxonomicName class="Insecta" family="Cerambycidae" genus="Acanthoderes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acanthoderes" order="Coleoptera" pageId="211" pageNumber="212" phylum="Arthropoda" rank="genus">Acanthoderes</taxonomicName>
