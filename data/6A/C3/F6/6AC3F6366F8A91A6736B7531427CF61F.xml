<taxonomicName class="Insecta" family="Cerambycidae" genus="Oplosia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oplosia" order="Coleoptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="genus">Oplosia</taxonomicName>
