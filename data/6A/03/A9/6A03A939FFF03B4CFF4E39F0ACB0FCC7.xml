<subSubSection pageId="7" pageNumber="430" type="etymology">
<paragraph blockId="7.[151,1436,151,827]" pageId="7" pageNumber="430">
<emphasis bold="true" box="[199,334,514,539]" pageId="7" pageNumber="430">Etymology.</emphasis>
<taxonomicName box="[342,642,515,539]" class="Insecta" family="Braconidae" genus="Neoclarkinella" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="430" phylum="Arthropoda" rank="species" species="narendrani">
