<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[1037,1118,1660,1684]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="178" pageNumber="179">USNM</collectionCode>
