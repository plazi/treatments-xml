<collectingCountry box="[347,418,1123,1146]" name="South Korea" pageId="24" pageNumber="25">Korea</collectingCountry>
, southern 
<collectingCountry box="[532,596,1123,1146]" name="Japan" pageId="24" pageNumber="25">Japan</collectingCountry>
