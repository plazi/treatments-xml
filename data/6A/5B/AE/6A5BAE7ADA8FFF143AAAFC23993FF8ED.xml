<quantity box="[1124,1225,1771,1795]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="152" pageNumber="153" unit="m" value="15.0" valueMax="20.0" valueMin="10.0">
