<emphasis bold="true" box="[308,542,1237,1261]" pageId="3" pageNumber="230">
cf. 
<taxonomicName authorityName="LINNAEUS" authorityYear="1758" box="[342,458,1237,1261]" class="Mammalia" family="Vespertilionidae" genus="Vespertilio" kingdom="Animalia" order="Chiroptera" pageId="3" pageNumber="230" phylum="Chordata" rank="genus">
