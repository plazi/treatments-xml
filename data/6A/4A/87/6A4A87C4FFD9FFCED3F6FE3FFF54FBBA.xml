<taxonomicName authority="Fabricius, 1794" authorityName="Fabricius" authorityYear="1794" box="[238,557,509,535]" class="Pycnogonida" family="Nymphonidae" genus="Nymphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pantopoda" pageId="5" pageNumber="190" phylum="Arthropoda" rank="genus">
