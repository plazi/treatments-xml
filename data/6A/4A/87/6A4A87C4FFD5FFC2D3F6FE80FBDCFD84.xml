<collectingRegion box="[252,415,465,490]" country="Brazil" name="Rio de Janeiro" pageId="9" pageNumber="194">Rio de Janeiro</collectingRegion>
(
<collectingCountry box="[430,501,465,490]" name="Brazil" pageId="9" pageNumber="194">Brazil</collectingCountry>
) and 
<collectingRegion box="[565,825,465,490]" country="Argentina" name="Buenos Aires" pageId="9" pageNumber="194">Buenos Aires Province</collectingRegion>
(
<collectingCountry box="[840,957,465,490]" name="Argentina" pageId="9" pageNumber="194">Argentina</collectingCountry>
