<taxonomicName id="8DD84D78FFCEBC54FCD22CC1FC21F84A" box="[826,968,1927,1949]" class="Aves" family="Muscicapidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="20" pageNumber="421" phylum="Chordata" rank="family">Muscicapidae</taxonomicName>
