<collectingCountry id="1CD352DFF51E3F7F4953EE50FC4AC00B" box="[819,942,312,355]" name="Sri Lanka" pageId="18" pageNumber="29">Ceylon</collectingCountry>
, 
<collectingCountry id="1CD352DFF51E3F7F49A8EE50FBE8C00B" box="[968,1036,312,355]" name="Fiji" pageId="18" pageNumber="29">Fiji</collectingCountry>
, the 
<collectingCountry id="1CD352DFF51E3F7F4E0DEE50FAD1C00B" box="[1133,1333,312,355]" name="Philippines" pageId="18" pageNumber="29">Philippines</collectingCountry>
, and 
