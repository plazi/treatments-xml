<quantity id="4CC0ACABFFAEAD017022AAD0FD6D5EC6" box="[623,691,434,454]" metricMagnitude="3" metricUnit="m" metricValue="3.9" pageId="2" pageNumber="2027" unit="m" value="3900.0">3900 m</quantity>
