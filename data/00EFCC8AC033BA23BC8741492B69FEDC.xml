<taxonomicName id="87F38FB84BD0AA22330243C541AC3875" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145356" authority="Andrei" authorityName="Andrei" class="Hexapoda" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sima hysterica Forel" order="Hymenoptera" pageNumber="258" phylum="Arthropoda" rank="species" species="hysterica">SIMA HYSTERICA</taxonomicName>
