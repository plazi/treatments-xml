<paragraph id="8BC936CEFFD87A19FF49BF04FE51F8E8" blockId="10.[151,401,1868,1894]" box="[151,401,1868,1894]" pageId="10" pageNumber="265">
<heading id="D08181A2FFD87A19FF49BF04FE51F8E8" bold="true" box="[151,401,1868,1894]" fontSize="11" level="1" pageId="10" pageNumber="265" reason="1">
<emphasis id="B902EADCFFD87A19FF49BF04FE51F8E8" bold="true" box="[151,401,1868,1894]" pageId="10" pageNumber="265">
<taxonomicName id="4C764D4DFFD87A19FF49BF04FEF8F8E8" authority="Leduc, 2016" authorityName="Leduc" authorityYear="2016" box="[151,312,1868,1894]" class="Chromadorea" family="Microlaimidae" genus="Maragnopsia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="10" pageNumber="265" phylum="Nematoda" rank="genus" status="n. gen.">
<emphasis id="B902EADCFFD87A19FF49BF04FEF8F8E8" bold="true" box="[151,312,1868,1894]" italics="true" pageId="10" pageNumber="265">Maragnopsia</emphasis>
