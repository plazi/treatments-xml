<subSubSection id="C325651C8D01FFBCFD4EFB00FB47FB7B" box="[677,1210,1227,1252]" pageId="1" pageNumber="784" type="materials_examined">
<paragraph id="8B8036978D01FFBCFD4EFB00FB47FB7B" blockId="1.[677,1210,1227,1252]" box="[677,1210,1227,1252]" pageId="1" pageNumber="784">
