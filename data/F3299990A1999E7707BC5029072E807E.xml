, 
<taxonomicName id="4108CACC6C58602477648107C1D24D97" class="Arachnida" family="Euscorpiidae" genus="Scorpiops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scorpiops" order="Scorpiones" pageId="15" pageNumber="66" phylum="Arthropoda" rank="genus">Scorpiops</taxonomicName>
sp (Lhasa) and 
<taxonomicName id="2C0F050CC9846A6D1D0C8225C0915EB4" class="Arachnida" family="Euscorpiidae" genus="Scorpiops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scorpiops tibetanus" order="Scorpiones" pageId="15" pageNumber="66" phylum="Arthropoda" rank="species" species="tibetanus">Scorpiops tibetanus</taxonomicName>
