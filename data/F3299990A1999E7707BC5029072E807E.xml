<paragraph id="2553A296448C3CC3AF5137B66359BCA0" pageId="15" pageNumber="66">
<taxonomicName id="3BFA907061D92CD5CC52BDC5663A3BB8" class="Arachnida" family="Euscorpiidae" genus="Scorpiops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scorpiops petersii" order="Scorpiones" pageId="15" pageNumber="66" phylum="Arthropoda" rank="species" species="petersii">Scorpiops petersii</taxonomicName>
