<taxonomicName id="1F947329FC5625018FD61A8657C26816" baseAuthorityName="Menke" baseAuthorityYear="1829" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="19" pageNumber="20" phylum="Mollusca" rank="species" species="flavidus" subGenus="Mesembrinus">
