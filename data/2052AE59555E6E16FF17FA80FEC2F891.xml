<treatmentCitationGroup id="88EB3861555E6E16FF17FA33FDEEFA36" box="[151,536,1436,1458]" pageId="9" pageNumber="60">
<taxonomicName id="6FFB64CC555E6E16FF17FA33FDEEFA36" authority="Faure, 1929: 14" authorityName="Faure" authorityPageNumber="14" authorityYear="1929" box="[151,536,1436,1458]" class="Insecta" family="Thripidae" genus="Scirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="9" pageNumber="60" phylum="Arthropoda" rank="species" species="zuluensis">
<emphasis id="9A8FC35D555E6E16FF17FA33FE84FA35" box="[151,370,1436,1458]" italics="true" pageId="9" pageNumber="60">Scirtothrips zuluensis</emphasis>
<treatmentCitation id="295A395E555E6E16FEF7FA32FDEEFA36" author="Faure" box="[375,536,1436,1458]" page="14" pageId="9" pageNumber="60" year="1929">
<bibRefCitation id="CC6A62BE555E6E16FEF7FA32FE02FA36" author="Faure" box="[375,500,1436,1458]" pageId="9" pageNumber="60" refString="Faure, J. C. (1929) The South African citrus thrips and five other new species of Scirtothrips Shull. Transvaal University College Bulletin, 18, 1 - 18." type="journal article" year="1929">
Faure, 
<number id="1E330D68555E6E16FE3EFA33FE02FA36" box="[446,500,1436,1457]" pageId="9" pageNumber="60" value="1929.0">1929</number>
</bibRefCitation>
