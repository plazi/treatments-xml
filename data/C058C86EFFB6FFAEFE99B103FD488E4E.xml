<number id="FE396B5FFFB7FFAEFB61B24BFAF18F86" box="[1264,1318,1644,1670]" pageId="5" pageNumber="44" value="0.26">0.26</number>
, 
<number id="FE396B5FFFB7FFAEFE99B2B3FEE98FAE" box="[264,318,1684,1710]" pageId="5" pageNumber="44" value="0.57">0.57</number>
