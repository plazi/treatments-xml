, alive at 
<quantity id="98099B14FFDE07411437FB95FAA2FB98" box="[113,224,1040,1064]" metricMagnitude="2" metricUnit="m" metricValue="6.5" metricValueMax="8.0" metricValueMin="5.0" pageId="3" pageNumber="336" unit="m" value="650.0" valueMax="800.0" valueMin="500.0">500-800 m</quantity>
