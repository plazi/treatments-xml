<paragraph id="7F5036DEFFBEFFB4E69AFBE8FD418E7D" blockId="4.[178,686,1047,1095]" box="[178,686,1074,1095]" pageId="4" pageNumber="193">
<treatmentCitationGroup id="5FFF11F0FFBEFFB4E69AFBE8FD418E7D" box="[178,686,1074,1095]" pageId="4" pageNumber="193">
<taxonomicName id="B8EF4D5DFFBEFFB4E69AFBE8FE778E7D" authority="REITTER, 1909" box="[178,408,1075,1095]" class="Insecta" family="Staphylinidae" genus="Quedius" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="193" phylum="Arthropoda" rank="genus">
