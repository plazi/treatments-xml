of 
<taxonomicName id="34B37486AB37FFE8FDF7FD97FD1BC447" authorityName="Behning &amp; Woltereck" authorityYear="1912" box="[601,694,559,584]" class="Malacostraca" family="Vibiliidae" genus="Vibilia" kingdom="Animalia" order="Amphipoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="chuni">
<emphasis id="C1C7D317AB37FFE8FDF7FD97FD1BC447" box="[601,694,559,584]" italics="true" pageId="58" pageNumber="59">V. chuni</emphasis>
</taxonomicName>
from 
<taxonomicName id="34B37486AB37FFE8FD50FD97FC32C446" authority="Stns." authorityName="Stns." box="[766,927,559,585]" class="Malacostraca" family="Trichodactylidae" genus="Valdivia" kingdom="Animalia" order="Decapoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
