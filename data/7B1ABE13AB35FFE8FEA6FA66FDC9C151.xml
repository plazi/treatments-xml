<typeStatus id="2C08B1A7AB37FFE8FB2CFDC7FB42C496" box="[1154,1263,639,665]" pageId="58" pageNumber="59">Holotype</typeStatus>
of 
<taxonomicName id="34B37486AB37FFE8FABBFDC7FEDEC4CF" authorityName="Stewart" authorityYear="1913" class="Malacostraca" family="Vibiliidae" genus="Vibilia" kingdom="Animalia" order="Amphipoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="hodgsoni">
<emphasis id="C1C7D317AB37FFE8FABBFDC7FEDEC4CF" italics="true" pageId="58" pageNumber="59">V. hodgsoni</emphasis>
