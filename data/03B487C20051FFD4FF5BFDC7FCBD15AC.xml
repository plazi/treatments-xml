<taxonomicName id="4C1D4D570051FFD4FCFCFD56FC1010D9" authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[816,958,691,717]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="angustum">
<emphasis id="B969EAC60051FFD4FCFCFD56FCEA10D8" box="[816,836,691,717]" italics="true" pageId="67" pageNumber="68">O</emphasis>
. 
<emphasis id="B969EAC60051FFD4FC9DFD56FC1010D9" box="[849,958,691,716]" italics="true" pageId="67" pageNumber="68">angustum</emphasis>
</taxonomicName>
