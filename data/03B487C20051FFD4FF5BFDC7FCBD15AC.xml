<quantity id="4CE59B310051FFD4FA84FD33FF681100" metricMagnitude="-3" metricUnit="m" metricValue="8.85" metricValueMax="9.5" metricValueMin="8.2" pageId="67" pageNumber="68" unit="mm" value="8.85" valueMax="9.5" valueMin="8.2">8.2–9.5 mm</quantity>
