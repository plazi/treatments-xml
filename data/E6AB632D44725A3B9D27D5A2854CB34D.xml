<taxonomicName id="B35A569CFEFFA4680A43AE467EFB48DE" authorityName="Born" authorityYear="1778" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Helix Polygyrata" order="Stylommatophora" pageId="0" pageNumber="91" phylum="Mollusca" rank="species" species="Polygyrata">
