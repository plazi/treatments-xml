<taxonomicName id="10BD3FF95E06FFC34616F8C9FC68C3B9" box="[861,966,1914,1935]" class="Pinopsida" family="Cupressaceae" genus="Cupressus" kingdom="Plantae" order="Pinales" pageId="4" pageNumber="179" phylum="Tracheophyta" rank="genus">
