<taxonomicName id="10BD3FF95E06FFC3466FF82AFC22C398" box="[804,908,1945,1966]" class="Pinopsida" family="Cupressaceae" genus="Cupressus" kingdom="Plantae" order="Pinales" pageId="4" pageNumber="179" phylum="Tracheophyta" rank="genus">
