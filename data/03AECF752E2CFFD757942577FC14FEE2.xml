<collectingRegion id="49C3B0812E2DFFD7550C22FEFD8EFF2A" box="[527,591,189,215]" country="New Zealand" name="West Coast" pageId="11" pageNumber="583">Costa</collectingRegion>
do 
<collectingRegion id="49C3B0812E2DFFD7557E22FEFD25FF2A" box="[637,740,189,215]" inferredBySuffix="true" pageId="11" pageNumber="583">Sol State</collectingRegion>
