<geoCoordinate id="EE3318A42E2CFFD6543E25E3FC04F846" box="[829,965,1952,1979]" degrees="22" direction="south" minutes="57" orientation="latitude" pageId="10" pageNumber="582" precision="15" seconds="50" value="-22.96389">22°57’50”S</geoCoordinate>
