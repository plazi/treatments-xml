<geoCoordinate id="EE3318A42E2DFFD7545122DAFC1AFF4E" box="[850,987,153,179]" degrees="22" direction="south" minutes="17" orientation="latitude" pageId="11" pageNumber="583" precision="15" seconds="39" value="-22.294167">22°17’39”S</geoCoordinate>
; 
<geoCoordinate id="EE3318A42E2DFFD754E522DBFBB8FF4E" box="[998,1145,152,179]" degrees="41" direction="west" minutes="40" orientation="longitude" pageId="11" pageNumber="583" precision="15" seconds="53" value="-41.68139">41°40’53”W</geoCoordinate>
