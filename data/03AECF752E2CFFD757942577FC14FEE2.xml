<geoCoordinate id="EE3318A42E2CFFD654D025E3FBA7F846" box="[979,1126,1952,1979]" degrees="42" direction="west" minutes="50" orientation="longitude" pageId="10" pageNumber="582" precision="15" seconds="44" value="-42.845554">42°50’44”W</geoCoordinate>
