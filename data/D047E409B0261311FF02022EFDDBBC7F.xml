<taxonomicName id="9FEE2E9CB0261311FECF0723FD5FBC5B" authority="Ferussac" authorityName="Ferussac" box="[346,677,1066,1091]" class="Gastropoda" family="Spiraxidae" genus="Euglandina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="10" pageNumber="281" phylum="Mollusca" rank="species" species="rosea">
