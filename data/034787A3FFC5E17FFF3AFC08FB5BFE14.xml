<subSubSection id="C3F4653EFFC5E17FFF6BF87DFEE9FF07" lastPageId="3" lastPageNumber="60" pageId="2" pageNumber="59" type="etymology">
<paragraph id="8B5136B5FFC5E17FFF6BF87DFEE9FF07" blockId="2.[151,1437,1066,2012]" lastBlockId="3.[151,1437,152,418]" lastPageId="3" lastPageNumber="60" pageId="2" pageNumber="59">
<emphasis id="B99AEAA7FFC5E17EFF6BF87DFEF0F86A" bold="true" box="[198,340,1986,2012]" pageId="2" pageNumber="59">Etymology.</emphasis>
The specific designation 
<taxonomicName id="4CEE4D36FFC5E17EFD2BF87CFD57F86A" box="[646,755,1987,2012]" class="Arachnida" family="Eriophyidae" genus="Aculops" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="59" phylum="Arthropoda" rank="species" species="baligouis">
