<quantity id="5A261FAD80B8FF1BEFB9C8331A7A084D" box="[722,807,1562,1586]" metricMagnitude="1" metricUnit="m" metricValue="1.6" metricValueMax="3.0" metricValueMin="0.2" pageId="188" pageNumber="189" unit="m" value="16.0" valueMax="30.0" valueMin="2.0">2–30 m</quantity>
