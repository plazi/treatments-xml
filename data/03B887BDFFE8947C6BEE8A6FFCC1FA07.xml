<collectingCountry id="F306763BFFE8947C6A488A9BFE0FFD66" box="[305,388,693,719]" name="France" pageId="9" pageNumber="10">France</collectingCountry>
, Mont Mézenc; 
<typeStatus id="54AA8809FFE8947C693C8A9BFD3EFD66" box="[581,693,693,719]" pageId="9" pageNumber="10">paratypes</typeStatus>
(=‘paraholotypes’) Ƥ; 
<number id="3DD9248CFFE8947C68BE8A9BFC5EFD66" box="[967,981,693,719]" pageId="9" pageNumber="10" value="2.0">2</number>
<number id="3DD9248CFFE8947C68A48A9AFC78FD67" box="[989,1011,692,718]" pageId="9" pageNumber="10" value="3.0">3</number>
, 
<collectingCountry id="F306763BFFE8947C6F608A9BFBE7FD66" box="[1049,1132,693,719]" name="France" pageId="9" pageNumber="10">France</collectingCountry>
, Haute–Loire, Chambon– sur–Lignon (all 
<collectionCode id="ED00AE6EFFE8947C6A2A8AF2FE39FD5F" LSID="urn:lsid:biocol.org:col:34988" box="[339,434,732,758]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="9" pageNumber="10">MNHN</collectionCode>
