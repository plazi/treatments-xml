Usinger
</taxonomicName>
, 
<taxonomicName id="4C6F6991FFE0FF91FB83CCC5FB2EF85D" authority="Usinger" authorityName="Usinger" authorityYear="1947" box="[1095,1276,1979,2002]" class="Insecta" family="Naucoridae" genus="Cryphocricos" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="66" phylum="Arthropoda" rank="species" species="latus">
<emphasis id="B91BCE00FFE0FF91FB83CCC5FB4EF85D" box="[1095,1180,1979,2002]" italics="true" pageId="0" pageNumber="66">C. latus</emphasis>
Usinger
</taxonomicName>
, 
<taxonomicName id="4C6F6991FFE0FF91FACCCCC5FF3FF87D" authority="Usinger" authorityName="Usinger" authorityYear="1947" class="Insecta" family="Naucoridae" genus="Cryphocricos" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="66" phylum="Arthropoda" rank="species" species="mexicanus">
<emphasis id="B91BCE00FFE0FF91FACCCCC5FA4EF85D" box="[1288,1436,1979,2002]" italics="true" pageId="0" pageNumber="66">C. mexicanus</emphasis>
Usinger
</taxonomicName>
, 
<taxonomicName id="4C6F6991FFE0FF91FF33CCA5FE25F87D" authority="De Carlo" authorityName="De Carlo" authorityYear="1940" box="[247,503,2011,2034]" class="Insecta" family="Naucoridae" genus="Cryphocricos" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="66" phylum="Arthropoda" rank="species" species="peruvianus">
