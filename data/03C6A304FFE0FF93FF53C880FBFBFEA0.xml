, or 
<taxonomicName id="4C6F6991FFE0FF91FDDBCCA5FD3DF87D" authority="De Carlo." authorityName="De Carlo." box="[543,751,2011,2034]" class="Insecta" family="Naucoridae" genus="Cryphocricos" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="66" phylum="Arthropoda" rank="species" species="vianai">
