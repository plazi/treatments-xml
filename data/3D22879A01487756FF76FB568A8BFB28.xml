<collectingRegion id="774FF86E014E7756FCF6FCFA8CBAFC9D" box="[791,935,836,861]" country="India" name="West Bengal" pageId="5" pageNumber="44">
<collectingRegion id="774FF86E014E7756FCF6FCFA8C53FC9C" box="[791,846,837,860]" country="Tanzania" name="Mjini Magharibi" pageId="5" pageNumber="44">West</collectingRegion>
