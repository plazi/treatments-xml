<taxonomicName id="728B4D0F014E7756FDC8FCFB8DB0FC9D" authorityName="Karmakar &amp; Bhowmik &amp; Sherpa" authorityYear="2017" box="[553,685,836,861]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="44" phylum="Arthropoda" rank="species" species="bengalensis">
