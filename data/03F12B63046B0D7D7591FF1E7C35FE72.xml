<location id="8E87CCAE046A0D7D7003FF1E7D37FF4A" LSID="urn:lsid:plazi:treatment:03F12B63046B0D7D7591FF1E7C35FE72:8E87CCAE046A0D7D7003FF1E7D37FF4A" box="[1285,1389,151,177]" country="Brazil" name="Formally" pageId="19" pageNumber="420" stateProvince="Minas Gerais">Formally</location>
