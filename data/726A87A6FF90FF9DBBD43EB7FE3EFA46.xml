<taxonomicName id="3DC34D33FF90FF9DB853390DFC39FAEA" authorityName="Schiner" authorityYear="1868" box="[784,910,1289,1315]" class="Insecta" family="Anisopodidae" genus="Lobogaster" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="249" phylum="Arthropoda" rank="species" species="philippii">
