<collectingRegion id="F943F82DAD05FFEFFE046643FD81FBD7" box="[453,620,1052,1075]" country="South Africa" name="KwaZulu-Natal" pageId="26" pageNumber="671">KwaZulu-Natal</collectingRegion>
