<taxonomicName id="9D8B4D6AFFB96D7BFDF9FD6B63E1FADD" authorityName="Schwendinger" authorityYear="1989" box="[566,655,1289,1310]" class="Arachnida" family="Pacullidae" genus="Perania" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="597" phylum="Arthropoda" rank="species" species="nasuta">
