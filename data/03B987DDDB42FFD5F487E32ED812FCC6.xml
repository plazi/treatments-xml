<taxonomicName id="4C104D48DB44FFD4F577E253D920F81A" authority="Bey-Bienko, 1969" authorityName="Bey-Bienko" authorityYear="1969" box="[359,848,1898,1920]" class="Insecta" family="Ectobiidae" genus="Hemithyrsocera" kingdom="Animalia" order="Blattodea" pageId="4" pageNumber="547" phylum="Arthropoda" rank="species" species="simulans">
