. 
<normalizedToken id="16B6244597D589CE41A8BCB0D1ECBD07" originalValue="A–D">A-D</normalizedToken>
, 
<normalizedToken id="5166151BEF81283DB4EC7A89BE477B8A" originalValue="I–J">I-J</normalizedToken>
