<materialsCitation id="37E25223FF80FF9AFE3D79C6D550F87C" ID-GBIF-Occurrence="3396082307" pageId="7" pageNumber="650">
<collectionCode id="E19BC0BBFF80FF9AFE3D79C6D5E9F827" box="[391,476,1947,1971]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="7" pageNumber="650">MNHN</collectionCode>
