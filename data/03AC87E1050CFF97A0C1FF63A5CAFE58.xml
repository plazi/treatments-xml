<taxonomicName id="4C054D74050CFF97A0C1FE67A646FE3C" authority="(Bruner, 1916)" authorityName="Bruner" authorityYear="1916" baseAuthorityName="Bruner" baseAuthorityYear="1916" box="[151,573,402,427]" class="Insecta" family="Lathiceridae" genus="Gryllotalpella" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="501" phylum="Arthropoda" rank="species" species="minor">
