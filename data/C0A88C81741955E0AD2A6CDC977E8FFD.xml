<taxonomicName id="0795547EE35E08095130BB394C81B045" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus" order="Fagales" pageId="2" pageNumber="63" phylum="Tracheophyta" rank="genus">Quercus</taxonomicName>
