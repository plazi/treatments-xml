<taxonomicName id="4C394D3CFFEEFFD409BCFEE2AEC5CB55" authorityName="Cantor" authorityYear="1842" box="[219,384,293,316]" class="Amphibia" family="Bufonidae" genus="Bufo" kingdom="Animalia" order="Anura" pageId="65" pageNumber="66" phylum="Chordata" rank="species" species="gargarizans">
<emphasis id="B94DEAADFFEEFFD409BCFEE2AEC5CB55" box="[219,384,293,316]" italics="true" pageId="65" pageNumber="66">B. gargarizans</emphasis>
</taxonomicName>
(
<taxonomicName id="4C394D3CFFEEFFD408E9FEE3AD41CB55" box="[398,516,292,316]" class="Amphibia" kingdom="Animalia" pageId="65" pageNumber="66" phylum="Chordata" rank="class">Amphibia</taxonomicName>
: 
<taxonomicName id="4C394D3CFFEEFFD40B68FEE3AD1FCB55" box="[527,602,292,316]" class="Amphibia" kingdom="Animalia" order="Anura" pageId="65" pageNumber="66" phylum="Chordata" rank="order">Anura</taxonomicName>
: 
<taxonomicName id="4C394D3CFFEEFFD40B03FEE3AD9BCB55" authorityName="Gray" authorityYear="1825" box="[612,734,292,316]" class="Amphibia" family="Bufonidae" kingdom="Animalia" order="Anura" pageId="65" pageNumber="66" phylum="Chordata" rank="family">Bufonidae</taxonomicName>
