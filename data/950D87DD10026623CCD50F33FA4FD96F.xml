<date id="691A100B10026623CE010DBCFD88DF92" box="[579,603,941,962]" pageId="3" pageNumber="78">
<number id="AB6C24EC10026623CE010DBCFD9CDF92" box="[579,591,941,962]" pageId="3" pageNumber="78" value="2.0">2</number>
