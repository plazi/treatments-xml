<taxonomicName id="80B04D6BE926ED1743ECF9FECFCF7240" authority="Grimaldi, 2016" authorityName="Grimaldi" authorityYear="2016" box="[907,1170,1560,1584]" class="Insecta" family="Apsilocephalidae" genus="Burmapsilocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="78" phylum="Arthropoda" rank="species" species="evocoa" status="new species">
