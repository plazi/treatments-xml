<taxonomicName id="4C054D4A9F33FFA9FD04EEADFC6334EF" authorityName=": Stewart" authorityYear="2000" box="[683,924,1503,1527]" class="Insecta" family="Taeniopterygidae" genus="Strophopteryx" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="88" phylum="Arthropoda" rank="species" species="fasciata">
