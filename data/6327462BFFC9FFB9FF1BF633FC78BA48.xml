<quantity id="2C765AD8FFCAFFB9FCE6F773FC21B90D" box="[874,967,1341,1367]" metricMagnitude="3" metricUnit="m" metricValue="1.9" pageId="6" pageNumber="58" unit="m" value="1900.0">1900 m</quantity>
