<taxonomicName id="20884D6EFFC0FFB1286A9A95FE76FF2C" box="[383,505,151,177]" class="Insecta" family="Culicidae" genus="Culex" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="sudanicus">
