<taxonomicName id="4C35DCDBFFE4FFE8DEE6F977FC5D7E6A" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[724,967,1120,1143]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="yuani" status="new species">
