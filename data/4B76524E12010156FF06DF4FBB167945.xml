<taxonomicName id="04DF98DB12000159FDC5DCE5BC157EC0" box="[596,843,1728,1751]" class="Insecta" family="Lycidae" genus="Sulabanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="mamasensis">
