, PL 
<quantity id="04274EBD120F0156FC93DABDBC3078A5" box="[770,878,152,178]" metricMagnitude="-3" metricUnit="m" metricValue="1.16" pageId="8" pageNumber="9" unit="mm" value="1.16">1.16 mm</quantity>
, PW 
<quantity id="04274EBD120F0156FC3EDABDBB4278A5" box="[943,1052,152,178]" metricMagnitude="-3" metricUnit="m" metricValue="1.34" pageId="8" pageNumber="9" unit="mm" value="1.34">1.34 mm</quantity>
, Ediam 
<quantity id="04274EBD120F0156FBECDABDBB8578A5" box="[1149,1243,152,178]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="8" pageNumber="9" unit="mm" value="0.3">0.3 mm</quantity>
, Edist 
<quantity id="04274EBD120F0156FABADABDBAC678A5" box="[1323,1432,152,178]" metricMagnitude="-4" metricUnit="m" metricValue="3.9" pageId="8" pageNumber="9" unit="mm" value="0.39">0.39 mm</quantity>
, EL 
<quantity id="04274EBD120F0156FF53DAE5BE7E78CD" box="[194,288,192,218]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="8" pageNumber="9" unit="mm" value="5.5">5.5 mm</quantity>
