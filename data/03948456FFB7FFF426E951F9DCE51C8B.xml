. 
<emphasis id="B949E952FFB6FFF4277C5237DC721CF2" box="[258,351,1150,1175]" italics="true" pageId="9" pageNumber="10">iheringi</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3D4EC3FFB6FFF427E45237DE6E1CF2" authority="Werneck 1948" authorityName="Werneck" authorityYear="1948" box="[410,835,1149,1175]" class="Insecta" family="Gyropidae" genus="Gyropus" kingdom="Animalia" order="Phthiraptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="subSpecies" species="martini" subSpecies="matthaeensis">
