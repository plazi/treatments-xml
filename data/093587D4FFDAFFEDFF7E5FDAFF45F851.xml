<subSubSection id="C9866549FFDAFFEDFF23598AFE2AF8C1" pageId="58" pageNumber="59" type="etymology">
<paragraph id="812336C2FFDAFFEDFF23598AFE2AF8C1" blockId="58.[151,1436,258,1938]" pageId="58" pageNumber="59">
<emphasis id="B3E8EAD0FFDAFFEDFF23598AFEE3F91C" bold="true" box="[202,336,1735,1759]" inLineHeading="true" pageId="58" pageNumber="59" reason="1">Etymology.</emphasis>
