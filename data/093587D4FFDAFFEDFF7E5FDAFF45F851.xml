<taxonomicName id="469C4D41FFDAFFEDFD6D598AFCBEF91D" box="[644,781,1735,1758]" class="Arachnida" family="Eriophyidae" genus="Epitrimerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="wuyingensis">
