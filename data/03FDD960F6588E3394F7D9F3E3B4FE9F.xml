907), 
<collectingCountry id="F34328E6F6588E3296A1D85EE363FAE4" box="[705,873,1358,1382]" name="Philippines" pageId="8" pageNumber="333">PHILIPPINES</collectingCountry>
, 
<taxonomicName id="4C5413F5F6588E329716D85EE3B4FAE7" box="[886,958,1358,1381]" class="Arachnida" family="Linyphiidae" genus="Gnathonarium" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="334" phylum="Arthropoda" rank="species" species="luzon" status="n. sp.">Luzon</taxonomicName>
