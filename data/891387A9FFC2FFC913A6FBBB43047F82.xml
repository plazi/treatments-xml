<taxonomicName id="C6BA4D3CFFC2FFC913A6FB4243227DB2" box="[151,347,1203,1228]" class="Insecta" family="Pieridae" genus="Catasticta" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="hebra">
