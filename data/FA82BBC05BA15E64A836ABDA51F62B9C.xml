<taxonomicName id="EDCFFF47DDCA13273B8203A1A2103A20" authorityName="J.R. Sahlberg" authorityYear="1880" class="Hexapoda" family="Carabidae" genus="Elaphrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="267" pageNumber="268" phylum="Arthropoda" rank="subSpecies" species="angusticollis" subSpecies="longicollis">
