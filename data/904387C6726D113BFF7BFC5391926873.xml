<quantity id="DF129B35726D113BFB56FB9D974E694B" box="[1210,1298,1050,1074]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="12" pageNumber="215" unit="m" value="3000.0">3000 m</quantity>
