<specimenCount id="75D5348DF774AF03FE78FC4CFE11FC5F" box="[386,407,906,929]" pageId="6" pageNumber="303" type="female">♀</specimenCount>
/ pulchricaudis 
<taxonomicNameLabel id="4A949E6DF774AF03FDACFC4CFD0BFC5F" box="[598,653,905,929]" pageId="6" pageNumber="303" rank="species">sp. n.</taxonomicNameLabel>
