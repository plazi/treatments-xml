7. 
<taxonomicName id="029BE71EB663F70386D02F2117AE3EDE" LSID="http://zoobank.org/E414FE48-C4B8-4AF4-A7CA-3D377AE806EA" authority="Liebherr, 2016" authorityName="Liebherr" authorityYear="2016" class="Insecta" family="Carabidae" genus="Cyphocoleus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyphocoleus angustatus" order="Coleoptera" pageId="22" pageNumber="233" phylum="Arthropoda" rank="species" species="angustatus" status="sp. n.">Cyphocoleus angustatus</taxonomicName>
