<location id="CC69607B111EF301EBD5FB33EFC0FB07" LSID="urn:lsid:plazi:treatment:411F87B6111EF301EBBAFC16EFE6FA9D:CC69607B111EF301EBD5FB33EFC0FB07" box="[1020,1234,1185,1209]" county="Tishomingo County" municipality="Tishomingo State Park" name="Itawamba County" pageId="2" pageNumber="82" stateProvince="Mississippi">Itawamba County</location>
