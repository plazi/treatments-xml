<collectorName id="64435376111EF301ECFCFB81E8ABFBF0" pageId="2" pageNumber="82">
<collectingMunicipality id="296DACDA111EF301ECFCFB81E86FFBF0" pageId="2" pageNumber="82">Tishomingo State Park</collectingMunicipality>
