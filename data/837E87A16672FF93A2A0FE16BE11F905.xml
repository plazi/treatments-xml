<taxonomicName id="CCD74D346672FF93A2A0FDA3BE6DFDBA" authority="Flint 1978" authorityName="Flint" authorityYear="1978" box="[151,546,544,566]" class="Insecta" family="Hydropsychidae" genus="Macrostemum" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="negrense">
<emphasis id="39A3EAA56672FF93A2A0FDA3BDDFFDBA" box="[151,400,545,566]" italics="true" pageId="22" pageNumber="323">Macrostemum negrense</emphasis>
