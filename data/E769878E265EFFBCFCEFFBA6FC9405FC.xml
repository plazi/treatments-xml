<collectingCountry id="17D77608265EFFBFFA99F950FAFB00E2" box="[1300,1386,1675,1697]" name="Vietnam" pageId="1" pageNumber="98">Vietnam</collectingCountry>
, 
<collectingCountry id="17D77608265EFFBFFAFAF957FA3900E2" box="[1399,1448,1676,1697]" name="Laos" pageId="1" pageNumber="98">Laos</collectingCountry>
