<taxonomicName id="38A13BEFB87DE5436100FCB961B3FC64" authorityName="Linnaeus" authorityYear="1758" box="[614,735,801,827]" kingdom="Plantae" order="Diptera" pageId="95" pageNumber="96" rank="order">DIPTERA</taxonomicName>
, 
<taxonomicName id="38A13BEFB87DE543618CFCB967DAFC64" authority="(Heteroptera, Homoptera)" authorityName="Heteroptera, Homoptera" baseAuthorityName="Heteroptera, Homoptera" box="[746,1206,801,827]" class="Insecta" kingdom="Animalia" order="Hemiptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="order">HEMIPTERA (Heteroptera, Homoptera)</taxonomicName>
