<taxonomicName id="4C694D2FFF8B995CFD88F9F602C18572" authority="Iannella &amp; Biondi, 2019" authorityName="Iannella &amp; Biondi" authorityYear="2019" box="[571,729,1568,1591]" class="Insecta" family="Chrysomelidae" genus="Blepharidina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" phylum="Arthropoda" rank="species" species="kasigauensis" status="sp. nov.">
<emphasis id="B91DEABEFF8B995CFD88F9F602528573" box="[571,586,1570,1591]" italics="true" pageId="14">B</emphasis>
. 
<emphasis id="B91DEABEFF8B995CFDEBF9F402C18572" box="[600,729,1568,1590]" italics="true" pageId="14">kasigauensis</emphasis>
</taxonomicName>
