<paragraph id="8BED36D0FFC6FF97FF74D13FFA1CF8F9" blockId="6.[151,1436,1801,2027]" box="[151,1436,1830,1851]" pageId="6" pageNumber="61">
<keyLead id="30A395E5FFC6FF97FF74D13FFA1CF8F9" box="[151,1436,1830,1851]" pageId="6" pageNumber="61">
- Females............................................................................................ 
<number id="3D9A24F7FFC6FF97FA73D13FFA1CF8F9" box="[1424,1436,1830,1851]" pageId="6" pageNumber="61" value="6.0">6</number>
