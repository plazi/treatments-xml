, 
<taxonomicName id="1DFB83C3FFA0FFE1ACC03D89FCC1FD09" box="[732,833,679,704]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="35" pageNumber="42" phylum="Chordata" rank="species" species="dichrous">
