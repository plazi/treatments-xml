<taxonomicName id="4C761EB328464B12FF35FD12FEBBE5E7" box="[151,364,709,731]" class="Insecta" family="Cecidomyiidae" genus="Haplusia" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="216" phylum="Arthropoda" rank="species" species="longipalpis">
