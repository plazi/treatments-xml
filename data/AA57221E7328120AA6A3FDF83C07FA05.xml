<collectingCountry id="5AE9D3987328120AA751FAAF39E0FA04" box="[357,467,1395,1419]" name="Myanmar" pageId="17" pageNumber="18">Myanmar</collectingCountry>
, 
<collectingCountry id="5AE9D3987328120AA7EAFAAE3A72FA05" box="[478,577,1394,1418]" name="Thailand" pageId="17" pageNumber="18">Thailand</collectingCountry>
