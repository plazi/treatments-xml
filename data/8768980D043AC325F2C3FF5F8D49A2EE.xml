locality 
<location id="0A1E7FC0043AC32BF16BFD2D8DFBA38A" LSID="urn:lsid:plazi:treatment:8768980D043AC325F2C3FF5F8D49A2EE:0A1E7FC0043AC32BF16BFD2D8DFBA38A" box="[637,711,724,748]" country="French Guiana" municipality="Nouragues Reserve" name="Parare" pageId="7" pageNumber="1764">Pararé</location>
