<taxonomicName id="4C4F1798FF93FFA76652FADCFB2FCF40" box="[1135,1238,1342,1366]" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="31" pageNumber="132" phylum="Chordata" rank="species" species="pageli">
