</taxonomicName>
, 
<taxonomicName id="4C4F1798FF93FFA763E6FAF8FDB4CF27" box="[475,589,1306,1329]" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="31" pageNumber="132" phylum="Chordata" rank="species" species="jaegeri">
