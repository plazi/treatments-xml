<taxonomicName id="4C4F1798FF93FFA767BFFB14FF09CF24" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="31" pageNumber="132" phylum="Chordata" rank="species" species="calamei">
