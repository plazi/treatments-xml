<collectingRegion id="49A87C9BFFCCFFAD6F8EFFB8FCC29BB3" box="[741,867,1445,1470]" country="Argentina" name="Corrientes" pageId="6" pageNumber="286">Corrientes</collectingRegion>
, 
<collectingRegion id="49A87C9BFFCCFFAD6E18FFBBFC569BB3" box="[883,1015,1446,1470]" country="Argentina" name="Entre Rios" pageId="6" pageNumber="286">Entre Ríos</collectingRegion>
, 
<collectingRegion id="49A87C9BFFCCFFAD696DFFBBFBD29BB0" box="[1030,1139,1446,1470]" country="Argentina" name="Santa Fe" pageId="6" pageNumber="286">Santa Fe</collectingRegion>
