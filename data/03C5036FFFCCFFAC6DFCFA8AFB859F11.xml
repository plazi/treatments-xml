: 
<collectingRegion id="49A87C9BFFCCFFAD6F4BFFB8FDFA9BB3" box="[544,603,1445,1470]" country="Argentina" name="Salta" pageId="6" pageNumber="286">Salta</collectingRegion>
, 
<collectingRegion id="49A87C9BFFCCFFAD6F01FFBBFD779BB3" box="[618,726,1446,1470]" country="Argentina" name="Tucuman" pageId="6" pageNumber="286">Tucumán</collectingRegion>
