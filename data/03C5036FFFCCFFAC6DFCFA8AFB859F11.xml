<collectingRegion id="49A87C9BFFCCFFAD6DFCFFD7FE989BEF" box="[151,313,1481,1506]" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="6" pageNumber="286">Buenos Aires</collectingRegion>
