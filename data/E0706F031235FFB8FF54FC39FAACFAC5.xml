<taxonomicName id="AFD9A5961235FFB8FC4BFB19FAA0FAC5" authority="Payraudeau, 1826" authorityName="Payraudeau" authorityYear="1826" box="[904,1285,1255,1280]" class="Bivalvia" family="Ostreidae" genus="Ostrea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pterioida" pageId="79" pageNumber="80" phylum="Mollusca" rank="species" species="stentina">
