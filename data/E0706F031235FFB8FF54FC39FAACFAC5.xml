<taxonomicName id="AFD9A5961235FFB8FF60FB16FE86FAC5" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[163,291,1255,1280]" class="Bivalvia" family="Pectinidae" genus="Pecten" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pterioida" pageId="79" pageNumber="80" phylum="Mollusca" rank="species" species="opercularis">
<emphasis id="5AAD02071235FFB8FF60FB16FE86FAC5" box="[163,291,1255,1280]" italics="true" pageId="79" pageNumber="80">opercularis</emphasis>
</taxonomicName>
