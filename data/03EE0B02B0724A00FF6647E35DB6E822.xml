</taxonomicName>
<emphasis id="B9336606B0714A00FBEB473B5918E966" bold="true" box="[1050,1114,513,539]" pageId="11" pageNumber="412">
<taxonomicNameLabel id="A200DB7DB0714A00FBEB473B5918E966" box="[1050,1114,513,539]" pageId="11" pageNumber="412" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C47C197B0714A00FB98473B580CE966" authority="Sanborn, 2019" authorityName="Sanborn" authorityYear="2019" box="[1129,1358,513,539]" class="Insecta" family="Cicadidae" genus="Selymbria" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="412" phylum="Arthropoda" rank="species" species="madredediosensis" status="n. sp.">
