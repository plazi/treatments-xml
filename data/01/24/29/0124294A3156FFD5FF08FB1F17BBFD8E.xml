<taxonomicName authority="Miko &amp; Masner" authorityName="Miko &amp; Masner" authorityYear="2010" box="[151,636,1154,1180]" class="Insecta" family="Scelionidae" genus="Xenomerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="vanharteni" status="new species">
