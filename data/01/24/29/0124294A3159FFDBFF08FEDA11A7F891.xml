<taxonomicName authority="Miko &amp; Masner" authorityName="Miko &amp; Masner" authorityYear="2010" box="[151,581,327,353]" class="Insecta" family="Scelionidae" genus="Xenomerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="noyesi" status="new species">
