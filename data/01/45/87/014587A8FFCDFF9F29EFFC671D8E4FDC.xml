<taxonomicName authority="(Granger)" baseAuthorityName="Granger" class="Insecta" family="Braconidae" genus="Doryctes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="2722" phylum="Arthropoda" rank="species" species="conspicuus">
