<taxonomicName box="[494,738,1982,2004]" class="Insecta" family="Cleridae" genus="Cymatodera" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="534" phylum="Arthropoda" rank="species" species="brailovskyi">
<emphasis box="[494,738,1982,2004]" italics="true" pageId="15" pageNumber="534">
<taxonomicName box="[494,619,1982,2004]" class="Insecta" family="Cleridae" genus="Cymatodera" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="534" phylum="Arthropoda" rank="genus">Cymatodera</taxonomicName>
brailovskyi
</emphasis>
</taxonomicName>
Rifkind
</taxonomicName>
, 
<taxonomicNameLabel box="[834,890,1982,2003]" pageId="15" pageNumber="534" rank="species">
