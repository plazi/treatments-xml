<taxonomicName authority="Mocsary, 1909: 35" authorityName="Mocsary" authorityPageNumber="35" authorityYear="1909" box="[260,761,1817,1841]" class="Insecta" family="Tenthredinidae" genus="Tenthredo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="fulviventris">
