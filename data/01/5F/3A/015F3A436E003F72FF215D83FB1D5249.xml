<taxonomicName authority="Scopoli, 1763" authorityName="Scopoli" authorityYear="1763" box="[749,1268,1853,1877]" class="Insecta" family="Tenthredinidae" genus="Eurhadinoceraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="fulviventris">
<emphasis box="[749,1079,1853,1877]" italics="true" pageId="14" pageNumber="215">Eurhadinoceraea fulviventris</emphasis>
(Scopoli, 
<number box="[1197,1259,1853,1877]" pageId="14" pageNumber="215" value="1763.0">1763</number>
)
</taxonomicName>
]. 
<typeStatus box="[1288,1400,1854,1878]" pageId="14" pageNumber="215">Lectotype</typeStatus>
