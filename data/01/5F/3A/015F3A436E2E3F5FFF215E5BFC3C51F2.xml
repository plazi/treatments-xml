<emphasis box="[373,601,1851,1873]" italics="true" pageId="33" pageNumber="234">
<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[373,597,1851,1873]" class="Insecta" family="Tenthredinidae" genus="Tenthredo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="234" phylum="Arthropoda" rank="species" species="unifasciata">Tenthredo unifasciata</taxonomicName>
