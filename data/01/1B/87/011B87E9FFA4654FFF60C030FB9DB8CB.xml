<geoCoordinate box="[534,612,405,431]" degrees="0.870" direction="west" orientation="longitude" pageId="54" pageNumber="55" precision="55" value="-0.87">–0.870</geoCoordinate>
mm
</quantity>
, Ant /head ratio=2.2 Body 
<quantity box="[985,1180,404,431]" metricMagnitude="-3" metricUnit="m" metricValue="1.875" metricValueMax="2.18" metricValueMin="1.57" pageId="54" pageNumber="55" unit="mm" value="1.875" valueMax="2.18" valueMin="1.57">
