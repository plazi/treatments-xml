<subSubSection pageId="63" pageNumber="64" type="description">
<paragraph blockId="63.[151,1437,1128,2043]" box="[199,621,1199,1227]" pageId="63" pageNumber="64">
<emphasis bold="true" box="[199,345,1199,1226]" pageId="63" pageNumber="64">Description.</emphasis>
Size. Length 
<quantity box="[500,616,1200,1227]" metricMagnitude="-3" metricUnit="m" metricValue="2.334" pageId="63" pageNumber="64" unit="mm" value="2.334">2.334 mm</quantity>
