<subSubSection pageId="37" pageNumber="38" type="etymology">
<paragraph blockId="37.[151,1437,153,359]" pageId="37" pageNumber="38">
<emphasis bold="true" box="[199,328,297,323]" pageId="37" pageNumber="38">Etymology</emphasis>
