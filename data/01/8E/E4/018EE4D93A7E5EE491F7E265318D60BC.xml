<paragraph pageId="0" pageNumber="17">
<taxonomicName authorityName="Weiner &amp; Fiera" authorityYear="2020" class="Collembola" family="Onychiuridae" genus="Hymenaphorura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hymenaphorura urbana" order="Poduromorpha" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="urbana">
