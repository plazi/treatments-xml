<taxonomicName authorityName="Stal" authorityYear="1866" box="[640,735,1532,1557]" class="Insecta" family="Reduviidae" genus="Voconia" kingdom="Animalia" order="Hemiptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="genus">
