<taxonomicName authority="Andrewes, 1923" authorityName="Andrewes" authorityYear="1923" class="Insecta" family="Carabidae" genus="Psilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="369" phylum="Arthropoda" rank="species" species="porphyreus">
