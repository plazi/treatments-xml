<geoCoordinate degrees="21" direction="south" minutes="15.99" orientation="latitude" precision="9" value="-21.2665">21°15.99'S</geoCoordinate>
, 
<geoCoordinate degrees="47" direction="east" minutes="25.21" orientation="longitude" precision="9" value="47.420166">47°25.21'E</geoCoordinate>
, coll. M. Irwin, R. 
