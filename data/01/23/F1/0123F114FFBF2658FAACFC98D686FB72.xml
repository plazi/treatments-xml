<quantity box="[466,604,1107,1131]" metricMagnitude="3" metricUnit="m" metricValue="1.6585" metricValueMax="1.683" metricValueMin="1.634" pageId="10" pageNumber="483" unit="m" value="1658.5" valueMax="1683.0" valueMin="1634.0">1634-1683 m</quantity>
