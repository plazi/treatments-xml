<paragraph pageId="0" pageNumber="23">
<materialsCitation collectingDate="2012-05-27" collectorName="S. Piribebuy, R. Scheffrahn" country="Paraguay" latitude="-25.5368" location="Paraguay" longLatPrecision="7" longitude="-57.0244" specimenCount="2" specimenCount-soldier="2" typeStatus="types">
<collectingCountry name="Paraguay">Paraguay</collectingCountry>
, 
<collectorName>S. Piribebuy</collectorName>
(
<geoCoordinate degrees="25.5368" direction="south" orientation="latitude" precision="5" value="-25.5368">-25.5368</geoCoordinate>
, 
<geoCoordinate degrees="57.0244" direction="west" orientation="longitude" precision="5" value="-57.0244">-57.0244</geoCoordinate>
), elev. 229, 
<collectingDate value="2012-05-27">27 May 2012</collectingDate>
, 
<collectorName>R. Scheffrahn</collectorName>
et al., UFTC no. PA89 containing 
<specimenCount type="soldier">2 soldiers</specimenCount>
