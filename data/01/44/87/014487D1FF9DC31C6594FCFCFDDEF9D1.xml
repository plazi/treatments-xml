<quantity box="[465,608,948,973]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" metricValueMax="2.0" metricValueMin="1.6" pageId="1" pageNumber="558" unit="mm" value="18.0" valueMax="20.0" valueMin="16.0">16 to 20 mm</quantity>
(n = 
<number box="[667,681,949,973]" pageId="1" pageNumber="558" value="9.0">9</number>
) (
<figureCitation box="[706,771,948,973]" captionStart="FIGURE 1" captionStartId="1.[151,250,1703,1725]" captionText="FIGURE 1. Hydropsyche rhadamanthys Malicky 2001. 1, larva, right lateral." pageId="1" pageNumber="558">
Fig. 
<number box="[758,771,949,973]" pageId="1" pageNumber="558" value="1.0">1</number>
</figureCitation>
).
</paragraph>
