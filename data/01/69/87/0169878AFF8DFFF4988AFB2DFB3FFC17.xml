<subSubSection box="[151,792,1222,1246]" pageId="1" pageNumber="420" type="materials_examined">
<paragraph blockId="1.[151,792,1222,1246]" box="[151,792,1222,1246]" pageId="1" pageNumber="420">
<typeStatus box="[151,201,1222,1246]" pageId="1" pageNumber="420">Type</typeStatus>
species. 
<taxonomicName authority="Sobczyk, 2021" authorityName="Sobczyk" authorityYear="2021" box="[294,552,1222,1246]" class="Insecta" family="Psychidae" genus="Guttulapsyche" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="420" phylum="Arthropoda" rank="species" species="mayaensis" status="sp. nov.">
<emphasis box="[294,552,1222,1246]" italics="true" pageId="1" pageNumber="420">Guttulapsyche mayaensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[558,639,1222,1246]" pageId="1" pageNumber="420">
<taxonomicNameLabel box="[558,639,1222,1246]" pageId="1" pageNumber="420" rank="species">sp. nov.</taxonomicNameLabel>
