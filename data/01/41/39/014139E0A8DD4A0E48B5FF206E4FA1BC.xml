</subSubSection>
<subSubSection pageId="1" pageNumber="126" type="nomenclature">
<paragraph pageId="1" pageNumber="126">
<taxonomicName LSID="http://zoobank.org/B5C859DD-C5B1-4C49-8742-FF0E93BFF688" authority="Brown" authorityName="Brown" authorityYear="2014" class="Insecta" family="Tortricidae" genus="Circanota" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Circanota" order="Lepidoptera" pageId="1" pageNumber="126" phylum="Arthropoda" rank="genus">Circanota Brown</taxonomicName>
<taxonomicNameLabel pageId="1" pageNumber="126">gen. n.</taxonomicNameLabel>
