<emphasis box="[467,480,461,485]" italics="true" pageId="34" pageNumber="2544">–</emphasis>
29.0 mm [mean (
<emphasis box="[672,709,461,485]" italics="true" pageId="34" pageNumber="2544">n =</emphasis>
5) 
<quantity box="[745,860,461,485]" metricMagnitude="-2" metricUnit="m" metricValue="2.582" pageId="34" pageNumber="2544" unit="mm" value="25.82">25.82 mm</quantity>
, SD = 1.003]; female 24.0 
