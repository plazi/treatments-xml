7) 
<quantity box="[745,860,880,904]" metricMagnitude="-2" metricUnit="m" metricValue="2.696" pageId="11" pageNumber="2521" unit="mm" value="26.96">26.96 mm</quantity>
, SD = 1.577]; female 
<quantity metricMagnitude="-2" metricUnit="m" metricValue="2.75" metricValueMax="3.25" metricValueMin="2.25" pageId="11" pageNumber="2521" unit="mm" value="27.5" valueMax="32.5" valueMin="22.5">
22.5 
<emphasis box="[1142,1155,880,904]" italics="true" pageId="11" pageNumber="2521">–</emphasis>
32.5 mm
</quantity>
[mean (
<emphasis box="[356,395,915,939]" italics="true" pageId="11" pageNumber="2521">n =</emphasis>
11) 
<quantity box="[447,563,915,939]" metricMagnitude="-2" metricUnit="m" metricValue="2.655" pageId="11" pageNumber="2521" unit="mm" value="26.55">26.55 mm</quantity>
