: 
<collectorName box="[533,611,953,973]" pageId="61" pageNumber="454">Ziéla, U.</collectorName>
<collectionCode box="[610,629,953,973]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="61" pageNumber="454" type="Museum">V</collectionCode>
., 
<date box="[641,751,953,973]" pageId="61" pageNumber="454" value="1957-02">
<collectingDate box="[641,751,953,973]" pageId="61" pageNumber="454" value="1957-02">février 1957</collectingDate>
</date>
; plateau 
<collectorName box="[840,960,953,973]" pageId="61" pageNumber="454">de Zouguépo</collectorName>
