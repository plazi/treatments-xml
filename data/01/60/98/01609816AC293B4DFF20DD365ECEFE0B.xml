<collectingRegion box="[457,519,1579,1599]" country="Liberia" name="Nimba" pageId="101" pageNumber="494">Nimba</collectingRegion>
: 
<collectingMunicipality box="[537,623,1579,1599]" pageId="101" pageNumber="494">Yalanzou</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:01609816AC293B4DFF20DD365ECEFE0B:8C167FDBAC293B4EFDA8DE6E5EEAF99E" box="[633,681,1579,1599]" municipality="Yalanzou" name="Nion" pageId="101" pageNumber="494" stateProvince="Nimba">Nion</location>
, II-VI.1942; 
<collectorName box="[805,884,1579,1599]" pageId="101" pageNumber="494">Ziéla, U.</collectorName>
<collectionCode box="[883,902,1579,1599]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="101" pageNumber="494" type="Museum">V</collectionCode>
