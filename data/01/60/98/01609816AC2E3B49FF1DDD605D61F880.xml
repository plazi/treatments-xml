<collectingRegion box="[417,479,1506,1526]" country="Liberia" name="Nimba" pageId="98" pageNumber="491">Nimba</collectingRegion>
: 
<location LSID="urn:lsid:plazi:treatment:01609816AC2E3B49FF1DDD605D61F880:8C167FDBAC2E3B49FE21DDA75E5DFA57" box="[496,542,1506,1526]" name="Ziela" pageId="98" pageNumber="491" stateProvince="Nimba">Ziéla</location>
, forêt, U. 
<collectionCode box="[626,645,1506,1526]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="98" pageNumber="491" type="Museum">V</collectionCode>
., 
<date box="[657,762,1506,1526]" pageId="98" pageNumber="491" value="1957-03-30">
<collectingDate box="[657,762,1506,1526]" pageId="98" pageNumber="491" value="1957-03-30">30.III.1957</collectingDate>
</date>
; 
<collectorName box="[779,865,1506,1526]" pageId="98" pageNumber="491">Yalanzou</collectorName>
, forêt, II-VI.1942; plateau 
<collectorName box="[1112,1231,1506,1526]" pageId="98" pageNumber="491">de Zouguépo</collectorName>
(à la lumière), vers 750 mètres d’altitude, 
<date box="[380,495,1534,1554]" pageId="98" pageNumber="491" value="1981-03-03" valueMax="1981-03-05" valueMin="1981-03-03">
