; 
<collectingMunicipality box="[602,688,487,507]" pageId="81" pageNumber="474">Yalanzou</collectingMunicipality>
, II-VI.1942; 
<location LSID="urn:lsid:plazi:treatment:01609816AC1D3B7AFF23D9695FA0FC8A:8C167FDBAC1D3B7AFF02DA465D42FDB6" box="[211,257,515,535]" county="Nion" municipality="Yalanzou" name="Ziela" pageId="81" pageNumber="474" stateProvince="Nimba">Ziéla</location>
, 
