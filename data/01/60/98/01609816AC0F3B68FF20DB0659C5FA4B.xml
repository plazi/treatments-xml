<location LSID="urn:lsid:plazi:treatment:01609816AC0F3B68FF20DB0659C5FA4B:8C167FDBAC0F3B68FD6FDC585EAFFB90" box="[702,748,1053,1073]" municipality="Nzo" name="Ziela" pageId="67" pageNumber="460" stateProvince="Nimba">Ziéla</location>
