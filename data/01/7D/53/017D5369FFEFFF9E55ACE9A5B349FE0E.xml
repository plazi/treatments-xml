<taxonomicName authority="Dana, 1852" authorityName="Dana" authorityYear="1852" box="[264,655,439,464]" class="Malacostraca" family="Thalassocarididae" genus="Thalassocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="crinita">
