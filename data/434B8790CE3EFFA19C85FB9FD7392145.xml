<taxonomicName id="0CE24D05CE3EFFA19F88FD75D77A27D7" box="[966,1077,725,746]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="352" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
