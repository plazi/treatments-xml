<taxonomicName id="0CE24D05CE3EFFA19EA6FD75D06527D7" box="[744,810,725,746]" class="Liliopsida" family="Asparagaceae" genus="Nolina" kingdom="Plantae" order="Asparagales" pageId="9" pageNumber="352" phylum="Tracheophyta" rank="genus">
