<taxonomicName id="0CE24D05CE3EFFA19F14FC2FD0B52699" authority="Adans." authorityName="Adans." box="[858,1018,911,932]" class="Magnoliopsida" family="Asteraceae" genus="Encelia" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="352" phylum="Tracheophyta" rank="genus">
