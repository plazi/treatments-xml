</taxonomicName>
, 
<taxonomicName id="0CE24D05CE3EFFA19F72FD75D0DC27D7" box="[828,915,725,746]" class="Magnoliopsida" family="Asteraceae" genus="Erigeron" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="352" phylum="Tracheophyta" rank="genus">
