<collectingCountry id="F1DA763CFFF4FF8A8038FE879CBACA76" box="[554,650,264,284]" name="Zimbabwe" pageId="3" pageNumber="468">Zimbabwe</collectingCountry>
, 
<collectingCountry id="F1DA763CFFF4FF8A8080FE879CD5CA76" box="[658,741,264,284]" name="Zimbabwe" pageId="3" pageNumber="468">Rhodesia</collectingCountry>
, Salisbury, 
<date id="FD73106CFFF4FF8A815BFE879D9DCA76" box="[841,941,264,284]" pageId="3" pageNumber="468" value="1969-10-25">25.10.1969</date>
