<taxonomicName id="4C754D5EFF8C870FB3E4FBF5FCDDAE70" authority="Linnavuori, 2010" authorityName="Linnavuori" authorityYear="2010" box="[213,404,1124,1142]" class="Insecta" family="Miridae" genus="Paredrocoris" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="387" phylum="Arthropoda" rank="species" species="ilamicus" status="sp. nov.">
