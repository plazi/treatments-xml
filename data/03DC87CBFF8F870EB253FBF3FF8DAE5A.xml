<subSubSection id="C36F6556FF8C870EB3A6FA8EFE7BAE1A" lastPageId="19" lastPageNumber="388" pageId="18" pageNumber="387" type="diagnosis">
<paragraph id="8BCA36DDFF8C870EB3A6FA8EFC48A9FA" blockId="18.[151,1127,1215,1592]" lastBlockId="19.[113,1089,897,1116]" lastPageId="19" lastPageNumber="388" pageId="18" pageNumber="387">
<emphasis id="B901EACFFF8C870FB3A6FA8EFCDDAF31" bold="true" box="[151,404,1311,1335]" pageId="18" pageNumber="387">Differential diagnosis.</emphasis>
The genus 
<taxonomicName id="4C754D5EFF8C870FB127FAB0FE71AF3E" authority="Reuter, 1878" authorityName="Reuter" authorityYear="1878" box="[534,824,1312,1336]" class="Insecta" family="Miridae" genus="Paredrocoris" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="387" phylum="Arthropoda" rank="genus">
