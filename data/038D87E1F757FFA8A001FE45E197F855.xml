in 
<taxonomicName id="4C244D74F757FFA8A433F861E1B9F852" baseAuthorityName="Phillips" baseAuthorityYear="1836" box="[1314,1438,2002,2027]" class="Stenolaemata" family="Fistuliporidae" genus="Fistulipora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cystoporida" pageId="7" pageNumber="158" phylum="Bryozoa" rank="species" species="incrustans">
