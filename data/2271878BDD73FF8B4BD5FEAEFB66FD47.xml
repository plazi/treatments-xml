are in the Museum für Naturkunde der Humboldt Universität zu Berlin (
<collectionCode id="CCC9AE58DD73FF8B4E19FDCBFA09FDCF" box="[1371,1436,515,541]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="29" pageNumber="54">ZMB</collectionCode>
<number id="1C1024BADD73FF8B4BD5FDE3FE8BFD97" box="[151,286,555,581]" pageId="29" pageNumber="54" value="3349.335">3349, 3350</number>
, 
<number id="1C1024BADD73FF8B4A6BFDE3FEE3FD97" box="[297,374,555,581]" pageId="29" pageNumber="54" value="10056.0">10056</number>
- 
<number id="1C1024BADD73FF8B4A3CFDE3FE61FD97" box="[382,500,555,581]" pageId="29" pageNumber="54" value="58.5026">58, 50260</number>
, 
<number id="1C1024BADD73FF8B4942FDE3FDD9FD97" box="[512,588,555,581]" pageId="29" pageNumber="54" value="50290.0">50290</number>
, and 
<number id="1C1024BADD73FF8B49CBFDE3FD42FD97" box="[649,727,555,581]" pageId="29" pageNumber="54" value="50301.0">50301</number>
