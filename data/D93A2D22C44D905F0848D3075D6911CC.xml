<taxonomicName id="87E3E6BA65FEBF2776B31648AF865A86" LSID="http://zoobank.org/855A02E1-9776-4EAA-ABE4-46401516DF01" authority="Samuelson, 2015" authorityName="Samuelson" authorityYear="2015" class="Insecta" family="Chrysomelidae" genus="Acronymolpus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acronymolpus turbo" order="Coleoptera" pageId="2" pageNumber="95" phylum="Arthropoda" rank="species" species="turbo">Acronymolpus turbo</taxonomicName>
