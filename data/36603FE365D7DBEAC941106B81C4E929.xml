<taxonomicName id="7E212F4D65029E0A3AB07D98A1D6F6F6" class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristiphora astragali" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="astragali">Pristiphora astragali</taxonomicName>
