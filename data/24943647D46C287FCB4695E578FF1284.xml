<taxonomicName id="B717B7673CA4D7276DF80293118683E8" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fagus sylvatica" order="Fagales" pageId="0" pageNumber="105" phylum="Tracheophyta" rank="species" species="sylvatica">Fagus sylvatica</taxonomicName>
