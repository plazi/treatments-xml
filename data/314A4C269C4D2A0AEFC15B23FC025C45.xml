(
<figureCitation id="21D8E1B59C4D2A07EFC95B01FE875877" box="[159,257,1342,1367]" captionStart="FIGURE 5" captionStartId="12.[151,249,1921,1943]" captionTargetBox="[224,1351,211,1896]" captionTargetId="figure-64@12.[209,1365,193,1898]" captionTargetPageId="12" captionText="FIGURE 5. Head of 3rd instar larvae of Myrmeleontini, ventral view; A, Myrmeleon formicarius; B, Myrmeleon gerlindae; C, Myrmeleon punicanus; D, Myrmeleon bore; E, Myrmeleon inconspicuus; F, Myrmeleon mariaemathildae; G, Myrmeleon hyalinus; H, Myrmeleon fasciatus; I, Euroleon nostras." figureDoi="http://doi.org/10.5281/zenodo.4909377" httpUri="https://zenodo.org/record/4909377/files/figure.png" pageId="53" pageNumber="54">Figs. 5C</figureCitation>
, 
<figureCitation id="21D8E1B59C4D2A07EE5B5B01FEB65877" box="[269,304,1342,1367]" captionStart="FIGURE 6" captionStartId="13.[151,249,1926,1948]" captionTargetBox="[245,1291,199,1896]" captionTargetId="figure-69@13.[229,1336,193,1898]" captionTargetPageId="13" captionText="FIGURE 6. IX abdominal sternite of 3rd instar larvae of Myrmeleontini, ventral view; A, Myrmeleon gerlindae; B, Myrmeleon punicanus; C, Myrmeleon formicarius; D, Myrmeleon bore; E, Myrmeleon inconspicuus; F, Myrmeleon mariaemathildae; G, Myrmeleon hyalinus; H, Myrmeleon fasciatus; I, Euroleon nostras. Scale bar: 1 mm." figureDoi="http://doi.org/10.5281/zenodo.4909381" httpUri="https://zenodo.org/record/4909381/files/figure.png" pageId="53" pageNumber="54">6C</figureCitation>
