<taxonomicName id="4C1C4D67AAE24DA7B775FD643A07FD3B" authority="(Bonaparte, 1832)" authorityName="Bonaparte" authorityYear="1832" baseAuthorityName="Bonaparte" baseAuthorityYear="1832" box="[282,819,655,680]" class="Elasmobranchii" family="Dasyatidae" genus="Pteroplatytrygon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="143" pageNumber="144" phylum="Chordata" rank="species" species="violacea">
