<taxonomicName id="4C1C4D67AAE24DA7B373FC8C38EDFC36" authority="rhinobatos (Linnaeus, 1758)" authorityName="Linnaeus" authorityYear="1758" class="Pisces" family="Rhinobatidae" genus="Rhinobatos" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="143" pageNumber="144" phylum="Chordata" rank="species" species="rhinobatos">
