<number id="3DD824A8061F9D590004F962FCEAF92D" box="[800,830,1687,1711]" pageId="0" pageNumber="72" value="24.0">24</number>
° 
<number id="3DD824A8061F9D59006CF962FCB1F92D" box="[840,869,1687,1711]" pageId="0" pageNumber="72" value="38.0">38</number>
