<quantity id="664DCFD25303DC69FB70FC7F8782FC0E" box="[1162,1263,901,927]" metricMagnitude="-3" metricUnit="m" metricValue="3.29" pageId="20" pageNumber="471" unit="mm" value="3.29">3.29 mm</quantity>
