<quantity id="664DCFD25303DC69FCC8FC7F80FAFC0E" box="[818,919,901,927]" metricMagnitude="-3" metricUnit="m" metricValue="2.66" pageId="20" pageNumber="471" unit="mm" value="2.66">2.66 mm</quantity>
, PL= 
<quantity id="664DCFD25303DC69FC23FC7F8752FC0E" box="[985,1087,901,928]" metricMagnitude="-3" metricUnit="m" metricValue="2.54" pageId="20" pageNumber="471" unit="mm" value="2.54">2.54 mm</quantity>
