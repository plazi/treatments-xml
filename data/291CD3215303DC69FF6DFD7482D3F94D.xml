<quantity id="664DCFD25303DC69FD95FC7F818FFC0E" box="[623,738,901,927]" metricMagnitude="-2" metricUnit="m" metricValue="1.224" pageId="20" pageNumber="471" unit="mm" value="12.24">12.24 mm</quantity>
