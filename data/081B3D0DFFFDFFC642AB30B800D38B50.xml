<emphasis id="B2C65009FFFDFFC642FB31FA0377888B" bold="true" box="[199,352,469,494]" pageId="3" pageNumber="136">Distribution.</emphasis>
<taxonomicName id="47B2F798FFFDFFC6435731FA005F8888" box="[363,584,469,493]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="136" phylum="Arthropoda" rank="species" species="modesta">
<emphasis id="B2C65009FFFDFFC6435731FA005F8888" box="[363,584,469,493]" italics="true" pageId="3" pageNumber="136">Dasyhelea modesta</emphasis>
</taxonomicName>
