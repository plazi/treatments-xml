<collectingCountry id="F316F19677E8FF13FC96A4C19308F858" box="[789,885,1962,1986]" name="Curaçao" pageId="191" pageNumber="192">Curaçao</collectingCountry>
, 
<collectingCountry id="F316F19677E8FF13FC01A4C2938EF858" box="[898,1011,1961,1986]" name="Colombia" pageId="191" pageNumber="192">Colombia</collectingCountry>
