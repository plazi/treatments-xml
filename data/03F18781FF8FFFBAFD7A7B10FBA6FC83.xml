<collectionCode id="ED49AE52FF8FFFB8FE5F79D3FE49F977" box="[416,505,1710,1736]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="37" pageNumber="38" type="Museum">FMNH</collectionCode>
