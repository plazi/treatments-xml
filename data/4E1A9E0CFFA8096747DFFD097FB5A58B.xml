<collectingCountry id="BEA46F8AFFA8096447DFFB9C7B42A0D5" box="[151,363,1146,1172]" name="New Zealand" pageId="5" pageNumber="6">
<collectionCode id="A0A2B7DFFFA8096447DFFB9C7AF3A0D5" box="[151,218,1146,1172]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="5" pageNumber="6">NEW</collectionCode>
<collectionCode id="A0A2B7DFFFA8096447A9FB9C7B42A0D5" box="[225,363,1146,1172]" pageId="5" pageNumber="6">ZEALAND</collectionCode>
