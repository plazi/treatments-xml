considered 
<taxonomicName id="36EABFE5361FFF91FD4DFA45A6F46715" authority="Karg" authorityName="Karg" authorityYear="1982" box="[682,1013,1521,1548]" class="Arachnida" family="Ascidae" genus="Neojordensia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="147" phylum="Arthropoda" rank="species" species="lativentris">
