<taxonomicName id="78854D3DB655FF814CC7703B9BDEF54C" authority=", Diva" authorityName="Diva" box="[232,478,1636,1655]" class="Malacostraca" family="Oedicerotidae" genus="Oediceropsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="16" pageNumber="582" phylum="Arthropoda" rank="species" species="bicornuta">
