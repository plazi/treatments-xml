<figureCitation id="39F82A0C5665752D5D54F9FFFCC40B2F" box="[778,881,1571,1595]" captionStart="FIGURE 6" captionStartId="39.[120,206,1492,1513]" captionTargetBox="[122,1224,224,1466]" captionTargetId="figure-8@39.[120,1224,224,1466]" captionTargetPageId="39" captionText="FIGURE 6. Habitus of Felisacus species." figureDoi="http://doi.org/10.5281/zenodo.5379661" httpUri="https://zenodo.org/record/5379661/files/figure.png" pageId="122" pageNumber="122">Figures 6</figureCitation>
, 
<figureCitation id="39F82A0C5665752D5D23F9FFFC2E0B2F" box="[893,923,1571,1595]" captionStart="FIGURE 9" captionStartId="43.[120,206,1733,1754]" captionTargetBox="[166,1179,224,1706]" captionTargetId="figure-7@43.[166,1179,224,1706]" captionTargetPageId="43" captionText="FIGURE 9. Vesical spicules of Felisacus species." figureDoi="http://doi.org/10.5281/zenodo.5379673" httpUri="https://zenodo.org/record/5379673/files/figure.png" pageId="122" pageNumber="122">9B</figureCitation>
, 
<figureCitation id="39F82A0C5665752D5DF9F9FFFC750B2F" box="[935,960,1571,1595]" captionStart="FIGURE 12" captionStartId="46.[123,209,1609,1630]" captionTargetBox="[140,1224,224,1576]" captionTargetId="figure-9@46.[120,1224,224,1582]" captionTargetPageId="46" captionText="FIGURE 12. Parameres of Felisacus species." figureDoi="http://doi.org/10.5281/zenodo.5379687" httpUri="https://zenodo.org/record/5379687/files/figure.png" pageId="122" pageNumber="122">12</figureCitation>
