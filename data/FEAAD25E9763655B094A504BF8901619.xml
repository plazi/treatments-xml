<taxonomicName id="B3AA305301CD819CC26A2D37FD7E5CA6" class="Liliopsida" family="Arecaceae" genus="Astrocaryum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Astrocaryum chonta" order="Arecales" pageId="275" pageNumber="276" phylum="Tracheophyta" rank="species" species="chonta">Astrocaryum chonta</taxonomicName>
