<taxonomicName id="5A329756E4F6DEEDC40E3C41AD678A16" authorityName="E. v. Martens" authorityYear="1864" class="Gastropoda" family="Diplommatinidae" genus="Diancta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diancta" order="Architaenioglossa" pageId="0" pageNumber="1" phylum="Mollusca" rank="genus">
