<taxonomicName id="4C174D43FF15FF3198D8F880F8D53789" authority="(Soldatov, 1930)" baseAuthorityName="Soldatov" baseAuthorityYear="1930" box="[165,660,1905,1932]" class="Actinopterygii" family="Cyclopteridae" genus="Eumicrotremus" kingdom="Animalia" order="Scorpaeniformes" pageId="132" pageNumber="133" phylum="Chordata" rank="species" species="lindbergi">
