<quantity id="B5199B092423BD7AFB4EDF6E1E18A242" box="[1262,1319,300,323]" metricMagnitude="3" metricUnit="m" metricValue="1.609344" pageId="61" pageNumber="62" unit="mi" value="1.0">1 mi.</quantity>
along side road E, 
<geoCoordinate id="17D5502B2423BD7AFEBDDF0E1A53A262" box="[285,364,332,355]" degrees="8" direction="north" minutes="55" orientation="latitude" pageId="61" pageNumber="62" precision="925" value="8.916667">8°55’N</geoCoordinate>
<geoCoordinate id="17D5502B2423BD7AFED4DF0E1AE8A262" box="[372,471,332,355]" degrees="82" direction="west" minutes="05" orientation="longitude" pageId="61" pageNumber="62" precision="925" value="-82.083336">82°05’W</geoCoordinate>
, 
<quantity id="B5199B092423BD7AFE43DF0E1960A262" box="[483,607,332,355]" metricMagnitude="2" metricUnit="m" metricValue="3.0" metricValueMax="5.0" metricValueMin="1.0" pageId="61" pageNumber="62" unit="m" value="300.0" valueMax="500.0" valueMin="100.0">100–500 m</quantity>
