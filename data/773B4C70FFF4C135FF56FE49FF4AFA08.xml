<subSubSection id="B788AEEDFFF4C135FF06FD36FC2FFB15" pageId="14" pageNumber="24" type="description">
<paragraph id="FF2DFD66FFF4C135FF06FD36FECFFD30" blockId="14.[151,1437,591,1483]" pageId="14" pageNumber="24">
<emphasis id="CDE62174FFF4C135FF06FD36FE29FD6C" bold="true" box="[199,424,661,686]" pageId="14" pageNumber="24">Worker diagnosis:</emphasis>
