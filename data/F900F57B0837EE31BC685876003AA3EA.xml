3. 
<taxonomicName id="E234BA065DBE57C7312B4AD73B36FEDE" class="Arachnida" family="Eremaeidae" genus="Eremaeus" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="3" pageNumber="213" phylum="Arthropoda" rank="species" species="silvestris">Eremaeus silvestris</taxonomicName>
