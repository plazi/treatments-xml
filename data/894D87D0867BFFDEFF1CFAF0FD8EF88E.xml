<emphasis id="3390EAD48678FFDEFEF7FAC0FE59FAD5" box="[380,503,1383,1405]" italics="true" pageId="70" pageNumber="71">
<taxonomicName id="C6E44D458678FFDEFEF7FAC0FE5DFAD5" authorityName=": Carpintero" authorityYear="2002" baseAuthorityName="Poppius" baseAuthorityYear="1909" box="[380,499,1383,1405]" class="Insecta" family="Hemiptera (awaiting allocation)" genus="Lasiocolpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="70" pageNumber="71" phylum="Arthropoda" rank="species" species="rostralis">L. rostralis</taxonomicName>
