<geoCoordinate id="9EC150208A4FFFB79BF6F8395B128EB4" box="[151,286,2012,2037]" direction="south" orientation="latitude" pageId="25" pageNumber="26" precision="1" value="-7.3763666">7°22.582’ S</geoCoordinate>
