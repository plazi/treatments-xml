<quantity id="3C0D9B028A4FFFB79BF6F85E5ACF8E93" box="[151,195,1978,2003]" metricMagnitude="-1" metricUnit="m" metricValue="2.032" pageId="25" pageNumber="26" unit="in" value="8.0">8 in</quantity>
