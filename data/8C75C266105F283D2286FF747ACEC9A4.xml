<taxonomicName id="C3DC08F3105E283D2129FF037DD9C8A8" box="[824,933,224,248]" class="Insecta" family="Phalacridae" genus="Acylomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="genus">
<emphasis id="36A8AF62105E283D2129FF037DD9C8A8" box="[824,933,224,248]" italics="true" pageId="119" pageNumber="120">Acylomus</emphasis>
