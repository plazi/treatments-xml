<location id="010325AB105E283D21ACFF3C7A37C8A7" LSID="urn:lsid:plazi:treatment:8C75C266105F283D2286FF747ACEC9A4:010325AB105E283D21ACFF3C7A37C8A7" box="[957,1099,223,247]" country="Sri Lanka" name="Distribution" pageId="119" pageNumber="120">Distribution</location>
: 
<collectingCountry id="7CCB33E0105E283D2644FF3C7AB8C8A7" box="[1109,1220,223,247]" name="Sri Lanka" pageId="119" pageNumber="120">Sri Lanka</collectingCountry>
