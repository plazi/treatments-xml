<taxonomicNameLabel id="A225185AFFCAFFD4FD478B1FFD64FCC8" box="[711,765,773,792]" pageId="13" pageNumber="166" rank="species">n. sp.</taxonomicNameLabel>
<emphasis id="B916A521FFCAFFD4FC868B1EFC8BFCC7" bold="true" box="[774,786,772,791]" pageId="13" pageNumber="166">a</emphasis>
) sipho; 
<emphasis id="B916A521FFCAFFD4FCE48B1EFCE8FCC7" bold="true" box="[868,881,772,791]" pageId="13" pageNumber="166">b</emphasis>
) tegmen, lateral; 
<emphasis id="B916A521FFCAFFD4FB9F8B1EFBB0FCC7" bold="true" box="[1055,1065,772,791]" pageId="13" pageNumber="166">c</emphasis>
