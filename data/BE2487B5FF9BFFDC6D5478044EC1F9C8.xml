<taxonomicName id="F18D4D20FF9BFFDC6DA678044EECFADA" authority="Johnson, 1982" authorityName="Johnson" authorityYear="1982" box="[682,988,1366,1392]" class="Insecta" family="Ptiliidae" genus="Dipentium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
