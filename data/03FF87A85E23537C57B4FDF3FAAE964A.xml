<taxonomicName id="4C564D3D5E23537C5624FD77FDED90DA" authority="Bates, 1873" authorityName="Bates" authorityYear="1873" box="[263,600,681,703]" class="Insecta" family="Carabidae" genus="Perileptus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="species" species="japonicus">
