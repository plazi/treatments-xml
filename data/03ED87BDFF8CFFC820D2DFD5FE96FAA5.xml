<taxonomicName id="4C444D28FF8BFFC820ADDFD1FDCBFA85" authority="forest" authorityName="Forest" box="[456,625,1259,1283]" class="Insecta" family="Figitidae" genus="Muhaka" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="2605" phylum="Arthropoda" rank="genus">Muhaka forest</taxonomicName>
