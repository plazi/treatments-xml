<taxonomicName id="A4DEB23C17A61767955AF05C2E0307B6" authorityName="Zhang" authorityYear="2002" class="Insecta" family="Dermapteridae" genus="Sinopalaeodermata" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinopalaeodermata" order="Dermaptera" pageId="0" pageNumber="125" phylum="Arthropoda" rank="genus">Sinopalaeodermata</taxonomicName>
