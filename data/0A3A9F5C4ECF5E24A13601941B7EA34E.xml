<taxonomicName id="D7F7AEBC7B88987872A7328CEEF72CA9" class="Insecta" family="Dermapteridae" genus="Sinopalaeodermata" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinopalaeodermata neimonggolense" order="Dermaptera" pageId="0" pageNumber="125" phylum="Arthropoda" rank="species" species="neimonggolense">Sinopalaeodermata neimonggolense</taxonomicName>
