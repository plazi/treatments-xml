<taxonomicName id="4C6D4D5EEE643B00FE57FD3EFDC3FCC8" baseAuthorityName="Aurivillius" baseAuthorityYear="1882" box="[402,518,751,778]" class="Maxillopoda" family="Notodelphyidae" genus="Doropygella" kingdom="Animalia" order="Cyclopoida" pageId="309" pageNumber="310" phylum="Arthropoda" rank="species" species="thorelli">
<emphasis id="B919EACFEE643B00FE57FD3EFE63FCC8" box="[402,422,752,777]" italics="true" pageId="309" pageNumber="310">D</emphasis>
. 
<emphasis id="B919EACFEE643B00FE70FD21FDC3FCC8" box="[437,518,751,777]" italics="true" pageId="309" pageNumber="310">thorelli</emphasis>
</taxonomicName>
