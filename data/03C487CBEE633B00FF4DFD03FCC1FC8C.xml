<taxonomicName id="4C6D4D5EEE643B00FE31FD62FD88FD04" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[500,589,683,709]" class="Maxillopoda" family="Notodelphyidae" genus="Doropygella" kingdom="Animalia" order="Cyclopoida" pageId="309" pageNumber="310" phylum="Arthropoda" rank="species" species="calla" status="sp. nov.">
<emphasis id="B919EACFEE643B00FE31FD62FD88FD04" box="[500,589,683,709]" italics="true" pageId="309" pageNumber="310">D. calla</emphasis>
</taxonomicName>
<emphasis id="B919EACFEE643B00FD91FD65FD69FD04" bold="true" box="[596,684,683,709]" pageId="309" pageNumber="310">
