<taxonomicName id="4C608BEAFFD6AD05192D4AF8018997DD" authorityName="Meyrick" authorityYear="1908" box="[545,668,1948,1970]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="505" phylum="Arthropoda" rank="genus">
