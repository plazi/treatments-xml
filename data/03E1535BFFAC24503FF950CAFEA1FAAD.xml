<specimenCount id="9D4E29C4FFAC24503FAA5358FDF9FD2F" box="[502,576,639,663]" pageId="11" pageNumber="1840" type="generic">
<quantity id="4CB04FA8FFAC24503FAA5358FD85FD2F" box="[502,572,639,663]" metricMagnitude="2" metricUnit="m" metricValue="6.2" pageId="11" pageNumber="1840" unit="m" value="620.0">620 m</quantity>
