<number id="292F2487FFACB5719AE1F824FD0940E2" box="[672,728,2004,2030]" pageId="4" pageNumber="5" value="0.28">0.28</number>
, 
<number id="292F2487FFACB5719AA7F824FCC140E2" box="[742,784,2004,2030]" pageId="4" pageNumber="5" value="2.8">2.8</number>
; IV: 
<number id="292F2487FFACB5719B0DF824FC5540E2" box="[844,900,2004,2030]" pageId="4" pageNumber="5" value="1.22">1.22</number>
