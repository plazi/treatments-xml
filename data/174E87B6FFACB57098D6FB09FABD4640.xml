<number id="292F2487FFACB5719B89F824FBD140E2" box="[968,1024,2004,2030]" pageId="4" pageNumber="5" value="1.04">1.04</number>
, 
<number id="292F2487FFACB5719C4FF824FBE640E2" box="[1038,1079,2004,2030]" pageId="4" pageNumber="5" value="1.1">1.1</number>
