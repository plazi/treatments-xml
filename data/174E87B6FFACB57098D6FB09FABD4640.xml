<number id="292F2487FFACB57198D6F824FF1E40E2" box="[151,207,2004,2030]" pageId="4" pageNumber="5" value="1.04">1.04</number>
, 
<number id="292F2487FFACB571989CF824FEC440E2" box="[221,277,2004,2030]" pageId="4" pageNumber="5" value="0.28">0.28</number>
, 
<number id="292F2487FFACB5719963F824FE8A40E2" box="[290,347,2004,2030]" pageId="4" pageNumber="5" value="3.66">3.66</number>
