<taxonomicName id="4C204D1FFF9A2166D9E3FA69FE0A6470" box="[212,421,1535,1559]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="23" pageNumber="2805" phylum="Firmicutes" rank="species" species="heilongjiangensis">
