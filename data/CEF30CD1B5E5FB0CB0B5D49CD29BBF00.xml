<paragraph id="654D50185C5B653709D3BE092A549CBA" pageNumber="382">
<taxonomicName id="561F684D4A63919F5B200E4BAB7662F7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:226121" class="Insecta" family="Formicidae" genus="Meranoplus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Meranoplus convexius Schödl" order="Hymenoptera" pageNumber="382" phylum="Arthropoda" rank="species" species="convexius">Meranoplus convexius Schoedl</taxonomicName>
<taxonomicNameLabel id="5356A6BE36FDAAE29587C6067B73BE1A" rank="species">sp. n.</taxonomicNameLabel>
