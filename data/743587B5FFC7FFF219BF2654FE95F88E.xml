<emphasis id="CEE8EAB1FFC7FFF219BF26C4FEA0FA75" bold="true" box="[151,306,1485,1511]" pageId="2" pageNumber="31">
<typeStatus id="23278801FFC7FFF219BF26C4FF46FA75" box="[151,212,1485,1511]" pageId="2" pageNumber="31">Type</typeStatus>
species
</emphasis>
<taxonomicName id="3B9C4D20FFC7FFF2181126C4FD31FA75" authority="Rathbun, 1918" authorityName="Rathbun" authorityYear="1918" box="[313,675,1485,1511]" class="Malacostraca" family="Pinnotheridae" genus="Fabia" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="canfieldi">
<emphasis id="CEE8EAB1FFC7FFF2181126C4FE7FFA74" box="[313,493,1485,1510]" italics="true" pageId="2" pageNumber="31">Fabia canfieldi</emphasis>
<bibRefCitation id="980D4B52FFC7FFF218DC26C4FD31FA75" author="Rathbun" box="[500,675,1485,1511]" pageId="2" pageNumber="44" refString="Rathbun, M. J. (1918) The grapsoid crabs of America. Bulletin of the United States National Museum, 97, 1 - 461." type="journal article" year="1918">
Rathbun, 
<number id="4A542484FFC7FFF21B4E26C4FD31FA75" box="[614,675,1485,1511]" pageId="2" pageNumber="31" value="1918.0">1918</number>
</bibRefCitation>
</taxonomicName>
