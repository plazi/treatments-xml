species: 
<taxonomicName id="4C284D3CFF86FFC2FE842672F88CD55B" authority="Ewing, 1908" authorityName="Ewing" authorityYear="1908" box="[359,722,1319,1344]" class="Arachnida" family="Phthiracaridae" genus="Phthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageId="23" pageNumber="979" phylum="Arthropoda" rank="species" species="flavus">
<emphasis id="B95CEAADFF86FFC2FE842672F864D524" box="[359,570,1319,1343]" italics="true" pageId="23" pageNumber="979">Phthiracarus flavus</emphasis>
Ewing, 1908
</taxonomicName>
