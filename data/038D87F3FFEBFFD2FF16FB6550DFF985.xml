and 
<taxonomicName id="4C244D66FFEBFFD2FBCAFA5D56F2FA6E" authority="Hsiao &amp; Pollock, 2019" authorityName="Hsiao &amp; Pollock" authorityYear="2019" box="[1099,1235,1413,1437]" class="Insecta" family="Mycteridae" genus="Omineus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="534" phylum="Arthropoda" rank="species" species="chungae" status="n. sp.">
