<emphasis id="B9122551FF8BEE76FF57C52FFD57F9B6" bold="true" box="[151,685,1536,1562]" pageId="25" pageNumber="26">
Genus 
<taxonomicName id="4C6682C0FF8BEE76FF2EC52FFD57F9B6" authority="Park and Atyeo, 1971" authorityName="Park and Atyeo" authorityYear="1971" box="[238,685,1536,1562]" class="Arachnida" family="Proctophyllodidae" genus="Dolichodectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis id="B9122551FF8BEE76FF2EC52FFE62F9B6" bold="true" box="[238,408,1536,1562]" italics="true" pageId="25" pageNumber="26">Dolichodectes</emphasis>
Park and Atyeo, 
<number id="3DAEEB64FF8BEE76FDB2C52FFD57F9B6" box="[626,685,1536,1562]" pageId="25" pageNumber="26" value="1971.0">1971</number>
</taxonomicName>
