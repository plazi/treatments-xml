<quantity id="8E2F63DDFFD8200090484DBEFCCBD6CA" box="[822,881,1432,1455]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="6" unit="mm" value="2.0">2mm</quantity>
