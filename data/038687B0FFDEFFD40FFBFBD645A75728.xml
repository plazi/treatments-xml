<taxonomicName id="4C2F4D25FFDEFFD40EB1FB0F45275693" box="[477,558,1253,1277]" class="Insecta" family="Drilidae" genus="Selasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="564" phylum="Arthropoda" rank="genus">
<emphasis id="B95BEAB4FFDEFFD40EB1FB0F45275693" box="[477,558,1253,1277]" italics="true" pageId="1" pageNumber="564">Selasia</emphasis>
</taxonomicName>
differs from 
<taxonomicName id="4C2F4D25FFDEFFD40DB7FB0F447E5693" box="[731,887,1253,1277]" class="Insecta" family="Drilidae" genus="Malacogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="564" phylum="Arthropoda" rank="genus">
