<taxonomicName id="BEA84D20F1563260C4A5C9529B3C1B64" box="[401,617,1097,1123]" class="Liliopsida" family="Orchidaceae" genus="Aganisia" kingdom="Plantae" order="Asparagales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="species" species="ionoptera">
