<caption id="DF2C665CFFEBFFC3B1823811FEB3FEAC" ID-DOI="http://doi.org/10.5281/zenodo.4761807" ID-Zenodo-Dep="4761807" httpUri="https://zenodo.org/record/4761807/files/figure.png" pageId="4" pageNumber="317" startId="4.[151,264,1884,1908]" targetBox="[159,1429,349,1861]" targetPageId="4">
