<taxonomicName id="4C3C898CFFF84E3372B2F9CA8419CE20" authority="J. Joling, Pereira" authorityName="J. Joling, Pereira" box="[168,576,1543,1567]" class="Liliopsida" family="Araceae" genus="Pothos" kingdom="Plantae" order="Alismatales" pageId="25" pageNumber="956" phylum="Tracheophyta" rank="species" species="fractiflexus">
<emphasis id="B9482E1DFFF84E3372B2F9CA8722CE20" bold="true" box="[168,379,1543,1567]" italics="true" pageId="25" pageNumber="956">Pothos fractiflexus</emphasis>
