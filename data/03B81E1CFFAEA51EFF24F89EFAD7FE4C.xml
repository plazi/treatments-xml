<collectingDate id="EFEB7022FFACA51FFEE7F813FE54F843" box="[340,443,2015,2041]" pageId="2" pageNumber="141" value="2018-05-04">2018.V.4</collectingDate>
</date>
, 
<collectingMethod id="5250D71DFFACA51FFE75F813FDC1F840" box="[454,558,2015,2042]" pageId="2" pageNumber="141">light trap</collectingMethod>
, 
<quantity id="4CE902EFFFACA51FFD89F812FD6CF840" box="[570,643,2014,2042]" metricMagnitude="2" metricUnit="m" metricValue="5.9799999999999995" pageId="2" pageNumber="141" unit="m" value="598.0">
<elevation id="003C4839FFACA51FFD89F812FD6CF840" box="[570,643,2014,2042]" metricMagnitude="2" metricUnit="m" metricValue="5.9799999999999995" pageId="2" pageNumber="141" unit="m" value="598.0">598 m</elevation>
