— 
<collectingCountry id="1293E383F102FFBBFEFA9E6B9189F712" box="[373,468,515,541]" name="Vietnam" pageId="8" pageNumber="265">Vietnam</collectingCountry>
(
<collectingRegion id="A8406DF1F102FFBBFE6D9E6B92E6F711" box="[482,699,515,542]" country="Vietnam" name="Lai Chau" pageId="8" pageNumber="265">Lai Chau Province</collectingRegion>
