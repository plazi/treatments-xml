<taxonomicName id="07B84D3EFFF2FFC0FF0BFBCCD596AAD2" authority="Ghauri, 1975" authorityName="Ghauri" authorityYear="1975" box="[151,491,1132,1158]" class="Insecta" family="Cicadellidae" genus="Tasnimocerus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="284" phylum="Arthropoda" rank="genus">
