<taxonomicName id="4C264D63FFC5A142FC90FC9CF911FC80" authority="FROM" authorityName="FROM" box="[820,1026,787,813]" class="Magnoliopsida" family="Cunoniaceae" genus="Geissois" kingdom="Plantae" order="Oxalidales" pageId="11" pageNumber="322" phylum="Tracheophyta" rank="genus">
<emphasis id="B952EAF2FFC5A142FC90FC9CFEBAFC80" box="[820,937,787,813]" italics="true" pageId="11" pageNumber="322">GEISSOIS</emphasis>
FROM
</taxonomicName>
<collectingCountry id="F3317670FFC5A142FBADFC9CF925FC80" box="[1033,1078,787,813]" name="Fiji" pageId="11" pageNumber="322">FIJI</collectingCountry>
</heading>
</paragraph>
