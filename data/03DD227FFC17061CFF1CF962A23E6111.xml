<collectingMunicipality id="6BAF0913FC16061CFD02FDA6A49D6745" box="[662,725,555,577]" pageId="7" pageNumber="61">Hedge</collectingMunicipality>
and 
<location id="8EABC5B2FC16061CFC92FDA6A5216745" LSID="urn:lsid:plazi:treatment:03DD227FFC17061CFF1CF962A23E6111:8EABC5B2FC16061CFC92FDA6A5216745" box="[774,873,555,577]" municipality="Hedge" name="Lammond" pageId="7" pageNumber="61" stateProvince="Tbilisi">Lammond</location>
