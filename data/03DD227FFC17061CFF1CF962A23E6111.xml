<collectionCode id="ED650BACFC16061CFE98FDA1A76B6745" box="[268,291,556,577]" country="Austria" lsid="urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="7" pageNumber="61" type="Herbarium">W</collectionCode>
