<collectorName id="2681F6BFFC16061CFE0DFDC2A7AC6760" box="[409,484,591,612]" pageId="7" pageNumber="61">Stevens</collectorName>
<typeStatus id="54CF2DCBFC16061CFE7EFDDDA47D6761" box="[490,565,592,613]" pageId="7" pageNumber="61">syntype</typeStatus>
