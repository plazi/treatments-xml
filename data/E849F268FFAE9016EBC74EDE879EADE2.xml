<paragraph id="605F437EFFAF9014EBC74C2086A4AE1D" blockId="1.[151,964,1092,1114]" box="[151,964,1092,1114]" pageId="1" pageNumber="383">
<emphasis id="52949F6CFFAF9014EBC74C208473AE1E" bold="true" box="[151,275,1092,1114]" pageId="1" pageNumber="383">FIGURE 1.</emphasis>
<taxonomicName id="A7E038FDFFAF9014EA494C20849AAE1D" box="[281,506,1092,1114]" class="Insecta" family="Gomphidae" genus="Leptogomphus" kingdom="Animalia" order="Odonata" pageId="1" pageNumber="383" phylum="Arthropoda" rank="species" species="tioman">
