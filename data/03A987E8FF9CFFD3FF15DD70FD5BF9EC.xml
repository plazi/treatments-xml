<taxonomicName id="4C004D7DFF9DFFD3FED9DE58FDCCFAE8" authorityName="Boheman" authorityYear="1854" box="[347,612,1340,1364]" class="Insecta" family="Chrysomelidae" genus="Cassida" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="quadriplagiata">
