<taxonomicName id="78101315B4F456E81669491D91915B2F" authorityName="Baker" authorityYear="1917" class="Insecta" family="Braconidae" genus="Colastomion" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Colastomion" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
