, 
<collectingCountry id="F323761EFF96FFA5FCFC2F84B8E2FA2B" box="[882,949,1473,1498]" name="China" pageId="5">China</collectingCountry>
(
<figureCitation id="130F2A0BFF96FFA5FC4B2F84BF4FFA2B" box="[965,1048,1473,1498]" captionStart="FIGURE 16" captionStartId="19.[151,250,1289,1311]" captionTargetBox="[164,1418,203,1257]" captionTargetId="figure@19.[153,1433,193,1268]" captionTargetPageId="19" captionText="FIGURE 16. Distribution map of Hahnia species from Gaoligong Mountains." httpUri="https://zenodo.org/record/1043683/files/figure.png" pageId="5">Fig. 16</figureCitation>
