<number id="A04B0415FFF2FFF1FEA6FD0DC517FCAC" box="[273,335,759,785]" pageId="36" pageNumber="37" value="1999.0">1999</number>
</bibRefCitation>
, 
<bibRefCitation id="72126BC3FFF2FFF1FEECFD0DC5C2FCAC" author="Bolton" box="[347,410,759,785]" pageId="36" pageNumber="37" refString="Bolton, B. (2000) The ant tribe Dacetini. Memoirs of the American Entomological Institute, 65, 1 - 1028." type="journal article" year="2000">
