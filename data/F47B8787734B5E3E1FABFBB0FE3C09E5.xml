<quantity id="BB2A9B74734A5E3E1A72FE24FA7B0AB0" box="[1358,1436,440,464]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="310" unit="m" value="2000.0">2000m</quantity>
