<quantity id="BB2A9B74734A5E3E1A3BFE40FAA00A94" box="[1287,1351,476,500]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="9" pageNumber="310" unit="m" value="800.0">800m</quantity>
