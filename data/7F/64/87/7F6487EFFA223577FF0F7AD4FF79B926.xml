<quantity box="[619,754,295,322]" metricMagnitude="-3" metricUnit="m" metricValue="5.55" metricValueMax="5.8" metricValueMin="5.3" pageId="2" pageNumber="413" unit="mm" value="5.55" valueMax="5.8" valueMin="5.3">5.3–5.8 mm</quantity>
