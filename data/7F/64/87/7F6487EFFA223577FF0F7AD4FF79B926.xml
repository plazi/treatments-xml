<quantity box="[775,910,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="6.85" metricValueMax="6.9" metricValueMin="6.8" pageId="2" pageNumber="413" unit="mm" value="6.85" valueMax="6.9" valueMin="6.8">6.8–6.9 mm</quantity>
(N = 6), female 
<quantity box="[1096,1231,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="7.15" metricValueMax="7.5" metricValueMin="6.8" pageId="2" pageNumber="413" unit="mm" value="7.15" valueMax="7.5" valueMin="6.8">6.8–7.5 mm</quantity>
(N = 8); forewing length: male 
<quantity box="[297,432,295,322]" metricMagnitude="-3" metricUnit="m" metricValue="5.4" metricValueMax="5.5" metricValueMin="5.3" pageId="2" pageNumber="413" unit="mm" value="5.4" valueMax="5.5" valueMin="5.3">5.3–5.5 mm</quantity>
