<materialsCitation ID-GBIF-Occurrence="1836204071" collectingDate="1960-10" collectionCode="ARUAN, MZSP" country="Brazil" location="NO" pageId="14" pageNumber="63" specimenCount="1" stateProvince="NO" typeStatus="holotype">
