<emphasis bold="true" box="[321,359,1943,1964]" pageId="5" pageNumber="54">1–5</emphasis>
, 
<taxonomicName baseAuthorityName="Erichson" baseAuthorityYear="1848" box="[372,563,1943,1965]" class="Insecta" family="Cerambycidae" genus="Dorcasta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="dasycera">
