<taxonomicName authorityName="Breuning" authorityYear="1940" box="[774,956,1973,1996]" class="Insecta" family="Cerambycidae" genus="Dorcasta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="54" phylum="Arthropoda" rank="species" species="borealis">
