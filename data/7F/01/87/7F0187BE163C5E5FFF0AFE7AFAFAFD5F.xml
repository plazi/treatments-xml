<emphasis bold="true" box="[841,854,1943,1964]" pageId="5" pageNumber="54">2</emphasis>
, ventral habitus, female; 
<emphasis bold="true" box="[1113,1125,1943,1964]" pageId="5" pageNumber="54">3</emphasis>
