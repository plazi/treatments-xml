, 
<taxonomicName box="[300,515,834,859]" class="Magnoliopsida" family="Orobanchaceae" genus="Cistanche" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="370" phylum="Tracheophyta" rank="species" species="tubulosa">
<emphasis box="[300,515,834,859]" italics="true" pageId="3" pageNumber="370">Cistanche tubulosa</emphasis>
</taxonomicName>
, 
<taxonomicName box="[530,741,834,859]" class="Magnoliopsida" family="Zygophyllaceae" genus="Fagonia" kingdom="Plantae" order="Zygophyllales" pageId="3" pageNumber="370" phylum="Tracheophyta" rank="species" species="bruguyeri">
