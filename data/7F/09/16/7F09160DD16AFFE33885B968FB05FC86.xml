<date box="[583,604,2008,2038]" pageId="0" pageNumber="53">
<emphasis box="[583,596,2008,2033]" italics="true" pageId="0" pageNumber="53">v</emphasis>
<number box="[596,604,2024,2038]" pageId="0" pageNumber="53" value="2.0">2</number>
