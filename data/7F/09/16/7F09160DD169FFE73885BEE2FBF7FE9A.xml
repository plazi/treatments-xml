<date box="[846,868,1886,1915]" pageId="3" pageNumber="56">
<emphasis box="[846,859,1886,1911]" italics="true" pageId="3" pageNumber="56">v</emphasis>
<number box="[860,868,1901,1915]" pageId="3" pageNumber="56" value="2.0">2</number>
</date>
