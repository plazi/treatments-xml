[
<taxonomicName box="[739,904,1791,1815]" class="Magnoliopsida" family="Polygonaceae" kingdom="Plantae" order="Caryophyllales" pageId="16" pageNumber="57" phylum="Tracheophyta" rank="family">Polygonaceae</taxonomicName>
