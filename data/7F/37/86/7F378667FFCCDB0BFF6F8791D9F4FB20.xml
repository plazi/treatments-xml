<quantity box="[856,963,690,714]" metricMagnitude="-5" metricUnit="m" metricValue="8.0" pageId="6" pageNumber="47" unit="mm" value="0.08">0.08 mm</quantity>
, II 
<quantity box="[1011,1118,690,714]" metricMagnitude="-4" metricUnit="m" metricValue="6.1" pageId="6" pageNumber="47" unit="mm" value="0.61">0.61 mm</quantity>
, III 
<quantity box="[1176,1283,690,714]" metricMagnitude="-4" metricUnit="m" metricValue="1.7" pageId="6" pageNumber="47" unit="mm" value="0.17">0.17 mm</quantity>
