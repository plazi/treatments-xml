<taxonomicName box="[714,774,1418,1442]" class="Magnoliopsida" family="Ehretiaceae" genus="Bourreria" kingdom="Plantae" order="Boraginales" pageId="10" pageNumber="147" phylum="Tracheophyta" rank="species" species="glabra">glabra</taxonomicName>
; petiolo 
<quantity box="[854,943,1417,1441]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="10" pageNumber="147" unit="mm" value="7.5" valueMax="10.0" valueMin="5.0">5-10 mm</quantity>
longo. Inflorescentiae terminale, cymosae, usque ad 
<quantity box="[900,948,1443,1466]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" pageId="10" pageNumber="147" unit="cm" value="5.0">5 cm</quantity>
latae. 
<collectingCountry box="[1019,1075,1444,1468]" name="Indonesia" pageId="10" pageNumber="147">Flores</collectingCountry>
