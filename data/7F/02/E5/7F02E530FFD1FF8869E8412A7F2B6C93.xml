<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Aves" family="Anatidae" genus="Spatula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anseriformes" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="querquedula">
<emphasis italics="true" pageId="36" pageNumber="37">Spatula querquedula</emphasis>
