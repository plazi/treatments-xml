<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[860,1043,798,824]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="caeruleus">
