(
<bibRefCitation author="Balat, F." box="[386,514,1590,1616]" pageId="54" pageNumber="55" pagination="45 - 52" refId="ref45138" refString="Balat, F. (1977) Enumeratio insectorum Bohemoslovakiae: Mallophaga. Acta Faunistica Entomologica Musei Nationalis Prague, 15 (Supplementum 4), 45 - 52. [in German]" type="journal article" year="1977">Balát 1977</bibRefCitation>
); 
<location LSID="urn:lsid:plazi:treatment:7F02E530FFC4FF9A69E843B77FA46A8A:F27402FDFFC4FF9A6B69404A7E536A3A" box="[534,630,1590,1616]" country="Slovakia" name="Blatnica" pageId="54" pageNumber="55">Blatnica</location>
, 
<date box="[641,776,1590,1617]" pageId="54" pageNumber="55" value="1979-05-04">
<collectingDate box="[641,776,1590,1617]" pageId="54" pageNumber="55" value="1979-05-04">4 May 1979</collectingDate>
