<number box="[447,461,727,751]" pageId="9" pageNumber="10" value="1.0">1</number>
<number box="[469,553,727,751]" pageId="9" pageNumber="10" value="0.55" valueMax="0.6" valueMin="0.5">0.5–0.6</number>
length of protarsomere 
<number box="[827,844,727,751]" pageId="9" pageNumber="10" value="2.0">2</number>
; apical spine of tarsomere 
<number box="[1150,1164,727,751]" pageId="9" pageNumber="10" value="2.0">2</number>
<number box="[1172,1256,727,751]" pageId="9" pageNumber="10" value="0.7" valueMax="0.8" valueMin="0.6">0.6–0.8</number>
