<emphasis box="[1105,1159,1069,1093]" italics="true" pageId="17" pageNumber="18">
<number box="[1105,1118,1069,1093]" pageId="17" pageNumber="18" value="2.0">2</number>
r-m
</emphasis>
cell equal to 
<emphasis box="[1307,1360,1069,1093]" italics="true" pageId="17" pageNumber="18">
<number box="[1307,1320,1069,1093]" pageId="17" pageNumber="18" value="3.0">3</number>
r-m
</emphasis>
cell in width in 
<taxonomicName box="[250,463,1105,1129]" class="Insecta" family="Pompilidae" genus="Arachnospila" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="eoabnormis" subGenus="Ammosphex">
