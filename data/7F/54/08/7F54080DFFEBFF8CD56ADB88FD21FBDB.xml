<subSubSection pageId="8" pageNumber="377" type="diagnosis">
<paragraph blockId="8.[264,1325,283,1070]" pageId="8" pageNumber="377">
<emphasis bold="true" box="[312,442,283,309]" pageId="8" pageNumber="377">Diagnosis:</emphasis>
<taxonomicName box="[454,687,284,309]" class="Insecta" family="Coleoptera" genus="Laena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="377" phylum="Arthropoda" rank="species" species="champasaka" status="n. sp.">
