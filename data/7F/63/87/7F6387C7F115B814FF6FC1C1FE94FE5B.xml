<number box="[181,212,223,247]" pageId="13" pageNumber="14" value="37.0">37</number>
; VI, 
<number box="[267,298,223,247]" pageId="13" pageNumber="14" value="31.0">31</number>
; VII, 
<number box="[362,393,223,247]" pageId="13" pageNumber="14" value="19.0">19</number>
; VIII, 
<number box="[467,484,223,247]" pageId="13" pageNumber="14" value="6.0">6</number>
