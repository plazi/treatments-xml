, 
<number box="[740,855,1116,1140]" pageId="1" pageNumber="2" value="0.11" valueMax="0.12" valueMin="0.1">0.10–0.12</number>
; MW, 
<number box="[935,1051,1116,1140]" pageId="1" pageNumber="2" value="0.345" valueMax="0.35" valueMin="0.34">0.34–0.35</number>
; 
