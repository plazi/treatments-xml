, 
<number box="[1155,1271,1116,1140]" pageId="1" pageNumber="2" value="0.14500000000000002" valueMax="0.15" valueMin="0.14">0.14–0.15</number>
; 
<collectionCode box="[1285,1359,1116,1140]" pageId="1" pageNumber="2">AWIV</collectionCode>
, 
<number pageId="1" pageNumber="2" value="0.435" valueMax="0.44" valueMin="0.43">0.43– 0.44</number>
