<number box="[640,671,295,319]" pageId="16" pageNumber="17" value="25.0">25</number>
; V, 
<number box="[716,747,295,319]" pageId="16" pageNumber="17" value="30.0">30</number>
; VI, 
<number box="[804,835,295,319]" pageId="16" pageNumber="17" value="26.0">26</number>
