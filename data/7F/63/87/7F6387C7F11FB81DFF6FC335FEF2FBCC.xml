, 
<number box="[831,883,727,751]" pageId="7" pageNumber="8" value="0.11">0.11</number>
; MW, 
<number box="[959,1011,727,751]" pageId="7" pageNumber="8" value="0.41">0.41</number>
; 
<collectionCode box="[1024,1100,727,751]" pageId="7" pageNumber="8">MSPL</collectionCode>
, 
<number box="[1112,1164,727,751]" pageId="7" pageNumber="8" value="0.14">0.14</number>
; 
