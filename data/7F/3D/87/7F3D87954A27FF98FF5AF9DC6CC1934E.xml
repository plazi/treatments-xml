</taxonomicName>
species group (
<taxonomicName box="[687,775,1628,1655]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="jendeki">
