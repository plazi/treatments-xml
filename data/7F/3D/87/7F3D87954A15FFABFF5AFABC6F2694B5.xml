<subSubSection pageId="13" pageNumber="14" type="etymology">
<paragraph blockId="13.[151,1436,152,251]" pageId="13" pageNumber="14">
<emphasis bold="true" box="[199,334,189,215]" pageId="13" pageNumber="14">Etymology.</emphasis>
