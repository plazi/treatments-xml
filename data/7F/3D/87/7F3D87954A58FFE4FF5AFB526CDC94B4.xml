<taxonomicName authority="jendeki" authorityName="jendeki Masumoto" authorityYear="1998" box="[443,851,1830,1856]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="subSpecies" species="jendeki" subSpecies="jendeki">
