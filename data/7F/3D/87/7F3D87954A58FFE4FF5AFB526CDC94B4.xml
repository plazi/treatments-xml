resembles 
<taxonomicName box="[980,1103,1831,1857]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="crassus">
