<emphasis bold="true" box="[151,528,1788,1815]" italics="true" pageId="8" pageNumber="9">Morphostenophanes aenescens</emphasis>
</taxonomicName>
species group (
<taxonomicName box="[721,843,1789,1815]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="aenescens">
