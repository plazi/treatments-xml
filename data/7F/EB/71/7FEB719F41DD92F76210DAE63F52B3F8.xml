<paragraph pageId="125" pageNumber="126">
<taxonomicName authorityName="Germar" authorityYear="1840" class="Insecta" family="CARABIDAE" genus="Rhysodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="125" pageNumber="126" phylum="Arthropoda" rank="species" species="conjungens">Rhysodes conjungens</taxonomicName>
