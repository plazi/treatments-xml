<number box="[436,450,1988,2013]" pageId="2" pageNumber="49" value="4.0">4</number>
<number box="[457,472,1988,2013]" pageId="2" pageNumber="49" value="5.0">5</number>
y a (
<number box="[530,543,1988,2013]" pageId="2" pageNumber="49" value="1.0">1</number>
&lt;
<number box="[561,577,1988,2013]" pageId="2" pageNumber="49" value="3.0">3</number>
