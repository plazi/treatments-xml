<taxonomicName authority="Josifor, 1974: 63" authorityName="Josifor" authorityPageNumber="63" authorityYear="1974" box="[333,433,1205,1228]" class="Insecta" family="Miridae" genus="Leucodellus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="nathaliae">nathaliae</taxonomicName>
