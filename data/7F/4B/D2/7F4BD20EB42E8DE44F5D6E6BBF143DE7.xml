<geoCoordinate degrees="32" direction="north" minutes="31" orientation="latitude" precision="925" value="32.516666">32°31'N</geoCoordinate>
, 
<geoCoordinate degrees="80" direction="east" minutes="04" orientation="longitude" precision="925" value="80.066666">80°04'E</geoCoordinate>
