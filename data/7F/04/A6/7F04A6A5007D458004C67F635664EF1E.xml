<taxonomicName class="Hexapoda" family="Carabidae" genus="Cymindis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="796" pageNumber="797" phylum="Arthropoda" rank="species" species="alternans">
