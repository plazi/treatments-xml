<collectingCountry box="[1081,1190,1474,1499]" name="Mongolia" pageId="3" pageNumber="311">Mongolia</collectingCountry>
(
<bibRefCitation author="Josifov, M. &amp; Kerzhner, I. M." pageId="3" pageNumber="311" pagination="1 - 15" refId="ref17805" refString="Josifov, M. &amp; Kerzhner, I. M. (1967) Heteroptera von der Mongolischen Volksrepublik. Fragmenta Faunistica, 14 (1), 1 - 15. https: // doi. org / 10.3161 / 00159301 FF 1967.14.1.001" type="journal article" year="1967">Josifov &amp; Kerzhner 1967</bibRefCitation>
, as 
<taxonomicName authorityName=": Hua" authorityYear="2000" box="[260,373,1511,1534]" class="Insecta" family="Rhopalidae" genus="Agraphopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="311" phylum="Arthropoda" rank="species" species="majorinus">
<emphasis box="[260,373,1511,1534]" italics="true" pageId="3" pageNumber="311">majorinus</emphasis>
</taxonomicName>
; 
<bibRefCitation author="Kiritshenko, A. N. &amp; Kerzhner, I. M." box="[393,744,1510,1535]" pageId="3" pageNumber="311" pagination="383 - 428" refId="ref18035" refString="Kiritshenko, A. N. &amp; Kerzhner, I. M. (1972) Land Heteroptera of the Mongolian People's Republic. I. Nasekomye Mongolii, 1 (1), 383 - 428. [in Russian]" type="journal article" year="1972">Kiritshenko &amp; Kerzhner 1972</bibRefCitation>
, 
<bibRefCitation author="Kiritshenko, A. N. &amp; Kerzhner, I. M." box="[759,815,1510,1535]" pageId="3" pageNumber="311" pagination="87 - 114" refId="ref18077" refString="Kiritshenko, A. N. &amp; Kerzhner, I. M. (1976) Land Heteroptera of the Mongolian People's Republic. III. Nasekomye Mongolii, 4, 87 - 114. [in Russian]" type="journal article" year="1976">1976</bibRefCitation>
etc.) and 
<collectingCountry box="[939,1010,1510,1535]" name="China" pageId="3" pageNumber="311">China</collectingCountry>
: Turkestan [= 
<collectingRegion box="[1184,1285,1510,1535]" country="China" name="Xinjiang Uygur" pageId="3" pageNumber="311">Xinjiang</collectingRegion>
] (
<bibRefCitation author="Wu, C. F." box="[1311,1423,1510,1535]" pageId="3" pageNumber="311" refId="ref20308" refString="Wu, C. F. (1935) Catalogus insectorum sinensium. Vol. II. Fan Memorial Institute of Biology, Peiping, 634 pp." type="book" year="1935">Wu 1935</bibRefCitation>
), 
<collectingRegion box="[151,254,1546,1571]" country="China" name="Xinjiang Uygur" pageId="3" pageNumber="311">Xinjiang</collectingRegion>
: western part (
<bibRefCitation author="Hsiao, T. Y." box="[419,565,1546,1571]" pageId="3" pageNumber="311" pagination="49 - 64" refId="ref17360" refString="Hsiao, T. Y. (1965 a) A short essay on Chinese Coreidae (Hemiptera, Heteroptera) III, Pseudophloeinae and Rhopalinae. Acta Scientiarum Naturalium Universitatis Nan Kaiensis, 6 (1), 49 - 64. [in Chinese with English summary, August 1965]" type="journal article" year="1965">Hsiao 1965a</bibRefCitation>
; not included in 
<bibRefCitation author="Hsiao, T. Y." box="[755,888,1546,1571]" pageId="3" pageNumber="311" pagination="33 - 50" refId="ref17457" refString="Hsiao, T. Y. (1977) Coreidae. In: Hsiao, T. Y., Ren, S. Z., Zheng, L. Y., Jing, H. L. &amp; Liu, S. L. (Eds.), A handbook for the determination of the Chinese Hemiptera-Heteroptera. Vol. I. Science Press, Beijing, pp. 198 - 279 + 304, pls. 33 - 50. [in Chinese with English summary]" type="journal article" year="1977">Hsiao 1977</bibRefCitation>
