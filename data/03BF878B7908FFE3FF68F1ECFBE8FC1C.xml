<paragraph id="8BA9369D790FFFE2FF68F2FBFDCEFE96" blockId="4.[178,1085,218,270]" pageId="4" pageNumber="481">
<specimenCount id="9D10FD14790FFFE2FF68F2FBFE95FF68" box="[178,325,219,240]" pageId="4" pageNumber="481" type="generic" typeStatus="paratypes">two paratypes</specimenCount>
