<taxonomicName id="D29F3C553009B5F1709BBE98E07FBBD5" class="Magnoliopsida" family="Fabaceae" genus="Diphysa" higherTaxonomySource="CoL" kingdom="Plantae" order="Fabales" pageNumber="89" phylum="Tracheophyta" rank="genus">Diphysa</taxonomicName>
