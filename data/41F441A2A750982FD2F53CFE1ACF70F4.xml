<taxonomicName id="018AF6F0BF6C7F2D9AB609DD8253310F" class="Pucciniomycetes" family="Pucciniaceae" genus="Uromyces" higherTaxonomySource="CoL" kingdom="Fungi" order="Pucciniales" pageNumber="89" phylum="Basidiomycota" rank="species" species="diphysae">U. diphysae</taxonomicName>
