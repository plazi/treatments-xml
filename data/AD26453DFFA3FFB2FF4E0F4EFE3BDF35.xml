<taxonomicName id="E28F8FA8FFA1FFBDFEFE0BE3FCC5D88E" authority="Holovachov &amp; Boström, 2013" authorityName="Holovachov &amp; Boström" authorityYear="2013" box="[295,511,1708,1732]" class="Adenophorea" family="Leptolaimidae" genus="Leptolaimus" kingdom="Animalia" order="Araeolaimida" pageId="7" pageNumber="8" phylum="Nematoda" rank="species" species="primus" status="sp. n.">
<emphasis id="17FB2839FFA1FFBDFEFE0BE3FCC5D88E" box="[295,511,1708,1732]" italics="true" pageId="7" pageNumber="8">Leptolaimus primus</emphasis>
</taxonomicName>
<emphasis id="17FB2839FFA1FFBDFDD30BE2FF73D88E" bold="true" box="[522,585,1709,1732]" pageId="7" pageNumber="8">
<taxonomicNameLabel id="0CC89542FFA1FFBDFDD30BE2FF73D88E" box="[522,585,1709,1732]" pageId="7" pageNumber="8" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
(A–B), 
<taxonomicName id="E28F8FA8FFA1FFBDFD730BE3F97CD88E" authority="de Man, 1876" authorityName="de Man" authorityYear="1876" box="[682,1094,1708,1732]" class="Adenophorea" family="Leptolaimidae" genus="Leptolaimus" kingdom="Animalia" order="Araeolaimida" pageId="7" pageNumber="8" phylum="Nematoda" rank="species" species="papilliger">
<emphasis id="17FB2839FFA1FFBDFD730BE3FE9BD88E" box="[682,929,1708,1732]" italics="true" pageId="7" pageNumber="8">Leptolaimus papilliger</emphasis>
<bibRefCitation id="411E89DAFFA1FFBDFC720BE2F97CD88E" author="de Man, J. G." box="[939,1094,1709,1732]" pageId="7" pageNumber="8" pagination="78 - 196" refId="ref48468" refString="de Man, J. G. (1876) Onderzoekingen over vrij in de aarde levende Nematoden. Tijdschrift der Nederlandsche dierkundige Vereeniging, 2, 78 - 196." type="journal article" year="1876">de Man, 1876</bibRefCitation>
</taxonomicName>
(C–D), 
<taxonomicName id="E28F8FA8FFA1FFBDFB7F0BE3FC04D8AE" authority="Lorenzen, 1972" authorityName="Lorenzen" authorityYear="1972" class="Adenophorea" family="Leptolaimidae" genus="Leptolaimus" kingdom="Animalia" order="Araeolaimida" pageId="7" pageNumber="8" phylum="Nematoda" rank="species" species="cupulatus">
<emphasis id="17FB2839FFA1FFBDFB7F0BE3F8A6D88E" box="[1190,1436,1708,1732]" italics="true" pageId="7" pageNumber="8">Leptolaimus cupulatus</emphasis>
Lorenzen, 1972
</taxonomicName>
(E–F) and 
