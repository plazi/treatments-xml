<taxonomicName id="893C70E5683BC857B49D95F1345D1280" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:150763" class="Insecta" family="Formicidae" genus="Oecophylla" kingdom="Animalia" lsidName="Oecophylla longinoda rufescens Santschi" lsidName-HNS="Oecophylla longinoda rufescens Santschi" order="Hymenoptera" pageNumber="212" phylum="Arthropoda" rank="variety" species="longinoda" variety="rufescens">Oecophylla longinoda Lar. v. rufescens</taxonomicName>
