<quantity id="7D4879BFFF3AFF00353C574D25EAFDE1" box="[579,665,583,608]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="208" pageNumber="209" unit="mm" value="3.5">3.5 mm</quantity>
(
<specimenCount id="ACB61FD3FF3AFF0035D6574225B3FDDE" box="[681,704,584,608]" pageId="208" pageNumber="209" type="male">♂</specimenCount>
), 
<quantity id="7D4879BFFF3AFF0035AA574D2458FDE1" box="[725,811,583,608]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="208" pageNumber="209" unit="mm" value="3.5">3.5 mm</quantity>
(
<specimenCount id="ACB61FD3FF3AFF00344257422423FDDE" box="[829,848,584,608]" pageId="208" pageNumber="209" type="female">♀</specimenCount>
