<geoCoordinate id="EE0D5044FFE54B06FF3A8D5BD943F92F" box="[163,311,1646,1668]" degrees="43" direction="north" minutes="19" orientation="latitude" pageId="6" pageNumber="742" precision="1" seconds="41.5" value="43.328194">43°19’41.5”N</geoCoordinate>
