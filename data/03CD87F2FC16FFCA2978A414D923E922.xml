<bibRefCitation id="EFF54B15FC10FFCA29E1A0C8DF00EEC6" author="Ramellini P." box="[665,852,967,993]" pageId="61" pageNumber="62" pagination="103 - 114" refId="ref67030" refString="Ramellini P. 2000. Note su Campodeidi del Piemonte (Diplura, Campodeidae). Rivista Piemontese di Storia Naturale 21: 103 - 114." type="journal article" year="2000">Ramellini 2000</bibRefCitation>
), 
<collectingCountry id="F3737674FC10FFCA2813A0C8DFEEEEC6" box="[875,954,967,993]" name="France" pageId="61" pageNumber="62">France</collectingCountry>
(
<bibRefCitation id="EFF54B15FC10FFCA28B4A0C8D824EEC6" author="Conde B." box="[972,1136,967,993]" pageId="61" pageNumber="62" pagination="85 - 92" refId="ref64087" refString="Conde B. 1947 d. Quelques campodeides du nord-est de la France. Bulletin Societe Sciences Nancy nouvelle serie 6: 85 - 92." type="journal article" year="1947">Condé 1947d</bibRefCitation>
, 
<bibRefCitation id="EFF54B15FC10FFCA2F07A0C8D89FEEC6" author="Conde B." box="[1151,1227,967,993]" pageId="61" pageNumber="62" pagination="139 - 144" refId="ref64196" refString="Conde B. 1947 h. Nouvelles stations francaises de campodeides avec description d'une forme nouvelle. Annales des Sciences naturelles Zool o gie 11 eme serie (9): 139 - 144." type="journal article" year="1947">1947h</bibRefCitation>
, 
<bibRefCitation id="EFF54B15FC10FFCA2FA1A0C8D977EEC5" author="Conde B." box="[1241,1315,967,994]" pageId="61" pageNumber="62" pagination="128 - 132" refId="ref64407" refString="Conde B. 1950 a. Campodeides du Var et des Alpes-Maritimes. Bulletin de la Societe linneenne de Lyon 6: 128 - 132." type="journal article" year="1950">1950a</bibRefCitation>
; 
<bibRefCitation id="EFF54B15FC10FFCA2E4BA0C8DCAEE922" author="Pages J." pageId="61" pageNumber="62" pagination="1 - 97" refId="ref66726" refString="Pages J. 1951. Contribution a la connaissance des Diploures. Supplement du Bulletin scientifique de Bourgogne 9: 1 - 97." type="journal article" year="1951">Pagés 1951</bibRefCitation>
), 
<collectingCountry id="F3737674FC10FFCA2A76A0E4DD2EE922" box="[270,378,1003,1029]" name="Germany" pageId="61" pageNumber="62">Germany</collectingCountry>
(
<bibRefCitation id="EFF54B15FC10FFCA2AF0A0E5DE62E922" author="Christian E." box="[392,566,1002,1029]" pageId="61" pageNumber="62" pagination="26 - 32" refId="ref63839" refString="Christian E. 2003. Checklist of the Diplura of Germany. Entomofauna Germanica 6: 26 - 32." type="journal article" year="2003">Christian 2003</bibRefCitation>
) and around the Dinaric Mountains (
<bibRefCitation id="EFF54B15FC10FFCA28A3A0E5D821E922" author="Blesic B." box="[987,1141,1002,1029]" pageId="61" pageNumber="62" pagination="87 - 90" refId="ref63510" refString="Blesic B. 1998 a. Investigations of Protura and Diplura of South Serbia. Proceedings for Natural Sciences Matica Srpska 94: 87 - 90." type="journal article" year="1998">Blesić 1998a</bibRefCitation>
