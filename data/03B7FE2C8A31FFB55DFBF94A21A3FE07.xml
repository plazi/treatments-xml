<taxonomicName id="4C1E34B98A31FFB65C61F94A20E1F8A8" authority="Neboiss, 1984" authorityName="Neboiss" authorityYear="1984" box="[295,721,1767,1802]" class="Insecta" family="Hydropsychidae" genus="Baliomorpha" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="88" phylum="Arthropoda" rank="genus">
