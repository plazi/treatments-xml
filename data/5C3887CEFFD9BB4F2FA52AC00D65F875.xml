<taxonomicName id="13914D5BFFD9BB4F2F812AC00B28F956" authority="Costa, 1853" authorityName="Costa" authorityYear="1853" box="[179,546,1727,1753]" class="Malacostraca" family="Dexaminidae" genus="Nototropis" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="382" phylum="Arthropoda" rank="species" species="guttatus">
