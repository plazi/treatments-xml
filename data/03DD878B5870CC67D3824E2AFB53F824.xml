<taxonomicName id="4C744D1E5870CC67D0854B14FA4FF94C" authority="Krapp-Schickel &amp; Jarrett, 2000" authorityName="Krapp-Schickel &amp; Jarrett" authorityYear="2000" box="[912,1432,1655,1681]" class="Malacostraca" family="Melitidae" genus="Maera" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="452" phylum="Arthropoda" rank="species" species="bousfieldi">
<emphasis id="B900EA8F5870CC67D0854B14FBF1F94C" box="[912,1062,1655,1681]" italics="true" pageId="1" pageNumber="452">M. bousfieldi</emphasis>
<bibRefCitation id="EFE54B6C5870CC67D73A4B1BFA4FF94C" author="Krapp-Schickel, T. &amp; Jarrett, N. E." box="[1071,1432,1655,1681]" pageId="1" pageNumber="452" pagination="23 - 61" refId="ref12125" refString="Krapp-Schickel, T. &amp; Jarrett, N. E. (2000) The amphipod family Melitidae on the Pacific coast of North America. Part II. The Maera - Ceradocus complex. Amphipacifica, 2, 23 - 61." type="journal article" year="2000">Krapp-Schickel &amp; Jarrett, 2000</bibRefCitation>
</taxonomicName>
; 
<taxonomicName id="4C744D1E5870CC67D3824BF0FE1BF968" authority="(Stimpson, 1853)" baseAuthorityName="Stimpson" baseAuthorityYear="1853" box="[151,460,1690,1717]" class="Malacostraca" family="Melitidae" genus="Maera" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="452" phylum="Arthropoda" rank="species" species="danae">
