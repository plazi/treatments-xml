<paragraph id="8BFA36FD1D38FFF6FF74FD05FD5BFCBB" blockId="13.[178,1085,709,790]" pageId="13" pageNumber="920">
<taxonomicName id="4C454D7E1D38FFF6FF74FD05FE01FD76" baseAuthorityName="Mayr" baseAuthorityYear="1862" box="[178,418,710,731]" class="Insecta" family="Hymenoptera" genus="Nesomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="920" phylum="Arthropoda" rank="species" species="angulatus">
