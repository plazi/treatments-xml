<collectingCountry id="F352766D1D38FFF6FE99FD20FE3CFD55" box="[351,415,739,760]" name="Oman" pageId="13" pageNumber="920">Oman</collectingCountry>
, 
<collectingCountry id="F352766D1D38FFF6FE6DFD20FD97FD54" box="[427,564,739,761]" name="Saudi Arabia" pageId="13" pageNumber="920">Saudi Arabia</collectingCountry>
