<collectingCountry id="40F2DE88F1504D6B61AEDE25FE9EFDA6" box="[252,359,534,560]" name="Morocco" pageId="15" pageNumber="522">Morocco</collectingCountry>
and 
<collectingCountry id="40F2DE88F1504D6B60F0DE25FE02FDA6" box="[418,507,534,560]" name="Algeria" pageId="15" pageNumber="522">Algeria</collectingCountry>
