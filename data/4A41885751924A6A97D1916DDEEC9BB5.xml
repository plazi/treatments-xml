<taxonomicName id="1AA11FB7BFCC038C793B720648AB875D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147041" authority="Schouteden, 1904" authorityName="Schouteden" authorityYear="1904" class="Insecta" family="Pentatomidae" genus="Lobopelta" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lobopelta Mayr" lsidName-HNS="Lobopelta Mayr" order="Hemiptera" pageNumber="733" phylum="Arthropoda" rank="genus">Lobopelta</taxonomicName>
