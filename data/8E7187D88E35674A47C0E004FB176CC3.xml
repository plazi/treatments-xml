] OP I # 
<number id="B01024E98E34674D448DE036FC1F6B69" box="[986,1017,1606,1630]" pageId="11" pageNumber="12" value="62.0">62</number>
; 
<number id="B01024E98E34674D4352E036FBAD6B69" box="[1029,1099,1606,1630]" pageId="11" pageNumber="12" value="18531.0">18531</number>
[
<number id="B01024E98E34674D430CE036FB8E6B69" box="[1115,1128,1606,1630]" pageId="11" pageNumber="12" value="1.0">1</number>
] OP I # 
<number id="B01024E98E34674D439EE036FB0E6B69" box="[1225,1256,1606,1630]" pageId="11" pageNumber="12" value="72.0">72</number>
; 
<number id="B01024E98E34674D43A4E036FADF6B69" box="[1267,1337,1606,1630]" pageId="11" pageNumber="12" value="18532.0">18532</number>
[
<number id="B01024E98E34674D421DE036FAB16B69" box="[1354,1367,1606,1630]" pageId="11" pageNumber="12" value="1.0">1</number>
