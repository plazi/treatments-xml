<quantity id="4CBAF7CE7135147DC00EF75F909D5FB3" box="[1037,1194,2247,2285]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="9" pageNumber="212" unit="mm" value="0.35">0.35 mm</quantity>
