<quantity id="4CBAF7CE7135147DC554F76C95C65E45" box="[343,497,2292,2331]" metricMagnitude="-4" metricUnit="m" metricValue="1.9" pageId="9" pageNumber="212" unit="mm" value="0.19">0.19 mm</quantity>
