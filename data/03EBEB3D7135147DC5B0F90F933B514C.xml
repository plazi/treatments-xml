<quantity id="4CBAF7CE7135147DC629F59296EE5D6E" box="[554,729,2570,2608]" metricMagnitude="-1" metricUnit="m" metricValue="8.75" metricValueMax="9.0" metricValueMin="8.5" pageId="9" pageNumber="212" unit="cm" value="87.5" valueMax="90.0" valueMin="85.0">85-90 cm</quantity>
