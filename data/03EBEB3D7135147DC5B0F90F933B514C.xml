<quantity id="4CBAF7CE7135147DC069F76C95175E17" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="9" pageNumber="212" unit="mm" value="0.35">0.35 mm</quantity>
, height 
<quantity id="4CBAF7CE7135147DC5B0F6BA96785E17" box="[435,591,2338,2377]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="212" unit="mm" value="0.2">0.20 mm</quantity>
