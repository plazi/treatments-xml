<quantity id="4CBAF7CE7135147DC6BAF70197685FE1" box="[697,863,2201,2239]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="9" pageNumber="212" unit="mm" value="0.34">0.34 mm</quantity>
, height 
<quantity id="4CBAF7CE7135147DC006F701909D5FE1" box="[1029,1194,2201,2239]" metricMagnitude="-4" metricUnit="m" metricValue="1.9" pageId="9" pageNumber="212" unit="mm" value="0.19">0.19 mm</quantity>
