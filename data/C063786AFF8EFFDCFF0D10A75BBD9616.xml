; pronotum 
<number id="FE02DB5BFF8EFFDDFE9114DD5E1C9346" box="[267,317,1518,1542]" pageId="32" pageNumber="33" value="0.32">0.32</number>
/ 
<number id="FE02DB5BFF8EFFDDFEDC14DD5E599346" box="[326,376,1518,1542]" pageId="32" pageNumber="33" value="0.44">0.44</number>
, PA 
<number id="FE02DB5BFF8EFFDDFE3714DD5EC09346" box="[429,481,1518,1542]" pageId="32" pageNumber="33" value="0.34">0.34</number>
, PB 
<number id="FE02DB5BFF8EFFDDFD8C14DD5D6A9346" box="[534,587,1518,1542]" pageId="32" pageNumber="33" value="0.39">0.39</number>
; elytra 
<number id="FE02DB5BFF8EFFDDFD0414DD5DF19346" box="[670,720,1518,1542]" pageId="32" pageNumber="33" value="0.73">0.73</number>
