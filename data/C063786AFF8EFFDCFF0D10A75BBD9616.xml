<number id="FE02DB5BFF8EFFDDFBEB14F95B8390A2" box="[1137,1186,1482,1506]" pageId="32" pageNumber="33" value="1.24">1.24</number>
/ 
<number id="FE02DB5BFF8EFFDDFB3114F95BFF90A2" box="[1195,1246,1482,1506]" pageId="32" pageNumber="33" value="0.54">0.54</number>
; head 
<number id="FE02DB5BFF8EFFDDFAB014F95A7F90A2" box="[1322,1374,1482,1506]" pageId="32" pageNumber="33" value="0.22">0.22</number>
/ 
