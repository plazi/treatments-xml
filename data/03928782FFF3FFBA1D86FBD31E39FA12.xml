<taxonomicName id="4C3B4D17FFF3FFBA1FB7FB49180FFB32" authority=", Zemun, Kej" authorityName="Zemun, Kej" box="[678,1028,1158,1183]" class="Magnoliopsida" family="Fabaceae" genus="Albizia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="39" phylum="Magnoliopsida" rank="species" species="julibrissin">
