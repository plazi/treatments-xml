<taxonomicName id="02409D36C0F57C8DF43DE73AADCA91A8" authorityName="Linnaeus" authorityYear="1758" class="Aves" family="Fringillidae" genus="Fringilla" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Fringilla montifringilla" order="Passeriformes" pageId="4" pageNumber="21" phylum="Chordata" rank="species" species="montifringilla">
