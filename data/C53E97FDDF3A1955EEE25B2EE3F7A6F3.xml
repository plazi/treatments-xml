<taxonomicName id="2706C745EC0B624E59387FE72B788D38" authorityName="Balat" authorityYear="1955" class="insecta" family="Philopteridae" genus="Brueelia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brueelia glizi" order="Phthiraptera" pageId="4" pageNumber="21" phylum="Arthropoda" rank="species" species="glizi">
<emphasis id="5FD45D4ED0555FB4361C81573B70ECFC" italics="true" pageId="4" pageNumber="21">Brueelia glizi</emphasis>
</taxonomicName>
