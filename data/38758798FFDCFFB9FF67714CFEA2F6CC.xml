<paragraph id="B063368EFFD8FFBBFF557059FF1CF0D1" pageId="12" pageNumber="226">
<table id="C2DCC42EFFD80048FF5571AAFB99F0D1" box="[165,1092,1763,2013]" colsContinueFrom="8.[165,1094,484,735]" colsContinueIn="15.[165,1095,1866,2036]" gridcols="3" gridrows="6" pageId="12" pageNumber="226">
