<taxonomicName id="A028A5D259E1AAAB82CBD33A30351DC0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28534" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex major Forel" order="Hymenoptera" pageNumber="490" phylum="Arthropoda" rank="species" species="major">Cyphomyrmex major Forel</taxonomicName>
