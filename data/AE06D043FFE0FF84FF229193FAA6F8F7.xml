<taxonomicName id="E1AF1AD6FFE0FF84FF229193FE22F8BE" authorityName="Dusmet y Alsonso" authorityYear="1915" box="[145,396,1790,1814]" class="Insecta" family="Megachilidae" genus="Anthidium" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="astilleroi">
