<key id="CC313233FFF2FFB939B6F937FCAFA4A2" box="[151,830,1734,1760]" pageId="11" pageNumber="112">
<keyStep id="03547DA7FFF2FFB939B6F937FCAFA4A2" box="[151,830,1734,1760]" pageId="11" pageNumber="112">
<emphasis id="8AD4B910FFF2FFB939B6F937FCAFA4A2" bold="true" box="[151,830,1734,1760]" pageId="11" pageNumber="112">
<number id="0E687725FFF2FFB939B6F937FF36A4A2" box="[151,167,1734,1760]" pageId="11" pageNumber="112" value="6.0">6</number>
. 
<taxonomicName id="7FA01E81FFF2FFB93995F937FCAFA4A2" authority="Moser, 1918" authorityName="Moser" authorityYear="1918" box="[180,830,1734,1760]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="112" phylum="Arthropoda" rank="species" species="brevisetosa" subGenus="Phyllophaga">
<emphasis id="8AD4B910FFF2FFB93995F937FEDCA4A2" bold="true" box="[180,333,1734,1760]" italics="true" pageId="11" pageNumber="112">Phyllophaga</emphasis>
(
<emphasis id="8AD4B910FFF2FFB9387FF937FE66A4A2" bold="true" box="[350,503,1734,1760]" italics="true" pageId="11" pageNumber="112">Phyllophaga</emphasis>
) 
<emphasis id="8AD4B910FFF2FFB93B29F937FD1FA4A2" bold="true" box="[520,654,1734,1760]" italics="true" pageId="11" pageNumber="112">brevisetosa</emphasis>
(Moser, 
<number id="0E687725FFF2FFB93BDAF937FCA6A4A2" box="[763,823,1734,1760]" pageId="11" pageNumber="112" value="1918.0">1918</number>
)
