<elevation id="0040D1C13572C155FBEDFA30FB70FA2D" box="[1073,1145,1446,1467]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="25" pageNumber="586" unit="m" value="700.0">700 m</elevation>
