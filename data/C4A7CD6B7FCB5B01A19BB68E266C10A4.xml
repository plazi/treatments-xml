<taxonomicName id="CB2234FB8E4028793DD80437156EF7A0" authorityName="Stomp" authorityYear="1971" class="Insecta" family="Entomobryidae" genus="Pseudosinella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudosinella huetheri" order="Collembola" pageId="0" pageNumber="121" phylum="Arthropoda" rank="species" species="huetheri">
<emphasis id="70872C6DBA32C01DAD54B518AC4E3F6E" italics="true" pageId="0" pageNumber="121">Pseudosinella huetheri</emphasis>
</taxonomicName>
