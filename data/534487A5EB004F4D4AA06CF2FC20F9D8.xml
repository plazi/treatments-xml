<quantity id="1C159B56EB004F4C4B8D6DBEFE6FF9A7" box="[442,502,1411,1435]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="6" pageNumber="421" unit="m" value="30.0">30 m</quantity>
