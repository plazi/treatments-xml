<taxonomicName id="1CED4D30EB004F4C4F156DBEFAF1F9A6" authorityName="Jałoszyński" authorityYear="2016" box="[1314,1384,1411,1435]" class="Insecta" family="Staphylinidae" genus="Cephennodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="421" phylum="Arthropoda" rank="species" species="setifer">
