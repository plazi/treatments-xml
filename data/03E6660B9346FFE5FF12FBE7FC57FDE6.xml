<taxonomicName id="4C4FAC9E9345FFE4FEA3F9A0FE5EF9FD" authority="Jaschhof, 2009" authorityName="Jaschhof" authorityYear="2009" box="[260,430,1563,1584]" class="Insecta" family="Cecidomyiidae" genus="Ansifera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="519" phylum="Arthropoda" rank="species" species="malayensis" status="sp. n.">
