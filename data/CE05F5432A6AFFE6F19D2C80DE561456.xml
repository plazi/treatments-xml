<taxonomicName id="81AC3FD62A68FFE6F0602E4BD8A9172A" box="[362,645,260,284]" class="Insecta" family="Veliidae" genus="Rhagovelia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="74" phylum="Arthropoda" rank="species" species="barbacoensis" status="sp. n.">
