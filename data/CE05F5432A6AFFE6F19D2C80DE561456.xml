<number id="F06456722A68FFE6F4142E4BDF05172A" box="[1310,1321,260,284]" pageId="3" pageNumber="74" value="1.0">1</number>
º 
<number id="F06456722A68FFE6F4302E4BDF7B172B" box="[1338,1367,260,285]" pageId="3" pageNumber="74" value="15.0">15</number>
' 
<number id="F06456722A68FFE6F46B2E4BDFBF172B" box="[1377,1427,260,285]" pageId="3" pageNumber="74" value="25.2">25.2</number>
” N
</geoCoordinate>
, 
<geoCoordinate id="239822922A68FFE6F1B22E67DB491709" box="[184,357,295,320]" direction="west" orientation="longitude" pageId="3" pageNumber="74" precision="1" value="-78.08939">
