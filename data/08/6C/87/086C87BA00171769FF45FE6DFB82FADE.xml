<taxonomicName authority="Conrad, 1849" authorityName="Conrad" authorityYear="1849" box="[362,695,973,998]" class="Bivalvia" family="Solemyidae" genus="Acharax" kingdom="Animalia" order="Solemyoida" pageId="4" pageNumber="5" phylum="Mollusca" rank="species" species="ventricosa">
