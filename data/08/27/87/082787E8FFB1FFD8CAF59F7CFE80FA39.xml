<subSubSection pageId="22" pageNumber="97" type="etymology">
<paragraph blockId="22.[831,1436,1291,1347]" pageId="22" pageNumber="97">
<emphasis bold="true" box="[831,965,1291,1314]" pageId="22" pageNumber="97">Etymology:</emphasis>
