<subSubSection pageId="32" pageNumber="107" type="etymology">
<paragraph blockId="32.[831,1436,418,474]" pageId="32" pageNumber="107">
<emphasis bold="true" box="[831,965,418,441]" pageId="32" pageNumber="107">Etymology:</emphasis>
