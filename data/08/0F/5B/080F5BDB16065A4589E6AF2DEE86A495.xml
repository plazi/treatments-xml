<geoCoordinate degrees="20.2930" direction="north" orientation="latitude" precision="5" value="20.293">20.2930N</geoCoordinate>
, 
<geoCoordinate degrees="105.6670" direction="east" orientation="longitude" precision="5" value="105.667">105.6670E</geoCoordinate>
