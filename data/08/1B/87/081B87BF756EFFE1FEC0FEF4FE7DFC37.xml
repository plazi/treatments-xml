<emphasis bold="true" box="[264,462,376,402]" pageId="2" pageNumber="3">
<typeStatus box="[264,325,376,402]" pageId="2" pageNumber="3">Type</typeStatus>
species. —
</emphasis>
<taxonomicName authority="Wunderlich 1992" authorityName="Wunderlich" authorityYear="1992" box="[470,946,376,402]" class="Arachnida" family="Pholcidae" genus="Spermophorides" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="justoi">
