) and 
<taxonomicName authorityName="B. Axelius" authorityYear="1994" baseAuthorityName="Phil." class="Magnoliopsida" family="Solanaceae" genus="Exodeconus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Exodeconus integrifolius" order="Solanales" pageId="0" pageNumber="47" phylum="Tracheophyta" rank="species" species="integrifolius">
