<normalizedToken originalValue="Blütenstiele" pageId="null" pageNumber="379">Bluetenstiele</normalizedToken>
senkrecht abstehend behaart; 
<normalizedToken originalValue="Kronblätter" pageId="null" pageNumber="379">Kronblaetter</normalizedToken>
