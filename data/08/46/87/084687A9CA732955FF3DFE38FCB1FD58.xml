)
</bibRefCitation>
; and (
<number box="[1097,1112,260,284]" pageId="38" pageNumber="39" value="6.0">6</number>
) basis of pereopod 
<number box="[1331,1345,260,284]" pageId="38" pageNumber="39" value="1.0">1</number>
with 
<number pageId="38" pageNumber="39" value="2.0" valueMax="3.0" valueMin="1.0">1– 3</number>
