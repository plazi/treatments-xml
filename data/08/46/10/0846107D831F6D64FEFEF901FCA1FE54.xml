Figs. 
<quantity box="[1157,1240,281,307]" metricMagnitude="0" metricUnit="m" metricValue="2.3876" metricValueMax="2.54" metricValueMin="2.2352" pageId="36" pageNumber="37" unit="in" value="94.0" valueMax="100.0" valueMin="88.0">88­100</quantity>
