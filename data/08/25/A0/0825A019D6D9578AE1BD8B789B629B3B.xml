<taxonomicName authority="Perris, 1840" authorityName="Perris" authorityYear="1840" class="Insecta" family="Ormyridae" genus="Ormyrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ormyrus papaveris" order="Hymenoptera" pageId="15" pageNumber="66" phylum="Arthropoda" rank="species" species="papaveris">
