], Weixi County, Yunnan Province, 
<collectingCountry box="[383,456,1497,1523]" name="China" pageId="1" pageNumber="42">China</collectingCountry>
, 
<quantity box="[467,552,1497,1523]" metricMagnitude="3" metricUnit="m" metricValue="2.25" pageId="1" pageNumber="42" unit="m" value="2250.0">2250m</quantity>
, 
