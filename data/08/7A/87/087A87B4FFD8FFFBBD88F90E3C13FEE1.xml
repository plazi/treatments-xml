of 
<collectingRegion box="[520,730,1857,1883]" country="Panama" name="Panama" pageId="10" pageNumber="135">Ciudad de Panamá</collectingRegion>
(
<collectingRegion box="[744,894,1857,1884]" country="Panama" name="Panama" pageId="10" pageNumber="135">Panama City</collectingRegion>
), 
<collectingMunicipality box="[913,991,1857,1883]" pageId="10" pageNumber="135">Pacific</collectingMunicipality>
