<quantity box="[212,257,1085,1112]" metricMagnitude="1" metricUnit="m" metricValue="3.3" pageId="4" pageNumber="2130" unit="m" value="33.0">
33 
<emphasis bold="true" box="[243,257,1088,1112]" italics="true" pageId="4" pageNumber="2130">m</emphasis>
</quantity>
