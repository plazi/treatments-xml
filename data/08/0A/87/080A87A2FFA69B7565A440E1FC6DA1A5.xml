nities. 
<taxonomicName authority="Urhan, 2002" authorityName="Urhan" authorityYear="2002" box="[292,502,383,407]" class="Arachnida" family="Zerconidae" genus="Prozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="2131" phylum="Arthropoda" rank="species" species="denizliensis" status="sp. nov." subGenus="Plumatozercon">P. (P.) denizliensis</taxonomicName>
