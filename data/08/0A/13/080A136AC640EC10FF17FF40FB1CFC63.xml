; HL 
<quantity box="[678,816,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" metricValueMax="4.5" metricValueMin="2.7" pageId="11" pageNumber="363" unit="mm" value="3.6" valueMax="4.5" valueMin="2.7">2.7–4.5 mm</quantity>
; PL 
