<subSubSection pageId="12" pageNumber="13" type="diagnosis">
<paragraph blockId="12.[151,1437,383,1376]" pageId="12" pageNumber="13">
<emphasis bold="true" box="[197,339,531,557]" pageId="12" pageNumber="13">Diagnosis:</emphasis>
