<subSubSection pageId="null" pageNumber="557" type="nomenclature">
<paragraph pageId="null" pageNumber="557">
<taxonomicName authority="L." class="Liliopsida" family="Asparagaceae" genus="Ornithogalum" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="557" phylum="Tracheophyta" rank="species" species="nutans">
Ornithogalum 
<normalizedToken originalValue="nútans" pageId="null" pageNumber="557">nutans</normalizedToken>
<authorityName pageId="null" pageNumber="557">L.</authorityName>
</taxonomicName>
</paragraph>
