<taxonomicName authority="(Merz 1994)" authorityName="Merz" authorityYear="1994" baseAuthorityName="Merz" baseAuthorityYear="1994" box="[1134,1395,440,464]" class="Magnoliopsida" family="Asteraceae" genus="Carduus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="61" pageNumber="62" phylum="Tracheophyta" rank="species" species="crispus">
<emphasis box="[1134,1149,441,464]" italics="true" pageId="61" pageNumber="62">L</emphasis>
