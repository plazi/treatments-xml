<taxonomicName authority="Latreille 1804" authorityName="Latreille" authorityYear="1804" box="[151,437,1661,1687]" class="Insecta" family="Tephritidae" genus="Tephritis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
