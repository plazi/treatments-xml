<subSubSection box="[151,430,1086,1113]" pageId="4" pageNumber="543" type="nomenclature">
<paragraph blockId="4.[151,430,1086,1113]" box="[151,430,1086,1113]" pageId="4" pageNumber="543">
<heading bold="true" box="[151,430,1086,1113]" fontSize="11" level="1" pageId="4" pageNumber="543" reason="1">
<emphasis bold="true" box="[151,430,1086,1113]" pageId="4" pageNumber="543">
<taxonomicName authority="Watts &amp; Bradford &amp; Cooper, 2021" authorityName="Watts &amp; Bradford &amp; Cooper" authorityYear="2021" box="[151,312,1086,1113]" class="Insecta" family="Scirtidae" genus="Perplexacara" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="543" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,312,1086,1113]" italics="true" pageId="4" pageNumber="543">Perplexacara</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[320,430,1087,1113]" pageId="4" pageNumber="543" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
