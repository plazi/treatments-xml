<taxonomicName class="Insecta" family="Staphylinidae" genus="Ischnosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="nepalicum">
