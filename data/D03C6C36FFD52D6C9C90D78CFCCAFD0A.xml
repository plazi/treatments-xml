<taxonomicName id="9F95A6A3FFD52D6C9FA1D5EBF967FD2F" box="[934,1075,688,714]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="22" phylum="Arthropoda" rank="order">lepidoptera</taxonomicName>
