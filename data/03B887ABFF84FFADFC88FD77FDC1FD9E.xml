<taxonomicName id="4C114D3EFF84FFACFD0CFC82FC94FC86" authorityName="Poyato-Ariza F. J. &amp; Wenz S." authorityYear="2002" baseAuthorityName="Wagner" baseAuthorityYear="1851" box="[676,862,864,888]" class="Actinopterygii" family="Pycnodontiformes incertae sedis" genus="Eomesodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="80" pageNumber="219" phylum="Chordata" rank="species" species="gibbosus">Eomesodon gibbosus</taxonomicName>
