<heading id="D0AB81BDF52DFFCAFF6CFBB7DA04CE33" bold="true" box="[151,581,1088,1114]" fontSize="11" level="1" pageId="17" pageNumber="468" reason="1">
<taxonomicName id="4C5C4D52F52DFFCAFF6CFBB7DA04CE33" authority="Walter, 1907" authorityName="Walter" authorityYear="1907" box="[151,581,1088,1114]" class="Arachnida" family="Feltriidae" genus="Feltria" kingdom="Animalia" order="Trombidiformes" pageId="17" pageNumber="468" phylum="Arthropoda" rank="species" species="rouxi" subGenus="Feltria">
<emphasis id="B928EAC3F52DFFCAFF6CFBB7DA04CE33" bold="true" box="[151,581,1088,1114]" pageId="17" pageNumber="468">
<emphasis id="B928EAC3F52DFFCAFF6CFBB7D8ABCE33" bold="true" box="[151,234,1088,1114]" italics="true" pageId="17" pageNumber="468">Feltria</emphasis>
(
<emphasis id="B928EAC3F52DFFCAFF00FBB7D90FCE33" bold="true" box="[251,334,1088,1114]" italics="true" pageId="17" pageNumber="468">Feltria</emphasis>
) 
<emphasis id="B928EAC3F52DFFCAFEA4FBB7D9E1CE33" bold="true" box="[351,416,1088,1114]" italics="true" pageId="17" pageNumber="468">rouxi</emphasis>
