<taxonomicName id="8A8E4D24FFD9F81614FDFBCDFA99FF34" authority="Linnaeus (1758)" authorityName="Linnaeus" authorityYear="1758" box="[958,1291,1121,1145]" class="Magnoliopsida" family="Vitaceae" genus="Vitis" kingdom="Plantae" order="Vitales" pageId="2" pageNumber="566" phylum="Tracheophyta" rank="species" species="vinifera">
