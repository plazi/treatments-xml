<taxonomicName id="5ADEC9CB8139FE9AE987CB821C270BB8" authorityName="Hamilton" authorityYear="1822" box="[1260,1402,1451,1476]" class="Actinopterygii" family="Cynoglossidae" genus="Cynoglossus" kingdom="Animalia" order="Pleuronectiformes" pageId="317" pageNumber="318" phylum="Chordata" rank="genus">
