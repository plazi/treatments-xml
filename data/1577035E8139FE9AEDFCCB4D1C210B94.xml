<taxonomicName id="5ADEC9CB8139FE9AEC4CCBE618F90B94" authorityName="Rafinesque" authorityYear="1810" box="[295,420,1487,1512]" class="Actinopterygii" family="Cynoglossidae" genus="Symphurus" kingdom="Animalia" order="Pleuronectiformes" pageId="317" pageNumber="318" phylum="Chordata" rank="genus">
