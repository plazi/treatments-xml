<typeStatus id="037C883BFFC7FFCDFEB4FE3FFE69FE3A" box="[327,436,432,458]" pageId="3" pageNumber="26">Holotype</typeStatus>
female, 
<collectingCountry id="A4D07609FFC7FFCDFDD6FE20FD42FE39" box="[549,671,431,457]" name="Indonesia" pageId="3" pageNumber="26">
<emphasis id="EEB3EA8BFFC7FFCDFDD6FE20FD42FE39" bold="true" box="[549,671,431,457]" pageId="3" pageNumber="26">Indonesia</emphasis>
</collectingCountry>
, Sulawesi-Utara, Dumoga-Bone National Park, Toraut Forest, Malaise trap, 
<date id="A8791059FFC7FFCDFEC4FE57FDFAFE02" box="[311,551,472,498]" pageId="3" pageNumber="26" value="1985-01-28" valueMax="1985-01-31" valueMin="1985-01-28">
<number id="6A0F24BEFFC7FFCDFEC4FE57FE5DFE02" box="[311,384,472,498]" pageId="3" pageNumber="26" value="29.5" valueMax="31.0" valueMin="28.0">28–31</number>
January 
<number id="6A0F24BEFFC7FFCDFE19FE57FDFAFE02" box="[490,551,472,498]" pageId="3" pageNumber="26" value="1985.0">1985</number>
</date>
, R. H. L. Disney (
<collectionCode id="BAD6AE5CFFC7FFCDFD0FFE57FC8BFE02" box="[764,854,472,498]" pageId="3" pageNumber="26">UCZM</collectionCode>
, 
<number id="6A0F24BEFFC7FFCDFC92FE57FC77FE02" box="[865,938,472,498]" pageId="3" pageNumber="26" value="18.5" valueMax="21.0" valueMin="16.0">16-21</number>
).
</paragraph>
