<taxonomicName id="4C2C7491FFFC7B5FFC7AF85E30040E99" authorityName="Clerck" authorityYear="1764" box="[956,1066,2000,2024]" class="Insecta" family="Noctuidae" genus="Othreis" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="1108" phylum="Arthropoda" rank="species" species="fullonia">
