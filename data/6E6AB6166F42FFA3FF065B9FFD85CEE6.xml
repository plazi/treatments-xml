<taxonomicName id="21C37C836F44FFA3FE705DF4FD85CF86" authorityName="Xu" authorityYear="2001" box="[481,563,655,680]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="baka">
