<taxonomicName id="21C37C836F44FFA3FA865C44FA23CE16" authorityName="Wheeler" authorityYear="1906" box="[1303,1429,800,824]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="japonica">
<emphasis id="D4B7DB126F44FFA3FA865C44FA93CE16" box="[1303,1317,801,824]" italics="true" pageId="53" pageNumber="54">P</emphasis>
