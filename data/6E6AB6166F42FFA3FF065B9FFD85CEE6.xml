<emphasis id="D4B7DB126F44FFA3FDCF5C7AFD7FCE16" box="[606,713,799,824]" italics="true" pageId="53" pageNumber="54">shennong</emphasis>
</taxonomicName>
, 
<taxonomicName id="21C37C836F44FFA3FD475C44FCF1CE16" authorityName=": Wilson" authorityYear="1957" box="[726,839,800,824]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="swezeyi">
