<taxonomicName id="21C37C836F44FFA3FD495DF4FCFFCF86" authorityName=": Wilson" authorityYear="1957" box="[728,841,656,680]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="swezeyi">
