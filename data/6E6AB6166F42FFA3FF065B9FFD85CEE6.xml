<taxonomicName id="21C37C836F44FFA3FE725C44FD82CE16" authorityName="Xu" authorityYear="2001" box="[483,564,799,824]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="baka">
