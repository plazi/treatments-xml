<taxonomicName id="21C37C836F44FFA3FCED5C44FC60CE16" authorityName="Xu" authorityYear="2001" box="[892,982,799,824]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="xatha">
