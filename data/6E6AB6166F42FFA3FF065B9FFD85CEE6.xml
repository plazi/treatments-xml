<taxonomicName id="21C37C836F44FFA3FBE95C44FB55CE16" baseAuthorityName="Wheeler" baseAuthorityYear="1933" box="[1144,1251,800,824]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="incerta">
