<number id="132B24AAFFAFFFCC306E4381D5D0FE5D" box="[365,386,395,419]" pageId="2" pageNumber="85" value="3.0">3</number>
<number id="132B24AAFFAFFFCC308A4380D5FDFE5C" box="[393,431,394,418]" pageId="2" pageNumber="85" value="6.5">6.5</number>
; postfemora: 
