<figureCitation id="97A82A5514231B1F5A9A7383FDAFFA24" box="[491,573,1485,1511]" captionStart="FIGURE 10" captionStartId="6.[151,250,1586,1611]" captionTargetBox="[314,1274,841,1561]" captionTargetId="figure@6.[314,1274,841,1561]" captionTargetPageId="6" captionText="FIGURE 10. Perlodinella mazehaoi sp. nov., type locality: Xiaojinggou stream, Daqing Mountain, Hohhot City, Inner Mongolia, China." figureDoi="http://doi.org/10.5281/zenodo.3995573" httpUri="https://zenodo.org/record/3995573/files/figure.png" pageId="1" pageNumber="298">Fig. 10</figureCitation>
), 
<geoCoordinate id="6AA7501714231B1F59237383FD59FA2B" box="[594,715,1485,1512]" degrees="41.0004" direction="north" orientation="latitude" pageId="1" pageNumber="298" precision="5" value="41.0004">41.0004 N</geoCoordinate>
, 
<geoCoordinate id="6AA7501714231B1F59A77383FCC4FA2B" box="[726,854,1485,1512]" degrees="111.8358" direction="east" orientation="longitude" pageId="1" pageNumber="298" precision="5" value="111.8358">111.8358 E</geoCoordinate>
, 
<quantity id="C86B9B3514231B1F58137383FC2BFA2B" box="[866,953,1485,1512]" metricMagnitude="3" metricUnit="m" metricValue="1.395" pageId="1" pageNumber="298" unit="m" value="1395.0">
<elevation id="84BED1E314231B1F58137383FC2BFA2B" box="[866,953,1485,1512]" metricMagnitude="3" metricUnit="m" metricValue="1.395" pageId="1" pageNumber="298" unit="m" value="1395.0">1395 m</elevation>
</quantity>
, 
<date id="7B2D101014231B1F58B47380FBF6FA24" box="[965,1124,1485,1512]" pageId="1" pageNumber="298" value="2019-05-28">
