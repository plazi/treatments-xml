<collectingMunicipality id="EF48ACAA14231B1F58AF73E7FB3AFA00" box="[990,1192,1449,1475]" pageId="1" pageNumber="298">Daqing Mountain</collectingMunicipality>
, 
<location id="0A4C600B14231B1F5FC273E7FA0AFA00" LSID="urn:lsid:plazi:treatment:873A87C614231B1C5BE676AFFB0FFE48:0A4C600B14231B1F5FC273E7FA0AFA00" box="[1203,1432,1449,1475]" country="China" county="Hohhot City" latitude="41.0004" longLatPrecision="6" longitude="111.8358" municipality="Daqing Mountain" name="Xiaojinggou Stream" pageId="1" pageNumber="298" stateProvince="Nei Mongol">Xiaojinggou Stream</location>
