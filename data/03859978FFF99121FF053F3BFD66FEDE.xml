<collectionCode id="ED3DB0ABFFF9912EFB093A02FB02F989" LSID="urn:lsid:biocol.org:col:34871" box="[1179,1265,1563,1587]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="7" pageNumber="218">USNM</collectionCode>
