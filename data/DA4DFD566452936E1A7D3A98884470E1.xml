<taxonomicName id="E55F0E7D468A4B6D905B874CE87DAD3E" LSID="http://zoobank.org/7AD61C28-DF55-4874-9ED6-A231A162D2CF" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax bicoloratus" order="Coleoptera" pageId="275" pageNumber="276" phylum="Arthropoda" rank="species" species="bicoloratus">Mecyclothorax bicoloratus</taxonomicName>
