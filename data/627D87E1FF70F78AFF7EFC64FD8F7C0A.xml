<taxonomicName id="2DD44D74FF70F794FF7EFC64FD6D7911" authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,701,1023,1049]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="142" pageNumber="143" phylum="Arthropoda" rank="species" species="suswanad" status="new species">
<emphasis id="D8A0EAE5FF70F794FF7EFC64FE347911" bold="true" box="[151,484,1023,1049]" italics="true" pageId="142" pageNumber="143">Cheumatopsyche suswanad</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFF70F794FD23FC64FC8A7911" box="[714,858,1023,1049]" pageId="142" pageNumber="143" rank="species">new species</taxonomicNameLabel>
</emphasis>
