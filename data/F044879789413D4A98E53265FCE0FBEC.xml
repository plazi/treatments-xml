<taxonomicName id="BFED4D0289403D4A99B833D7FD09FC35" box="[458,744,964,989]" class="Arachnida" family="Monoscutidae" genus="Neopantopsalis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="5" pageNumber="46" phylum="Arthropoda" rank="species" species="camelus">
