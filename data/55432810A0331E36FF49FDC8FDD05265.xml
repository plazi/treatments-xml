<paragraph id="DD559906A0331E37FCA3FE49FA625236" blockId="4.[806,1422,426,571]" pageId="4" pageNumber="581">
<materialsCitation id="6D82935BA0331E37FCA3FE49FA625236" collectionCode="MNHN" collectorName="Right &amp; Sa &amp; Arambourg C &amp; The" pageId="4" pageNumber="581" specimenCount="1" typeStatus="lectotype">
