<taxonomicName id="4C6C4D30FFF4F945FF3BFC6E6432FC6D" authority="Sars M. in Sars G.O, 1872" authorityName="Sars M. in Sars G.O" authorityYear="1872" box="[151,583,910,932]" class="Scaphopoda" family="Dentaliidae" genus="Dentalium" kingdom="Animalia" order="Dentaliida" pageId="48" pageNumber="49" phylum="Mollusca" rank="species" species="agile">
