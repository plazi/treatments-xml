<taxonomicName id="30DA125C2B8D0BCF7F35A3EF2CE069A8" class="Insecta" family="Dryophthoridae" genus="Stromboscerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stromboscerus" order="Coleoptera" pageId="250" pageNumber="251" phylum="Arthropoda" rank="genus">Stromboscerus</taxonomicName>
