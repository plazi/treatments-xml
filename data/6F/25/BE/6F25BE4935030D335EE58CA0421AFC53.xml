<quantity box="[210,288,640,667]" metricMagnitude="-7" metricUnit="kg" metricValue="5.0" pageId="1" pageNumber="292" unit="mg" value="0.5">0.5 mg</quantity>
L- 
<superScript attach="right" box="[318,325,640,654]" fontSize="6" pageId="1" pageNumber="292">1</superScript>
), pH 6.8 (± 0.3), TDS 0.02, 
