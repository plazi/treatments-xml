and 
<taxonomicName box="[1040,1178,1646,1670]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="fansipana">
<emphasis box="[1040,1053,1646,1669]" italics="true" pageId="15" pageNumber="316">T</emphasis>
. 
