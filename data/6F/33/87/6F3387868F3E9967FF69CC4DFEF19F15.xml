<collectionCode LSID="urn:lsid:biocol.org:col:14498" box="[582,630,1610,1634]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14498" name="University of Coimbra Botany Department" pageId="15" pageNumber="316">COI</collectionCode>
<collectionCode LSID="urn:lsid:biocol.org:col:15536" box="[640,701,1610,1634]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15536" name="Department of Natural Resources, Environment, The Arts and Sport" pageId="15" pageNumber="316">DNA</collectionCode>
barcodes between 
<taxonomicName box="[921,1028,1609,1634]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="xilinga">
<emphasis box="[921,934,1610,1633]" italics="true" pageId="15" pageNumber="316">T</emphasis>
. 
<emphasis box="[950,1028,1609,1634]" italics="true" pageId="15" pageNumber="316">xilinga</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1087,1218,1609,1634]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="mekonga">
<emphasis box="[1087,1100,1610,1633]" italics="true" pageId="15" pageNumber="316">T</emphasis>
. 
<emphasis box="[1117,1218,1609,1634]" italics="true" pageId="15" pageNumber="316">mekonga</emphasis>
</taxonomicName>
is 
<number box="[1254,1307,1610,1634]" pageId="15" pageNumber="316" value="3.79">3,79</number>
%; distance between 
<taxonomicName box="[251,356,1645,1670]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="xilinga">
<emphasis box="[251,265,1646,1669]" italics="true" pageId="15" pageNumber="316">T</emphasis>
. 
<emphasis box="[278,356,1645,1670]" italics="true" pageId="15" pageNumber="316">xilinga</emphasis>
</taxonomicName>
and 
<taxonomicName box="[411,522,1645,1670]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="helmuti">
<emphasis box="[411,425,1646,1669]" italics="true" pageId="15" pageNumber="316">T</emphasis>
. 
<emphasis box="[438,522,1645,1670]" italics="true" pageId="15" pageNumber="316">helmuti</emphasis>
</taxonomicName>
is 
<number box="[555,605,1646,1670]" pageId="15" pageNumber="316" value="7.81">7,81</number>
% and distance between 
<taxonomicName box="[881,986,1645,1670]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="xilinga">
