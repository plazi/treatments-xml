<emphasis box="[751,888,152,175]" italics="true" pageId="11" pageNumber="312">Anacronicta</emphasis>
</taxonomicName>
spp. 
<taxonomicName box="[948,1082,152,176]" class="Insecta" family="Pantheidae" genus="Tambana" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="312" phylum="Arthropoda" rank="species" species="succincta">
