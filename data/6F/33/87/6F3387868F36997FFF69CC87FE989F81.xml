<collectionCode LSID="urn:lsid:biocol.org:col:14498" box="[574,622,152,176]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14498" name="University of Coimbra Botany Department" pageId="8" pageNumber="309">COI</collectionCode>
