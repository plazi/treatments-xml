and 
<taxonomicName box="[672,801,188,213]" class="Insecta" family="Pantheidae" genus="Smilepholcia" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="burmana">
