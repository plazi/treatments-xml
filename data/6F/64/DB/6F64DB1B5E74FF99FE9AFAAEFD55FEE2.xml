<emphasis bold="true" box="[813,1022,1163,1183]" italics="true" pageId="53" pageNumber="82">
Heraeus 
<taxonomicName box="[914,1022,1163,1182]" pageId="53" pageNumber="82" rank="species" species="bahiensis" status="sp. nov.">bahiensis</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1028,1112,1163,1182]" pageId="53" pageNumber="82" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<emphasis bold="true" box="[1125,1435,1163,1183]" pageId="53" pageNumber="82">
<taxonomicName box="[1125,1346,1163,1183]" class="Insecta" family="Rhyparochromidae" genus="Heraeus" kingdom="Animalia" order="Hemiptera" pageId="53" pageNumber="82" phylum="Arthropoda" rank="species" species="bolivianus" status="sp. nov.">
<emphasis bold="true" box="[1125,1346,1163,1183]" italics="true" pageId="53" pageNumber="82">
Heraeus 
<taxonomicName box="[1226,1346,1163,1182]" pageId="53" pageNumber="82" rank="species" species="bolivianus" status="sp. nov.">bolivianus</taxonomicName>
</emphasis>
