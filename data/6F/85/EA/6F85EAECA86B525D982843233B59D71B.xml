<emphasis bold="true" pageId="0" pageNumber="183">Figures 31-32.</emphasis>
<taxonomicName authorityName="Warncke" authorityYear="1972" class="Insecta" family="Andrenidae" genus="Flavipanurgus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flavipanurgus" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="183">Flavipanurgus</emphasis>
</taxonomicName>
male genitalia 
<emphasis bold="true" pageId="0" pageNumber="183">31</emphasis>
<taxonomicName baseAuthorityName="Warncke" baseAuthorityYear="1987" class="Insecta" family="Andrenidae" genus="Flavipanurgus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flavipanurgus granadensis" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="species" species="granadensis">
<emphasis italics="true" pageId="0" pageNumber="183">Flavipanurgus granadensis</emphasis>
</taxonomicName>
<emphasis bold="true" pageId="0" pageNumber="183">32</emphasis>
<taxonomicName baseAuthorityName="Vachal" baseAuthorityYear="1910" class="Insecta" family="Andrenidae" genus="Flavipanurgus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flavipanurgus merceti" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="species" species="merceti">
