<taxonomicName baseAuthorityName="Warncke" baseAuthorityYear="1972" class="Insecta" family="Andrenidae" genus="Flavipanurgus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flavipanurgus ibericus" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="species" species="ibericus">
