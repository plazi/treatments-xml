<taxonomicName baseAuthorityName="Vachal" baseAuthorityYear="1910" class="Insecta" family="Andrenidae" genus="Flavipanurgus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flavipanurgus merceti" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="species" species="merceti">
