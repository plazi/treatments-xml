<taxonomicName baseAuthorityName="Warncke" baseAuthorityYear="1987" class="Insecta" family="Andrenidae" genus="Flavipanurgus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flavipanurgus granadensis" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="species" species="granadensis">
<emphasis italics="true" pageId="0" pageNumber="183">Flavipanurgus granadensis</emphasis>
</taxonomicName>
