<subSubSection pageId="11" pageNumber="397" type="discussion">
<paragraph blockId="11.[151,1436,151,357]" pageId="11" pageNumber="397">
<emphasis bold="true" box="[199,315,295,320]" pageId="11" pageNumber="397">Remarks.</emphasis>
<taxonomicName box="[324,588,295,320]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="397" phylum="Arthropoda" rank="species" species="tuberculata">
