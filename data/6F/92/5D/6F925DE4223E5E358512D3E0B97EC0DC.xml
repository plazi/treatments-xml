<taxonomicName class="Liliopsida" family="Orchidaceae" genus="Habenaria" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Habenaria" order="Asparagales" pageId="0" pageNumber="109" phylum="Tracheophyta" rank="genus">
