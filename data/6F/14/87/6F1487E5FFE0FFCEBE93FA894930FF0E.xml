<taxonomicName authority="Lankester, 1903" authorityName="Lankester" authorityYear="1903" box="[287,692,1820,1842]" class="Malacostraca" family="Cymonomidae" genus="Cymonomus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="388" phylum="Arthropoda" rank="species" species="valdiviae">
