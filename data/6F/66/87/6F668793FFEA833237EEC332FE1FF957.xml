<typeStatus box="[1099,1204,1572,1598]" pageId="10" pageNumber="26">holotype</typeStatus>
, from 
<collectingCountry box="[1287,1436,1572,1598]" name="Tajikistan" pageId="10" pageNumber="26">Tadzhikistan</collectingCountry>
(
<bibRefCitation author="Sruoga" box="[159,316,1610,1636]" pageId="10" pageNumber="28" refString="Sruoga, V. (1990) Seven new species of Elachistidae (Lepidoptera) from the USSR. Tijdschrift voor Entomologie, 133, 75 - 84." type="journal article" year="1990">
Sruoga 
<number box="[252,316,1610,1636]" pageId="10" pageNumber="26" value="1990.0">1990</number>
</bibRefCitation>
