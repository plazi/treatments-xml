(
<bibRefCitation author="Dandy, J. E." box="[1075,1275,1972,1998]" pageId="1" pageNumber="202" pagination="123 - 132" refId="ref2389" refString="Dandy, J. E. (1928) New or noteworthy Chinese Magnolieae. Notes from the Royal Botanic Garden, Edinburgh 16: 123 - 132." type="journal article" year="1928">Dandy 1928: 128</bibRefCitation>
) 
<bibRefCitation author="Kumar, V. S." pageId="1" pageNumber="202" pagination="183 - 186" refId="ref2884" refString="Kumar, V. S. (2006) New combinations and new names in Asian Magnoliaceae. Kew Bulletin 61: 183 - 186." type="journal article" year="2006">Kumar (2006: 185)</bibRefCitation>
</taxonomicName>
, 
<taxonomicName box="[202,302,2009,2034]" class="Magnoliopsida" family="Magnoliaceae" genus="Manglietia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Magnoliales" pageId="1" pageNumber="202" phylum="Tracheophyta" rank="species" species="ventii">
