<number box="[151,193,692,716]" pageId="2" pageNumber="95" value="120.0">120</number>
(
<number box="[209,254,692,717]" pageId="2" pageNumber="95" value="157.0">157</number>
), interocellar setae length 
<number box="[550,567,692,716]" pageId="2" pageNumber="95" value="9.0">9</number>
. Eye length, dorsal 
<number box="[789,819,692,717]" pageId="2" pageNumber="95" value="62.0">62</number>
, ventral 
<number box="[916,947,692,717]" pageId="2" pageNumber="95" value="95.0">95</number>
. Antenna length 
<number box="[1138,1184,692,716]" pageId="2" pageNumber="95" value="340.0">340</number>
; segments I–IX length (width): 
<number box="[251,279,727,752]" pageId="2" pageNumber="95" value="15.0">15</number>
(
<number box="[298,329,728,752]" pageId="2" pageNumber="95" value="30.0">30</number>
), 
<number box="[353,381,727,752]" pageId="2" pageNumber="95" value="50.0">50</number>
(
<number box="[400,431,727,752]" pageId="2" pageNumber="95" value="25.0">25</number>
), 
<number box="[455,483,728,752]" pageId="2" pageNumber="95" value="70.0">70</number>
(
<number box="[502,532,728,752]" pageId="2" pageNumber="95" value="18.0">18</number>
), 
<number box="[557,585,727,752]" pageId="2" pageNumber="95" value="64.0">64</number>
(
<number box="[603,635,728,752]" pageId="2" pageNumber="95" value="20.0">20</number>
), 
<number box="[659,687,728,752]" pageId="2" pageNumber="95" value="43.0">43</number>
(
<number box="[705,737,728,752]" pageId="2" pageNumber="95" value="22.0">22</number>
), 
<number box="[760,788,727,752]" pageId="2" pageNumber="95" value="15.0">15</number>
(
<number box="[807,838,727,752]" pageId="2" pageNumber="95" value="16.0">16</number>
), 
<number box="[862,890,727,752]" pageId="2" pageNumber="95" value="15.0">15</number>
(
