<taxonomicName authority="(Lam.) Mull. Arg." authorityName="Mull. Arg." baseAuthorityName="Lam." box="[725,1195,762,788]" class="Magnoliopsida" family="Euphorbiaceae" genus="Mallotus" kingdom="Plantae" order="Malpighiales" pageId="23" pageNumber="412" phylum="Tracheophyta" rank="species" species="philippinensis">
