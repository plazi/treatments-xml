<taxonomicName authority="Mull. Arg." authorityName="Mull. Arg." box="[990,1379,186,212]" class="Magnoliopsida" family="Euphorbiaceae" genus="Mallotus" kingdom="Plantae" order="Malpighiales" pageId="11" pageNumber="400" phylum="Tracheophyta" rank="species" species="roxburghianus">
