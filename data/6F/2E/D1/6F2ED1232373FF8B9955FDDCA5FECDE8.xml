<paragraph blockId="2.[869,1092,563,590]" box="[869,1092,563,590]" pageId="2" pageNumber="175">
<heading box="[869,1092,563,590]" centered="true" fontSize="12" level="2" pageId="2" pageNumber="175" reason="2">
<taxonomicName authority="Irmler, 2015" authorityName="Irmler" authorityYear="2015" box="[869,989,563,590]" class="Insecta" family="Staphylinidae" genus="Osoriosetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="175" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis box="[869,989,563,590]" italics="true" pageId="2" pageNumber="175">Osoriosetus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[995,1092,563,590]" pageId="2" pageNumber="175" rank="genus">gen. nov.</taxonomicNameLabel>
