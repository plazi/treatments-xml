<taxonomicName authorityName="Swainson" authorityYear="1840" class="Gastropoda" family="Melanopsidae" genus="Hemisinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemisinus" order="Sorbeoconcha" pageId="149" pageNumber="150" phylum="Mollusca" rank="genus">
<emphasis italics="true" pageId="149" pageNumber="150">Hemisinus</emphasis>
</taxonomicName>
[now 
<taxonomicName authorityName="Bourguignat" authorityYear="1877" class="Gastropoda" family="Melanopsidae" genus="Esperiana" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Esperiana" order="Sorbeoconcha" pageId="149" pageNumber="150" phylum="Mollusca" rank="genus">
<emphasis italics="true" pageId="149" pageNumber="150">Esperiana</emphasis>
</taxonomicName>
] 
<taxonomicName class="Gastropoda" family="Melanopsidae" genus="Esperiana" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="acicularis" order="Sorbeoconcha" pageId="149" pageNumber="150" phylum="Mollusca" rank="species" species="acicularis">
<emphasis italics="true" pageId="149" pageNumber="150">acicularis</emphasis>
