subspecies 
<taxonomicName box="[405,535,261,287]" class="Insecta" family="Culicidae" genus="Wyeomyia" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="45" phylum="Arthropoda" rank="species" species="leucotarsis">
