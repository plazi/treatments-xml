<taxonomicName box="[392,534,770,796]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="43" phylum="Arthropoda" rank="species" species="hargreavesi">
<emphasis bold="true" box="[392,534,770,796]" italics="true" pageId="8" pageNumber="43">hargreavesi</emphasis>
</taxonomicName>
(
<taxonomicName box="[556,727,770,795]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="43" phylum="Arthropoda" rank="species" species="caspius">
