<taxonomicName id="24E9059CFFB74B10F38AFDA1A69DFDF3" class="Magnoliopsida" family="Myrtaceae" genus="Syzygium" higherTaxonomySource="IPNI" kingdom="Plantae" order="Myrtales" pageId="3" pageNumber="268" phylum="Magnoliophyta" rank="species" species="jambos">
