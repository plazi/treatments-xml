<taxonomicName id="24E9059CFFB74B10F026FDE1A7DAFDD3" box="[267,447,608,632]" class="Insecta" family="Tachinidae" genus="Eugenia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="268" phylum="Arthropoda" rank="species" species="uniflora">
