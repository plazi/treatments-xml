<caption id="E3F1C79F505BFFECC1C5FAB9FD72FAB9" ID-DOI="http://doi.org/10.5281/zenodo.5437816" ID-Zenodo-Dep="5437816" httpUri="https://zenodo.org/record/5437816/files/figure.png" pageId="22" pageNumber="105" startId="22.[142,220,1298,1317]" targetBox="[148,1421,210,1263]" targetPageId="22">
<paragraph id="B7319717505BFFECC1C5FAB9FD72FAB9" blockId="22.[142,1422,1298,1347]" pageId="22" pageNumber="105">
<emphasis id="85FA4B05505BFFECC1C5FAB9FE83FADC" bold="true" box="[142,259,1298,1318]" pageId="22" pageNumber="105">Figure 11.</emphasis>
<taxonomicName id="708EEC94505BFFECC044FAB8FD22FADC" authority="(Sharp)" baseAuthorityName="Sharp" baseAuthorityYear="1883" box="[271,674,1299,1318]" class="Insecta" family="Staphylinidae" genus="Hoplandria" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="105" phylum="Arthropoda" rank="species" species="major" subGenus="Platonica">
