<taxonomicName id="FDDC4D13E47BA5155CD3FC2D2158FB88" authorityName="Fisher" authorityYear="1938" box="[669,857,1002,1027]" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="62" phylum="Arthropoda" rank="species" species="quetzalcoatli">
