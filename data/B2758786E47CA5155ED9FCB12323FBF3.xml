</taxonomicName>
and 
<taxonomicName id="FDDC4D13E47BA5155AE2FC03277DFC55" authority="Fisher" authorityName="Fisher" authorityYear="1928" box="[1196,1404,964,990]" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="62" phylum="Arthropoda" rank="species" species="acaciae">
