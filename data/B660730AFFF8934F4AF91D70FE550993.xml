<heading id="653E7570FFF8934C4AF91D70FD7D095E" box="[178,670,1044,1066]" fontSize="8" level="2" pageId="9" pageNumber="590" reason="2">
<emphasis id="0CBD1E0EFFF8934C4AF91D70FD7D095E" bold="true" box="[178,670,1044,1066]" pageId="9" pageNumber="590">
<taxonomicName id="F9C9B99FFFF8934C4AF91D70FDA8095E" authority="A.W, 2008" authorityName="A.W" authorityYear="2008" box="[178,587,1044,1066]" class="Lecanoromycetes" family="Teloschistaceae" genus="Dufourea" kingdom="Fungi" order="Teloschistales" pageId="9" pageNumber="590" phylum="Ascomycota" rank="species" species="bucharica" status="nov.sp." subGenus="Minutodufourea">
<emphasis id="0CBD1E0EFFF8934C4AF91D70FEF4095E" bold="true" box="[178,279,1044,1066]" italics="true" pageId="9" pageNumber="590">Dufourea</emphasis>
