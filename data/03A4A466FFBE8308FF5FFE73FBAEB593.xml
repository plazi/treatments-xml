(
<number id="3DC50757FFB88308FCDFF9BAFC7DB42B" box="[791,907,1552,1576]" pageId="7" pageNumber="50" value="58.05" valueMax="59.5" valueMin="56.6">56.6–59.5</number>
) [vs. 
<number id="3DC50757FFB88308FC01F9BAFC0CB42B" box="[969,1018,1552,1576]" pageId="7" pageNumber="50" value="43.7">43.7</number>
(
<number id="3DC50757FFB88308FBC0F9BAFB89B42B" box="[1032,1151,1552,1576]" pageId="7" pageNumber="50" value="43.8" valueMax="45.4" valueMin="42.2">42.2–45.4</number>
)] % of total length (Table 
<number id="3DC50757FFB88308FF5FF99EFF51B44F" box="[151,167,1588,1612]" pageId="7" pageNumber="50" value="2.0">2</number>
); (
<number id="3DC50757FFB88308FF00F99EFF21B44F" box="[200,215,1588,1612]" pageId="7" pageNumber="50" value="2.0">2</number>
) longer trunk length 
<number id="3DC50757FFB88308FE04F99EFE0BB44F" box="[460,509,1588,1612]" pageId="7" pageNumber="50" value="50.1">50.1</number>
(
<number id="3DC50757FFB88308FDC6F99EFD77B44F" box="[526,641,1588,1612]" pageId="7" pageNumber="50" value="50.3" valueMax="51.8" valueMin="48.8">48.8–51.8</number>
) [vs. 
<number id="3DC50757FFB88308FD0CF99EFD03B44F" box="[708,757,1588,1612]" pageId="7" pageNumber="50" value="34.7">34.7</number>
(
<number id="3DC50757FFB88308FCCEF99EFC8CB44F" box="[774,890,1588,1612]" pageId="7" pageNumber="50" value="34.5" valueMax="36.2" valueMin="32.8">32.8–36.2</number>
) % of total length; (
