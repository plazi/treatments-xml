<taxonomicName authority="O. F. Mueller" authorityName="O. F. Mueller" authorityYear="3000" class="Liliopsida" family="Orchidaceae" genus="Orchis" higherTaxonomySource="CoL" kingdom="Plantae" order="Asparagales" pageId="41" pageNumber="634" phylum="Tracheophyta" rank="species" species="cruenta">
