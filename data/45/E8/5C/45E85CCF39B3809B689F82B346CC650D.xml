<paragraph pageId="null" pageNumber="590">
<taxonomicName authority="All." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="staticifolium">
Hieracium 
<normalizedToken originalValue="staticifólium" pageId="null" pageNumber="590">staticifolium</normalizedToken>
All.
</taxonomicName>
</paragraph>
