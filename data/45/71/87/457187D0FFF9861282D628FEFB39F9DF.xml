<taxonomicName box="[1103,1259,1603,1627]" class="Insecta" family="Sesiidae" genus="Camaegeria" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="30" phylum="Arthropoda" rank="species" species="xanthomos" status="n. sp.">
<emphasis box="[1103,1259,1603,1627]" italics="true" pageId="2" pageNumber="30">C. xanthomos</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[1276,1341,1603,1627]" pageId="2" pageNumber="30">
<taxonomicNameLabel box="[1276,1341,1603,1627]" pageId="2" pageNumber="30" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
<taxonomicName class="Insecta" family="Sesiidae" genus="Camaegeria" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="30" phylum="Arthropoda" rank="species" species="xanthopimplaeformis">
<emphasis italics="true" pageId="2" pageNumber="30">C. xanthopimplaeformis</emphasis>
</taxonomicName>
(
<bibRefCitation box="[363,523,1639,1663]" pageId="2" pageNumber="30" refString="Viette, P. (1955 [imprint &quot; 1954 &quot;]) Nouveaux Tineoidea (s. l.) de Madagascar [Lep.]. Famille Aegeriidae. Annales de la Societe entomologique de France (Deuxieme Serie), 123, 75 - 114." type="journal article">
Viette, [
<number box="[453,516,1639,1663]" pageId="2" pageNumber="30" value="1955.0">1955</number>
]
</bibRefCitation>
), 
<taxonomicName box="[543,671,1639,1663]" class="Insecta" family="Sesiidae" genus="Camaegeria" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="30" phylum="Arthropoda" rank="species" species="lychnitis" status="n. sp.">
<emphasis box="[543,671,1639,1663]" italics="true" pageId="2" pageNumber="30">C. lychnitis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[678,740,1639,1663]" pageId="2" pageNumber="30">
