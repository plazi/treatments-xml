Southern 
<collectingRegion box="[469,586,1303,1327]" country="United States of America" name="California" pageId="51" pageNumber="52">California</collectingRegion>
, 
<collectingCountry box="[596,655,1303,1327]" name="United States of America" pageId="51" pageNumber="52">USA</collectingCountry>
, eastern North Pacific Ocean; 
<quantity box="[996,1109,1303,1327]" metricMagnitude="1" metricUnit="m" metricValue="9.0" metricValueMax="14.0" metricValueMin="4.0" pageId="51" pageNumber="52" unit="m" value="90.0" valueMax="140.0" valueMin="40.0">40–140 m</quantity>
depth.
</paragraph>
</subSubSection>
