; 
<taxonomicName authority="Lowry &amp; Stoddart, 2012" authorityName="Lowry &amp; Stoddart" authorityYear="2012" box="[1160,1293,368,392]" class="Malacostraca" family="Pakynidae" genus="Pachychelium" kingdom="Animalia" order="Amphipoda" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="tropicalis" status="sp. nov.">
