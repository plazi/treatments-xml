<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:148106" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Camponotus foraminosus aldabrensis Forel" lsidName-HNS="Camponotus foraminosus aldabrensis Forel" order="Hymenoptera" pageNumber="203" phylum="Arthropoda" rank="subSpecies" species="foraminosus" subSpecies="aldabrensis">Camponotus foraminosus For. r. aldabrensis</taxonomicName>
<taxonomicNameLabel rank="subSpecies">nov. st.</taxonomicNameLabel>
