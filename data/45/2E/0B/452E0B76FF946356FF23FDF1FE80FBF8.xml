<paragraph blockId="22.[151,1437,861,1036]" pageId="22" pageNumber="23">
Left ala not observed, body 
<number box="[528,579,861,887]" pageId="22" pageNumber="23" value="2.35">2.35</number>
long and 
<number box="[696,725,861,887]" pageId="22" pageNumber="23" value="50.0">50</number>
wide. Cephalic vesicle 
<number box="[1006,1035,861,887]" pageId="22" pageNumber="23" value="35.0">35</number>
long and 
<number box="[1152,1181,861,887]" pageId="22" pageNumber="23" value="18.0">18</number>
wide. Excretory pore and deirids situated 
<number box="[407,518,898,924]" pageId="22" pageNumber="23" value="150180.0">150, 180</number>
from apex, respectively. Oesophagus 
<number box="[1001,1048,898,924]" pageId="22" pageNumber="23" value="180.0">180</number>
long. Vulva situated 
<number box="[1321,1368,898,924]" pageId="22" pageNumber="23" value="120.0">120</number>
from posterior end. Vestibule 
<number box="[442,472,936,962]" pageId="22" pageNumber="23" value="50.0">50</number>
long, sphincter 
<number box="[665,694,936,962]" pageId="22" pageNumber="23" value="35.0">35</number>
long and 
<number box="[815,845,936,962]" pageId="22" pageNumber="23" value="33.0">33</number>
wide. Uterus 
<number box="[1014,1058,936,962]" pageId="22" pageNumber="23" value="350.0">350</number>
long, containing 
<specimenCount box="[1269,1349,936,962]" pageId="22" pageNumber="23" type="egg">
<number box="[1269,1284,936,962]" pageId="22" pageNumber="23" value="5.0">5</number>
eggs
</specimenCount>
, 
<number box="[1362,1436,936,962]" pageId="22" pageNumber="23" value="61.0" valueMax="62.0" valueMin="60.0">60–62</number>
long and 
<number box="[260,333,973,999]" pageId="22" pageNumber="23" value="32.5" valueMax="35.0" valueMin="30.0">30–35</number>
