<taxonomicName authorityName="Chevrolat" authorityYear="1854" class="Insecta" family="Carabidae" genus="Anchomenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anchomenus dorsalis subsp. infuscatus" order="Coleoptera" pageId="0" pageNumber="449" phylum="Arthropoda" rank="subSpecies" species="dorsalis" subSpecies="infuscatus">
