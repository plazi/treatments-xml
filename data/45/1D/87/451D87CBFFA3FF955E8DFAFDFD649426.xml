<subSubSection box="[460,788,1333,1357]" pageId="2" pageNumber="1790" type="nomenclature">
<paragraph blockId="2.[331,916,1269,1357]" box="[460,788,1333,1357]" pageId="2" pageNumber="1790">
<heading box="[460,788,1333,1357]" centered="true" fontSize="10" level="2" pageId="2" pageNumber="1790" reason="4">
Genus 
<taxonomicName box="[539,705,1333,1357]" class="Malacostraca" family="Callianassidae" genus="Ctenocheloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="1790" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[539,705,1333,1357]" italics="true" pageId="2" pageNumber="1790">Ctenocheloides</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[712,788,1333,1357]" pageId="2" pageNumber="1790" rank="genus">n. gen.</taxonomicNameLabel>
</heading>
