<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia" order="Fabales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="genus">Acacia</taxonomicName>
