<taxonomicName class="Insecta" genus="P." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="P. polita subsp. pittieri" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="subSpecies" species="polita" subSpecies="pittieri">
