<typeStatus box="[151,207,259,283]" pageId="2" pageNumber="537">Type</typeStatus>
species: 
<taxonomicName authority="Reid &amp; Beatson" authorityName="Reid &amp; Beatson" box="[309,674,258,283]" class="Insecta" family="Chrysomelidae" genus="Buburra" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="537" phylum="Arthropoda" rank="species" species="jeanae">
