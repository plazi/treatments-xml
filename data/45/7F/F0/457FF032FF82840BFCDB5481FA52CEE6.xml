, 
<collectingCountry box="[1385,1450,1112,1136]" name="Guam" pageId="10" pageNumber="139">Guam</collectingCountry>
, Hawaii, 
<collectingCountry box="[894,989,1138,1162]" name="Indonesia" pageId="10" pageNumber="139">Indonesia</collectingCountry>
, 
<collectingCountry box="[997,1083,1138,1162]" name="Malaysia" pageId="10" pageNumber="139">Malaysia</collectingCountry>
, Maldives’ Islands, 
<collectingCountry box="[1261,1326,1139,1162]" name="Nauru" pageId="10" pageNumber="139">Nauru</collectingCountry>
, 
<collectingCountry box="[1334,1453,1138,1162]" name="Netherlands" pageId="10" pageNumber="139">Netherlands</collectingCountry>
, 
<collectingCountry box="[811,1006,1165,1189]" name="Papua New Guinea" pageId="10" pageNumber="139">Papua New Guinea</collectingCountry>
, 
<collectingCountry box="[1017,1130,1165,1189]" name="Philippines" pageId="10" pageNumber="139">Philippines</collectingCountry>
, 
<collectingCountry box="[1141,1260,1165,1189]" name="Réunion" pageId="10" pageNumber="139">La Réunion</collectingCountry>
, 
<collectingCountry box="[1272,1402,1165,1189]" name="Sint Martin" pageId="10" pageNumber="139">Saint-Martin</collectingCountry>
, 
<collectingCountry name="Singapore" pageId="10" pageNumber="139">Singapore</collectingCountry>
, 
