<collectingCountry name="Micronesia" pageId="10" pageNumber="139">Federated States of Micronesia</collectingCountry>
, 
<collectingCountry box="[1027,1095,1112,1135]" name="France" pageId="10" pageNumber="139">France</collectingCountry>
, 
<collectingCountry box="[1106,1276,1112,1136]" name="French Polynesia" pageId="10" pageNumber="139">French Polynesia</collectingCountry>
, 
