<collectionCode box="[1204,1277,881,905]" country="New Zealand" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" lsid="urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="10" pageNumber="139" type="Other institutional type">NZAC</collectionCode>
