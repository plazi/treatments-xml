<taxonomicName authority="C. Henderson" authorityName="C. Henderson" box="[1264,1456,1703,1727]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="141" phylum="Tracheophyta" rank="genus">Rosa C. Henderson</taxonomicName>
det. (
<collectionCode box="[863,936,1730,1754]" country="New Zealand" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" lsid="urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="12" pageNumber="141" type="Other institutional type">NZAC</collectionCode>
, 
<collectionCode box="[946,1045,1730,1754]" pageId="12" pageNumber="141">CXMNC</collectionCode>
