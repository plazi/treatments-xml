<number box="[947,980,1976,2002]" pageId="12" pageNumber="40" value="23.0">23</number>
); 
<emphasis bold="true" box="[151,181,151,177]" pageId="13" pageNumber="41">
<number box="[151,167,151,177]" pageId="13" pageNumber="41" value="3.0">3</number>
