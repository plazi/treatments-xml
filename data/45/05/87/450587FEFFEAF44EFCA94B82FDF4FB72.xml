<taxonomicName authorityName="Silvestri" authorityYear="1896" box="[332,452,1221,1244]" class="Diplopoda" family="Julidae" genus="Amblyiulus" kingdom="Animalia" order="Julida" pageId="7" pageNumber="89" phylum="Arthropoda" rank="genus">
