<number box="[354,462,1066,1092]" pageId="13" pageNumber="48" value="29.2006">29, 2006</number>
</date>
, from 
<taxonomicName authority="Thunb." authorityName="Thunb." box="[543,852,1066,1092]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="13" pageNumber="48" phylum="Tracheophyta" rank="species" species="tomentosa">
