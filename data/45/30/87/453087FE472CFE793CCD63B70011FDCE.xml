<emphasis box="[180,209,1781,1806]" italics="true" pageId="8" pageNumber="43">
<number box="[180,193,1781,1806]" pageId="8" pageNumber="43" value="1.0">1</number>
a
</emphasis>
) 
<number box="[227,256,1781,1807]" pageId="8" pageNumber="43" value="26.0">26</number>
