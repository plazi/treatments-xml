<number box="[318,347,1581,1607]" pageId="8" pageNumber="43" value="36.0">36</number>
(
<number box="[365,443,1581,1607]" pageId="8" pageNumber="43" value="36.0" valueMax="37.0" valueMin="35.0">35–37</number>
), projecting obliquely down, pedipalp coxal setae (
<emphasis box="[1048,1076,1581,1606]" italics="true" pageId="8" pageNumber="43">ep</emphasis>
) 
<number box="[1093,1108,1581,1607]" pageId="8" pageNumber="43" value="2.0">2</number>
(
<number box="[1125,1173,1581,1607]" pageId="8" pageNumber="43" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis box="[269,284,1621,1646]" italics="true" pageId="8" pageNumber="43">d</emphasis>
) 
<number box="[301,316,1621,1647]" pageId="8" pageNumber="43" value="7.0">7</number>
(
<number box="[332,380,1621,1647]" pageId="8" pageNumber="43" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
