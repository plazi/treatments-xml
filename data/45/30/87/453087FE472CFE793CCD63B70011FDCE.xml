<number box="[1148,1177,1541,1567]" pageId="8" pageNumber="43" value="77.0">77</number>
(
<number box="[1194,1270,1541,1567]" pageId="8" pageNumber="43" value="75.0" valueMax="77.0" valueMin="73.0">73–77</number>
