<number box="[799,828,1701,1727]" pageId="8" pageNumber="43" value="12.0">12</number>
(
<number box="[848,923,1701,1727]" pageId="8" pageNumber="43" value="12.5" valueMax="13.0" valueMin="12.0">12–13</number>
) projecting posteriorly. 
<emphasis bold="true" box="[1216,1373,1701,1727]" pageId="8" pageNumber="43">Coxal plates</emphasis>
with granules and short lines; anterolateral setae on coxisternum І (
<emphasis box="[883,912,1741,1766]" italics="true" pageId="8" pageNumber="43">
<number box="[883,896,1741,1766]" pageId="8" pageNumber="43" value="1.0">1</number>
b
</emphasis>
) 
<number box="[930,959,1741,1767]" pageId="8" pageNumber="43" value="12.0">12</number>
(
<number box="[976,1053,1741,1767]" pageId="8" pageNumber="43" value="12.0" valueMax="14.0" valueMin="10.0">10–14</number>
