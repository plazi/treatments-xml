<number box="[660,689,1701,1727]" pageId="13" pageNumber="48" value="17.0">17</number>
(
<number box="[708,786,1701,1727]" pageId="13" pageNumber="48" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
). 
<emphasis bold="true" box="[808,1014,1701,1727]" pageId="13" pageNumber="48">Prodorsal shield</emphasis>
<number box="[1023,1052,1701,1727]" pageId="13" pageNumber="48" value="40.0">40</number>
(
<number box="[1070,1148,1701,1727]" pageId="13" pageNumber="48" value="40.5" valueMax="41.0" valueMin="40.0">40–41</number>
