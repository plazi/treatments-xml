<number box="[1171,1200,1701,1727]" pageId="13" pageNumber="48" value="43.0">43</number>
(
<number box="[1218,1293,1701,1727]" pageId="13" pageNumber="48" value="41.5" valueMax="43.0" valueMin="40.0">40–43</number>
) wide; subpentagonal; anterior shield lobe broad; median, admedian and submedian lines present, median and admedian lines connected near base. Scapular tubercles on rear margin, 
<number box="[874,903,1781,1807]" pageId="13" pageNumber="48" value="27.0">27</number>
(
<number box="[919,995,1781,1807]" pageId="13" pageNumber="48" value="25.5" valueMax="27.0" valueMin="24.0">24–27</number>
