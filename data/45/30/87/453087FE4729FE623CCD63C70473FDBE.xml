<number box="[376,405,1661,1687]" pageId="13" pageNumber="48" value="18.0">18</number>
(
<number box="[423,501,1661,1687]" pageId="13" pageNumber="48" value="18.5" valueMax="20.0" valueMin="17.0">17–20</number>
), projecting obliquely down, pedipalp coxal setae (
<emphasis box="[1107,1134,1661,1686]" italics="true" pageId="13" pageNumber="48">ep</emphasis>
) 
<number box="[1153,1168,1661,1687]" pageId="13" pageNumber="48" value="3.0">3</number>
(
<number box="[1185,1233,1661,1687]" pageId="13" pageNumber="48" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis box="[316,331,1701,1726]" italics="true" pageId="13" pageNumber="48">d</emphasis>
) 
<number box="[349,364,1701,1727]" pageId="13" pageNumber="48" value="9.0">9</number>
(
<number box="[382,430,1701,1727]" pageId="13" pageNumber="48" value="8.0" valueMax="9.0" valueMin="7.0">7–9</number>
