<paragraph pageId="0" pageNumber="123">
<taxonomicName authorityName="Hering" authorityYear="1939" class="Insecta" family="Tephritidae" genus="Platensina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platensina tetrica" order="Diptera" pageId="0" pageNumber="123" phylum="Arthropoda" rank="species" species="tetrica">Platensina tetrica</taxonomicName>
