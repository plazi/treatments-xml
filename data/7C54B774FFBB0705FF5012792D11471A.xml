<taxonomicName id="33FD7DE1FFBB0707FB8B13A22B2E44EA" authority="Schatz &amp; Lowry Ii &amp; Dc &amp; Dc, 2020" authorityName="Schatz &amp; Lowry Ii &amp; Dc &amp; Dc" authorityYear="2020" box="[1138,1335,1671,1695]" class="Magnoliopsida" family="Ebenaceae" genus="Diospyros" kingdom="Plantae" order="Ericales" pageId="12" pageNumber="211" phylum="Tracheophyta" rank="species" species="darainensis" status="sp. nov.">
