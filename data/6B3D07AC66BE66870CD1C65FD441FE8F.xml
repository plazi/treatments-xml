<taxonomicName id="9E7BEBDFE96D5593CE5E672B22CDB1E5" authorityName="Latreille" authorityYear="1802" class="Hexapoda" family="Formicidae" genus="Formica" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="3" pageNumber="74" phylum="Arthropoda" rank="species" species="longicornis">Formica longicornis</taxonomicName>
