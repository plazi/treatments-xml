<subSubSection id="B8C3653FFFAAFFD9FF24F0CECC0EFF42" pageId="15" pageNumber="272" type="distribution">
<paragraph id="F06636B4FFAAFFD9FF24F0CECC0EFF42" blockId="15.[151,1437,152,359]" pageId="15" pageNumber="272">
<emphasis id="C2ADEAA6FFAAFFD9FF24F0CECC1BFF26" bold="true" box="[199,355,152,179]" pageId="15" pageNumber="272">Distribution:</emphasis>
<collectingCountry id="88CE7624FFAAFFD9FE8EF0CFCCBDFF26" box="[365,453,153,179]" name="Mexico" pageId="15" pageNumber="272">Mexico</collectingCountry>
, 
<collectingCountry id="88CE7624FFAAFFD9FE37F0CFCF35FF26" box="[468,589,153,179]" name="Guatemala" pageId="15" pageNumber="272">Guatemala</collectingCountry>
and 
<collectingCountry id="88CE7624FFAAFFD9FD69F0CFCF85FF26" box="[650,765,153,179]" name="Nicaragua" pageId="15" pageNumber="272">Nicaragua</collectingCountry>
(
<bibRefCitation id="94484B45FFAAFFD9FCECF0CFC90DFF26" author="Randolph, R. P. &amp; McCafferty, W. P." box="[783,1141,153,179]" pageId="15" pageNumber="272" pagination="15 - 21" refId="ref7446" refString="Randolph, R. P. &amp; McCafferty, W. P. (2001) New species and records of Ephemeroptera (Insecta) from central Mexico. Dugesiana, 8 (1), 15 - 21." type="journal article" year="2001">Randolph &amp; McCafferty, 2001</bibRefCitation>
; 
