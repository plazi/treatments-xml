<quantity id="4CE39B1AFFD2FFCFFEB261B2B8CCFB70" box="[375,420,1209,1233]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="69" pageNumber="472" unit="m" value="1.0">1 m</quantity>
