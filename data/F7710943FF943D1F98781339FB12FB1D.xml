<taxonomicName id="B8D8C3D6FF943D1E98A51516FD7AF8F1" box="[469,650,1843,1866]" class="Diplopoda" family="Trichopetalidae" genus="Trigenotyla" higherTaxonomySource="CoL" kingdom="Animalia" order="Chordeumatida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="vaga">
