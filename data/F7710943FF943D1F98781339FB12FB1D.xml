<taxonomicName id="B8D8C3D6FF943D1E9B261516FD7AF8F1" authority="Causey." authorityName="Causey." box="[598,650,1843,1866]" pageId="24" pageNumber="25" rank="species" species="vaga">vaga</taxonomicName>
