<paragraph id="B29A9830FD66CFEAE0EABBBC3DA238AF" pageId="4" pageNumber="47">
<taxonomicName id="E92FC80B27BCD2BEBF22B195744C317A" class="Insecta" family="Myrmeleontidae" genus="Gymnocnemia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnocnemia variegata" order="Neuroptera" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="variegata">Gymnocnemia variegata</taxonomicName>
