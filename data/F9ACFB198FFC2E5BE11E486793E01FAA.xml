<taxonomicName id="07EF5AB8805B94260B854A3241CCFD4F" class="Insecta" family="Myrmeleontidae" genus="Gymnocnemia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnocnemia editaerevayae" order="Neuroptera" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="editaerevayae">Gymnocnemia editaerevayae</taxonomicName>
, and 
<taxonomicName id="666395BA22218C61B090202E9284A704" class="Insecta" family="Myrmeleontidae" genus="Gymnocnemia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnocnemia mirabilis" order="Neuroptera" pageId="4" pageNumber="47" phylum="Arthropoda" rank="species" species="mirabilis">Gymnocnemia mirabilis</taxonomicName>
