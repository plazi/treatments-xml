<taxonomicName id="893835810D7D0CCB7CA055627F235C38" class="Insecta" family="Ichneumonidae" genus="Campoplex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Campoplex" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Campoplex</taxonomicName>
