<taxonomicName id="D3455F4796F0E02271F7C73F27072D66" class="Insecta" family="Ichneumonidae" genus="Campoplex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Campoplex difformis" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="difformis">Campoplex difformis</taxonomicName>
