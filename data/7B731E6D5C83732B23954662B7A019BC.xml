<taxonomicName id="2FC22105157CA83F78BA3D61384863B7" class="Insecta" family="Ichneumonidae" genus="Venturia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Venturia deficiens" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="deficiens">Venturia deficiens</taxonomicName>
