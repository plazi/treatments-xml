<taxonomicName id="8FEE4D41FF8EFFA5FF57FDE16499C926" authorityName="in Roberts" authorityYear="1964" box="[199,474,582,609]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="loculosum">
