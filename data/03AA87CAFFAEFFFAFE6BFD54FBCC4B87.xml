<taxonomicName id="4C034D5FFFAEFFFAFC31FCC8FB844B7C" baseAuthorityName="Dana" baseAuthorityYear="1846" box="[1006,1127,843,864]" class="Anthozoa" family="Acroporidae" genus="Acropora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="69" pageNumber="68" phylum="Cnidaria" rank="species" species="horrida">
