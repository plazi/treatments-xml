<taxonomicName id="F1A74D36897F2657869A13B5EFC24CDF" authority="BRONN" authorityName="BRONN" box="[918,1221,673,694]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="7" pageNumber="299" phylum="Tracheophyta" rank="species" species="tricuspidatum">
<emphasis id="04D3EAA7897F2657869A13B5E8C44CDC" box="[918,963,673,694]" italics="true" pageId="7" pageNumber="299">Acer</emphasis>
