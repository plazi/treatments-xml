<subSubSection id="0FCA9165E4AB428428CA594070D9B2E6" pageId="31" pageNumber="46" type="diagnosis">
<paragraph id="7A8EC80AA48FCC460ED59B1F7EB5B4D1" pageId="31" pageNumber="46">Diagnosis.</paragraph>
<paragraph id="A372277EC5A1355D326883931AF7DFE9" pageId="31" pageNumber="46">Small to medium-sized (5.1-7.5 mm); vestiture of lanceolate scales and setae; postocular lobes present; elytra lacking anteapical tubercle.</paragraph>
</subSubSection>
<subSubSection id="B5247C9F55650E17515C712A746026F2" pageId="31" pageNumber="46" type="relationships">
<paragraph id="FD228AB81F427B58F45A6A665134669C" pageId="31" pageNumber="46">Relationships.</paragraph>
<paragraph id="E8420E9525224EDB79FC1564DA038342" pageId="31" pageNumber="46">
<taxonomicName id="CC2B9DCB752ECEAEA29F546F27F44348" authorityName="Marshall" authorityYear="1914" class="Insecta" family="Curculionidae" genus="Hyperoides" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="31" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="6FCE10BD3714C732CA021FC3AAD0E55C" italics="true" pageId="31" pageNumber="46">Hyperoides</emphasis>
</taxonomicName>
