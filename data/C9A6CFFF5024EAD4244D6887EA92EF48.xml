<taxonomicName id="90DD8C6DBF048C06A46D4CFDA485D224" LSID-HNS="urn:lsid:biosci.ohio-state.edu:137937" class="Insecta" family="Formicidae" genus="Dorylus" kingdom="Animalia" lsidName-HNS="Dorylus (Anomma) stanleyi Forel" order="Hymenoptera" pageNumber="218" phylum="Arthropoda" rank="species" species="stanleyi" subGenus="Anomma">D. (Anomma) stanleyi</taxonomicName>
