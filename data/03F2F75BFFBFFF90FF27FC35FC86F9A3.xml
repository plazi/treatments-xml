<location id="8E841096FFBFFF90FAA7FAD0FA42FA5F" LSID="urn:lsid:plazi:treatment:03F2F75BFFBFFF90FF27FC35FC86F9A3:8E841096FFBFFF90FAA7FAD0FA42FA5F" box="[1303,1425,1383,1408]" country="Peru" county="Cuzco" municipality="Junin" name="Alemanha" pageId="1" pageNumber="128" stateProvince="Para">Alemanha</location>
)
</materialsCitation>
