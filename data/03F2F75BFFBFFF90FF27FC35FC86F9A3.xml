<collectingMunicipality id="6B80DC37FFBFFF90FA94FAF4FAB1FA83" box="[1316,1378,1347,1372]" pageId="1" pageNumber="128">Junin</collectingMunicipality>
) and 
<collectingCountry id="F34C06DDFFBFFF90FF27FAD0FF08FA5F" box="[151,219,1383,1408]" name="Brazil" pageId="1" pageNumber="128">Brazil</collectingCountry>
