<paragraph id="8B8636BFFF8FFFB509F0F9E0ACD0CC56" blockId="32.[151,1436,1575,2031]" box="[151,917,1575,1599]" pageId="32" pageNumber="33">
Host: 
<taxonomicName id="4C394D3CFF8FFFB509BCF9E0AD42CC56" authorityName="Dybowski" authorityYear="1870" box="[219,519,1575,1599]" class="Amphibia" family="Hynobiidae" genus="Salamandrella" kingdom="Animalia" order="Caudata" pageId="32" pageNumber="33" phylum="Chordata" rank="species" species="keyserlingii">
<emphasis id="B94DEAADFF8FFFB509BCF9E0AD42CC56" box="[219,519,1575,1599]" italics="true" pageId="32" pageNumber="33">Salamandrella keyserlingii</emphasis>
</taxonomicName>
(
<taxonomicName id="4C394D3CFF8FFFB50B70F9E0ADC8CC56" box="[535,653,1575,1599]" class="Amphibia" kingdom="Animalia" pageId="32" pageNumber="33" phylum="Chordata" rank="class">Amphibia</taxonomicName>
