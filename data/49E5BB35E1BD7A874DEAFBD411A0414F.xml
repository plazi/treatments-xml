<taxonomicName id="85DA7ED4E9EE13C4808DBE76BD06D7F4" class="Arachnida" family="Haplozetidae" genus="Trachyoribates" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trachyoribates (Rostrozetes) glaber" order="Sarcoptiformes" pageId="10" pageNumber="53" phylum="Arthropoda" rank="species" species="glaber" subGenus="Rostrozetes">Trachyoribates (Rostrozetes) glaber</taxonomicName>
