</taxonomicName>
, 
<taxonomicName id="6ACB4D39FF9D270FFF36FF73E69083E4" authorityName="Watts &amp; Humphreys" authorityYear="2006" box="[1097,1185,224,248]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="species" species="exilis">
<emphasis id="9FBFEAA8FF9D270FFF36FF73E69083E4" box="[1097,1185,224,248]" italics="true" pageId="88" pageNumber="89">L. exilis</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FF9D270FFFCFFF73E74683E4" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" box="[1200,1399,224,248]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="species" species="fridaywellensis">
