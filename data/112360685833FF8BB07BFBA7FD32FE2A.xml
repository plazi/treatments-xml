<heading id="C27D66125833FF84B07BFBA7FC1FFB8D" box="[597,983,1118,1142]" centered="true" fontSize="10" level="2" pageId="6" pageNumber="2854" reason="3">
<taxonomicName id="5E8AAAFD5833FF84B07BFBA7FCBCFB8D" authority="Akingbohungbe, 2003" authorityName="Akingbohungbe" authorityYear="2003" box="[597,884,1118,1142]" class="Insecta" family="Miridae" genus="Isometopus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="2854" phylum="Arthropoda" rank="species" species="quadrivittatus" status="sp. nov.">
<emphasis id="ABFE0D6C5833FF84B07BFBA7FCBCFB8D" bold="true" box="[597,884,1118,1142]" italics="true" pageId="6" pageNumber="2854">Isometopus quadrivittatus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="B0CDB0175833FF84B152FBA7FC1FFB8D" box="[892,983,1119,1142]" pageId="6" pageNumber="2854" rank="species">sp. nov.</taxonomicNameLabel>
