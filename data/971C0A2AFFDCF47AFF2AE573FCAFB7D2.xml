<taxonomicName id="D8B5C0BFFFDDF47CFEA7E076FE5EB085" authority="Lan &amp; Yao &amp; Zheng &amp; Wongprom &amp; Li, 2020" authorityName="Lan &amp; Yao &amp; Zheng &amp; Wongprom &amp; Li" authorityYear="2020" box="[282,427,1896,1920]" class="Arachnida" family="Pholcidae" genus="Savarna" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="322" phylum="Arthropoda" rank="species" species="satun" status="sp. nov.">
