<taxonomicName id="4C114D5BB57DFEE1C44F77C4E2B0FC5E" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[313,459,939,960]" class="Arachnida" family="Lamponidae" genus="Longepi" kingdom="Animalia" order="Araneae" pageId="294" pageNumber="295" phylum="Arthropoda" rank="species" species="tarra" status="new species">
