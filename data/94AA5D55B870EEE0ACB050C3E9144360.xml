[[ 
<taxonomicName id="C68C2AD3CF953018A2D35CF9E6A9EAC5" LSID-ZBK="urn:lsid:zoobank.org:act:1654F3F7-4BF7-44EE-BD21-80A538552441" class="Actinopterygii" family="Gobiidae" genus="Akko" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Akko Birdsong &amp; Robins 1995:676" order="Perciformes" pageNumber="10" phylum="Chordata" rank="genus">Akko Birdsong and Robins</taxonomicName>
