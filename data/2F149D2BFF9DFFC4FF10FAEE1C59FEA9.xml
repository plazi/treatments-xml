<subSubSection id="EFA77FB6FF9DFFC8FF40FA5E1D43F9BC" pageId="20" pageNumber="21" type="etymology">
<paragraph id="A7022C3DFF9DFFC8FF40FA5E1D43F9BC" blockId="20.[151,1436,1482,2019]" pageId="20" pageNumber="21">
<emphasis id="95C9F02FFF9DFFC8FF40FA5E1E15F991" bold="true" box="[199,334,1519,1543]" pageId="20" pageNumber="21">Etymology.</emphasis>
