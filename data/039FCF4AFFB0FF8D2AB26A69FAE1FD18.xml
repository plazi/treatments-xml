<taxonomicName id="4C3605DFFFBDFF8D2F0A6FBDFA6BFD7D" authorityName="Weise" authorityYear="1901" box="[1327,1437,671,697]" class="Insecta" family="Coccinellidae" genus="Horniolus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="553" phylum="Arthropoda" rank="genus">Horniolus</taxonomicName>
