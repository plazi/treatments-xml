<taxonomicName id="4C3605DFFFBDFF8D286C6FE3FD40FD1F" authorityName="Weise" authorityYear="1901" box="[585,694,705,731]" class="Insecta" family="Coccinellidae" genus="Horniolus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="553" phylum="Arthropoda" rank="genus">Horniolus</taxonomicName>
