<taxonomicName id="4C344D4A034CE162FC0CF8DEFAC7505F" authority="Pasc., DEI Muncheberg" authorityName="Pasc., DEI Muncheberg" box="[920,1375,1861,1887]" class="Insecta" family="Tenebrionidae" genus="Phaedis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="443" phylum="Arthropoda" rank="species" species="elysius">
<emphasis id="B940EADB034CE162FC0CF8DEFBDF505F" box="[920,1095,1861,1887]" italics="true" pageId="4" pageNumber="443">Phaedis elysius</emphasis>
Pasc., DEI Müncheberg
</taxonomicName>
