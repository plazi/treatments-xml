, 
<taxonomicName id="4C344D4A034CE162FE67F8DEFDF1505F" authority="Balu" authorityName="Balu" box="[499,617,1861,1887]" class="Echinoidea" family="Schizasteridae" genus="Kina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spatangoida" pageId="4" pageNumber="443" phylum="Echinodermata" rank="genus">Kina Balu</taxonomicName>
