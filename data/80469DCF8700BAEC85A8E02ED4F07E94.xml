<taxonomicName id="CBE84DCD0863EEF0762D2E5C4FC6EB6F" authorityName="Schmidt" authorityYear="1862" class="Demospongiae" family="Ancorinidae" genus="Stelletta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stelletta grubii" order="Tetractinellida" pageId="12" pageNumber="75" phylum="Annelida" rank="species" species="grubii">
