<taxonomicName id="9286DE30D9E8148DFA60313C14F76E56" baseAuthorityName="G. B. Sowerby II" baseAuthorityYear="1871" class="Bivalvia" family="Ostreidae" genus="Cryptostrea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptostrea permollis" order="Ostreida" pageId="12" pageNumber="75" phylum="Mollusca" rank="species" species="permollis">
