<taxonomicName id="CC4A0ED4F89A87DB731C805192E28A12" baseAuthorityName="Walbaum" baseAuthorityYear="1792" class="Actinopterygii" family="Pleuronectidae" genus="Pseudopleuronectes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Pseudopleuronectes americanus" order="Pleuronectiformes" pageId="12" pageNumber="75" phylum="Chordata" rank="species" species="americanus">
