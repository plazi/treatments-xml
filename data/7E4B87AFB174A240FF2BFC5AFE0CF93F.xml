<quantity id="311A9B5CB174A240FF58FB88FEB3FB86" box="[228,293,1092,1116]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="66" pageNumber="67" unit="cm" value="2.0">2 cm</quantity>
, bottom rocky, water temperature 7.0˚C, partially shaded, elevation 
<quantity id="311A9B5CB174A240FBDFFB88FB5FFB86" box="[1123,1225,1092,1116]" metricMagnitude="3" metricUnit="m" metricValue="1.893" pageId="66" pageNumber="67" unit="m" value="1893.0">1,893 m</quantity>
