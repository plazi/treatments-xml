<quantity id="311A9B5CB174A240FAF3FBECFA0EFBED" box="[1359,1432,1056,1080]" metricMagnitude="-1" metricUnit="m" metricValue="2.0" pageId="66" pageNumber="67" unit="cm" value="20.0">20 cm</quantity>
