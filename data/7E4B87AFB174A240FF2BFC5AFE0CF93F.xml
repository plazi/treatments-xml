and 
<collectingCountry id="8EF57629B174A240FDBDFB63FDF6FB12" box="[513,608,1199,1224]" name="Vietnam" pageId="66" pageNumber="67">Vietnam</collectingCountry>
(
<collectingRegion id="3426F85BB174A240FDD3FB7CFD5DFB12" box="[623,715,1199,1224]" country="Vietnam" name="Lao Cai" pageId="66" pageNumber="67">Lao Cai</collectingRegion>
