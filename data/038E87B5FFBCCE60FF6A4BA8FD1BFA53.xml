<taxonomicName id="4C274D20FFA3CE60FEE84F0FFE4DFA88" authorityName="Lopes-Andrade" authorityYear="2008" box="[277,392,1376,1401]" class="Insecta" family="Ciidae" genus="Scolytocis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="lawrencei">
<emphasis id="B953EAB1FFA3CE60FEE84F0FFE4DFA88" box="[277,392,1376,1401]" italics="true" pageId="16" pageNumber="17">lawrencei</emphasis>
</taxonomicName>
