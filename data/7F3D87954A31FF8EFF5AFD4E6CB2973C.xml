<taxonomicName id="30944D004A31FF8EFD68FD4F6C8696A6" baseAuthorityName="Kaszab" baseAuthorityYear="1960" box="[677,755,719,745]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="atavus">
