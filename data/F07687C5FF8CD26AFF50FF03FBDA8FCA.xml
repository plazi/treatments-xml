<quantity id="BF279B36FF8CD26AFC9DFECBFC738D0A" box="[858,962,297,324]" metricMagnitude="-3" metricUnit="m" metricValue="5.95" pageId="19" pageNumber="320" unit="mm" value="5.95">5.95 mm</quantity>
