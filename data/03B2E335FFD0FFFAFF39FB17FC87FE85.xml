<taxonomicName id="4C1B29A0FFD2FFFAFD7DFF0BFC61FF4C" box="[723,978,188,212]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="384" phylum="Tracheophyta" rank="species" species="drepanolobium">
