<taxonomicName id="4C1B29A0FFD3FFFBFC53F8E6FAC2F8FD" authority="Meyrick" authorityName="Meyrick" box="[1021,1393,1871,1894]" class="Insecta" family="Tortricidae" genus="Paraeccopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="383" phylum="Arthropoda" rank="species" species="phoeniodes">
