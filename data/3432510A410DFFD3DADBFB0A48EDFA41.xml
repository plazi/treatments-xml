Figs. 
<number id="0A53F23B410DFFD2DAAEFAD048FCFAD1" box="[226,242,1312,1338]" pageId="8" pageNumber="9" value="8.18">8</number>
</figureCitation>
, 
<figureCitation id="24A0FC99410DFFD2DAB3FAD04910FAD1" box="[255,286,1312,1338]" captionStart="FIGURES 11 – 20" captionStartId="13.[151,272,1649,1673]" captionTargetBox="[151,1396,248,1578]" captionTargetId="figure@13.[151,1436,197,1624]" captionTargetPageId="13" captionText="FIGURES 11 – 20. Anastoechus wings, dorsal view. 11. Anastoechus asiaticus Becker; 12. Anastoechus aurecrinitus Yang et Du; 13. Anastoechus candidus sp. nov.; 14. Anastoechus chakanus Du et al.; 15. Anastoechus chinensis Paramonov; 16. Anastoechus doulananus Du et al.; 17. Anastoechus fulvus sp. nov.; 18. Anastoechus turriformis sp. nov.; 19. Anastoechus xuthus sp. nov.; 20. Anastoechus neimongolanus Du &amp; Yang." httpUri="https://zenodo.org/record/195262/files/figure.png" pageId="8" pageNumber="9">18</figureCitation>
, 
<figureCitation id="24A0FC99410DFFD2DB67FAD0497BFAD1" box="[299,373,1312,1338]" captionStart="FIGURES 61 – 66" captionStartId="21.[151,276,1812,1836]" captionTargetBox="[159,1426,204,1776]" captionTargetId="figure@21.[151,1436,194,1790]" captionTargetPageId="21" captionText="FIGURES 61 – 66. Anastoechus turriformis sp. nov. male genitalia. 61. epandrium and cercus, dorsal view; 62. epandrium and cercus, lateral view; 63. gonocoxite and gonostylus, lateral view; 64. phallus, dorsal view; 65. phallus, lateral view; 66. gonocoxite and gonostyli, ventral view." httpUri="https://zenodo.org/record/195270/files/figure.png" pageId="8" pageNumber="9">
<number id="0A53F23B410DFFD2DB67FAD0497BFAD1" box="[299,373,1312,1338]" pageId="8" pageNumber="9" value="63.5" valueMax="66.0" valueMin="61.0">61–66</number>
