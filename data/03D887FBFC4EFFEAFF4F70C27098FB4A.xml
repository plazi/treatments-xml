<number id="3DB924CAFC4FFFE9FDB376C971C5FE09" box="[619,675,434,459]" pageId="25" pageNumber="426" value="1956.0">1956</number>
sub 
<taxonomicName id="4C714D6EFC4FFFE9FD0076C97753FE09" box="[728,1077,434,459]" class="Insecta" family="Carabidae" genus="Cicindela" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="426" phylum="Arthropoda" rank="species" species="cincta" subGenus="Euryarthron">
