<emphasis id="B9191934FFF5FF9FFE321F6EFEA5FCB9" bold="true" pageId="21" pageNumber="374">
<collectingCountry id="F37A85B6FFF5FF9FFE321F6EFEA5FCB9" name="Dominican Republic" pageId="21" pageNumber="374">DOMINICAN REPUBLIC</collectingCountry>
