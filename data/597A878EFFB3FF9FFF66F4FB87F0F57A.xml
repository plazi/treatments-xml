<taxonomicName id="16D34D1BFFB3FF9FFC84F13B8309F522" authorityName="Osborn" authorityYear="1934" box="[885,1030,1555,1581]" class="Insecta" family="Cicadellidae" genus="Hecaloidella" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="396" phylum="Arthropoda" rank="genus">
