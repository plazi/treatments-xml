<taxonomicName id="3CFB1FF0FB01FF81FC20F9A46397F9C7" authorityName="Kott" authorityYear="2005" box="[996,1185,1636,1655]" class="Ascidiacea" family="Didemnidae" genus="Lissoclinum" kingdom="Animalia" order="Aplousobranchia" pageId="51" pageNumber="2459" phylum="Chordata" rank="species" species="clavatum">
