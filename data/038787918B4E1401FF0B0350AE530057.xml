<taxonomicName id="4C2E4D048B4E1401FF6A0350AADF0122" box="[246,391,1592,1618]" class="Insecta" family="Naucoridae" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="199" phylum="Arthropoda" rank="family">Naucoridae</taxonomicName>
