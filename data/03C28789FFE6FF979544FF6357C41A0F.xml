<taxonomicName id="4C6B4D1CFFE7FF9797AAFA705215198F" box="[999,1096,1443,1461]" class="Insecta" family="Staphylinidae" genus="Pronomaea" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="380" phylum="Arthropoda" rank="species" species="rostrata">
