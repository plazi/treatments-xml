<quantity id="4C939B7AFFE7FF979579FA3357DA19CD" box="[308,391,1504,1528]" metricMagnitude="3" metricUnit="m" metricValue="2.02" pageId="13" pageNumber="380" unit="m" value="2020.0">2020 m</quantity>
