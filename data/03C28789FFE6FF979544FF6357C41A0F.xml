<taxonomicName id="4C6B4D1CFFE7FF9794A9FAC157FD1918" authority="Assing, 2007" authorityName="Assing" authorityYear="2007" box="[228,416,1298,1314]" class="Insecta" family="Staphylinidae" genus="Pronomaea" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="380" phylum="Arthropoda" rank="species" species="corsicana" status="sp. n.">
