<taxonomicName id="985FD4AB29DC369C97BDA0C796693482" class="Arachnida" family="Psilodercidae" genus="Althepus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Althepus sepakuensis" order="Araneae" pageId="12" pageNumber="39" phylum="Arthropoda" rank="species" species="sepakuensis">Althepus sepakuensis</taxonomicName>
