<bibRefCitation id="EFFC1F82FFA8C412FE6853C7FC80FB68" author="Ek-Amnuay" box="[474,769,1250,1273]" pageId="0" pageNumber="445" refString="Ek-Amnuay, P., Chiba, H., Kimura, Y., Inayoshi, Y., Saito, K., Seki, Y. &amp; Uemura, Y. (2007) Corrigenda to &quot; Butterflies of Thailand &quot; (Ek-Amnuay, 2006). Yadoriga, 213, 2 - 20." type="journal article" year="2007">
Ek-Amnuay 
<emphasis id="B919BE61FFA8C412FDDC53C7FD26FB68" box="[622,679,1250,1273]" italics="true" pageId="0" pageNumber="445">et al</emphasis>
. (
<number id="3DA57054FFA8C412FD7553C7FC80FB68" box="[711,769,1250,1273]" pageId="0" pageNumber="445" value="2007.0">2007</number>
</bibRefCitation>
: 
<number id="3DA57054FFA8C412FCA453C7FCB0FB68" box="[790,817,1250,1273]" pageId="0" pageNumber="445" value="16.0">16</number>
</treatmentCitation>
) (nomenclature); de 
<treatmentCitation id="0ACC4462FFA8C412FB8353C7FA1DFB68" author="Jong" box="[1073,1436,1250,1273]" page="10" pageId="0" pageNumber="445" year="2008">
<bibRefCitation id="EFFC1F82FFA8C412FB8353C7FA1DFB68" author="Jong" box="[1073,1436,1250,1273]" pageId="0" pageNumber="445" refString="de Jong, R. &amp; Treadaway, C. G. (2008) Hesperiidae I: Hesperiidae of the Philippine Islands. Butterflies of the World, Part 29, 1 - 17. [plates. 1 - 39]" type="book chapter" year="2008">
Jong &amp; Treadaway (
<number id="3DA57054FFA8C412FA9B53C7FAE2FB68" box="[1321,1379,1250,1273]" pageId="0" pageNumber="445" value="2008.0">2008</number>
: 
<number id="3DA57054FFA8C412FACA53C7FA12FB68" box="[1400,1427,1250,1273]" pageId="0" pageNumber="445" value="10.0">10</number>
)
</bibRefCitation>
</treatmentCitation>
(distribution); 
<treatmentCitation id="0ACC4462FFA8C412FED05227FD5EFA88" author="Mohanraj" box="[354,735,1282,1305]" page="6" pageId="0" pageNumber="445" year="2011">
<bibRefCitation id="EFFC1F82FFA8C412FED05227FD5EFA88" author="Mohanraj" box="[354,735,1282,1305]" pageId="0" pageNumber="445" refString="Mohanraj, P. &amp; Veenakumari, K. (2011) Butterflies of the Andaman and Nicobar islands: History of collection and checklist. Zootaxa, 3050, 1 - 36." type="journal article" year="2011">
Mohanraj &amp; Veenakumari (
<number id="3DA57054FFA8C412FD345227FD3EFA88" box="[646,703,1282,1305]" pageId="0" pageNumber="445" value="2011.0">2011</number>
: 
<number id="3DA57054FFA8C412FD7B5227FD56FA88" box="[713,727,1282,1305]" pageId="0" pageNumber="445" value="6.0">6</number>
)
