<paragraph blockId="9.[819,1435,1638,1905]" pageId="9" pageNumber="388">
<typeStatus box="[819,921,1668,1690]" pageId="9" pageNumber="388">Holotype</typeStatus>
lacking posterior end, 
<quantity box="[1196,1274,1668,1690]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" pageId="9" pageNumber="388" unit="mm" value="15.0">15 mm</quantity>
