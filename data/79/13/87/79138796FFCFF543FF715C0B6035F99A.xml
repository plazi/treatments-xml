<taxonomicName authority="Sm." authorityName="Smith" authorityYear="1854" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="species" species="mexicana">
