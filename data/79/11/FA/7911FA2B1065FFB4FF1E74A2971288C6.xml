<subSubSection pageId="7" pageNumber="108" type="materials_examined">
<paragraph blockId="7.[151,1437,1522,2014]" pageId="7" pageNumber="108">
<emphasis bold="true" box="[151,378,1522,1547]" pageId="7" pageNumber="108">Material examined</emphasis>
. Le Danois Bank, 
<number box="[593,653,1522,1546]" pageId="7" pageNumber="108" value="2008.0">2008</number>
: Stn. R 
<number box="[738,748,1522,1546]" pageId="7" pageNumber="108" value="1.0">1</number>
R 
<quantity box="[769,871,1522,1546]" metricMagnitude="3" metricUnit="m" metricValue="2.751" pageId="7" pageNumber="108" unit="m" value="2751.0">2, 751 m</quantity>
