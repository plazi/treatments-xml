<taxonomicName authorityName="Yan &amp; Lee &amp; Forges &amp; Ng" authorityYear="2021" baseAuthorityName="Rathbun" baseAuthorityYear="1932" box="[842,1027,1277,1301]" class="Malacostraca" family="Epialtidae" genus="Siderochinia" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="36" phylum="Arthropoda" rank="species" species="kagoshimensis">
