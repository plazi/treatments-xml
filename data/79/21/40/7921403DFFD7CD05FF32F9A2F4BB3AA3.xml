<collectingCountry box="[711,826,1146,1172]" name="Indonesia" pageId="21" pageNumber="52">Indonesia</collectingCountry>
[Sumatra (
<bibRefCitation author="Reimoser" box="[959,1145,1146,1172]" pageId="21" pageNumber="52" refString="Reimoser, E. (1927) Spinnen von Sumatras Ostkuste. Miscellanea Zoologica Sumatrana, 13, 1 - 6." type="journal article" year="1927">
Reimoser 
<number box="[1082,1145,1146,1172]" pageId="21" pageNumber="52" value="1927.0">1927</number>
</bibRefCitation>
), Java (
<bibRefCitation author="Reimoser" box="[1236,1421,1146,1172]" pageId="21" pageNumber="52" refString="Reimoser, E. (1931) Echte Spinnen der deutschen limnologischen Sunda-Expedition. Archiv fur Hydrobiologie, 8 (Suppl.), 759 - 770." type="journal article" year="1931">
Reimoser 
<number box="[1359,1421,1146,1172]" pageId="21" pageNumber="52" value="1931.0">1931</number>
</bibRefCitation>
), sub 
<taxonomicName box="[200,479,1184,1209]" class="Arachnida" family="Lycosidae" genus="Lycosa" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="52" phylum="Arthropoda" rank="species" species="tenasserimensis">
