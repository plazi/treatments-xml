<taxonomicName authorityName="Silvestri" authorityYear="1907" box="[1259,1432,675,699]" class="Entognatha" family="Acerentomidae" kingdom="Animalia" order="Protura" pageId="1" pageNumber="156" phylum="Arthropoda" rank="family">
