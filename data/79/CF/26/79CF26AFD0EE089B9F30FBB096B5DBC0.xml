<geoCoordinate direction="north" orientation="latitude" precision="9" value="24.520334">24°31.22'N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="9" value="113.123">113°07.38'E</geoCoordinate>
], ca650m, 23.IX.2005, Wang XM leg; 1♂1♀, Yinna Mountain, Meizhou City, [
<geoCoordinate direction="north" orientation="latitude" precision="9" value="24.402666">24°24.16'N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="9" value="116.39">116°23.40'E</geoCoordinate>
