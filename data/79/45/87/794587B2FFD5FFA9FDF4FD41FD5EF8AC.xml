<quantity box="[554,645,368,395]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" pageId="21" pageNumber="22" unit="mm" value="5.6">5.6 mm</quantity>
(n = 9, r: 3.5–9.1, SD = 2.2); thorax length = 
<quantity box="[1203,1295,368,394]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="21" pageNumber="22" unit="mm" value="1.6">1.6 mm</quantity>
(n = 11, r: 1.1–2.5, SD = 0.4), thorax width = 
<quantity box="[639,730,403,430]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="21" pageNumber="22" unit="mm" value="0.6">0.6 mm</quantity>
(n = 11, r: 0.3–0.8, SD = 0.1); peduncle and operculum length = 
<quantity box="[291,379,438,465]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="21" pageNumber="22" unit="mm" value="1.3">1.3 mm</quantity>
(n = 10, r: 0.7–2.4, SD = 0.6); operculum length = 
<quantity box="[964,1053,438,465]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="21" pageNumber="22" unit="mm" value="0.4">0.4 mm</quantity>
(n = 11, r: 0.2–0.6, SD = 0.1); operculum diameter = 
