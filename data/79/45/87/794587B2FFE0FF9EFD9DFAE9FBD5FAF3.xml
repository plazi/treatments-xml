<collectingRegion box="[366,493,1942,1968]" country="Mexico" name="Campeche" pageId="33" pageNumber="34">Campeche</collectingRegion>
, eastern 
<collectingCountry box="[596,689,1942,1968]" name="Mexico" pageId="33" pageNumber="34">Mexico</collectingCountry>
; 
<quantity box="[702,810,1942,1969]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="4.0" metricValueMin="1.0" pageId="33" pageNumber="34" unit="m" value="25.0" valueMax="40.0" valueMin="10.0">10–40 m</quantity>
; on shells of 
<taxonomicName authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[968,1398,1942,1969]" class="Bivalvia" family="Pectinidae" genus="Argopecten" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ostreida" pageId="33" pageNumber="34" phylum="Mollusca" rank="species" species="gibbus">
<emphasis box="[968,1189,1942,1969]" italics="true" pageId="33" pageNumber="34">Argopecten gibbus</emphasis>
(Linnaeus, 1758)
</taxonomicName>
and a submerged derrick).
</treatmentCitationGroup>
</paragraph>
</subSubSection>
