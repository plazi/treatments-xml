<date box="[446,600,160,184]" pageId="12" pageNumber="797" value="2016-05-22">May 22, 2016</date>
camera trap 12 (
<geoCoordinate box="[176,303,192,218]" degrees="25" direction="south" minutes="02.55" orientation="latitude" pageId="12" pageNumber="797" precision="9" value="-25.0425">25°02.55ʹ S</geoCoordinate>
