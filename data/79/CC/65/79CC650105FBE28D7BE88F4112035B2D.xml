<taxonomicName authority="Chamberlin, 1952" authorityName="Chamberlin" authorityYear="1952" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="7" pageNumber="172" phylum="Arthropoda" rank="species" species="segregans">
E. segregans 
<authority pageId="8" pageNumber="173">Chamberlin, 1952</authority>
</taxonomicName>
, 
<taxonomicName authority="Chamberlin, 1952" authorityName="Chamberlin" authorityYear="1952" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="7" pageNumber="172" phylum="Arthropoda" rank="species" species="sternethus">
E. sternethus 
<authority pageId="8" pageNumber="173">Chamberlin, 1952</authority>
</taxonomicName>
, 
<taxonomicName authority="Chamberlin, 1952" authorityName="Chamberlin" authorityYear="1952" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="7" pageNumber="172" phylum="Arthropoda" rank="species" species="syngenes">
E. syngenes 
<authority pageId="8" pageNumber="173">Chamberlin, 1952</authority>
