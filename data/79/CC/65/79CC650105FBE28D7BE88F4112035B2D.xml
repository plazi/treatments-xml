<taxonomicName authority="Chamberlin, 1952" authorityName="Chamberlin" authorityYear="1952" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="7" pageNumber="172" phylum="Arthropoda" rank="species" species="telus">
