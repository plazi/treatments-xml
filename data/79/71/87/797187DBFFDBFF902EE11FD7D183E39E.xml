(as 
<taxonomicName authorityName="Casey" authorityYear="1910" box="[379,532,1117,1143]" class="Insecta" family="Staphylinidae" genus="Clusiota" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="101" phylum="Arthropoda" rank="species" species="claviventris">
