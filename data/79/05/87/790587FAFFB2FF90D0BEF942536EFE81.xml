(
<emphasis box="[472,587,1414,1438]" italics="true" pageId="2" pageNumber="69">Esenbeckia</emphasis>
) 
<emphasis box="[604,731,1414,1438]" italics="true" pageId="2" pageNumber="69">auribrunnea</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[740,797,1415,1438]" pageId="2" pageNumber="69" rank="species">n. sp.</taxonomicNameLabel>
