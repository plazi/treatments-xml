<taxonomicName authorityName="Beeson" authorityYear="1937" class="Insecta" family="Curculionidae" genus="Crossotarsus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crossotarsus emorsus" order="Coleoptera" pageId="0" pageNumber="69" phylum="Arthropoda" rank="species" species="emorsus">
<emphasis italics="true" pageId="0" pageNumber="69">Crossotarsus emorsus</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Chapuis" authorityYear="1865" class="Insecta" family="Curculionidae" genus="Crossotarsus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crossotarsus terminatus" order="Coleoptera" pageId="0" pageNumber="69" phylum="Arthropoda" rank="species" species="terminatus">
