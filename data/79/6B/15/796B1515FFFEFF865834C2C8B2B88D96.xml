<taxonomicName authorityName="Ariyama and Azuma" authorityYear="2011" class="Malacostraca" family="Paracalliopiidae" genus="Mucrocalliope" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="88" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="9" pageNumber="88">Mucrocalliope</emphasis>
</taxonomicName>
