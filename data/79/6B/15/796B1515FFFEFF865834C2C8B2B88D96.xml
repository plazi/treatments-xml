<subSubSection pageId="9" pageNumber="88" type="discussion">
<paragraph blockId="9.[156,771,1109,1459]" pageId="9" pageNumber="88">
<emphasis bold="true" box="[181,281,1370,1394]" pageId="9" pageNumber="88">Remarks.</emphasis>
<taxonomicName authorityName="Stebbing" authorityYear="1899" box="[289,410,1369,1393]" class="Malacostraca" family="Paracalliopiidae" genus="Paracalliope" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="88" phylum="Arthropoda" rank="genus">
<emphasis box="[289,410,1369,1393]" italics="true" pageId="9" pageNumber="88">Paracalliope</emphasis>
</taxonomicName>
