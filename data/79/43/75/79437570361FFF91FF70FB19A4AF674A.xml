<taxonomicName authority="Krantz. However, Rueda-Ramirez et al. (2019: 2390)" authorityName="Krantz. However, Rueda-Ramirez" authorityPageNumber="2390" authorityYear="2019" class="Arachnida" family="Ascidae" genus="Africoseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="147" phylum="Arthropoda" rank="species" species="areolatus">
