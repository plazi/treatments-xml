. 
<taxonomicName authority="Whitelegge, 1897" authorityName="Whitelegge" authorityYear="1897" box="[295,764,1731,1754]" class="Malacostraca" family="Alpheidae" genus="Metabetaeus" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="49" phylum="Arthropoda" rank="species" species="minutus">
