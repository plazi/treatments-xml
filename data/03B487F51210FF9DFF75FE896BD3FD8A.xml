<taxonomicName id="4C1D4D601210FF9DFC6DFE7C6A26FE6D" authorityName="Robins &amp; Robins" authorityYear="1976" box="[911,1123,465,489]" class="Actinopterygii" family="Synaphobranchidae" genus="Dysomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="10" pageNumber="15" phylum="Chordata" rank="species" species="goslinei">
