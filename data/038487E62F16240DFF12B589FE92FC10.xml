<taxonomicName id="4C2D4D732F16240CFEA3B297FCBBF879" authority="Pall-Gergely &amp; Hunyadi" authorityName="Pall-Gergely &amp; Hunyadi" authorityYear="2021" box="[294,800,1929,1953]" class="Gastropoda" family="Cyclophoridae" genus="Metalycaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda" pageId="54" pageNumber="55" phylum="Mollusca" rank="species" species="pygmachos" status="n. sp.">
<emphasis id="B959EAE22F16240CFEA3B297FD85F879" box="[294,542,1929,1953]" italics="true" pageId="54" pageNumber="55">Metalycaeus pygmachos</emphasis>
