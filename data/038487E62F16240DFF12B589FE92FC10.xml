<taxonomicName id="4C2D4D732F192403FD8BB2CEFC38F831" authority="(Godwin-Austen, 1914)" baseAuthorityName="Godwin-Austen" baseAuthorityYear="1914" box="[526,931,2000,2025]" class="Gastropoda" family="Cyclophoridae" genus="Dicharax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda" pageId="57" pageNumber="58" phylum="Mollusca" rank="species" species="notus">
