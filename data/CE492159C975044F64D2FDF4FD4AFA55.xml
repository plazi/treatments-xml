<taxonomicName id="81E0EBCCC975044F64D2FDF4FEF5FD98" box="[151,383,597,623]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="cyanescens">
<emphasis id="74944C5DC975044F64D2FDF4FEF5FD98" bold="true" box="[151,383,597,623]" italics="true" pageId="1" pageNumber="44">Agrilus cyanescens</emphasis>
