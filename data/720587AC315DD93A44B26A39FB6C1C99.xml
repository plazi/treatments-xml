<caption id="AED366323158D93A44B269FBFCE91E90" ID-DOI="http://doi.org/10.5281/zenodo.3677911" ID-Zenodo-Dep="3677911" box="[151,885,1173,1198]" httpUri="https://zenodo.org/record/3677911/files/figure.png" pageId="37" pageNumber="438" startId="37.[151,250,1173,1198]" targetBox="[159,1429,193,1137]" targetPageId="37">
<paragraph id="FA1336BA3158D93A44B269FBFCE91E90" blockId="37.[151,885,1173,1198]" box="[151,885,1173,1198]" pageId="37" pageNumber="438">
