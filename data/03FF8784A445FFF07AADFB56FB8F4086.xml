<taxonomicName id="4C564D11A445FFF07A48F9D1FF414668" authorityName="Milne-Edwards" authorityYear="1871" box="[124,239,1567,1590]" class="Mammalia" family="Talpidae" genus="Uropsilus" kingdom="Animalia" order="Soricomorpha" pageId="36" pageNumber="37" phylum="Chordata" rank="genus">
