<taxonomicName id="4C564D11A445FFF07BD5F9D1FDE44668" authorityName="E. Geoffroy Saint-Hilaire" authorityYear="1803" box="[481,586,1567,1590]" class="Mammalia" family="Talpidae" genus="Scalopus" kingdom="Animalia" order="Soricomorpha" pageId="36" pageNumber="37" phylum="Chordata" rank="genus">
