<emphasis id="B922EA80A445FFF07B05F9D1FE164668" box="[305,440,1567,1590]" italics="true" pageId="36" pageNumber="37">
<taxonomicName id="4C564D11A445FFF07B05F9D1FE1A4668" authorityName="Temminck" authorityYear="1841" box="[305,436,1567,1590]" class="Mammalia" family="Talpidae" genus="Urotrichus" kingdom="Animalia" order="Soricomorpha" pageId="36" pageNumber="37" phylum="Chordata" rank="genus">Urotrichus</taxonomicName>
