<subSubSection id="C346429BFFD0B53C337CDF54028EFBBB" pageId="3" pageNumber="4" type="materials_examined">
<paragraph id="8BE31110FFD0B53C337CDF5403E8FE13" blockId="3.[264,1323,283,1190]" pageId="3" pageNumber="4">
<emphasis id="B928CD02FFD0B53C337CDF5400C2FEA0" bold="true" box="[312,498,283,309]" pageId="3" pageNumber="4">
<typeStatus id="54E7AFB2FFD0B53C337CDF540045FEA0" box="[312,373,283,309]" pageId="3" pageNumber="4">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54E7AFB2FFD0B53C304EDF530347FEA3" box="[522,631,284,310]" pageId="3" pageNumber="4">Holotype</typeStatus>
ɗ (on slide) (
<collectionCode id="ED4D89D5FFD0B53C316EDF53024DFEA3" box="[810,893,284,310]" httpUri="http://grbio.org/cool/5xp9-edpx" name="Universidade Federal do Parana, Colecao de Entomologia Pe. Jesus Santiago Moure" pageId="3" pageNumber="4">DZUP</collectionCode>
