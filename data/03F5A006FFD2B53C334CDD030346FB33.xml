<taxonomicName id="4C5C6A93FFD1B53D3384D8A603A9F895" box="[448,665,1769,1792]" class="Insecta" family="Hippoboscidae" genus="Metelasmus" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="wenzeli" status="new species">
<emphasis id="B928CD02FFD1B53D3384D8A603A9F895" box="[448,665,1769,1792]" italics="true" pageId="2" pageNumber="3">Metelasmus wenzeli</emphasis>
</taxonomicName>
