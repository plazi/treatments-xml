<taxonomicName id="4C784D597A44FFC2FE2BFCDDFE19FC96" authorityName="Waldock" authorityYear="2008" box="[384,475,798,828]" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="linnaei">
