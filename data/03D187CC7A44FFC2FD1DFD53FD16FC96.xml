<taxonomicName id="4C784D597A44FFC2FDB6FCDDFDAAFC96" authorityName="Otto &amp; Hill" authorityYear="2016" box="[541,616,798,828]" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="vespa">
