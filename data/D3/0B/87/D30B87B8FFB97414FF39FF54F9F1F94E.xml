<number box="[1138,1147,868,892]" pageId="1" pageNumber="440" value="1.0">1</number>
–ST 
<number box="[1199,1212,868,892]" pageId="1" pageNumber="440" value="3.0">3</number>
) 
<number box="[1229,1257,868,893]" pageId="1" pageNumber="440" value="62.0">62</number>
(
<number box="[1275,1350,868,893]" pageId="1" pageNumber="440" value="62.5" valueMax="65.0" valueMin="60.0">60–65</number>
), width (distance between setae ST 
<number box="[459,473,904,928]" pageId="1" pageNumber="440" value="2.0">2</number>
) 
<number box="[489,517,903,928]" pageId="1" pageNumber="440" value="65.0">65</number>
