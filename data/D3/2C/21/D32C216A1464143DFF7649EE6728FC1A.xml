<subSubSection pageId="6" pageNumber="597" type="etymology">
<paragraph blockId="6.[151,1437,252,960]" pageId="6" pageNumber="597">
<emphasis bold="true" box="[199,334,900,925]" pageId="6" pageNumber="597">Etymology.</emphasis>
