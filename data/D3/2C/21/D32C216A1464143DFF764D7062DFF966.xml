, data as for 
<typeStatus box="[552,650,1598,1623]" pageId="6" pageNumber="597">holotype</typeStatus>
(
<collectionCode LSID="urn:lsid:biocol.org:col:32981" box="[665,738,1599,1623]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="6" pageNumber="597">ANIC</collectionCode>
