<typeStatus box="[441,545,1805,1829]" pageId="11" pageNumber="12">Holotype</typeStatus>
female, 
<emphasis bold="true" box="[642,848,1805,1830]" pageId="11" pageNumber="12">
<collectingCountry box="[642,717,1805,1830]" name="Brazil" pageId="11" pageNumber="12">Brazil</collectingCountry>
, São Paulo
</emphasis>
, Campinas, Sincontron, (
<geoCoordinate box="[1134,1267,1805,1829]" direction="south" orientation="latitude" pageId="11" pageNumber="12" precision="15" value="-22.802776">
