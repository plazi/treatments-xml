<taxonomicName authorityName="W. H. Chen &amp; Y. M. Shui" authorityYear="2019" class="Insecta" family="Curculionidae" genus="Oreocharis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oreocharis wenshanensis" order="Coleoptera" pageId="0" pageNumber="83" phylum="Arthropoda" rank="species" species="wenshanensis">
<emphasis italics="true" pageId="0" pageNumber="83">Oreocharis wenshanensis</emphasis>
</taxonomicName>
and 
<taxonomicName genus="O." lsidName="O. concava" pageId="0" pageNumber="83" rank="species" species="concava">
