<taxonomicName box="[980,1212,1019,1043]" class="Magnoliopsida" family="Myrtaceae" genus="Melaleuca" kingdom="Plantae" order="Myrtales" pageId="66" pageNumber="66" phylum="Tracheophyta" rank="species" species="viridiflora">
<emphasis box="[980,1212,1019,1043]" italics="true" pageId="66" pageNumber="66">Melaleuca viridiflora</emphasis>
</taxonomicName>
(
<taxonomicName box="[691,810,1052,1076]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="66" pageNumber="66" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
: 
<taxonomicName authorityName="Burnett" authorityYear="1835" box="[820,956,1052,1076]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="66" pageNumber="66" phylum="Tracheophyta" rank="tribe" tribe="Melaleuceae">Melaleuceae</taxonomicName>
