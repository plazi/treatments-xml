<subSubSection type="reference_group">
<paragraph pageNumber="227">
(= 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179620" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" lsidName="Crematogaster foraminiceps nigrans Forel" lsidName-HNS="Crematogaster foraminiceps nigrans Forel" order="Hymenoptera" pageNumber="227" phylum="Arthropoda" rank="subSpecies" species="foraminiceps" subSpecies="nigrans">Crematogaster foraminiceps Sants. st. nigrans Forel</taxonomicName>
