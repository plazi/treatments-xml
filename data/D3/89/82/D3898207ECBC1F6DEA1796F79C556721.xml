<subSubSection pageId="1" pageNumber="66" type="reference_group">
<paragraph pageId="1" pageNumber="66">
<taxonomicName class="Insecta" family="Chrysididae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis antennata" order="Hymenoptera" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="antennata">Chrysis antennata</taxonomicName>
