<emphasis bold="true" box="[478,491,1974,1995]" pageId="34" pageNumber="235">F</emphasis>
, metasoma, dorsal aspect; 
<emphasis bold="true" box="[783,803,1973,1996]" pageId="34" pageNumber="235">G,</emphasis>
mesosoma, lateral aspect; 
<emphasis bold="true" box="[1096,1122,1974,1995]" pageId="34" pageNumber="235">H,</emphasis>
propodeum, dorsal aspect; 
<emphasis bold="true" box="[1420,1430,1974,1995]" pageId="34" pageNumber="235">I</emphasis>
