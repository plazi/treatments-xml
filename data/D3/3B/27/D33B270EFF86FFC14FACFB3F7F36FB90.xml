<taxonomicName box="[430,597,1937,1960]" pageId="30" pageNumber="231" rank="species" species="prodigiosus" status="sp. nov." subGenus="H.">
(
<emphasis box="[439,465,1939,1960]" italics="true" pageId="30" pageNumber="231">H.</emphasis>
) 
<taxonomicName box="[479,597,1937,1959]" pageId="30" pageNumber="231" rank="species" species="prodigiosus" status="sp. nov.">
<emphasis box="[479,597,1937,1959]" italics="true" pageId="30" pageNumber="231">prodigiosus</emphasis>
