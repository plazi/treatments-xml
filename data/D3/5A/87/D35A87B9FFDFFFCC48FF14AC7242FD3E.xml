<emphasis box="[853,1074,1900,1924]" italics="true" pageId="13" pageNumber="56">Nidirana occidentalis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1082,1163,1900,1924]" pageId="13" pageNumber="56">
<taxonomicNameLabel box="[1082,1163,1900,1924]" pageId="13" pageNumber="56" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(A, SYS a007830) and 
<taxonomicName baseAuthorityName="Boulenger" baseAuthorityYear="1904" class="Amphibia" family="Ranidae" genus="Nidirana" higherTaxonomySource="CoL" kingdom="Animalia" order="Anura" pageId="13" pageNumber="56" phylum="Chordata" rank="species" species="pleuraden">
