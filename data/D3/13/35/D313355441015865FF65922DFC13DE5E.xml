<collectingCountry box="[541,827,1292,1318]" name="Central African Republic" pageId="34" pageNumber="335">Central African Republic</collectingCountry>
; 
<collectingCountry box="[838,1054,1292,1318]" name="Republic of the Congo" pageId="34" pageNumber="335">Republic of Congo</collectingCountry>
; 
<collectingCountry box="[1066,1200,1292,1319]" name="Ivory Coast" pageId="34" pageNumber="335">Ivory Coast</collectingCountry>
; 
<collectingCountry box="[1212,1303,1292,1318]" name="Uganda" pageId="34" pageNumber="335">Uganda</collectingCountry>
.
</paragraph>
</subSubSection>
<subSubSection box="[199,915,1328,1355]" pageId="34" pageNumber="335" type="discussion">
<paragraph blockId="34.[151,1437,500,1355]" box="[199,915,1328,1355]" pageId="34" pageNumber="335">
<emphasis bold="true" box="[199,308,1328,1354]" pageId="34" pageNumber="335">Remarks</emphasis>
