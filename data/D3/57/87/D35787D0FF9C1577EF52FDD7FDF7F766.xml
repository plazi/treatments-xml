<subSubSection pageId="6" pageNumber="373" type="discussion">
<paragraph blockId="6.[92,1409,595,2220]" pageId="6" pageNumber="373">
<emphasis bold="true" box="[268,292,596,635]" pageId="6" pageNumber="373">3</emphasis>
.— 
<taxonomicName box="[341,845,596,635]" class="Reptilia" family="Ornithomimidae" group="Ornithomimid Group" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="6" pageNumber="373" phylum="Chordata" rank="group">
<emphasis bold="true" box="[341,420,596,635]" pageId="6" pageNumber="373">The</emphasis>
<emphasis box="[436,704,596,635]" italics="true" pageId="6" pageNumber="373">Ornithomiimid</emphasis>
<emphasis box="[719,845,596,635]" italics="true" pageId="6" pageNumber="373">Group</emphasis>
</taxonomicName>
of the Cretaceous 
<emphasis box="[310,802,655,694]" italics="true" pageId="6" pageNumber="373">
<taxonomicName authorityName="Leidy" authorityYear="1865" box="[310,513,655,694]" class="Reptilia" family="Ornithomimidae" genus="Coelosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="6" pageNumber="373" phylum="Chordata" rank="genus">Coelosaurus</taxonomicName>
, 
<taxonomicName authorityName="Marsh" authorityYear="1890" box="[542,796,655,694]" class="Reptilia" family="Ornithomimidae" genus="Ornithomimus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="6" pageNumber="373" phylum="Chordata" rank="genus">Ornithomimus</taxonomicName>
,
</emphasis>
and 
<taxonomicName authority="Osborn, 1916" authorityName="Osborn" authorityYear="1917" box="[914,1175,655,694]" class="Reptilia" family="Not" genus="Struthiomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="6" pageNumber="373" phylum="Chordata" rank="genus">
<emphasis box="[914,1175,655,694]" italics="true" pageId="6" pageNumber="373">Struthiomimus</emphasis>
</taxonomicName>
</paragraph>
