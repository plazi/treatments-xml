species:
</emphasis>
<taxonomicName box="[436,712,1708,1733]" class="Insecta" family="Tingidae" genus="Oblongomorpha" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="lutetia" status="n. sp.">
