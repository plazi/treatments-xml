<typeStatus box="[264,325,988,1014]" pageId="7" pageNumber="8">Type</typeStatus>
species:
</emphasis>
<taxonomicName box="[436,711,988,1013]" class="Insecta" family="Tingidae" genus="Chorotingioites" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="priscus" status="n. sp.">
