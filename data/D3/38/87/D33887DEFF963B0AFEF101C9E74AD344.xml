<subSubSection box="[312,790,684,710]" pageId="14" pageNumber="15" type="diagnosis">
<paragraph blockId="14.[264,1324,283,1870]" box="[312,790,684,710]" pageId="14" pageNumber="15">
<emphasis bold="true" box="[312,442,684,710]" pageId="14" pageNumber="15">Diagnosis:</emphasis>
<collectingCountry box="[449,481,684,710]" name="American Samoa" pageId="14" pageNumber="15">As</collectingCountry>
