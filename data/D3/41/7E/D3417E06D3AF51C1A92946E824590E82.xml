<geoCoordinate degrees="16" direction="north" minutes="59.6630" orientation="latitude" precision="1" value="16.994383">16°59.6630'N</geoCoordinate>
, 
<geoCoordinate degrees="121" direction="east" minutes="1.1120" orientation="longitude" precision="1" value="121.01853">121°1.1120'E</geoCoordinate>
