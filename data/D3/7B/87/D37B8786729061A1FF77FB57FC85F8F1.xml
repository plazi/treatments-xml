<taxonomicName authorityName="Al" authorityYear="2014" box="[1258,1389,1790,1815]" class="Insecta" family="Eupelmidae" genus="Eupelmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="species" species="gemellus">
