<taxonomicName authority="Maur" authorityName="Maur" box="[656,1005,1718,1743]" class="Insecta" family="Cynipidae" genus="Timaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="species" species="phoenixopodos">
