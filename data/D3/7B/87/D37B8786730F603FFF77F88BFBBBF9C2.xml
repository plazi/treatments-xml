<taxonomicName authorityName="Silvestri" authorityYear="1914" box="[635,715,404,428]" class="Insecta" family="Eupelmidae" genus="Eupelmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="313" pageNumber="314" phylum="Arthropoda" rank="species" species="afer">
