<taxonomicName box="[982,1055,1375,1400]" class="Insecta" family="Tingidae" genus="Ulmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="genus">
