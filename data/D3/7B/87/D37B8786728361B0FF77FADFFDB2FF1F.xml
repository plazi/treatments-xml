<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[1027,1210,1340,1365]" class="Insecta" family="Buprestidae" genus="Anthaxia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="species" species="manca">
