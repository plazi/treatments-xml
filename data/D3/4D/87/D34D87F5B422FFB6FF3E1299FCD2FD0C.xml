</subSubSection>
<subSubSection box="[199,918,620,644]" pageId="9" pageNumber="46" type="etymology">
<paragraph blockId="9.[151,1436,151,644]" box="[199,918,620,644]" pageId="9" pageNumber="46">
<emphasis bold="true" box="[199,334,620,644]" pageId="9" pageNumber="46">Etymology.</emphasis>
