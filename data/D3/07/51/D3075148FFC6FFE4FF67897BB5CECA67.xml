, 
<taxonomicName authority="(Hooper &amp; Levi, 1993)" baseAuthorityName="Hooper &amp; Levi" baseAuthorityYear="1993" class="Demospongiae" family="Axinellidae" genus="Dragmacidon" kingdom="Animalia" lastPageId="31" lastPageNumber="32" order="Axinellida" pageId="30" pageNumber="31" phylum="Porifera" rank="species" species="debitusae">
