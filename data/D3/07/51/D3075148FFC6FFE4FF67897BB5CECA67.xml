<taxonomicName authorityName="Schmidt" authorityYear="1875" class="Demospongiae" family="Axinellidae" genus="Pseudaxinella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Axinellida" pageId="31" pageNumber="32" phylum="Porifera" rank="genus">
