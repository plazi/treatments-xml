<taxonomicName box="[1112,1192,1225,1249]" class="Insecta" family="Thripidae" genus="Danothrips" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="139" phylum="Arthropoda" rank="species" species="moundi">
<emphasis box="[1112,1192,1225,1249]" italics="true" pageId="2" pageNumber="139">moundi</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1243,1311,1225,1249]" class="Insecta" family="Thripidae" genus="Danothrips" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="139" phylum="Arthropoda" rank="species" species="salicis">
<emphasis box="[1243,1311,1225,1249]" italics="true" pageId="2" pageNumber="139">salicis</emphasis>
</taxonomicName>
