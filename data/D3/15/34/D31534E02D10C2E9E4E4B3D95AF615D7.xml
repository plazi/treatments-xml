<geoCoordinate direction="north" orientation="latitude" precision="1" value="35.527">35째31'37.2&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="27.2245">27째13'28.2&quot;E</geoCoordinate>
