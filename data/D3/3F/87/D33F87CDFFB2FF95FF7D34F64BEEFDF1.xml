<number box="[218,275,301,327]" pageId="3" pageNumber="42" value="0.26">0.26</number>
, PME-PME 
<number box="[431,488,301,327]" pageId="3" pageNumber="42" value="0.14">0.14</number>
, PME-PLE 
<number box="[635,691,301,327]" pageId="3" pageNumber="42" value="0.35">0.35</number>
