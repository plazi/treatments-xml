<paragraph pageId="22" pageNumber="81">
<taxonomicName authorityName="Blume" authorityYear="1850" class="Magnoliopsida" family="Myrtaceae" genus="Jossinia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Jossinia richardii" order="Myrtales" pageId="22" pageNumber="81" phylum="Tracheophyta" rank="species" species="richardii">
