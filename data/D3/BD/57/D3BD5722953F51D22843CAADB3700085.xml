<taxonomicName class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia punctatissima" order="Coleoptera" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="punctatissima">Cephaloleia punctatissima</taxonomicName>
