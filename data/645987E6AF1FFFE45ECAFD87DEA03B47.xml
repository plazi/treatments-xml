<emphasis id="DE84EAE2AF1FFFE45A33FC2BD9B53D94" box="[1134,1191,922,945]" italics="true" pageId="62" pageNumber="63">Stipa</emphasis>
</taxonomicName>
sp., 
<taxonomicName id="2BF04D73AF1FFFE45A80FC2BD8563D94" box="[1245,1348,922,945]" class="Liliopsida" family="Poaceae" genus="Imperata" kingdom="Plantae" order="Poales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="genus">
