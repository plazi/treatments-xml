<paragraph id="B10FAB69077DFF892DA9E8437E15F94E" blockId="116.[151,579,1627,1686]" box="[151,579,1662,1686]" pageId="116" pageNumber="117">South American Small-handed Skinks</paragraph>
</subSubSection>
<subSubSection id="F9AAF8E2077DFF892DA9E8FA7EBFF906" box="[151,745,1734,1759]" pageId="116" pageNumber="117" type="type_taxon">
<paragraph id="B10FAB69077DFF892DA9E8FA7EBFF906" blockId="116.[151,1436,1734,1938]" box="[151,745,1734,1759]" pageId="116" pageNumber="117">
<emphasis id="83C4777B077DFF892DA9E8FA7D64F907" bold="true" box="[151,306,1734,1759]" inLineHeading="true" pageId="116" pageNumber="117" reason="1">
<typeStatus id="6E0B15CB077DFF892DA9E8FA7C87F907" box="[151,209,1735,1759]" pageId="116" pageNumber="117">Type</typeStatus>
