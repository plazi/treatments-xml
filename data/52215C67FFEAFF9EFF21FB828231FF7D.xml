<emphasis id="E8FC3163FFEAFF99FF21F94481CDF913" bold="true" box="[151,172,1698,1723]" pageId="27" pageNumber="28">1.</emphasis>
<taxonomicName id="1D8896F2FFEAFF99FF05F94483BEF913" authority="(Daday, 1901) Inglis, 1969" authorityName="Inglis" authorityYear="1969" baseAuthorityName="Daday" baseAuthorityYear="1901" box="[179,735,1698,1723]" class="Chromadorea" family="Chromadoridae" genus="Steineridora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="27" pageNumber="28" phylum="Nematoda" rank="species" species="adriatica">
<emphasis id="E8FC3163FFEAFF99FF05F94480CDF913" box="[179,428,1698,1723]" italics="true" pageId="27" pageNumber="28">Steineridora adriatica</emphasis>
