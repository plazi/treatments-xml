<paragraph id="9D3B9CB0FD07FD5F2EE2B2A8FD089AEB" pageId="9" pageNumber="9">
<taxonomicName id="B3127C06BC118FFDAEA03B37A13FD67D" class="Arachnida" family="Oppiidae" genus="Oppia" kingdom="Animalia" order="Sarcoptiformes" pageId="9" pageNumber="9" phylum="Arthropoda" rank="species" species="tridentata">Oppia tridentata</taxonomicName>
