<emphasis id="688B0E66FFACFFF9FF75FB0BD955FB4C" bold="true" box="[151,515,1175,1201]" pageId="10" pageNumber="11">
Tribe 
<taxonomicName id="9DFFA9F7FFACFFF9FF00FB0BDA32FB4C" box="[226,356,1175,1201]" class="Insecta" family="Geometridae" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="tribe" tribe="Chesiadini">
