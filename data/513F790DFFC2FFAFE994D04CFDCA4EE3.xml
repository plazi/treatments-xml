<taxonomicName id="1E96B398FFC2FFAFED3DD0E3FF4E4E7F" authority="(Polejaeff 1883)" authorityName="Polejaeff" authorityYear="1883" baseAuthorityName="Polejaeff" baseAuthorityYear="1883" class="Calcarea" family="Achramorphidae" genus="Megapogon" kingdom="Animalia" order="Leucosolenida" pageId="23" pageNumber="224" phylum="Porifera" rank="species" species="crucifer">
