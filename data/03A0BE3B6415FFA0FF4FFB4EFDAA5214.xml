<taxonomicName id="4C0974AE6415FFA0FF36FB4EFD935398" authority="Lacepede, 1799" authorityName="Lacepede" authorityYear="1799" box="[238,594,1240,1266]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="31" pageNumber="32" phylum="Chordata" rank="genus">
