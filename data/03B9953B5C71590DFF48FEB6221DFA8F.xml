plants: 
<taxonomicName id="4C105FAE5C71590DFE3EFBA723C5FB3F" baseAuthorityName="Zinovjev" baseAuthorityYear="1999" box="[481,648,1150,1175]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="86" pageNumber="87" phylum="Chordata" rank="species" species="herbacea">
