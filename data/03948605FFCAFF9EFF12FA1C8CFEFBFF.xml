<quantity id="4CC59AF6FFCBFF9EFEE6FD908FA4FDE0" box="[355,451,538,564]" metricMagnitude="-2" metricUnit="m" metricValue="1.44" pageId="49" pageNumber="50" unit="mm" value="14.4">14.4mm</quantity>
