on 
<taxonomicName id="4C7A4D1FB768FFDD67467F17DF59FDDB" authority="Thomson, 1858" authorityName="Thomson" authorityYear="1858" box="[530,809,549,576]" class="Insecta" family="Staphylinidae" genus="Acrulia" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
<emphasis id="B90EEA8EB768FFDD67467F17DE1AFDA4" box="[530,618,549,575]" italics="true" pageId="57" pageNumber="58">Acrulia</emphasis>
Thomson, 1858
</taxonomicName>
and 
<taxonomicName id="4C7A4D1FB768FFDD66357F14D8F6FDA4" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" box="[865,1158,549,575]" class="Insecta" family="Staphylinidae" genus="Proteinus" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
