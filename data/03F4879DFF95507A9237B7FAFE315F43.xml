<taxonomicName id="4C5D4D08FF955078929AB79AFD105ED9" authority="(Meigen)" baseAuthorityName="Meigen" box="[462,705,1216,1241]" class="Insecta" family="Clusiidae" genus="Clusia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="2168" phylum="Arthropoda" rank="species" species="flava">
