<taxonomicName id="4C5D4D08FF9550789237B7FAFCA45EB9" authority="(Loew)" baseAuthorityName="Loew" baseAuthorityYear="1847" box="[355,885,1184,1209]" class="Insecta" family="Lauxaniidae" genus="Meiosimyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="2168" phylum="Arthropoda" rank="species" species="platycephala">
<emphasis id="B929EA99FF9550789237B7FAFDAC5EB8" box="[355,637,1185,1209]" italics="true" pageId="29" pageNumber="2168">Meiosimyza platycephala</emphasis>
