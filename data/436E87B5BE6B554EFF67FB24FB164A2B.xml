<taxonomicName id="0CC74D20BE6B554DFF67F9AEFEC04D7E" box="[151,324,1583,1608]" class="Trematoda" family="Opecoelidae" genus="Hamacreadium" kingdom="Animalia" order="Plagiorchiida" pageId="13" pageNumber="164" phylum="Platyhelminthes" rank="genus">
