<taxonomicName id="4C174D43FF30FF149B9FFD99FB4B32AF" authority="(Steindachner, 1876)" baseAuthorityName="Steindachner" baseAuthorityYear="1876" class="Actinopterygii" family="Haemulidae" genus="Haemulon" kingdom="Animalia" order="Perciformes" pageId="161" pageNumber="162" phylum="Chordata" rank="species" species="californiensis">
