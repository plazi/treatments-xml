<taxonomicName id="4C174D43FF30FF1499C6FD99F9EF3283" authority="(Steindachner, 1876)" baseAuthorityName="Steindachner" baseAuthorityYear="1876" box="[443,942,619,646]" class="Actinopterygii" family="Haemulidae" genus="Xenistius" kingdom="Animalia" order="Perciformes" pageId="161" pageNumber="162" phylum="Chordata" rank="species" species="californiensis">
