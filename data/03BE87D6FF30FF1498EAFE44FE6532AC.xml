(
<quantity id="4CEF9B25FF30FF149BE5FDD1F9BC323B" box="[920,1021,547,574]" metricMagnitude="1" metricUnit="m" metricValue="2.0574" metricValueMax="3.99288" metricValueMin="0.12192" pageId="161" pageNumber="162" unit="ft" value="67.5" valueMax="131.0" valueMin="4.0">4â131 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FF30FF149C21FDD1FF0F3238" author="Miller and Lea" box="[1116,1358,547,573]" firstAuthor="Miller" pageId="161" pageNumber="162" refId="ref302113" refString="Miller, D. J. &amp; Lea, R. N. (1972) Guide to the coastal marine fishes of California. California Department of Fish and Game Fish Bulletin, 157." type="book" year="1972">Miller and Lea 1972</bibRefCitation>
