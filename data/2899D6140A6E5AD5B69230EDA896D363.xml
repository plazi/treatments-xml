<geoCoordinate id="92BA1CF1491C15B1356A438A092A9A3C" degrees="00" direction="south" minutes="57.030" orientation="latitude" precision="1" value="-0.95049995">00°57.030'S</geoCoordinate>
, 
<geoCoordinate id="2F7E230AB034247BE88EB66D0D711CA5" degrees="79" direction="west" minutes="08.717" orientation="longitude" precision="1" value="-79.14529">79°08.717'W</geoCoordinate>
