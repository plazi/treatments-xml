<taxonomicName id="D1CC9C592AD4CAD1D1BD8CEA706AA80E" class="Magnoliopsida" family="Fabaceae" genus="Senna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Senna" order="Fabales" pageId="0" pageNumber="609" phylum="Tracheophyta" rank="genus">
