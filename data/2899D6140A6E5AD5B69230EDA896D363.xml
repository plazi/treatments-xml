<geoCoordinate id="DA9D84A1157A3E6F5DFC2174F51926C9" degrees="00" direction="south" minutes="38.544" orientation="latitude" precision="1" value="-0.64239997">00°38.544'S</geoCoordinate>
, 
<geoCoordinate id="2AA5A59186195F86E8BACA9117795C12" degrees="79" direction="west" minutes="08.902" orientation="longitude" precision="1" value="-79.14837">79°08.902'W</geoCoordinate>
