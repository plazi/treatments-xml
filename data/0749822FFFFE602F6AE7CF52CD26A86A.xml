Remarks. 
<taxonomicName id="48E048BAFFFD602F6B4EC92DCA6CA819" authorityName="Cartwright" authorityYear="2008" baseAuthorityName="Cartwright" baseAuthorityYear="2008" box="[318,584,512,537]" class="Insecta" family="Ecnomidae" genus="Daternomina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="scindens">
