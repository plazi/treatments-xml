<quantity id="0736F4FB3412471EFD84444A77ABF9BE" box="[555,650,1562,1588]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="9" pageNumber="63" unit="mm" value="4.8">4.8 mm</quantity>
