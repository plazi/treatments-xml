<quantity id="0736F4FB3412471EFCFD444A7690F9BE" box="[850,945,1562,1588]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" pageId="9" pageNumber="63" unit="mm" value="4.9">4.9 mm</quantity>
