<taxonomicName box="[904,1010,2012,2036]" class="Insecta" family="Hydrophilidae" genus="Armostus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="52" phylum="Arthropoda" rank="genus">
