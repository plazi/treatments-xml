<paragraph blockId="3.[151,1437,151,501]" pageId="3" pageNumber="70">
<taxonomicName box="[151,382,152,176]" class="Insecta" family="Hymenoptera" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="70" phylum="Arthropoda" rank="species" species="hainanensis" status="sp. nov.">
