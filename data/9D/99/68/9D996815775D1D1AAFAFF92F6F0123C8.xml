<taxonomicName authority="Crotch, 1874" authorityName="Crotch" authorityYear="1874" class="Insecta" family="Coccinellidae" genus="Tytthaspis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tytthaspis" order="Coleoptera" pageId="171" pageNumber="172" phylum="Arthropoda" rank="genus">Tytthaspis</taxonomicName>
