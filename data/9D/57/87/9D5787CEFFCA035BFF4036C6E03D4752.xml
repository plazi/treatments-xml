<quantity box="[656,791,1822,1846]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="4.5" metricValueMin="3.5" pageId="6" pageNumber="49" unit="mm" value="4.0" valueMax="4.5" valueMin="3.5">3.5–4.5 mm</quantity>
(
