<taxonomicName authorityName="Quaintance y Baker" authorityYear="1914" box="[686,922,524,548]" class="Insecta" family="Aleyrodidae" genus="Aleuroparadoxus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[686,922,524,548]" italics="true" pageId="5" pageNumber="4">Aleuroparadoxus</emphasis>
