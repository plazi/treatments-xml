<paragraph blockId="26.[113,784,179,1314]" box="[145,608,1071,1091]" pageId="26" pageNumber="63">
<emphasis box="[145,348,1071,1091]" italics="true" pageId="26" pageNumber="63">Geographical records</emphasis>
. 
<collectingCountry box="[360,418,1072,1091]" name="Brazil" pageId="26" pageNumber="63">Brazil</collectingCountry>
, 
<emphasis box="[429,497,1071,1091]" italics="true" pageId="26" pageNumber="63">
