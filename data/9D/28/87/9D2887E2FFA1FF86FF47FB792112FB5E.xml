<paragraph blockId="2.[151,1437,1340,2015]" box="[199,1317,1412,1439]" pageId="2" pageNumber="129">
<materialsCitation ID-GBIF-Occurrence="2332675084" box="[199,1314,1412,1439]" collectionCode="USNM" pageId="2" pageNumber="129" specimenCode="USNM 1569237-1569245" specimenCount="2" typeStatus="paratype">
