<collectingRegion box="[565,675,1829,1853]" country="Colombia" name="Antioquia" pageId="5" pageNumber="1426">Antioquia</collectingRegion>
, 
<collectingRegion box="[688,771,1829,1853]" country="Colombia" name="Boyaca" pageId="5" pageNumber="1426">Boyacá</collectingRegion>
, 
<collectingRegion box="[168,244,1863,1887]" country="Colombia" name="Caldas" pageId="5" pageNumber="1426">Caldas</collectingRegion>
, 
<collectingRegion box="[257,327,1863,1887]" country="Colombia" name="Cauca" pageId="5" pageNumber="1426">Cauca</collectingRegion>
, 
<collectingRegion box="[339,500,1863,1887]" country="Colombia" name="Cundinamarca" pageId="5" pageNumber="1426">Cundinamarca</collectingRegion>
, 
<collectingRegion box="[514,576,1863,1887]" country="Colombia" name="Huila" pageId="5" pageNumber="1426">Huila</collectingRegion>
, 
<collectingRegion box="[589,666,1863,1887]" country="Colombia" name="Narino" pageId="5" pageNumber="1426">Nariño</collectingRegion>
, 
<collectingRegion country="Colombia" name="Norte de Santander" pageId="5" pageNumber="1426">Norte de Santander</collectingRegion>
, 
<collectingRegion box="[292,382,1897,1921]" country="Colombia" name="Quindio" pageId="5" pageNumber="1426">Quindío</collectingRegion>
, 
<collectingRegion box="[398,503,1897,1921]" country="Colombia" name="Risaralda" pageId="5" pageNumber="1426">Risaralda</collectingRegion>
, 
<collectingRegion box="[519,628,1897,1921]" country="Colombia" name="Santander" pageId="5" pageNumber="1426">Santander</collectingRegion>
, 
<collectingRegion box="[643,721,1897,1921]" country="Colombia" name="Tolima" pageId="5" pageNumber="1426">Tolima</collectingRegion>
, and 
<collectingRegion box="[168,338,1931,1955]" country="Colombia" name="Valle del Cauca" pageId="5" pageNumber="1426">Valle del Cauca</collectingRegion>
