<paragraph blockId="9.[151,1437,681,922]" pageId="9" pageNumber="70">
<materialsCitation ID-GBIF-Occurrence="1572326124" collectionCode="MSS" county="Pyrenees region" location="Outside" municipality="Ebro valley" pageId="9" pageNumber="70" specimenCount="1" stateProvince="Sardinia">
