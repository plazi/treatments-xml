<subSubSection pageId="11" pageNumber="512" type="etymology">
<paragraph blockId="11.[151,1436,151,392]" pageId="11" pageNumber="512">
<emphasis bold="true" box="[199,326,188,213]" pageId="11" pageNumber="512">Etymology</emphasis>
