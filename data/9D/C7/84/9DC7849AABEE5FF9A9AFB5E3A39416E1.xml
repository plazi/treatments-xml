</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Clethraceae" genus="Clethra" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Clethra arborea" order="Ericales" pageId="0" pageNumber="53690" phylum="Tracheophyta" rank="species" species="arborea">
<emphasis italics="true" pageId="0" pageNumber="53690">Clethra arborea</emphasis>
</taxonomicName>
, 
