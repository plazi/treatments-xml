<emphasis box="[856,1002,1626,1652]" italics="true" pageId="41" pageNumber="42">Echinophora</emphasis>
sp.
</taxonomicName>
; 
<taxonomicName box="[1059,1186,1626,1652]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="41" pageNumber="42" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
