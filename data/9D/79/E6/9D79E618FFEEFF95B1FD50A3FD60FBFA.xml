<emphasis box="[789,993,871,897]" italics="true" pageId="100" pageNumber="101">Centaurea virgata</emphasis>
</taxonomicName>
; 
<taxonomicName authorityName="G.T.Burnett" authorityYear="1835" box="[1008,1157,871,897]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
: 
<taxonomicName box="[1168,1389,871,897]" class="Magnoliopsida" family="Brassicaceae" genus="Descurainia" kingdom="Plantae" order="Brassicales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="species" species="sophia">
<emphasis box="[1168,1389,871,897]" italics="true" pageId="100" pageNumber="101">Descurainia sophia</emphasis>
