; 
<taxonomicName box="[388,497,907,933]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="100" pageNumber="101" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
