<subSubSection pageId="66" pageNumber="67" type="etymology">
<paragraph blockId="66.[151,1437,152,978]" pageId="66" pageNumber="67">
<emphasis bold="true" box="[198,339,911,937]" pageId="66" pageNumber="67">Etymology.</emphasis>
