<collectionCode id="9D99AE14B553663EFC83E54FE6BEFEBA" box="[860,936,284,310]" httpUri="http://grbio.org/cool/6pr5-9z6g" name="Department of Primary Industries" pageId="20" pageNumber="21">QPIM</collectionCode>
, 
<collectionCode id="9D99AE14B553663EFC6BE54FE11AFEBA" LSID="urn:lsid:biocol.org:col:34982" box="[948,1036,284,310]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34982" name="South Australia Museum" pageId="20" pageNumber="21">SAMA</collectionCode>
, 
<collectionCode id="9D99AE14B553663EFBC8E54FE17DFEBA" box="[1047,1131,284,310]" httpUri="http://grbio.org/cool/s8gp-ktsi" name="Transvaal Museum" pageId="20" pageNumber="21">TMSA</collectionCode>
, 
<collectionCode id="9D99AE14B553663EFBA7E54FE1D5FEBA" LSID="urn:lsid:biocol.org:col:34515" box="[1144,1219,284,310]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34515" name="University of Queensland Insect Collection" pageId="20" pageNumber="21">UQIC</collectionCode>
, 
<collectionCode id="9D99AE14B553663EFB11E54FE030FEBA" LSID="urn:lsid:biocol.org:col:34871" box="[1230,1318,284,310]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="20" pageNumber="21">USNM</collectionCode>
, and 
<collectionCode id="9D99AE14B553663EFEE6E517E484FED2" box="[313,402,324,350]" pageId="20" pageNumber="21">WAMP</collectionCode>
