<emphasis id="B9543C0D5C1D3D0967AEFB12AA3EFB3A" box="[404,572,1238,1260]" italics="true" pageId="21" pageNumber="98">Madrasostes agostii</emphasis>
</taxonomicName>
<emphasis id="B9543C0D5C1D3D096479FB12AA57FB3A" bold="true" box="[579,597,1238,1260]" pageId="21" pageNumber="98">C</emphasis>
<emphasis id="B9543C0D5C1D3D096466FB12AB0DFB3A" box="[604,783,1238,1260]" italics="true" pageId="21" pageNumber="98">
<taxonomicName id="4C209B9C5C1D3D096466FB12AAC7FB3A" box="[604,709,1238,1260]" class="Insecta" family="Hybosoridae" genus="Madrasostes" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="98" phylum="Arthropoda" rank="genus">Madrasostes</taxonomicName>
clypeale
</emphasis>
male 
<emphasis id="B9543C0D5C1D3D096572FB12AB5EFB3A" bold="true" box="[840,860,1238,1260]" pageId="21" pageNumber="98">D</emphasis>
idem female 
<emphasis id="B9543C0D5C1D3D0965E1FB12ABE9FB3A" bold="true" box="[987,1003,1238,1260]" pageId="21" pageNumber="98">E</emphasis>
<emphasis id="B9543C0D5C1D3D0965C8FB12A8E5FADA" italics="true" pageId="21" pageNumber="98">
<taxonomicName id="4C209B9C5C1D3D0965C8FB12A8BDFADA" class="Insecta" family="Hybosoridae" genus="Eusphaeropeltis" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="98" phylum="Arthropoda" rank="genus">Eusphaeropeltis</taxonomicName>
sp. b
</emphasis>
<emphasis id="B9543C0D5C1D3D0966D7FB32A8F9FADA" bold="true" box="[237,251,1270,1292]" pageId="21" pageNumber="98">F</emphasis>
