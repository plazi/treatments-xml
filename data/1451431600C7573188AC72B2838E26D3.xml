<taxonomicName id="377E63236FAB671CE523D3D1AECF87C0" class="Amphibia" family="Strabomantidae" genus="Pristimantis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristimantis lacrimosus" order="Anura" pageId="0" pageNumber="151" phylum="Chordata" rank="species" species="lacrimosus">
<emphasis id="F59E4C760B99543DA8E95B2D0083E19A" italics="true" pageId="0" pageNumber="151">Pristimantis lacrimosus</emphasis>
</taxonomicName>
