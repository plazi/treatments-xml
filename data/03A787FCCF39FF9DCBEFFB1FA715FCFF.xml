<taxonomicName id="4C0E4D69CF3AFF9CC815FA88A697FAED" authorityName="Szepligeti" authorityYear="1906" box="[543,645,1352,1371]" class="Insecta" family="Braconidae" genus="Bathyaulax" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="169" phylum="Arthropoda" rank="genus">
