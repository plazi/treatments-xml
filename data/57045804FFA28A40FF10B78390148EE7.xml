<taxonomicName id="18AD9291FFA28A40FE60B14E90538EC3" authorityName="Seeman &amp; Lindquist &amp; Husband" authorityYear="2018" box="[487,866,1914,1939]" class="Arachnida" family="Tarsonemidae" genus="Podotarsonemus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="queenslandensis">
