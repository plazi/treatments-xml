<taxonomicName id="18AD9291FFA28A40FB02B148962D8EC3" authorityName="Seeman &amp; Lindquist &amp; Husband" authorityYear="2018" box="[1157,1308,1916,1939]" class="Arachnida" family="Tarsonemidae" genus="Podotarsonemus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="nipponicus">
