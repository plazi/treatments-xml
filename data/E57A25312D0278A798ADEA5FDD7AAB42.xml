<taxonomicName id="983DDD5AA76289F2270B95527EAE91CB" authority="Solier, 1851" authorityName="Solier" authorityYear="1851" class="Insecta" family="Elateridae" genus="Olotelus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Olotelus" order="Coleoptera" pageId="193" pageNumber="194" phylum="Arthropoda" rank="genus">Olotelus</taxonomicName>
Solier, 1851 [
<taxonomicName id="65DEBDE129DF40CC47D67871B35065A5" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Coleoptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Coleoptera" order="Malvales" pageId="193" pageNumber="194" phylum="Tracheophyta" rank="genus">Coleoptera</taxonomicName>
: 
<taxonomicName id="5718D8AF0B44E6BC5ACBF7AC73AA8248" family="Elateridae" lsidName="" pageId="193" pageNumber="194" rank="family">Elateridae</taxonomicName>
