<taxonomicName id="A20F49E7D866AC394ED6A64EF39E9D75" authority="Alonso-Zarazaga, 2010" authorityName="Alonso-Zarazaga" authorityYear="2010" class="Insecta" family="Aderidae" genus="Gompelia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gompelia" order="Coleoptera" pageId="193" pageNumber="194" phylum="Arthropoda" rank="genus">Gompelia</taxonomicName>
