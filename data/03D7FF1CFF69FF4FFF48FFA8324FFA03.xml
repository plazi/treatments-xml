<taxonomicName id="4C7E3589FF69FF4FFDB6FE5D3235FABB" authorityName="Endrodi" authorityYear="1963" box="[574,841,1331,1355]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="162" pageNumber="161" phylum="Arthropoda" rank="species" species="unicolor">
