<typeStatus id="4C07885CFFCA3F631AAAFC6F64F0FC23" box="[151,201,936,960]" pageId="1" pageNumber="432">Type</typeStatus>
species: 
<taxonomicName id="54BC4D7DFFCA3F631B1BFC6E66A7FC23" authority="Tattersall, 1909" authorityName="Tattersall" authorityYear="1909" box="[294,670,935,960]" class="Malacostraca" family="Aegidae" genus="Xenuraega" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="432" phylum="Arthropoda" rank="species" species="ptilocera">
<emphasis id="A1C8EAECFFCA3F631B1BFC6E65CEFC23" box="[294,503,936,960]" italics="true" pageId="1" pageNumber="432">Xenuraega ptilocera</emphasis>
<bibRefCitation id="F72D4B0FFFCA3F631BC0FC6066A7FC23" author="Tattersall, W. M." box="[509,670,935,960]" pageId="1" pageNumber="432" pagination="210 - 219" refId="ref3341" refString="Tattersall, W. M. (1909) II. Amphipoda and Isopoda, with descriptions of two new species. Memoirs of the Challenger Society, 1, 210 - 219." type="journal article" year="1909">Tattersall, 1909</bibRefCitation>
