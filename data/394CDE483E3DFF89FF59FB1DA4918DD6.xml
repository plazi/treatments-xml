<taxonomicName id="76E514DD3E3DFF88FF59FB1DA4A68D54" authority="PORTER 1959" authorityName="PORTER" authorityYear="1959" box="[151,455,1272,1294]" class="Insecta" family="Hebridae" genus="Hebrus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="1015" phylum="Arthropoda" rank="species" species="harrisi">
