<emphasis id="7DF43702FF9AEE42FF56067B816BFB9D" bold="true" box="[151,324,1087,1112]" inLineHeading="true" pageId="5" pageNumber="81" reason="1">Type material.</emphasis>
<materialsCitation id="FFE8E14DFF9AEE42FE93060484CEFB92" box="[338,1249,1087,1112]" collectorName="Jian-Feng Wang." location="Guangdong" pageId="5" pageNumber="81" specimenCount="1" specimenCount-male="1" stateProvince="Guangdong" typeStatus="holotype">
