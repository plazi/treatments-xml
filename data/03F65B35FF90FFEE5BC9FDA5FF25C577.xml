<emphasis id="B92B3631FF90FFEE5BC9FDA5FDA3C3C4" bold="true" box="[151,617,617,643]" pageId="7" pageNumber="214">
Genus 
<taxonomicName id="4C5F91A0FF90FFEE5BB0FDA5FDA3C3C4" authority="Lundblad, 1941" authorityName="Lundblad" authorityYear="1941" box="[238,617,617,643]" class="Arachnida" family="Hygrobatidae" genus="Australiobates" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="214" phylum="Arthropoda" rank="genus">
