<quantity id="AD2569D8386CFFBE390C28B3FCE2D5EB" box="[637,772,927,951]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" metricValueMax="2.7" metricValueMin="2.5" pageId="31" pageNumber="32" unit="mm" value="2.6" valueMax="2.7" valueMin="2.5">2.5–2.7 mm</quantity>
