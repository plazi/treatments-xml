<taxonomicName id="ADDDBFBE386CFFBE3F7B28E8FA7ED580" authority="Fage, 1928" authorityName="Fage" authorityYear="1928" box="[1034,1432,964,989]" class="Malacostraca" family="Pseudocumatidae" genus="Pseudocuma" kingdom="Animalia" order="Cumacea" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="chevreuxi">
