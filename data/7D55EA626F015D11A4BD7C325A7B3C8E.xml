<taxonomicName id="EF5644D5689D53B6D2C8AE36B1309295" authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="67" pageNumber="68" phylum="Angiospermae" rank="species" species="stricta">
