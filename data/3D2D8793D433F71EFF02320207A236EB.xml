<taxonomicName id="72844D06D433F71EFE8C3288035936C6" authorityName="H. Adams &amp; A. Adams" authorityYear="1856" box="[281,353,1967,1990]" class="Gastropoda" family="Diplommatinidae" genus="Arinia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Architaenioglossa" pageId="37" pageNumber="238" phylum="Mollusca" rank="genus">
