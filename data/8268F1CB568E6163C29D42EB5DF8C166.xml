<taxonomicName id="F17637CC52CC7FB4D39293CEFECF0197" class="Malacostraca" family="Palaemonidae" genus="Macrobrachium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Macrobrachium" order="Decapoda" pageId="0" pageNumber="30507" phylum="Arthropoda" rank="genus">Macrobrachium</taxonomicName>
