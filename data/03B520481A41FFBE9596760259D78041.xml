<typeStatus id="54A72FFC1A47FFBE96D574FF5BDF8040" box="[450,501,1014,1040]" pageId="29" pageNumber="30">type</typeStatus>
locality, off 
<collectingCountry id="F30BD1CE1A47FFBE959E74FF59158040" box="[649,831,1014,1040]" name="New Caledonia" pageId="29" pageNumber="30">New Caledonia</collectingCountry>
