<taxonomicName id="16AECEBCC35CBA1CFF18F96D8035F924" box="[151,297,1744,1770]" class="Insecta" family="Diaspididae" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="family">Diaspididae</taxonomicName>
