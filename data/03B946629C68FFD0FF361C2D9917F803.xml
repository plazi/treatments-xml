<emphasis id="B9642B669C68FFD3FF661F439B4CFD96" bold="true" box="[199,345,546,571]" pageId="5" pageNumber="170">Description.</emphasis>
Body length 
<quantity id="4CE85A919C68FFD3FDA01F439884FD97" box="[513,657,546,570]" metricMagnitude="-3" metricUnit="m" metricValue="8.75" metricValueMax="9.8" metricValueMin="7.7" pageId="5" pageNumber="170" unit="mm" value="8.75" valueMax="9.8" valueMin="7.7">7.7–9.8 mm</quantity>
