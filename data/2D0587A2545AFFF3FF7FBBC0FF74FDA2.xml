<taxonomicName id="62AC4D37545AFFCCFB6CBEA1FAF3F942" authorityName="New" authorityYear="1985" box="[1235,1372,1666,1690]" class="Insecta" family="Myrmeleontidae" genus="Xantholeon" kingdom="Animalia" order="Neuroptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="species" species="xadnus">
