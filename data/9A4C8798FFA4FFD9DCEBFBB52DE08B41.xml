<taxonomicName id="D5E54D0DFFA4FFD9DD45FB7D2B2089B1" authorityName="Rowe &amp; Achmadi &amp; Esselstyn" authorityYear="2014" box="[313,526,1160,1184]" class="Mammalia" family="Muridae" genus="Waiomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="5" pageNumber="546" phylum="Chordata" rank="species" species="mamasae">
