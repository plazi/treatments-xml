<taxonomicName id="4C2CAEF79541FFEEFE19FDC4FD847FAA" authorityName="Cockerell" authorityYear="1891" box="[505,606,620,645]" class="Gastropoda" family="Athoracophoridae" genus="Aneitella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="37" pageNumber="238" phylum="Mollusca" rank="genus">
