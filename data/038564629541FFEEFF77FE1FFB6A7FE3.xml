<taxonomicName id="4C2CAEF79541FFEEFC73FDC4FC227FAA" authorityName="Cockerell" authorityYear="1891" box="[915,1016,620,645]" class="Gastropoda" family="Athoracophoridae" genus="Aneitella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="37" pageNumber="238" phylum="Mollusca" rank="genus">
