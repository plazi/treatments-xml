</paragraph>
<paragraph id="EC37194FFFF54F19E3830C15B24AFD34" blockId="26.[677,1210,536,931]" pageId="26" pageNumber="43">
<taxonomicName id="2B8862CCFFF54F19E3830C15B243FDCB" authority="d'Orbigny, 1850 b: 420" authorityName="d'Orbigny" authorityPageNumber="420" authorityYear="1850" box="[677,1102,536,560]" class="Gastropoda" family="Buccinidae" genus="Buccinum" kingdom="Animalia" order="Neogastropoda" pageId="26" pageNumber="43" phylum="Mollusca" rank="species" species="subandrei">
