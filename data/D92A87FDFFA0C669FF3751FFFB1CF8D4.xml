<taxonomicName id="96834D68FFA0C669FBEB5259FA32F8F1" authority="Kunth (1817: 47)" authorityName="Kunth" authorityPageNumber="47" authorityYear="1817" box="[1108,1447,1849,1875]" class="Magnoliopsida" family="Moraceae" genus="Ficus" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="150" phylum="Tracheophyta" rank="species" species="padifolia">
<emphasis id="63F7EAF9FFA0C669FBEB5259FB43F8F1" box="[1108,1238,1849,1875]" italics="true" pageId="1" pageNumber="150">F. padifolia</emphasis>
<bibRefCitation id="35124B1AFFA0C669FB60525AFA32F8F1" author="Kunth, C. S." box="[1247,1447,1849,1875]" pageId="1" pageNumber="150" pagination="28 - 50" refId="ref6377" refString="Kunth, C. S. (1817) Urticeae. In: F. W. H. A. von Humboldt, A. J. A. Bonpland &amp; C. S. Kunth, Nova genera et species plantarum 2: 28 - 50. Paris." type="journal article" year="1817">Kunth (1817: 47)</bibRefCitation>
</taxonomicName>
, and by Berg (2007), 
<bibRefCitation id="35124B1AFFA0C669FECF523FFD53F8D4" author="Ibarra-Manriquez, G. &amp; Cornejo-Tenorio, G. &amp; Gonzalez-Castaneda, N. &amp; Piedra-Malagon, E. M. &amp; Luna, A." box="[368,710,1884,1910]" pageId="1" pageNumber="150" pagination="389 - 452" refId="ref6291" refString="Ibarra-Manriquez, G., Cornejo-Tenorio, G., Gonzalez-Castaneda, N., Piedra-Malagon, E. M. &amp; Luna, A. (2012) El genero Ficus L. (Moraceae) en Mexico. Botanical Sciences 90: 389 - 452. https: // doi. org / 10.17129 / botsci. 472" type="journal article" year="2012">
Ibarra-Manríquez 
<emphasis id="63F7EAF9FFA0C669FD83523EFDE0F8D4" box="[572,629,1884,1910]" italics="true" pageId="1" pageNumber="150">et al.</emphasis>
(2012)
</bibRefCitation>
and 
<bibRefCitation id="35124B1AFFA0C669FD42523FFC79F8D4" author="Pelissari, G. &amp; Gaglioti, A. &amp; Romaniuc-Neto, S." box="[765,1004,1884,1910]" pageId="1" pageNumber="150" refId="ref7106" refString="Pelissari, G., Gaglioti, A. &amp; Romaniuc-Neto, S. (2019) Sinopse de Ficus subg. Spherosuke secao Americanae: citrifolia / pertusa. Novas Edicoes Academicas, International Book Market Service Ltd., Beau Bassin, Mauritius. 142 p." type="book" year="2019">
Pelissari 
<emphasis id="63F7EAF9FFA0C669FCDD523EFC0EF8D4" box="[866,923,1884,1910]" italics="true" pageId="1" pageNumber="150">et al.</emphasis>
(2019)
</bibRefCitation>
as 
<emphasis id="63F7EAF9FFA0C669FBAE523EFB1CF8D4" box="[1041,1161,1885,1910]" italics="true" pageId="1" pageNumber="150">
<taxonomicName id="96834D68FFA0C669FBAE523EFB10F8D4" box="[1041,1157,1885,1910]" class="Magnoliopsida" family="Moraceae" genus="Ficus" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="150" phylum="Tracheophyta" rank="species" species="pertusa">F. pertusa</taxonomicName>
