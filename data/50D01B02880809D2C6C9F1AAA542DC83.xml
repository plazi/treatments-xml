<taxonomicName id="7DD318F7C8B97433578BA161747CCCFE" LSID-ZBK="urn:lsid:zoobank.org:act:8723E2FA-0F3F-4066-AF11-DE57ECE17DF8" class="Actinopterygii" family="Doradidae" genus="Hypodoras" kingdom="Animalia" lsidName-ZBK="Hypodoras Eigenmann 1925:329" order="Siluriformes" pageNumber="33" phylum="Chordata" rank="genus">Hypodoras Eigenmann, 1925</taxonomicName>
