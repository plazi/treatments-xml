<taxonomicName id="465181550C24AA5BFBC9EC31B1940CCB" LSID-ZBK="urn:lsid:zoobank.org:act:F467A760-CA9E-4450-8DEC-7336C85E1072" class="Actinopterygii" family="Doradidae" genus="Anadoras" kingdom="Animalia" lsidName-ZBK="Anadoras Eigenmann 1925:305, 327" order="Siluriformes" pageNumber="33" phylum="Chordata" rank="genus">Anadoras Eigenmann, 1925</taxonomicName>
