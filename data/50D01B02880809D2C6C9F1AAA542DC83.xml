; 
<taxonomicName id="26DB35E8625432F44CCBC4FACAFB2B67" class="Actinopterygii" family="Doradidae" genus="Merodoras" kingdom="Animalia" order="Siluriformes" pageNumber="33" phylum="Chordata" rank="genus">Merodoras</taxonomicName>
