<quantity id="DF6E9B13A866FFB1FAD9FB74FAA8FB58" box="[1304,1353,1194,1217]" metricMagnitude="1" metricUnit="m" metricValue="1.8" pageId="17" pageNumber="724" unit="m" value="18.0">18m</quantity>
