<subSubSection id="45BB653FF365FF8EFF49FA0DFCCEF9A4" box="[151,813,1527,1549]" pageId="10" pageNumber="221" type="materials_examined">
<paragraph id="0D1E36B4F365FF8EFF49FA0DFCCEF9A4" blockId="10.[151,813,1527,1549]" box="[151,813,1527,1549]" pageId="10" pageNumber="221">
<emphasis id="3FD5EAA6F365FF8EFF49FA0DFEC1F9A4" bold="true" box="[151,290,1527,1549]" pageId="10" pageNumber="221">
<typeStatus id="D21A8816F365FF8EFF49FA0DFF2FF9A4" box="[151,204,1527,1549]" pageId="10" pageNumber="221">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="CAA14D37F365FF8EFEF7FA0DFE1FF9A4" box="[297,508,1527,1549]" class="Malacostraca" family="Varunidae" genus="Proexotelson" kingdom="Animalia" order="Decapoda" pageId="10" pageNumber="221" phylum="Arthropoda" rank="species" species="tokoroi" status="n. sp.">
