<taxonomicName id="4C225CA2693A2A65B3F3FD8712B3FDBD" authority="C.Chr., 1934 (C.Presl)" authorityName="C.Chr., 1934 (C.Presl)" box="[754,954,579,606]" class="Polypodiopsida" family="Hymenophyllaceae" genus="Sphaerocionium" higherTaxonomySource="GBIF" kingdom="Plantae" order="Hymenophyllales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="genus">
