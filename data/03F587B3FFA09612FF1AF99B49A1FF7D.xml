<taxonomicName id="4C5C4D26FFA29613FEF0F8D04DD2F8BB" box="[381,495,1802,1825]" class="Insecta" family="Ascalaphidae" genus="Kimulodes" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="39" phylum="Arthropoda" rank="genus">
