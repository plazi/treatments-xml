<number id="3DEA481E4D5C6538FDBAF97AD572F914" box="[549,560,1677,1699]" pageId="5" pageNumber="516" value="3.0">3</number>
/ 
<number id="3DEA481E4D5C6538FDA7F97AD507F914" box="[568,581,1677,1699]" pageId="5" pageNumber="516" value="4.0">4</number>
