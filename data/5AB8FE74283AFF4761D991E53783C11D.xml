<taxonomicName id="0FD74B88DCFA699475260204BBB19695" class="Insecta" family="Yponomeutidae" genus="Eucalantica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucalantica polita" order="Lepidoptera" pageId="14" pageNumber="89" phylum="Arthropoda" rank="species" species="polita">Eucalantica polita</taxonomicName>
