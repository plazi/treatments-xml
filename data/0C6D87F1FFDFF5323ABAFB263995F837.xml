<subSubSection id="CCDE656CFFDFF5313ABAFAEE381EFA8D" box="[151,844,1340,1364]" pageId="9" pageNumber="44" type="materials_examined">
<paragraph id="847B36E7FFDFF5313ABAFAEE381EFA8D" blockId="9.[151,844,1340,1364]" box="[151,844,1340,1364]" pageId="9" pageNumber="44">
<typeStatus id="5B7F8845FFDFF5313ABAFAEE3B9EFA8D" box="[151,204,1341,1364]" pageId="9" pageNumber="44">Type</typeStatus>
