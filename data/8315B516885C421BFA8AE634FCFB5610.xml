<bibRefCitation id="6F2D79F1885C421BFB56E697FDE6548B" author="YU D &amp; VAN ACHTERBERG K" box="[366,514,429,451]" pageId="2" pageNumber="683" refId="ref5679" refString="YU D. S., VAN ACHTERBERG K. &amp; K. HORSTMANN (2012): World Ichneumonoidae 2011. Taxonomy, Biology, Morphology and Distribution. - www. Taxapad. com, Ottawa, Ontario, Canada." type="book" year="2012">YU et al. 2012</bibRefCitation>
) and 
<collectingCountry id="73AB4490885C421BF802E694FD87548B" box="[570,611,430,451]" name="Iran" pageId="2" pageNumber="683">Iran</collectingCountry>
