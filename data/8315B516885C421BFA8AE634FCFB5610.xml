<taxonomicName id="CCBC7F83885C421BFB7EE41FFDB45672" authorityName="HORSTMANN" authorityYear="1969" box="[326,592,805,826]" class="Insecta" family="Ichneumonidae" genus="Diadegma" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="683" phylum="Arthropoda" rank="species" species="longicaudatum">
