<paragraph id="8567DC0764C3093EDFB1CE74461F8260" pageId="17" pageNumber="76">
<taxonomicName id="40B5966316A323577BE4383FAC7C6D90" LSID="E029AE8B-22B6-375C-3E8C-7733EAFA54B8" authority="N. Snow &amp; D. Turk" authorityName="N. Snow &amp; D. Turk" authorityYear="2015" class="Insecta" family="Myrtaceae" genus="Eugenia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eugenia ranomafana" order="Myrtales" pageId="17" pageNumber="76" phylum="Arthropoda" rank="species" species="ranomafana" status="sp. nov.">Eugenia ranomafana N. Snow &amp; D. Turk</taxonomicName>
