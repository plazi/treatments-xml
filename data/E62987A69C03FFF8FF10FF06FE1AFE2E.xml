<taxonomicName id="A9804D339C03FFF8FE49FF41FD73FF5E" authority="Hentz, 1844" authorityName="Hentz" authorityYear="1844" box="[462,743,223,247]" class="Arachnida" family="Lycosidae" genus="Allocosa" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="263" phylum="Arthropoda" rank="species" species="funerea">
