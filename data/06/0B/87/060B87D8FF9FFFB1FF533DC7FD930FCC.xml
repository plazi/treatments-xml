, 
<number box="[1133,1148,1674,1700]" pageId="30" pageNumber="31" value="2.0">2</number>
<emphasis box="[1156,1183,1675,1700]" italics="true" pageId="30" pageNumber="31">pv</emphasis>
; hind tibia with 
<number box="[1380,1395,1674,1700]" pageId="30" pageNumber="31" value="4.0">4</number>
<emphasis box="[1403,1429,1675,1700]" italics="true" pageId="30" pageNumber="31">av</emphasis>
, 
<number box="[151,195,1714,1740]" pageId="30" pageNumber="31" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
