, 
<number box="[249,293,1714,1740]" pageId="30" pageNumber="31" value="6.0" valueMax="7.0" valueMin="5.0">5–7</number>
<emphasis box="[302,331,1715,1740]" italics="true" pageId="30" pageNumber="31">pd</emphasis>
, 
<number box="[347,391,1714,1740]" pageId="30" pageNumber="31" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
