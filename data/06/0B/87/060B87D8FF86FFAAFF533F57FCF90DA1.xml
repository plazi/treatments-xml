<taxonomicName authority="Zetterstedt, 1845" authorityName="Zetterstedt" authorityYear="1845" class="Insecta" family="Anthomyiidae" genus="Botanophila" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="latifrons">
