<collectingCountry box="[627,697,1469,1495]" name="China" pageId="5" pageNumber="6">China</collectingCountry>
are 
<taxonomicName box="[748,894,1469,1495]" class="Insecta" family="Anthomyiidae" genus="Botanophila" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="askoldica">
