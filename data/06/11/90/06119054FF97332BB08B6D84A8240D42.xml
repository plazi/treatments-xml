<collectingRegion box="[1040,1217,1119,1145]" country="Bulgaria" name="Burgas" pageId="21" pageNumber="22">Burgas Region</collectingRegion>
) [2]; Asenovgrad, Harmanli [3]; Belogradchik, Karlovo, 
<collectingRegion box="[659,764,1161,1187]" country="Bulgaria" name="Gabrovo" pageId="21" pageNumber="22">Gabrovo</collectingRegion>
