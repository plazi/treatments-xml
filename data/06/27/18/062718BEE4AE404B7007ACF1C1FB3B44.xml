<taxonomicName authority="Eschscholtz, 1829" authorityName="Eschscholtz" authorityYear="1829" class="Insecta" family="Tenebrionidae" genus="Praocis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Praocis silphoides" order="Coleoptera" pageId="17" pageNumber="70" phylum="Arthropoda" rank="species" species="silphoides">Praocis silphoides</taxonomicName>
