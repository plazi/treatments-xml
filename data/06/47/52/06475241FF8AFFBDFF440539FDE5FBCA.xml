<taxonomicName authority="Linnaeus, 1758" authorityName="Duges" authorityYear="1889" box="[757,952,978,1003]" class="Insecta" family="Cantharidae" genus="Cantharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="alfredi">
