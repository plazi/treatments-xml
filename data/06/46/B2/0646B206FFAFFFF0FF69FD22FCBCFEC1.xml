<number box="[916,957,1375,1399]" pageId="6" pageNumber="83" value="4.0" valueMax="5.0" valueMin="3.0">3-5</number>
- 
<number box="[966,977,1375,1399]" pageId="6" pageNumber="83" value="1.0">1</number>
; Leg III: 
<number box="[1083,1094,1375,1399]" pageId="6" pageNumber="83" value="1.0">1</number>
- 
<number box="[1103,1140,1375,1399]" pageId="6" pageNumber="83" value="1.5" valueMax="2.0" valueMin="1.0">
1- 
<date box="[1123,1140,1375,1399]" pageId="6" pageNumber="83" value="1914-03-02" valueMax="1914-06-03" valueMin="1914-03-02">2</date>
</number>
