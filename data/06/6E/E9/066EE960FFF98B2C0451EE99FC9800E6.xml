<subSubSection pageId="11" pageNumber="12" type="description">
<paragraph blockId="11.[264,1325,284,1230]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[312,440,804,830]" italics="true" pageId="11" pageNumber="12">Bionomics</emphasis>
