<emphasis bold="true" box="[151,515,951,977]" pageId="4" pageNumber="49">
<taxonomicName box="[151,357,951,977]" class="Insecta" family="Ephemerellidae" genus="Crinitella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="lacuna" status="new species">
<emphasis bold="true" box="[151,357,951,977]" italics="true" pageId="4" pageNumber="49">Crinitella lacuna</emphasis>
