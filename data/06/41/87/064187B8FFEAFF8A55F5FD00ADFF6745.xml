(
<number box="[159,237,1145,1171]" pageId="19" pageNumber="66" value="40.5" valueMax="41.0" valueMin="40.0">40–41</number>
; n = 
<number box="[294,310,1145,1171]" pageId="19" pageNumber="66" value="3.0">3</number>
); outer length 
<number box="[481,510,1145,1171]" pageId="19" pageNumber="66" value="42.0">42</number>
(
<number box="[526,604,1145,1171]" pageId="19" pageNumber="66" value="41.5" valueMax="43.0" valueMin="40.0">40–43</number>
; n = 
<number box="[661,677,1145,1171]" pageId="19" pageNumber="66" value="3.0">3</number>
); point 
<number box="[768,797,1145,1171]" pageId="19" pageNumber="66" value="32.0">32</number>
(
<number box="[814,891,1145,1171]" pageId="19" pageNumber="66" value="32.0" valueMax="33.0" valueMin="31.0">31–33</number>
; n = 
<number box="[948,963,1145,1171]" pageId="19" pageNumber="66" value="3.0">3</number>
) long. One bar (ventral): 
<number box="[1263,1292,1145,1171]" pageId="19" pageNumber="66" value="65.0">65</number>
(
<number box="[1308,1385,1145,1171]" pageId="19" pageNumber="66" value="65.5" valueMax="67.0" valueMin="64.0">64–67</number>
; n = 
<number box="[151,166,1184,1210]" pageId="19" pageNumber="66" value="3.0">3</number>
) long, 
<number box="[250,265,1184,1210]" pageId="19" pageNumber="66" value="7.0">7</number>
(
<number box="[282,329,1184,1210]" pageId="19" pageNumber="66" value="7.0" valueMax="8.0" valueMin="6.0">6–8</number>
