<number box="[407,436,1097,1123]" pageId="17" pageNumber="64" value="10.0">10</number>
(
<number box="[453,514,1097,1123]" pageId="17" pageNumber="64" value="10.0" valueMax="11.0" valueMin="9.0">9–11</number>
