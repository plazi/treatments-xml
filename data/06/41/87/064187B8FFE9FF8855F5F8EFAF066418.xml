<number box="[600,629,1058,1084]" pageId="17" pageNumber="64" value="40.0">40</number>
(
<number box="[645,723,1058,1084]" pageId="17" pageNumber="64" value="40.0" valueMax="42.0" valueMin="38.0">38–42</number>
