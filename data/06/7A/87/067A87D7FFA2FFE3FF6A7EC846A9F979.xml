<quantity box="[474,636,1296,1322]" metricMagnitude="-2" metricUnit="m" metricValue="1.215" metricValueMax="1.25" metricValueMin="1.18" pageId="5" pageNumber="45" unit="mm" value="12.15" valueMax="12.5" valueMin="11.8">11.8–12.5mm</quantity>
long, 
<number box="[718,754,1296,1322]" pageId="5" pageNumber="45" value="2.9">2.9</number>
–3.0mm wide across eyes, 
<number box="[1086,1121,1296,1322]" pageId="5" pageNumber="45" value="3.9">3.9</number>
–4.0mm wide across hind margin of pronotum. Female 
<number box="[499,547,1333,1359]" pageId="5" pageNumber="45" value="14.1">14.1</number>
–15.0mm long, 
<quantity box="[738,871,1333,1359]" metricMagnitude="-3" metricUnit="m" metricValue="3.3499999999999996" metricValueMax="3.4" metricValueMin="3.3" pageId="5" pageNumber="45" unit="mm" value="3.3499999999999996" valueMax="3.4" valueMin="3.3">3.3–3.4mm</quantity>
