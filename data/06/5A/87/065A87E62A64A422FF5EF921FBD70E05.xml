. 
<location LSID="urn:lsid:plazi:treatment:065A87E62A64A422FF5EF921FBD70E05:8B2C602B2A64A42DFE4CF894FDD60D0E" box="[389,576,1801,1824]" country="Tanzania" name="Lake Tanganyika" pageId="23" pageNumber="24">Lake Tanganyika</location>
, tangled in trawl from 
<elevation box="[821,931,1801,1824]" metricMagnitude="0" metricUnit="m" metricValue="8.0" pageId="23" pageNumber="24" unit="m" value="8.0">
<quantity box="[821,862,1801,1824]" metricMagnitude="0" metricUnit="m" metricValue="8.0" pageId="23" pageNumber="24" unit="m" value="8.0">8 m</quantity>
