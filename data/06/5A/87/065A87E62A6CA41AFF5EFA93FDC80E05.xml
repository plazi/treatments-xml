<emphasis box="[548,569,964,989]" italics="true" pageId="32" pageNumber="33">H</emphasis>
. 
<emphasis box="[586,654,964,989]" italics="true" pageId="32" pageNumber="33">acuta</emphasis>
</taxonomicName>
(
<figureCitation box="[672,850,964,990]" captionStart="FIGURES 443–498" captionStartId="50.[151,270,1654,1678]" captionTargetBox="[264,1324,194,1630]" captionTargetId="figure-42@50.[264,1324,194,1630]" captionTargetPageId="50" captionText="FIGURES 443–498: LM micrographs. Figs 443–469. Hippodonta subelegans. Figs 470–481. Hippodonta acuta (type material). Figs 482–498. Hippodonta microcostulata. Scale bar = 10 µm." figureDoi="http://doi.org/10.5281/zenodo.5072006" httpUri="https://zenodo.org/record/5072006/files/figure.png" pageId="32" pageNumber="33">Figs 470–481</figureCitation>
). 
<taxonomicName box="[872,1084,964,989]" class="Bacillariophyceae" family="Naviculaceae" genus="Hippodonta" higherTaxonomySource="GBIF" kingdom="Chromista" order="Naviculales" pageId="32" pageNumber="33" phylum="Bacillariophyta" rank="species" species="acus">
<emphasis box="[872,1084,964,989]" italics="true" pageId="32" pageNumber="33">Hippodonta acus</emphasis>
