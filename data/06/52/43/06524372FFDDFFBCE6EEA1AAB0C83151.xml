<subSubSection pageId="47" pageNumber="48" type="materials_examined">
<paragraph blockId="47.[151,1437,151,1333]" pageId="47" pageNumber="48">
<emphasis bold="true" box="[199,434,1234,1259]" pageId="47" pageNumber="48">Material Examined</emphasis>
. 
<materialsCitation collectingDate="1994-06-13" collectingDateMax="1994-06-23" collectingDateMin="1994-06-13" collectionCode="CNCI" country="Ecuador" elevation="270" latitude="-0.5" location="Sacha Lodge" longLatPrecision="7856" longitude="-76.5" municipality="River" pageId="47" pageNumber="48" specimenCount="1" specimenCount-female="1" stateProvince="Sucumbios" typeStatus="holotype">
<typeStatus box="[452,556,1234,1258]" pageId="47" pageNumber="48">Holotype</typeStatus>
