Nakhon 
<collectingRegion box="[576,817,506,531]" inferredBySuffix="true" pageId="56" pageNumber="57">Ratchasima Province</collectingRegion>
</collectingRegion>
, environs of 
<location LSID="urn:lsid:plazi:treatment:0636885FFF897243FF74549CFE5A874F:8B406F92FF897243FC2255F1FB588722" box="[961,1237,506,531]" country="Thailand" name="Khao Yao National Park" pageId="56" pageNumber="57" stateProvince="Nakhon Ratchasima Province">Khao Yao National Park</location>
, 
<elevation box="[1249,1393,506,531]" metricMagnitude="2" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="56" pageNumber="57" unit="m" value="750.0" valueMax="1000.0" valueMin="500.0">
<quantity box="[1249,1393,506,531]" metricMagnitude="2" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="56" pageNumber="57" unit="m" value="750.0" valueMax="1000.0" valueMin="500.0">500–1000 m</quantity>
