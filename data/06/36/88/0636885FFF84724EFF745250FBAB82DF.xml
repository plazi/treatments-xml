(= 
<taxonomicName baseAuthorityName="Bolivar" baseAuthorityYear="1887" box="[699,965,1758,1780]" class="Insecta" family="Tetrigidae" genus="Tagaloscelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="aurivillii">
