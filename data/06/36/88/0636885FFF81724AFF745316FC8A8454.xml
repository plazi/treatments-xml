<collectionCode box="[511,558,189,212]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33211" name="Senckenberg Deutsches Entomologisches Institut" pageId="49" pageNumber="50">DEI</collectionCode>
and 
<collectionCode box="[612,678,189,212]" country="Germany" httpUri="http://grbio.org/cool/4j3t-w40g" name="Museum of Zoology Senckenberg Dresden" pageId="49" pageNumber="50">MTD</collectionCode>
