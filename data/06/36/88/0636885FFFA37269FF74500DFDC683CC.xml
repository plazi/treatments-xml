), 
<taxonomicName authorityName="Zheng" authorityYear="1994" box="[1053,1243,1727,1752]" class="Insecta" family="Tetrigidae" genus="Paragavialidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
<emphasis box="[1053,1243,1727,1752]" italics="true" pageId="18" pageNumber="19">Paragavialidium</emphasis>
