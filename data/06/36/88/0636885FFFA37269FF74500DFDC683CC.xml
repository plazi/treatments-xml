<taxonomicName authorityName="Bolívar" authorityYear="1887" box="[348,482,1764,1789]" class="Insecta" family="Tetrigidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="tribe" tribe="Scelimenini">Scelimenini</taxonomicName>
