<taxonomicName baseAuthorityName="Bolivar" baseAuthorityYear="1887" box="[740,991,438,461]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="novaeguineae">
