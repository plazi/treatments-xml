(= 
<taxonomicName baseAuthorityName="Hancock" baseAuthorityYear="1915" box="[733,920,222,244]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="discalis">
