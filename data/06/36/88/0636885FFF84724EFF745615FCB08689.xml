<collectingRegion box="[1115,1205,892,917]" country="China" name="Sichuan" pageId="53" pageNumber="54">Sichuan</collectingRegion>
(Mabian), 
<collectingRegion box="[1340,1436,892,917]" country="China" name="Guizhou" pageId="53" pageNumber="54">Guizhou</collectingRegion>
(Chishui), 
<collectingRegion box="[270,339,927,952]" country="China" name="Hubei" pageId="53" pageNumber="54">Hubei</collectingRegion>
(Lichuan), and 
<collectingRegion box="[518,593,928,951]" country="China" name="Hunan" pageId="53" pageNumber="54">Hunan</collectingRegion>
