<taxonomicName authorityName="Muhammad &amp; Tan &amp; Abdullah &amp; Azirun &amp; Bhaskar &amp; Skejo" authorityYear="2018" baseAuthorityName="Walker" baseAuthorityYear="1871" box="[1285,1436,1018,1043]" class="Insecta" family="Tetrigidae" genus="Platygavialidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="productum" status="comb. nov.">
<emphasis box="[1285,1436,1018,1043]" italics="true" pageId="39" pageNumber="40">P. productum</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[151,279,1054,1079]" pageId="39" pageNumber="40">
<emphasis bold="true" box="[151,279,1054,1079]" pageId="39" pageNumber="40">comb. nov.</emphasis>
</taxonomicNameLabel>
), PR 
<collectingCountry box="[344,411,1054,1079]" name="China" pageId="39" pageNumber="40">China</collectingCountry>
(
<taxonomicName authorityName=": Gunther" authorityYear="1939" box="[427,538,1055,1078]" class="Insecta" family="Tetrigidae" genus="Platygavialidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="sinicum">
