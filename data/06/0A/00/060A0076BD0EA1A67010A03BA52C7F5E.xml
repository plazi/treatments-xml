<subSubSection pageId="null" pageNumber="470" type="nomenclature">
<paragraph pageId="null" pageNumber="470">
<taxonomicName authority="Medikus" class="Magnoliopsida" family="Rosaceae" genus="Amelanchier" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="470" phylum="Tracheophyta" rank="genus">
