<emphasis bold="true" box="[151,270,525,551]" italics="true" pageId="11" pageNumber="44">Patellapis</emphasis>
(
<emphasis bold="true" box="[287,438,525,551]" italics="true" pageId="11" pageNumber="44">Chaetalictus</emphasis>
) 
<emphasis bold="true" box="[455,564,525,551]" italics="true" pageId="11" pageNumber="44">upembae</emphasis>
