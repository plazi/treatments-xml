<taxonomicName box="[151,563,1119,1145]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="itigiensis" status="sp. n." subGenus="Chaetalictus">
<emphasis bold="true" box="[151,270,1119,1145]" italics="true" pageId="6" pageNumber="39">Patellapis</emphasis>
(
<emphasis bold="true" box="[287,438,1119,1145]" italics="true" pageId="6" pageNumber="39">Chaetalictus</emphasis>
) 
<emphasis bold="true" box="[455,563,1119,1145]" italics="true" pageId="6" pageNumber="39">itigiensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[570,636,1119,1145]" pageId="6" pageNumber="39" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
