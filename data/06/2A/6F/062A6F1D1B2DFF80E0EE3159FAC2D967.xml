<typeStatus box="[151,212,674,700]" pageId="9" pageNumber="40">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName authority="Moroni, 1952" authorityName="Moroni" authorityYear="1952" box="[321,861,674,700]" class="Maxillopoda" family="Archaeobalanidae" genus="Actinobalanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sessilia" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="actinomorphus">
