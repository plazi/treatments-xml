<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[397,487,1557,1583]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="7" pageNumber="38">MNHN</collectionCode>
