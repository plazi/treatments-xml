<taxonomicName class="Monocotyledoneae" family="Commelinaceae" genus="Murdannia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Commelinales" pageId="5" pageNumber="14" phylum="Angiospermae" rank="species" species="nudiflora">
