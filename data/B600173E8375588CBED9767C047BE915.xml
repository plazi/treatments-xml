<geoCoordinate id="90B17083BCC34CAA555FF41CD6E7C4CD" degrees="98.75917" direction="east" orientation="longitude" precision="1" value="98.75917">98.75917E</geoCoordinate>
, 2010 m, 26.05.2005, H.M. Yan leg. (GKJ026); 2♀, Longyang District, Bawan Village, Nankang Valley, 
<geoCoordinate id="EE6405F021987B277A174EC0C74F9A2F" degrees="24.82587" direction="north" orientation="latitude" precision="1" value="24.82587">24.82587N</geoCoordinate>
, 
<geoCoordinate id="7AE4F91E806734D0AFCE048A8B1E9A60" degrees="98.76832" direction="east" orientation="longitude" precision="1" value="98.76832">98.76832E</geoCoordinate>
