<taxonomicName id="4C574D570702FFDEFED8FD17FE21A8F5" baseAuthorityName="Pallas" baseAuthorityYear="1774" box="[318,418,725,742]" class="Echinoidea" family="Diadematidae" genus="Echinothrix" kingdom="Animalia" order="Diadematoida" pageId="4" pageNumber="97" phylum="Echinodermata" rank="species" species="calamaris">
<emphasis id="B923EAC60702FFDEFED8FD17FE21A8F5" box="[318,418,725,742]" italics="true" pageId="4" pageNumber="97">E. calamaris</emphasis>
