</taxonomicName>
, 
<taxonomicName id="4C2D4D375B5AFFE8FD42FB31FC6731FC" authority="Spaeth" authorityName="Spaeth" authorityYear="1938" box="[692,951,1247,1273]" class="Insecta" family="Chrysomelidae" genus="Parimatidium" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="genus">
