and 
<emphasis id="B956EADFEB3DFFB3FBC4FAAA570EFAA5" box="[1059,1087,1374,1398]" italics="true" pageId="3" pageNumber="518">ad</emphasis>
) or marginally pilose (
<date id="FF9C100DEB3DFFB3FAA7FAAB5657FAA4" box="[1344,1382,1375,1399]" pageId="3" pageNumber="518">
V 
<number id="3DEA24EAEB3DFFB3FABFFAAB5657FAA4" box="[1368,1382,1375,1399]" pageId="3" pageNumber="518" value="8.0">8</number>
</date>
). 
<date id="FF9C100DEB3DFFB3FA9DFAAB56ADFAA4" box="[1402,1436,1375,1399]" pageId="3" pageNumber="518">
V 
<number id="3DEA24EAEB3DFFB3FA69FAAB56ADFAA4" box="[1422,1436,1375,1399]" pageId="3" pageNumber="518" value="2.0">2</number>
</date>
(
