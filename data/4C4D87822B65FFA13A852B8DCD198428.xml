<taxonomicName id="03E44D172B64FFA13E7A2DA5CB808598" box="[1128,1252,189,215]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
