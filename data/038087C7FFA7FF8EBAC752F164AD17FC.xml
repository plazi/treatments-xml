<taxonomicName id="4C294D52FFA7FF8EB88651FD634E17FC" authorityName="Rao &amp; Ganapati" authorityYear="1968" box="[726,843,690,715]" class="Gastrotricha (awaiting allocation)" family="Thaumastodermatidae" genus="Tetranchyroderma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macrodasyida" pageId="8" pageNumber="503" phylum="Gastrotricha" rank="species" species="indicum">
