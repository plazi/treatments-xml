<collectionCode id="03FAAE5559059824D2EF881E71699D1A" box="[1013,1099,830,856]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="80" pageNumber="81">MNHN</collectionCode>
