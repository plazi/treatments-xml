<normalizedToken id="7F14D6D9373663166C18D6F8AF395FAB" originalValue="εἶδος">εἶδος</normalizedToken>
, 
<normalizedToken id="CC03EC0C20455B543B097735BCC5919D" originalValue="eîdos">eidos</normalizedToken>
