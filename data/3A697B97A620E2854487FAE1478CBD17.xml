<taxonomicName id="66E4D70394DD28DC9E6EDCAFE38765E8" class="Gastropoda" family="Ficidae" genus="Ficus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ficus" order="Littorinimorpha" pageId="2" pageNumber="59" phylum="Mollusca" rank="genus">Ficus</taxonomicName>
sp., 
<taxonomicName id="3419C1C824620AF1356F094E897F17E5" genus="F." lsidName="F. microcarpa" pageId="2" pageNumber="59" rank="species" species="microcarpa">F. microcarpa</taxonomicName>
