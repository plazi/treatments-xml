: 
<taxonomicName id="1909D71EA340A9C0AE3476477DEBF49B" class="Magnoliopsida" family="Lauraceae" genus="Cinnamomum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cinnamomum japonicum" order="Laurales" pageId="2" pageNumber="59" phylum="Tracheophyta" rank="species" species="japonicum">Cinnamomum japonicum</taxonomicName>
