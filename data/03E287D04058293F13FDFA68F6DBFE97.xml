</treatmentCitationGroup>
</paragraph>
<paragraph id="8BF436C64058293E13FDF8B1F02DF8E2" blockId="20.[151,871,1547,1847]" box="[151,521,1823,1847]" pageId="20" pageNumber="473">
<treatmentCitationGroup id="AB5B11E84058293E13FDF8B1F02DF8E2" box="[151,521,1823,1847]" pageId="20" pageNumber="473">
