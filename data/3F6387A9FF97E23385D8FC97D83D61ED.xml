<taxonomicName id="70CA4D3CFF97E23384EBFC24DE8B6139" authority="El Beshbeeshy, 1991" authorityName="El Beshbeeshy" authorityYear="1991" box="[420,901,1015,1040]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="122" pageNumber="123" phylum="Cnidaria" rank="species" species="vervoorti">
