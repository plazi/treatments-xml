<taxonomicName id="70CA4D3CFF97E2338147FC24D95A6139" box="[1032,1108,1015,1040]" class="Hydrozoa" family="Oceaniidae" genus="Rhizogeton" kingdom="Animalia" order="Anthoathecata" pageId="122" pageNumber="123" phylum="Cnidaria" rank="species" species="nudum">nudum</taxonomicName>
