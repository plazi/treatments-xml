<taxonomicName id="8FEE4D41FF53FF78FF67FB5666F9CE4C" authority="Tomlinson and Apanaskevich, 2019" authorityName="Tomlinson and Apanaskevich" authorityYear="2019" box="[247,954,1265,1292]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="186" pageNumber="187" phylum="Arthropoda" rank="species" species="princeps">
