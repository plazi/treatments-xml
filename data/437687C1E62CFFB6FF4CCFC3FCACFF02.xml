) from 
<taxonomicName id="0CDF4D54E62CFFB7FBA4CA44FF10F85A" authority="(Kuhl)" baseAuthorityName="Kuhl" baseAuthorityYear="1817" class="Mammalia" family="Miniopteridae" genus="Miniopterus" kingdom="Animalia" order="Chiroptera" pageId="54" pageNumber="55" phylum="Chordata" rank="species" species="schreibersi">
