<paragraph id="FE66A62AFF982D1349EAFED8FE4E5C75" blockId="6.[189,1399,313,481]" box="[234,455,313,340]" pageId="6" pageNumber="7">
<materialsCitation id="4EB1AC77FF982D1349EAFED8FE4E5C75" ID-GBIF-Occurrence="2625648362" box="[234,455,313,340]" country="Germany" pageId="6" pageNumber="7" typeStatus="holotype">
