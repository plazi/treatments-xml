) or Odostomellinae (
<taxonomicName id="29E64D07FF8FB31C3FED540A28232862" box="[161,310,1841,1866]" class="Gastropoda" family="Pyramidellidae" genus="Odostomella" kingdom="Animalia" pageId="4" pageNumber="43" phylum="Mollusca" rank="genus">
