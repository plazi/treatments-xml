and 
<taxonomicName id="29E64D07FF8FB31C3B7754302DA7280C" box="[1083,1202,1803,1828]" class="Gastropoda" family="Pyramidellidae" genus="Trabecula" kingdom="Animalia" pageId="4" pageNumber="43" phylum="Mollusca" rank="genus">
