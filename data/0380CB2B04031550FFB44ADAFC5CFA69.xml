<taxonomicName id="4C2901BE04031550FD184BAAFD2CFA69" authorityName="Linnaeus" authorityYear="1758" box="[571,711,1411,1437]" class="Gastropoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Mollusca" rank="order">Lepidoptera</taxonomicName>
