<bibRefCitation id="EFB8EBA9FB143E04FD0D2EC813FAFE05" author="Fisher" box="[331,491,476,501]" pageId="30" pageNumber="481" refString="Fisher, W. S. (1942) New West Indian cerambycid beetles. III. Torreia, 10, 1 - 43." type="journal article" year="1942">Fisher (1942)</bibRefCitation>
described 
<taxonomicName id="4C29EDDBFB143E04FE312EC81151FE04" authorityName="Fisher" authorityYear="1942" box="[631,832,476,501]" class="Insecta" family="Cerambycidae" genus="Ataxia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="481" phylum="Arthropoda" rank="species" species="monticola">
