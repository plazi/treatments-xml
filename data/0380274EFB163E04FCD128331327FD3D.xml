<taxonomicName id="4C29EDDBFB143E04FF112EC811C0FE04" authorityName="J. Thomson" authorityYear="1860" box="[855,977,476,501]" class="Insecta" family="Cerambycidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="481" phylum="Arthropoda" rank="tribe" tribe="Pteropliini">Pteropliini</taxonomicName>
