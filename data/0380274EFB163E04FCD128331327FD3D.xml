<collectingCountry id="F33ED6C8FB143E04F8692EC8169DFE04" box="[1071,1164,476,501]" name="Jamaica" pageId="30" pageNumber="481">Jamaica</collectingCountry>
, and 
<bibRefCitation id="EFB8EBA9FB143E04F89F2EC8178DFE04" author="Breuning" box="[1241,1436,476,501]" pageId="30" pageNumber="481" refString="Breuning, S. (1954) Nouvelles formes de Lamiaires (Septieme partie). Bulletin de l'Institut de Sciences Naturelles de Belgique, 30 (41), 1 - 24." type="journal article" year="1954">Breuning (1954)</bibRefCitation>
described 
<taxonomicName id="4C29EDDBFB143E04FD492EEB10C3FDE9" authorityName="Breuning" authorityYear="1954" box="[271,722,511,536]" class="Insecta" family="Cerambycidae" genus="Estoloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="481" phylum="Arthropoda" rank="species" species="spinipennis" subGenus="Spinestoloides">
<emphasis id="B95D4A4AFB143E04FD492EEB1392FDE9" box="[271,387,511,536]" italics="true" pageId="30" pageNumber="481">Estoloides</emphasis>
(
<emphasis id="B95D4A4AFB143E04FDDC2EEB102DFDE9" box="[410,572,511,536]" italics="true" pageId="30" pageNumber="481">Spinestoloides</emphasis>
) 
<emphasis id="B95D4A4AFB143E04FE142D1510C3FDE9" box="[594,722,513,536]" italics="true" pageId="30" pageNumber="481">spinipennis</emphasis>
</taxonomicName>
(
<taxonomicName id="4C29EDDBFB143E04FEA02EEB1196FDE9" authorityName="J. Thomson" authorityYear="1860" box="[742,903,511,536]" class="Insecta" family="Cerambycidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="481" phylum="Arthropoda" rank="tribe" tribe="Desmiphorini">Desmiphorini</taxonomicName>
), also from 
<collectingCountry id="F33ED6C8FB143E04F85A2EEB166BFDE9" box="[1052,1146,511,536]" name="Jamaica" pageId="30" pageNumber="481">Jamaica</collectingCountry>
. Subsequently, 
<bibRefCitation id="EFB8EBA9FB143E04F9722EEB12FEFDCC" author="Breuning" pageId="30" pageNumber="481" refString="Breuning, S. (1961 b) Revision des Pteropliini (Col., Cerambycidae). Pesquisas, Series 5, 9, 5 - 60." type="journal article" year="1961" yearSuffix="b">Breuning (1961b)</bibRefCitation>
