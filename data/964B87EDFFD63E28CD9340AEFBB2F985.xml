<number id="A82A24DCFFD63E28CEA54166FC71FA81" box="[929,989,1342,1367]" pageId="21" pageNumber="925" value="1925.0">1925</number>
a: 
<number id="A82A24DCFFD63E28CEFE4167FB89FA81" box="[1018,1061,1343,1367]" pageId="21" pageNumber="925" value="130.0">130</number>
)
</bibRefCitation>
