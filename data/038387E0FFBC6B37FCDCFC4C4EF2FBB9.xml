<taxonomicName id="4C2A4D75FFBF6B37FEA1FBD54BBBFB9A" box="[324,510,1051,1073]" class="Arachnida" family="Liphistiidae" genus="Ganthela" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="302" phylum="Arthropoda" rank="species" species="undetermined">
