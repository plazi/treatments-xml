<taxonomicName id="4C1E4AACFFB8A70DFF69F9E3C33E0537" authority="(Gauthier, 1900)" baseAuthorityName="Gauthier" baseAuthorityYear="1900" box="[255,571,1612,1629]" class="Echinoidea" family="Faujasiidae" genus="Petalobrissus" kingdom="Animalia" order="Clypeasteroida" pageId="28" pageNumber="603" phylum="Echinodermata" rank="species" species="waltheri">
