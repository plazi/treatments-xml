<taxonomicName id="4C364D1DFFE6FFCEAA8FFBCF153E440E" baseAuthorityName="Dreyfus" baseAuthorityYear="1889" box="[923,1035,1084,1110]" class="Insecta" family="Adelgidae" genus="Pineus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="orientalis">
