<emphasis id="9FA6E03CFFD6FFEBFF3EFBAFE810FB9E" bold="true" box="[151,267,1123,1149]" pageId="7" pageNumber="8">Remarks.</emphasis>
<taxonomicName id="6AD247ADFFD6FFEBFEBBFBAFEB07FB9F" authorityName="Stal" authorityYear="1870" box="[274,540,1123,1149]" class="Insecta" family="Pentatomidae" genus="Coryzorhaphis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="cruciata">
