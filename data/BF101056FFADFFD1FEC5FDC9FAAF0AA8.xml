), II 
<number id="8171B367FFADFFD2FB8CFA50FBE60E87" box="[1089,1133,1423,1449]" pageId="5" pageNumber="6" value="107.0">107</number>
(
<number id="8171B367FFADFFD2FBB2FA50FB7D0E87" box="[1151,1270,1423,1449]" pageId="5" pageNumber="6" value="112.0" valueMax="114.0" valueMin="110.0">110 ­ 114</number>
), III 
<number id="8171B367FFADFFD2FEC5FA68FEB80EFF" box="[264,307,1463,1489]" pageId="5" pageNumber="6" value="113.0">113</number>
(
<number id="8171B367FFADFFD2FE8FFA68FE380EFF" box="[322,435,1463,1489]" pageId="5" pageNumber="6" value="120.0" valueMax="123.0" valueMin="117.0">117 ­ 123</number>
