<emphasis id="05CD7D52FFADFFD2FC3AFC18FB9A08CE" box="[1015,1041,967,992]" italics="true" pageId="5" pageNumber="6">ve</emphasis>
<number id="8171B367FFADFFD2FBD5FC18FBBC08CF" box="[1048,1079,967,993]" pageId="5" pageNumber="6" value="13.0">13</number>
