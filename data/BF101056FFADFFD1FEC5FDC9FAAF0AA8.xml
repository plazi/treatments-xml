<number id="8171B367FFADFFD2FDB1F9D8FD280D0F" box="[636,675,1543,1569]" pageId="5" pageNumber="6" value="2.5" valueMax="3.0" valueMin="2.0">2­3</number>
­ 
<number id="8171B367FFADFFD2FD60F9D8FD350D0F" box="[685,702,1543,1569]" pageId="5" pageNumber="6" value="2.0">2</number>
