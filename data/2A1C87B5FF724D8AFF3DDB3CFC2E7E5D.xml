<paragraph id="A20A36A3FF724D89FF5FDA6CFA977E3E" blockId="228.[174,1368,1311,1337]" box="[174,1368,1311,1337]" pageId="228" pageNumber="229">
<emphasis id="90C1EAB1FF724D89FF5FDA6CFEF77E30" bold="true" box="[174,312,1311,1334]" pageId="228" pageNumber="229">ETYMOLOGY„</emphasis>
