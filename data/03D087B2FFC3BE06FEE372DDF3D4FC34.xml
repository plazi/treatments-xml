<collectionCode id="ED68AE61FFC3BE09FF0170F8F68DF94C" box="[242,420,1687,1711]" pageId="39" pageNumber="626">DISTRIBUTION</collectionCode>
. — The Coral Sea, 
<collectingCountry id="F36E7634FFC3BE09FD9770F7F420F94C" box="[612,777,1687,1711]" name="New Caledonia" pageId="39" pageNumber="626">New Caledonia</collectingCountry>
, 
<collectingCountry id="F36E7634FFC3BE09FCE070F7F447F94C" box="[787,878,1688,1711]" name="Vanuatu" pageId="39" pageNumber="626">Vanuatu</collectingCountry>
, 
<collectingCountry id="F36E7634FFC3BE09FC8B70F8F4B3F94C" box="[888,922,1687,1711]" name="Fiji" pageId="39" pageNumber="626">Fiji</collectingCountry>
and 
<collectingCountry id="F36E7634FFC3BE09FC3C70F7F339F953" box="[975,1040,1688,1712]" name="Tonga" pageId="39" pageNumber="626">Tonga</collectingCountry>
(Fig. 21), in 
<quantity id="4C819B41FFC3BE09FB6A70F8F232F94C" box="[1177,1307,1687,1711]" metricMagnitude="2" metricUnit="m" metricValue="8.834999999999999" metricValueMax="10.0" metricValueMin="7.67" pageId="39" pageNumber="626" unit="m" value="883.5" valueMax="1000.0" valueMin="767.0">767-1000 m</quantity>
