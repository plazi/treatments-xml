<collectionCode id="ED6C8CCBFFA22941186EDF6554F7FD8F" box="[617,708,615,639]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="8" pageNumber="339">MNHN</collectionCode>
