<geoCoordinate id="EE4972C9FFA229411984DF40523FFDAB" box="[899,1036,578,603]" direction="south" orientation="latitude" pageId="8" pageNumber="339" precision="1" value="-12.900333">12°54'1,2&quot;S</geoCoordinate>
