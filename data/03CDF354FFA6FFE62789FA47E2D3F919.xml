<taxonomicName id="4C6439C1FFA6FFE62789FA47E400FA65" authority="Ringdahl, 1937" authorityName="Ringdahl" authorityYear="1937" box="[151,561,1483,1510]" class="Insecta" family="Syrphidae" genus="Cheilosia" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="169" phylum="Arthropoda" rank="species" species="scanica">
