<number id="3DB624B1FF8D81418472FDC06C76FDDF" box="[339,356,561,587]" pageId="10" pageNumber="11" value="14.11" valueMax="24.0" valueMin="4.22">4</number>
</figureCitation>
, 
<figureCitation id="13452A13FF8D81418451FDC06CA8FDDF" box="[368,442,561,587]" captionStart="FIGURES 22 – 24" captionStartId="10.[264,383,1683,1707]" captionTargetBox="[478,1113,801,1682]" captionTargetId="figure@10.[473,1113,776,1686]" captionTargetPageId="10" captionText="FIGURES 22 – 24. Genitalia of Neochauliodes fuscus sp. nov. 22. Male genitalia, lateral view. 23. Aedeagus, ventral view. 24. Male tenth tergum, dorsal view." httpUri="https://zenodo.org/record/169887/files/figure.png" pageId="10" pageNumber="11">22–24</figureCitation>
