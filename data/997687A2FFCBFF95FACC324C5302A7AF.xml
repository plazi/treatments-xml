<date id="65611074FFCBFF9AF8943335525EA382" box="[719,889,1905,1931]" pageId="7" pageNumber="341" value="2019-09-11" valueMax="2019-09-17" valueMin="2019-09-11">
<collectingDate id="7525E99CFFCBFF9AF8943335525EA382" box="[719,889,1905,1931]" pageId="7" pageNumber="341" value="2019-09-11" valueMax="2019-09-17" valueMin="2019-09-11">11–17.IX.2019</collectingDate>
</date>
, 
<location id="1400606FFFCBFF9AF9DE333452F3A383" LSID="urn:lsid:plazi:treatment:997687A2FFCBFF95FACC324C5302A7AF:1400606FFFCBFF9AF9DE333452F3A383" box="[901,980,1904,1930]" country="Taiwan" latitude="23.7239" longLatPrecision="7" longitude="120.9145" name="Project" pageId="7" pageNumber="341" stateProvince="Nantou County">Project</location>
no. TFBC-1070517 (FSHc)
</materialsCitation>
