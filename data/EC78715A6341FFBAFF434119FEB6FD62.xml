pv 
<number id="D219D26B6341FFB9FA4544DDFA1AF91C" box="[1425,1436,1747,1772]" pageId="45" pageNumber="46" value="1.0">1</number>
</emphasis>
and 
<emphasis id="56A51C5E6341FFB9FF1C44F5FF74F8E4" box="[200,242,1787,1812]" italics="true" pageId="45" pageNumber="46">
pv 
