<quantity id="4CE99B44A470FFECEBBCF85DFCFB9CB7" box="[680,832,1970,1994]" metricMagnitude="2" metricUnit="m" metricValue="2.52" metricValueMax="2.7399999999999998" metricValueMin="2.3" pageId="30" pageNumber="31" unit="m" value="252.0" valueMax="274.0" valueMin="230.0">
230 to 
<specimenCount id="9D17FD28A470FFECEBE2F85DFCFB9CB7" box="[758,832,1970,1994]" pageId="30" pageNumber="31" type="generic">274 m</specimenCount>
</quantity>
