<taxonomicNameLabel id="A246721DFFF6FFDBFE7B54C530A7FB19" box="[397,450,1167,1188]" pageId="10" pageNumber="111" rank="species">sp. n.</taxonomicNameLabel>
, Ƌ 
<typeStatus id="54BAADD6FFF6FFDBFE1D54C5335BFB19" box="[491,574,1167,1188]" pageId="10" pageNumber="111">paratype</typeStatus>
