<taxonomicName id="4C0168F7FFF6FFDBFF7E54C430E3FB19" authority="V. &amp; Tanasevitch, 2013" authorityName="V. &amp; Tanasevitch" authorityYear="2013" box="[136,390,1166,1188]" class="Arachnida" family="Linyphiidae" genus="Improphantes" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="111" phylum="Arthropoda" rank="species" species="breviscapus" status="sp. n.">
<emphasis id="B975CF66FFF6FFDBFF7E54C430E3FB19" box="[136,390,1166,1188]" italics="true" pageId="10" pageNumber="111">Improphantes breviscapus</emphasis>
