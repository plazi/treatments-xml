<taxonomicName id="9CB45FECB735FF9E9C7EFBFAE7803A65" authorityName="Gressitt" authorityYear="1951" box="[868,1008,1073,1099]" class="Insecta" family="Cerambycidae" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="tribe" tribe="Rhodopinini">Rhodopinini</taxonomicName>
