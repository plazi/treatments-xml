<paragraph id="97AF4B50064C14CB4BF15D16C2139327" pageId="0" pageNumber="85">Type species.</paragraph>
<paragraph id="7D9C7CE774B128756A360C96AC1A35DA" pageId="0" pageNumber="85">
<taxonomicName id="401D249E53DCEAF7798C7C23103C40D5" class="Arachnida" family="Salticidae" genus="Papuamyr" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papuamyr omhifosga" order="Araneae" pageId="0" pageNumber="85" phylum="Arthropoda" rank="species" species="omhifosga">Papuamyr omhifosga</taxonomicName>
