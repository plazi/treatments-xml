<subSubSection id="1388C593FF8AFFCF4FFCF98E7F86F91E" pageId="17" pageNumber="218" type="diagnosis">
<paragraph id="5B2D9618FF8AFFCF4FFCF98E7F86F91E" blockId="17.[151,1437,258,1795]" pageId="17" pageNumber="218">
<emphasis id="69E64A0AFF8AFFCF4FFCF98E7C23F933" bold="true" box="[199,314,1662,1687]" pageId="17" pageNumber="218">Diagnosis</emphasis>
