<quantity id="EC2C9B673948A948FD8CE8005FD4FE9D" box="[631,698,339,361]" metricMagnitude="-3" metricUnit="m" metricValue="9.1" pageId="3" pageNumber="510" unit="mm" value="9.1">9.1mm</quantity>
