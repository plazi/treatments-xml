<quantity id="EC2C9B673948A948FE5EE8005C86FE9D" box="[421,488,339,361]" metricMagnitude="-3" metricUnit="m" metricValue="8.9" pageId="3" pageNumber="510" unit="mm" value="8.9">8.9mm</quantity>
