<paragraph id="8BF236B2FF94C90CE7F541FCFBDFFC57" blockId="50.[151,1054,946,972]" box="[151,1054,946,972]" pageId="50" pageNumber="51">
<emphasis id="B939EAA0FF94C90CE7F541FCFEFAFC57" bold="true" box="[151,315,946,972]" pageId="50" pageNumber="51">
<typeStatus id="54F68810FF94C90CE7F541FCFF15FC57" box="[151,212,946,972]" pageId="50" pageNumber="51">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C4D4D31FF94C90CE62141FCFCFDFC57" authority="Lin and Huber" authorityName="Lin and Huber" authorityYear="2007" box="[323,828,946,972]" class="Insecta" family="Mymaridae" genus="Kompsomymar" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="bicoloratum">
<emphasis id="B939EAA0FF94C90CE62141FCFD4BFC50" box="[323,650,946,971]" italics="true" pageId="50" pageNumber="51">Kompsomymar bicoloratum</emphasis>
Lin and Huber
</taxonomicName>
