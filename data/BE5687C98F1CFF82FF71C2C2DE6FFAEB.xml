<number id="803724F88F1BFF83FF71C1EADCEAFA2F" box="[151,175,1477,1499]" pageId="4" pageNumber="131" value="11.0">11</number>
th flagellomere), 
<emphasis id="048BEACD8F1BFF83FEB3C1E9DD23FA2F" bold="true" box="[341,358,1478,1499]" pageId="4" pageNumber="131">B</emphasis>
