<subSubSection id="D78A6579FFEB46483EADFF3DFAC0FF62" box="[199,1280,223,248]" pageId="20" pageNumber="221" type="etymology">
<paragraph id="9F2F36F2FFEB46483EADFF3DFAC0FF62" blockId="20.[151,1436,151,536]" box="[199,1280,223,248]" pageId="20" pageNumber="221">
<emphasis id="ADE4EAE0FFEB46483EADFF3DFE86FF62" bold="true" box="[199,326,223,248]" pageId="20" pageNumber="221">Etymology</emphasis>
