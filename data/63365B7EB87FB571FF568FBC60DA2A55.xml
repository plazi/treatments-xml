<collectingCountry id="9388AAF8B878B571FAEA89A6646929AD" name="Papua New Guinea" pageId="4">Papua New Guinea</collectingCountry>
)........................................................................... 
<taxonomicName id="2C9F91EBB878B571FB078984600829AD" box="[1222,1353,181,202]" class="Insecta" family="Dolichopodidae" genus="Phrudoneura" kingdom="Animalia" order="Diptera" pageId="4" phylum="Arthropoda" rank="species" species="popondetta" status="sp. nov.">
