<geoCoordinate id="EE6397BCFFE0E24CFA92FECCDCFB1E72" box="[1303,1436,354,377]" degrees="34.47806" direction="north" orientation="latitude" pageId="19" pageNumber="120" precision="1" value="34.47806">34.47806°N</geoCoordinate>
