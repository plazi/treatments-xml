<emphasis id="2AE1ED35875D5972FEDABD92FDACFC45" bold="true" box="[299,570,962,986]" pageId="51" pageNumber="310">
<taxonomicName id="DF954AA4875D5972FEDABD92FE7AFC45" authority="Cairns, 2004" authorityName="Cairns" authorityYear="2004" box="[299,492,962,986]" class="Anthozoa" family="Gardineriidae" genus="Stolarskicyathus" kingdom="Animalia" order="Scleractinia" pageId="51" pageNumber="310" phylum="Cnidaria" rank="genus" status="n.gen.">
<emphasis id="2AE1ED35875D5972FEDABD92FE7AFC45" bold="true" box="[299,492,962,986]" italics="true" pageId="51" pageNumber="310">Stolarskicyathus</emphasis>
