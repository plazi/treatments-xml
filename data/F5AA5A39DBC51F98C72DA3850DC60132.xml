<taxonomicName id="6CB2B07A4D5EB5FA6E9CBAD9F68068DC" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Inula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="525" phylum="Tracheophyta" rank="species" species="helenium">
