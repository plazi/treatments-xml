, 
<taxonomicName id="AD28EE7B3C3CCEB7978239C8CAB60578" authorityName="Dejean" authorityYear="1821" class="Hexapoda" family="Carabidae" genus="Ophonus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="684" pageNumber="685" phylum="Arthropoda" rank="genus">
