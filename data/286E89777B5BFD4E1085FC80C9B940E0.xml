<taxonomicName id="67C743E27B5BFD4E13DBF929C85541C1" box="[1018,1144,1707,1732]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Phlegmariurus" kingdom="Plantae" order="Lycopodiales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="acerosus">
