, 
<taxonomicName id="67C743E27B5BFD4E1377F905CFE641A5" box="[854,971,1670,1696]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Phlegmariurus" kingdom="Plantae" order="Lycopodiales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="pringlei">
