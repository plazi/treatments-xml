<taxonomicName id="67C743E27B5BFD4E1421F905C8B941A5" authorityName="B. Ollgaard" authorityYear="2012" baseAuthorityName="Kunze" box="[1024,1172,1670,1696]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Phlegmariurus" kingdom="Plantae" order="Lycopodiales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="curvifolius">
