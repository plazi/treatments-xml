<subSubSection id="E8DD6BEA7B5BFD4E109CF904C90D41ED" pageId="21" pageNumber="22" type="discussion">
<paragraph id="A07838617B5BFD4E109CF904C90D41ED" blockId="21.[136,1452,1130,2021]" pageId="21" pageNumber="22">
<emphasis id="92B3E4737B5BFD4E109CF904CD0B41A5" bold="true" box="[189,294,1670,1696]" pageId="21" pageNumber="22">Notes:—</emphasis>
<taxonomicName id="67C743E27B5BFD4E1107F904CE1641A5" authorityName="W. H. Wagner" authorityYear="1995" baseAuthorityName="Swartz" box="[294,571,1670,1696]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Phlegmariurus" kingdom="Plantae" order="Lycopodiales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="filiformis">
