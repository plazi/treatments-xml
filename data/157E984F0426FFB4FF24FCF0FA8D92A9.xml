<materialsCitation id="2DBF23040426FFB4FF19FACDFDCC90F4" ID-GBIF-Occurrence="2621717301" collectingDate="1922-04-01" collectionCode="AM, SPHTM" collectorName="F. Hendel" location="Townsville" municipality="Types" pageId="2" pageNumber="97" specimenCount="2" specimenCount-female="1" specimenCount-male="1" stateProvince="Queensland" typeStatus="holotype">
