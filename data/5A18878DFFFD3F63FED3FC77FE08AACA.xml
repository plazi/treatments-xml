<taxonomicName id="15B14D18FFFE3F62FE46F8B7FDDAAF17" box="[413,590,1836,1859]" class="Insecta" family="Anomalopsychidae" genus="Contulma" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="meloi" status="n. sp.">
<emphasis id="E0C5EA89FFFE3F62FE46F8B7FDDAAF17" box="[413,590,1836,1859]" italics="true" pageId="6" pageNumber="55">
