<collectingRegion id="49DBF84D2D72FFD677EDFBA1FE5BA418" box="[453,506,1087,1113]" country="Brazil" name="Para" pageId="15" pageNumber="64">Pará</collectingRegion>
, 
<collectingCountry id="F308763F2D72FFD6742DFBA1FDECA418" box="[517,589,1087,1113]" name="Brazil" pageId="15" pageNumber="64">Brazil</collectingCountry>
