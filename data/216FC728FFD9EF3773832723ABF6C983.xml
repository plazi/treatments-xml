<location id="AC1920E5FFD9EF3670C622A0AB5CCDDB" LSID="urn:lsid:plazi:treatment:216FC728FFD9EF3773832723ABF6C983:AC1920E5FFD9EF3670C622A0AB5CCDDB" box="[651,804,1167,1191]" country="Mozambique" county="Indian Ocean" municipality="Cahora Bassa" name="Indian Ocean" pageId="22" pageNumber="2689" stateProvince="Tete">Indian Ocean</location>
