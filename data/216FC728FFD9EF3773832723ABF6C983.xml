<location id="AC1920E5FFD9EF367048225FAA23CDF4" LSID="urn:lsid:plazi:treatment:216FC728FFD9EF3773832723ABF6C983:AC1920E5FFD9EF367048225FAA23CDF4" box="[517,603,1136,1160]" country="Mozambique" county="Indian Ocean" municipality="Cahora Bassa" name="Licuare" pageId="22" pageNumber="2689" stateProvince="Tete">Licuare</location>
