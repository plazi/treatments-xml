ii. 
<number id="3DBC2485FFF3FFEBFF3CF90CFE8DDDD1" box="[205,269,1721,1747]" pageId="3" pageNumber="52" value="1959.0">1959</number>
<number id="3DBC2485FFF3FFEBFEE4F90CFEA4DDD1" box="[277,292,1721,1747]" pageId="3" pageNumber="52" value="1.0">1</number>
