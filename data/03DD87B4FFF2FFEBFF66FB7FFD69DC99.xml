<collectionCode id="ED65AE67FFF3FFEBFDDAF90CFD00DDD1" LSID="urn:lsid:biocol.org:col:34697" box="[555,640,1721,1747]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="3" pageNumber="52">BPBM</collectionCode>
