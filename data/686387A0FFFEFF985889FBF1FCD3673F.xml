<taxonomicName id="27CA4D35FFFEFF985AFAFABDFCF26433" baseAuthorityName="Martin &amp; Papavero" baseAuthorityYear="1970" box="[740,833,1328,1354]" class="Insecta" family="Asilidae" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="family">Asilidae</taxonomicName>
