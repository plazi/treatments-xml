<location id="E515606DFFFEFF9858F3FB65FE82647A" LSID="urn:lsid:plazi:treatment:686387A0FFFEFF985889FBF1FCD3673F:E515606DFFFEFF9858F3FB65FE82647A" box="[237,305,1256,1283]" county="Napo River" municipality="America" name="South" pageId="51" pageNumber="52">South</location>
