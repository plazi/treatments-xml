<location id="E515606DFFFEFF985C4DFB48FB0D65A7" LSID="urn:lsid:plazi:treatment:686387A0FFFEFF985889FBF1FCD3673F:E515606DFFFEFF985C4DFB48FB0D65A7" box="[1107,1214,1221,1246]" county="Napo River" municipality="America" name="Rio Napo" pageId="51" pageNumber="52">Rio Napo</location>
