<taxonomicName id="18D54D3EFF80FB03FC7BFB2A4EA081B4" box="[926,1005,1164,1190]" class="Arachnida" family="Diptilomiopidae" genus="Trimeroptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="58" phylum="Arthropoda" rank="species" species="rubi">
