<taxonomicName id="18D54D3EFF80FB03FE17FB124F7281DC" box="[498,575,1204,1230]" class="Arachnida" family="Diptilomiopidae" genus="Trimeroptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="58" phylum="Arthropoda" rank="species" species="rubi">
