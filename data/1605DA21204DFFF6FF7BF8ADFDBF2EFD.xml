<taxonomicName id="59AC10B4204BFFF6FDB3FE54FD4D2D8D" box="[607,723,476,500]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="45" pageNumber="97" phylum="Tracheophyta" rank="genus">Epigynum</taxonomicName>
