<taxonomicName id="30991A41C2B9353CE09B3207768D247F" authority="Dodd, 1920" authorityName="Masner &amp; Miko" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Gryonoides" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gryonoides fuscoclavatus" order="Hymenoptera" pageId="0" pageNumber="523" phylum="Arthropoda" rank="species" species="fuscoclavatus">
