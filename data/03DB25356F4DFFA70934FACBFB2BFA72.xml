<paragraph id="8BCD94236F4DFFA00934FACBFC0EFAD9" blockId="51.[629,958,1291,1317]" box="[629,958,1291,1317]" pageId="51" pageNumber="52">
<heading id="D085234F6F4DFFA00934FACBFC0EFAD9" box="[629,958,1291,1317]" centered="true" fontSize="11" level="2" pageId="51" pageNumber="52" reason="2">
<taxonomicName id="4C72EFA06F4DFFA00934FACBFC0EFAD9" authorityName="Enderlein" authorityYear="1922" box="[629,958,1291,1317]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="speciesGroup" species="infestus" speciesGroup="infestus">
