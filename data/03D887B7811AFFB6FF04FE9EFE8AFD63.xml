<subSubSection id="C36B652A811AFFB6FF31FD9AFE8AFD63" pageId="53" pageNumber="54" type="distribution">
<paragraph id="8BCE36A1811AFFB6FF31FD9AFCE8FD96" blockId="53.[136,1452,518,689]" box="[189,859,554,581]" pageId="53" pageNumber="54">
<emphasis id="B905EAB3811AFFB6FF31FD9AFEC6FD96" bold="true" box="[189,373,554,580]" pageId="53" pageNumber="54">Distribution:—</emphasis>
Eastern 
<collectingCountry id="F3667631811AFFB6FE5CFD9BFD8CFD97" box="[464,575,555,581]" name="Myanmar" pageId="53" pageNumber="54">Myanmar</collectingCountry>
, 
