<taxonomicName id="00F375BA3E55A15E791DF876C75FF856" authority="(COCKERELL)" baseAuthorityName="COCKERELL" baseAuthorityYear="1936" box="[1133,1391,2019,2044]" class="Insecta" family="Colletidae" genus="Hylaeus" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="14" phylum="Arthropoda" rank="species" species="stictifrons">
