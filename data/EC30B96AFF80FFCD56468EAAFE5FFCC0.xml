<taxonomicName id="A39973FFFF80FFCD579A8D68FDCDFDEC" authority="(Franch.) H. J. P. Winkl., Das Pflanzenreich" authorityName="H. J. P. Winkl., Das Pflanzenreich" baseAuthorityName="Franch." class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="variety" species="turczaninovii" variety="polyneura">
