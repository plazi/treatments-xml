<taxonomicName id="1FEF4D6EC74AFFB4FF65FB8F38C8FB1D" box="[151,439,1147,1172]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="ignacio">
