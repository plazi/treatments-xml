<taxonomicName id="1FEF4D6EC74AFFB4FD33FB543A3AFB30" box="[705,837,1184,1209]" class="Insecta" family="Mutillidae" genus="Sphaeropthalma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="nokomis">S. nokomis</taxonomicName>
