<taxonomicName id="4C054D77FFA9FFDFFD64FA3A3A3EFA4F" authority="Mayr and Rand, 1935: 16" authorityName="Mayr and Rand" authorityPageNumber="16" authorityYear="1935" class="Aves" family="Zosteropidae" genus="Zosterops" kingdom="Animalia" order="Passeriformes" pageId="18" pageNumber="18" phylum="Chordata" rank="subSpecies" species="fuscicapilla" subSpecies="crookshanki">
