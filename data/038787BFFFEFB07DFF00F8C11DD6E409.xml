<subSubSection id="C3346522FFE2B07DFF50FCB11DD6E409" box="[199,1215,837,864]" pageId="8" pageNumber="324" type="etymology">
<paragraph id="8B9136A9FFE2B07DFF50FCB11DD6E409" blockId="8.[151,1437,152,864]" box="[199,1215,837,864]" pageId="8" pageNumber="324">
<emphasis id="B95AEABBFFE2B07DFF50FCB11827E436" bold="true" box="[199,334,837,863]" pageId="8" pageNumber="324">Etymology.</emphasis>
