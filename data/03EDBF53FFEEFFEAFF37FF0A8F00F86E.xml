<geoCoordinate id="EE706882FFEEFFEAFBDDFA9F8896FA9E" box="[1149,1220,1282,1308]" degrees="4" direction="south" minutes="21" orientation="latitude" pageId="22" pageNumber="23" precision="925" value="-4.35">S4 21</geoCoordinate>
