<geoCoordinate id="EE706882FFEEFFEAFC2FFA9F8F82FA9E" box="[911,976,1282,1308]" degrees="4" direction="south" minutes="23" orientation="latitude" pageId="22" pageNumber="23" precision="925" value="-4.383333">s4 23</geoCoordinate>
