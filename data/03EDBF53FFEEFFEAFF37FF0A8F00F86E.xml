<geoCoordinate id="EE706882FFEEFFEAFC7DFA9F884DFA9E" box="[989,1055,1282,1308]" degrees="6" direction="south" minutes="20" orientation="latitude" pageId="22" pageNumber="23" precision="925" value="-6.3333335">s6 20</geoCoordinate>
