<emphasis id="FED0EADAFFC20C6F5B5F519D09DC39F7" box="[1038,1205,467,491]" italics="true" pageId="24" pageNumber="317">
<collectorName id="6151531EFFC20C6F5B5F519D09E339F7" box="[1038,1162,467,491]" pageId="24" pageNumber="317">Paradis, G.</collectorName>
