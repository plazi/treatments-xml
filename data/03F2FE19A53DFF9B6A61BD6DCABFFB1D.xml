<heading id="D0ACF863A53DFF9A6A61BD6DC86BFCC4" centered="true" fontSize="9" level="2" pageId="40" pageNumber="139" reason="4">
<taxonomicName id="4C5B348CA53DFF9A6A61BD6DC835FCE4" authorityName="Thompson" authorityYear="2012" box="[287,615,824,845]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="40" pageNumber="124" phylum="Arthropoda" rank="species" species="distinctum">
<emphasis id="B92F931DA53DFF9A6A61BD6DC835FCE4" bold="true" box="[287,615,824,845]" italics="true" pageId="40" pageNumber="139">CITROGRAMMA DISTINCTUM</emphasis>
</taxonomicName>
THOMPSON 
<emphasis id="B92F931DA53DFF9A6AAABD0EC86BFCC4" bold="true" box="[468,569,856,877]" pageId="40" pageNumber="139">
<taxonomicNameLabel id="A21C2E66A53DFF9A6AAABD0EC86BFCC4" box="[468,569,856,877]" pageId="40" pageNumber="139" rank="species">SP. NOV.</taxonomicNameLabel>
