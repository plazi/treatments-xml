<taxonomicName id="4C2901BE04721521FB1949BEFAF4F845" authorityName="A. Adams" authorityYear="1854" box="[1082,1311,1943,1969]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="123" pageNumber="124" phylum="Mollusca" rank="species" species="nigrofusca">
