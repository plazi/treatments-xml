<paragraph id="E3013683FF8A7B6EFF25FC87FE1F4039" blockId="37.[151,1059,695,873]" box="[196,426,851,873]" pageId="37" pageNumber="928">
<quantity id="24469B66FF8A7B6EFF25FC87FEB64038" box="[196,259,851,872]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="37" pageNumber="928" unit="m" value="1000.0">1000m</quantity>
