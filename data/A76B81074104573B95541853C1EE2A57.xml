<quantity id="7FD2B8F689C5650C0AC6D7229E5766E0" metricMagnitude="-2" metricUnit="m" metricValue="2.25" metricValueMax="3.0" metricValueMin="1.5" unit="mm" value="22.5" valueMax="30.0" valueMin="15.0">15-30 mm</quantity>
