<taxonomicName id="EDA40EC0DA5B2357FF67FEA78530A67D" authority="Herrich-Schäffer, 1855" authorityName="Herrich-Schäffer" authorityYear="1855" box="[192,289,295,321]" class="Insecta" family="Hepialidae" genus="Aenetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="251" phylum="Arthropoda" rank="genus">
