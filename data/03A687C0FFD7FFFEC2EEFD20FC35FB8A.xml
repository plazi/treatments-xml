<quantity id="4CF79B33FFD7FFFEC190FC9CFB89FCB5" box="[1001,1055,837,857]" metricMagnitude="2" metricUnit="m" metricValue="6.15" pageId="56" pageNumber="211" unit="m" value="615.0">615m</quantity>
