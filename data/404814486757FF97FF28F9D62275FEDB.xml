<taxonomicName id="0FE1DEDD6753FF96FF78F96A27F1F933" authorityName="Karaman &amp; Horvatović" authorityYear="2018" box="[199,435,1757,1782]" class="Malacostraca" family="Trichoniscidae" genus="Cetinjella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="12" pageNumber="273" phylum="Arthropoda" rank="species" species="monasterii">
