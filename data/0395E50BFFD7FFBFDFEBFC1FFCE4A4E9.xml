<taxonomicName id="4C3C2F9EFFD7FFBFDFEBFC1FFE86A19C" box="[128,351,915,938]" class="Insecta" family="Miridae" genus="Slaterocoris" kingdom="Animalia" order="Hemiptera" pageId="93" pageNumber="93" phylum="Arthropoda" rank="species" species="apache">
<emphasis id="B948880FFFD7FFBFDFEBFC1FFE86A19C" bold="true" box="[128,351,915,938]" italics="true" pageId="93" pageNumber="93">Slaterocoris apache</emphasis>
