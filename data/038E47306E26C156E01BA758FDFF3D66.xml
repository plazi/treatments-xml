<subSubSection id="C33DA5AD6E26C151E743A784FB6D38AB" box="[839,1265,1196,1220]" pageId="18" pageNumber="31" type="type_taxon">
<paragraph id="8B98F6266E26C151E743A784FB6D38AB" blockId="18.[839,1265,1196,1220]" box="[839,1265,1196,1220]" pageId="18" pageNumber="31">
