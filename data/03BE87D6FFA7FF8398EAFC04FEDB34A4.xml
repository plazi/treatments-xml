<geoCoordinate id="EE235007FFA7FF8399E0FB91FBBE3478" box="[413,511,1123,1149]" degrees="77" direction="north" minutes="41" orientation="latitude" pageId="54" pageNumber="55" precision="925" value="77.683334">77¬∞41‚ÄôN</geoCoordinate>
