<emphasis id="3BA2C852AF69FFE3FC2AFC8FFAA1FCA5" bold="true" box="[911,1343,768,793]" pageId="5" pageNumber="267">
Genus 
<taxonomicName id="CED66FC3AF69FFE3FC40FC8EFAA1FCA5" authority="Berlese, 1904" authorityName="Berlese" authorityYear="1904" box="[997,1343,768,793]" class="Arachnida" family="Veigaiidae" genus="Gamasolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="267" phylum="Arthropoda" rank="genus">
<emphasis id="3BA2C852AF69FFE3FC40FC8EFB00FCA4" bold="true" box="[997,1182,768,792]" italics="true" pageId="5" pageNumber="267">Gamasolaelaps</emphasis>
