<quantity id="17289B17FFAAA656FDC54AA4FD23FA5E" box="[637,739,1421,1445]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="4" pageNumber="335" unit="mm" value="3.3">3.30 mm</quantity>
