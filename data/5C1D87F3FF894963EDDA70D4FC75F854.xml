<taxonomicName id="13B44D66FF964963EF5D7421FC1FF8B2" authority="Michaelsen, 1902" authorityName="Michaelsen" authorityYear="1902" box="[528,917,1823,1848]" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="16" pageNumber="79" phylum="Annelida" rank="species" species="dunckeri">
<emphasis id="E6C0EAF7FF964963EF5D7421FD2AF8B2" box="[528,672,1824,1848]" italics="true" pageId="16" pageNumber="79">M. dunckeri</emphasis>
(
<bibRefCitation id="B0254B14FF964963EFF5741FFC04F8B2" author="Michaelsen" box="[696,910,1823,1848]" pageId="16" pageNumber="79" refString="Michaelsen, W. (1902) Neue Oligochaeten und neue Fundorte alt-bekannter. Jahrbuch der Hamburgischen Wissenschaftlichen Anstalten, 19, 1 - 54." type="journal article" year="1902">
Michaelsen, 
<number id="627C24C2FF964963EE1F7420FC04F8B2" box="[850,910,1824,1848]" pageId="16" pageNumber="79" value="1902.0">1902</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="13B44D66FF964963EEE77421FB75F8B2" authority="Ohfuchi, 1956" authorityName="Ohfuchi" authorityYear="1956" box="[938,1279,1823,1848]" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="16" pageNumber="79" phylum="Annelida" rank="species" species="parvula">
