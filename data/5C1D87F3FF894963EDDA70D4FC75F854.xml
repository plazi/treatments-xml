(
<bibRefCitation id="B0254B14FF964963EEFE7441FBC8F8D0" author="Gates" box="[947,1090,1857,1882]" pageId="16" pageNumber="79" refString="Gates, G. E. (1926) Note on a new species of Pheretima from Rangoon. Annals and Magazine of Natural History, Series 9, 17, 411 - 415. http: // dx. doi. org / 10.1080 / 00222932608633432" type="journal article" year="1926">
