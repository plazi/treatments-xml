(
<bibRefCitation id="B0254B14FF964963E90B741FFB7DF8B2" author="Ohfuchi" box="[1094,1271,1823,1848]" pageId="16" pageNumber="79" refString="Ohfuchi, S. (1956) On a collection of the terrestrial Oligochaeta obtained from the various localities in Riu-kiu Islands, together with the consideration their geographical distribution (Part I). Journal of Agricultural Science, (Tokyo Nagyo Daigaku), 3, 131 - 176." type="journal article" year="1956">
Ohfuchi, 
<number id="627C24C2FF964963E9F6741FFB7DF8B2" box="[1211,1271,1823,1848]" pageId="16" pageNumber="79" value="1956.0">1956</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="13B44D66FF964963E85E7421FEFDF8D3" authority="Michaelsen, 1922" authorityName="Michaelsen" authorityYear="1922" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="16" pageNumber="79" phylum="Annelida" rank="species" species="sintangi">
<emphasis id="E6C0EAF7FF964963E85E7421FA11F8BD" box="[1299,1435,1824,1848]" italics="true" pageId="16" pageNumber="79">M. sintangi</emphasis>
(
<bibRefCitation id="B0254B14FF964963EDD37441FEFAF8D3" author="Michaelsen" box="[158,368,1857,1882]" pageId="16" pageNumber="79" refString="Michaelsen, W. (1922) Oligochaten aus dem Rijks Museum van Natuurlijke Historie zu Leiden. Capita Zoologica, 1, 1 - 68." type="journal article" year="1922">
Michaelsen, 
<number id="627C24C2FF964963EC797441FEFAF8D3" box="[308,368,1857,1881]" pageId="16" pageNumber="79" value="1922.0">1922</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="13B44D66FF964963ECCA7442FC9BF8D3" authority="Michaelsen, 1891" authorityName="Michaelsen" authorityYear="1891" box="[391,785,1857,1882]" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="16" pageNumber="79" phylum="Annelida" rank="species" species="ferdinandi">
<emphasis id="E6C0EAF7FF964963ECCA7442FDAFF8D3" box="[391,549,1857,1881]" italics="true" pageId="16" pageNumber="79">M. ferdinandi</emphasis>
(
<bibRefCitation id="B0254B14FF964963EF757441FC82F8D3" author="Michaelsen" box="[568,776,1857,1882]" pageId="16" pageNumber="79" refString="Michaelsen, W. (1891) Oligochaeten des Naturhistorischen Museum in Hamburg, IV. Jahrbuch der Hamburgischen Wissenschaftlichen Anstalten, 8, 1 - 42." type="journal article" year="1891">
Michaelsen, 
<number id="627C24C2FF964963EF807441FC82F8D3" box="[717,776,1857,1881]" pageId="16" pageNumber="79" value="1891.0">1891</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="13B44D66FF964963EE6D7442FBC0F8D0" authority="Gates, 1926" authorityName="Gates" authorityYear="1926" box="[800,1098,1857,1882]" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="16" pageNumber="79" phylum="Annelida" rank="species" species="planata">
