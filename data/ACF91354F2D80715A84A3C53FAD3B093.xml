<taxonomicName id="621638A6F99F72CBCF4A566993FDFD10" authority="von Marenzeller, 1904 a" authorityName="von Marenzeller" authorityYear="1904" class="Anthozoa" family="Caryophylliidae" genus="Aulocyathus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aulocyathus" order="Scleractinia" pageId="0" pageNumber="1" phylum="Cnidaria" rank="genus">Aulocyathus von Marenzeller, 1904a</taxonomicName>
