<taxonomicName id="E8F9BCCDEA71FF8BC40C3E7D3E3FF9C9" authorityName="S. Julia" authorityYear="2013" box="[304,513,1580,1603]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="umbratica">
