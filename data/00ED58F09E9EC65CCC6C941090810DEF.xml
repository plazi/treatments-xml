<taxonomicName id="D11AB8107733FE66EF39318B835ECAB3" class="Insecta" family="Cossidae" genus="Azygophleps" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Azygophleps" order="Lepidoptera" pageId="8" pageNumber="53" phylum="Arthropoda" rank="genus">Azygophleps</taxonomicName>
