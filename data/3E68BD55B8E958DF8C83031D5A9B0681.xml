<taxonomicName id="000153B459CFFD326C3DA3560506449C" class="Dicotyledoneae" family="Fagaceae" genus="Quercus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Quercus incana" order="Fagales" pageId="357" pageNumber="358" phylum="Angiospermae" rank="species" species="incana">
