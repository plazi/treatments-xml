</taxonomicName>
managed plot, 
<taxonomicName id="EE5F5F5B69BCBE0B1A792A8561353E43" class="Pinopsida" family="Cupressaceae" genus="Juniperus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Juniperus" order="Pinales" pageId="357" pageNumber="358" phylum="Coniferophyta" rank="genus">
