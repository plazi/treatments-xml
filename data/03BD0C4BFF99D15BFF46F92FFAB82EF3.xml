<emphasis id="B960614FFF99D15AFF46F8F4FEF32A6F" bold="true" box="[151,291,1810,1834]" pageId="32" pageNumber="33">
<typeStatus id="54AF03FFFF99D15AFF46F8F4FF1F2A6F" box="[151,207,1810,1834]" pageId="32" pageNumber="33">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="4C14C6DEFF99D15AFEE3F8F4FE3D2A6C" box="[306,493,1810,1833]" class="Insecta" family="Cicadellidae" genus="Theronus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="priapus" status="sp.n.">
<emphasis id="B960614FFF99D15AFEE3F8F4FE3D2A6C" box="[306,493,1810,1833]" italics="true" pageId="32" pageNumber="33">Theronus priapus</emphasis>
