<collectionCode id="ED2EAE1CFFD4AB76FDCC95FDFD11DAAE" box="[581,662,964,985]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="20" pageNumber="605" type="Museum">USNM</collectionCode>
