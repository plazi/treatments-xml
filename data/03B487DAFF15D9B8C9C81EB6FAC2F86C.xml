<taxonomicName id="4C1D4D4FFF15D9B8C9C81EB6FE0BFA39" authority="(Leach, 1814)" baseAuthorityName="Leach" baseAuthorityYear="1814" box="[151,511,1446,1473]" class="Malacostraca" family="Ischyroceridae" genus="Jassa" kingdom="Animalia" order="Amphipoda" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="variegatus">
