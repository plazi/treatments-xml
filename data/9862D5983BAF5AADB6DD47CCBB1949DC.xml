<taxonomicName id="88D4E59063F5D4E649E24B00CF68266B" class="Insecta" family="Braconidae" genus="Chelonus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chelonus" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Chelonus</taxonomicName>
