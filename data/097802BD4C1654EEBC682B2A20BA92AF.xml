<taxonomicName id="6533C5A852FF7B8B65DE82F6180FE6DD" baseAuthorityName="Brusina" baseAuthorityYear="1903" class="Gastropoda" family="Melanopsidae" genus="Microcolpia" kingdom="Animalia" lsidName="Microcolpia parreyssii subsp. sikorai" order="Sorbeoconcha" pageId="276" pageNumber="277" phylum="Mollusca" rank="subSpecies" species="parreyssii" subSpecies="sikorai">
<emphasis id="3C03CECFF1FF84984C93FC013F63B5A3" italics="true" pageId="276" pageNumber="277">Microcolpia parreyssii sikorai</emphasis>
