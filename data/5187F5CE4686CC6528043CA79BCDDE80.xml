<taxonomicName id="1B20D15A306B3A425EE86F14F793907D" class="Insecta" family="Asilidae" genus="Gibbasilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gibbasilus crinitus" order="Diptera" pageId="5" pageNumber="72" phylum="Arthropoda" rank="species" species="crinitus">Gibbasilus crinitus</taxonomicName>
