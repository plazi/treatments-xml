<taxonomicName id="19F34D1E93096220FB67FE73FAC4FD8C" authorityName="Osten-Sacken" authorityYear="1877" box="[1218,1357,511,536]" class="Insecta" family="Bombyliidae" genus="Oncodocera" kingdom="Animalia" order="Diptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus">
