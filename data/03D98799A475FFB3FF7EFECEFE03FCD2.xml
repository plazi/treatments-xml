<taxonomicName id="4C704D0CA475FFB3FF7EFCD5FEA5FCB2" box="[136,308,780,804]" class="Magnoliopsida" family="Rubiaceae" genus="Eumachia" kingdom="Plantae" order="Gentianales" pageId="20" pageNumber="309" phylum="Tracheophyta" rank="species" species="carnea">
