<subSubSection id="C301653F4271FFB8FEA8FBC1FCBDFB6D" pageId="37" pageNumber="541" type="materials_examined">
<paragraph id="8BA436B44271FFB8FEA8FBC1FE3DFBC1" blockId="37.[313,1269,1075,1255]" box="[313,411,1075,1099]" pageId="37" pageNumber="541">
<emphasis id="B96FEAA64271FFB8FEA8FBC1FE3DFBC1" box="[313,411,1075,1099]" italics="true" pageId="37" pageNumber="541">Material</emphasis>
