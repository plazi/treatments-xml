7. 
<taxonomicName id="29F44D3B7C086342B9909976797AFB32" authority="Spring" authorityName="Spring" authorityYear="1843" box="[993,1303,1139,1166]" class="Lycopodiopsida" family="Selaginellaceae" genus="Selaginella" kingdom="Plantae" order="Selaginellales" pageId="2" pageNumber="11" phylum="Tracheophyta" rank="species" species="epirrhizos">
<emphasis id="DC80EAAA7C086342B990997678A5FB32" bold="true" box="[993,1224,1139,1166]" italics="true" pageId="2" pageNumber="11">Selaginella epirrhizos</emphasis>
Spring
</taxonomicName>
