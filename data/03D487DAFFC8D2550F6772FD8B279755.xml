, 
<date id="FFC3100CFFC8D2570C4973168B9B9700" box="[550,661,839,865]" pageId="61" pageNumber="62" value="1993-04-03">
<number id="3DB524EBFFC8D2570C4973168B3A9700" box="[550,564,839,865]" pageId="61" pageNumber="62" value="3.0">3</number>
