<subSubSection id="50DF655EFFA8675FFF4D4332EA3F670D" box="[189,1068,1577,1601]" pageId="72" pageNumber="339" type="etymology">
<paragraph id="187A36D5FFA8675FFF4D4332EA3F670D" blockId="72.[189,1068,1577,1601]" box="[189,1068,1577,1601]" pageId="72" pageNumber="339">
<emphasis id="2AB1EAC7FFA8675FFF4D4332EF24670D" bold="true" box="[189,311,1577,1601]" pageId="72" pageNumber="339">Etymology:</emphasis>
Vrom the Latin 
<emphasis id="2AB1EAC7FFA8675FFE284332EFEE670D" box="[472,509,1577,1601]" italics="true" pageId="72" pageNumber="339">rufi</emphasis>
