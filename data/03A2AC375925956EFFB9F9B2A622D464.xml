<taxonomicName id="4C0B66A25925956EFC3FFF49A617D438" authority="L. However" authorityName="L. However" class="Magnoliopsida" family="Apocynaceae" genus="Nerium" kingdom="Plantae" order="Gentianales" pageId="9" pageNumber="300" phylum="Tracheophyta" rank="species" species="scandens">
