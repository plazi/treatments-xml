<taxonomicName id="4C074D71E66EF606FF66FD7F59C30393" authority="Karny, 1925a: 10" authorityName="Karny" authorityPageNumber="10" authorityYear="1925" box="[151,479,709,732]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="palmi">
