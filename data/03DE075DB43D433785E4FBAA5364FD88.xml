<taxonomicName id="4C77CDC8B43F4330877AF7CA504DF7C8" authority="Clarkson, Santos" authorityName="Clarkson, Santos" box="[266,587,2074,2099]" class="Insecta" family="Hydrophilidae" genus="Helobata" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="amazonensis">
