51. 
<taxonomicName id="4C4A9AB999248556E8D1F95BD71EF6D1" authority="Haliday, 1838" authorityName="Haliday" authorityYear="1838" box="[193,591,1700,1727]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="sabinus">
