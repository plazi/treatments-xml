<quantity id="3C6B9B090303FF81FC59FE7DFB4014DB" box="[993,1164,461,487]" metricMagnitude="3" metricUnit="m" metricValue="5.175" metricValueMax="5.25" metricValueMin="5.1" pageId="45" pageNumber="46" unit="m" value="5175.0" valueMax="5250.0" valueMin="5100.0">5100–5250 m</quantity>
