<quantity id="89339B673332FF8BFB6442908A6EF8E3" box="[1201,1288,1769,1795]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" pageId="8" pageNumber="51" unit="mm" value="30.0">30 mm</quantity>
