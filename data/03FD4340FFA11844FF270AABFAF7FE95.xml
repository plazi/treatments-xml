<quantity id="4CAC5FB3FFA61844FCC90F8CFC6EFF00" box="[889,990,186,213]" metricMagnitude="0" metricUnit="m" metricValue="3.8" pageId="28" pageNumber="29" unit="m" value="3.8">3,800 m</quantity>
