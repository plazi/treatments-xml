<taxonomicName id="4C674D72B32C927B30DAFBF4F3F7FBA3" authority="Bigot, 1887: 45" authorityName="Bigot" authorityPageNumber="45" authorityYear="1887" box="[151,489,1050,1072]" class="Insecta" family="Conopidae" genus="Conops" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="528" phylum="Arthropoda" rank="species" species="punctum">
<emphasis id="B913EAE3B32C927B30DAFBF4F35CFBA3" box="[151,322,1051,1072]" italics="true" pageId="7" pageNumber="528">Conops punctum</emphasis>
<treatmentCitation id="0AC610E0B32C927B3105FBF5F3F7FBA3" author="Bigot" box="[328,489,1050,1072]" page="45" pageId="7" pageNumber="528" year="1887">
