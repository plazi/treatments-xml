<taxonomicName id="97390F77070CB9748B1CFECC423FAF4E" class="Insecta" family="Therevidae" genus="Salentia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Salentia" order="Diptera" pageId="3" pageNumber="38" phylum="Arthropoda" rank="genus">Salentia</taxonomicName>
and 
<taxonomicName id="A031E2E4531DB42787DAC9BC97D63313" class="Insecta" family="Therevidae" genus="Neotabuda" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neotabuda" order="Diptera" pageId="3" pageNumber="38" phylum="Arthropoda" rank="genus">Neotabuda</taxonomicName>
, but not with 
<taxonomicName id="6BA18B597EBD789D204132B36781168E" class="Insecta" family="Therevidae" genus="Phycus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phycus" order="Diptera" pageId="3" pageNumber="38" phylum="Arthropoda" rank="genus">Phycus</taxonomicName>
.
