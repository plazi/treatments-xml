<taxonomicName id="98BC10F63A5E60AF953317BF34EA2AF6" authority="Winterton, Hauser &amp; Badrawy, 2012" authorityName="Winterton, Hauser &amp; Badrawy" authorityYear="2012" class="Insecta" family="Therevidae" genus="Salwaea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Salwaea" order="Diptera" pageId="3" pageNumber="38" phylum="Arthropoda" rank="genus">Salwaea</taxonomicName>
