<taxonomicName id="27CA4D35FEF3FE955A65FEE3FCEA60F1" authorityName="Rondani" authorityYear="1862" box="[635,857,366,392]" class="Insecta" family="Tachinidae" genus="Gastrolepta" kingdom="Animalia" order="Diptera" pageId="318" pageNumber="319" phylum="Arthropoda" rank="species" species="gentilis">
