<subSubSection id="33BCA4C0FFE7FFA2BEF67439FE0FFB41" pageId="7" pageNumber="356" type="reference_group">
<paragraph id="7B19F74BFFE7FFA2BEF67439FDD4FBD1" blockId="7.[151,546,1045,1069]" box="[151,546,1045,1069]" pageId="7" pageNumber="356">
<treatmentCitationGroup id="5BB6D065FFE7FFA2BEF67439FDD4FBD1" box="[151,546,1045,1069]" pageId="7" pageNumber="356">
<treatmentCitation id="FA07D15AFFE7FFA2BEF67439FDE8FBD1" author="Karny, H." box="[151,542,1045,1069]" page="156" pageId="7" pageNumber="356" year="1912">
<taxonomicName id="BCA68CC8FFE7FFA2BEF67439FDE8FBD1" authority="Karny, 1912: 156" authorityName="Karny" authorityPageNumber="156" authorityYear="1912" box="[151,542,1045,1069]" class="Insecta" family="Phlaeothripidae" genus="Liothrips" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="356" phylum="Arthropoda" rank="species" species="brevitubus">
<emphasis id="49D22B59FFE7FFA2BEF67439FE92FBD1" box="[151,356,1045,1069]" italics="true" pageId="7" pageNumber="356">Liothrips brevitubus</emphasis>
<bibRefCitation id="1F378ABAFFE7FFA2BF0A7439FDE8FBD1" author="Karny, H." box="[363,542,1045,1069]" pageId="7" pageNumber="356" pagination="115 - 169" refId="ref14399" refString="Karny, H. (1912) Gallenbewohnende Thysanopteren aus Java. Marcellia, 11, 115 - 169." type="journal article" year="1912">Karny, 1912: 156</bibRefCitation>
