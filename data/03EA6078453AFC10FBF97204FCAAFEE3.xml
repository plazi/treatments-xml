<location id="8E9C87B5453AFC17FAC472B1FC75F8C8" LSID="urn:lsid:plazi:treatment:03EA6078453AFC10FBF97204FCAAFEE3:8E9C87B5453AFC17FAC472B1FC75F8C8" county="Jackson Corner" municipality="This" name="Standing Stone Creek" pageId="3" pageNumber="172" stateProvince="Pennsylvania">Standing Stone Creek</location>
