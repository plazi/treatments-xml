<location id="8E9C87B5453AFC17FAD57295FACBF8C8" LSID="urn:lsid:plazi:treatment:03EA6078453AFC10FBF97204FCAAFEE3:8E9C87B5453AFC17FAD57295FACBF8C8" box="[1320,1389,1788,1812]" county="Jackson Corner" municipality="This" name="Ridge" pageId="3" pageNumber="172" stateProvince="Pennsylvania">Ridge</location>
and 
<location id="8E9C87B5453AFC17FCBE7376FB0BF8EB" LSID="urn:lsid:plazi:treatment:03EA6078453AFC10FBF97204FCAAFEE3:8E9C87B5453AFC17FCBE7376FB0BF8EB" box="[835,1197,1823,1847]" county="Jackson Corner" municipality="This" name="Valley Physiographic Province" pageId="3" pageNumber="172" stateProvince="Pennsylvania">Valley Physiographic Province</location>
, fills in a gap between its known range of 
<collectingRegion id="49871F8C453AFC17FBCC732AFA9EF887" box="[1073,1336,1859,1883]" isEnumeration="true" pageId="3" pageNumber="172">Georgia and Alabama</collectingRegion>
