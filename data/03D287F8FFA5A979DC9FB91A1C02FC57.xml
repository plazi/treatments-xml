<taxonomicName id="4C7B4D6DFFA5A979DEA6BA011B54FD76" box="[686,829,619,642]" class="Arachnida" family="Phytoseiidae" genus="Proprioseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
