<taxonomicName id="4C7B4D6DFFA5A979D87FBA011D36FD77" authority="Berlese" authorityName="Berlese" box="[1143,1375,618,643]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
