<taxonomicName id="4C7B4D6DFFA5A979DD32BA011981FD77" box="[314,488,619,643]" class="Arachnida" family="Phytoseiidae" genus="Paraphytoseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
