</taxonomicName>
and 
<taxonomicName id="4C7B4D6DFFA5A979DC9FBAE5192EFD52" box="[151,327,655,678]" class="Arachnida" family="Phytoseiidae" genus="Proprioseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
<emphasis id="B90FEAFCFFA5A979DC9FBAE5192EFD52" box="[151,327,655,678]" italics="true" pageId="6" pageNumber="299">Proprioseiopsis</emphasis>
</taxonomicName>
