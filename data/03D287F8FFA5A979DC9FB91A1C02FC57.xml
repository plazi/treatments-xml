<taxonomicName id="4C7B4D6DFFA5A979DC9FBA011977FD76" box="[151,286,619,642]" class="Arachnida" family="Phytoseiidae" genus="Maunaseius" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="299" phylum="Arthropoda" rank="genus">
