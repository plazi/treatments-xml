<taxonomicNameLabel id="99EB8C76FB77FF9FFC24F74BFC76FBF2" box="[929,987,1040,1066]" pageId="21" pageNumber="712" rank="species">n. sp.</taxonomicNameLabel>
– 
<taxonomicName id="77AC969CFB77FF9FFC73F74BFBD7FBF1" box="[1014,1146,1039,1065]" class="Reptilia" family="Testudinidae" genus="Testudo" kingdom="Animalia" order="Testudines" pageId="21" pageNumber="712" phylum="Chordata" rank="species" species="kenitrensis">
