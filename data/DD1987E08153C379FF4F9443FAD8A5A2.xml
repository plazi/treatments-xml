<taxonomicName id="92B04D758153C37AFF4F9467FD80A32C" authority="Priesner, 1935: 313" authorityName="Priesner" authorityPageNumber="313" authorityYear="1936" box="[151,581,1060,1081]" class="Insecta" family="Thripidae" genus="Anaphothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="0" pageNumber="597" phylum="Arthropoda" rank="species" species="graminum">
