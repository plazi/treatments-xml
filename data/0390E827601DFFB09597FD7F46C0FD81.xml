<taxonomicName id="4C3922B2601AFFB09432FEF946A2FE35" authority="Huber" authorityName="Huber" authorityYear="2015" box="[811,1003,375,402]" class="Arachnida" family="Pholcidae" genus="Aetana" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="poring" status="sp. nov.">
