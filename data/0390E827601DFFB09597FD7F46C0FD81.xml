<taxonomicName id="4C3922B2601AFFB09607FE1A449DFE11" authority="Huber" authorityName="Huber" authorityYear="2015" box="[286,468,411,437]" class="Arachnida" family="Pholcidae" genus="Aetana" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="indah" status="sp. nov.">
