<taxonomicName id="78A0BAA1F87CFFB1FF6E3F65FDBAFE78" authority="Clastrier, 1962" authorityName="Clastrier" authorityYear="1962" box="[189,572,379,405]" class="Insecta" family="Ceratopogonidae" genus="Bezzia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="campanai">
