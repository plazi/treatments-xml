<quantity id="07599B69CF7DFFCAFB88B8CEFBE97ABF" box="[1086,1129,964,988]" metricMagnitude="-1" metricUnit="m" metricValue="1.016" pageId="12" pageNumber="13" unit="in" value="4.0">4 in</quantity>
