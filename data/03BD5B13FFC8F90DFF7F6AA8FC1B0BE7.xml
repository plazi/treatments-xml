<geoCoordinate id="EE208CC2FFCAF90DFCD46A80FCC10B88" box="[793,839,1412,1432]" degrees="39" direction="north" orientation="latitude" pageId="2" pageNumber="1447" precision="55555" value="39.0">39°N</geoCoordinate>
