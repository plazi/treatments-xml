<geoCoordinate id="E3254A1E35533D00E00EC849E7D96E84" degrees="24" direction="north" minutes="35" orientation="latitude" precision="925" value="24.583334">24°35'N</geoCoordinate>
, 
<geoCoordinate id="FFC1F45FDBAC9210E2F1FBC7AF130027" degrees="98" direction="east" minutes="41" orientation="longitude" precision="925" value="98.683334">98°41'E</geoCoordinate>
