<taxonomicName id="77E44D584138FFDBFCBA665799F03A79" box="[827,981,770,795]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="5" pageNumber="58" phylum="Tracheophyta" rank="species" species="anomala">
