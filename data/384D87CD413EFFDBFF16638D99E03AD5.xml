<taxonomicName id="77E44D584138FFDBFC6266579EAE3A7E" box="[995,1163,770,796]" class="Magnoliopsida" family="Aquifoliaceae" kingdom="Plantae" order="Aquifoliales" pageId="5" pageNumber="58" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
