<taxonomicName id="84E7EBEE2E12FFE9608AFAC9FB56FA79" authority="Walter, 1928" authorityName="Walter" authorityYear="1928" box="[921,1211,1405,1431]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="minutus">
<emphasis id="71934C7F2E12FFE9608AFAC9FBF4FA79" box="[921,1049,1406,1431]" italics="true" pageId="6" pageNumber="49">A. minutus</emphasis>
Walter, 
<number id="F52F824A2E12FFE9676EFACAFB56FA79" box="[1149,1211,1405,1431]" pageId="6" pageNumber="49" value="1928.0">1928</number>
</taxonomicName>
, 
<taxonomicName id="84E7EBEE2E12FFE967DBFAC9FF38FA53" authority="Cook, 1967" authorityName="Cook" authorityYear="1967" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="diversus">
<emphasis id="71934C7F2E12FFE967DBFAC9FAA0FA79" box="[1224,1357,1406,1431]" italics="true" pageId="6" pageNumber="49">A. diversus</emphasis>
<bibRefCitation id="2776ED9C2E12FFE96647FACAFF38FA53" author="Cook" pageId="6" pageNumber="49" refString="Cook, D. R. (1967) Water mites from India. Memoirs of the American Entomological Institute, 9, 1 - 411." type="journal article" year="1967">
Cook, 
<number id="F52F824A2E12FFE96384FA14FF38FA53" box="[151,213,1443,1469]" pageId="6" pageNumber="49" value="1967.0">1967</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="84E7EBEE2E12FFE963F1FA14FD78FA53" authority="Pesic &amp; Panesar, 2009" authorityName="Pesic &amp; Panesar" authorityYear="2009" box="[226,661,1441,1469]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="davecooki">
<emphasis id="71934C7F2E12FFE963F1FA14FE91FA52" box="[226,380,1443,1468]" italics="true" pageId="6" pageNumber="49">A. davecooki</emphasis>
<bibRefCitation id="2776ED9C2E12FFE96297FA16FD78FA53" author="Pesic" box="[388,661,1441,1469]" pageId="6" pageNumber="49" refString="Pesic, V. &amp; Vafaei, R. (2009) New records of water mites (Acari: Hydrachnidia) from Iran, with the first descriptions of the male of Nilotonia persica Pesic &amp; Saboori, 2006 and Atractides mirkopesici Pesic, 2004. Systematic and Applied Acarology, 14, 153 - 160." type="journal article" year="2009">
Pešić &amp; Panesar, 
<number id="F52F824A2E12FFE96145FA14FD78FA53" box="[598,661,1443,1469]" pageId="6" pageNumber="49" value="2009.0">2009</number>
</bibRefCitation>
</taxonomicName>
