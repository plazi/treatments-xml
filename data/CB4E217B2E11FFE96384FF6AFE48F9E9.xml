<taxonomicName id="84E7EBEE2E12FFE960D8FA14FAC0FA53" authority="K. Viets, 1922" authorityName="K. Viets" authorityYear="1922" box="[971,1325,1443,1469]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="polyporus">
<emphasis id="71934C7F2E12FFE960D8FA14FB89FA52" box="[971,1124,1443,1468]" italics="true" pageId="6" pageNumber="49">A. polyporus</emphasis>
(K. Viets, 
<number id="F52F824A2E12FFE967FBFA14FAC8FA53" box="[1256,1317,1443,1469]" pageId="6" pageNumber="49" value="1922.0">1922</number>
