<collectionCode id="C0CBAE32EF00FFBDFDE5FF35FDA9FF21" box="[516,581,152,176]" httpUri="http://grbio.org/cool/b9qz-7eka" name="Albany Museum" pageId="10">AMG</collectionCode>
