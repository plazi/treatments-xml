<quantity id="4CD9C1DCFFBD237B78EE73C8B368BAE4" box="[972,1036,440,467]" metricMagnitude="1" metricUnit="m" metricValue="6.2" pageId="11" pageNumber="264" unit="m" value="62.0">62 m</quantity>
