<taxonomicName id="4C48EAFCFFDBFFA6FC8856AFD6D370A5" authorityName=": Steyskal" authorityYear="1963" box="[824,913,1257,1278]" class="Insecta" family="Sciomyzidae" genus="Calliscia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="274" phylum="Arthropoda" rank="genus">Calliscia</taxonomicName>
