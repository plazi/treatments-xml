, 
<taxonomicName id="4C48EAFCFFDBFFA6FA8054F8D0DB7288" authorityName=": Becker" authorityYear="1919" box="[1328,1433,1726,1747]" class="Insecta" family="Sciomyzidae" genus="Perilimnia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="274" phylum="Arthropoda" rank="genus">Perilimnia</taxonomicName>
