<taxonomicName id="4C46D2AA2C09FFBBFF6467B9FDF1069B" authority="Wiedemann" authorityName="Wiedemann" box="[201,615,1713,1739]" class="Insecta" family="Anthomyiidae" genus="Anthomyia" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="tempestatum">
