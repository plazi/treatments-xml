<heading id="6F7CC55817A4A98B989DFE6C7B60FE3D" bold="true" box="[151,803,423,449]" fontSize="11" level="1" pageId="149" pageNumber="150" reason="1">
<emphasis id="06FFAE2617A4A98B989DFE6C7B60FE3D" bold="true" box="[151,803,423,449]" pageId="149" pageNumber="150">
<taxonomicName id="F38B09B717A4A98B989DFE6C7AC5FE3D" authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,646,423,449]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="shillonga" status="new species">
<emphasis id="06FFAE2617A4A98B989DFE6C79EFFE3D" bold="true" box="[151,428,423,449]" italics="true" pageId="149" pageNumber="150">Hydropsyche shillonga</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D17A4A98B9A99FE6C7B60FE3D" box="[659,803,423,449]" pageId="149" pageNumber="150" rank="species">new species</taxonomicNameLabel>
