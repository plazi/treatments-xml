<taxonomicName id="B1DCB9A47703C84CD32B5C3654EF8B03" authority="Christ" class="Magnoliopsida" family="Rosaceae" genus="Alchemilla" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="336" phylum="Tracheophyta" rank="species" species="splendens">
