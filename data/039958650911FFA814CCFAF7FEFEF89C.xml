<taxonomicName id="4C3092F00911FFA81528F88FFDA5F8E4" authorityName="Rodrigues da Costa" authorityYear="1959" box="[371,632,1797,1822]" class="Malacostraca" family="Parthenopidae" genus="Heterocrypta" kingdom="Animalia" order="Decapoda" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="tommasii">Heterocrypta tommasii</taxonomicName>
