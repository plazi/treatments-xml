Five specimens, unstained whole mounts, 
<accessionNumber id="947DAB2095649F0DFC359327FB85FC95" box="[904,1034,798,822]" httpUri="http://www.uniprot.org/uniprot/IG327183" pageId="11" pageNumber="519">
IG 
<number id="3DE624E495649F0DFC0E9327FB85FC95" box="[947,1034,798,822]" pageId="11" pageNumber="519" value="327183.0">327183</number>
</accessionNumber>
- 
<number id="3DE624E495649F0DFBAC9327FBA3FC95" box="[1041,1068,798,822]" pageId="11" pageNumber="519" value="28.0">28</number>
to 
<accessionNumber id="947DAB2095649F0DFBE69327FB52FC95" box="[1115,1245,798,822]" httpUri="http://www.uniprot.org/uniprot/IG327183" pageId="11" pageNumber="519">
IG 
<number id="3DE624E495649F0DFB389327FB52FC95" box="[1157,1245,798,822]" pageId="11" pageNumber="519" value="327183.0">327183</number>
</accessionNumber>
- 
<number id="3DE624E495649F0DFB589327FA8EFC95" box="[1253,1281,798,822]" pageId="11" pageNumber="519" value="32.0">32</number>
, [
<number id="3DE624E495649F0DFAA49324FE8CFCF9" pageId="11" pageNumber="519" value="51.854910499999995" valueMax="88.21247" valueMin="15.497351">15.497351- 88.21247</number>
, 
<number id="3DE624E495649F0DFEB39378FD8CFCF9" box="[270,515,833,858]" pageId="11" pageNumber="519" value="448.819014" valueMax="882.1247" valueMin="15.513328">15.513328 -882.1247</number>
, 
<number id="3DE624E495649F0DFDB39378FC8BFCF9" box="[526,772,833,858]" pageId="11" pageNumber="519" value="51.903326" valueMax="88.27633" valueMin="15.530322">15.530322 -88.27633</number>
], 
<quantity id="4CD69B2695649F0DFCAA937BFCE0FCFA" box="[791,879,834,858]" metricMagnitude="3" metricUnit="m" metricValue="2.07" pageId="11" pageNumber="519" unit="m" value="2070.0">2070 m</quantity>
, 
<quantity id="4CD69B2695649F0DFCC69378FC5FFCFA" box="[891,976,833,858]" metricMagnitude="3" metricUnit="m" metricValue="1.67" pageId="11" pageNumber="519" unit="m" value="1670.0">1670 m</quantity>
and 
<quantity id="4CD69B2695649F0DFBB59378FBD1FCFA" box="[1032,1118,833,858]" metricMagnitude="3" metricUnit="m" metricValue="1.608" pageId="11" pageNumber="519" unit="m" value="1608.0">1608 m</quantity>
asl, respectively, M. Jocque 
<date id="FF90100395649F0DFF2A935CFE95FCDD" box="[151,282,869,894]" pageId="11" pageNumber="519" value="2013-08-06">
