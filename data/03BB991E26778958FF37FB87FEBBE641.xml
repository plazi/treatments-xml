<quantity id="4CEA85ED26778958FE1AFB19FE76E484" box="[442,505,1248,1269]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="1" pageNumber="418" unit="mm" value="7.0">7 mm</quantity>
