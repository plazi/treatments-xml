<geoCoordinate id="EE205073C679A20BFA1FF1BFF97A4D0A" box="[1511,1685,3621,3660]" degrees="132" direction="east" minutes="56" orientation="longitude" pageId="15" pageNumber="242" precision="925" value="132.93333">132°56’E</geoCoordinate>
