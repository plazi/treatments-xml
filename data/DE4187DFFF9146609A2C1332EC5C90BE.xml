<number id="E02024EEFF914663980312A3EE279579" box="[807,838,1015,1041]" pageId="4" pageNumber="5" value="47.0">47</number>
(
<number id="E02024EEFF914663986A12A3EEFA9579" box="[846,923,1015,1041]" pageId="4" pageNumber="5" value="45.0" valueMax="47.0" valueMin="43.0">43–47</number>
), f 
