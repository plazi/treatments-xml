<taxonomicName id="EDD0C88AFF9FFFC2AE8211F6FEF682FD" box="[151,313,2001,2025]" class="Pinopsida" family="Cupressaceae" genus="Widdringtonia" kingdom="Plantae" order="Pinales" pageId="4" pageNumber="249" phylum="Tracheophyta" rank="genus">
