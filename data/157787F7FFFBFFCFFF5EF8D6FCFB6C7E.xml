<taxonomicName id="5ADE4D62FFFFFFCFFF5EFDAEFE836C71" box="[151,305,512,536]" class="Malacostraca" family="Sesarmidae" genus="Lithoselatium" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="67" phylum="Arthropoda" rank="genus">
