<taxonomicName id="5ADE4D62FFFFFFCFFD44FE16FC956FB9" box="[653,807,440,464]" class="Malacostraca" family="Sesarmidae" genus="Lithoselatium" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="67" phylum="Arthropoda" rank="genus">
