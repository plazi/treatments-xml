<paragraph id="8BD436E9EF05463C30E1FF1AFDC5674C" blockId="1.[151,1075,220,274]" box="[151,574,250,274]" pageId="1" pageNumber="568">
<treatmentCitationGroup id="AB7B11C7EF05463C30E1FF1AFDC5674C" box="[151,574,250,274]" pageId="1" pageNumber="568">
