INV 
<specimenCode id="934D9EBBFF9EDC25A099FB93FE59FE2A" box="[393,494,1069,1095]" collectionCode="CNI" pageId="9" pageNumber="10">CNI241</specimenCode>
, 
<specimenCount id="D5EDFD49FF9EDC25A0EDFB93FD3BFE2A" box="[509,652,1069,1095]" pageId="9" pageNumber="10" type="generic">1 specimen</specimenCount>
, 
<typeStatus id="1C508862FF9EDC25A38AFB93FCBEFE2A" box="[666,777,1069,1095]" pageId="9" pageNumber="10">holotype</typeStatus>
</materialsCitation>
, 
