see 
<taxonomicName id="8FEE4D41FF29FF02FE97F9FD64B8CD33" authorityName="Kohls" authorityYear="1950" box="[263,507,1626,1652]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="192" pageNumber="193" phylum="Arthropoda" rank="species" species="minuta">
