<taxonomicName id="87E24D5FCC0FFFACE8DD054C0D7AF077" authority="Jaderholm, 1903" authorityName="Jaderholm" authorityYear="1903" box="[151,613,1291,1316]" class="Hydrozoa" family="Lafoeidae" genus="Abietinella" kingdom="Animalia" order="Leptothecata" pageId="25" pageNumber="26" phylum="Cnidaria" rank="species" species="operculata">
