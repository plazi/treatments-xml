<taxonomicName id="4C01F08E4D6DFFCDFF51FE78FE60FE58" authority="Perez, 1903" authorityName="Perez" authorityYear="1903" box="[151,470,439,464]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="490" phylum="Arthropoda" rank="species" species="pusilla">
