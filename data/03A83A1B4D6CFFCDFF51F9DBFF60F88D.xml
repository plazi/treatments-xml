</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C01F08E4D6DFFCDFC2EFE5BFADEFE25" authority="Stockhert, 1935" authorityName="Stockhert" authorityYear="1935" box="[1000,1384,404,429]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="490" phylum="Arthropoda" rank="species" species="pauxilla">
