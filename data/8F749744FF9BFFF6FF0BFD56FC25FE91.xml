<taxonomicName id="C0DD5DD1FF9BFFF6FE89FD56FDC5F8E9" authority="Wesmael, 1845" authorityName="Wesmael" authorityYear="1845" box="[266,576,1814,1840]" class="Insecta" family="Ichneumonidae" genus="Banchinae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="640" phylum="Arthropoda" rank="genus">Banchinae Wesmael, 1845</taxonomicName>
