% ± 
<number id="6C33EA67FF92FFD3AF263D1AFEDEFD87" box="[314,350,564,590]" pageId="17" pageNumber="24" value="1.2">1.2</number>
% (n= 
<number id="6C33EA67FF92FFD3AFAC3D1AFE50FD87" box="[432,464,564,590]" pageId="17" pageNumber="24" value="52.0">52</number>
), 
<number id="6C33EA67FF92FFD3AFF93D1AFD82FD87" box="[485,514,564,590]" pageId="17" pageNumber="24" value="20.0">20</number>
