%, n= 
<number id="6C33EA67FF92FFD3ADB13D72FC4CFDBF" box="[941,972,604,630]" pageId="17" pageNumber="24" value="50.0">50</number>
, and 
<number id="6C33EA67FF92FFD3AA133D72FBC2FDBF" box="[1039,1090,604,630]" pageId="17" pageNumber="24" value="91.6">91.6</number>
% ± 
<number id="6C33EA67FF92FFD3AA963D72FB41FDBF" box="[1162,1217,604,630]" pageId="17" pageNumber="24" value="2.49">2.49</number>
%, n= 
<number id="6C33EA67FF92FFD3AB1B3D72FAA6FDBF" box="[1287,1318,604,630]" pageId="17" pageNumber="24" value="20.0">20</number>
