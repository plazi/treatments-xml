<taxonomicName id="1DFB83C3FF93FFD2AA973B4AFA89FBB4" box="[1163,1289,1124,1149]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="16" pageNumber="23" phylum="Chordata" rank="species" species="flavipectus">
