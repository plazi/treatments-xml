: Pinar Rio, Sierra Rosario, 
<quantity id="4CFD9B40FF8A7B29FC831830DF2FFAD2" box="[779,852,1338,1362]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="2" pageNumber="206" unit="m" value="400.0">400 m</quantity>
, 
<date id="FFBB1065FF8A7B29FCD71833DF9DFAD1" box="[863,998,1337,1362]" pageId="2" pageNumber="206" value="1989-10-04" valueMax="1989-10-06" valueMin="1989-10-04">
<number id="3DCD2482FF8A7B29FCD71833DFF7FAD1" box="[863,908,1337,1362]" pageId="2" pageNumber="206" value="5.0" valueMax="6.0" valueMin="4.0">4–6</number>
.X. 
<number id="3DCD2482FF8A7B29FC221833DF9DFAD1" box="[938,998,1337,1362]" pageId="2" pageNumber="206" value="1989.0">1989</number>
</date>
, V.O. Becker Col; Col. Becker 
<number id="3DCD2482FF8A7B29FACB1833D9F4FAD1" box="[1347,1423,1337,1362]" pageId="2" pageNumber="206" value="70686.0">70686</number>
&quot;; GS 
<number id="3DCD2482FF8A7B29FF4A1854DC82FAF5" box="[194,249,1374,1398]" pageId="2" pageNumber="206" value="1189.0">1189</number>
