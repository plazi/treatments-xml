<number id="3D9C24E5FFAA3D60FEBFC6AEFB46FDE4" box="[326,371,511,536]" pageId="29" pageNumber="530" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
; metatarsus II pv 
<number id="3D9C24E5FFAA3D60FDC2C551F87EFDE4" box="[571,587,512,536]" pageId="29" pageNumber="530" value="2.0">2</number>
, rv 
<number id="3D9C24E5FFAA3D60FD8CC551F8B3FDE4" box="[629,646,512,536]" pageId="29" pageNumber="530" value="2.0">2</number>
