<materialsCitation id="0DED9D018E15FF93D9E30C9BDD51BA3F" country="New Caledonia" county="Indian Ocean" elevation="151" location="Tropical Indo-West Pacific" municipality="eastern Arabia" pageId="76" pageNumber="557" specimenCount="1" stateProvince="Province des Iles">
<collectingCounty id="545BEFD08E15FF93D9E30C9BDB00BDFA" box="[420,567,994,1018]" pageId="76" pageNumber="557">Indian Ocean</collectingCounty>
