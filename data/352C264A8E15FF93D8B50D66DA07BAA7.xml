<collectingRegion id="7F4159BE8E15FF93DCF20B7CDC60BA1D" box="[1205,1367,1029,1053]" country="New Caledonia" name="Province des Iles" pageId="76" pageNumber="557">Loyalty Islands</collectingRegion>
, empty juvenile shells in 
<quantity id="7A7D3AB98E15FF93D9CB0B5ED8C2BA3F" box="[396,501,1063,1087]" metricMagnitude="2" metricUnit="m" metricValue="1.51" metricValueMax="2.5" metricValueMin="0.52" pageId="76" pageNumber="557" unit="m" value="151.0" valueMax="250.0" valueMin="52.0">
<elevation id="36A8706F8E15FF93D9CB0B5ED8C2BA3F" box="[396,501,1063,1087]" metricMagnitude="2" metricUnit="m" metricValue="1.51" metricValueMax="2.5" metricValueMin="0.52" pageId="76" pageNumber="557" unit="m" value="151.0" valueMax="250.0" valueMin="52.0">52-250 m</elevation>
</quantity>
