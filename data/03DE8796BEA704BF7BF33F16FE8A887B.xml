<taxonomicName id="4C774D03BE9904BE792C3B80FEE48E96" box="[101,304,682,704]" class="Magnoliopsida" family="Scrophulariaceae" kingdom="Plantae" order="Lamiales" pageId="160" pageNumber="160" phylum="Tracheophyta" rank="family">Scrophulariaceae</taxonomicName>
