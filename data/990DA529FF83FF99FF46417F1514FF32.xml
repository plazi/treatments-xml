specimens
</specimenCount>
, 
<specimenCount id="07A2DFB6FF83FF9AFD7D4555148BF8C4" box="[684,722,1802,1826]" pageId="29" pageNumber="480" type="generic">
<number id="A76C0618FF83FF9AFD7D455514E2F8C4" box="[684,699,1802,1826]" pageId="29" pageNumber="480" value="2.0">2</number>
