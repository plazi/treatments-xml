<taxonomicName id="4C714D57215AFFAB100FFF20D88A3CA0" box="[596,729,152,177]" class="Insecta" family="Chrysomelidae" genus="Agetocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="taiwana">
<emphasis id="B905EAC6215AFFAB100FFF20D88A3CA0" box="[596,729,152,177]" italics="true" pageId="15" pageNumber="16">A. taiwana</emphasis>
</taxonomicName>
. 
<taxonomicName id="4C714D57215AFFAB10B1FF20D9F93CA0" box="[746,938,152,177]" class="Insecta" family="Chrysomelidae" genus="Agetocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="yuae">
