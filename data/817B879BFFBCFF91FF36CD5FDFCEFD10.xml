<taxonomicName id="CED24D0EFFBCFF92FE87CD07DCE6F866" authority="(Khramov, 2013)" authorityName="Khramov" authorityYear="2013" baseAuthorityName="Khramov" baseAuthorityYear="2013" box="[294,746,1904,1926]" class="Insecta" family="Mantispidae" genus="Karataumantispa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="564" phylum="Arthropoda" rank="species" species="carnaria">
