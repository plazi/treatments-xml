<heading id="D08E3F39FFFFFFA5FEF4BBB2FCCCFD74" bold="true" box="[264,841,657,683]" fontSize="11" level="1" pageId="5" pageNumber="6" reason="1">
<emphasis id="B90D5447FFFFFFA5FEF4BBB2FCCCFD74" bold="true" box="[264,841,657,683]" pageId="5" pageNumber="6">
<taxonomicName id="4C79F3D6FFFFFFA5FEF4BBB2FD31FD74" authority="Paulsen and Mondaca" authorityName="Paulsen and Mondaca" authorityYear="2006" box="[264,692,657,683]" class="Insecta" family="Lucanidae" genus="Hilophyllus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B90D5447FFFFFFA5FEF4BBB2FE10FD74" bold="true" box="[264,405,657,683]" italics="true" pageId="5" pageNumber="6">Hilophyllus</emphasis>
Paulsen and Mondaca
</taxonomicName>
, 
<taxonomicNameLabel id="A23EE93CFFFFFFA5FD3CBBB2FCC1FD74" box="[704,836,657,683]" pageId="5" pageNumber="6" rank="genus">new genus</taxonomicNameLabel>
