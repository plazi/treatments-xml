</taxonomicName>
(
<taxonomicName id="558F0FD7FF09497DFE248E17D60AFC92" box="[436,533,848,869]" class="Magnoliopsida" family="Ulmaceae" kingdom="Plantae" order="Rosales" pageId="6" pageNumber="744" phylum="Tracheophyta" rank="family">Ulmaceae</taxonomicName>
