, 
<taxonomicName id="7DE84D2B3B214C41FB57F86F9A44CAA2" authorityName="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy" authorityYear="2019" baseAuthorityName="Aubert" baseAuthorityYear="1961" box="[1155,1287,1944,1970]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="rufinator" status="stat. rev.">
