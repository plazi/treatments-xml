<taxonomicNameLabel id="93AF57C13B204C40FED0FEFB9E22CC36" box="[260,353,268,294]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName id="7DE84D2B3B204C40FEB8FEFB9E93CC35" authority="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy, 2019" authorityName="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy" authorityYear="2019" box="[364,464,267,293]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="angka" status="sp. nov.">
<emphasis id="889CEABA3B204C40FEB8FEFB9E93CC35" box="[364,464,267,293]" italics="true" pageId="9" pageNumber="10">P. angka</emphasis>
</taxonomicName>
<taxonomicNameLabel id="93AF57C13B204C40FE03FEFB9D70CC36" box="[471,563,268,294]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName id="7DE84D2B3B204C40FDEAFEFB9DA4CC35" authority="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy, 2019" authorityName="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy" authorityYear="2019" box="[574,743,267,293]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="hainanensis" status="sp. nov.">
<emphasis id="889CEABA3B204C40FDEAFEFB9DA4CC35" box="[574,743,267,293]" italics="true" pageId="9" pageNumber="10">P. hainanensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="93AF57C13B204C40FD3AFEFB9C09CC36" box="[750,842,268,294]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
, 
