</taxonomicName>
; (2) the 
<taxonomicName id="7DE84D2B3B214C41FD30F84C9C33CAC5" authorityName="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy" authorityYear="2019" box="[740,880,1979,2005]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="hainanensis">
<emphasis id="889CEABA3B214C41FD30F84C9C33CAC5" box="[740,880,1979,2005]" italics="true" pageId="8" pageNumber="9">hainanensis</emphasis>
</taxonomicName>
