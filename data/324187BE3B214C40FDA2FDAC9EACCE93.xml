and 
<taxonomicName id="7DE84D2B3B214C41FDC4F84B9DC7CAC5" authorityName="Aubert" authorityYear="1961" box="[528,644,1980,2005]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="striator">
