, 
<taxonomicName id="7DE84D2B3B204C40FF69FED89E59CC58" authority="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy, 2019" authorityName="Reshchikov &amp; Santos &amp; Liu &amp; Barthélémy" authorityYear="2019" box="[189,282,303,328]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="ranui" status="sp. nov.">
<emphasis id="889CEABA3B204C40FF69FED89E59CC58" box="[189,282,303,328]" italics="true" pageId="9" pageNumber="10">P. ranui</emphasis>
</taxonomicName>
<taxonomicNameLabel id="93AF57C13B204C40FEF5FED89E3DCC59" box="[289,382,303,329]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName id="7DE84D2B3B204C40FE5EFED89D67CC59" authorityName="Sheng &amp; Sun in Sheng, Sun, Ding &amp; Luo" authorityYear="2013" box="[394,548,303,329]" class="Insecta" family="Ichneumonidae" genus="Palpostilpnus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="rotundatus">
