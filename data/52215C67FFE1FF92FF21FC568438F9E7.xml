Genus 
<taxonomicName id="1D8896F2FFE1FF92FF58FC56834AFC62" authority="Filipjev, 1929" authorityName="Filipjev" authorityYear="1929" box="[238,555,944,970]" class="Chromadorea" family="Chromadoridae" genus="Punctodora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="16" pageNumber="17" phylum="Nematoda" rank="genus">
