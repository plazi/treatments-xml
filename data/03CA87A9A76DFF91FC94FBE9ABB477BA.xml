<geoCoordinate id="EE575078A76DFF91FB3CFB64ABDD76E1" box="[1163,1243,1254,1278]" degrees="22" direction="south" minutes="38" orientation="latitude" pageId="20" pageNumber="701" precision="925" value="-22.633333">22°38’S</geoCoordinate>
