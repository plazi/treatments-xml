<number id="B802001ED10B6742CC81FEFF2DE26C89" box="[1194,1251,367,391]" pageId="6" pageNumber="7" value="2001.0">2001</number>
</bibRefCitation>
) within 
<taxonomicName id="C9CA69BAD10B6742CD69FEE02C956C86" box="[1346,1428,368,392]" class="Polychaeta" family="Phyllodocidae" genus="Eulalia" kingdom="Animalia" order="Phyllodocida" pageId="6" pageNumber="7" phylum="Annelida" rank="genus">
<emphasis id="3CBECE2BD10B6742CD69FEE02C956C86" box="[1346,1428,368,392]" italics="true" pageId="6" pageNumber="7">Eulalia</emphasis>
</taxonomicName>
