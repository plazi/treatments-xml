<taxonomicName id="C9CA69BAD10B6742CB47FEE12AD56C86" box="[876,980,368,392]" class="Polychaeta" family="Phyllodocidae" genus="Eulalia" kingdom="Animalia" order="Phyllodocida" pageId="6" pageNumber="7" phylum="Annelida" rank="species" species="viridis">
