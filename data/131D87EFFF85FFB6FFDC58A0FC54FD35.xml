<taxonomicName id="5CB44D7AFF84FFB5FDD359EEFC5CF898" authorityName="Shear &amp; Richart &amp; Wong" authorityYear="2020" box="[664,931,1774,1798]" class="Diplopoda" family="Conotylidae" genus="Brunsonia" kingdom="Animalia" order="Chordeumatida" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="chelanomagna">Brunsonia chelanomagna</taxonomicName>
