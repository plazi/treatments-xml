<number id="EA7324CFFF98FF984890FA2BFD8CFA70" box="[506,518,1477,1501]" pageId="18" pageNumber="19" value="1.0">1</number>
B; basifemur 
<number id="EA7324CFFF98FF984BCFFA2BFD3AFA70" box="[677,688,1477,1501]" pageId="18" pageNumber="19" value="1.0">1</number>
B; telofemur 
<number id="EA7324CFFF98FF984A26FA2AFCD1FA70" box="[844,859,1476,1501]" pageId="18" pageNumber="19" value="5.0">5</number>
B; genu 
<number id="EA7324CFFF98FF984AD4FA2BFC44FA70" box="[958,974,1477,1501]" pageId="18" pageNumber="19" value="4.0">4</number>
B, 
<number id="EA7324CFFF98FF984A84FA2BFC76FA70" box="[1006,1020,1477,1501]" pageId="18" pageNumber="19" value="2.0">2</number>
genualae, microgenuala; tibia 
<number id="EA7324CFFF98FF984C37FA2BFAE1FA70" box="[1373,1387,1477,1501]" pageId="18" pageNumber="19" value="8.0">8</number>
