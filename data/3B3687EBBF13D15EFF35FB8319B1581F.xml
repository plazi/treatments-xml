<emphasis id="81EBEAEFBF13D15EFB08FA1119F05AE9" box="[1194,1276,1480,1505]" italics="true" pageId="5" pageNumber="56">iniqua</emphasis>
(Sluiter)
</taxonomicName>
, 
<taxonomicName id="749F4D7EBF13D15EFA20FA111CD75900" class="Sipunculidea" family="Golfingiidae" genus="Golfingia" kingdom="Animalia" order="Golfingiiformes" pageId="5" pageNumber="56" phylum="Sipuncula" rank="subSpecies" species="margaritacea" subSpecies="margaritacea">
<emphasis id="81EBEAEFBF13D15EFA20FA1118995AE9" box="[1410,1429,1480,1505]" italics="true" pageId="5" pageNumber="56">G</emphasis>
. 
<emphasis id="81EBEAEFBF13D15EFF35FA361CD75900" box="[151,475,1519,1544]" italics="true" pageId="5" pageNumber="56">margaritacea margaritacea</emphasis>
</taxonomicName>
, 
<emphasis id="81EBEAEFBF13D15EFE49FA361CF15900" box="[491,509,1519,1544]" italics="true" pageId="5" pageNumber="56">G</emphasis>
. 
<emphasis id="81EBEAEFBF13D15EFDA9FA361F2C5900" box="[523,544,1519,1544]" italics="true" pageId="5" pageNumber="56">m</emphasis>
. 
<emphasis id="81EBEAEFBF13D15EFD92FA361F785900" box="[560,628,1519,1544]" italics="true" pageId="5" pageNumber="56">ohlini</emphasis>
(Théel), 
<taxonomicName id="749F4D7EBF13D15EFD43FA361EC15900" authority="Murina" authorityName="Murina" box="[737,973,1518,1544]" class="Sipunculidea" family="Golfingiidae" genus="Golfingia" kingdom="Animalia" order="Golfingiiformes" pageId="5" pageNumber="56" phylum="Sipuncula" rank="species" species="mirabilis">
<emphasis id="81EBEAEFBF13D15EFD43FA361FFF5900" box="[737,755,1519,1544]" italics="true" pageId="5" pageNumber="56">G</emphasis>
. 
<emphasis id="81EBEAEFBF13D15EFCA0FA361E605900" box="[770,876,1519,1544]" italics="true" pageId="5" pageNumber="56">mirabilis</emphasis>
Murina
</taxonomicName>
, 
<taxonomicName id="749F4D7EBF13D15EFC7BFA3619CD5900" box="[985,1217,1518,1544]" class="Sipunculidea" family="Golfingiidae" genus="Golfingia" kingdom="Animalia" order="Golfingiiformes" pageId="5" pageNumber="56" phylum="Sipuncula" rank="subSpecies" species="vulgaris" subSpecies="vulgaris">
