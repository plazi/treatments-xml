<taxonomicName id="749F4D7EBF13D15EFA21FA361CA75927" authority="Shipley" authorityName="Shipley" class="Sipunculidea" family="Golfingiidae" genus="Golfingia" kingdom="Animalia" order="Golfingiiformes" pageId="5" pageNumber="56" phylum="Sipuncula" rank="variety" variety="herdmani">
