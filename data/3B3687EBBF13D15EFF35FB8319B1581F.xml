<taxonomicName id="749F4D7EBF13D15EFB27F9CC18485926" box="[1157,1348,1557,1582]" class="Sipunculidea" family="Golfingiidae" genus="Golfingia" kingdom="Animalia" order="Golfingiiformes" pageId="5" pageNumber="56" phylum="Sipuncula" rank="species" species="muricaudata">
