<taxonomicName id="4C3B4D36FE0E371A22A26C64FD7DF9F1" box="[498,669,1626,1651]" class="Insecta" family="Raphidiidae" genus="Dictyoraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="genus">
