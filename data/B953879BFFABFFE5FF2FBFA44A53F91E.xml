<collectionCode id="57EBAE48FFA8FFE5FE9CBA9C4A64FA8A" box="[292,371,1268,1292]" country="Brazil" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="2" pageNumber="386">MZSP</collectionCode>
