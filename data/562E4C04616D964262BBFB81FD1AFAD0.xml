<quantity id="197F50F7616D964261C9FA83FC22FAA3" box="[997,1012,1285,1311]" metricMagnitude="-1" metricUnit="m" metricValue="1.778" pageId="1" pageNumber="48" unit="in" value="7.0">7</quantity>
</figureCitation>
