<number id="CA1A24B673685E1C1A53FAA7FA690E33" box="[1391,1422,1339,1363]" pageId="43" pageNumber="344" value="23.0">23</number>
), 
<number id="CA1A24B673685E1C1FABFAC3FF540E17" box="[151,179,1375,1399]" pageId="43" pageNumber="344" value="23.0">23</number>
(
