</date>
(
<collectionCode id="0FD7D98CFFE9FFB3613D9AE3D28FFC28" LSID="urn:lsid:biocol.org:col:34925" box="[1128,1220,906,930]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="4" pageNumber="83">AMNH</collectionCode>
