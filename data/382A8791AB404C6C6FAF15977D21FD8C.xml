<location id="B55C605CAB474C6F6A3B138A7B78F829" LSID="urn:lsid:plazi:treatment:382A8791AB404C6C6FAF15977D21FD8C:B55C605CAB474C6F6A3B138A7B78F829" box="[1283,1372,1963,1989]" country="Russia" county="Forewing" municipality="Distribution" name="Russian" pageId="5" pageNumber="388" stateProvince="Krasnodar">Russian</location>
