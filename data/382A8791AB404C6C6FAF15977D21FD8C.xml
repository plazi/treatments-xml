<location id="B55C605CAB474C6F6C64138A7DF6F829" LSID="urn:lsid:plazi:treatment:382A8791AB404C6C6FAF15977D21FD8C:B55C605CAB474C6F6C64138A7DF6F829" box="[860,978,1963,1989]" country="Russia" county="Forewing" municipality="Distribution" name="Kosheleva" pageId="5" pageNumber="388" stateProvince="Krasnodar">Kosheleva</location>
