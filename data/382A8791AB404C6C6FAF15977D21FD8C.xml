<collectionCode id="D692AE42AB474C6F6C04138D7D74F82A" box="[828,848,1964,1990]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="5" pageNumber="388" type="Museum">V</collectionCode>
