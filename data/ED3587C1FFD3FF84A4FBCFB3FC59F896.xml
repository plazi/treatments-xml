<emphasis id="57E8EAC5FFD3FF84A4FBCFB3FEBAF967" bold="true" box="[136,300,1696,1722]" pageId="2" pageNumber="178">
1. 
<taxonomicName id="A29C4D54FFD3FF84A4D7CFB3FF71F967" authorityName="Aublet" authorityYear="1775" box="[164,231,1696,1722]" class="Magnoliopsida" family="Chrysobalanaceae" genus="Acioa" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="178" phylum="Tracheophyta" rank="genus">
<emphasis id="57E8EAC5FFD3FF84A4D7CFB3FF71F967" bold="true" box="[164,231,1696,1722]" italics="true" pageId="2" pageNumber="178">Acioa</emphasis>
