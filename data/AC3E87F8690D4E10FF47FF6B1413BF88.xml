<bibRefCitation id="40064B1F690D4E10FEC7FDA616F7BE20" author="Starega" box="[279,447,602,626]" pageId="3" pageNumber="43" refString="Starega, W. (1992) An annotated check-list of harvestmen, excluding Phalangiidae, of the Afrotropical Region (Opiliones). Annals of the Natal Museum, Pietermaritzburg, 33, 271 - 336." type="journal article" year="1992">
Staręga (
<number id="925F24C9690D4E10FEADFDA616F0BE20" box="[381,440,602,626]" pageId="3" pageNumber="43" value="1992.0">1992</number>
)
</bibRefCitation>
into 
<taxonomicName id="E3974D6D690D4E10FE2FFDA71530BE21" box="[511,632,603,627]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="genus">
