of 
<taxonomicName id="84954D02C301871EFE9018A5FE4CFE08" box="[353,495,391,415]" class="Insecta" family="Philopteridae" genus="Columbicola" kingdom="Animalia" order="Psocodea" pageId="59" pageNumber="3602" phylum="Arthropoda" rank="species" species="sphenurus">
