<paragraph id="432A3681C301871EFF8C1A80FB26FBED" blockId="59.[125,1173,930,1146]" pageId="59" pageNumber="3602">
<materialsCitation id="F3FD3CDCC301871EFF8C1A80FB22FBED" collectionCode="APW, HW, PW, MW" latitude="0.152" longLatPrecision="78" longitude="-0.157" pageId="59" pageNumber="3602" specimenCount="1">
Male head with 
<collectionCode id="2584AE44C301871EFEDE1A80FED2FC2D" box="[303,369,930,954]" pageId="59" pageNumber="3602">APW</collectionCode>
, 
<geoCoordinate id="26A15046C301871EFE881A80FE18FC2D" box="[377,443,930,954]" degrees="0.142" direction="north" orientation="latitude" pageId="59" pageNumber="3602" precision="55" value="0.142">0.142</geoCoordinate>
<geoCoordinate id="26A15046C301871EFE4A1A80FDA8FC2D" box="[443,523,930,954]" degrees="0.157" direction="west" orientation="longitude" pageId="59" pageNumber="3602" precision="55" value="-0.157">–0.157</geoCoordinate>
(0.149); 
<collectionCode id="2584AE44C301871EFD801A80FD05FC2D" box="[625,678,930,954]" pageId="59" pageNumber="3602">HW</collectionCode>
, 0.31–0.32 (0.313); HL, 0.58–0.60 (0.593); HL/ 
<collectionCode id="2584AE44C301871EFF5E1AE0FF41FC4D" box="[175,226,962,986]" pageId="59" pageNumber="3602">HW</collectionCode>
