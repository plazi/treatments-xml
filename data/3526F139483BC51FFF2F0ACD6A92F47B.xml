<date id="C93166EF483BC51EFE0E09FB6EC4F2D4" box="[438,461,2010,2031]" pageId="12" pageNumber="117">
<number id="0B475208483BC51EFE0E09FB6ECBF2D4" box="[438,450,2010,2031]" pageId="12" pageNumber="117" value="2.0">2</number>
