<quantity id="7A77EDCA483AC51FFD8E0E946D9DF5F1" box="[566,660,181,202]" metricMagnitude="-3" metricUnit="m" metricValue="5.25" metricValueMax="5.5" metricValueMin="5.0" pageId="13" pageNumber="118" unit="mm" value="5.25" valueMax="5.5" valueMin="5.0">5–5.5mm</quantity>
. 
<collectingCountry id="C59800BF483AC51FFD250E946DFDF5F1" box="[669,756,181,202]" name="Morocco" pageId="13" pageNumber="118">Morocco</collectingCountry>
to 
<collectingCountry id="C59800BF483AC51FFCAA0E946C55F5F1" box="[786,860,181,202]" name="Tunisia" pageId="13" pageNumber="118">Tunisia</collectingCountry>
, 
<collectingCountry id="C59800BF483AC51FFCDE0E946C92F5F1" box="[870,923,181,202]" name="Israel" pageId="13" pageNumber="118">Israel</collectingCountry>
............................ 
