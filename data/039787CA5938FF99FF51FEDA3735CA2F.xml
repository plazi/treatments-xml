<taxonomicName id="4C3E4D5F593AFF99FE6CF9913039CA0B" authorityName="Sanborn" authorityYear="2009" box="[426,563,1593,1619]" class="Insecta" family="Cicadidae" genus="Herrera" kingdom="Animalia" order="Hemiptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="infuscata">
