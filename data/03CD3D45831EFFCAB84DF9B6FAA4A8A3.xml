<geoCoordinate id="EE50EA94831EFFCABE3DF959FC34A9A7" degrees="101" direction="east" minutes="16.333" orientation="longitude" pageId="10" pageNumber="1065" precision="1" value="101.27222">101° 16.333′E</geoCoordinate>
