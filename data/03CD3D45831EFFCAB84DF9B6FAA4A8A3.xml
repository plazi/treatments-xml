<quantity id="4C9C21B6831EFFCABF65F863FB96A8A3" box="[1029,1070,1972,1996]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="10" pageNumber="1065" unit="m" value="3.0">3 m</quantity>
