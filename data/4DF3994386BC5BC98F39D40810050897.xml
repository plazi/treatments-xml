<paragraph id="832ABC2A607D1F5EEC741A2271B5E8E9" pageId="0" pageNumber="161">Etymology.</paragraph>
<paragraph id="977A95629FAAE5A0171B7CFA96389F87" pageId="0" pageNumber="161">
<taxonomicName id="87ED7C943A7662E5E137A35E36C3BB43" authorityName="J. J. Hu, Bo Zhang &amp; Y. Li" authorityYear="2021" class="Ascomycetes" family="Cordycipitaceae" genus="Cordyceps" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Cordyceps jinyuetanensis" order="Lepidoptera" pageId="0" pageNumber="161" phylum="Ascomycota" rank="species" species="jinyuetanensis">
<emphasis id="C22D46858A4041BC711B565D90361C80" italics="true" pageId="0" pageNumber="161">Cordyceps jinyuetanensis</emphasis>
</taxonomicName>
