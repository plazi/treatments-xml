, 
<location id="186E6027FF83FFDEFE26D2AFE632FE0A" LSID="urn:lsid:plazi:treatment:951887EAFFFCFFDEFF51D5ECE699FA56:186E6027FF83FFDEFE26D2AFE632FE0A" box="[480,626,439,465]" country="Bermuda" municipality="Harrington Sound" name="Trunk Island" pageId="64" pageNumber="300">Trunk Island</location>
