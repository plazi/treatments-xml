<typeStatus id="54833CD6FF8DFF8B99BEFA809A8E486B" box="[151,203,1354,1377]" pageId="2" pageNumber="3">Type</typeStatus>
species: 
<taxonomicName id="4C38F9F7FF8DFF8B981AFA839BBA486B" authority="Fabricius, 1780" authorityName="Fabricius" authorityYear="1780" box="[307,511,1353,1377]" class="Polychaeta" family="Capitellidae" genus="Capitella" kingdom="Animalia" pageId="2" pageNumber="3" phylum="Annelida" rank="species" species="capitata">Capitella capitata</taxonomicName>
</emphasis>
(Fabricius, 
<number id="3DF09053FF8DFF8B9BACFA8398FB486B" box="[645,702,1353,1377]" pageId="2" pageNumber="3" value="1780.0">1780</number>
