and 
<taxonomicName id="8DD84D78FFCBBC51FAE02A8BFEF9FDDF" authority="(Linnaeus)" authorityName="Linnaeus" baseAuthorityName="Linnaeus" baseAuthorityYear="1766" class="Aves" family="Charadriidae" genus="Vanellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Charadriiformes" pageId="17" pageNumber="418" phylum="Chordata" rank="species" species="senegallus">
