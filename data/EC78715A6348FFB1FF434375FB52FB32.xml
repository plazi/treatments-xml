<number id="D219D26B6349FFB1FBF340FEFBF7FCFA" box="[1063,1137,752,778]" pageId="37" pageNumber="38" value="76.5" valueMax="83.0" valueMin="70.0">70–83</number>
long, seta 
<emphasis id="56A51C5E6349FFB1FB3940FEFA9EFCF9" box="[1261,1304,752,777]" italics="true" pageId="37" pageNumber="38">
av 
