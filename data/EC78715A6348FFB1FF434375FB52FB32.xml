<number id="D219D26B6349FFB1FB0140AEFB64FD49" box="[1237,1250,672,697]" pageId="37" pageNumber="38" value="1.0">1</number>
</date>
</emphasis>
<number id="D219D26B6349FFB1FB3E40AEFAB1FD4A" box="[1258,1335,672,698]" pageId="37" pageNumber="38" value="32.5" valueMax="35.0" valueMin="30.0">30–35</number>
; 
