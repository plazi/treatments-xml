<geoCoordinate id="7950C83AC78475C0A9D024618B2D594B" direction="south" orientation="latitude" precision="15" value="-18.689722">18°41'23&quot;S</geoCoordinate>
, 
<geoCoordinate id="FEE682F98044DD48A6A7363AC1192526" direction="west" orientation="longitude" precision="15" value="-64.90723">64°54'26&quot;W</geoCoordinate>
