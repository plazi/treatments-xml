<taxonomicName id="4C624D1CFFE2FFA1FE24F8F39A54E641" authority="Robineau-Desvoidy, 1830" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[431,861,1794,1820]" class="Insecta" family="Anthomyiidae" genus="Pegomya" kingdom="Animalia" order="Diptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="genus">
