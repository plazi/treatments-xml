<taxonomicName id="4FB9E89CFFB7FFA7FDEF458EFC72FD6F" authority="(Rehn, 1904)" baseAuthorityName="Rehn" baseAuthorityYear="1904" box="[568,899,737,763]" class="Insecta" family="Pseudophasmatidae" genus="Parastratocles" kingdom="Animalia" order="Phasmida" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="multilineatus">
