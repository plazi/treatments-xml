<date id="FFF31067FFF6FFE7252B30EEFBF2FD01" box="[888,1035,691,717]" pageId="19" pageNumber="260" value="2019-07-12">
<collectingDate id="EFB7E98FFFF6FFE7252B30EEFBF2FD01" box="[888,1035,691,717]" pageId="19" pageNumber="260" value="2019-07-12">12 July 2019</collectingDate>
</date>
, 
<collectorName id="26B85371FFF6FFE7224530EEFB53FD01" box="[1046,1194,691,717]" pageId="19" pageNumber="260">C.H.Dietrich</collectorName>
, sweeping understory in 2º pine forest, VN19-8-1 (
<collectionCode id="ED5CAE62FFF6FFE7278B308AFDE6FD3D" box="[472,543,727,753]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" lsid="urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="19" pageNumber="260" type="Other type of research institution/biorepository">INHS</collectionCode>
