<typeStatus id="7C6C18827736FFF7FC1AFD1708C248B7" box="[963,1011,686,712]" pageId="13" pageNumber="394">type</typeStatus>
specimens from Central 
<collectingCountry id="DBC0E6B07736FFF7FACAFD140EA848B8" box="[1299,1433,685,711]" name="Kazakhstan" pageId="13" pageNumber="394">Kazakhstan</collectingCountry>
