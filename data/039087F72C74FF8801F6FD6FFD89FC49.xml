<paragraph id="8B8636E12C74FF8801DCFC8DFDC3FCB7" blockId="15.[151,1060,737,818]" pageId="15" pageNumber="136">
<emphasis id="B94DEAF32C74FF8801DCFC8DFF60FC9D" bold="true" box="[189,199,771,792]" pageId="15" pageNumber="136">•</emphasis>
Ruderalflur 
<quantity id="4CC19B042C74FF880023FC8BFED3FC9C" box="[322,372,773,793]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="15" pageNumber="136" unit="m" value="20.0">20 m</quantity>
