<number id="3DBE3F260945FFA5FBF7E796FBB5FD12" box="[1037,1051,724,748]" pageId="1" pageNumber="46" value="6.0">6</number>
+ 
<number id="3DBE3F260945FFA5FBCDE796FBEBFD12" box="[1079,1093,724,748]" pageId="1" pageNumber="46" value="1.0">1</number>
φ, 
<number id="3DBE3F260945FFA5FB99E796FBDFFD12" box="[1123,1137,724,748]" pageId="1" pageNumber="46" value="6.0">6</number>
+ 
<number id="3DBE3F260945FFA5FB74E796FB32FD12" box="[1166,1180,724,748]" pageId="1" pageNumber="46" value="1.0">1</number>
φ; tarsi 
<number id="3DBE3F260945FFA5FB0BE796FB51FD12" box="[1265,1279,724,748]" pageId="1" pageNumber="46" value="9.0">9</number>
+ 
<number id="3DBE3F260945FFA5FAE6E796FA84FD12" box="[1308,1322,724,748]" pageId="1" pageNumber="46" value="1.0">1</number>
ω, 
<number id="3DBE3F260945FFA5FAB0E796FAF6FD12" box="[1354,1368,724,748]" pageId="1" pageNumber="46" value="9.0">9</number>
+ 
<number id="3DBE3F260945FFA5FA8EE796FA2CFD12" box="[1396,1410,724,748]" pageId="1" pageNumber="46" value="1.0">1</number>
ω, 
