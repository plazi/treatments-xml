<number id="3DBE3F260945FFA5FC4CE797FC6AFD12" box="[950,964,725,748]" pageId="1" pageNumber="46" value="7.0">7</number>
+ 
<number id="3DBE3F260945FFA5FC1BE796FC41FD12" box="[993,1007,724,748]" pageId="1" pageNumber="46" value="1.0">1</number>
