<collectionCode id="728304E3FF92FFD2D63CF4DDB4DFFC41" LSID="urn:lsid:biocol.org:col:34991" box="[613,705,990,1014]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="6" pageNumber="7">HNHM</collectionCode>
