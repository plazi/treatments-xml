<taxonomicName id="4C5E35B3CC50FFB56B32FBD8FD17AF12" box="[357,655,1075,1098]" class="Insecta" family="Carabidae" genus="Antireicheia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="camerounensis" status="sp. nov.">
