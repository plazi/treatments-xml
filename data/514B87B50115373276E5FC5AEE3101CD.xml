(
<figureCitation id="41D92A260115373276EDFC26ED6D0791" box="[159,315,977,1004]" captionStart-0="FIGURES 186–188" captionStart-1="FIGURES 189, 190" captionStartId-0="134.[151,264,1933,1957]" captionStartId-1="135.[151,264,1933,1957]" captionTargetBox-0="[151,1435,181,1909]" captionTargetBox-1="[264,1323,181,1909]" captionTargetId-0="figure-18@134.[151,1435,181,1909]" captionTargetId-1="figure-20@135.[264,1323,181,1909]" captionTargetPageId-0="134" captionTargetPageId-1="135" captionText-0="FIGURES 186–188. Octomicromeris compacta Huber. 186a, head, anterior; 186b, head, posterior; 187, holotype antenna; 188, holotype wings. Scale bars: 186 = 100 μm; 187, 188 = 200 μm." captionText-1="FIGURES 189, 190. Octomicromeris compacta Huber, holotype. 189, mesosoma, dorsal; 190a, metasoma, dorsal; 190b, genitalia seen through gaster. Scale bars = 200 μm." figureDoi-0="http://doi.org/10.5281/zenodo.5503233" figureDoi-1="http://doi.org/10.5281/zenodo.5503239" httpUri-0="https://zenodo.org/record/5503233/files/figure.png" httpUri-1="https://zenodo.org/record/5503239/files/figure.png" pageId="37" pageNumber="38">Figs 186–190</figureCitation>
)
</paragraph>
<paragraph id="D95D36A30115373276E5FBE6E9590055" blockId="37.[151,1295,1040,1065]" box="[151,1295,1040,1065]" pageId="37" pageNumber="38">
<taxonomicName id="1EE24D200115373276E5FBE6EDBE0055" authority="Huber, 2015: 51" authorityName="Huber" authorityPageNumber="51" authorityYear="2015" box="[151,488,1040,1064]" class="Insecta" family="Mymaridae" genus="Octomicromeris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="genus">
<emphasis id="EB96EAB10115373276E5FBE6ED6D0055" box="[151,315,1041,1064]" italics="true" pageId="37" pageNumber="38">Octomicromeris</emphasis>
Huber, 2015: 51
</taxonomicName>
. 
<typeStatus id="06598801011537327780FBE6EE720054" box="[498,548,1041,1065]" pageId="37" pageNumber="38">Type</typeStatus>
species: 
<taxonomicName id="1EE24D200115373274F3FBE6E8420055" authority="Huber, 2015" authorityName="Huber" authorityYear="2015" box="[641,1044,1040,1064]" class="Insecta" family="Mymaridae" genus="Octomicromeris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="compacta">
