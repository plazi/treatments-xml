[[ 
<taxonomicName id="C78496A6D8312518CD15CE256A12E379" LSID-ZBK="urn:lsid:zoobank.org:act:CA8755B1-B115-41B4-BD5A-DAD46F20242C" class="Actinopterygii" family="Characidae" genus="Oligosarcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Oligosarcus Günther 1864:353" order="Characiformes" pageNumber="43" phylum="Chordata" rank="genus">Oligosarcus</taxonomicName>
