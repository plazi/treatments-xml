<caption id="DF33AEAEDD6DAC2527BD1EE8FB24FB14" ID-DOI="http://doi.org/10.5281/zenodo.3823718" ID-Zenodo-Dep="3823718" box="[151,1153,1236,1261]" httpUri="https://zenodo.org/record/3823718/files/figure.png" pageId="16" pageNumber="187" startId="16.[151,250,1236,1261]" targetBox="[151,1436,181,1210]" targetPageId="16">
