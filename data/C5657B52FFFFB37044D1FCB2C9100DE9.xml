<quantity id="8A3467A1FFFFB3704770F9ACCBAC0D51" box="[822,938,1637,1661]" metricMagnitude="2" metricUnit="m" metricValue="3.475" metricValueMax="6.49" metricValueMin="0.45999999999999996" pageId="99" pageNumber="100" unit="m" value="347.5" valueMax="649.0" valueMin="46.0">46–649 m</quantity>
