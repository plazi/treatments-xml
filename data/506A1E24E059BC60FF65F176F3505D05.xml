<taxonomicName id="1FC3D4B1E058BC60FE3EF44EF79D5C02" authority="Londt, 1983" authorityName="Londt" authorityYear="1983" box="[489,757,1546,1604]" class="Insecta" family="Asilidae" genus="Remotomyia" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="304" phylum="Arthropoda" rank="species" species="brunales" status="spec. nov .">
<emphasis id="EAB77320E058BC60FE3EF44EF79D5C02" box="[489,757,1546,1604]" italics="true" pageId="21" pageNumber="304">Remotomyia brunales</emphasis>
