<taxonomicName id="9B844D64FF82622BD7E5FE66FD80FE6B" authority="(Pallas, 1771)" authorityName="Pallas" authorityYear="1771" baseAuthorityName="Pallas" baseAuthorityYear="1771" box="[179,523,402,427]" class="Insecta" family="Acrididae" genus="Celes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="variabilis">
