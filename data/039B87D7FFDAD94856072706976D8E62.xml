<paragraph id="8B8D36C1FFDAD94B56072706947E8C3D" blockId="1.[1657,1918,1068,1106]" box="[1657,1918,1068,1106]" pageId="1" pageNumber="2">
<heading id="D0C581ADFFDAD94B56072706947E8C3D" box="[1657,1918,1068,1106]" centered="true" fontSize="10" level="2" pageId="1" pageNumber="2" reason="2">
<taxonomicName id="4C324D42FFDAD94B5607270695FF8C3D" authority="Parker, 1998" authorityName="Parker" authorityYear="1998" box="[1657,1791,1068,1106]" class="Ostracoda" family="Cypridinidae" genus="Lowrya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus" status="n.gen.">
<emphasis id="B946EAD3FFDAD94B5607270695FF8C3D" box="[1657,1791,1068,1106]" italics="true" pageId="1" pageNumber="2">Lowrya</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27557A8FFDAD94B576D2706947E8C3D" box="[1811,1918,1068,1106]" pageId="1" pageNumber="2" rank="genus">n.gen.</taxonomicNameLabel>
