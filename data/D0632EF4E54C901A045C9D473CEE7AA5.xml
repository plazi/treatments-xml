<paragraph id="8905C60E6DE8DAED1C0C556622491815" pageId="0" pageNumber="4692">
<taxonomicName id="8C0DDF6B6371296754234AE0E9B41ADA" class="Insecta" family="Mymaridae" genus="Alaptus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Alaptus" order="Hymenoptera" pageId="0" pageNumber="4692" phylum="Arthropoda" rank="genus">Alaptus</taxonomicName>
