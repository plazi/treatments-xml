<materialsCitation id="21D73CA77724FFDFFF62CB9BFEADFE6E" ID-GBIF-Occurrence="1809435692" collectingDate="1997-02-24" collectingDateMax="1997-03-14" collectingDateMin="1997-02-24" collectionCode="NHRS" collectorName="H. Hippa &amp; M. Jaschhof &amp; B. Viklund" country="Malaysia" county="Male" elevation="120" location="Ulu Gombak" municipality="Malay Peninsula" pageId="8" pageNumber="537" specimenCount="1" stateProvince="Selangor" typeStatus="holotype">
<emphasis id="A3CBEAE87724FFDFFF62CB9BFEECFE36" bold="true" box="[199,315,406,431]" pageId="8" pageNumber="537">
<typeStatus id="4E0488587724FFDFFF62CB9BFEE1FE36" box="[199,310,406,431]" pageId="8" pageNumber="537">Holotype</typeStatus>
.
</emphasis>
<collectingCounty id="78614E767724FFDFFEE2CB9BFE54FE36" box="[327,387,406,431]" pageId="8" pageNumber="537">Male</collectingCounty>
, 
