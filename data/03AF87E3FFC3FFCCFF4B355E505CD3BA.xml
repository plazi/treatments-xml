<taxonomicName id="4C064D76FFC1FFCCFDEC36EB505BD3D9" authority="Reuter, 1881" authorityName="Reuter" authorityYear="1881" box="[559,828,652,676]" class="Insecta" family="Reduviidae" genus="Isyndus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="413" phylum="Arthropoda" rank="species" species="pilosipes">
<emphasis id="B972EAE7FFC1FFCCFDEC36EB51C0D3D9" box="[559,679,652,676]" italics="true" pageId="3" pageNumber="413">I. pilosipes</emphasis>
