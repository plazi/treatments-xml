</taxonomicName>
and 
<taxonomicName id="4C064D76FFC1FFCCFCB536EB57E9D3D9" authority="Distant, 1919" authorityName="Distant" authorityYear="1919" box="[886,1166,652,676]" class="Insecta" family="Reduviidae" genus="Isyndus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="413" phylum="Arthropoda" rank="species" species="modestus">
