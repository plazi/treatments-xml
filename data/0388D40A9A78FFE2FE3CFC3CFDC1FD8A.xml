<taxonomicName id="4C211E9F9A78FFE1FEA3F902FD65F90E" authority="Kong et al., 2014" authorityName="Kong" authorityYear="2014" box="[276,713,1675,1698]" class="Ostracoda" family="Cyprididae" genus="Chrissia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="25" pageNumber="1849" phylum="Arthropoda" rank="species" species="dongqianhuensis">
