<paragraph id="6CFDA0404231A32E753A584D10BA03D9" pageId="181" pageNumber="182">
Subtribe 
<taxonomicName id="B9674B869AC78A9CBA4E72C257E95D59" authority="Bouchard, Loebl and Merkl, 2007" authorityName="Bouchard, Loebl and Merkl" authorityYear="2007" class="Insecta" family="Tenebrionidae" genus="Neopachypterina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neopachypterina" order="Coleoptera" pageId="181" pageNumber="182" phylum="Arthropoda" rank="subTribe" subTribe="Neopachypterina">
