<taxonomicName id="64D7DDA3773EFFFFFCF3FE9E08FB4B3F" authorityName="Nasarow" authorityYear="1936" box="[810,970,295,321]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="5" pageNumber="386" phylum="Chordata" rank="species" species="turanica">
