(section 
<taxonomicName id="64D7DDA3773EFFFFFBECFE9E0F914B3E" authorityName="Gray" authorityYear="1870" box="[1077,1184,295,321]" class="Anthozoa" family="Ellisellidae" genus="Viminella" kingdom="Animalia" order="Alcyonacea" pageId="5" pageNumber="386" phylum="Cnidaria" rank="genus">
