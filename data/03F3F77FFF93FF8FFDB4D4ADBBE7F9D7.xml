<paragraph id="8BE54669FF93FF8FFDB4D648BBE7F9D7" blockId="13.[533,1526,1247,1653]" pageId="13" pageNumber="14">
<taxonomicName id="4C5A3DEAFF93FF8FFDB4D648BD83F9F0" authorityName="Temminck" authorityYear="1838" box="[565,712,1594,1618]" class="Mammalia" family="Emballonuridae" genus="Saccolaimus" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="saccolaimus">
<emphasis id="B92E9A7BFF93FF8FFDB4D648BD0AF9F3" box="[565,577,1594,1617]" italics="true" pageId="13" pageNumber="14">S</emphasis>
. 
