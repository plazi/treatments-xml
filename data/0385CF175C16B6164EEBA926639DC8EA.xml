<taxonomicName id="4C2C05825C16B6164F79A96F6333C9F3" authority="Bertolani, 1981" authorityName="Bertolani" authorityYear="1981" box="[261,548,1132,1154]" class="Eutardigrada" family="Eohypsibiidae" genus="Amphibolus" kingdom="Animalia" order="Parachela" pageId="3" pageNumber="42" phylum="Tardigrada" rank="genus">
