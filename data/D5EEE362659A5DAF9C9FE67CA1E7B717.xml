<quantity id="6686458031BE262F89B57474B4291A1A" metricMagnitude="-3" metricUnit="m" metricValue="9.0" metricValueMax="11.0" metricValueMin="7.0" unit="mm" value="9.0" valueMax="11.0" valueMin="7.0">7-11 mm</quantity>
