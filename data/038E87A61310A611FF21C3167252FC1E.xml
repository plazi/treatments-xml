<emphasis id="B953EAA2131EA611FF21C1D0707FFE7E" bold="true" box="[192,375,387,411]" pageId="24" pageNumber="23">
<typeStatus id="549C8812131EA611FF21C1D0700CFE7E" box="[192,260,387,411]" pageId="24" pageNumber="23">Type</typeStatus>
locality.
</emphasis>
<collectingCountry id="F3307620131EA611FE9DC1D070EEFE7E" box="[380,486,387,411]" name="Brazil" pageId="24" pageNumber="23">BRAZIL</collectingCountry>
: 
<collectingRegion id="49E3F852131EA611FE0FC1D0736DFE7E" box="[494,613,387,411]" country="Brazil" name="Rondonia" pageId="24" pageNumber="23">Rondônia</collectingRegion>
; 
<quantity id="4CDF9B55131EA611FD8EC1D073BBFE7E" box="[623,691,387,411]" metricMagnitude="4" metricUnit="m" metricValue="6.2" pageId="24" pageNumber="23" unit="km" value="62.0">62km</quantity>
