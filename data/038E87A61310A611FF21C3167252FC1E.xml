<taxonomicName id="4C274D331311A61EFD27C71F724AF885" baseAuthorityName="Mabille" baseAuthorityYear="1888" box="[710,834,1867,1889]" class="Insecta" family="Hesperiidae" genus="Dyscophellus" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="erythras">
