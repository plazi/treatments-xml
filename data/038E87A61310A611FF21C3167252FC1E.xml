<taxonomicName id="4C274D331311A61EFDA5C6A773DFF8EC" box="[580,727,1780,1801]" class="Insecta" family="Hesperiidae" genus="Dyscophellus" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="genus">
