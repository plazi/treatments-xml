<collectingCounty id="62F94E3C131EA611FCFCC1D072ACFE7E" box="[797,932,387,411]" pageId="24" pageNumber="23">Ariquemes</collectingCounty>
, 
<collectingMunicipality id="6BFCACCA131EA611FC4CC1D072FEFE7E" box="[941,1014,387,411]" pageId="24" pageNumber="23">Linha</collectingMunicipality>
C-20, 7km (by road) east of route B- 65, 
<location id="8EF8606B131EA611FF0DC1F07328FE5E" LSID="urn:lsid:plazi:treatment:038E87A61310A611FF21C3167252FC1E:8EF8606B131EA611FF0DC1F07328FE5E" box="[236,544,419,443]" country="Brazil" county="Ariquemes" municipality="Linha" name="Fazenda Rancho Grande" pageId="24" pageNumber="23" stateProvince="Rondonia">Fazenda Rancho Grande</location>
, 
<elevation id="000AD183131EA611FDCAC1F073E4FE5E" box="[555,748,419,443]" metricMagnitude="2" metricUnit="m" metricValue="1.8" pageId="24" pageNumber="23" unit="m" value="180.0">
elevation 
<quantity id="4CDF9B55131EA611FD45C1F073E4FE5E" box="[676,748,419,443]" metricMagnitude="2" metricUnit="m" metricValue="1.8" pageId="24" pageNumber="23" unit="m" value="180.0">180m</quantity>
</elevation>
