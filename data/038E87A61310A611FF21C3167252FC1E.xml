<location id="8EF8606B131EA611FD16C1F07226FE5E" LSID="urn:lsid:plazi:treatment:038E87A61310A611FF21C3167252FC1E:8EF8606B131EA611FD16C1F07226FE5E" box="[759,814,419,443]" country="Brazil" county="Ariquemes" municipality="Linha" name="This" pageId="24" pageNumber="23" stateProvince="Rondonia">This</location>
