<taxonomicName id="4C7A4D1FB733FF8661787E1ED907FCDD" authority="(Peyr.) Thaxt." authorityName="Thaxt." baseAuthorityName="Peyr." box="[1068,1399,812,839]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Chitonomyces" kingdom="Fungi" order="Laboulbeniales" pageId="98" pageNumber="99" phylum="Ascomycota" rank="species" species="paradoxus">
