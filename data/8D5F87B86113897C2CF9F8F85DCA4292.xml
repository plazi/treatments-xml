<taxonomicName id="C2F64D2D6111897C28CFFA555AF64335" box="[1185,1436,1516,1539]" class="Insecta" family="Tortricidae" genus="Sorolopha" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="ferruginosa">
<emphasis id="3782EABC6111897C28CFFA555AF64335" box="[1185,1436,1516,1539]" italics="true" pageId="6" pageNumber="7">Sorolopha ferruginosa</emphasis>
