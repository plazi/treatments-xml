spp. 9. 
<taxonomicName id="C2F64D2D6111897C2F66FA555B014335" authority="Meyrick" authorityName="Meyrick" box="[776,1131,1516,1539]" class="Insecta" family="Tortricidae" genus="Sorolopha" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="camarotis">
