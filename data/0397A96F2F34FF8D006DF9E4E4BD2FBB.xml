<quantity id="4CC6B59C2F37FF8D031CFE8BE1972F08" box="[323,413,314,340]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="90" pageNumber="91" unit="cm" value="200.0">200 cm</quantity>
