<quantity id="4CE19B1EFFD2FF82FDA6F971FD3AD9DD" box="[616,671,1701,1727]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="27" pageNumber="488" unit="cm" value="1.0">1 cm</quantity>
