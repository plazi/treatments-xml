<quantity id="4CE19B1EFFD2FF82FAF8F955FAD8D9F9" box="[1334,1405,1665,1691]" metricMagnitude="-1" metricUnit="m" metricValue="1.0" pageId="27" pageNumber="488" unit="cm" value="10.0">10 cm</quantity>
