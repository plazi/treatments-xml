Shelf 
<quantity id="4CF91CE377D8FF23FEC3A1079100FD1E" box="[320,381,620,644]" metricMagnitude="1" metricUnit="m" metricValue="7.1" pageId="143" pageNumber="144" unit="m" value="71.0">71 m</quantity>
