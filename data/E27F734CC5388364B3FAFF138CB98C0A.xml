<taxonomicName id="ADD6B9D9C5388363B2B5F8548D6388F6" authorityName="Sundevall" authorityYear="1833" box="[455,565,1963,1985]" class="Arachnida" family="Theridiidae" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="150" phylum="Arthropoda" rank="family">Theridiidae</taxonomicName>
