<taxonomicNameLabel id="4391A333C5388363B110F8348DF988D7" box="[610,687,1995,2016]" pageId="59" pageNumber="150" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; C–D, 
<taxonomicName id="ADD6B9D9C5388363B185F8338C8788D6" authority="Lin &amp; Li" authorityName="Lin &amp; Li" authorityYear="2021" box="[759,977,1995,2017]" class="Arachnida" family="Theridiidae" genus="Episinus" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="150" phylum="Arthropoda" rank="species" species="jiangweni" status="sp. nov.">
<emphasis id="58A21E48C5388363B185F8338C2788D6" box="[759,881,1996,2017]" italics="true" pageId="59" pageNumber="150">E. jiangweni</emphasis>
Lin &amp; Li
</taxonomicName>
, 
<emphasis id="58A21E48C5388363B0AEF8348B7F88D7" bold="true" box="[988,1065,1995,2016]" pageId="59" pageNumber="150">
<taxonomicNameLabel id="4391A333C5388363B0AEF8348B7F88D7" box="[988,1065,1995,2016]" pageId="59" pageNumber="150" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; E–F. 
<taxonomicName id="ADD6B9D9C5388363B718F8338A6F88D6" authority="Lin &amp; Li" authorityName="Lin &amp; Li" authorityYear="2021" box="[1130,1337,1995,2017]" class="Arachnida" family="Theridiidae" genus="Episinus" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="150" phylum="Arthropoda" rank="species" species="tongyani" status="sp. nov.">
<emphasis id="58A21E48C5388363B718F8338B8F88D6" box="[1130,1241,1996,2017]" italics="true" pageId="59" pageNumber="150">E. tongyani</emphasis>
Lin &amp; Li
</taxonomicName>
, 
<emphasis id="58A21E48C5388363B636F8348AC788D7" bold="true" box="[1348,1425,1995,2016]" pageId="59" pageNumber="150">
<taxonomicNameLabel id="4391A333C5388363B636F8348AC788D7" box="[1348,1425,1995,2016]" pageId="59" pageNumber="150" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; G–H. 
<taxonomicName id="ADD6B9D9C5388363B3A5F8138E818736" authority="Lin &amp; Li" authorityName="Lin &amp; Li" authorityYear="2021" box="[215,471,2027,2049]" class="Arachnida" family="Theridiidae" genus="Meotipa" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="150" phylum="Arthropoda" rank="species" species="luoqiae" status="sp. nov.">
