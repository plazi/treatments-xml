<taxonomicName id="4C714D30FFBC8904FFA7FD84CFEEFDBF" authority="Peters, 1882" authorityName="Peters" authorityYear="1882" box="[123,391,605,626]" class="Amphibia" family="Hylidae" genus="Hylonomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="83" pageNumber="84" phylum="Chordata" rank="genus">
