<taxonomicName id="4C16BABF9510FFD7FC30FD0B7291FCDB" authorityName="Gillette" authorityYear="1896" box="[322,508,655,681]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="atripes">
