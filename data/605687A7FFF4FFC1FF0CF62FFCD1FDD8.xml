<taxonomicName id="2FFF4D32FFF4FFC0FAEBF2DAF887F952" baseAuthorityName="Hewitson" baseAuthorityYear="1874" box="[1319,1440,1758,1782]" class="Insecta" family="Lycaenidae" genus="Electrostrymon" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="angelia">
