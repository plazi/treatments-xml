<taxonomicName id="2FFF4D32FFF4FFC0FE69F2FAFF08F8B3" box="[421,559,1791,1815]" class="Magnoliopsida" family="Sapotaceae" kingdom="Plantae" order="Ericales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Sapotaceae</taxonomicName>
