<collectingCountry id="F9CA79CAB653FFF1FB9EF9BA06D5F9E0" box="[1125,1182,1567,1590]" name="Laos" pageId="32" pageNumber="33">Laos</collectingCountry>
, 
<collectingCountry id="F9CA79CAB653FFF1FB54F9B80752F9E0" box="[1199,1305,1565,1590]" name="Malaysia" pageId="32" pageNumber="33">Malaysia</collectingCountry>
, 
<collectingCountry id="F9CA79CAB653FFF1FAD2F9BA07D3F9E1" box="[1321,1432,1567,1591]" name="Myanmar" pageId="32" pageNumber="33">Myanmar</collectingCountry>
, 
<collectingCountry id="F9CA79CAB653FFF1FF6CF9E40353F98C" box="[151,280,1601,1626]" name="Philippines" pageId="32" pageNumber="33">Philippines</collectingCountry>
