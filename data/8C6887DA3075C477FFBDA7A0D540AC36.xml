<taxonomicName id="C3C14D4F3075C477FFA4A07ED7A8AE7B" authority="Kepner &amp; Carter, 1931" authorityName="Kepner &amp; Carter" authorityYear="1931" class="Catenulida" family="Stenostomidae" genus="Stenostomum" kingdom="Animalia" order="Gentianales" pageId="15" pageNumber="52" phylum="Platyhelminthes" rank="species" species="membranosum">
