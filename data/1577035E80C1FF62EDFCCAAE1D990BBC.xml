<quantity id="5A261FAD80C1FF62E87ECADD1C120B70" box="[1301,1359,1268,1292]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="197" pageNumber="198" unit="m" value="10.0">10 m</quantity>
