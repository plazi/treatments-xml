<subSubSection id="0D8ECCB9CC66FFDFB191FEFCFBC41D13" box="[151,1026,333,360]" pageId="41" pageNumber="242" type="materials_examined">
<paragraph id="452B9F32CC66FFDFB191FEFCFBC41D13" blockId="41.[151,1436,333,648]" box="[151,1026,333,360]" pageId="41" pageNumber="242">
<emphasis id="77E04320CC66FFDFB191FEFCFE471D13" bold="true" box="[151,385,333,360]" pageId="41" pageNumber="242">Material examined.</emphasis>
