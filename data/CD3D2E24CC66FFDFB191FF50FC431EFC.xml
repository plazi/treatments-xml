<taxonomicName id="8294E4B1CC66FFDFB259FEFCFC1D1D1C" authorityName="Lamarck" authorityYear="1816" box="[863,987,333,359]" class="Stenolaemata" family="Tubuliporidae" genus="Tubulipora" kingdom="Animalia" order="Cyclostomatida" pageId="41" pageNumber="242" phylum="Bryozoa" rank="genus">
