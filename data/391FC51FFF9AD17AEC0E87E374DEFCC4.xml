<paragraph id="B1097409FF9AD178EDDB815076C4FB2E" blockId="8.[138,1185,1116,1205]" pageId="8" pageNumber="2319">
<taxonomicName id="76B60F8AFF9AD178EDDB81507745FBEF" authorityName="Gai and Scheller" authorityYear="2007" box="[138,372,1116,1140]" class="Pauropoda" family="Pauropodidae" genus="Allopauropus" kingdom="Animalia" order="Tetramerocerata" pageId="8" pageNumber="2319" phylum="Arthropoda" rank="species" species="apicalis">
