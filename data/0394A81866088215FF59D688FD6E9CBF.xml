<taxonomicName id="4C3D628D66088215FF59D688FDF89EE3" authority="Cooper, 1981" authorityName="Cooper" authorityYear="1981" box="[151,594,1454,1480]" class="Rhynchonellata" family="Dyscoliidae" genus="Xenobrochus" kingdom="Animalia" order="Terebratulida" pageId="10" pageNumber="11" phylum="Brachiopoda" rank="species" species="australis">
<emphasis id="B949C51C66088215FF59D688FDF89EE3" bold="true" box="[151,594,1454,1480]" pageId="10" pageNumber="11">
<emphasis id="B949C51C66088215FF59D688FE0D9EE3" bold="true" box="[151,423,1454,1480]" italics="true" pageId="10" pageNumber="11">Xenobrochus australis</emphasis>
<bibRefCitation id="EFAC64FF66088215FE61D688FDF89EE3" author="Cooper" box="[431,594,1454,1480]" pageId="10" pageNumber="31" refString="Cooper, G. A. (1981) Brachiopods from the Southern Indian Ocean (Recent). Smithsonian Contributions to Paleobiology, 43, 1 - 93." type="journal article" year="1981">
Cooper, 
<number id="3DF50B2966088215FDD6D688FDF89EE3" box="[536,594,1454,1480]" pageId="10" pageNumber="11" value="1981.0">1981</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
