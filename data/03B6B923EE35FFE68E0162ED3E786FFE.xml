FAMILY 
<taxonomicName id="4C1F73B6EE35FFE18EE262ED394E6D87" authorityName="Dall" authorityYear="1882" box="[425,596,1228,1252]" class="Gastropoda" family="Cocculinidae" kingdom="Animalia" order="Cocculinida" pageId="3" pageNumber="450" phylum="Mollusca" rank="family">COCCULINIDAE</taxonomicName>
