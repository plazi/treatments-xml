<taxonomicName id="4C6C9DA2FFFAFFD3FD1DF88FD298F882" authority="" authorityName="Chamberlin &amp; Ivie" authorityYear="1940" box="[730,951,1835,1858]" class="Arachnida" family="Hahniidae" genus="Cicurina" kingdom="Animalia" order="Araneae" pageId="44" pageNumber="45" phylum="Arthropoda" rank="subGenus" subGenus="Cicurella">
<emphasis id="B9183A33FFFAFFD3FD1DF88FD216F882" box="[730,825,1835,1858]" italics="true" pageId="44" pageNumber="45">Cicurina</emphasis>
(
<emphasis id="B9183A33FFFAFFD3FC8CF88FD280F882" box="[843,943,1835,1858]" italics="true" pageId="44" pageNumber="45">Cicurella</emphasis>
)
