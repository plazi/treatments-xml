<taxonomicName id="AC791C654EC81CEE49A85039C8A750A8" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus virginiana" order="Fagales" pageId="2" pageNumber="11" phylum="Tracheophyta" rank="species" species="virginiana">Quercus virginiana</taxonomicName>
and 
<taxonomicName id="58B7EA73EE184DC6EDD91033AF87107A" authority="Nalepa, 1887" authorityName="Nalepa" authorityYear="1887" class="Arachnida" family="Eriophyidae" genus="Cecidophyes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cecidophyes tristernalis" order="Trombidiformes" pageId="2" pageNumber="11" phylum="Arthropoda" rank="species" species="tristernalis">Cecidophyes tristernalis</taxonomicName>
