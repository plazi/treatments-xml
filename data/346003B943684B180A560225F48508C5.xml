<taxonomicName id="DCEDE4D10FD844D72C7A8AF4C8A7360D" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus cerris" order="Fagales" pageId="2" pageNumber="11" phylum="Tracheophyta" rank="species" species="cerris">Quercus cerris</taxonomicName>
