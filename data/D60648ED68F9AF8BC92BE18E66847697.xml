<taxonomicName id="8B2FC3E7562C6E4D007A45A8180BBDB6" authority="A.Berlese, 1904" authorityName="A.Berlese" authorityYear="1904" class="Arachnida" family="Ameroseiidae" genus="Ameroseius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ameroseius lanceosetis" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="lanceosetis">Ameroseius lanceosetis</taxonomicName>
