<taxonomicName id="05B6248DF34F66EC10315AA637232058" authority="A.Berlese, 1904" authorityName="A.Berlese" authorityYear="1904" class="Arachnida" family="Ameroseiidae" genus="Ameroseius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ameroseius gracilis" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gracilis">Ameroseius gracilis</taxonomicName>
