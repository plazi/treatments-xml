<paragraph id="1C3DC3E0B9707A15EDD08827D089E1B4" pageId="0" pageNumber="5">
<taxonomicName id="C6B2C05C6010EEE7C037C1248FA0380B" LSID="http://zoobank.org/EFE1EECC-26D4-4839-977E-35004769C41A" authority="Awad" authorityName="Awad" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Synopeas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synopeas valavala" order="Hymenoptera" pageId="0" pageNumber="5" phylum="Arthropoda" rank="species" species="valavala" status="sp. nov.">Synopeas valavala Awad</taxonomicName>
