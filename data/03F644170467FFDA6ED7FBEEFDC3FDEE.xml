. 
<taxonomicName id="4C5F8E820464FFDB6F00F9D7FDF8F9A5" authorityName="Mound" authorityYear="2020" baseAuthorityName="Karny" baseAuthorityYear="2020" box="[320,603,1628,1652]" class="Insecta" family="Phlaeothripidae" genus="Ocnothrips" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="589" phylum="Arthropoda" rank="species" species="cochinchinensis">
<emphasis id="B92B29130464FFDB6F00F9D7FDF8F9A5" box="[320,603,1628,1652]" italics="true" pageId="2" pageNumber="589">Ocnothrips cochinchinensis</emphasis>
