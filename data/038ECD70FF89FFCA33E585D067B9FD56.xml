<date id="FF995AA6FF89FFC832D684AC6169FE46" box="[443,597,392,414]" pageId="4" pageNumber="47" value="2018-08-15">15 August 2018</date>
, 
<collectionCode id="ED36E4A3FF89FFC8313284AC614DFE46" box="[607,625,392,413]" country="Germany" lsid="urn:lsid:biocol.org:col:15534" name="Botanischer Garten und Botanisches Museum Berlin-Dahlem, Zentraleinrichtung der Freien Universitaet" pageId="4" pageNumber="47" type="Herbarium">B</collectionCode>
. 
<collectionCode id="ED36E4A3FF89FFC8311984AC61B5FE46" box="[628,649,392,413]" country="United Kingdom" lsid="urn:lsid:biocol.org:col:15867" name="Royal Botanic Gardens" pageId="4" pageNumber="47" type="Herbarium">K</collectionCode>
. Cui, 
<emphasis id="B953A074FF89FFC831AC84AC601AFE46" box="[705,806,392,413]" italics="true" pageId="4" pageNumber="47">Cui 17023</emphasis>
(
<typeStatus id="549CC2C4FF89FFC8305F84AC60B3FE45" box="[818,911,392,414]" pageId="4" pageNumber="47">Holotype</typeStatus>
