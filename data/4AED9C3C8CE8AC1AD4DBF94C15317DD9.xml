<taxonomicName id="373D35173681EA33556E8B9375DA320D" class="Ascomycetes" family="Cladoniaceae" genus="Cladonia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lecanorales" phylum="Ascomycota" rank="genus">Cladonia</taxonomicName>
