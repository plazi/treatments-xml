<taxonomicName id="4C27A985FFAFFF9779B4744C1A06FDEC" authority="NOSKIEWICZ" authorityName="NOSKIEWICZ" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="467" phylum="Arthropoda" rank="species" species="brumalis">
