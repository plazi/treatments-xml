NOSKIEWICZ
</taxonomicName>
, 
<taxonomicName id="4C27A985FFAFFF97784F74331843FDEC" authority="NOSKIEWICZ" authorityName="NOSKIEWICZ" box="[601,850,590,612]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="467" phylum="Arthropoda" rank="species" species="reinigi">
