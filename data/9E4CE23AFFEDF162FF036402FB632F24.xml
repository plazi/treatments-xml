, as 
<taxonomicName id="D1E528AFFFEDF161FAB466D7FEFD28B4" baseAuthorityName="Alder" baseAuthorityYear="1860" class="Hydrozoa" family="Tiarannidae" genus="Stegopoma" kingdom="Animalia" order="Leptothecata" pageId="33" pageNumber="34" phylum="Cnidaria" rank="species" species="fastigiata">
<emphasis id="24918F3EFFEDF161FAB466D7FEFD28B4" italics="true" pageId="33" pageNumber="34">Stegopoma fastigiata</emphasis>
</taxonomicName>
; 
<bibRefCitation id="72742EDDFFEDF161FE8466FBFE5C28B4" author="Kramp, P. L." box="[272,416,1720,1747]" pageId="33" pageNumber="34" pagination="1 - 283" refId="ref137835" refString="Kramp, P. L. (1959) The hydromedusae of the Atlantic Ocean and adjacent waters. Dana-Report, 46, 1 - 283." type="journal article" year="1959">Kramp 1959</bibRefCitation>
