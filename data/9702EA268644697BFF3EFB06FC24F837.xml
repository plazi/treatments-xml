<taxonomicName id="D8AB20B38645697BFC81F994FC6FF9AE" box="[808,1015,1616,1640]" class="Magnoliopsida" family="Euphorbiaceae" genus="Alchornea" kingdom="Plantae" order="Malpighiales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="species" species="pearcei">
