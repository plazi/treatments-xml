<number id="3DCF24B6FF96FFDEFCDCFDF3EF1CFD7E" box="[829,861,626,652]" pageId="20" pageNumber="21" value="93.0">93</number>
, ML 
<number id="3DCF24B6FF96FFDEFC7FFDF3EF95FD7E" box="[926,980,626,652]" pageId="20" pageNumber="21" value="0.82">0.82</number>
