<paragraph id="8BE08B6AFC55FFDCF346F96FFB3AF8E3" blockId="20.[340,1247,1689,1798]" pageId="20" pageNumber="1029">
<taxonomicName id="4C5FF0E9FC55FFDCF346F96FFE0CF94A" authorityName="SCHMID" authorityYear="1958" box="[340,489,1689,1711]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="20" pageNumber="1029" phylum="Arthropoda" rank="species" species="bogambara">
