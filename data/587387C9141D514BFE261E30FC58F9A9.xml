<collectionCode id="B6CBAE1A141D5148FD421EB3FCD2FC44" box="[712,776,975,1001]" pageId="37" pageNumber="38">RAM</collectionCode>
locality 
<accessionNumber id="CF89AB3C141D5148FCE21EB3FC1FFC44" box="[872,965,975,1001]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/V94014" pageId="37" pageNumber="38">V94014</accessionNumber>
