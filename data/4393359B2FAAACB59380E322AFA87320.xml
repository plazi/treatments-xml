<taxonomicName id="88B4E49141D03D64E170B8DCB58845E7" class="Gastropoda" family="Gastrocoptidae" genus="Tonkinospira" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tonkinospira pauperrima" order="Stylommatophora" pageId="9" pageNumber="32" phylum="Mollusca" rank="species" species="pauperrima">Tonkinospira pauperrima</taxonomicName>
