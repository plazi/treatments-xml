<taxonomicName id="76FE65E74DD091CE42DD178D5CFD9360" authorityName="With" authorityYear="1904" class="Arachnida" family="Opilioacaridae" genus="Eucarus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Opilioacarida" pageId="783" pageNumber="784" phylum="Arthropoda" rank="genus">
