<taxonomicName id="CE7FF172AD21FCBFA91D982726D251C2" authorityName="Bates" authorityYear="1872" class="Insecta" family="Carabidae" genus="Lachnaces" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="783" pageNumber="784" phylum="Arthropoda" rank="genus">
