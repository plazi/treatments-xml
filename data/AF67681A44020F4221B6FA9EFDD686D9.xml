<subSubSection id="6FD48A8744020F4320D1F995FE618092" lastPageId="18" lastPageNumber="2627" pageId="16" pageNumber="2625" type="description">
<paragraph id="2771D90C44020F4120D1F995FB5D83C4" blockId="16.[212,1172,1595,1692]" pageId="16" pageNumber="2625">
<taxonomicName id="E0CEA28F44020F4120D1F995FEBB8324" authorityName="Tikader" authorityYear="1973" box="[212,370,1635,1659]" class="Arachnida" family="Salticidae" genus="Myrmarachne" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="2625" phylum="Arthropoda" rank="species" species="orientales">
