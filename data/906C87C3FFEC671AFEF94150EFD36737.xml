<taxonomicName id="DFC54D56FFED671AFF2B4039EF4E647B" authorityName="Smith" authorityYear="2008" box="[219,349,1314,1335]" class="Insecta" family="Curculionidae" genus="Aulacus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="280" phylum="Arthropoda" rank="species" species="fascius">
