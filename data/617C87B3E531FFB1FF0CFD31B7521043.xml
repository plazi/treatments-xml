<emphasis id="DBA1EAB7E531FFB1FF0CFD78B2471526" bold="true" box="[151,290,760,782]" pageId="1" pageNumber="239">Type species.</emphasis>
<taxonomicName id="2ED54D26E531FFB1FEB2FD78B1F91526" authority="Frieser, 2010: 18" authorityName="Frieser" authorityPageNumber="18" authorityYear="2010" box="[297,668,760,782]" class="Insecta" family="Anthribidae" genus="Adapterops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="239" phylum="Arthropoda" rank="species" species="nasalis">
