<taxonomicName id="4C294D361800F32C526E7AF465739A7F" box="[844,1056,1021,1045]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="404" phylum="Arthropoda" rank="species" species="agassizii">
