; maximum width of head 
<quantity id="284F9B79432F5A50FC232DF0C669A397" box="[939,1031,150,177]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="6" pageNumber="339" unit="mm" value="1.5">1.5 mm</quantity>
; median dorsal length of pronotum 
<quantity id="284F9B79432F5A50FF1F2DDCC29FA3F3" box="[151,241,186,213]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="6" pageNumber="339" unit="mm" value="1.2">1.2 mm</quantity>
