<typeStatus id="5482EEF38F4DA757FEFAF1173DC6FE8D" box="[338,384,354,377]" pageId="34" pageNumber="31">type</typeStatus>
material of 
<taxonomicName id="4C392BD28F4DA757FDADF1173F0DFE8D" authorityName="Pic" authorityYear="1906" box="[517,843,354,377]" class="Insecta" family="Chrysomelidae" genus="Labidostomis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="31" phylum="Arthropoda" rank="variety" species="bigemina" variety="semidetciens">
