<quantity id="4CD09B77FF9E630BB837FE75C3AAFE7F" box="[625,690,481,502]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="1462" unit="m" value="2000.0">2000m</quantity>
