</taxonomicName>
for 
<taxonomicName id="3190C390FFB3416AFD1D201C4EC0F8E2" authority="Kimmins, 1938" authorityName="Kimmins" authorityYear="1938" box="[694,1164,1836,1862]" class="Insecta" family="Ascalaphidae" genus="Ptyngidricerus" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="species" species="iranensis">
