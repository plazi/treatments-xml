<taxonomicName id="3190C390FFB3416AFC75208D4EDCF872" authorityName="Abraham &amp; Meszaros" authorityYear="2002" box="[990,1168,1980,2006]" class="Insecta" family="Ascalaphidae" genus="Ptyngidricerus" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="species" species="pakistanensis">
<emphasis id="C4E46401FFB3416AFC75208D4EDCF872" box="[990,1168,1980,2006]" italics="true" pageId="41" pageNumber="442">P. pakistanensis</emphasis>
</taxonomicName>
and 
<taxonomicName id="3190C390FFB3416AFB6F208D4F73F872" authority="Abraham &amp; Meszaros, 2002" authorityName="Abraham &amp; Meszaros" authorityYear="2002" baseAuthorityName="Tjeder and Waterston" baseAuthorityYear="1977" box="[1220,1343,1981,2006]" class="Insecta" family="Ascalaphidae" genus="Omanoidricerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="species" species="venustus">
<emphasis id="C4E46401FFB3416AFB6F208D4F73F872" box="[1220,1343,1981,2006]" italics="true" pageId="41" pageNumber="442">P. venustus</emphasis>
</taxonomicName>
in 
<collectingCountry id="8E87F883FFB3416AFACA208C4A8BF85F" name="Pakistan" pageId="41" pageNumber="442">Pakistan</collectingCountry>
, which are narrowly distributed toward the northeastern part of 
<collectingCountry id="8E87F883FFB3416AFC3920D049B9F85E" box="[914,1013,2016,2042]" name="Pakistan" pageId="41" pageNumber="442">Pakistan</collectingCountry>
