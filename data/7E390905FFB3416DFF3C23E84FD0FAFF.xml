<taxonomicName id="3190C390FFB3416AFBED20A84EB2F816" authority="Abraham &amp; Meszaros, 2002" authorityName="Abraham &amp; Meszaros" authorityYear="2002" box="[1094,1278,1944,1970]" class="Insecta" family="Ascalaphidae" genus="Omanoidricerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="genus">
<emphasis id="C4E46401FFB3416AFBED20A84EB2F816" box="[1094,1278,1944,1970]" italics="true" pageId="41" pageNumber="442">Omanoidricerus</emphasis>
</taxonomicName>
to be a junior synonym of 
<taxonomicName id="3190C390FFB3416AFE8A208C4B8BF872" authorityName="van der Weele" authorityYear="1909" box="[289,455,1980,2006]" class="Insecta" family="Ascalaphidae" genus="Ptyngidricerus" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="genus">
<emphasis id="C4E46401FFB3416AFE8A208C4B8BF872" box="[289,455,1980,2006]" italics="true" pageId="41" pageNumber="442">Ptyngidricerus</emphasis>
</taxonomicName>
. 
<taxonomicName id="3190C390FFB3416AFE7E208C4837F872" authorityName="van der Weele" authorityYear="1909" box="[469,635,1980,2006]" class="Insecta" family="Ascalaphidae" genus="Ptyngidricerus" kingdom="Animalia" order="Neuroptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="genus">
<emphasis id="C4E46401FFB3416AFE7E208C4837F872" box="[469,635,1980,2006]" italics="true" pageId="41" pageNumber="442">Ptyngidricerus</emphasis>
</taxonomicName>
