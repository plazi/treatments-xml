<number id="394F24FB246DFFEFFCD70BD1E35CF8FB" box="[855,886,1789,1813]" pageId="7" pageNumber="423" value="23.0">23</number>
</figureCitation>
. Width across eyes 
<quantity id="487F9B39246DFFEFFBD20BD0E493F8FA" box="[1106,1209,1788,1813]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="7" pageNumber="423" unit="mm" value="0.15">0.15 mm</quantity>
. Pronotum 
<quantity id="487F9B39246DFFEFFAB90BD1E5B7F8FA" box="[1337,1437,1789,1813]" metricMagnitude="-4" metricUnit="m" metricValue="1.7999999999999998" pageId="7" pageNumber="423" unit="mm" value="0.18">0.18 mm</quantity>
