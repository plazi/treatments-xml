<quantity id="487F9B39246DFFEFFBA70A0DE4B8F8D6" box="[1063,1170,1825,1849]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="7" pageNumber="423" unit="mm" value="0.34">0.34 mm</quantity>
