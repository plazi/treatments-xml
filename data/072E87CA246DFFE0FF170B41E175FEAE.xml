<quantity id="487F9B39246DFFEFFF620A0DE167F8D6" box="[226,333,1825,1849]" metricMagnitude="-4" metricUnit="m" metricValue="1.7" pageId="7" pageNumber="423" unit="mm" value="0.17">0.17 mm</quantity>
