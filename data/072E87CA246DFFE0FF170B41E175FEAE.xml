<quantity id="487F9B39246DFFEFFB660A0DE57BF8D6" box="[1254,1361,1825,1849]" metricMagnitude="-4" metricUnit="m" metricValue="2.3" pageId="7" pageNumber="423" unit="mm" value="0.23">0.23 mm</quantity>
