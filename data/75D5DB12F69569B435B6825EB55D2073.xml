<taxonomicName id="3AE54C586FADF72D5FAF81BF20BC4E03" authority="Adans." class="Liliopsida" family="Poaceae" genus="Calamagrostis" kingdom="Plantae" order="Poales" pageId="null" pageNumber="265" phylum="Tracheophyta" rank="genus">
