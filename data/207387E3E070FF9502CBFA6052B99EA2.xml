<paragraph id="A86536F5E070FF94029BF9525001981F" blockId="4.[151,1437,1710,2025]" pageId="4" pageNumber="76">
<materialsCitation id="18B23CA8E070FF94029BF9525005981F" ID-GBIF-Occurrence="3091107330" country="Ecuador" location="Rio Cristal" pageId="4" pageNumber="76" specimenCount="2" stateProvince="Bolivar" typeStatus="holotype">
<emphasis id="9AAEEAE7E070FF94029BF952506E99C7" bold="true" box="[199,363,1746,1772]" pageId="4" pageNumber="76">
<typeStatus id="77618857E070FF94029BF952500799C7" box="[199,258,1746,1772]" pageId="4" pageNumber="76">Type</typeStatus>
