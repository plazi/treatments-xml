</materialsCitation>
.”
</subSubSection>
<subSubSection id="C30E653FC668A21AF562F64EF4824996" pageId="30" pageNumber="257" type="discussion">
<taxonomicName id="4C144D37C668A21AF562F64EF3D04ABD" baseAuthorityName="Waterhouse" baseAuthorityYear="1843" box="[2714,3135,2516,2555]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="30" pageNumber="257" phylum="Chordata" rank="species" species="pygmaeus">
