, 
<emphasis id="B960EAA6C668A21AF3DBF4DDF3764828" bold="true" box="[3107,3225,2887,2926]" pageId="30" pageNumber="257">
<collectingRegion id="49D0F856C668A21AF3DBF4DDF3764828" box="[3107,3225,2887,2926]" country="Philippines" name="Bohol" pageId="30" pageNumber="257">Bohol</collectingRegion>
