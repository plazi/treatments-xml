<collectingCountry id="59DF764E517C2265FDCE8521FDBA221C" box="[529,586,1614,1640]" name="Cyprus" pageId="1" pageNumber="46">Chip</collectingCountry>
– 
<collectingCountry id="59DF764E517C2265FD858521FD63221C" box="[602,659,1614,1640]" name="Cyprus" pageId="1" pageNumber="46">Chip</collectingCountry>
