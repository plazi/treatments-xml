, 
<taxonomicName id="E6C84D5D517C2265FF488547FE712235" box="[151,385,1576,1601]" class="Insecta" family="Ceratopogonidae" genus="Atrichopogon" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="46" phylum="Arthropoda" rank="species" species="argus">
