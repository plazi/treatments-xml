<emphasis id="63EBF0CDAB54955FD513D2D59D302438" bold="true" pageId="0" pageNumber="39">Figure 3.</emphasis>
<taxonomicName id="E9D6B44ADFA2C84F36635DD65AE2CFAE" authorityName="Y. M. Ju &amp; J. D. Rogers, Nova Hedwigia 74 (1 - 2): 90" authorityYear="2002" baseAuthorityName="Schwein." class="Sordariomycetes" family="Xylariaceae" genus="Nemania" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Nemania caries" order="Xylariales" pageId="0" pageNumber="39" phylum="Ascomycota" rank="species" species="caries">
