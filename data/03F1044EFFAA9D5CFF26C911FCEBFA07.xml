<taxonomicName id="4C58CEDBFFAA9D5CFE73C87DFDB5FAE3" authority="Cuvier, 1817" authorityName="Cuvier" authorityYear="1817" box="[450,585,1361,1385]" class="Reptilia" family="Gekkonidae" genus="Ascalabotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="1" pageNumber="366" phylum="Chordata" rank="genus">
