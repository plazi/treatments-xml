<taxonomicName id="4C624D1CFFD1FF92FDFEF8BD9BD2E63B" authorityName="Robineau-Desvoidy" authorityYear="1863" box="[629,731,1869,1894]" class="Insecta" family="Tachinidae" genus="Lespesia" kingdom="Animalia" order="Diptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="genus">
<emphasis id="B916EA8DFFD1FF92FDFEF8BD9BD2E63B" box="[629,731,1869,1894]" italics="true" pageId="96" pageNumber="97">Lespesia</emphasis>
