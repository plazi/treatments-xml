<taxonomicName id="4C624D1CFFD1FF92FE97F8F39B75E641" authority="Macquart, 1834" authorityName="Macquart" authorityYear="1834" box="[284,636,1794,1820]" class="Insecta" family="Tachinidae" genus="Erycia" kingdom="Animalia" order="Diptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="ciliata">
