<paragraph id="8BC83680BE6104467BAE3D62FB8F8809" blockId="88.[702,1156,1096,1148]" box="[743,1115,1096,1119]" pageId="88" pageNumber="88">
<taxonomicName id="4C774D03BE6104467BAE3D62FC138808" box="[743,967,1096,1119]" class="Insecta" family="Miridae" genus="Grandivesica" kingdom="Animalia" order="Hemiptera" pageId="88" pageNumber="88" phylum="Arthropoda" rank="species" species="agnew" status="new species">
<emphasis id="B903EA92BE6104467BAE3D62FC138808" bold="true" box="[743,967,1096,1119]" italics="true" pageId="88" pageNumber="88">Grandivesica agnew</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A23057E9BE6104467A9E3D63FB8F8809" box="[983,1115,1097,1119]" pageId="88" pageNumber="88" rank="species">new species</taxonomicNameLabel>
