<taxonomicName id="6DD60FC7FF85E96C4E00F8F8FD7E2B07" box="[609,767,1871,1896]" class="Arachnida" family="Thomisidae" genus="Lysiteles" kingdom="Animalia" order="Araneae" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="clavellatus">
