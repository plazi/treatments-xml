<taxonomicName id="0985E04B0865BC9D857AC5D39BACEF0F" authorityName="Deeleman-Reinhold" authorityYear="2001" class="Arachnida" family="Clubionidae" genus="Nusatidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nusatidia" order="Araneae" pageId="0" pageNumber="73" phylum="Arthropoda" rank="genus">Nusatidia</taxonomicName>
