<taxonomicName id="4C774D29FF91D342FAAEFF7617D8FF21" authorityName="Szelegiewicz" authorityYear="1962" box="[1304,1436,187,213]" class="Insecta" family="Aphididae" genus="Uroleucon" kingdom="Animalia" order="Aphidomorpha" pageId="10" pageNumber="558" phylum="Arthropoda" rank="species" species="altaicum">
