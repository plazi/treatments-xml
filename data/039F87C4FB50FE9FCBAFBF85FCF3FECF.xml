<emphasis id="B942EAC0FB53FE9ECA1ABCC5FE00FA5E" box="[290,488,1502,1524]" italics="true" pageId="310" pageNumber="311">
<taxonomicName id="4C364D51FB53FE9ECA1ABCC5FE72FA5E" box="[290,410,1502,1524]" class="Gastropoda" family="Drilliidae" genus="Splendrillia" kingdom="Animalia" order="Neogastropoda" pageId="310" pageNumber="311" phylum="Mollusca" rank="genus">Splendrillia</taxonomicName>
<taxonomicName id="4C364D51FB53FE9ECAA7BCC5FE00FA5E" box="[415,488,1502,1524]" pageId="310" pageNumber="311" rank="species" species="subtilis" status="new species">subtilis</taxonomicName>
</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A27157BBFB53FE9ECACDBCC4FD99FA5E" box="[501,625,1502,1524]" pageId="310" pageNumber="311" rank="species">new species</taxonomicNameLabel>
