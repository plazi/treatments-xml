<taxonomicName id="4C6F4D55FFE9FFDFFE4BF650A7D7FCEA" box="[423,713,779,800]" class="Insecta" family="Pamphagidae" genus="Humphaplotropis" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="138" phylum="Arthropoda" rank="species" species="hebeiensis" status="sp. nov">
