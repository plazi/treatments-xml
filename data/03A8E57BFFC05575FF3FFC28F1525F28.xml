<taxonomicName id="4C012FEEFFC05575FF3FFBC2F72A5E14" authority="Sharp 1889: 289" authorityName="Sharp" authorityYear="1889" box="[151,568,1032,1056]" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="299" phylum="Arthropoda" rank="species" species="similaris">
