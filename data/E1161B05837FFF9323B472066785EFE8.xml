<number id="DF77B834837AFF9323C574D564E2EFB0" box="[230,244,404,429]" pageId="6" pageNumber="430" value="2.0">2</number>
/ 
<number id="DF77B834837AFF9323DE74D5651FEFB0" box="[253,265,404,429]" pageId="6" pageNumber="430" value="3.0">3</number>
