<emphasis id="98DBF9547855FF21CC87FC6CFD06C837" box="[423,696,1006,1027]" italics="true" pageId="43">
<taxonomicName id="6DAF5EC57855FF21CC87FC6CFD0AC837" authority="ENGEL, 2001" authorityName="Engel" authorityYear="2001" box="[423,692,1006,1027]" class="Insecta" family="Paleomelittidae" genus="Paleomelitta" kingdom="Animalia" order="Hymenoptera" pageId="43" phylum="Arthropoda" rank="species" species="nigripennis" status="new species">Paleomelitta nigripennis</taxonomicName>
