<normalizedToken id="6D560EE0B6C6481B46A4E64D026B020F" originalValue="arvénsis" pageId="null" pageNumber="195">arvensis</normalizedToken>
<authorityName id="064E1B47B1D3CA006CFC4DB6D0156FF0" pageId="null" pageNumber="195">L.</authorityName>
