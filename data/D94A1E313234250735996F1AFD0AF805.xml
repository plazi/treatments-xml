<number id="E72BBD003235250434696F6EFE37F8A0" box="[359,409,1865,1891]" pageId="9" pageNumber="10" value="11.6">11.6</number>
(
<number id="E72BBD003235250434A76F6EFE7FF8A0" box="[425,465,1865,1891]" pageId="9" pageNumber="10" value="3.2">3.2</number>
