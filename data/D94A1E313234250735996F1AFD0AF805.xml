<number id="E72BBD003235250434D36F6EFDABF8A0" box="[477,517,1865,1891]" pageId="9" pageNumber="10" value="3.6">3.6</number>
, 
<number id="E72BBD0032352504371E6F6EFD99F8A0" box="[528,567,1865,1891]" pageId="9" pageNumber="10" value="3.3">3.3</number>
