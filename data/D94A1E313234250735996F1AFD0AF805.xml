<number id="E72BBD003236250731226E4BFBFDF945" box="[1068,1107,1644,1670]" pageId="10" pageNumber="11" value="3.2">3.2</number>
, 
<number id="E72BBD003236250731516E4BFB29F945" box="[1119,1159,1644,1670]" pageId="10" pageNumber="11" value="1.9">1.9</number>
); IV: 
<number id="E72BBD003236250731C96E4BFB54F945" box="[1223,1274,1644,1670]" pageId="10" pageNumber="11" value="15.8">15.8</number>
(
<number id="E72BBD003236250730046E4BFA9CF945" box="[1290,1330,1644,1670]" pageId="10" pageNumber="11" value="4.1">4.1</number>
