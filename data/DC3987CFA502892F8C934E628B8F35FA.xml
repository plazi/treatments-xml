South-eastern Europe and Middle East including 
<collectingCountry id="2C877649A502892F8FDD4D9B8BD035DA" box="[941,988,1194,1217]" name="Iran" pageId="28" pageNumber="301">Iran</collectingCountry>
. 
<collectingCountry id="2C877649A502892F8F984D988C3A35DA" box="[1000,1078,1193,1217]" name="Turkey" pageId="28" pageNumber="301">Turkey</collectingCountry>
: 
<collectingRegion id="9654F83BA502892F88314D988C8535DA" box="[1089,1161,1193,1217]" country="Turkey" name="Adana" pageId="28" pageNumber="301">Adana</collectingRegion>
, 
<collectingRegion id="9654F83BA502892F88E54D9B8CD835DA" box="[1173,1236,1194,1217]" country="Turkey" name="Bursa" pageId="28" pageNumber="301">Bursa</collectingRegion>
, 
<collectingCountry id="2C877649A502892F88904D9B8D1335D9" box="[1248,1311,1194,1218]" name="Turkey" pageId="28" pageNumber="301">Hatay</collectingCountry>
(
<bibRefCitation id="30014B28A502892F89404D988B7435FA" author="WARNCKE, K." pageId="28" pageNumber="301" pagination="746 - 768" refId="ref70935" refString="WARNCKE, K. 1972: Beitrag zur Systematik und Verbreitung der Bienengattung Prosopis F. in der Westpalaarktis (Hymenoptera, Apoidea, Colletidae). - Bulletin des Recherches agronomique de Gembloux, New Series 5 [1970]: 746 - 768." type="journal article" year="1972">WARNCKE 1972</bibRefCitation>
).
</paragraph>
