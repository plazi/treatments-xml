<taxonomicName id="C24211D8C7823770F31B3E47E761AD89" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:247371" authority="Klingenberg &amp; Brandão, 2009" authorityName="Klingenberg &amp; Brandão" authorityYear="2009" class="Insecta" family="Formicidae" genus="Kalathomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Kalathomyrmex emeryi (Forel)" order="Hymenoptera" pageNumber="21" phylum="Arthropoda" rank="species" species="emeryi" subGenus.completionEvidence="missing, matching species" subGenus.innerRound="1" subGenus.outerRound="1">Kalathomyrmex emeryi</taxonomicName>
