<geoCoordinate id="05BC503DD7429D5AFDD8F9B516A0501B" box="[612,740,1584,1608]" degrees="100.5827" direction="east" orientation="longitude" pageId="6" pageNumber="797" precision="5" value="100.5827">100.5827°E</geoCoordinate>
; 
<quantity id="A7709B1FD7429D5AFD53F9B414F95039" metricMagnitude="1" metricUnit="m" metricValue="4.0" pageId="6" pageNumber="797" unit="m" value="40.0">40 m</quantity>
a.s.l.; 
<date id="1436103AD7429D5AFEB8F9D6153A5038" box="[260,382,1619,1643]" pageId="6" pageNumber="797" value="2018-09-24">24.IX.2018</date>
; Evan S. H. Quah, Hong Zijia leg.; 
