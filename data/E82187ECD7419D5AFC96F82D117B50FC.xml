<paragraph id="603736FAD7429D5AFF14F9B4152D50DE" blockId="6.[168,779,1584,2091]" pageId="6" pageNumber="797">
Hampar Recreational Forest; 
<geoCoordinate id="05BC503DD7429D5AFE5BF9B5161D501B" box="[487,601,1584,1608]" degrees="05.1966" direction="north" orientation="latitude" pageId="6" pageNumber="797" precision="5" value="5.1966">05.1966°N</geoCoordinate>
