<taxonomicName id="E5D4A4B7EF0365FE4D869820683BEAEF" authority="" authorityName="" class="Gastropoda" family="Helicinidae" genus="Orthalicoidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Orthalicoidea incarum" order="Cycloneritida" pageId="20" pageNumber="21" phylum="Mollusca" rank="species" species="incarum">incarum</taxonomicName>
