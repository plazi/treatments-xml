<subSubSection id="352D85981284004497419811AC662681" pageId="20" pageNumber="21" type="description">
<paragraph id="B15253C500B1F61B6911737FC1297B0F" pageId="20" pageNumber="21">Dimensions.</paragraph>
<paragraph id="D782888E82CF08D7E84AF87607ADF048" pageId="20" pageNumber="21">
