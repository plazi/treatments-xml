<taxonomicName id="4C56FA9AEB0AFF9F2F2CFA8BFAAB64B1" authority="Evenhuis &amp; Baez" authorityName="Evenhuis &amp; Baez" authorityYear="2001" box="[1110,1391,1383,1408]" class="Insecta" family="Bombyliidae" genus="Reissa" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="592" phylum="Arthropoda" rank="genus">
