<taxonomicName id="4C56FA9AEB0AFF9F2F65FAFFFAAB6404" authority="Hall &amp; Evenhuis" authorityName="Hall &amp; Evenhuis" box="[1055,1391,1308,1333]" class="Insecta" family="Bombyliidae" genus="Mythenteles" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="592" phylum="Arthropoda" rank="genus">
