in
</quantity>
<taxonomicName id="4C56FA9AEB0AFF9F2BCFFA84FEF864B1" box="[181,316,1383,1408]" class="Insecta" family="Bombyliidae" genus="Mythenteles" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="592" phylum="Arthropoda" rank="genus">
