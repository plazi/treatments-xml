<taxonomicName id="4C364D51FB21FEECCAE8BFEBFDBDF8AF" box="[464,597,1776,1797]" pageId="324" pageNumber="325" rank="species" species="curacaoensis" status="new species">curacaoensis</taxonomicName>
</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A27157BBFB21FEECC950BFEBFD03F8AF" box="[616,747,1775,1797]" pageId="324" pageNumber="325" rank="species">new species</taxonomicNameLabel>
