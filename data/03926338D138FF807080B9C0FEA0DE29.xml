<taxonomicName id="4C3BA9ADD139FF8E715ABBF2FE2CDD8A" authorityName=", Kieffer" authorityYear="1911" box="[333,435,1733,1755]" class="Insecta" family="Diapriidae" genus="Paramesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="460" phylum="Arthropoda" rank="species" species="dolosus">
