<taxonomicName id="4C0C4D08753EFF92FE275ADADFB12B8B" authority="Thorell, 1876" authorityName="Thorell" authorityYear="1876" box="[439,733,1212,1238]" class="Insecta" family="Geometridae" genus="Oncopus" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="256" phylum="Arthropoda" rank="genus">
