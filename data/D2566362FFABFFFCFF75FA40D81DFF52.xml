<taxonomicName id="9DFFA9F7FFA8FFFDFEE0F806D934F84D" authority="Duponchel" authorityName="Duponchel" box="[258,610,1946,1968]" class="Insecta" family="Geometridae" genus="Eupithecia" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cauchiata">
