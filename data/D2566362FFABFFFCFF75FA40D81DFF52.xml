<taxonomicName id="9DFFA9F7FFA8FFFDFC4FF806DFB9F84D" authority="Clerck" authorityName="Clerck" box="[941,1263,1946,1968]" class="Insecta" family="Geometridae" genus="Eupithecia" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="absinthiata">
