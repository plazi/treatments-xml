<emphasis id="B97FEACE7809FF93FCF6FCA69F6EFCC2" box="[786,858,804,829]" italics="true" pageId="28" pageNumber="29">
<taxonomicName id="4C0B4D5F7809FF93FCF6FCA69F62FCC2" box="[786,854,804,829]" class="Insecta" family="Carabidae" genus="Agra" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="fugax">fugax</taxonomicName>
