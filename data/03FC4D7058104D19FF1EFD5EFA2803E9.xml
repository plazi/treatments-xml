<taxonomicName id="4C5587E558174D19FBCFF901FB70028C" box="[1094,1219,1688,1710]" class="Insecta" family="Syrphidae" genus="Cheilosia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="284" phylum="Arthropoda" rank="species" species="lamproptera" status="sp. n.">lamproptera</taxonomicName>
