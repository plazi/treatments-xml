<taxonomicName id="DB974D55FFCAFF92FF5D6BABFEFDF6F1" authority="Zone" authorityName="Zone" box="[151,335,641,666]" class="Cephalopoda" family="Aspidoceratidae" genus="Hybonoticeras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ammonoidea" pageId="44" pageNumber="6" phylum="Mollusca" rank="species" species="hybonotum">Hybonotum Zone</taxonomicName>
