<taxonomicName id="08E47587FFF0E54896290CAEFDECFC5B" authority="Knab, 1914" authorityName="Knab" authorityYear="1914" box="[279,586,915,941]" class="Insecta" family="Drosophilidae" genus="Titanochaeta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="58" phylum="Arthropoda" rank="genus">
