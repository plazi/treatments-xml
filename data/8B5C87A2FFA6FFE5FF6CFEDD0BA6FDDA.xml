Subfamily: 
<taxonomicName id="C4F54D37FFA6FFE5FED2FEDD09A4FE9B" authorityName="Krauss" authorityYear="1902" box="[297,460,292,318]" class="Insecta" family="Tettigoniidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="subFamily" subFamily="Tettigoniinae">Tettigoniinae</taxonomicName>
