<subSubSection id="CEFEBC9CC862FFB6FEEAC01EFCC7F941" box="[344,835,1318,1339]" pageId="0" pageNumber="489" type="nomenclature">
<paragraph id="865BEF17C862FFB6FEEAC01EFCC7F941" blockId="0.[344,835,1318,1339]" box="[344,835,1318,1339]" pageId="0" pageNumber="489">
<heading id="DD13587BC862FFB6FEEAC01EFCC7F941" box="[344,835,1318,1339]" centered="true" fontSize="9" level="2" pageId="0" pageNumber="489" reason="2">
<emphasis id="B4903305C862FFB6FEEAC01EFCC7F941" bold="true" box="[344,835,1318,1339]" pageId="0" pageNumber="489">
<taxonomicName id="41E49494C862FFB6FEEAC01EFDC2F941" authority="Beal" authorityName="Beal" authorityYear="2005" box="[344,582,1318,1339]" class="Insecta" family="Dermestidae" genus="Apphianus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="489" phylum="Arthropoda" rank="species" species="yuccae" status="new genus, new species">
<emphasis id="B4903305C862FFB6FEEAC01EFD8BF941" bold="true" box="[344,527,1318,1339]" italics="true" pageId="0" pageNumber="489">Apphianus yuccae</emphasis>
Beal
</taxonomicName>
, 
<taxonomicNameLabel id="AFA38E7EC862FFB6FDE1C01EFCC7F941" box="[595,835,1318,1339]" pageId="0" pageNumber="489" rank="species">new genus, new species</taxonomicNameLabel>
