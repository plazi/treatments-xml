<taxonomicName id="4C2C4D2EE309FF9FFF75F865B8EBF853" authority="Kugelann, 1794: 578" authorityName="Kugelann" authorityPageNumber="578" authorityYear="1794" box="[151,471,1984,2006]" class="Insecta" family="Hydraenidae" genus="Hydraena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
