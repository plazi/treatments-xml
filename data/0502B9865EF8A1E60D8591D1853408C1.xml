<taxonomicName id="60F6B9B8C72B2BB66F940C6BC7AB3B7F" authority="Hübner, 1820" authorityName="Hübner" authorityYear="1820" class="Insecta" family="Erebidae" genus="Ecpantheria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ecpantheria kinkelini" order="Lepidoptera" pageId="9" pageNumber="74" phylum="Arthropoda" rank="species" species="kinkelini">Ecpantheria kinkelini</taxonomicName>
