<materialsCitation id="13873CEB9E21FFE9FD697659FC42149D" ID-GBIF-Occurrence="3059738519" collectionCode="AMNH" collectorName="In &amp; Chapman" location="Valley" pageId="37" pageNumber="37" specimenCount="1" stateProvince="Cauca" typeStatus="holotype">
