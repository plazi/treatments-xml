<taxonomicName id="4C6684BDFFB8CF22FEB7953F8AF4F8A2" box="[301,437,1868,1891]" class="Insecta" family="Muscidae" genus="Coenosia" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="ukokensis">
