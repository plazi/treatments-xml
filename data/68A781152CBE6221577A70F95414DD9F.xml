<taxonomicName id="937C026C9649A47EA680CDF47E18FCC8" LSID="http://zoobank.org/7382F617-C278-4D3D-9A22-33537ECE1908" class="Gastropoda" family="Diplommatinidae" genus="Moussonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Moussonia longipalatalis" order="Architaenioglossa" pageId="28" pageNumber="29" phylum="Mollusca" rank="species" species="longipalatalis">Moussonia longipalatalis</taxonomicName>
<taxonomicNameLabel id="B00DE6CE76FCD59E5DF2EDE25C2BE6EB" pageId="28" pageNumber="29">sp. n.</taxonomicNameLabel>
