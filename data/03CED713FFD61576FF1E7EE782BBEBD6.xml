<geoCoordinate id="EE5300C2FFD61576FAFF7B0783A2EA36" box="[1313,1436,1059,1083]" degrees="49.148" direction="west" orientation="longitude" pageId="5" pageNumber="4" precision="55" value="-49.148">49.148°W</geoCoordinate>
