<materialsCitation id="81B11B1EFFE2FFC7FED524B1FCA5FC02" box="[334,796,998,1018]" collectionCode="NHMW" pageId="34" pageNumber="123" specimenCode="NHMW 1985" specimenCount="1" typeStatus="holotype">
