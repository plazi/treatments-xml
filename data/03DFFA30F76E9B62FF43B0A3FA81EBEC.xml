<subSubSection id="C36C18ADF76E9B62FF43B143FC56E94C" box="[192,973,290,314]" pageId="6" pageNumber="5" type="type_taxon">
<paragraph id="8BC94B26F76E9B62FF43B143FC56E94C" blockId="6.[192,973,290,314]" box="[192,973,290,314]" pageId="6" pageNumber="5">
