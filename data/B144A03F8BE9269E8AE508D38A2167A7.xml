<paragraph id="313E73E67D0FB205C608EF961FB5550F" pageId="null" pageNumber="743">
Artengruppe des 
<taxonomicName id="B9301FC994738D4CD03DCD3E667977A5" class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="743" phylum="Tracheophyta" rank="genus">Polygonum</taxonomicName>
