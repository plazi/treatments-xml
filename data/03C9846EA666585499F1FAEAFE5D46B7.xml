<taxonomicName id="4C604EFBA66858559A6EF882FE55455C" authority="Peng &amp; Zhang, 2020" authorityName="Peng &amp; Zhang" authorityYear="2020" box="[284,491,1886,1913]" class="Insecta" family="Flatidae" genus="Atracis" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="116" phylum="Arthropoda" rank="species" species="patefacta" status="sp. nov.">
