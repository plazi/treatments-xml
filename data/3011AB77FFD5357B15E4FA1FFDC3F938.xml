<taxonomicName id="7FB861E2FFD5357B1176F988FAF0F9DE" authority="(Pan 1988 a)" authorityName="Craib" authorityYear="1918" baseAuthorityName="Pan" baseAuthorityYear="1988" box="[1029,1317,1631,1657]" class="Magnoliopsida" family="Gesneriaceae" genus="Tremacron" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis id="8ACCC673FFD5357B1176F988FB52F9DF" box="[1029,1159,1631,1656]" italics="true" pageId="23" pageNumber="24">Tremacron</emphasis>
(
<bibRefCitation id="DC296790FFD5357B11EAF988FAC8F9DE" author="Pan, K. Y." box="[1177,1309,1631,1657]" pageId="23" pageNumber="24" pagination="429 - 442" refId="ref19763" refString="Pan, K. Y. (1988 a) Notulae de Gesneriaceis sinensibus. IX. Acta Phytotaxonomica Sinica 26: 429 - 442." type="journal article" year="1988">Pan 1988a</bibRefCitation>
