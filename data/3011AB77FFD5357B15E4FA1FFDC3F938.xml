<taxonomicName id="7FB861E2FFD5357B14E9F988FD65F9DE" authority="(Pan 1987)" baseAuthorityName="Pan" baseAuthorityYear="1987" box="[410,688,1631,1657]" class="Insecta" family="Curculionidae" genus="Oreocharis" kingdom="Plantae" order="Coleoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
