(
<bibRefCitation id="DC296790FFD5357B1742F988FD72F9DE" author="Pan, K. Y." box="[561,679,1631,1657]" pageId="23" pageNumber="24" pagination="264 - 293" refId="ref19736" refString="Pan, K. Y. (1987) Taxonomy of the genus Oreocharis (Gesneriaceae). Acta Phytotaxonomica Sinica 25: 264 - 293." type="journal article" year="1987">Pan 1987</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="7FB861E2FFD5357B17CDF988FC1DF9DE" authority="(Pan 1986)" authorityName="Craib" authorityYear="1920" baseAuthorityName="Pan" baseAuthorityYear="1986" box="[702,968,1631,1657]" class="Magnoliopsida" family="Gesneriaceae" genus="Isometrum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis id="8ACCC673FFD5357B17CDF988FCEEF9DF" box="[702,827,1631,1656]" italics="true" pageId="23" pageNumber="24">Isometrum</emphasis>
(
<bibRefCitation id="DC296790FFD5357B163FF988FC6AF9DE" author="Pan, K. Y." box="[844,959,1631,1657]" pageId="23" pageNumber="24" pagination="23 - 36" refId="ref19707" refString="Pan, K. Y. (1986) The second revision of the genus Isometrum (Gesneriaceae). Acta Botanica Yunnanica 8: 23 - 36." type="journal article" year="1986">Pan 1986</bibRefCitation>
