<taxonomicName id="F171D2F309C96BFB594D19481A9B0331" LSID="0D0466FE-3F7D-518D-B4EB-49D24456C7A5" authority="Casey, 1918" authorityName="Casey" authorityYear="1918" class="Insecta" family="Carabidae" genus="Bembidium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="(Cyclolopha)" order="Coleoptera" pageId="385" pageNumber="386" phylum="Arthropoda" rank="subGenus" subGenus="Cyclolopha">Cyclolopha Casey, 1918</taxonomicName>
