<number id="3D8D24EDB837FFDBB177FED4173EFE3A" box="[273,322,369,394]" pageId="19" pageNumber="470" value="0.46">0.46</number>
(
<number id="3D8D24EDB837FFDBB137FED417B9FE39" box="[337,453,369,393]" pageId="19" pageNumber="470" value="0.44999999999999996" valueMax="0.49" valueMin="0.41">0.41–0.49</number>
, n= 
<number id="3D8D24EDB837FFDBB196FED41782FE3A" box="[496,510,369,394]" pageId="19" pageNumber="470" value="6.0">6</number>
