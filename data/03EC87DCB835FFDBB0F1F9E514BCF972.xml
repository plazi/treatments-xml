<taxonomicName id="4C454D49B837FFDBB4A5FF1B13F7FF65" box="[1219,1419,189,213]" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="19" pageNumber="470" phylum="Chordata" rank="species" species="tuberohumerus">
