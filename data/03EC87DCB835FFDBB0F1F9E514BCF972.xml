<taxonomicName id="4C454D49B837FFDBB21BFEEB153AFED5" box="[637,838,333,357]" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="19" pageNumber="470" phylum="Chordata" rank="species" species="tuberohumerus">
