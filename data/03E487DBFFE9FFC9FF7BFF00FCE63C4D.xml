<collectionCode id="ED5CAE08FFE9FFC8FDE7FE6EFD9F3C4B" box="[523,615,505,529]" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="6" pageNumber="44">MHNG</collectionCode>
); Ƥ, idem, 
<date id="FFF3100DFFE9FFC8FD0FFE6EFCAD3C4B" box="[739,853,505,529]" pageId="6" pageNumber="44" value="1963-05-03">
03.V. 
<number id="3D8524EAFFE9FFC8FCF0FE6EFCAD3C4B" box="[796,853,505,529]" pageId="6" pageNumber="44" value="1963.0">1963</number>
</date>
(
<emphasis id="B939EADFFFE9FFC8FC8AFE6DFC1E3C4B" box="[870,998,505,529]" italics="true" pageId="6" pageNumber="44">R. Mussard</emphasis>
) (
<collectionCode id="ED5CAE08FFE9FFC8FC12FE6EFBA23C4B" box="[1022,1114,505,529]" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="6" pageNumber="44">MHNG</collectionCode>
); 
<number id="3D8524EAFFE9FFC8FB81FE6EFB7A3C4B" box="[1133,1154,505,529]" pageId="6" pageNumber="44" value="3.0">3</number>
, 
<collectingCountry id="F35A765DFFE9FFC8FB7CFE6DFB213C4B" box="[1168,1241,506,529]" name="Morocco" pageId="6" pageNumber="44">Maroc</collectingCountry>
(
<collectionCode id="ED5CAE08FFE9FFC8FB04FE6EFABC3C4B" box="[1256,1348,505,529]" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="6" pageNumber="44">MHNG</collectionCode>
