<taxonomicName id="F9D47DCF0264FFD08293ED496C75FB7F" authority="Bhatti, 1961" authorityName="Bhatti" authorityYear="1961" box="[667,1052,1141,1164]" class="Insecta" family="Thripidae" genus="Anascirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="arorai">
