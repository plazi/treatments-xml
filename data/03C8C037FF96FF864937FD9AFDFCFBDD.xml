, 
<quantity id="4C99DCC4FF96FF864A74FCA4FE40FCA2" box="[313,395,791,816]" metricMagnitude="3" metricUnit="m" metricValue="1.65" pageId="33" pageNumber="1495" unit="m" value="1650.0">
