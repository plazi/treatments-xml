<number id="6C33EA67FF95FFD4AF3E3E95FEBFFE1C" box="[290,319,443,469]" pageId="22" pageNumber="29" value="24.0">24</number>
</emphasis>
<taxonomicName id="1DFB83C3FF95FFD4AF543E92FE25FE1C" box="[328,421,444,469]" class="Aves" family="Paridae" genus="Periparus" kingdom="Animalia" order="Passeriformes" pageId="22" pageNumber="29" phylum="Chordata" rank="species" species="beavani">
