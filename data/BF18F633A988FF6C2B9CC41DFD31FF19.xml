<materialsCitation id="87D94D78A988FF6D2A07C4F0FE1DFA34" ID-GBIF-Occurrence="2331574306" box="[268,436,1468,1495]" collectionCode="MGUH" pageId="136" pageNumber="137" specimenCode="MGUH 33255" specimenCount="1">
