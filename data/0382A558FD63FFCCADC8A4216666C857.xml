<number id="3DE30669FD62FFCCAD81A22063D3CF64" box="[222,250,321,345]" pageId="3" pageNumber="188" value="49.0">49</number>
female
</specimenCount>
and 
<number id="3DE30669FD62FFCCACD4A22062B0CF64" box="[395,409,321,345]" pageId="3" pageNumber="188" value="3.0">3</number>
