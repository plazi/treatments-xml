<emphasis id="17C9D6299D101C4032DD1AD9FCA3F909" bold="true" box="[151,835,1668,1694]" pageId="9" pageNumber="284">
<taxonomicName id="E2BD71B89D101C4032DD1AD9FD55F909" authority="Spungis &amp; Jaschhof" authorityName="Spungis &amp; Jaschhof" box="[151,693,1668,1694]" class="Insecta" family="Cecidomyiidae" genus="Catocha" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="284" phylum="Arthropoda" rank="species" species="brachycornis" status="comb. nov.">
<emphasis id="17C9D6299D101C4032DD1AD9FE40F909" bold="true" box="[151,416,1668,1694]" italics="true" pageId="9" pageNumber="284">Catocha brachycornis</emphasis>
(Spungis &amp; Jaschhof)
</taxonomicName>
<taxonomicNameLabel id="0CFA6B529D101C4030F61AD9FCA3F909" box="[700,835,1668,1694]" pageId="9" pageNumber="284">comb. nov.</taxonomicNameLabel>
