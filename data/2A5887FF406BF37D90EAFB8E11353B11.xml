<collectingCountry id="DAE67679406BF37D93DAF925139A3A81" box="[935,1010,1746,1772]" name="Russia" pageId="1" pageNumber="472">Russia</collectingCountry>
(
<collectingRegion id="6035F80B406BF37D947FF92514F23A81" box="[1026,1178,1746,1772]" country="Russia" name="Amur" pageId="1" pageNumber="472">Amur Oblast</collectingRegion>
, 
<collectingRegion id="6035F80B406BF37D94D5F925150D3A81" box="[1192,1381,1746,1772]" country="Russia" name="Magadan" pageId="1" pageNumber="472">Magadan Oblast</collectingRegion>
, 
<collectingRegion id="6035F80B406BF37D950FF925115C3B7C" country="Russia" name="Primorskiy" pageId="1" pageNumber="472">Primorskiy Kray</collectingRegion>
) (
<bibRefCitation id="C6604B18406BF37D9137F901118F3B7D" author="Ozerov, A. L. &amp; Krivosheina, M. G." box="[330,487,1782,1808]" pageId="1" pageNumber="472" pagination="203 - 222" refId="ref7255" refString="Ozerov, A. L. &amp; Krivosheina, M. G. (2014) To the fauna of dung flies (Diptera: Scathophagidae) of Russian Far East. Russian Entomological Journal, 23, 203 - 222. https: // doi. org / 10.15298 / rusentj. 23.3.07" type="journal article" year="2014">Ozerov, 2014</bibRefCitation>
). New to 
<collectingCountry id="DAE67679406BF37D922FF90012FD3B7C" box="[594,661,1783,1809]" name="Japan" pageId="1" pageNumber="472">Japan</collectingCountry>
