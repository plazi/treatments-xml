<emphasis id="9085EAFB406BF37D90BAF92611093A81" bold="true" box="[199,353,1745,1772]" pageId="1" pageNumber="472">Distribution.</emphasis>
Europe; 
<collectingCountry id="DAE67679406BF37D91B6F92412633A80" box="[459,523,1747,1773]" name="Japan" pageId="1" pageNumber="472">Japan</collectingCountry>
(
<collectingRegion id="6035F80B406BF37D9261F92512E73A81" box="[540,655,1746,1772]" country="Japan" name="Hokkaido" pageId="1" pageNumber="472">Hokkaido</collectingRegion>
