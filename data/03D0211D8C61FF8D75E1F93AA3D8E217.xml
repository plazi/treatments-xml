<keyStep id="308D88AE8C60FF8D75E1FF03A3D9E178" pageId="5" pageNumber="202">
<paragraph id="8BC6900B8C60FF8D75E1FF03A3D8E13E" blockId="5.[136,1452,159,527]" pageId="5" pageNumber="202">
<keyLead id="3088333E8C60FF8D75E1FF03A3D8E13E" pageId="5" pageNumber="202">
6. Root 
<quantity id="4C813DEE8C60FF8D7460FF03A706E112" box="[265,373,245,267]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.5" metricValueMin="0.5" pageId="5" pageNumber="202" unit="cm" value="1.0" valueMax="1.5" valueMin="0.5">0.5–1.5 cm</quantity>
in diameter; leaf blade ovate, oblong, elliptic or lanceolate, 
<quantity id="4C813DEE8C60FF8D76ACFF00A269E112" box="[965,1050,246,267]" metricMagnitude="-2" metricUnit="m" metricValue="7.5" metricValueMax="13.0" metricValueMin="2.0" pageId="5" pageNumber="202" unit="cm" value="7.5" valueMax="13.0" valueMin="2.0">2–13 cm</quantity>
long, 
