<emphasis id="AEB1EA90FFEDB924FAB2ABB4FA30FBA1" bold="true" box="[1309,1432,1698,1722]" pageId="23" pageNumber="22">
<collectingCountry id="E4D27612FFEDB924FAB2ABB4FA30FBA1" box="[1309,1432,1698,1722]" name="Canada" pageId="23" pageNumber="22">CANADA</collectingCountry>
</emphasis>
. 
<collectingRegion id="5E01F860FFEDB924FF6FABD4FE7EFBC1" box="[192,470,1730,1754]" country="Canada" name="British Columbia" pageId="23" pageNumber="22">BRITISH COLUMBIA</collectingRegion>
