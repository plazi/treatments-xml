<taxonomicName id="2AF7A6A2BA60761916669651FE22F991" box="[356,475,1579,1604]" class="Insecta" family="Miridae" genus="Goodeniaphila" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="53" phylum="Arthropoda" rank="species" species="cassisi">
