<taxonomicName id="EB120E8B03C674714F2FF6AAA4BE70FB" class="Arachnida" family="Agelenidae" genus="Coelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coelotes armeniacus" order="Araneae" pageId="1" pageNumber="46" phylum="Arthropoda" rank="species" species="armeniacus">Coelotes armeniacus</taxonomicName>
