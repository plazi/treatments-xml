<taxonomicName id="4C774D07FF83655CFEBBF933FE30F918" box="[330,432,1667,1688]" class="Mammalia" family="Louisinidae" genus="Gigarton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="17" pageNumber="873" phylum="Chordata" rank="genus">
