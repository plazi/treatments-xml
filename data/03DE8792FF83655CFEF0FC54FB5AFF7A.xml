<taxonomicName id="4C774D07FF83655CFDEBF912FDFEF937" authorityName="Russell" authorityYear="1964" box="[538,638,1698,1719]" class="Mammalia" family="Louisinidae" genus="Louisina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="17" pageNumber="873" phylum="Chordata" rank="genus">
