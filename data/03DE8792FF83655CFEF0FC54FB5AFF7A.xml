<taxonomicName id="4C774D07FF83655CFF61F8E9FE95F8EE" authorityName="Hooker &amp; Russell" authorityYear="2012" box="[144,277,1881,1902]" class="Mammalia" family="Louisinidae" genus="Thryptodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="17" pageNumber="873" phylum="Chordata" rank="genus">
