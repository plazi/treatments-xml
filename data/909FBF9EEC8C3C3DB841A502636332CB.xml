117. 
<taxonomicName id="667292BB577B71F6462318B028EDD851" LSID-HNS="urn:lsid:biosci.ohio-state.edu:135671" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica cognata Smith" order="Hymenoptera" pageId="35" pageNumber="35" phylum="Arthropoda" rank="species" species="cognata">Formica cognata</taxonomicName>
