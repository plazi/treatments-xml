<taxonomicName id="3DDA4D7EAC21A553FB314CA8FAF3A6C6" authorityName="Theischinger" authorityYear="1986" box="[1273,1388,1960,1983]" class="Insecta" family="Thaumaleidae" genus="Austrothaumalea" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tonnoiri">
