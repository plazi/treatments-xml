<paragraph id="DB22A131FF98FF9A6878FD82FA55FD21" blockId="7.[877,1616,143,1963]" pageId="7" pageNumber="332">
Like 
<taxonomicName id="1C9DDAB2FF98FF9A68BFFD82FBCDFD05" box="[947,1077,622,645]" class="Liliopsida" family="Orchidaceae" genus="Gymnadenia" kingdom="Plantae" order="Asparagales" pageId="7" pageNumber="332" phylum="Tracheophyta" rank="genus">
