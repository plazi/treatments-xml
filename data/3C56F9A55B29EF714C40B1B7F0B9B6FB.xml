<paragraph id="1046899CA63ED4E77C7B008A619BF926" pageId="12" pageNumber="37">
<taxonomicName id="94600B52ADF6D09EABB6DB0EA4A65AF4" class="Arachnida" family="Gonyleptidae" genus="Gonyleptes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gonyleptes banana" order="Opiliones" pageId="12" pageNumber="37" phylum="Arthropoda" rank="species" species="banana">Gonyleptes banana</taxonomicName>
