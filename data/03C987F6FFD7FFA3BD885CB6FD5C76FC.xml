, 1 
<superScript id="7C159BA8FFD7FFA4B9705E39FBC07032" attach="left" box="[1135,1145,1847,1861]" fontSize="6" pageId="3" pageNumber="404">st</superScript>
gully 
<locationDeviation id="9A6D2FC2FFD7FFA4B9DC5E32FA2E7023" box="[1219,1431,1851,1876]" location="Peacock Flat" pageId="3" pageNumber="404">W of Peacock Flat</locationDeviation>
, 
<quantity id="4C989B05FFD7FFA4BD885E51FF56700F" box="[151,239,1887,1912]" metricMagnitude="2" metricUnit="m" metricValue="4.572" pageId="3" pageNumber="404" unit="ft" value="1500.0">
<elevation id="004DD1D3FFD7FFA4BD885E51FF56700F" box="[151,239,1887,1912]" metricMagnitude="2" metricUnit="m" metricValue="4.572" pageId="3" pageNumber="404" unit="ft" value="1500.0">1500 ft.</elevation>
