<paragraph id="A7ED5D581BD91C9FE534E243ED4ABB2B" pageId="0" pageNumber="35">
<taxonomicName id="0C225851F5BD5234E120DF73EBC38F93" LSID="urn:lsid:zoobank.org:act:E6B103F4-2FFE-4A05-BCD8-356A90754AC5" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amynthas daeari" order="Opisthopora" pageId="0" pageNumber="35" phylum="Annelida" rank="species" species="daeari">Amynthas daeari</taxonomicName>
