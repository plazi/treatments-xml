<quantity id="0407B587FFA17753FAD0F6FDFA6E4084" box="[1347,1416,1486,1510]" metricMagnitude="-1" metricUnit="m" metricValue="5.8" pageId="11" pageNumber="300" unit="cm" value="58.0">58 cm</quantity>
