<taxonomicName id="4C554D06FFF76F4D846AFE20C4891C9E" authority="Mosely 1954: 342" authorityName="Mosely" authorityPageNumber="342" authorityYear="1954" box="[264,652,398,422]" class="Insecta" family="Glossosomatidae" genus="Culoptila" kingdom="Animalia" order="Trichoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="saltena">
<emphasis id="B921EA97FFF76F4D846AFE20C7C51C9D" box="[264,448,399,422]" italics="true" pageId="38" pageNumber="39">Culoptila saltena</emphasis>
<treatmentCitation id="0AF41094FFF76F4D84A5FE21C4891C9E" author="Mosely" box="[455,652,398,421]" page="342" pageId="38" pageNumber="39" year="1954">
<bibRefCitation id="EFC44B74FFF76F4D84A5FE21C4521C9E" author="Mosely" box="[455,599,398,421]" pageId="38" pageNumber="51" refString="Mosely, M. E. (1954) The Protoptila group of the Glossosomatinae (Trichoptera: Rhyacophilidae). Bulletin of the British Museum (Natural History) Entomology, 3, 317 - 346." type="journal article" year="1954">
Mosely 
<number id="3D9D24A2FFF76F4D877CFE21C4521C9E" box="[542,599,398,421]" pageId="38" pageNumber="39" value="1954.0">1954</number>
</bibRefCitation>
