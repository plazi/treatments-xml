<taxonomicName id="4C37F2AECA50731F7ACDFE21A13E99E9" box="[1257,1394,428,452]" class="Malacostraca" family="Philosciidae" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="39" phylum="Arthropoda" rank="family">Philosciidae</taxonomicName>
