<number id="E02024EEFF9446669F4C1273E9129229" box="[1128,1139,807,833]" pageId="1" pageNumber="2" value="1.0">1</number>
<number id="E02024EEFF9446669FA41273E9FE9229" box="[1152,1183,807,833]" pageId="1" pageNumber="2" value="57.0">57</number>
(
