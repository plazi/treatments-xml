<paragraph id="565736C9FF9446669A2C10EBE99690B1" blockId="1.[264,1325,447,1881]" box="[264,1271,447,473]" pageId="1" pageNumber="2">
Phoretic female. Length of idiosoma 
<number id="E02024EEFF944666999910EBEF8890B1" box="[701,745,447,473]" pageId="1" pageNumber="2" value="205.0">205</number>
(
<number id="E02024EEFF94466699DD10EBEE0490B1" box="[761,869,447,473]" pageId="1" pageNumber="2" value="195.5" valueMax="210.0" valueMin="181.0">181–210</number>
), maximum width 
<number id="E02024EEFF9446669F6610EBE90F90B1" box="[1090,1134,447,473]" pageId="1" pageNumber="2" value="155.0">155</number>
(
<number id="E02024EEFF9446669F5A10EBE98A90B1" box="[1150,1259,447,473]" pageId="1" pageNumber="2" value="154.0" valueMax="158.0" valueMin="150.0">150–158</number>
).
