<number id="E02024EEFF94466699D4140CEF9E941A" box="[752,767,1368,1394]" pageId="1" pageNumber="2" value="1.0">1</number>
<number id="E02024EEFF9446669823140CEE42941A" box="[775,803,1368,1394]" pageId="1" pageNumber="2" value="11.0">11</number>
