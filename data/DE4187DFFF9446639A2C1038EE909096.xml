<number id="E02024EEFF94466698EA14ABEEB99771" box="[974,984,1535,1561]" pageId="1" pageNumber="2" value="1.0">1</number>
­Fe 
<number id="E02024EEFF9446669F2414ABE96E9771" box="[1024,1039,1535,1561]" pageId="1" pageNumber="2" value="2.0">2</number>
­Ge 
<number id="E02024EEFF9446669F1914ABE92D9771" box="[1085,1100,1535,1561]" pageId="1" pageNumber="2" value="2.0">2</number>
­Ti 
<number id="E02024EEFF9446669F5514ABE9E09771" box="[1137,1153,1535,1561]" pageId="1" pageNumber="2" value="4.0">4</number>
(
<number id="E02024EEFF9446669FAD14ABE9F29771" box="[1161,1171,1535,1561]" pageId="1" pageNumber="2" value="1.0">1</number>
