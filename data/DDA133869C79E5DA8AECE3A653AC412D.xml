<taxonomicName id="469E8BD4F69B1BD42BECFC7D7ED01384" LSID="http://zoobank.org/8E3E41C6-254E-4B0F-8917-BB58AD9A9E30" authority="Haliday, 1835" authorityName="Haliday" authorityYear="1835" class="Insecta" family="Diapriidae" genus="Ismarus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ismarus kenyensis" order="Hymenoptera" pageId="13" pageNumber="140" phylum="Arthropoda" rank="species" species="kenyensis">Ismarus kenyensis</taxonomicName>
