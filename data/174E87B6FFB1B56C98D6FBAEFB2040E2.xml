<subSubSection id="D7FD652BFFB1B56C9887FAEBFAB0423A" box="[198,1377,1307,1334]" pageId="25" pageNumber="26" type="etymology">
<paragraph id="9F5836A0FFB1B56C9887FAEBFAB0423A" blockId="25.[151,1437,1230,2030]" box="[198,1377,1307,1334]" pageId="25" pageNumber="26">
<emphasis id="AD93EAB2FFB1B56C9887FAEBFE9A4239" bold="true" box="[198,331,1307,1333]" pageId="25" pageNumber="26">Etymology</emphasis>
