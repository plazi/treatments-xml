<paragraph id="8BD9294AFF8DEB5DFF78FF19FDCB04F0" blockId="34.[151,739,151,211]" box="[151,547,186,211]" pageId="34" pageNumber="135">
(
<figureCitation id="135D35CFFF8DEB5DFF70FF19FF0D04F0" box="[159,229,186,211]" captionStart="FIGURE 3" captionStartId="7.[151,250,1863,1885]" captionTargetBox="[151,1436,193,1841]" captionTargetId="figure@7.[151,1436,193,1842]" captionTargetPageId="7" captionText="FIGURE 3. Wing of male (Scale bars = 1 mm): A. Leucophenga fuscithorax sp. nov.; B. L. glabella sp. nov.; C. L. hirsutina sp. nov.; D. L. hirudinis sp. nov.; E. L. piscifoliacea sp. nov.; F. L. rectifoliacea sp. nov.; G. L. serrateifoliacea sp. nov.; H. L. subulata sp. nov.; I. L. uncinata sp. nov." httpUri="https://zenodo.org/record/249343/files/figure.png" pageId="34" pageNumber="135">
Figs 
<number id="3DAE3B6DFF8DEB5DFF38FF18FF0D04F0" box="[215,229,187,211]" pageId="34" pageNumber="135" value="3.0">3</number>
</figureCitation>
C, 
<number id="3DAE3B6DFF8DEB5DFEE9FF19FEFC04F0" box="[262,276,186,211]" pageId="34" pageNumber="135" value="6.0">6</number>
E, 
<number id="3DAE3B6DFF8DEB5DFEDCFF19FEA804F0" box="[307,320,186,211]" pageId="34" pageNumber="135" value="6.0">6</number>
F, 
<number id="3DAE3B6DFF8DEB5DFEB3FF18FE8304F0" box="[348,363,187,211]" pageId="34" pageNumber="135" value="9.0">9</number>
B, 
<number id="3DAE3B6DFF8DEB5DFE64FF18FE4F04F0" box="[395,423,187,211]" pageId="34" pageNumber="135" value="12.0">12</number>
E, 
<number id="3DAE3B6DFF8DEB5DFE29FF18FE0904F0" box="[454,481,187,211]" pageId="34" pageNumber="135" value="12.0">12</number>
F, 
<number id="3DAE3B6DFF8DEB5DFE12FF19FDF204F0" box="[509,538,186,211]" pageId="34" pageNumber="135" value="26.0">26</number>
)
