<taxonomicName id="8FEE4D41FFD4FFFFFD53FA6D662BCEA3" box="[707,872,1482,1508]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="rubidus">
