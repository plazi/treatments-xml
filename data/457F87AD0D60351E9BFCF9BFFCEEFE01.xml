<taxonomicName id="0AD64D380D60351F9BFCF97EFE9CF89D" box="[151,381,1775,1798]" class="Magnoliopsida" family="Clusiaceae" genus="Garcinia" kingdom="Plantae" order="Malpighiales" pageId="4" pageNumber="61" phylum="Tracheophyta" rank="subSpecies" species="cornea" subSpecies="auct">Garcinia cornea auct</taxonomicName>
