<quantity id="4CCE9B37FA3DFFF0C930BC1CFD86FAB4" box="[520,622,1287,1311]" metricMagnitude="-2" metricUnit="m" metricValue="1.7899999999999998" pageId="88" pageNumber="89" unit="mm" value="17.9">17.9 mm</quantity>
