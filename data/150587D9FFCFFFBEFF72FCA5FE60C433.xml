<subSubSection id="D5B66544FFCFFFBEFF72FC35FE34C616" pageId="17" pageNumber="18" type="reference_group">
<paragraph id="9D1336CFFFCFFFBEFF72FC35FDACC0BD" blockId="17.[151,595,998,1021]" box="[151,595,998,1021]" pageId="17" pageNumber="18">
<treatmentCitationGroup id="BDBC11E1FFCFFFBEFF72FC35FDACC0BD" box="[151,595,998,1021]" pageId="17" pageNumber="18">
<treatmentCitation id="1C0D10DEFFCFFFBEFF72FC35FDACC0BD" author="Pitkin, B. R." box="[151,595,998,1021]" page="353" pageId="17" pageNumber="18" year="1978">
