<taxonomicName id="4C0F4D7EAB5BFFFEFE46FD39FDC2FD2F" baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[418,559,726,748]" class="Insecta" family="Scathophagidae" genus="Scathophaga" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="736" phylum="Arthropoda" rank="species" species="stercoraria">
