, 
<specimenCount id="9D09FD74AB5BFFFEFEB6FB6DFE82FB54" box="[338,367,1154,1175]" pageId="5" pageNumber="736" type="male">1♂</specimenCount>
, 
<specimenCount id="9D09FD74AB5BFFFEFE9CFB6DFE79FB59" box="[376,404,1154,1178]" pageId="5" pageNumber="736" type="female">1♀</specimenCount>
