<taxonomicName id="47F9C1B5029A19F444604AE1B1CA76B5" authorityName="S. W. Leonard, W. C. Taylor, L. J. Musselman &amp; R. D. Bray" authorityYear="2016" class="Lycopodiopsida" family="Isoetaceae" genus="Isoetes" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Isoetes mississippiensis" order="Isoetales" pageId="3" pageNumber="100" phylum="Lycophyta" rank="species" species="mississippiensis">
