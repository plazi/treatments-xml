<taxonomicName id="4C76A0CE8320CF4D667728FBFD98C57A" authority="Zhang &amp; Maddison, 2012" authorityName="Zhang &amp; Maddison" authorityYear="2012" box="[371,570,1773,1795]" class="Arachnida" family="Salticidae" genus="Omoedus" kingdom="Animalia" order="Araneae" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="papuanus" status="sp. nov.">
