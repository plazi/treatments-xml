<taxonomicName id="2E7E3A4E6C171865E7AC3DEFE6FD0F9D" class="Gastropoda" family="Plectopylidae" genus="Endothyrella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Endothyrella nepalica" order="Stylommatophora" pageId="33" pageNumber="34" phylum="Mollusca" rank="species" species="nepalica">Endothyrella nepalica</taxonomicName>
