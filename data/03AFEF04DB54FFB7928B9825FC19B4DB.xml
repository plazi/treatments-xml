<paragraph id="8BB95E12DB54FFB6928B9848FEF0B756" blockId="4.[151,547,295,354]" box="[151,330,330,354]" pageId="4" pageNumber="517">
(
<figureCitation id="133D4297DB54FFB692829848FEB1B756" box="[158,267,330,354]" captionStart="FIGURE 1 – 2" captionStartId="2.[151,250,1628,1651]" captionTargetBox="[151,1429,214,1589]" captionTargetId="figure@2.[151,1436,195,1604]" captionTargetPageId="2" captionText="FIGURE 1 – 2. Habitus of Protohermes spp. 1. Protohermes sonus sp. nov., male holotype; 2. Protohermes sublunatus sp. nov. Scale bar = 5.0 mm." httpUri="https://zenodo.org/record/220608/files/figure.png" pageId="4" pageNumber="517">
Figures 
<number id="3DCE4C35DB54FFB692E79848FEB1B756" box="[251,267,330,354]" pageId="4" pageNumber="517" value="5.3" valueMax="8.0" valueMin="2.6">2</number>
</figureCitation>
, 
<figureCitation id="133D4297DB54FFB6930B9848FEF8B756" box="[279,322,330,354]" captionStart="FIGURES 6 – 8" captionStartId="4.[151,264,1759,1782]" captionTargetBox="[170,1420,1081,1721]" captionTargetId="figure@4.[151,1436,1055,1738]" captionTargetPageId="4" captionText="FIGURES 6 – 8. Protohermes sublunatus sp. nov., male holotype. 6. genitalia, dorsal view; 7. genitalia, ventral view; 8. tenth gonocoxite, ventral view. Scale bar = 1.0 mm." httpUri="https://zenodo.org/record/220610/files/figure.png" pageId="4" pageNumber="517">6–8</figureCitation>
)
