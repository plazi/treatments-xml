<taxonomicName id="FD7B39F13EED010C72AF85D20FF0286E" authorityName="Casey" authorityYear="1918" class="Hexapoda" family="Carabidae" genus="Tachyura" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="441" pageNumber="442" phylum="Arthropoda" rank="species" species="saturata">Tachyura saturata</taxonomicName>
