<taxonomicName id="56F94D3AFFEAFFA6332CF8ECFC0F4E68" authority="Feng &amp; Wang &amp; Tong, 2021" authorityName="Feng &amp; Wang &amp; Tong" authorityYear="2021" box="[832,956,1824,1848]" class="Insecta" family="Thripidae" genus="Neohydatothrips" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="566" phylum="Arthropoda" rank="species" species="sophorae" status="sp. n.">
