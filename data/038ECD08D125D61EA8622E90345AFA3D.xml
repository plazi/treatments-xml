<taxonomicName id="4C27079DD124D61EAD502E7D37E0FAED" authority="Duben and Koren" authorityName="Duben and Koren" class="Ophiuroidea" family="Ophiomyxidae" genus="Ophiolycus" kingdom="Animalia" order="Ophiurida" pageId="5" pageNumber="6" phylum="Echinodermata" rank="species" species="purpureus">
