<taxonomicName id="226714EA84AB278EB097ACDAAFC7D02E" LSID-ZBK="urn:lsid:zoobank.org:act:AC273B3A-FA06-469F-9A73-E1E5BD9033D6" class="Actinopterygii" family="Sisoridae" genus="Pseudexostoma" kingdom="Animalia" lsidName-ZBK="Pseudexostoma Chu 1979:78, 81" order="Siluriformes" pageNumber="66" phylum="Chordata" rank="genus">Pseudexostoma</taxonomicName>
