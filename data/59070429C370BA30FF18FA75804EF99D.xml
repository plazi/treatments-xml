<taxonomicName id="16AECEBCC370BA30FEDAFA5383C1F9C6" authority="(Linnaeus)" baseAuthorityName="Linnaeus" baseAuthorityYear="1763" box="[341,733,1518,1544]" class="Insecta" family="Conchaspididae" genus="Conchaspis" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="capensis">
