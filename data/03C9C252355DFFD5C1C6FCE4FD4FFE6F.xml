<taxonomicName id="4C6008C7355CFFD5C5F6FE82FB49FE82" box="[1191,1328,295,320]" class="Insecta" family="Acroceridae" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="138" phylum="Arthropoda" rank="family">Acroceridae</taxonomicName>
