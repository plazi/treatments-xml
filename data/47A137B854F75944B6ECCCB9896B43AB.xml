<taxonomicName id="6AA0FE37F2CFD71A4B8AE78085D3AC2E" LSID="47A137B8-54F7-5944-B6EC-CCB9896B43AB" authorityName="De Mattia &amp; Reier &amp; Haring" authorityYear="2021" class="Gastropoda" family="Clausiliidae" genus="Charpentieria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Charpentieria itala subsp. zalloti" order="Stylommatophora" pageId="0" pageNumber="1" phylum="Mollusca" rank="subSpecies" species="itala" status="nom. nov." subSpecies="zalloti">Charpentieria itala zalloti</taxonomicName>
