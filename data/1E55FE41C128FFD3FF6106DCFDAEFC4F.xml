<taxonomicName id="51FC34D4C12AFFDCFF6105DAFE56F868" authorityName="Monne &amp; Monne, 2007:" authorityYear="1060" box="[151,424,1968,1992]" class="Insecta" family="Cerambycidae" genus="Paranisopodus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="202" phylum="Arthropoda" rank="species" species="granulosus">
