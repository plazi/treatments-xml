<collectorName id="5073530DFFD4FFE26D98F84CFD6B54A0" box="[496,652,2011,2037]" pageId="77" pageNumber="78">A. W. Moore</collectorName>
, genit. prep. 3736 
<collectorName id="5073530DFFD4FFE26F0DF84CFC0354A0" box="[869,996,2011,2037]" pageId="77" pageNumber="78">M. Fibiger</collectorName>
, 
<specimenCode id="AD209EA0FFD4FFE26F98F84CFB7E54A0" box="[1008,1177,2011,2037]" collectionCode="BM" country="United Kingdom" name="Bristol Museum" pageId="77" pageNumber="78">BM 1933-254</specimenCode>
. 
<collectorName id="5073530DFFD4FFE268CCF84CFB3954A0" box="[1188,1246,2011,2037]" pageId="77" pageNumber="78">Coll.</collectorName>
<collectionCode id="9B97AE1EFFD4FFE2688DF84CFAA654A0" box="[1253,1345,2011,2037]" pageId="77" pageNumber="78">BMNH</collectionCode>
. 
