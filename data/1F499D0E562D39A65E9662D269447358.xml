<taxonomicName id="363E0F5EAE5EEAF9DFE3AA0C96319D57" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Brassicaceae" genus="Lunaria" kingdom="Plantae" order="Brassicales" phylum="Tracheophyta" rank="species" species="annua">Lunaria annua</taxonomicName>
