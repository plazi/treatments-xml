<taxonomicName id="4F8A7AE5FFF2AE772C68F8DDD62AF889" authority="Gonçalves, 2021" authorityName="Gonçalves" authorityYear="2021" box="[345,526,1830,1854]" class="Insecta" family="Cicadellidae" genus="Mejdalania" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="114" phylum="Arthropoda" rank="species" species="longa" status="gen. et sp. nov.">
