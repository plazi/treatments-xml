<taxonomicName id="4C334D5CFF97FF88FD54FBC3FC58FB56" authority="BLAGODEROV &amp; GRIMALDI, 2004" authorityName="BLAGODEROV &amp; GRIMALDI" authorityYear="2004" box="[740,1017,1146,1170]" class="Insecta" family="Mycetophilidae" genus="Allocotocera" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="burmitica" status="new species">
