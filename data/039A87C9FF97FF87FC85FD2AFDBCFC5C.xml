<location id="8EEC6004FF98FF87FEF8FD68FE06FD2C" LSID="urn:lsid:plazi:treatment:039A87C9FF97FF87FC85FD2AFDBCFC5C:8EEC6004FF98FF87FEF8FD68FE06FD2C" box="[328,423,721,744]" country="Myanmar" name="Katchin" pageId="24" pageNumber="25">Katchin</location>
