<taxonomicName id="CC884D7CFF42FF0BFF2465DDFDA8F855" authority="Ansari, 1957 c: 122" authorityName="Ansari" authorityPageNumber="122" authorityYear="1957" box="[199,609,2013,2038]" class="Insecta" family="Philopteridae" genus="Brueelia" kingdom="Animalia" order="Phthiraptera" pageId="200" pageNumber="201" phylum="Arthropoda" rank="species" species="elegans">
