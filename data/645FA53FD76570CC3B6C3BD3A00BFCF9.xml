<paragraph id="8F86E136718F6628D6B78E74EAACD34D" pageId="17" pageNumber="54">
<taxonomicName id="FA01C2E20318A15802A88B136254E52E" class="Insecta" family="Hydroptilidae" genus="Oxyethira" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oxyethira dorsennus" order="Trichoptera" pageId="17" pageNumber="54" phylum="Arthropoda" rank="species" species="dorsennus">Oxyethira dorsennus</taxonomicName>
