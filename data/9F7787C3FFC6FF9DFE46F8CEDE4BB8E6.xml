<materialsCitation id="A7B63C88FFC8FF9FFCBCF946DF83BFC1" ID-GBIF-Occurrence="3033396301" country="Madagascar" location="Andasibe-Mantadia National Park" longitude="48.44778" pageId="8" pageNumber="552" specimenCount="2" typeStatus="holotype">
