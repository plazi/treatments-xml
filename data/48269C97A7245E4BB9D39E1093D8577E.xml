<taxonomicName id="51AF202E71C2D9302CCA05F1F7489CD5" authorityName="Pall-Gergely" authorityYear="2022" class="Gastropoda" family="Camaenidae" genus="Bouchetcamaena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bouchetcamaena foveata" order="Testacea" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="foveata">
