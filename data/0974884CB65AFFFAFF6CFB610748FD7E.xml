<collectingCountry id="F9CA79CAB658FFFAFC23FECA0603FE5E" box="[984,1096,367,392]" name="Indonesia" pageId="43" pageNumber="44">Indonesia</collectingCountry>
, 
<collectingCountry id="F9CA79CAB658FFFAFBACFED506DBFE51" box="[1111,1168,368,391]" name="Laos" pageId="43" pageNumber="44">Laos</collectingCountry>
