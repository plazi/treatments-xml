<subSubSection id="D9BC651EFFCFFFCED4C5E43956BB1F53" pageId="19" pageNumber="20" type="description">
<paragraph id="91193695FFCFFFCED4C5E439552B1E82" blockId="19.[151,617,622,645]" box="[151,617,622,645]" pageId="19" pageNumber="20">
<treatmentCitationGroup id="B1B611BBFFCFFFCED4C5E439552B1E82" box="[151,617,622,645]" pageId="19" pageNumber="20">
<taxonomicName id="56A64D16FFCFFFCED4C5E43956361E83" authorityName="Costa Lima" authorityYear="1935" box="[151,372,622,645]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="ananasi">
