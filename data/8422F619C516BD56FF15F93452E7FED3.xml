</collectingMunicipality>
(“ 
<location id="095411D4C517BD56FC04FF73556BFF4F" LSID="urn:lsid:plazi:treatment:8422F619C516BD56FF15F93452E7FED3:095411D4C517BD56FC04FF73556BFF4F" box="[902,1027,223,250]" country="Venezuela" municipality="Sierra de Parima" name="St. Parime" pageId="7" pageNumber="238" stateProvince="Amazonas">St. Parime</location>
”)
