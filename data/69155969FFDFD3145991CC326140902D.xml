<taxonomicName id="26BC93FCFFD8D3155B98CF8367A097DF" box="[670,761,1492,1514]" class="Insecta" family="Staphylinidae" genus="Batraxis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="pilosa">
