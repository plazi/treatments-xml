<paragraph id="D1ABDBFEFCE035ED6A48AA993B2B9025" pageId="2" pageNumber="3">
<taxonomicName id="B24CC2A1FDC7EB6E5D67D69662CBA3D6" authority="Kang &amp; Long, 2020" authorityName="Kang" authorityYear="2020" class="Insecta" family="Braconidae" genus="Orientocardiochiles" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Orientocardiochiles joeburrowi" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="joeburrowi">
