<taxonomicName id="4C317AFA5C72E16DF324FA69FF73FA08" class="Elasmobranchii" family="Rajidae" genus="Raja" kingdom="Animalia" order="Rajiformes" pageId="20" pageNumber="61" phylum="Chordata" rank="species" species="texana">
