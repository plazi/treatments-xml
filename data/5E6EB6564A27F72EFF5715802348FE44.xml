<subSubSection id="9EDD54CB4A23F72EFF0715CF2348FE44" pageId="5" pageNumber="399" type="etymology">
<paragraph id="D67807404A23F72EFF0715CF2348FE44" blockId="5.[151,1436,151,464]" pageId="5" pageNumber="399">
<emphasis id="E4B3DB524A23F72EFF0715CF2302FE39" bold="true" box="[199,334,404,429]" pageId="5" pageNumber="399">Etymology.</emphasis>
