. 
<taxonomicName id="83D64D3D772AFFDBFCF32A9F6FFBB0AD" baseAuthorityName="Kemp" baseAuthorityYear="1911" box="[853,975,394,419]" class="Malacostraca" family="Tetrasquillidae" genus="Heterosquilloides" kingdom="Animalia" order="Stomatopoda" pageId="14" pageNumber="361" phylum="Arthropoda" rank="species" species="insignis">
