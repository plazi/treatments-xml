(
<bibRefCitation id="20474B4F772AFFDBFD742ABE6F4BB0CB" author="BIGELOW R. P." box="[722,895,427,453]" pageId="14" pageNumber="361" pagination="105 - 191" refId="ref13500" refString="BIGELOW R. P. 1931. - Stomatopoda of the southern and eastern Pacific Ocean and the Hawaiian Islands. Bulletin of the Museum of Comparative Zoology, Harvard University 72 (4): 105 - 191, pls 1, 2." type="book chapter" year="1931">Bigelow, 1931</bibRefCitation>
)
</taxonomicName>
and 
<taxonomicName id="83D64D3D772AFFDBFC612ABE6F4DB0EB" authority="(Schmitt, 1940)" baseAuthorityName="Schmitt" baseAuthorityYear="1940" class="Malacostraca" family="Tetrasquillidae" genus="Tetrasquilla" kingdom="Animalia" order="Stomatopoda" pageId="14" pageNumber="361" phylum="Arthropoda" rank="species" species="mccullochae">
