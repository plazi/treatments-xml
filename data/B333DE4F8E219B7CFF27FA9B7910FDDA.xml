<emphasis id="09EEB34B8E219B7FFF27FA9B7BA4FA4E" bold="true" box="[151,507,1404,1430]" italics="true" pageId="25" pageNumber="26">
<taxonomicName id="FC9A14DA8E219B7FFF27FA9B7BAAFA4E" box="[151,501,1404,1430]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="congkem" status="new species">Pseudoneureclipsis congkem</taxonomicName>
