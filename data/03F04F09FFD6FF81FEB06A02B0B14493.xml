<collectingCountry id="F34EBE8FFFD8FF81FE6768CBB31F43CB" box="[479,553,981,1007]" name="China" pageId="25" pageNumber="26">China</collectingCountry>
: 
<collectingRegion id="499D30FDFFD8FF81FD8D68CBB3B843CB" box="[565,654,981,1007]" country="China" name="Yunnan" pageId="25" pageNumber="26">Yunnan</collectingRegion>
