<geoCoordinate id="8A936EF2C2A9E761DA2C480F519218ED" direction="north" orientation="latitude" precision="925" value="35.933334">35°56'N</geoCoordinate>
, 
<geoCoordinate id="C6E53AE53082B890D34009306CDE0CBC" direction="east" orientation="longitude" precision="925" value="51.366665">51°22'E</geoCoordinate>
, alt. 2000 m, 11 July 2011, O. Joharchi coll., in nest of 
<taxonomicName id="C6BD171F2F5253D0543BF5925FD75AED" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium caespitum" order="Hymenoptera" pageId="3" pageNumber="20" phylum="Arthropoda" rank="species" species="caespitum">Tetramorium caespitum</taxonomicName>
