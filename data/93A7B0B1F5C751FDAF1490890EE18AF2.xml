<taxonomicName id="ADE7AC022905CB235DBFF6A34E8F3145" class="Arachnida" family="Liphistiidae" genus="Heptathela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heptathela yakushimaensis" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="yakushimaensis">Heptathela yakushimaensis</taxonomicName>
