, 
<locationDeviation id="248B2FCFFF8BF43AFBF8F99BFABFF948" box="[1055,1370,1656,1682]" location="Yahualica Hwy" pageId="23" pageNumber="24">
<quantity id="F27E9B08FF8BF43AFBF8F99BFB89F948" box="[1055,1132,1656,1682]" metricMagnitude="4" metricUnit="m" metricValue="2.7358848" pageId="23" pageNumber="24" unit="mi" value="17.0">17 mi</quantity>
