<collectionCode id="ED034BECFF9EC15FFD0816F7AF88FA4E" box="[653,743,1415,1438]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="24" pageNumber="1314" type="Museum">RMNH</collectionCode>
<accessionNumber id="94414ECAFF9EC15FFD7516F7AE3EFA4E" box="[752,849,1414,1438]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/D41619" pageId="24" pageNumber="1314">D 41619</accessionNumber>
