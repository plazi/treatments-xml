<emphasis id="B970113BD47F4337B6D8FE27FD84239F" bold="true" box="[151,583,404,430]" pageId="21" pageNumber="322">
<taxonomicName id="4C04B6AAD47F4337B6D8FE27FE3E239F" box="[151,509,404,430]" class="Insecta" family="Ichneumonidae" genus="Netelia" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="longipad" status="n. sp." subGenus="Bessobates">
<emphasis id="B970113BD47F4337B6D8FE27FF2E239F" bold="true" box="[151,237,404,430]" italics="true" pageId="21" pageNumber="322">Netelia</emphasis>
(
<emphasis id="B970113BD47F4337B6B1FE27FE41239F" bold="true" box="[254,386,404,430]" italics="true" pageId="21" pageNumber="322">Bessobates</emphasis>
) 
<emphasis id="B970113BD47F4337B7DCFE27FE3E239F" bold="true" box="[403,509,404,430]" italics="true" pageId="21" pageNumber="322">longipad</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A243AC40D47F4337B44BFE27FD84239F" box="[516,583,404,430]" pageId="21" pageNumber="322" rank="species">n. sp.</taxonomicNameLabel>
