<subSubSection id="C31E9EA2D47F4337B688F911FC36248B" box="[199,1013,1698,1723]" pageId="21" pageNumber="322" type="etymology">
<paragraph id="8BBBCD29D47F4337B688F911FC36248B" blockId="21.[151,1436,510,1903]" box="[199,1013,1698,1723]" pageId="21" pageNumber="322">
<emphasis id="B970113BD47F4337B688F911FE8D248A" bold="true" box="[199,334,1698,1723]" pageId="21" pageNumber="322">Etymology.</emphasis>
