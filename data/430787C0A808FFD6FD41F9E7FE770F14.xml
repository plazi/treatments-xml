<collectionCode id="ADBFAE13A809FFD6FDA5FCC1FD300A33" box="[560,653,788,809]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="17" pageNumber="17">AMNH</collectionCode>
