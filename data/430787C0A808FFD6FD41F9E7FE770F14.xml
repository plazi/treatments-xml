<collectionCode id="ADBFAE13A809FFD6FF37FD0DFF420BF7" box="[162,255,728,749]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="17" pageNumber="17">AMNH</collectionCode>
