<collectionCode id="ADBFAE13A809FFD6FDBCFD48FD370BA8" box="[553,650,669,690]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="17" pageNumber="17">AMNH</collectionCode>
