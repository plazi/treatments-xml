<emphasis id="F9DAEAC4A809FFD6FF15FD6FFE990BD5" bold="true" box="[128,292,698,719]" pageId="17" pageNumber="17">
<specimenCode id="9B089EADA809FFD6FF15FD6FFE990BD5" box="[128,292,698,719]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="17" pageNumber="17">AMNH 70325</specimenCode>
