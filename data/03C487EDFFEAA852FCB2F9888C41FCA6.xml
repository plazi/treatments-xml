<taxonomicName id="4C6D4D78FFEAA857FC36F9888051F927" authority="Andersen, 1905" authorityName="Andersen" authorityYear="1905" box="[917,1913,1589,1684]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="1" pageNumber="76" phylum="Chordata" rank="species" species="simplex" status="sp. n.">Rhinolophus simplex</taxonomicName>
