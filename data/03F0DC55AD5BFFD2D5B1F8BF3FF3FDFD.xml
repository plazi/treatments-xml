<taxonomicName id="4C5916C0AD5AFFD1D6CBF8703CE5F81F" authority="Nguyen, 2020" authorityName="Nguyen" authorityYear="2020" box="[238,458,1922,1943]" class="Insecta" family="Vespidae" genus="Pareumenes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="589" phylum="Arthropoda" rank="species" species="caoduong" status="new species">
<emphasis id="B92DB151AD5AFFD1D6CBF8703CE5F81F" box="[238,458,1922,1943]" italics="true" pageId="1" pageNumber="589">Pareumenes caoduong</emphasis>
</taxonomicName>
