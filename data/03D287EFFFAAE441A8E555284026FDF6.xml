<taxonomicName id="4C7B4D7AFFABE441A97F52F8408AFF51" authorityName="" authorityYear="1789" box="[269,393,151,176]" class="Magnoliopsida" family="Urticaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="11" pageNumber="12" phylum="Magnoliopsida" rank="family">Urticaceae</taxonomicName>
