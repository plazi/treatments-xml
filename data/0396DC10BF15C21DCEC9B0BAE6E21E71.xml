<taxonomicName id="4C3F1685BF14C218C911B030E22C1A4B" authority="Friis &amp; Crane &amp; Pedersen, 2019" authorityName="Friis &amp; Crane &amp; Pedersen" authorityYear="2019" box="[1097,1364,1541,1562]" class="Liliopsida" family="Poaceae" genus="Anaspermum" kingdom="Plantae" order="Poales" pageId="27" pageNumber="180" phylum="Tracheophyta" rank="species" species="operculatum" status="gen. et sp. nov.">
