<emphasis id="B903EAF8FFC03512FE9CF8DCFE71BE7F" box="[300,498,1894,1918]" italics="true" pageId="37" pageNumber="774">Paraclodes guyanensis</emphasis>
Desutter-Grandcolas
</taxonomicName>
<emphasis id="B903EAF8FFC03512FD78F8DDFC8BBE7F" box="[712,776,1895,1918]" italics="true" pageId="37" pageNumber="774">n. nov.</emphasis>
for 
<taxonomicName id="4C774D69FFC03512FF19F83AFD86BE98" authority="(Chopard, 1912)" authorityName="" authorityYear="1992" baseAuthorityName="Chopard" baseAuthorityYear="1912" box="[169,517,1920,1945]" class="Insecta" family="Phalangopsidae" genus="Paraclodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="37" pageNumber="774" phylum="Arthropoda" rank="species" species="aptera">
<emphasis id="B903EAF8FFC03512FF19F83AFECDBE99" box="[169,334,1920,1944]" italics="true" pageId="37" pageNumber="774">Paraclodes aptera</emphasis>
(
<bibRefCitation id="EFE64B1BFFC03512FEEEF83AFE7EBE98" author="Chopard" box="[350,509,1920,1945]" firstAuthor="Chopard" journalOrPublisher="Annales de la Societe entomologique de France" pageId="37" pageNumber="774" pagination="401 - 432" part="81" refId="ref36356" refString="CHOPARD L. 1912. - Contribution a la faune des Orthopteres de la Guyane francaise. Annales de la Societe entomologique de France 81: 401 - 432." title="Contribution a la faune des Orthopteres de la Guyane francaise" type="journal article" year="1912">Chopard, 1912</bibRefCitation>
)
</taxonomicName>
, with which 
<taxonomicName id="4C774D69FFC03512FD3BF83BFE0BBEB2" authority="Desutter-Grandcolas, 1992" authorityName="Desutter-Grandcolas" authorityYear="1992" class="Insecta" family="Phalangopsidae" genus="Paraclodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="37" pageNumber="774" phylum="Arthropoda" rank="species" species="guyanensis">
