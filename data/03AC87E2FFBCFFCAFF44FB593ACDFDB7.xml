<emphasis id="B971EAE6FFBCFFCAFBEEFEAA3DA3FEE6" bold="true" box="[1041,1220,301,322]" pageId="7" pageNumber="7">
<specimenCode id="DBA39E8FFFBCFFCAFBEEFEAA3DA3FEE6" box="[1041,1220,301,322]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="7" pageNumber="7">AMNH 466908</specimenCode>
</emphasis>
