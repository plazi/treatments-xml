<date id="FFBB1034FFBCFFCAFB4FFF753ABBFE81" pageId="7" pageNumber="7" value="1937-12-18" valueMax="1937-12-19" valueMin="1937-12-18">
<collectingDate id="EFFFE9DCFFBCFFCAFB4FFF753ABBFE81" pageId="7" pageNumber="7" value="1937-12-18" valueMax="1937-12-19" valueMin="1937-12-18">18 and 19 December 1937</collectingDate>
</date>
