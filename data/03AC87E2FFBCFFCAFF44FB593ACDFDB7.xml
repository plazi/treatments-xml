<quantity id="4CFD9B11FFBCFFCAFD02FECC3A37FEC4" box="[765,848,331,353]" metricMagnitude="-2" metricUnit="m" metricValue="5.5" pageId="7" pageNumber="7" unit="mm" value="55.0">55 mm</quantity>
