<taxonomicName id="4C454D29FF92D770FCEEF8FDFB39F8FD" authority="(Rehn, 1903)" baseAuthorityName="Rehn" baseAuthorityYear="1903" box="[839,1260,1863,1890]" class="Insecta" family="Forficulidae" genus="Sarcinatrix" kingdom="Animalia" order="Dermaptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="anomalia">
