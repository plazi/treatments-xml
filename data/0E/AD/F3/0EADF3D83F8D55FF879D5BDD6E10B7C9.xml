<taxonomicName authority="Guenée, 1857" authorityName="Guenee" authorityYear="1858" class="Insecta" family="Geometridae" genus="Pachyodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pachyodes" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="52190">Pachyodes</emphasis>
</taxonomicName>
(
<taxonomicName authority="Prout, 1912" authorityName="Prout" authorityYear="1912" class="Insecta" family="Geometridae" genus="Lophophelma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lophophelma albapex" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="species" species="albapex">
<emphasis italics="true" pageId="0" pageNumber="52190">Lophophelma albapex</emphasis>
</taxonomicName>
