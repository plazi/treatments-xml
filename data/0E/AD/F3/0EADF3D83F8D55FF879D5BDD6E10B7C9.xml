<taxonomicName authority="Prout, 1912" authorityName="Prout" authorityYear="1912" class="Insecta" family="Geometridae" genus="Lophophelma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lophophelma" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="52190">Lophophelma</emphasis>
</taxonomicName>
