), 
<taxonomicName box="[903,1124,223,248]" class="Magnoliopsida" family="Fabaceae" genus="Tephrosia" kingdom="Plantae" order="Fabales" pageId="24" pageNumber="460" phylum="Tracheophyta" rank="species" species="purpurea">
<emphasis box="[903,1124,223,248]" italics="true" pageId="24" pageNumber="460">Tephrosia purpurea</emphasis>
