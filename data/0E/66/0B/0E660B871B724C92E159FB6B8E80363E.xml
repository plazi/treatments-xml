<taxonomicName authorityName="Macquart" authorityYear="1834" class="Hexapoda" family="Nemestrinidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Nemestrinidae</taxonomicName>
, 
<taxonomicName authorityName="Latreille" authorityYear="1802" class="Insecta" family="Bombyliidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Diptera" pageId="44" pageNumber="97" phylum="Arthropoda" rank="family">Bombyliidae</taxonomicName>
