<taxonomicName authorityName="Shelley &amp; Delaval &amp; Le" authorityYear="2017" class="Actinopterygii" family="Terapontidae" genus="Syncomistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="versicolor">
