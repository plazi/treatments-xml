</taxonomicName>
sp. and 
<taxonomicName authority="Shelley &amp; Delaval &amp; Le, 2017" authorityName="Shelley &amp; Delaval &amp; Le" authorityYear="2017" box="[363,510,980,1004]" class="Actinopterygii" family="Terapontidae" genus="Syncomistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="wunambal" status="sp. nov.">
