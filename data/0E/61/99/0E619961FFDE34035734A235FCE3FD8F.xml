<taxonomicName authority="deserta" class="Insecta" family="Sarcophagidae" genus="Senotainia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="2497" phylum="Arthropoda" rank="subSpecies" species="deserta" subSpecies="deserta">
