August 
<number box="[373,434,1865,1891]" pageId="41" pageNumber="42" value="2003.0">2003</number>
</date>
, pitfall trap (
<accessionNumber box="[584,718,1865,1891]" httpUri="http://www.ncbi.nlm.nih.gov/protein/HUJ15550" pageId="41" pageNumber="42">
<collectionCode LSID="urn:lsid:biocol.org:col:15399" box="[584,642,1865,1891]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15399" name="Hebrew University" pageId="41" pageNumber="42">HUJ</collectionCode>
