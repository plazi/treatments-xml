<subSubSection pageId="12" pageNumber="13" type="materials_examined">
<paragraph blockId="12.[151,1436,1089,1579]" pageId="12" pageNumber="13">
<emphasis bold="true" box="[151,334,1089,1115]" pageId="12" pageNumber="13">
<typeStatus box="[151,211,1089,1115]" pageId="12" pageNumber="13">Type</typeStatus>
material.
</emphasis>
Adult male 
<typeStatus box="[487,592,1089,1115]" pageId="12" pageNumber="13">holotype</typeStatus>
from 
<taxonomicName box="[666,765,1089,1115]" class="Arachnida" family="Gnaphosidae" genus="Leptopilos" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="45" phylum="Arthropoda" rank="species" species="lakhish">Lakhish</taxonomicName>
, 
<collectingCountry box="[778,848,1089,1115]" name="Israel" pageId="12" pageNumber="13">Israel</collectingCountry>
, leg. Yoni Gavish on cereal (
<taxonomicName box="[1198,1418,1089,1114]" class="Liliopsida" family="Poaceae" genus="Hyparrhenia" kingdom="Plantae" order="Poales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="hirta">
