<number box="[502,517,1623,1649]" pageId="30" pageNumber="31" value="6.0">6</number>
ΨΨ): total length 
<number box="[756,848,1623,1649]" pageId="30" pageNumber="31" value="6.3" valueMax="6.8" valueMin="5.8">5.8–6.8</number>
; carapace length 
<number box="[1071,1161,1623,1649]" pageId="30" pageNumber="31" value="2.65" valueMax="2.9" valueMin="2.4">2.4–2.9</number>
, width 
<number box="[1261,1352,1623,1649]" pageId="30" pageNumber="31" value="2.0" valueMax="2.2" valueMin="1.8">1.8–2.2</number>
, index 
<number box="[151,273,1662,1688]" pageId="30" pageNumber="31" value="1.3250000000000002" valueMax="1.35" valueMin="1.3">1.30–1.35</number>
; labial index 
<number box="[435,558,1662,1688]" pageId="30" pageNumber="31" value="1.145" valueMax="1.24" valueMin="1.05">1.05–1.24</number>
; clypeal index 
<number box="[738,861,1662,1688]" pageId="30" pageNumber="31" value="1.46" valueMax="1.67" valueMin="1.25">1.25–1.67</number>
; MOQ index 
<number box="[1025,1134,1662,1688]" pageId="30" pageNumber="31" value="10.05" valueMax="10.8" valueMin="9.3">9.3–10.8</number>
; legs length: I 
<number box="[1311,1404,1662,1688]" pageId="30" pageNumber="31" value="5.8" valueMax="6.3" valueMin="5.3">5.3–6.3</number>
, II 
<number box="[151,242,1701,1727]" pageId="30" pageNumber="31" value="5.3" valueMax="5.8" valueMin="4.8">4.8–5.8</number>
, III 
<number box="[290,381,1701,1727]" pageId="30" pageNumber="31" value="4.800000000000001" valueMax="5.2" valueMin="4.4">4.4–5.2</number>
, IV 
<number box="[431,522,1701,1727]" pageId="30" pageNumber="31" value="7.2" valueMax="7.7" valueMin="6.7">6.7–7.7</number>
