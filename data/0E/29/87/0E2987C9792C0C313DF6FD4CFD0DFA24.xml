<typeStatus box="[464,568,1110,1136]" pageId="28" pageNumber="29">holotype</typeStatus>
+ 
<number box="[604,619,1110,1136]" pageId="28" pageNumber="29" value="7.0">7</number>
ɗɗ; 
<typeStatus box="[686,789,1110,1136]" pageId="28" pageNumber="29">holotype</typeStatus>
