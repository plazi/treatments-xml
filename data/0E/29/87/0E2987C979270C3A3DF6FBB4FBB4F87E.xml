<date box="[299,546,1181,1207]" pageId="23" pageNumber="24" value="2006-12-14">December 14, 2006</date>
, pitfall trap (
<accessionNumber box="[709,849,1181,1207]" httpUri="http://www.ncbi.nlm.nih.gov/protein/HUJ15555" pageId="23" pageNumber="24">
<collectionCode LSID="urn:lsid:biocol.org:col:15399" box="[709,770,1181,1207]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15399" name="Hebrew University" pageId="23" pageNumber="24">HUJ</collectionCode>
<number box="[770,849,1181,1207]" pageId="23" pageNumber="24" value="15555.0">15555</number>
</accessionNumber>
); female 
<typeStatus box="[964,1071,1181,1207]" pageId="23" pageNumber="24">paratype</typeStatus>
