: 
<number box="[551,593,809,832]" pageId="33" pageNumber="34" value="120.0">120</number>
, figs 54, 55 (as 
<taxonomicName box="[762,910,809,832]" class="Arachnida" family="Gnaphosidae" genus="Zelotes" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="meronensis">
