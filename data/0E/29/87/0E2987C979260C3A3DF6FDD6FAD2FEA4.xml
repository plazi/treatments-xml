<collectingCountry box="[365,497,306,332]" name="Azerbaijan" pageId="23" pageNumber="24">Azerbaijan</collectingCountry>
, 
<collectingCountry box="[509,674,306,332]" name="Turkmenistan" pageId="23" pageNumber="24">Turkmenistan</collectingCountry>
, 
<collectingCountry box="[685,750,306,332]" name="Israel" pageId="23" pageNumber="24">Israel</collectingCountry>
- new record. Records. 
<collectingCountry box="[1031,1101,306,332]" name="Israel" pageId="23" pageNumber="24">Israel</collectingCountry>
: 
<taxonomicName box="[1111,1198,306,332]" class="Arachnida" family="Gnaphosidae" genus="Zelotes" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="47" phylum="Arthropoda" rank="species" species="geshur">Geshur</taxonomicName>
, southern 
<taxonomicName box="[1318,1393,306,332]" class="Arachnida" family="Gnaphosidae" genus="Poecilochroa" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="46" phylum="Arthropoda" rank="species" species="golan">Golan</taxonomicName>
.
</paragraph>
