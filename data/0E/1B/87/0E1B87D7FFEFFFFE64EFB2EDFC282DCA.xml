<taxonomicName box="[940,1029,1877,1901]" class="Magnoliopsida" family="Asteraceae" genus="Arctium" kingdom="Plantae" order="Asterales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">
<emphasis box="[940,1029,1877,1901]" italics="true" pageId="10" pageNumber="11">Arctium</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1043,1133,1878,1901]" class="Magnoliopsida" family="Asteraceae" genus="Cirsium" kingdom="Plantae" order="Asterales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">
<emphasis box="[1043,1133,1878,1901]" italics="true" pageId="10" pageNumber="11">Cirsium</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1147,1341,1877,1901]" class="Magnoliopsida" family="Asteraceae" genus="Cynara" kingdom="Plantae" order="Asterales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="scolymus">
<emphasis box="[1147,1341,1877,1901]" italics="true" pageId="10" pageNumber="11">Cynara scolymus</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Doronicum" kingdom="Plantae" order="Asterales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">
<emphasis italics="true" pageId="10" pageNumber="11">Doronicum</emphasis>
</taxonomicName>
, 
<taxonomicName box="[222,338,1913,1937]" class="Magnoliopsida" family="Onagraceae" genus="Epilobium" kingdom="Plantae" order="Myrtales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">
<emphasis box="[222,338,1913,1937]" italics="true" pageId="10" pageNumber="11">Epilobium</emphasis>
</taxonomicName>
, 
<taxonomicName box="[355,425,1913,1937]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">
<emphasis box="[355,425,1913,1937]" italics="true" pageId="10" pageNumber="11">Malus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[442,523,1914,1937]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">
<emphasis box="[442,523,1914,1937]" italics="true" pageId="10" pageNumber="11">Prunus</emphasis>
</taxonomicName>
