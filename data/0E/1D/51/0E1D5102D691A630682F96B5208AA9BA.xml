<taxonomicName class="Insecta" family="Curculionidae" genus="Platygaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platygaster striativentris" order="Coleoptera" pageId="3" pageNumber="102" phylum="Arthropoda" rank="species" species="striativentris">Platygaster striativentris</taxonomicName>
