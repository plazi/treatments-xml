<taxonomicName box="[151,381,1628,1651]" class="Magnoliopsida" family="Euphorbiaceae" genus="Macaranga" kingdom="Plantae" order="Malpighiales" pageId="10" pageNumber="89" phylum="Tracheophyta" rank="species" species="tanarius">
<emphasis box="[151,381,1628,1651]" italics="true" pageId="10" pageNumber="89">Macaranga tanarius</emphasis>
</taxonomicName>
, 
<date box="[396,500,1626,1651]" pageId="10" pageNumber="89" value="2010-04-09">
<collectingDate box="[396,500,1626,1651]" pageId="10" pageNumber="89" value="2010-04-09">
<number box="[396,409,1626,1651]" pageId="10" pageNumber="89" value="9.0">9</number>
.iv. 
<number box="[439,500,1626,1651]" pageId="10" pageNumber="89" value="2010.0">2010</number>
</collectingDate>
</date>
(
<collectionCode box="[516,538,1627,1651]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="10" pageNumber="89">R</collectionCode>
