<taxonomicName authorityName="Sinclair &amp; Shamshev" authorityYear="2021" baseAuthorityName="Melander" box="[514,615,1303,1328]" class="Insecta" genus="Iteaphila" kingdom="Animalia" order="Diptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="stentor">
