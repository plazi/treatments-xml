, 
<taxonomicName authority="Sinclair" authorityName="Sinclair" box="[390,587,1483,1509]" class="Insecta" genus="Iteaphila" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pumila">
