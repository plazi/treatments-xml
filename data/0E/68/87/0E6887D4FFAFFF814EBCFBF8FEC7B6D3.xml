<taxonomicName authority="Shamshev" authorityName="Shamshev" box="[1176,1406,1148,1174]" class="Insecta" genus="Iteaphila" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="cirrata">
