<taxonomicName authority="Zetterstedt" authorityName="Zetterstedt" authorityYear="1838" box="[284,519,1184,1210]" class="Insecta" genus="Iteaphila" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="nitidula">
<emphasis box="[284,393,1184,1210]" italics="true" pageId="30" pageNumber="31">I. nitidula</emphasis>
