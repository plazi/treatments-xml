<collectingRegion box="[1213,1280,1375,1402]" country="Russia" name="Altay" pageId="60" pageNumber="61">Altay</collectingRegion>
), 
<collectingCountry box="[1300,1375,1375,1401]" name="Russia" pageId="60" pageNumber="61">Russia</collectingCountry>
(
