<number box="[1347,1391,1240,1266]" pageId="1" pageNumber="2" value="5.0" valueMax="6.0" valueMin="4.0">4–6</number>
[
<number box="[1410,1428,1240,1266]" pageId="1" pageNumber="2" value="4.0">4</number>
