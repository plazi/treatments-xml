<quantity box="[918,1026,698,721]" metricMagnitude="4" metricUnit="m" metricValue="3.3" metricValueMax="3.4" metricValueMin="3.2" pageId="4" pageNumber="5" unit="km" value="33.0" valueMax="34.0" valueMin="32.0">32–34 km</quantity>
