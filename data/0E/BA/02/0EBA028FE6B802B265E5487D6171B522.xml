<taxonomicName authority="Foerster, 1869" authorityName="Foerster" authorityYear="1869" class="Insecta" family="Figitidae" genus="Trybliographa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trybliographa" order="Hymenoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="genus">
