<taxonomicName authorityName="Fischer" authorityYear="1965" box="[807,995,1294,1321]" class="Insecta" family="Agromyzidae" genus="Sternulopius" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="species" species="bisternaulicus">
<emphasis box="[807,995,1294,1321]" italics="true" pageId="7" pageNumber="595">S. bisternaulicus</emphasis>
</taxonomicName>
are larvae from species 
<taxonomicName authority="Graham, C." authorityName="Graham, C." class="Insecta" family="Tephritidae" genus="Ceratitis" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="species" species="anonae">
<emphasis box="[1277,1436,1298,1319]" italics="true" pageId="7" pageNumber="595">Ceratitis anonae</emphasis>
Graham, C.
</taxonomicName>
<emphasis box="[295,406,1334,1355]" italics="true" pageId="7" pageNumber="595">fascioentris</emphasis>
(Bezzi) of genus 
<taxonomicName authority="MacLeay, 1829" authorityName="MacLeay" authorityYear="1829" box="[624,896,1330,1357]" class="Insecta" family="Tephritidae" genus="Ceratitis" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="genus">
<emphasis box="[624,708,1334,1355]" italics="true" pageId="7" pageNumber="595">Ceratitis</emphasis>
MacLeay, 1829
</taxonomicName>
and 
<taxonomicName authority="Munro" authorityName="Munro" authorityYear="1938" box="[960,1293,1331,1357]" class="Insecta" family="Tephritidae" genus="Trirhithrum" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="595" phylum="Arthropoda" rank="species" species="meladiscum">
<emphasis box="[960,1205,1333,1354]" italics="true" pageId="7" pageNumber="595">Trirhithrum meladiscum</emphasis>
Munro
