<collectingCountry box="[359,426,477,503]" name="China" pageId="7" pageNumber="595">China</collectingCountry>
(Palaearctic: 
<collectingRegion box="[580,684,477,503]" country="China" name="Liaoning" pageId="7" pageNumber="595">Liaoning</collectingRegion>
, 
<collectingRegion box="[695,746,477,503]" country="China" name="Jilin" pageId="7" pageNumber="595">Jilin</collectingRegion>
, 
<collectingRegion box="[758,910,477,504]" country="China" name="Heilongjiang" pageId="7" pageNumber="595">Heilongjiang</collectingRegion>
).
</paragraph>
</subSubSection>
<subSubSection box="[199,422,512,539]" pageId="7" pageNumber="595" type="biology_ecology">
