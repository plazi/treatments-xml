<taxonomicName id="4C16FAD7C87CFFE68DE79AEEFCB7211E" authorityName="J.V. Lamouroux" authorityYear="1809" baseAuthorityName="P.Forsskal" box="[656,860,1015,1039]" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" kingdom="Plantae" order="Bryopsidales" pageId="11" pageNumber="2292" phylum="Chlorophyta" rank="species" species="prolifera">
