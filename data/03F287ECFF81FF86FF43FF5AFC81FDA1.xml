, 
<collectingCountry id="F34C766AFF81FF84FD90F98BFD65F90C" box="[607,678,1654,1676]" name="Canada" pageId="37" pageNumber="118">Canada</collectingCountry>
of 
<taxonomicName id="4C5B4D79FF81FF84FD0BF98BFCB2F90C" authorityName="Casey" authorityYear="1906" box="[708,881,1654,1676]" class="Insecta" family="Staphylinidae" genus="Gyrophaena" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="118" phylum="Arthropoda" rank="species" species="uteana">
