and 
<taxonomicName id="4C00DEEEFFEDFFC0B1C7FE06FF5C42DA" authority="Turner, 2021" authorityName="Turner" authorityYear="2021" class="Demospongiae" family="Scopalinidae" genus="Scopalina" kingdom="Animalia" order="Halichondrida" pageId="11" pageNumber="364" phylum="Porifera" rank="species" species="nausicae" status="sp. nov.">
