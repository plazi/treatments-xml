<taxonomicName id="3949A80799188548E50F3DB368678BD6" LSID="http://zoobank.org/C2408FE6-35E7-45B4-9858-FE61B1747C3B" authority="Cao &amp; Li" class="Arachnida" family="Salticidae" genus="Carrhotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Carrhotus kevinlii" order="Araneae" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="kevinlii">Carrhotus kevinlii Cao &amp; Li</taxonomicName>
