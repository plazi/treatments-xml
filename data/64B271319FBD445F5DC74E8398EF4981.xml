<taxonomicName id="0CBCA8C3C91F460B9EFC169F312A4918" authority="" authorityName="" class="Centricae" family="Chironomidae" genus="C." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="C. cunicularius" order="Diptera" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="cunicularius" subSpecies="celticus">C. cunicularius celticus</taxonomicName>
