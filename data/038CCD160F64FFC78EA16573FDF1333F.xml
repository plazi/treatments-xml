<taxonomicName id="4C2507830F63FFC78F44677BFD85333F" baseAuthorityName="Chen" baseAuthorityYear="1936" box="[370,514,836,860]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="4" pageNumber="153" phylum="Annelida" rank="species" species="fornicatus">
