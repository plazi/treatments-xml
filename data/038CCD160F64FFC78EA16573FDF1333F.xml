<taxonomicName id="4C2507830F63FFC78F06671FFD333354" authority="(Chen, 1936)" baseAuthorityName="Chen" baseAuthorityYear="1936" box="[304,692,799,824]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="4" pageNumber="153" phylum="Annelida" rank="species" species="fornicatus">
<emphasis id="B951A0120F63FFC78F06671FFD91335B" box="[304,534,800,824]" italics="true" pageId="4" pageNumber="153">Amynthas fornicatus</emphasis>
