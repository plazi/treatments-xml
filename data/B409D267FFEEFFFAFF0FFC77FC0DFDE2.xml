<taxonomicName id="FBA018F2FFECFFFDFE85F939FE5BF94D" box="[285,420,1721,1743]" class="Arachnida" family="Gonyleptidae" genus="Nanophareus" higherTaxonomySource="CoL" kingdom="Animalia" order="Opiliones" pageId="18" pageNumber="119" phylum="Arthropoda" rank="genus">Nanophareus</taxonomicName>
<taxonomicName id="FBA018F2FFECFFFDFE34F939FDD3F94D" box="[428,556,1721,1743]" pageId="18" pageNumber="119" rank="species" species="polyhastatus" status="sp. nov.">polyhastatus</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="15E70218FFECFFFDFDADF939FD78F94D" box="[565,647,1721,1743]" pageId="18" pageNumber="119" rank="species">
