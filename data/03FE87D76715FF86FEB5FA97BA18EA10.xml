<number id="3D9F24E66712FF86FC1DFEC2BDC9EF18" box="[928,972,324,350]" pageId="20" pageNumber="21" value="669.0">669</number>
(
<number id="3D9F24E66712FF86FC62FEC2BA4DEF18" box="[991,1096,324,350]" pageId="20" pageNumber="21" value="661.5" valueMax="738.0" valueMin="585.0">585–738</number>
