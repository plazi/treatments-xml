<taxonomicName id="4C74F2A5FF91FFB5FDE3F8E5FD3AAD1C" box="[544,674,1895,1919]" class="Insecta" family="Cercopidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="80" phylum="Arthropoda" rank="family">Cercopidae</taxonomicName>
, 
