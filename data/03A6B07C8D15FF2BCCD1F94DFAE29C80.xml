<taxonomicName id="4C0F7AE98D16FF2BCD04FF73FD839CCE" box="[322,635,152,177]" class="Actinopterygii" family="Synaphobranchidae" genus="Synaphobranchus" kingdom="Animalia" order="Anguilliformes" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="oregoni">
