<taxonomicName id="4C214D4BFFC29F08FABBA9F6541EFC65" authority="Wygodzinsky, 1942" authorityName="Wygodzinsky" authorityYear="1942" box="[151,786,922,948]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="2" pageNumber="42" phylum="Arthropoda" rank="species" species="galilaea" subGenus="Campodea">
