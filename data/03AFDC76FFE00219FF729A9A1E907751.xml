<taxonomicName id="4C0616E3FFE10219FEAE9E521BC87454" authorityName="Vercammen-Grandjean and Langston" authorityYear="1971" box="[331,484,295,321]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="furcagaleala">
<emphasis id="B972B172FFE10219FEAE9E521BC87454" bold="true" box="[331,484,295,321]" italics="true" pageId="45" pageNumber="46">furcagaleala</emphasis>
</taxonomicName>
</emphasis>
</paragraph>
</subSubSection>
<subSubSection id="C31C3EEBFFE10219FF729E1B1E907751" pageId="45" pageNumber="46" type="diagnosis">
<paragraph id="8BB96D60FFE10219FF729E1B1E907751" blockId="45.[151,1436,366,580]" pageId="45" pageNumber="46">
