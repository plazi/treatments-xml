<taxonomicName id="4C414D38FF667AB7FD94F808F8B3F81A" authority="Huber" authorityName="Huber" authorityYear="2020" box="[543,744,1926,1952]" class="Arachnida" family="Pholcidae" genus="Metagonia" kingdom="Animalia" order="Araneae" pageId="182" pageNumber="183" phylum="Arthropoda" rank="species" species="latigo" status="sp. nov.">
