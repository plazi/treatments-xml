<collectingCountry id="F36CB195FD5EFFCCFC80FC99C00EFC3C" box="[801,872,890,915]" name="Iceland" pageId="13" pageNumber="62">Island</collectingCountry>
, 
<collectingCountry id="F36CB195FD5EFFCCFCD2FC98C0D0FC3D" box="[883,950,891,914]" name="Japan" pageId="13" pageNumber="62">Japan</collectingCountry>
, 
<quantity id="4C835CE0FD5EFFCCFC60FC99C76CFC3D" box="[961,1034,890,915]" metricMagnitude="0" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="13" pageNumber="62" unit="m" value="4.5" valueMax="5.0" valueMin="4.0">
4– 
<specimenCount id="9D7D3A8CFD5EFFCCFC7FFC99C76CFC3D" box="[990,1034,890,915]" pageId="13" pageNumber="62" type="generic">5 m</specimenCount>
</quantity>
. And 
