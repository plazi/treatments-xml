<taxonomicName id="03FD4D09B64FCD275B3CFB27FD7FA0A0" authorityName="Boulard &amp; Martinelli" authorityYear="1996" box="[533,650,1223,1249]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="subTribe" subTribe="Guyalnina">Guyalnina</taxonomicName>
