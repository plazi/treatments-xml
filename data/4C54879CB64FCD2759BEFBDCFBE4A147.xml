<taxonomicName id="03FD4D09B64FCD275B7BFB0BFBF8A147" authority="(Boulard &amp; Martinelli 1996)" baseAuthorityName="Boulard &amp; Martinelli" baseAuthorityYear="1996" box="[594,1037,1259,1286]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="subTribe" subTribe="Fidicinina">
