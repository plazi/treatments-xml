<taxonomicName id="57BC4D0BCD4AFFCFFEA17534FDFBBD50" authority="Stekolnikov &amp; Daniel, 2012" authorityName="Stekolnikov &amp; Daniel" authorityYear="2012" box="[293,622,1343,1365]" class="Arachnida" family="Trombiculidae" genus="Neotrombicula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="kizlarsivrisiensis" status="sp. nov.">
