<taxonomicName id="4C2B4D26A219FFD2AAC024F0FD9CFEE5" authority="(Keferstein, 1862)" baseAuthorityName="Keferstein" baseAuthorityYear="1862" box="[212,558,241,264]" class="Polychaeta" family="Syllidae" genus="Pionosyllis" kingdom="Animalia" order="Phyllodocida" pageId="24" pageNumber="281" phylum="Annelida" rank="species" species="divaricata">
