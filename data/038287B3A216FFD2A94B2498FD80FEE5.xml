<taxonomicName id="4C2B4D26A219FFD2A81B24D5FCA4FF06" authority="Day, 1953" authorityName="Day" authorityYear="1953" box="[527,790,212,235]" class="Polychaeta" family="Syllidae" genus="Pionosyllis" kingdom="Animalia" order="Phyllodocida" pageId="24" pageNumber="281" phylum="Annelida" rank="species" species="magnidens">
