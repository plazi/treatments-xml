<taxonomicName id="B06429C6B6AADF8FD640664CBCB95619" authorityName="Linnaeus" authorityYear="1767" class="Insecta" family="Papilionidae" genus="Papilio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papilio belia" order="Lepidoptera" pageId="0" pageNumber="193" phylum="Arthropoda" rank="species" species="belia">
