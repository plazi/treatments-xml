<paragraph id="1168BA7343DCE833DFC2AA268A27FA0F" pageId="0" pageNumber="193">
<taxonomicName id="589099E316152FC7D5A23F93482F39E9" authorityName="Linnaeus" authorityYear="1767" class="Insecta" family="Papilionidae" genus="Papilio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papilio belia" order="Lepidoptera" pageId="0" pageNumber="193" phylum="Arthropoda" rank="species" species="belia">
