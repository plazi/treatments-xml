<geoCoordinate id="A67E7BE0E995354D6D37776FE447D087" direction="east" orientation="longitude" precision="15" value="18.894722">18°53'41&quot;E</geoCoordinate>
, 
<geoCoordinate id="2A1CC9F6906108B54A7AF2E7D3EF28D1" direction="east" orientation="longitude" precision="1" value="98.82544">98°49'31.59&quot;E</geoCoordinate>
