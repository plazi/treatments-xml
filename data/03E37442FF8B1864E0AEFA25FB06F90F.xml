<taxonomicName id="4C4ABED7FF8B1864E2A3FA6AFBC8FA24" authority="Karny, 1920" authorityName="Karny" authorityYear="1920" box="[666,1049,1485,1508]" class="Insecta" family="Phlaeothripidae" genus="Trichothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="56" phylum="Arthropoda" rank="species" species="erinaceus">
<emphasis id="B93E1946FF8B1864E2A3FA6AFC5DFA24" box="[666,908,1485,1508]" italics="true" pageId="15" pageNumber="56">Trichothrips erinaceus</emphasis>
