<paragraph id="8BB136B7FFCCFFA5FF0EFF6AEEB0188F" blockId="8.[151,1436,151,392]" box="[199,493,223,248]" pageId="8" pageNumber="9">
<emphasis id="B97AEAA5FFCCFFA5FF0EFF6AEE3D1880" bold="true" box="[199,352,223,248]" pageId="8" pageNumber="9">Distribution.</emphasis>
<collectingCountry id="F3197627FFCCFFA5FEAEFF6AEEB4188F" box="[359,489,223,247]" name="Philippines" pageId="8" pageNumber="9">Philippines</collectingCountry>
