(
<emphasis id="CFF8EAC54D03FFF4FF08F9C41FD8F9B1" bold="true" box="[170,223,1537,1558]" pageId="6" pageNumber="1239">13-15</emphasis>
) and 
<taxonomicName id="3A8C4D544D03FFF4FEB1F9C41EF1F9B1" authority="Bordoni, 2017" authorityName="Bordoni" authorityYear="2017" box="[275,502,1537,1558]" class="Insecta" family="Staphylinidae" genus="Notolinopsis" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="1239" phylum="Arthropoda" rank="species" species="lemniscatus" status="nov.sp.">
