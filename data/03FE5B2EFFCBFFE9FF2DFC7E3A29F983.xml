<taxonomicName id="4C5791BBFFCBFFE9FF2DFC7E39A5FBDD" authority="Fabricius, 1775" authorityName="Fabricius" authorityYear="1775" box="[151,420,1014,1040]" class="Insecta" family="Therevidae" genus="Bibio" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="74" phylum="Arthropoda" rank="genus">
<emphasis id="B923362AFFCBFFE9FF2DFC7E38D9FBDD" bold="true" box="[151,216,1014,1040]" italics="true" pageId="1" pageNumber="74">Bibio</emphasis>
<bibRefCitation id="EFC697C9FFCBFFE9FF65FC7E39A5FBDD" author="Fabricius, J. C." box="[223,420,1014,1040]" pageId="1" pageNumber="74" refId="ref4560" refString="Fabricius, J. C. (1775) Systema entomologiae, sistens insectorum classes, ordines, genera, species, adiectis synonymis, locis, descriptionibus, observationibus. Kortii, Flensbvrgi et Lipsiae [= Flensburg and Leipzig], [30] + 832 pp. http: // dx. doi. org / 10.5962 / bhl. title. 36510" type="book" year="1775">Fabricius, 1775</bibRefCitation>
