<taxonomicName id="E2A74D5EBB57FF87FF2DF9872D67F9B4" authorityName="Burmeister" authorityYear="1842" box="[173,268,1549,1570]" class="Insecta" family="Cetoniidae" genus="Allorrhina" kingdom="Animalia" order="Coleoptera" pageId="21" phylum="Arthropoda" rank="genus">
