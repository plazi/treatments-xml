<taxonomicName id="76E94D1A2836FFF8CD824B0B61E4FE90" authority="Emery, 1896" authorityName="Emery" authorityYear="1896" box="[151,406,260,286]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="20" phylum="Arthropoda" rank="species" species="treubi">
<emphasis id="839DEA8B2836FFF8CD824B0B61E4FE90" bold="true" box="[151,406,260,286]" italics="true" pageId="2" pageNumber="20">Crematogaster treubi</emphasis>
</taxonomicName>
