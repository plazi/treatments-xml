<taxonomicName id="4C734D612408CF13E28351EDFCBDFD7E" authority="Saigusa et Sugimoto" authorityName="Saigusa et Sugimoto" box="[470,889,718,741]" class="Insecta" family="Psychidae" genus="Proutia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="677" phylum="Arthropoda" rank="species" species="maculatella">
