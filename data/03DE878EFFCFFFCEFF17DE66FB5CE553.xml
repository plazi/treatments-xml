<collectingCountry id="F3607608FFCCFFCEFC9ADA12FC06E553" box="[794,914,810,833]" name="Azerbaijan" pageId="3" pageNumber="600">Azerbaijan</collectingCountry>
, West 
<collectingCountry id="F3607608FFCCFFCEFC58DA12FBFFE553" box="[984,1131,810,833]" name="Turkmenistan" pageId="3" pageNumber="600">Turkmenistan</collectingCountry>
(
<figureCitation id="134C2A1DFFCCFFCEFBF9DA12FB2EE553" box="[1145,1210,810,833]" captionStart="FIGURE 5" captionStartId="2.[151,244,1798,1820]" captionTargetBox="[151,1436,191,1777]" captionTargetId="figure@2.[151,1436,191,1777]" captionTargetPageId="2" captionText="FIGURE 5. Distribution of Heterocerus fausti Reitter, 1879. Red circles—studied material." httpUri="https://zenodo.org/record/1301564/files/figure.png" pageId="3" pageNumber="600">Fig. 5</figureCitation>
