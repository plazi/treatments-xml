<taxonomicName id="4C774D1BFFCDFFCFFE25DE3FFD4FE10E" authority="Reitter, 1879" authorityName="Reitter" authorityYear="1879" box="[421,731,1799,1820]" class="Insecta" family="Heteroceridae" genus="Heterocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="599" phylum="Arthropoda" rank="species" species="fausti">
