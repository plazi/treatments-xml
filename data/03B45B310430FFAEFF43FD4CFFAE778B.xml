<quantity id="4CE547C20430FFA0FC8FF96EFEF57388" box="[859,948,1686,1710]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="23" pageNumber="24" unit="mm" value="3.2">3.2 mm</quantity>
