<taxonomicName id="448A77F6FFD0FFF4FC57FD06FBAFB08C" box="[983,1035,741,766]" class="Magnoliopsida" family="Malvaceae" genus="Tilia" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="154" phylum="Tracheophyta" rank="genus">
