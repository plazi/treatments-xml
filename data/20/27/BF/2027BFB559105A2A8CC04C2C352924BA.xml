species, including 
<taxonomicName class="Insecta" family="Polycitoridae" genus="Salix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Salix alba" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="alba">
<emphasis italics="true" pageId="0" pageNumber="187">Salix alba</emphasis>
</taxonomicName>
, 
<taxonomicName class="Insecta" family="Polycitoridae" genus="Salix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Salix fragilis" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="fragilis">
