<taxonomicName authorityName="Chamberlin &amp; Ivie" authorityYear="1943" box="[1014,1155,368,392]" class="Arachnida" family="Linyphiidae" genus="Arcuphantes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="509" phylum="Arthropoda" rank="genus">
<emphasis box="[1014,1155,368,392]" italics="true" pageId="11" pageNumber="509">Arcuphantes</emphasis>
</taxonomicName>
species with 
<taxonomicName authorityName="Chamberlin &amp; Ivie" authorityYear="1943" box="[1308,1427,368,392]" class="Arachnida" family="Linyphiidae" genus="Arcuphantes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="509" phylum="Arthropoda" rank="species" species="arcuatulus">
<emphasis box="[1308,1427,368,392]" italics="true" pageId="11" pageNumber="509">arcuatulus</emphasis>
</taxonomicName>
- 
<typeStatus box="[151,199,405,429]" pageId="11" pageNumber="509">type</typeStatus>
genitalia.
</paragraph>
</subSubSection>
