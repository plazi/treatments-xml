<taxonomicName authority="Svihla 2008a" authorityName="Svihla" authorityYear="2008" box="[900,1196,295,320]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="325" phylum="Arthropoda" rank="order">
