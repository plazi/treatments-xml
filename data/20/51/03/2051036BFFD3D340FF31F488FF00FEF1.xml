<taxonomicName box="[151,242,295,320]" class="Insecta" family="Oedemeridae" genus="Chitona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="325" phylum="Arthropoda" rank="genus">
