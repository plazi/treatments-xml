<collectingCountry box="[651,699,547,570]" name="Iran" pageId="5" pageNumber="326">Iran</collectingCountry>
, Transcaucasus, 
<collectingCountry box="[899,980,546,571]" name="Turkey" pageId="5" pageNumber="326">Turkey</collectingCountry>
, 
<collectingCountry box="[998,1153,546,571]" name="Turkmenistan" pageId="5" pageNumber="326">Turkmenistan</collectingCountry>
(
<bibRefCitation author="Vazquez" box="[1174,1346,547,571]" pageId="5" pageNumber="326" refString="Vazquez, X. A. (2002) European fauna of Oedemeridae (Coleoptera). Agraria Editio, Barcelona, 178." type="book chapter" year="2002">Vázquez 2002</bibRefCitation>
