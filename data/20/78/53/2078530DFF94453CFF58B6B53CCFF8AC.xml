<collectingCountry box="[600,670,1569,1594]" name="China" pageId="2" pageNumber="489">China</collectingCountry>
, including 
<specimenCount box="[811,958,1569,1594]" pageId="2" pageNumber="489" type="generic">4 specimens</specimenCount>
