<taxonomicName class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus" order="Malpighiales" pageId="5" pageNumber="60" phylum="Tracheophyta" rank="genus">Populus</taxonomicName>
