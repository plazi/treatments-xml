<taxonomicName authority="Mulsant, 1850" authorityName="Mulsant" authorityYear="1850" lsidName="" pageId="169" pageNumber="170" rank="tribe" tribe="Chnoodini">Chnoodini Mulsant, 1850</taxonomicName>
