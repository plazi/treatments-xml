<paragraph blockId="32.[178,1085,794,1006]" pageId="32" pageNumber="1709">
<collectingRegion box="[178,262,794,814]" country="Austria" name="Salzburg" pageId="32" pageNumber="1709">Salzburg</collectingRegion>
