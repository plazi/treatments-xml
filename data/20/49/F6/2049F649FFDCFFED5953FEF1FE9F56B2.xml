<subSubSection box="[189,310,713,739]" pageId="31" pageNumber="65" type="diagnosis">
<paragraph blockId="31.[189,310,713,739]" box="[189,310,713,739]" pageId="31" pageNumber="65">
<heading bold="true" box="[189,310,713,739]" fontSize="11" level="3" pageId="31" pageNumber="65" reason="3">
