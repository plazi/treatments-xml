<subSubSection pageId="31" pageNumber="65" type="diagnosis">
<paragraph blockId="31.[189,457,430,456]" box="[189,457,430,456]" pageId="31" pageNumber="65">
<heading bold="true" box="[189,457,430,456]" fontSize="11" level="3" pageId="31" pageNumber="65" reason="3">
