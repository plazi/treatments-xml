<geoCoordinate degrees="22" direction="north" minutes="26" orientation="latitude" precision="1" seconds="0.78" value="22.43355">22°26'0.78&quot;N</geoCoordinate>
, 
<geoCoordinate degrees="105" direction="east" minutes="53" orientation="longitude" precision="1" seconds="4.692" value="105.884636">105°53'4.692&quot;E</geoCoordinate>
), CAV [chasse 
