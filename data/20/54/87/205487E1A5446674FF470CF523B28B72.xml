<paragraph blockId="1.[151,932,222,245]" box="[151,932,222,245]" pageId="1" pageNumber="286">
<typeStatus box="[151,201,223,244]" pageId="1" pageNumber="286">Type</typeStatus>
species: 
<taxonomicName authority="Breddin, 1903" authorityName="Breddin" authorityYear="1903" box="[294,680,222,245]" class="Insecta" family="Cicadellidae" genus="Tettigonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="286" phylum="Arthropoda" rank="species" species="vespertinula">
