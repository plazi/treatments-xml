</emphasis>
<collectingCounty box="[641,732,488,514]" pageId="6" pageNumber="22">Nymph</collectingCounty>
, 
<collectingRegion box="[747,1001,488,514]" country="Brazil" name="Santa Catarina" pageId="6" pageNumber="22">SANTA CATARINA</collectingRegion>
, 
<collectingMunicipality box="[1016,1167,488,514]" pageId="6" pageNumber="22">Passos Maia</collectingMunicipality>
, 
