<materialsCitation collectingDate="2009-03-13" collectionCode="UFES" county="Nymph" elevation="1031" latitude="-26.767128" location="Rio Chapecozinho" longLatPrecision="1" longitude="-51.830498" municipality="Passos Maia" pageId="6" pageNumber="22" specimenCount="1" stateProvince="Santa Catarina" typeStatus="holotype">
