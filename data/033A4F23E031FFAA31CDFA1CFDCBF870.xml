<taxonomicName id="4C9385B6E031FFAA3516F94AFD0BF8C0" authority="Liu &amp; Chen" authorityName="Liu &amp; Chen" box="[1100,1372,1779,1801]" class="Insecta" family="Braconidae" genus="Apanteles" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="379" phylum="Arthropoda" rank="species" species="magnioculus" status="sp. n.">
