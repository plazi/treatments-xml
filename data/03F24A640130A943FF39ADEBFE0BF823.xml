<taxonomicName id="4C5B80F10130A943FCD1ADC0FC3FF818" authorityName="Costa" authorityYear="1836" box="[815,931,1974,1999]" class="Insecta" family="Nymphalidae" genus="Hyponephele" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="143" phylum="Arthropoda" rank="species" species="lupinus">
<emphasis id="B92F27600130A943FCD1ADC0FC3FF818" box="[815,931,1974,1999]" italics="true" pageId="15" pageNumber="143">H. lupinus</emphasis>
</taxonomicName>
from 
<collectingCountry id="F34CBBE20130A943FC18ADC0FBEEF807" box="[998,1138,1976,2000]" name="Serbia" pageId="15" pageNumber="143">Montenegro</collectingCountry>
