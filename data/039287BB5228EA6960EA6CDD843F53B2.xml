<taxonomicName id="4C3B4D2E5228EA6961AF6D6D872D53FA" authorityName="Stephens" authorityYear="1835" box="[466,564,366,391]" class="Insecta" family="Ectobiidae" genus="Ectobius" kingdom="Animalia" order="Blattodea" pageId="3" pageNumber="344" phylum="Arthropoda" rank="genus">
