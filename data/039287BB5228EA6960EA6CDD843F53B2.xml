<taxonomicName id="4C3B4D2E5228EA6962FB6D6D87EE53FA" authorityName="Brunner von Wattenwyl" authorityYear="1882" box="[646,759,366,391]" class="Insecta" kingdom="Animalia" order="Blattodea" pageId="3" pageNumber="344" phylum="Arthropoda" rank="order">Blattodea</taxonomicName>
