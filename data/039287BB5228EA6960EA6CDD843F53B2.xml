<taxonomicName id="4C3B4D2E5228EA6962D56D91861E53D6" box="[680,775,402,427]" class="Insecta" family="Asilidae" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="344" phylum="Arthropoda" rank="family">Asilidae</taxonomicName>
