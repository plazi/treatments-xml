<taxonomicName id="4C3B4D2E5228EA69623E6D91878453D6" authorityName="" authorityYear="1758" box="[579,669,402,427]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="344" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
