<taxonomicName id="B4962BDBD492300582F565CEFEF11861" authorityName="Zhang &amp; Xiang" authorityYear="2022" class="Insecta" family="Crambidae" genus="Ecpyrrhorrhoe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ecpyrrhorrhoe brevis" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="brevis">
