<collectingCountry id="93FD7657A371FF951273FEBBFB533DE0" box="[1033,1186,376,398]" name="Costa Rica" pageId="91" pageNumber="88">COSTA RICA</collectingCountry>
: 
<collectingRegion id="292EF825A371FF9512D1FEBBFAD73DE3" box="[1195,1318,376,397]" country="Costa Rica" name="Puntarenas" pageId="91" pageNumber="88">Puntarenas</collectingRegion>
