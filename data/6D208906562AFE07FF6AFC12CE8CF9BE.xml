<collectingCountry id="9D9E7880562AFE07FE4DFA37CE43FA76" box="[432,549,1416,1442]" name="Argentina" pageId="10" pageNumber="49">Argentina</collectingCountry>
and 
<collectingCountry id="9D9E7880562AFE07FDA3FA37CEADFA76" box="[606,715,1416,1442]" name="Paraguay" pageId="10" pageNumber="49">Paraguay</collectingCountry>
