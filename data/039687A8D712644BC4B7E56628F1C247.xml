<typeStatus id="5484881CD713644BC582E11C2854C247" box="[418,512,334,354]" pageId="11" pageNumber="486">Holotypus</typeStatus>
, 
<typeStatus id="5484881CD713644BC628E11C2836C247" box="[520,610,334,354]" pageId="11" pageNumber="486">Paratypen</typeStatus>
