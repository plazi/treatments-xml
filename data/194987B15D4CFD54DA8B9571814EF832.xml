<typeStatus id="4E5B88055D4CFD54DBB795CE82D5FA5F" box="[427,477,1410,1434]" pageId="48" pageNumber="203">Type</typeStatus>
species: 
<taxonomicName id="56E04D245D4CFD54D82695CE8012FA5C" authorityName="Jordan &amp; Starks" authorityYear="1904" box="[570,794,1410,1433]" class="Actinopterygii" family="Bothidae" genus="Engyprosopon" kingdom="Animalia" order="Pleuronectiformes" pageId="48" pageNumber="203" phylum="Chordata" rank="species" species="iijimae">
