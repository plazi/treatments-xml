<collectingCountry id="FABD538AFF9EFFBAFED4FA8850BAEE77" box="[359,426,1338,1363]" name="China" pageId="13" pageNumber="14">China</collectingCountry>
(
<collectingRegion id="406EDDF8FF9EFFBAFE0AFA895305EE77" box="[441,533,1339,1363]" country="China" name="Yunnan" pageId="13" pageNumber="14">Yunnan</collectingRegion>
