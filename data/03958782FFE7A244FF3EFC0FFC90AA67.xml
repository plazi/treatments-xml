<taxonomicName id="4C3C4D17FFE7A247FF3EFBEDFDE1A834" authority="O. Bang-Haas, 1927" authorityName="O. Bang-Haas" authorityYear="1927" box="[151,587,1121,1146]" class="Insecta" family="Papilionidae" genus="Cosmodesmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="82" phylum="Arthropoda" rank="species" species="hoenei">
