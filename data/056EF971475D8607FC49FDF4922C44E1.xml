, 
<collectingCountry id="F5D008F7475C8607FF7FFF4F916F4740" box="[216,349,235,259]" name="Serbia" pageId="17" pageNumber="16">Montenegro</collectingCountry>
, North 
<collectingCountry id="F5D008F7475C8607FE1DFF4F92014740" box="[442,563,235,259]" name="Macedonia" pageId="17" pageNumber="16">Macedonia</collectingCountry>
, 
<collectingCountry id="F5D008F7475C8607FDE2FF4F92BF4740" box="[581,653,235,259]" name="Serbia" pageId="17" pageNumber="16">Serbia</collectingCountry>
), formerly listed under 
<taxonomicName id="4AC733E4475C8607FF50FEAF91F44761" authorityName="Kovařík &amp; Štundlová &amp; Fet &amp; Šťáhlavský" authorityYear="2019" baseAuthorityName="Di Caporiacco" baseAuthorityYear="1950" box="[247,454,267,290]" class="Arachnida" family="Euscorpiidae" genus="Euscorpius" kingdom="Animalia" order="Scorpiones" pageId="17" pageNumber="16" phylum="Arthropoda" rank="species" species="gamma">
<emphasis id="BFB39475475C8607FF50FEAF91F44761" box="[247,454,267,290]" italics="true" pageId="17" pageNumber="16">Euscorpius gamma</emphasis>
</taxonomicName>
or 
<taxonomicName id="4AC733E4475C8607FE4CFEAF92B54760" authorityName="Kovařík &amp; Štundlová &amp; Fet &amp; Šťáhlavský" authorityYear="2019" baseAuthorityName="Kessler" baseAuthorityYear="1874" box="[491,647,267,291]" class="Arachnida" family="Euscorpiidae" genus="Alpiscorpius" kingdom="Animalia" order="Scorpiones" pageId="17" pageNumber="16" phylum="Arthropoda" rank="species" species="mingrelicus">
<emphasis id="BFB39475475C8607FE4CFEAF92B54760" box="[491,647,267,291]" italics="true" pageId="17" pageNumber="16">E. mingrelicus</emphasis>
</taxonomicName>
(
<bibRefCitation id="E9563596475C8607FD31FEAF90984700" author="BONACINA" pageId="17" pageNumber="16" pagination="47 - 100" refId="ref26484" refString="BONACINA, A. 1980. Sistematica specifica e sottospecifica del complesso &quot; Euscorpius germanus &quot; (Scorpiones, Chactidae). Rivista del Museo Civico di Scienze Naturali &quot; Enrico Caffi &quot; (Bergamo), 2: 47 - 100." type="journal article" year="1980">Bonacina, 1980</bibRefCitation>
; Fet &amp; Sissom, 2000; 
<bibRefCitation id="E9563596475C8607FE0DFE8F92BE4700" author="GRAHAM" box="[426,652,299,323]" pageId="17" pageNumber="16" pagination="41 - 50" refId="ref27267" refString="GRAHAM, M. R., M. M. WEBBER, G. BLAGOEV, N. IVANOVA &amp; V. FET. 2012. Molecular and morphological evidence supports the elevation of Euscorpius germanus croaticus Di Caporiacco, 1950 (Scorpiones: Euscorpiidae) to Euscorpius croaticus stat. nov., a rare species from Croatia. Revista Iberica de Aracnologia, 21: 41 - 50." type="journal article" year="2012">Graham et al., 2012</bibRefCitation>
