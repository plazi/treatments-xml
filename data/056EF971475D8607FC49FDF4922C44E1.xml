<taxonomicName id="4AC733E4475C8607FECAFE6F90944440" authority="Bonacina, 1980" authorityName="Bonacina" authorityYear="1980" class="Arachnida" family="Euscorpiidae" genus="Alpiscorpius" kingdom="Animalia" order="Scorpiones" pageId="17" pageNumber="16" phylum="Arthropoda" rank="subSpecies" species="mingrelicus" subSpecies="caporiaccoi">
