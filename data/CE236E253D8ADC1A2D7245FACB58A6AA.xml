<taxonomicName id="D832149C72BE32DD6983D1EE97A19B56" class="Insecta" family="Ciidae" genus="Cis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cis" order="Coleoptera" pageId="5" pageNumber="30" phylum="Arthropoda" rank="genus">Cis</taxonomicName>
