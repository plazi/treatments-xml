<paragraph id="FD1F783B9E932AC22AA66441E7585C17" pageId="5" pageNumber="30">
<taxonomicName id="B75593B4CA0BECF98C5395C6C5340D37" class="Agaricomycetes" family="Fomitopsidaceae" genus="Laetiporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Laetiporus" order="Polyporales" pageId="5" pageNumber="30" phylum="Basidiomycota" rank="genus">Laetiporus</taxonomicName>
sp. (
<taxonomicName id="BF0B8E0237412CA4E2D2247284756F37" family="Fomitopsidaceae" lsidName="" pageId="5" pageNumber="30" rank="family">Fomitopsidaceae</taxonomicName>
), one record; 
<taxonomicName id="331E26789631C317A32E8714119F6D35" class="Agaricomycetes" family="Russulaceae" genus="Russula" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Russula capensis" order="Russulales" pageId="5" pageNumber="30" phylum="Basidiomycota" rank="species" species="capensis">Russula capensis</taxonomicName>
A. Pearson (
<taxonomicName id="F039147694EEEE3EFFC3323C33FFC509" family="Russulaceae" lsidName="" pageId="5" pageNumber="30" rank="family">Russulaceae</taxonomicName>
