<taxonomicName id="4C6BF6CDFFE1FFDD1E98879FFC95FD5B" authority="Desmarest, 1817" authorityName="Desmarest" authorityYear="1817" class="Malacostraca" family="Eryonidae" genus="Eryon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="654" phylum="Arthropoda" rank="species" species="cuvieri">
