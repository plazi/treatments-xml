(
<bibRefCitation id="BC004B7ACE285505926C2AB6FD5DF926" author="Attems, C." box="[602,744,1717,1741]" pageId="3" pageNumber="575" pagination="133 - 323" refId="ref2812" refString="Attems, C. (1936) Diplopoda of India. Memoirs of the Indian Museum, 11 (4), 133 - 323." type="journal article" year="1936">Attems, 1936</bibRefCitation>
)
</taxonomicName>
, 
<emphasis id="EAE5EA99CE28550592CF2AB6FCD8F926" bold="true" box="[761,877,1717,1741]" pageId="3" pageNumber="575">
<taxonomicNameLabel id="F1D657E2CE28550592CF2AB6FCD8F926" box="[761,877,1717,1741]" pageId="3" pageNumber="575" rank="species">comb. nov.</taxonomicNameLabel>
</emphasis>
