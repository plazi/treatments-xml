<taxonomicName id="1F914D08CE2F550291682B16FCB3F8C6" authority="(Attems, 1936)" authorityName="Golovatch" authorityYear="2021" baseAuthorityName="Attems" baseAuthorityYear="1936" box="[350,774,1813,1837]" class="Diplopoda" family="Pyrgodesmidae" genus="Pyrgodesmus" kingdom="Animalia" order="Polydesmida" pageId="4" pageNumber="576" phylum="Arthropoda" rank="species" species="permutatus" status="comb. nov.">
