(
<bibRefCitation id="BC004B7ACE2F550292462B16FD4AF8C6" author="Attems, C." box="[624,767,1813,1837]" pageId="4" pageNumber="576" pagination="133 - 323" refId="ref2812" refString="Attems, C. (1936) Diplopoda of India. Memoirs of the Indian Museum, 11 (4), 133 - 323." type="journal article" year="1936">Attems, 1936</bibRefCitation>
)
</taxonomicName>
, 
<emphasis id="EAE5EA99CE2F550293252B16FC3DF8C6" bold="true" box="[787,904,1813,1837]" pageId="4" pageNumber="576">
