<taxonomicName id="ADB17E97FFE55162FB90FDFC48E7CB8B" box="[1140,1249,626,651]" class="Liliopsida" family="Cymodoceaceae" genus="Halodule" kingdom="Plantae" order="Alismatales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
<emphasis id="58C5D906FFE55162FB90FDFC48E7CB8B" box="[1140,1249,626,651]" italics="true" pageId="22" pageNumber="23">Halodule</emphasis>
</taxonomicName>
and 
<taxonomicName id="ADB17E97FFE55162FAC1FDFC499ACB8B" box="[1317,1436,626,651]" class="Gymnolaemata" family="Bugulidae" genus="Halophila" kingdom="Animalia" order="Cheilostomatida" pageId="22" pageNumber="23" phylum="Bryozoa" rank="genus">
<emphasis id="58C5D906FFE55162FAC1FDFC499ACB8B" box="[1317,1436,626,651]" italics="true" pageId="22" pageNumber="23">Halophila</emphasis>
</taxonomicName>
meadows. Intertidal (Melo 
<number id="DC791733FFE55162FE32FD144E13CBB4" box="[470,533,666,692]" pageId="22" pageNumber="23" value="1999.0">1999</number>
).
</paragraph>
</subSubSection>
