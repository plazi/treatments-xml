<taxonomicName id="5770CAFE80666638E8380FAAC7D90AE3" class="Insecta" family="Corylophidae" genus="Clypastraea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Clypastraea" order="Coleoptera" pageId="174" pageNumber="175" phylum="Arthropoda" rank="genus">Clypastraea</taxonomicName>
