<taxonomicName id="35483F93456C36E2E20D1E16A4692CB3" class="Insecta" family="Corylophidae" genus="Clypastraea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Clypastraea" order="Coleoptera" pageId="174" pageNumber="175" phylum="Arthropoda" rank="genus">Clypastraea</taxonomicName>
