</taxonomicName>
, 
<taxonomicName id="4C7B4D4DFFDEFF9EFCAC116CFC28FBA7" box="[869,1014,1051,1075]" class="Insecta" family="Ripiphoridae" genus="Macrosiagon" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="617" phylum="Arthropoda" rank="species" species="medvedevi">
<emphasis id="B90FEADCFFDEFF9EFCAC116CFCA5FBA7" box="[869,891,1052,1075]" italics="true" pageId="30" pageNumber="617">M</emphasis>
