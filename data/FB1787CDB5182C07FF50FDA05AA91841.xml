Syn. 
<taxonomicName id="B4BE4D58B5182C04FF0AFD17592418AF" authority="Douthitt, 1915" authorityName="Douthitt" authorityYear="1915" box="[205,589,661,684]" class="Cestoda" family="Anoplocephalidae" genus="Andrya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="4" pageNumber="55" phylum="Platyhelminthes" rank="species" species="primordialis">
