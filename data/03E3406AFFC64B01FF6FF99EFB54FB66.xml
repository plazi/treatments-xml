<taxonomicName id="4C4A8AFFFFC64B02FF6FF95DFE9EF96C" box="[151,274,1742,1765]" class="Insecta" family="Scelionidae" genus="Dicroscelio" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
