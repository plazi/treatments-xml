<taxonomicName id="4C614D68FFCDFFE2F2A95E3DD860FDD8" authority="A. H. Clark, 1909 b: 636" authorityName="A. H. Clark" authorityPageNumber="636" authorityYear="1909" box="[151,601,580,605]" class="Crinoidea" family="Himerometridae" genus="Heterometra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="10" pageNumber="257" phylum="Echinodermata" rank="species" species="compta">
