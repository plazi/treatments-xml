<specimenCount id="E2DBFD59FFFEA262FDF9AAE1FD5DF91E" box="[573,659,1687,1711]" pageId="1" pageNumber="42" type="male">
<number id="421524F7FFFEA262FDF9AAE1FD85F91E" box="[573,587,1687,1711]" pageId="1" pageNumber="42" value="3.0">3</number>
males
