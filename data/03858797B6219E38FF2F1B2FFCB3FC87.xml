<taxonomicName id="4C2C4D02B6219E37FA3A1BBEFECFFA85" authority="Gray, 1845" authorityName="Gray" authorityYear="1845" class="Reptilia" family="Scincidae" genus="Hinulia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="richardsonii">
<emphasis id="B958EA93B6219E37FA3A1BBEFA70FB63" box="[1410,1428,1247,1272]" italics="true" pageId="7" pageNumber="8">E</emphasis>
