<taxonomicName id="4C2C4D02B6219E37FB2D1BBEFAA7FB63" box="[1173,1347,1247,1272]" class="Reptilia" family="Scincidae" genus="Eremiascincus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="fasciolatus">
