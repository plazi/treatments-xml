<paragraph id="8B933681B6219E37FF2F1BBFFE5AFA21" blockId="7.[151,1437,1246,2007]" pageId="7" pageNumber="8">
<emphasis id="B958EA93B6219E37FF2F1BBFFEEFFB63" bold="true" box="[151,267,1246,1272]" pageId="7" pageNumber="8">Content.</emphasis>
<taxonomicName id="4C2C4D02B6219E37FEAC1BBEFE28FB63" box="[276,460,1247,1272]" class="Reptilia" family="Scincidae" genus="Eremiascincus" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="8" phylum="Chordata" rank="genus">
