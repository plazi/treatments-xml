<taxonomicName id="AFD9A5961249FFC4FE3EFED6FCC5FE85" authority="Lamarck, 1804" authorityName="Lamarck" authorityYear="1804" box="[509,864,295,320]" class="Gastropoda" family="Eulimidae" genus="Melania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neotaenioglossa" pageId="51" pageNumber="52" phylum="Mollusca" rank="species" species="nitida">
