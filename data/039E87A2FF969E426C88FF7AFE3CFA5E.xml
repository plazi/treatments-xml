<taxonomicName id="4C374D37FF969E42699BF93EFEAFFBA8" class="Insecta" family="Ephydridae" genus="Lemnaphila" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="44" phylum="Arthropoda" rank="species" species="neotropica">
