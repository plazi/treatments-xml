<taxonomicName id="4C374D37FF969E426867F944FB51FB83" box="[1144,1259,1763,1788]" class="Liliopsida" family="Araceae" genus="Lemna" kingdom="Plantae" order="Alismatales" pageId="2" pageNumber="44" phylum="Tracheophyta" rank="species" species="minuta">
