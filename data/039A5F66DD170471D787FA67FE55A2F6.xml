<taxonomicName id="4C3395F3DD100471D5C4FCB2F91BA521" box="[724,851,820,842]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="opulentus">
