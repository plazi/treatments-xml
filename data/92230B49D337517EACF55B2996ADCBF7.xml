<taxonomicName id="51941B7E25545D6857D9BB04773649A5" class="Arachnida" genus="P." kingdom="Animalia" lsidName="P. bawang" order="Araneae" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="bawang">
<emphasis id="916A308EB5C14336125E48923792E8C5" italics="true" pageId="0" pageNumber="27">P. bawang</emphasis>
</taxonomicName>
