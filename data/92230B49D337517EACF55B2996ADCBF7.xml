, 
<taxonomicName id="0505423F80E3361306E567348726A9D4" class="Arachnida" genus="P." kingdom="Animalia" lsidName="P. taiwanica" order="Araneae" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="taiwanica">
