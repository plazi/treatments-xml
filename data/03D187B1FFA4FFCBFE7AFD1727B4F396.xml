species. 
<taxonomicName id="4C784D24FFA4FFCBFEB1FCD72494F657" authority="Vandel, 1963" authorityName="Vandel" authorityYear="1963" box="[318,704,870,894]" class="Malacostraca" family="Pudeoniscidae" genus="Pudeoniscus" kingdom="Animalia" order="Isopoda" pageId="5" pageNumber="461" phylum="Arthropoda" rank="species" species="birabeni">
