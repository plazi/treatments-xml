<taxonomicName id="4C319EBF1916FFFA6BA996801D42F925" baseAuthorityName="P.L.Sclater" baseAuthorityYear="1869" box="[469,611,1676,1699]" class="Aves" family="Sturnidae" genus="Mino" kingdom="Animalia" order="Passeriformes" pageId="41" pageNumber="41" phylum="Chordata" rank="species" species="kreffti">
