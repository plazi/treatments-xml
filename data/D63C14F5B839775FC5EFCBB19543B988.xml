<paragraph id="B9ACE949FA729C83537E2E2BC81ADAFC" pageId="0" pageNumber="1">Similar species.</paragraph>
<paragraph id="EE39A02380456E783AAC8C70D14BA756" pageId="0" pageNumber="1">
<taxonomicName id="8E26F44FCA889E34F7A18DC702B7C355" baseAuthorityName="Eggers" baseAuthorityYear="1923" class="Insecta" family="Curculionidae" genus="Arixyleborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arixyleborus mediosectus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="mediosectus">
