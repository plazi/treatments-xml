<taxonomicName id="4B8285BBFFC82810FEB9196CE0C1FE81" authority="Prather Table Of Contents, 2004" authorityName="Prather Table Of Contents" authorityYear="2004" box="[264,703,283,310]" class="Insecta" family="Calamoceratidae" genus="Banyallarga" kingdom="Animalia" order="Trichoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="sylvana" status="new species" subGenus="Histricoverpa">
<emphasis id="BEF6222AFFC82810FEB9196CE3E0FE81" bold="true" box="[264,414,284,310]" italics="true" pageId="31" pageNumber="32">Banyallarga</emphasis>
(
<emphasis id="BEF6222AFFC82810FE1E196CE02DFE81" bold="true" box="[431,595,284,310]" italics="true" pageId="31" pageNumber="32">Histricoverpa</emphasis>
