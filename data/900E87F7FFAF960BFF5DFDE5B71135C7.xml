, 
<collectingCountry id="60B07671FFAF960BFEEAFA12B4563584" box="[287,379,1436,1460]" name="Ecuador" pageId="12" pageNumber="785">Ecuador</collectingCountry>
, French Guiana, 
<collectingCountry id="60B07671FFAF960BFDC4FA12B7A53584" box="[561,648,1436,1460]" name="Guyana" pageId="12" pageNumber="785">Guyana</collectingCountry>
, 
<collectingCountry id="60B07671FFAF960BFD66FA13B7E53584" box="[659,712,1437,1460]" name="Peru" pageId="12" pageNumber="785">Peru</collectingCountry>
, Suri- name and 
<collectingCountry id="60B07671FFAF960BFEE2FA30B4AA35E6" box="[279,391,1470,1494]" name="Venezuela" pageId="12" pageNumber="785">Venezuela</collectingCountry>
(de la 
<bibRefCitation id="7C364B10FFAF960BFE3AFA30B75135E6" author="Riva I de la &amp; Kohler J &amp; Lotters S &amp; Reichle S" box="[463,636,1470,1494]" pageId="12" pageNumber="785" pagination="19 - 164" refId="ref15873" refString="Riva I de la, Kohler J, Lotters S, Reichle S (2000) Ten years of research on Bolivian amphibians: updated checklist, distribution, taxonomic problems, literature and iconography. Revista Espanola de Herpetologia 14: 19 - 164." type="journal article" year="2000">Riva et al. 2000</bibRefCitation>
, M. 
<bibRefCitation id="7C364B10FFAF960BFD59FA30B5D235C7" author="Heyer M &amp; Heyer WR &amp; de Sa RO" pageId="12" pageNumber="785" refId="ref15347" refString="Heyer M, Heyer WR, de Sa RO (2012) Leptodactylus pentadactylus. Catalogue of American Amphibians and Reptiles 887: 887.1 - 887.48." type="journal volume" year="2012">Heyer et al. 2012</bibRefCitation>
, 
<bibRefCitation id="7C364B10FFAF960BFEFFFA51B49F35C7" author="Cole CJ &amp; Townsend CR &amp; Reynolds RP &amp; MacCulloch RD &amp; Lathrop A" box="[266,434,1503,1527]" pageId="12" pageNumber="785" pagination="317 - 578" refId="ref14069" refString="Cole CJ, Townsend CR, Reynolds RP, MacCulloch RD, Lathrop A (2013) Amphibians and reptiles of Guyana, South America: illustrated keys, annotated species accounts, and a biogeographic synopsis. Proceedings of the Biological Society of Washington 125 (4): 317 - 578. https: // doi. org / 10.2988 / 0006 - 324 x- 125.4.317" type="journal article" year="2013">Cole et al. 2013</bibRefCitation>
