<emphasis id="2AD3EAF3FFAF960BFF5DFAD7B4103541" bold="true" box="[168,317,1369,1393]" pageId="12" pageNumber="785">Distribution.</emphasis>
<taxonomicName id="DFA74D62FFAF960BFEB9FAD7B7A13541" baseAuthorityName="Laurenti" baseAuthorityYear="1768" box="[332,652,1369,1393]" class="Amphibia" family="Leptodactylidae" genus="Leptodactylus" kingdom="Animalia" order="Anura" pageId="12" pageNumber="785" phylum="Chordata" rank="species" species="pentadactylus">
