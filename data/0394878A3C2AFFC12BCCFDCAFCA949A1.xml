(
<figureCitation id="13062A193C2AFFC12BC4FDE6FE834C85" box="[159,258,633,659]" captionStart="FIGURES 1–8" captionStartId="3.[151,264,1945,1970]" captionTargetBox="[203,1385,254,1920]" captionTargetId="figure@3.[203,1385,181,1920]" captionTargetPageId="3" captionText="FIGURES 1–8. Stenetra californica Tselikh &amp; Burks sp. nov. (1, 2, 7 ♀ paratype, 3–6, 8 ♀ holotype). 1, antenna; 2, fore wing; 3, head frontal view; 4, head dorsal view; 5, scutellum and propodeum, dorsal view; 6, habitus, lateral view; 7, habitus, dorsal view; 8, metasoma dorsal view." figureDoi="http://doi.org/10.5281/zenodo.3737409" httpUri="https://zenodo.org/record/3737409/files/figure.png" pageId="2" pageNumber="193">Figs 1–8</figureCitation>
)
