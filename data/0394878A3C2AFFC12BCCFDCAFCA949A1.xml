<paragraph id="8B82369C3C2AFFC12BCCFD07FC304CA6" blockId="2.[151,945,597,688]" box="[151,945,664,688]" pageId="2" pageNumber="193">
<uri id="FFAC3A9E3C2AFFC12BCCFD07FC304CA6" box="[151,945,664,688]" pageId="2" pageNumber="193">
urn:lsid:zoobank.org:act: 
<uuid id="FF9B0C493C2AFFC12AC6FD07FC304CA6" box="[413,945,664,688]" pageId="2" pageNumber="193">FFEBF115-F485-41B4-BBE6-0CEDF2E0A1AD</uuid>
