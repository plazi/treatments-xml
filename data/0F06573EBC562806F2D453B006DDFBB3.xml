<taxonomicName id="40AF9DABBC562800F35B54CA0782F87E" authority="Ou, Liu &amp; Zheng, 2007" authorityName="Ou, Liu &amp; Zheng" authorityYear="2007" box="[263,729,1942,1964]" class="Insecta" family="Acrididae" genus="Caryanda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="454" phylum="Arthropoda" rank="species" species="amplexicerca">
