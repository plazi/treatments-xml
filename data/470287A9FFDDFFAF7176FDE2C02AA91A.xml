<subSubSection id="87B16534FFDDFFAE7094FDACC762AEED" pageId="34" pageNumber="35" type="materials_examined">
<paragraph id="CF1436BFFFDDFFAE7094FDACC762AEED" blockId="34.[701,1228,618,1754]" pageId="34" pageNumber="35">
<typeStatus id="1010881DFFDDFFAE7094FDACC729AECC" box="[728,784,618,642]" pageId="34" pageNumber="35">TYPE</typeStatus>
SPECIES: 
<taxonomicName id="08AB4D3CFFDDFFAE71EBFDADC76EAEED" authority="Knight, 1964" authorityName="Knight" authorityYear="1964" class="Insecta" family="Miridae" genus="Plagiognathus" kingdom="Animalia" order="Hemiptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="paddocki">
<emphasis id="FDDFEAADFFDDFFAE71EBFDADC0F5AECE" box="[935,1228,619,643]" italics="true" pageId="34" pageNumber="35">Plagiognathus paddocki</emphasis>
<bibRefCitation id="AB3A4B4EFFDDFFAE70F1FD4EC76EAEED" author="Knight, H. H." box="[701,855,648,672]" pageId="34" pageNumber="35" pagination="127 - 152" refId="ref28827" refString="Knight, H. H. 1964. Phymatopsallus new genus, and new species of Phylinae from North America (Hemiptera, Miridae). Iowa State University Journal of Science 39: 127 - 152." type="journal article" year="1964">Knight, 1964</bibRefCitation>
</taxonomicName>
