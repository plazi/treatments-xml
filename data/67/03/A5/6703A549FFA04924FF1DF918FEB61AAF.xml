<subSubSection pageId="30" pageNumber="331" type="diagnosis">
<paragraph blockId="30.[151,1436,1887,2021]" pageId="30" pageNumber="331">
<emphasis bold="true" box="[199,322,1923,1949]" pageId="30" pageNumber="331">Diagnosis.</emphasis>
