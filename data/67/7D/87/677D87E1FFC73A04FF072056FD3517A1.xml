<subSubSection pageId="6" pageNumber="5" type="etymology">
<paragraph blockId="6.[192,1440,876,932]" pageId="6" pageNumber="5">
<emphasis bold="true" box="[192,347,876,900]" pageId="6" pageNumber="5">Etymology.</emphasis>
