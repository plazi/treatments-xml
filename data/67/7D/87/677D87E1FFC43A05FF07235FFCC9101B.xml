<subSubSection pageId="7" pageNumber="6" type="etymology">
<paragraph blockId="7.[192,1440,998,1054]" pageId="7" pageNumber="6">
<emphasis bold="true" box="[192,347,998,1022]" pageId="7" pageNumber="6">Etymology.</emphasis>
