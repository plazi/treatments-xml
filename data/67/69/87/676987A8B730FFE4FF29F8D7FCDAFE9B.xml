<number box="[986,1056,872,896]" pageId="51" pageNumber="52" value="42.0" valueMax="44.0" valueMin="40.0">40–44</number>
(
<number box="[1082,1098,872,896]" pageId="51" pageNumber="52" value="4.0">4</number>
