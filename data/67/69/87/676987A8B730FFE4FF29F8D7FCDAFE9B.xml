<paragraph blockId="51.[151,1437,151,1304]" pageId="51" pageNumber="52">
<emphasis box="[199,280,871,896]" italics="true" pageId="51" pageNumber="52">Thorax</emphasis>
. Antepronotum with 
<number box="[552,594,871,896]" pageId="51" pageNumber="52" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
(
<number box="[620,635,872,896]" pageId="51" pageNumber="52" value="4.0">4</number>
