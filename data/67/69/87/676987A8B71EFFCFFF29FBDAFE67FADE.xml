(
<number box="[408,424,1813,1837]" pageId="28" pageNumber="29" value="2.0">2</number>
); 
<number box="[448,490,1813,1837]" pageId="28" pageNumber="29" value="199.0">199</number>
(
<number box="[510,522,1813,1837]" pageId="28" pageNumber="29" value="1.0">1</number>
