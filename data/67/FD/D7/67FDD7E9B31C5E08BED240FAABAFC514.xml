<emphasis italics="true" pageId="0" pageNumber="69">Paulownia fortunei</emphasis>
</taxonomicName>
(Seem.) Hemsl. (
<taxonomicName class="Magnoliopsida" family="Paulowniaceae" genus="Paulownia" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Lamiales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="family">Paulowniaceae</taxonomicName>
), 
<taxonomicName authorityName="Lour" authorityYear="1790" class="Magnoliopsida" family="Euphorbiaceae" genus="Vernicia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vernicia montana" order="Malpighiales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="montana">
<emphasis italics="true" pageId="0" pageNumber="69">Vernicia montana</emphasis>
