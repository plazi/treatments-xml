</taxonomicName>
, and 
<taxonomicName baseAuthorityName="Chamberlin" baseAuthorityYear="1919" box="[1010,1102,1260,1286]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="225" phylum="Arthropoda" rank="species" species="tardatus">
