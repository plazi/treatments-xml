, 
<taxonomicName authorityName="Chamberlin &amp; Ivie" authorityYear="1942" box="[772,844,1260,1286]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="225" phylum="Arthropoda" rank="species" species="devius">
<emphasis box="[772,844,1260,1286]" italics="true" pageId="1" pageNumber="225">devius</emphasis>
