, 
<taxonomicName authorityName="Chamberlin &amp; Ivie" authorityYear="1942" box="[857,949,1261,1286]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="225" phylum="Arthropoda" rank="species" species="septatus">
