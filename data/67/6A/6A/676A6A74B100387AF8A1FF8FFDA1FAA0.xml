and 
<taxonomicName box="[790,932,1189,1214]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="225" phylum="Arthropoda" rank="species" species="angustiarum">
<emphasis box="[790,932,1189,1214]" italics="true" pageId="1" pageNumber="225">angustiarum</emphasis>
