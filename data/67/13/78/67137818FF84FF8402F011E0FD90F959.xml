<emphasis bold="true" box="[199,345,916,941]" pageId="1" pageNumber="516">Distribution</emphasis>
. Central Ryukyu Islands, 
<collectingCountry box="[642,706,917,940]" name="Japan" pageId="1" pageNumber="516">Japan</collectingCountry>
(
<number box="[723,754,916,940]" pageId="1" pageNumber="516" value="28.0">28</number>
°N); possibly 
<collectingCountry box="[910,992,916,941]" name="Taiwan" pageId="1" pageNumber="516">Taiwan</collectingCountry>
