<collectingCountry box="[490,599,1260,1283]" name="Colombia" pageId="24" pageNumber="299">Colombia</collectingCountry>
: 
<collectingRegion box="[610,693,1260,1283]" country="Colombia" name="Boyaca" pageId="24" pageNumber="299">Boyacá</collectingRegion>
, 
<collectingRegion box="[703,861,1260,1283]" country="Colombia" name="Cundinamarca" pageId="24" pageNumber="299">Cundinamarca</collectingRegion>
,? 
<collectingRegion box="[881,959,1260,1283]" country="Colombia" name="Tolima" pageId="24" pageNumber="299">Tolima</collectingRegion>
(
<bibRefCitation author="SPAETH" box="[974,1111,1260,1283]" firstAuthor="SPAETH" pageId="24" pageNumber="299" pagination="135 - 158" refId="ref51390" refString="SPAETH F. 1937: Neue Cassidinen des Rijksmuseums in Leiden und meiner Sammlung. Temminckia 2: 135 - 158." type="journal article" year="1937">SPAETH 1937</bibRefCitation>
).
</paragraph>
