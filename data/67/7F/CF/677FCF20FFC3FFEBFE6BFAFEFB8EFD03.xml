, 
<bibRefCitation author="BLAKE" box="[811,867,668,691]" firstAuthor="BLAKE" pageId="26" pageNumber="301" pagination="269 - 282" refId="ref47670" refString="BLAKE D. H. 1971: Fifteen new West Indian chrysomelid beetles. Proceedings of the Entomological Society of Washington 73: 269 - 282." type="journal article" year="1971">1971</bibRefCitation>
; 
<bibRefCitation author="BOROWIEC" box="[878,1061,668,691]" firstAuthor="BOROWIEC" pageId="26" pageNumber="301" pagination="567 - 570" refId="ref48480" refString="BOROWIEC L. 2009 b: Paratrikona albomaculata, a new species from the Dominican Republic (Coleoptera: Chrysomelidae: Cassidinae: Dorynotini). Genus 20: 567 - 570." type="journal article" year="2009" yearSuffix="b">BOROWIEC 2009b</bibRefCitation>
