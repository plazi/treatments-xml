<subSubSection pageId="1" pageNumber="59" type="etymology">
<paragraph blockId="1.[151,1437,1640,2002]" pageId="1" pageNumber="59">
<emphasis bold="true" box="[151,290,1640,1666]" pageId="1" pageNumber="59">Etymology</emphasis>
