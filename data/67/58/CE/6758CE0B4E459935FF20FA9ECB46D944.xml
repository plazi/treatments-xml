(
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[1178,1270,1915,1941]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="29" pageNumber="30">USNM</collectionCode>
