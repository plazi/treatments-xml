<emphasis bold="true" box="[736,762,1746,1770]" pageId="30" pageNumber="31">95</emphasis>
. 
<taxonomicName box="[776,873,1746,1769]" class="Insecta" family="Calliphoridae" genus="Lucilia" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="fayeae">
<emphasis box="[776,791,1746,1769]" italics="true" pageId="30" pageNumber="31">L</emphasis>
. 
<emphasis box="[804,873,1746,1769]" italics="true" pageId="30" pageNumber="31">fayeae</emphasis>
</taxonomicName>
. 
<emphasis bold="true" box="[887,913,1746,1770]" pageId="30" pageNumber="31">96</emphasis>
. 
<taxonomicName box="[926,1096,1746,1769]" class="Insecta" family="Calliphoridae" genus="Lucilia" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="problematica">
<emphasis box="[926,941,1746,1769]" italics="true" pageId="30" pageNumber="31">L</emphasis>
. 
<emphasis box="[955,1096,1746,1769]" italics="true" pageId="30" pageNumber="31">problematica</emphasis>
