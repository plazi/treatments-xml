<taxonomicName id="4FDF98B75A37FF99151AFCDCFC42FCB9" authority="Meyrick, 1915" authorityName="Meyrick" authorityYear="1915" box="[551,949,835,860]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="rotans">
