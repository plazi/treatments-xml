<geoCoordinate id="ACD505C30470FFF3FD86FDE974A01208" box="[546,690,548,575]" degrees="25" direction="west" minutes="24" orientation="longitude" pageId="32" pageNumber="33" precision="15" seconds="15" value="-25.404167">25°24’15”W</geoCoordinate>
