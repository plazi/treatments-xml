<taxonomicName id="4FA73887FF924A14FD5AFCF5BDB3FCA9" authority="Hoffman, 2005" authorityName="Hoffman" authorityYear="2005" box="[652,1018,817,844]" class="Diplopoda" family="Gomphodesmidae" genus="Agrophogonus" kingdom="Animalia" order="Polydesmida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">
