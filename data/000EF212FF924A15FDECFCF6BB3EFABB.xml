<taxonomicName id="4FA73887FF934A15FBDDFDFFBB3EFDB3" authority="Hoffman, 2005" authorityName="Hoffmann" authorityYear="2005" box="[1035,1399,571,598]" class="Diplopoda" family="Gomphodesmidae" genus="Agrophogonus" kingdom="Animalia" order="Polydesmida" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="mwanihana">
