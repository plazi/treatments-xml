<taxonomicName id="4FA73887FF934A15FBEEFE20BB3EFE1B" authority="(Verhoeff, 1941)" baseAuthorityName="Verhoeff" baseAuthorityYear="1941" box="[1080,1399,483,510]" class="Diplopoda" family="Gomphodesmidae" genus="Agrophogonus" kingdom="Animalia" order="Polydesmida" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="harrisi">
<emphasis id="BAD39F16FF934A15FBEEFE20BAE4FE1B" bold="true" box="[1080,1197,483,510]" italics="true" pageId="25" pageNumber="26">A. harrisi</emphasis>
(Verhoeff, 1941)
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="88184304FF934A15FF17FDD4BB3EFDB3" blockId="25.[189,1399,440,598]" pageId="25" pageNumber="26">
<keyLead id="3356E031FF934A15FF17FDD4BB3EFDB3" pageId="25" pageNumber="26">
