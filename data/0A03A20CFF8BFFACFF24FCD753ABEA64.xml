<taxonomicName id="45AA6899FF8AFFAEFBC9FEB75428EA39" box="[1146,1336,260,285]" class="Insecta" family="Chrysomelidae" genus="Doryscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="midanaoensis" status="sp. nov.">
