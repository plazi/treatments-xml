<subSubSection id="7D9C6566FFA8F419FF21FA3EFC36FA2D" box="[198,979,1501,1527]" pageId="52" pageNumber="53" type="biology_ecology">
<paragraph id="353936EDFFA8F419FF21FA3EFC36FA2D" blockId="52.[151,1437,263,1643]" box="[198,979,1501,1527]" pageId="52" pageNumber="53">
