<location id="1670602DFFC0467FFD35BF07FE821B9B" LSID="urn:lsid:plazi:treatment:9B0687E0FFC1467FFC96B961FCE31BFF:1670602DFFC0467FFD35BF07FE821B9B" country="Canada" municipality="northern Appalachians" name="Cape Breton Island" pageId="7" pageNumber="169" stateProvince="Quebec">Cape Breton Island</location>
