<collectorName id="BE5A5320FFC0467FFD6BBF5FFD1D1B6F" box="[702,764,300,324]" pageId="7" pageNumber="169">Stark</collectorName>
&amp; 
<collectorName id="BE5A5320FFC0467FFF68BF23FEC71B43" box="[189,294,336,360]" pageId="7" pageNumber="169">Szczytko</collectorName>
