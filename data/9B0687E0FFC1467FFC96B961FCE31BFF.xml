<taxonomicName id="D4AF4D75FFC0467FFD65BE97FEF21B0B" authorityName="Stark &amp; Szczytko" authorityYear="1988" class="Insecta" family="Perlodidae" genus="Malirekus" kingdom="Animalia" order="Plecoptera" pageId="7" pageNumber="169" phylum="Arthropoda" rank="species" species="iroquois">
