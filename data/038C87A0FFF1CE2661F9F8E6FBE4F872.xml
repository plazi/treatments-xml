<taxonomicName id="4C254D35FFF1CE266180F8E6FDA9F883" authority="Kaszab, 1952" authorityName="Kaszab" authorityYear="1952" box="[238,539,1826,1853]" class="Insecta" family="Meloidae" genus="Denierella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="265" phylum="Arthropoda" rank="genus">
