<taxonomicName id="4C3B4D607E608872FF0336AE444D10CE" box="[146,303,1243,1270]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="166" phylum="Arthropoda" rank="species" species="scopariella">
