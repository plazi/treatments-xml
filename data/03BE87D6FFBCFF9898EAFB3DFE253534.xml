<quantity id="4CEF9B25FFBCFF989AB9FAE5F94A3537" box="[708,779,1303,1330]" metricMagnitude="2" metricUnit="m" metricValue="7.1" pageId="45" pageNumber="46" unit="m" value="710.0">710 m</quantity>
