<quantity id="4CEF9B25FFBCFF989B64FAE5F9373537" box="[793,886,1303,1330]" metricMagnitude="2" metricUnit="m" metricValue="7.098792" pageId="45" pageNumber="46" unit="ft" value="2329.0">2,329 ft</quantity>
