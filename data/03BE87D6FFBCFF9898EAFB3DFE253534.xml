<taxonomicName id="4C174D43FFBCFF989899FAEAFB183534" authorityName="Parin" authorityYear="2004" box="[228,345,1303,1329]" class="Actinopterygii" family="Opisthoproctidae" genus="Ioichthys" kingdom="Animalia" order="Osmeriformes" pageId="45" pageNumber="46" phylum="Chordata" rank="species" species="kashkini">
