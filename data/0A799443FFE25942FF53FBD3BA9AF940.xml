<heading id="D9279239FFE25945FF53FBD3B992FBEF" bold="true" box="[151,755,1087,1114]" fontSize="11" level="1" pageId="3" pageNumber="389" reason="1">
<emphasis id="B0A4F947FFE25945FF53FBD3B992FBEF" bold="true" box="[151,755,1087,1114]" pageId="3" pageNumber="389">
<taxonomicName id="45D05ED6FFE25945FF53FBD3B9E8FBEF" authority="Gonzalez &amp; Santos" authorityName="Gonzalez &amp; Santos" box="[151,649,1087,1114]" class="Insecta" family="Coccinellidae" genus="Zenoria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="389" phylum="Arthropoda" rank="species" species="westerduijni" status="sp. nov.">
<emphasis id="B0A4F947FFE25945FF53FBD3BAF3FBEC" bold="true" box="[151,402,1087,1113]" italics="true" pageId="3" pageNumber="389">Zenoria westerduijni</emphasis>
González &amp; Santos
</taxonomicName>
, 
<taxonomicNameLabel id="AB97443CFFE25945FD52FBACB992FBEF" box="[662,755,1088,1114]" pageId="3" pageNumber="389" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
