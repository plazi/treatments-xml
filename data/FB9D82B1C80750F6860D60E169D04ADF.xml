<subSubSection id="02D78B533BF7CA16E03C8643F6F17B80" pageId="0" pageNumber="49" type="etymology">
<paragraph id="E69BFFCB5508ABE58230C682EA057E41" pageId="0" pageNumber="49">Etymology.</paragraph>
<paragraph id="DB026CDE32818DF2502F0CBF8F055D03" pageId="0" pageNumber="49">
<emphasis id="581B124F42B1A9DAC046BF74ED0858E0" italics="true" pageId="0" pageNumber="49">Sagittata</emphasis>
