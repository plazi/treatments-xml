</taxonomicName>
, 
<taxonomicName id="44A89581FFED2A58ABC7FC05FB560E78" authority="Koch, 1897" authorityName="Koch" authorityYear="1897" box="[914,1205,1004,1029]" class="Mammalia" family="Hyracodontidae" genus="Prohyracodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="13" pageNumber="234" phylum="Chordata" rank="genus">
