<collectionCode id="44EF0BCD73321210A7ABFAE739C7FADD" box="[415,500,1339,1362]" httpUri="http://grbio.org/cool/5hig-c780" name="University of Texas" pageId="11" pageNumber="12">TNHM</collectionCode>
