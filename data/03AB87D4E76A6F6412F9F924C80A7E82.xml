<taxonomicName id="4C024D41E7696F64112FF897CA967E7E" box="[833,967,1875,1900]" class="Insecta" family="Formicidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="530" phylum="Arthropoda" rank="family">Formicidae</taxonomicName>
), Isopoda, 
<taxonomicName id="4C024D41E7696F641629F890C8067E82" authority=", Acari (Dreybrodt &amp; Laumanns 2005)" authorityName=", Acari (Dreybrodt &amp; Laumanns" authorityYear="2005" class="Arachnida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="530" phylum="Arthropoda" rank="order">
