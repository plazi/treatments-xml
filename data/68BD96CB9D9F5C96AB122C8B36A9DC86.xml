<taxonomicName id="88C5C515117853AE64FC2551CA3231D2" LSID="68BD96CB-9D9F-5C96-AB12-2C8B36A9DC86" authority="B. Huang &amp; Y. Nie" authorityName="B. Huang &amp; Y. Nie" authorityYear="2020" class="Entomophthoromycetes" family="Ancylistaceae" genus="Conidiobolus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Conidiobolus variabilis" order="Entomophthorales" pageId="0" pageNumber="133" phylum="Entomophthoromycota" rank="species" species="variabilis" status="sp. nov.">Conidiobolus variabilis B. Huang &amp; Y. Nie</taxonomicName>
