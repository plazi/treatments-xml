, 
<collectorName id="269E535CFF91AB5066844639FAD41F09" box="[1208,1367,536,560]" pageId="5" pageNumber="4">N. Cerqueira</collectorName>
(
<specimenCount id="9D6DFD03FF91AB5067554639FA0C1F09" box="[1385,1423,536,560]" pageId="5" pageNumber="4" type="female">1 ♀</specimenCount>
