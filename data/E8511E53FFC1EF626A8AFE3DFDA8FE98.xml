<collectingRegion id="A23C61A7FFC1EF61697EF953FC73F94E" box="[820,959,1680,1706]" country="United States of America" name="Washington" pageId="29" pageNumber="28">Washington</collectingRegion>
<location id="6527F99EFFC1EF61698FF953FBD7F94E" LSID="urn:lsid:plazi:treatment:E8511E53FFC1EF626A8AFE3DFDA8FE98:6527F99EFFC1EF61698FF953FBD7F94E" box="[965,1051,1680,1706]" county="Grand County" municipality="Graham" name="County" pageId="29" pageNumber="28" stateProvince="Utah">County</location>
