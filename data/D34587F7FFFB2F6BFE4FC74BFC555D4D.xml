<taxonomicName id="9CEC4D62FFFB2F6BFAF4C7B6FA3F5C8B" box="[1360,1499,1705,1729]" class="Magnoliopsida" family="Araliaceae" genus="Hedera" kingdom="Plantae" order="Apiales" pageId="5" pageNumber="923" phylum="Tracheophyta" rank="species" species="helix">
