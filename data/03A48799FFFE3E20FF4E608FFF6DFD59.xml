<taxonomicName id="4C0D4D0CFFFE3E22FD906649FD70F8E8" box="[585,750,1852,1875]" class="Insecta" family="Keroplatidae" genus="Xenoplatyura" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="32" phylum="Arthropoda" rank="species" species="sichuanensis" status="n. sp.">
