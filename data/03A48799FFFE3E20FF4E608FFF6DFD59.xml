<taxonomicName id="4C0D4D0CFFFE3E22FC806649FC01F8E8" box="[857,927,1852,1875]" class="Insecta" family="Keroplatidae" genus="Xenoplatyura" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="32" phylum="Arthropoda" rank="species" species="lata" status="n. sp.">
