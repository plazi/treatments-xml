<paragraph id="F12D36EFFFD0FFE538FCFA85FE5DFAB7" blockId="15.[178,466,1302,1325]" box="[178,466,1302,1325]" pageId="15" pageNumber="848">
<heading id="AA658183FFD0FFE538FCFA85FE5DFAB7" box="[178,466,1302,1325]" fontSize="8" level="2" pageId="15" pageNumber="848" reason="2">
<emphasis id="C3E6EAFDFFD0FFE538FCFA85FE5DFAB7" bold="true" box="[178,466,1302,1325]" pageId="15" pageNumber="848">
<taxonomicName id="36924D6CFFD0FFE538FCFA85FEF0FAB6" authority="Malicky, 2008" authorityName="Malicky" authorityYear="2008" box="[178,383,1302,1324]" class="Insecta" family="Psychomyiidae" genus="Psychomyia" kingdom="Animalia" order="Trichoptera" pageId="15" pageNumber="848" phylum="Arthropoda" rank="species" species="phobos" status="nov.sp.">
<emphasis id="C3E6EAFDFFD0FFE538FCFA85FEF0FAB6" bold="true" box="[178,383,1302,1324]" italics="true" pageId="15" pageNumber="848">Psychomyia phobos</emphasis>
</taxonomicName>
<taxonomicNameLabel id="D8D55786FFD0FFE539C8FA84FE5DFAB7" box="[390,466,1303,1325]" pageId="15" pageNumber="848" rank="species">nov.sp.</taxonomicNameLabel>
