<taxonomicName id="2B8862CCFFF64F1AE3030C24B6D5FDA0" authorityName="Lamarck" authorityYear="1803" class="Gastropoda" family="Rostellariidae" genus="Rostellaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="25" pageNumber="42" phylum="Mollusca" rank="species" species="macroptera">
<emphasis id="DEFCC55DFFF64F1AE3030C24B6D5FDA0" italics="true" pageId="25" pageNumber="42">Rostellaria macroptera</emphasis>
</taxonomicName>
, mais s’en distinguant par l’aile peu développée, ne passant pas l’avant-dernier tour, et n’enveloppant d’aucune manière le reste de la spire » [= 
<taxonomicName id="2B8862CCFFF64F1AE32E0C75B77FFD51" authority="(d'Orbigny, 1850)" baseAuthorityName="d'Orbigny" baseAuthorityYear="1850" class="Gastropoda" family="Rostellariidae" genus="Amplogladius" kingdom="Animalia" order="Littorinimorpha" pageId="25" pageNumber="42" phylum="Mollusca" rank="species" species="athleta">
<emphasis id="DEFCC55DFFF64F1AE32E0C75B6BCFD51" italics="true" pageId="25" pageNumber="42">Amplogladius athleta</emphasis>
(d’Orbigny, 1850)
</taxonomicName>
]. 
<location id="E9574F94FFF64F1AE0A50C9FB7DDFD51" LSID="urn:lsid:plazi:treatment:6421A859FFF64F1AE0110FDAB46DFC85:E9574F94FFF64F1AE0A50C9FB7DDFD51" box="[387,464,658,682]" county="d'Orbigny" municipality="Le" name="Localite" pageId="25" pageNumber="42">Localité</location>
type: 
<location id="E9574F94FFF64F1AE3340C9FB48EFD51" LSID="urn:lsid:plazi:treatment:6421A859FFF64F1AE0110FDAB46DFC85:E9574F94FFF64F1AE3340C9FB48EFD51" box="[530,643,658,682]" county="d'Orbigny" municipality="Le" name="Bois-le-Roi" pageId="25" pageNumber="42">Bois-le-Roi</location>
, 
<location id="E9574F94FFF64F1AE1570CA0B70AFD3E" LSID="urn:lsid:plazi:treatment:6421A859FFF64F1AE0110FDAB46DFC85:E9574F94FFF64F1AE1570CA0B70AFD3E" box="[113,263,685,709]" county="d'Orbigny" municipality="Le" name="Auvers-sur-Oise" pageId="25" pageNumber="42">Auvers-sur-Oise</location>
(
<location id="E9574F94FFF64F1AE0320CA0B77AFD3E" LSID="urn:lsid:plazi:treatment:6421A859FFF64F1AE0110FDAB46DFC85:E9574F94FFF64F1AE0320CA0B77AFD3E" box="[276,375,685,709]" county="d'Orbigny" municipality="Le" name="Val d'Oise" pageId="25" pageNumber="42">Val d’Oise</location>
). 
<collectingMunicipality id="0C538335FFF64F1AE0A30CA0B791FD3F" box="[389,412,685,708]" pageId="25" pageNumber="42">Le</collectingMunicipality>
