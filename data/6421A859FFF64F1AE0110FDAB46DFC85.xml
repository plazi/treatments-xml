<typeStatus id="3333A7EDFFF64F1AE32A0CF1B46AFCEF" box="[524,615,764,788]" pageId="25" pageNumber="42">lectotype</typeStatus>
de 
<taxonomicName id="2B8862CCFFF64F1AE1570D1AB7BBFCD4" authority="d'Orbigny, 1850" authorityName="d'Orbigny" authorityYear="1850" box="[113,438,791,815]" class="Gastropoda" family="Rostellariidae" genus="Rostellaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="25" pageNumber="42" phylum="Mollusca" rank="species" species="athleta">
