</emphasis>
<taxonomicName id="87E84D601179FF9AE02AA6B327D985B6" authority="Silvestri, 1904" authorityName="Silvestri" authorityYear="1904" box="[300,646,473,498]" class="Insecta" family="Machilidae" genus="Machilis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeognatha" pageId="1" pageNumber="372" phylum="Arthropoda" rank="species" species="alternata">
