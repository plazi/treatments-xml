<taxonomicName id="4C114D3EFFDFFFF7FC85FF2EFBB7FF18" authority="Blainville, 1818" authorityName="Blainville" authorityYear="1818" box="[813,1149,204,230]" class="Actinopterygii" family="Pycnodontiformes (awaiting allocation)" genus="Palaeobalistum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="11" pageNumber="150" phylum="Chordata" rank="genus">
