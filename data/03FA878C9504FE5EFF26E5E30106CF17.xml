</taxonomicName>
, 
<taxonomicName id="4C534D199504FE5EFACCE6DB02FEC9F6" authority="Dall, 1902" authorityName="Dall" authorityYear="1902" class="Gastropoda" family="Ringiculidae" genus="Microglyphis" kingdom="Animalia" pageId="20" pageNumber="217" phylum="Mollusca" rank="species" species="brevicula">
