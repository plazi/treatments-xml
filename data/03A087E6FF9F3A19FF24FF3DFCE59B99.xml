<taxonomicName id="4C094D73FF9F3A19FF24FD4FFF089AF2" box="[151,224,741,767]" class="Insecta" family="Chironomidae" genus="Corynoneurella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="length">length</taxonomicName>
