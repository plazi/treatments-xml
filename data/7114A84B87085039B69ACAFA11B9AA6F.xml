<taxonomicName id="CD14385E72B19A1938A5F2CE3221BB47" authorityName="Chen, gen. et" authorityYear="2020" class="Gastropoda" family="Diplommatinidae" genus="Sinoarinia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinoarinia feii" order="Architaenioglossa" pageId="0" pageNumber="37" phylum="Mollusca" rank="species" species="feii">
