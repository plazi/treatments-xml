(
<typeStatus id="7E698808A27BFFCFFD8666C8FD79AAE3" box="[575,670,980,1004]" pageId="6" pageNumber="417">holotype</typeStatus>
<quantity id="662A9B4FA27BFFCFFD1D66C8FD1DAAE3" box="[676,762,980,1004]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="6" pageNumber="417" unit="mm" value="1.9">1.9 mm</quantity>
