<taxonomicName id="1897A41911D06B1A0E82E8AD5099A0EC" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis assimilis" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="assimilis">Chrysis assimilis</taxonomicName>
