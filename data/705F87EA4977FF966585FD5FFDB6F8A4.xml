<typeStatus id="274D885E4977FF966158FB12FB14FB48" box="[1098,1200,1178,1203]" pageId="6" pageNumber="243">lectotype</typeStatus>
and 
<typeStatus id="274D885E4977FF9661F5FB12FADAFB48" box="[1255,1406,1178,1203]" pageId="6" pageNumber="243">paralectotype</typeStatus>
