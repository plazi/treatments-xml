Boulenger (
<number id="3B6A5670CE78FFC5F755FD081759FD3C" box="[565,625,655,679]" pageId="5" pageNumber="6" value="1903.0">1903</number>
)
</bibRefCitation>
, 
<bibRefCitation id="E93339A6CE78FFC5F7E5FD0816A6FD3C" author="Rosler" box="[645,910,655,679]" pageId="5" pageNumber="6" refString="Rosler, H. &amp; Wranik, W. (1998) Beitrage zur Herpetologie der Republik Jemen. 3. Geckos des sudlichen Jemen und der Insel Sokotra. Zoologische Abhandlungen Staatliches Museum fur Tierkunde, Dresden, 21, 113 - 132." type="journal article" year="1998">
Rösler &amp; Wranik (
<number id="3B6A5670CE78FFC5F633FD0816A6FD3C" box="[851,910,655,679]" pageId="5" pageNumber="6" value="1998.2004">1998</number>
</bibRefCitation>
, 
<bibRefCitation id="E93339A6CE78FFC5F6FAFD0816FEFD3C" author="Rosler" box="[922,982,655,679]" pageId="5" pageNumber="6" refString="Rosler, H. &amp; Wranik, W. (2004) A key and annotated checklist to the reptiles of the Socotra Archipelago. Fauna of Saudi Arabia, 20, 505 - 534." type="journal article" year="2004">2004</bibRefCitation>
), 
<bibRefCitation id="E93339A6CE78FFC5F689FD0811B5FD3C" author="Wranik" box="[1001,1181,655,679]" pageId="5" pageNumber="6" refString="Wranik, W. (1998 b) Contributions to the herpetology of the Republic of Yemen. 4. Sokotra Island and southern Yemen mainland. Zoologische Abhandlungen Staatliches Museum fur Tierkunde Dresden, 21,163 - 179." type="book chapter" year="1998" yearSuffix="b">
Wranik (
<number id="3B6A5670CE78FFC5F12AFD0811ADFD3C" box="[1098,1157,655,679]" pageId="5" pageNumber="6" value="1998.0">1998</number>
b)
</bibRefCitation>
, 
<bibRefCitation id="E93339A6CE78FFC5F1C8FD0815CCFD57" author="Schatti" pageId="5" pageNumber="6" refString="Schatti, B. &amp; Desvoignes, A. (1999) The herpetofauna of Southern Yemen and the Sokotra Archipelago. Instrumenta Biodiversitatis IV, Museum d'Histoire Naturelle, Geneve, 178 pp." type="book" year="1999">
Schätti &amp; Desvoignes (
<number id="3B6A5670CE78FFC5F5FFFD3315F4FD57" box="[159,220,692,716]" pageId="5" pageNumber="6" value="1999.0">1999</number>
)
</bibRefCitation>
.
</paragraph>
<paragraph id="8D1D4457CE78FFC5F5A5FD5017DFFD74" blockId="5.[151,1436,151,752]" box="[197,759,727,752]" pageId="5" pageNumber="6">
