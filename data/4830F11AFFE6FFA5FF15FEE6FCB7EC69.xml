<taxonomicName id="07993B8FFFE6FFA6FD18FDC1FCCEEFC9" authorityName="Klimaszewski &amp; Majka" authorityYear="2006" box="[641,808,625,651]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="224" phylum="Arthropoda" rank="species" species="novaescotiae">
