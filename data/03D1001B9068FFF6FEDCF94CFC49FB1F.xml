<number id="3DB0A32A9068FFF5FEDCF94CFEFFF941" box="[264,310,1678,1704]" pageId="5" pageNumber="64" value="301.0">301</number>
. 
<taxonomicName id="4C78CA8E9068FFF5FE97F94CFD73F941" authority="Fabricius, 1804" authorityName="Fabricius" authorityYear="1804" box="[323,698,1678,1704]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="species" species="anator">
<emphasis id="B90C6D1F9068FFF5FE97F94CFE3DF941" bold="true" box="[323,500,1678,1704]" italics="true" pageId="5" pageNumber="64">Bracon anator</emphasis>
<bibRefCitation id="EFE9CCFC9068FFF5FE2FF94CFD73F941" author="Fabricius" box="[507,698,1678,1704]" pageId="5" pageNumber="67" refString="Fabricius, J. C. (1804) Systema Piezatorum. Carolum Reichard, Brunsviga. 439 pp." type="book" year="1804">
Fabricius, 
<number id="3DB0A32A9068FFF5FD54F94CFD73F941" box="[640,698,1678,1704]" pageId="5" pageNumber="64" value="1804.0">1804</number>
</bibRefCitation>
