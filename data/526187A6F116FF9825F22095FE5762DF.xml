<taxonomicName id="1DC84D33F116FF9C25F22095FE6866C3" box="[151,449,1198,1224]" class="Insecta" family="Limoniidae" genus="Praearchitipula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="243" phylum="Arthropoda" rank="species" species="apprima" status="sp. nov.">
<emphasis id="E8BCEAA2F116FF9C25F22095FE6866C3" bold="true" box="[151,449,1198,1224]" italics="true" pageId="3" pageNumber="243">Praearchitipula apprima</emphasis>
