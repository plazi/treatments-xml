<taxonomicName id="4C2001899A70292AFCCF5975FBF45CB9" baseAuthorityName="Alcock" baseAuthorityYear="1891" box="[856,1065,871,897]" class="Actinopterygii" family="Ophidiidae" genus="Porogadus" kingdom="Animalia" order="Ophidiiformes" pageId="66" pageNumber="67" phylum="Chordata" rank="species" species="melanocephalus">
