<number id="3D8A2498FFA0FFDAFB6CF94BFB758284" box="[1211,1221,1753,1779]" pageId="12" pageNumber="13" value="1.0">1</number>
and 
<number id="3D8A2498FFA0FFDAFB29F94BFABE8284" box="[1278,1294,1753,1779]" pageId="12" pageNumber="13" value="3.0">3</number>
