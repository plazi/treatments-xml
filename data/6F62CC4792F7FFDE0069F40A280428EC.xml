<taxonomicName id="4529AA5AE3459489E71581BAC0B45DB7" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="amplexicaule">
