<quantity id="04274EBD12080151FE29DF90BD7D7DD8" box="[440,547,1461,1487]" metricMagnitude="-3" metricUnit="m" metricValue="9.65" pageId="15" pageNumber="16" unit="mm" value="9.65">9.65 mm</quantity>
