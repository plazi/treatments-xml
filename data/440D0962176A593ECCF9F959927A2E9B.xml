<subSubSection id="84BEEBFF1769593ECCA8FCA092CE2E10" box="[198,1421,831,858]" pageId="10" pageNumber="29" type="etymology">
<paragraph id="CC1BB8741769593ECCA8FCA092CE2E10" blockId="10.[151,1437,151,978]" box="[198,1421,831,858]" pageId="10" pageNumber="29">
<emphasis id="FED064661769593ECCA8FCA096102E10" bold="true" box="[198,339,831,857]" pageId="10" pageNumber="29">Etymology.</emphasis>
