<number id="7A6CAA53176A593DCEDAF81695922AEA" box="[692,721,1929,1955]" pageId="9" pageNumber="28" value="16.0">16</number>
(
<number id="7A6CAA53176A593DCE8AF81694722AEA" box="[740,817,1929,1955]" pageId="9" pageNumber="28" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
