<taxonomicName id="0BA4C3F71769593ECEEEFCDF94442E10" box="[640,775,832,857]" class="Arachnida" family="Eriophyidae" genus="Tegolophus" kingdom="Animalia" order="Prostigmata" pageId="10" pageNumber="29" phylum="Arthropoda" rank="species" species="triarrhenae">
