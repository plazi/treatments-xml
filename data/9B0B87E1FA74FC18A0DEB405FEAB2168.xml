<date id="671C1037FA74FC1AA478B501FB23236A" box="[1073,1195,1305,1331]" pageId="8" pageNumber="353" value="1998-04-29">29.iv.1998</date>
</collectingDate>
, 
<collectorName id="BE575321FA74FC1AA4FEB501FABC236A" box="[1207,1332,1305,1331]" pageId="8" pageNumber="353">W.T. Yang</collectorName>
leg.
