<caption id="3FFC60ACFFB8FFDCFF12F89FC11AF810" ID-DOI="http://doi.org/10.5281/zenodo.4913946" ID-Zenodo-Dep="4913946" httpUri="https://zenodo.org/record/4913946/files/figure.png" pageId="39" pageNumber="40" startId="39.[151,264,1900,1922]" targetBox="[177,1422,301,1833]" targetPageId="39">
