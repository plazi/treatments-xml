<taxonomicName id="ADE24D772442BE15FBB7FEB456CEC492" authority="Kuang &amp; Zhuo 1989" authorityName="Kuang &amp; Zhuo" authorityYear="1989" class="Arachnida" family="Eriophyidae" genus="Neoshevtchenkella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="12" pageNumber="55" phylum="Arthropoda" rank="genus">
<emphasis id="5896EAE62442BE15FBB7FEB452EAC4B6" box="[1049,1259,368,392]" italics="true" pageId="12" pageNumber="55">Neoshevtchenkella</emphasis>
<bibRefCitation id="0E734B052442BE15FB5CFEB456CEC492" author="Kuang" pageId="12" pageNumber="55" refString="Kuang, H. - Y. &amp; Zhuo, W. - X. (1989) Description of a new genus and seven new species of Phyllocoptinae from China (Acariformes: Eriophyoidea). Acta Entomologica Sinica, 32 (1), 113 - 121." type="journal article" year="1989">
Kuang &amp; Zhuo 
<number id="DC2A24D32442BE15FF39FE5056CEC492" box="[151,207,404,428]" pageId="12" pageNumber="55" value="1989.0">1989</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="ADE24D772442BE15FEA8FE50547EC492" authority="Bagdasarian 1978" authorityName="Bagdasarian" authorityYear="1978" box="[262,639,404,428]" class="Arachnida" family="Eriophyidae" genus="Shevtchenkella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="12" pageNumber="55" phylum="Arthropoda" rank="genus">
<emphasis id="5896EAE62442BE15FEA8FE5057AFC492" box="[262,430,404,428]" italics="true" pageId="12" pageNumber="55">Shevtchenkella</emphasis>
<bibRefCitation id="0E734B052442BE15FE1BFE50547EC492" author="Bagdasarian" box="[437,639,404,428]" pageId="12" pageNumber="55" refString="Bagdasarian, A. T. (1978) Novii rod kleshchei Eriophyoidea. Zoologicheskii Zhurnal, 57 (6), 936 - 939." type="journal article" year="1978">
Bagdasarian 
<number id="DC2A24D32442BE15FDE9FE50547EC492" box="[583,639,404,428]" pageId="12" pageNumber="55" value="1978.0">1978</number>
</bibRefCitation>
</taxonomicName>
