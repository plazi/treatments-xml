<taxonomicName id="ADE24D772442BE15FF7AFEB45445C4B9" authority="(Lour.) Merr." authorityName="(Lour.) Merr." box="[212,580,367,392]" class="Magnoliopsida" family="Sapindaceae" genus="Schleichera" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="55" phylum="Tracheophyta" rank="species" species="oleosa">
<emphasis id="5896EAE62442BE15FF7AFEB457A8C4B6" box="[212,425,368,392]" italics="true" pageId="12" pageNumber="55">Schleichera oleosa</emphasis>
(Lour.) Merr.
</taxonomicName>
(
<taxonomicName id="ADE24D772442BE15FDFCFEAB54E5C4B9" box="[594,740,367,391]" class="Magnoliopsida" family="Sapindaceae" kingdom="Plantae" order="Sapindales" pageId="12" pageNumber="55" phylum="Tracheophyta" rank="family">Sapindaceae</taxonomicName>
