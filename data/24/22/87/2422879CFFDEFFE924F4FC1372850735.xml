<subSubSection pageId="4" pageNumber="3" type="materials_examined">
<paragraph blockId="4.[192,868,963,988]" box="[192,868,963,988]" pageId="4" pageNumber="3">
<typeStatus box="[192,253,964,988]" pageId="4" pageNumber="3">Type</typeStatus>
