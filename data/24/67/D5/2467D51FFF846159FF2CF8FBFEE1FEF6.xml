</taxonomicName>
, 
<taxonomicName authority="Papp, 1981" authorityName="Papp" authorityYear="1981" class="Insecta" family="Braconidae" genus="Synaldis" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="52" phylum="Arthropoda" rank="species" species="fraudulenda">
<emphasis italics="true" pageId="13" pageNumber="52">S. fraudulenda</emphasis>
(Papp, 
<number box="[373,432,1484,1508]" pageId="13" pageNumber="52" value="1981.0">1981</number>
