<paragraph blockId="30.[151,1437,1365,2031]" pageId="30" pageNumber="31">
<emphasis bold="true" box="[198,343,1645,1671]" pageId="30" pageNumber="31">Etymology:</emphasis>
<taxonomicName baseAuthorityName="Milne-Edwards" baseAuthorityYear="1867" box="[351,491,1645,1670]" class="Aves" family="Psittacidae" genus="Necropsittacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psittaciformes" pageId="30" pageNumber="31" phylum="Chordata" rank="species" species="rodericanus">
<emphasis box="[351,491,1645,1670]" italics="true" pageId="30" pageNumber="31">rodericanus</emphasis>
</taxonomicName>
, of 
<collectingRegion box="[541,664,1645,1671]" country="Mauritius" name="Rodrigues" pageId="30" pageNumber="31">Rodrigues</collectingRegion>
