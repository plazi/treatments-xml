<number box="[580,594,1658,1684]" pageId="4" pageNumber="45" value="2.0">2</number>
nd (between R 
<number box="[767,775,1674,1688]" pageId="4" pageNumber="45" value="1.0">1</number>
and R 
<number box="[853,861,1674,1688]" pageId="4" pageNumber="45" value="2.0">2</number>
+ 
<number box="[871,878,1674,1688]" pageId="4" pageNumber="45" value="3.0">3</number>
), 
<number box="[904,917,1658,1684]" pageId="4" pageNumber="45" value="3.0">3</number>
