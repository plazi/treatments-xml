(A); 
<taxonomicName genus="P." lsidName="P. cunfengensis" pageId="2" pageNumber="85" rank="species" species="cunfengensis">P. cunfengensis</taxonomicName>
sp. n. (B); 
<taxonomicName genus="P." lsidName="P. podiensis" pageId="2" pageNumber="85" rank="species" species="podiensis">P. podiensis</taxonomicName>
