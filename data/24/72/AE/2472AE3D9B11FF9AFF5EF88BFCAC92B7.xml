is distinguished from 
<taxonomicName authorityName="Kolbe" authorityYear="1898" box="[919,1075,223,248]" class="Insecta" family="Cupedidae" genus="Paracupes" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="150" phylum="Arthropoda" rank="species" species="brasiliensis">
