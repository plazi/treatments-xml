<taxonomicName class="Insecta" family="Lymantriidae" genus="Lymantria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lymantria" order="Lepidoptera" pageId="0" pageNumber="105" phylum="Arthropoda" rank="genus">Lymantria</taxonomicName>
