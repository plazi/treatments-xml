<taxonomicName class="Insecta" family="Psyllidae" genus="Psylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psylla" order="Hemiptera" pageId="0" pageNumber="105" phylum="Arthropoda" rank="genus">Psylla</taxonomicName>
(
<taxonomicName lsidName="" order="Hemiptera" pageId="0" pageNumber="105" rank="order">Hemiptera</taxonomicName>
