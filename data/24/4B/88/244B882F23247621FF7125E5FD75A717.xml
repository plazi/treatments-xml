<taxonomicName authority="Moanda" authorityName="Moanda" box="[349,567,1882,1904]" class="Insecta" family="Libellulidae" genus="Acisoma" kingdom="Animalia" order="Odonata" pageId="12" pageNumber="165" phylum="Arthropoda" rank="species" species="trifidum">
