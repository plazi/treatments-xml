<subSubSection pageId="12" pageNumber="61" type="etymology">
<paragraph blockId="12.[835,1474,478,534]" pageId="12" pageNumber="61">
<emphasis bold="true" box="[835,979,478,502]" pageId="12" pageNumber="61">
<emphasis bold="true" box="[835,952,478,502]" italics="true" pageId="12" pageNumber="61">Etymology</emphasis>
