<collectingCountry id="F32E849D3D73FFD4FD5FFEBFFCAFBB78" box="[681,847,274,300]" name="Saint Kitts and Nevis" pageId="31" pageNumber="32">São Cristovão</collectingCountry>
(
<geoCoordinate id="EE0DA2CA3D73FFD4FC97FEBFFC20BB78" box="[865,960,274,300]" degrees="11" direction="south" minutes="01" orientation="latitude" pageId="31" pageNumber="32" precision="925" value="-11.016666">11º01’S</geoCoordinate>
