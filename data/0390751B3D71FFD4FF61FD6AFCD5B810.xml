<geoCoordinate id="EE0DA2CA3D73FFD4FC3BFEBFFBD7BB78" box="[973,1079,274,300]" degrees="37" direction="west" minutes="12" orientation="longitude" pageId="31" pageNumber="32" precision="925" value="-37.2">37º12’W</geoCoordinate>
