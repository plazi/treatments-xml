<taxonomicName id="4C0D2EBFFFE5143B26806128F4FF8459" authority="Billberg" authorityName="Billberg" box="[238,399,290,316]" class="Aves" family="Tytonidae" genus="Tyto" kingdom="Animalia" order="Strigiformes" pageId="2" pageNumber="535" phylum="Chordata" rank="genus">
