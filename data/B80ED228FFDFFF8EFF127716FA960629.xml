<taxonomicName id="F7A718BDFFDFFF8EFE9C7426FE3D0602" authority="Pruthi" authorityName="Pruthi" box="[281,491,572,598]" class="Insecta" family="Cicadellidae" genus="Stirellus" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="khewrensis">
