<quantity id="FB67AFFFFFF4FFA96E3EFDA5FC7AC09F" box="[795,901,614,640]" metricMagnitude="-3" metricUnit="m" metricValue="1.21" pageId="1" pageNumber="212" unit="mm" value="1.21">1.21 mm</quantity>
, EL: 
<quantity id="FB67AFFFFFF4FFA96EEFFDA5FBCBC09F" box="[970,1076,614,640]" metricMagnitude="-4" metricUnit="m" metricValue="8.7" pageId="1" pageNumber="212" unit="mm" value="0.87">0.87 mm</quantity>
, EW: 
<quantity id="FB67AFFFFFF4FFA969A4FDA5FB14C09F" box="[1153,1259,614,640]" metricMagnitude="-4" metricUnit="m" metricValue="9.2" pageId="1" pageNumber="212" unit="mm" value="0.92">0.92 mm</quantity>
, PL: 
<quantity id="FB67AFFFFFF4FFA9680BFDA5FA67C09F" box="[1326,1432,614,640]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="1" pageNumber="212" unit="mm" value="0.34">0.34 mm</quantity>
