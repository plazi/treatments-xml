<quantity id="FB67AFFFFFF4FFA96DF4FD49FEC5C0BB" box="[209,314,650,676]" metricMagnitude="-4" metricUnit="m" metricValue="7.9" pageId="1" pageNumber="212" unit="mm" value="0.79">0.79 mm</quantity>
