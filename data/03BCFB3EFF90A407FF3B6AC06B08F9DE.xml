<paragraph id="8BAA4A28FF90A407FF3B6AC0696EFCD9" blockId="10.[151,367,757,783]" box="[151,367,757,783]" pageId="10" pageNumber="37">
<heading id="D0E2FD44FF90A407FF3B6AC0696EFCD9" bold="true" box="[151,367,757,783]" fontSize="11" level="1" pageId="10" pageNumber="37" reason="1">
<emphasis id="B961963AFF90A407FF3B6AC0696EFCD9" bold="true" box="[151,367,757,783]" pageId="10" pageNumber="37">
The 
<taxonomicName id="4C1531ABFF90A407FF636AC0691CFCD9" box="[207,285,757,783]" class="Insecta" family="Dolichopodidae" genus="Campsicnemus" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="37" phylum="Arthropoda" rank="species" species="zigzag">zigzag</taxonomicName>
