New 
<collectingCountry id="F3707618FFECFFB6D424588EFB6DFB19" box="[523,612,1221,1247]" name="Guinea-Bissau" pageId="12" pageNumber="31">Guinea</collectingCountry>
, New 
<collectingCountry id="F3707618FFECFFB6D49E588EFA01FB19" box="[689,776,1221,1247]" name="Ireland" pageId="12" pageNumber="31">Ireland</collectingCountry>
, (
<typeStatus id="54DC882AFFECFFB6D533588EFA80FB19" box="[796,905,1221,1247]" pageId="12" pageNumber="31">holotype</typeStatus>
, 
<collectionCode id="ED76AE4DFFECFFB6D5B9588EFAEDFB19" LSID="urn:lsid:biocol.org:col:32981" box="[918,996,1221,1247]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="12" pageNumber="31">ANIC</collectionCode>
); New 
<collectingCountry id="F3707618FFECFFB6D216588EFD9BFB19" box="[1081,1170,1221,1247]" name="Guinea-Bissau" pageId="12" pageNumber="31">Guinea</collectingCountry>
