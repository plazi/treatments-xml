<emphasis id="B962EAD93C03A802FF7F297F10A6FC91" bold="true" box="[151,659,837,864]" pageId="10" pageNumber="141">
Genus 
<taxonomicName id="4C164D483C03A802FF06297C1317FCAE" authorityName="D.S.Jordan &amp; J.Swain" authorityYear="1884" box="[238,290,838,864]" class="Actinopterygii" family="Mugilidae" genus="Liza" kingdom="Animalia" order="Mugiliformes" pageId="10" pageNumber="141" phylum="Chordata" rank="genus">
