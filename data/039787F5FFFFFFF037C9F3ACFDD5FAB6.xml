<collectingCountry id="F3297673FFFFFFF03692F775FD90FAB6" box="[489,570,1317,1339]" name="Cyprus" pageId="17" pageNumber="604">Zypern</collectingCountry>
, 
<collectingCountry id="F3297673FFFFFFF03545F775FDD1FAB6" box="[574,635,1317,1339]" name="Israel" pageId="17" pageNumber="604">Israel</collectingCountry>
