<emphasis id="B952EAD3FFC4A223AC7EFE70FA7C23E8" bold="true" box="[1367,1436,391,417]" pageId="21" pageNumber="22">
<taxonomicNameLabel id="A26157A8FFC4A223AC7EFE70FA7C23E8" box="[1367,1436,391,417]" pageId="21" pageNumber="22" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
from 
<collectingCountry id="F3317651FFC4A223A9FEFE47FE702383" box="[215,400,432,458]" name="New Caledonia" pageId="21" pageNumber="22">New Caledonia</collectingCountry>
