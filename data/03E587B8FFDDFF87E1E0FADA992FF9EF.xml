<quantity id="4CB49B4BFFDEFF89E14DFA959992FADD" box="[292,423,1376,1400]" metricMagnitude="-3" metricUnit="m" metricValue="1.35" metricValueMax="1.4" metricValueMin="1.3" pageId="6" pageNumber="2336" unit="mm" value="1.35" valueMax="1.4" valueMin="1.3">1.3–1.4 mm</quantity>
