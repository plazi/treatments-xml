<number id="6C33EA67FF9BFFDAAAE33DD2FAABFCDF" box="[1279,1323,764,790]" pageId="24" pageNumber="31" value="142.0">142</number>
“n= 
<number id="6C33EA67FF9BFFDAAF283C0AFEC3FCF7" box="[308,323,804,830]" pageId="24" pageNumber="31" value="7.0">7</number>
aus Bolu”); Roselaar (
<number id="6C33EA67FF9BFFDAAC533C0AFD0FFCF7" box="[591,655,804,830]" pageId="24" pageNumber="31" value="1995.0">1995</number>
