<materialsCitation id="A5CB3CDFFFC806780101624DBFF6EA37" ID-GBIF-Occurrence="2575928463" country="Brazil" location="Minas Gerais" pageId="31" pageNumber="68" specimenCount="1" stateProvince="Minas Gerais" typeStatus="holotype">
<collectingCountry id="6DB47612FFC806780101624DB852EA1B" box="[1068,1126,1797,1816]" name="Brazil" pageId="31" pageNumber="68">Brazil</collectingCountry>
