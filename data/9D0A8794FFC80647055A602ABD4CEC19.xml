<subSubSection id="5DB96509FFC806780668624CBFF6EA37" pageId="31" pageNumber="68" type="distribution">
<paragraph id="151C3682FFC806780668624CBFF6EA37" blockId="31.[805,1475,178,1984]" pageId="31" pageNumber="68">
<emphasis id="27D7EA90FFC806780668624CB82CEA1B" box="[837,1048,1796,1816]" italics="true" pageId="31" pageNumber="68">Geographical records</emphasis>
