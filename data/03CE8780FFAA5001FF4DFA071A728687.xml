(
<figureCitation id="135C2A13FFAA5006FF45FA2519F880F0" box="[159,245,1446,1471]" captionStart="FIGURES 13 – 21. 13 – 17" captionStartId="25.[151,264,1501,1524]" captionTargetBox="[151,1436,215,1455]" captionTargetId="figure@25.[151,1436,215,1456]" captionTargetPageId="25" captionText="FIGURES 13 – 21. 13 – 17. Head in dorsal view. 13. Caloapenesia paruwa sp. nov. 14. C. rikawa sp. nov. 15. C. sabeli sp. nov. 16. C. supra sp. nov. 17. C. thailandiana. 18 – 21. Mandible in frontal view. 18. C. edas sp. nov. 19. C. jailuna sp. nov. 20. C. lani sp. nov. 21. C. rikawa sp. nov. (Scale bar = 300 µm)." httpUri="https://zenodo.org/record/231451/files/figure.png" pageId="19" pageNumber="520">Figs 16</figureCitation>
, 
<figureCitation id="135C2A13FFAA5006FEDBFA24182D80F0" box="[257,288,1447,1471]" captionStart="FIGURES 31 – 39" captionStartId="27.[151,264,1862,1885]" captionTargetBox="[151,1436,193,1841]" captionTargetId="figure@27.[151,1436,193,1842]" captionTargetPageId="27" captionText="FIGURES 31 – 39. Mesosoma in dorsal view. 31. Caloapenesia leptata sp. nov. 32. C. mugra sp. nov. 33. C. nadaili sp. nov. 34. C. paruwa sp. nov. 35. C. rikawa sp. nov. 36. C. sabeli sp. nov. 37. C. supra sp. nov. 38. C. thailandiana. 39. Forewing. C. ana sp. nov. (Scale bar = 300 µm)." httpUri="https://zenodo.org/record/231453/files/figure.png" pageId="19" pageNumber="520">37</figureCitation>
, 
<figureCitation id="135C2A13FFAA5006FEF1FA25184780F0" box="[299,330,1446,1471]" captionStart="FIGURES 51 – 58" captionStartId="29.[151,264,1620,1643]" captionTargetBox="[161,1425,193,1597]" captionTargetId="figure@29.[156,1430,193,1599]" captionTargetPageId="29" captionText="FIGURES 51 – 58. Hypopygium in dorsal view. 51. Caloapenesia leptata sp. nov. 52. C. mugra sp. nov. 53. C. nadaili sp. nov. 54. C. paruwa sp. nov. 55. C. rikawa sp. nov. 56. C. sabeli sp. nov. 57. C. supra sp. nov. 58. C. thailandiana. (Scale bar = 250 µm)." httpUri="https://zenodo.org/record/231456/files/figure.png" pageId="19" pageNumber="520">57</figureCitation>
, 
<figureCitation id="135C2A13FFAA5006FE8FFA24189180F0" box="[341,412,1447,1471]" captionStart="FIGURES 86 – 94" captionStartId="33.[151,264,1953,1976]" captionTargetBox="[227,1363,201,1930]" captionTargetId="figure@33.[219,1368,193,1932]" captionTargetPageId="33" captionText="FIGURES 86 – 94. Male genitalia. 86. Caloapenesia paruwa sp. nov. Ventral view. 87 – 88. C. rikawa sp. nov. 87. Dorsal view. 88. Ventral view. 89 – 90. C. sabeli sp. nov. 89. Dorsal view. 90. Ventral view. 91 – 92. C. supra sp. nov. 91. Dorsal view. 92. Ventral view. 93 – 94. C. thailandiana. 93. Dorsal view. 94. Ventral view. (Scale bar scala = 250 µm)." httpUri="https://zenodo.org/record/231460/files/figure.png" pageId="19" pageNumber="520">
91, 
<number id="3DAF24B1FFAA5006FEA5FA24189180F0" box="[383,412,1447,1471]" pageId="19" pageNumber="520" value="92.0">92</number>
