<subSubSection id="C32FF4D3FFC5FFC9DC91FEB9FDC079D9" box="[163,602,942,965]" pageId="68" pageNumber="69" type="nomenclature">
<paragraph id="8B8AA758FFC5FFC9DC91FEB9FDC079D9" blockId="68.[163,602,942,965]" box="[163,602,942,965]" pageId="68" pageNumber="69">
<heading id="D0C21034FFC5FFC9DC91FEB9FDC079D9" box="[163,602,942,965]" centered="true" fontSize="9" level="2" pageId="68" pageNumber="69" reason="2">
<taxonomicName id="4C35DCDBFFC5FFC9DC91FEB9FE5979D8" box="[163,451,942,965]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="vannoorti" status="new species">
<emphasis id="B9417B4AFFC5FFC9DC91FEB9FE5979D8" bold="true" box="[163,451,942,965]" italics="true" pageId="68" pageNumber="69">Paramblynotus vannoorti</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFC5FFC9DDE1FEA7FDC079D9" box="[467,602,943,965]" pageId="68" pageNumber="69" rank="species">new species</taxonomicNameLabel>
