<quantity id="4CD39B07FFEE1A38CA3637754705FE21" box="[634,799,860,884]" metricMagnitude="-4" metricUnit="m" metricValue="2.4" metricValueMax="2.5" metricValueMin="2.3" pageId="32" pageNumber="33" unit="mm" value="0.24" valueMax="0.25" valueMin="0.23">0.23–0.25 mm</quantity>
