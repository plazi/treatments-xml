<quantity id="4CD39B07FFEE1A38CC49371E40B6FE1A" box="[1029,1196,823,847]" metricMagnitude="-4" metricUnit="m" metricValue="8.049999999999999" metricValueMax="8.299999999999999" metricValueMin="7.800000000000001" pageId="32" pageNumber="33" unit="mm" value="0.8049999999999999" valueMax="0.83" valueMin="0.78">0.78–0.83 mm</quantity>
