(n = 
<number id="3DE324C5FFEE1A38CA9D371E46F8FE1A" box="[721,738,823,847]" pageId="32" pageNumber="33" value="7.0">7</number>
): TL 
<quantity id="4CD39B07FFEE1A38CB6F371E47D0FE1A" box="[803,970,823,847]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" metricValueMax="3.75" metricValueMin="3.65" pageId="32" pageNumber="33" unit="mm" value="3.7" valueMax="3.75" valueMin="3.65">3.65–3.75 mm</quantity>
