; ML 
<quantity id="4CD39B07FFEE1A38C9EC3775465DFE21" box="[416,583,860,884]" metricMagnitude="-3" metricUnit="m" metricValue="1.1749999999999998" metricValueMax="1.2" metricValueMin="1.15" pageId="32" pageNumber="33" unit="mm" value="1.1749999999999998" valueMax="1.2" valueMin="1.15">1.15–1.20 mm</quantity>
