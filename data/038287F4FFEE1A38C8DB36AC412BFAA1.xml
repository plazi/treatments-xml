<quantity id="4CD39B07FFEE1A38CCBC371E4182FE1A" box="[1264,1432,823,847]" metricMagnitude="-4" metricUnit="m" metricValue="7.1499999999999995" metricValueMax="7.3" metricValueMin="7.0" pageId="32" pageNumber="33" unit="mm" value="0.715" valueMax="0.73" valueMin="0.7">0.70–0.73 mm</quantity>
