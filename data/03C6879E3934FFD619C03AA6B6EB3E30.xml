<taxonomicName id="4C6F4D0B3934FFD61AD739AFB6153E18" box="[896,1081,1771,1796]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="55" phylum="Tracheophyta" rank="species" species="sylvatica">
