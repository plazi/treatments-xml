<taxonomicName id="4C31C981C2730D0FFE4EFDFD0E6B6447" baseAuthorityName="Sars" baseAuthorityYear="1901" box="[440,528,513,539]" class="Ostracoda" family="Cyprididae" genus="Strandesia" kingdom="Animalia" order="Podocopida" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="elliptica">elliptica</taxonomicName>
</emphasis>
