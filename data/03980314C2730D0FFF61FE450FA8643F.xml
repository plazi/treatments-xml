<taxonomicName id="4C31C981C2730D0FFF3DFDFD0DD06447" authorityName="G. W. Muller" authorityYear="1912" baseAuthorityName="Sars" baseAuthorityYear="1901" box="[203,427,513,539]" class="Ostracoda" family="Cyprididae" genus="Strandesia" kingdom="Animalia" order="Podocopida" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="obtusata">Strandesia obtusata</taxonomicName>
