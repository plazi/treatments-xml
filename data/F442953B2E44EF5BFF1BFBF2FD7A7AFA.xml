<geoCoordinate id="19DF42EA2E44EF5BFBF5FBB3FB557A5A" box="[1107,1248,1115,1139]" degrees="39" direction="north" minutes="59.257" orientation="latitude" pageId="57" pageNumber="58" precision="1" value="39.987617">39º59.257’N</geoCoordinate>
