<quantity id="BB1389C82E44EF5BFD41FBB3FCFD7A5A" box="[743,840,1115,1139]" metricMagnitude="-1" metricUnit="kg" metricValue="3.7563" pageId="57" pageNumber="58" unit="g" value="375.63">375.63 g</quantity>
