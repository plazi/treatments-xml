<date id="085502ED2E44EF5BFECDFBB3FDAC7A5A" box="[363,537,1115,1139]" pageId="57" pageNumber="58" value="2008-11">November 2008</date>
(
<quantity id="BB1389C82E44EF5BFD8CFBB3FD337A5B" box="[554,646,1115,1139]" metricMagnitude="-1" metricUnit="m" metricValue="3.0" pageId="57" pageNumber="58" unit="mm" value="300.0">300 mm</quantity>
