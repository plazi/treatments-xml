<geoCoordinate id="19DF42EA2E44EF5BFB4BFBB3FADA7A5A" box="[1261,1391,1115,1139]" degrees="10" direction="west" minutes="3.025" orientation="longitude" pageId="57" pageNumber="58" precision="1" value="-10.050417">10º3.025’W</geoCoordinate>
