Genus 
<taxonomicName id="4C3B4D7CBF2AFFA90F4E6CFE9E253088" authority="Leach, 1815" authorityName="Leach" authorityYear="1815" box="[351,570,1252,1278]" class="Malacostraca" family="Aegidae" genus="Aega" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="24" phylum="Arthropoda" rank="genus">
