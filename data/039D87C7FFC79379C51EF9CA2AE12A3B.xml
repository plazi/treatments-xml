<taxonomicName id="4C344D52FFC69379C023FEEB2F082EA8" authority="Silvestri" authorityName="Silvestri" class="Chilopoda" family="Scutigeridae" genus="Scutigera" kingdom="Animalia" order="Scutigeromorpha" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="aethiopica">
