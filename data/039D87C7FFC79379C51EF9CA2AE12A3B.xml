<taxonomicName id="4C344D52FFC69379C09DFE372F392EFD" class="Chilopoda" family="Scutigeridae" genus="Thereuonema" kingdom="Animalia" order="Scutigeromorpha" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="syriaca">
