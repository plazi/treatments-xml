<taxonomicName id="4C344D52FFC69379C77DFE0D2B4D2EA8" authority="Ribaut, 1907" authorityName="Ribaut" authorityYear="1907" box="[875,1322,397,423]" class="Chilopoda" family="Scutigeridae" genus="Thereuonema" kingdom="Animalia" order="Scutigeromorpha" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tropicalis">
