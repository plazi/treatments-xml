<geoCoordinate id="EE7250156926FFA5FC72FFBA3A61FA5C" box="[898,1033,1425,1451]" degrees="17" direction="north" minutes="07" orientation="latitude" pageId="6" pageNumber="643" precision="15" seconds="58" value="17.132778">17°07ʹ58ʺ N</geoCoordinate>
, 
<geoCoordinate id="EE7250156926FFA5FBE3FFBA3AC5FA5C" box="[1043,1197,1425,1451]" degrees="092" direction="west" minutes="48" orientation="longitude" pageId="6" pageNumber="643" precision="15" seconds="19" value="-92.80528">092°48ʹ19ʺ W</geoCoordinate>
