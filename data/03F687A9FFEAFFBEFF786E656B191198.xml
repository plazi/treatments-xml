<taxonomicName id="4C5F4D3CFFEBFFB1FEDA6BE16A9814EE" authority="Leng, Heller and Huang" authorityName="Leng, Heller and Huang" authorityYear="2018" box="[298,757,1967,1989]" class="Insecta" family="Sciaridae" genus="Dichopygina" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="25" phylum="Arthropoda" rank="species" species="tibetana" status="sp. nov.">
