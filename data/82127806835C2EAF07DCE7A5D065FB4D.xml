<taxonomicName id="EE279022DC68B8D461D6FB22B81D417F" class="Gastropoda" family="Melanopsidae" genus="Microcolpia" kingdom="Animalia" lsidName="Microcolpia cornea" order="Sorbeoconcha" pageId="250" pageNumber="251" phylum="Mollusca" rank="species" species="cornea">
