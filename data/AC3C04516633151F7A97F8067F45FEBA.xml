<caption id="70EAE5CF6637151E7A97F8CC7DB4F879" ID-DOI="http://doi.org/10.5281/zenodo.4897337" ID-Zenodo-Dep="4897337" httpUri="https://zenodo.org/record/4897337/files/figure.png" pageId="4" pageNumber="587" startId="4.[151,250,1898,1922]" targetBox="[169,1437,421,1864]" targetPageId="4">
<paragraph id="242AB5476637151E7A97F8CC7DB4F879" blockId="4.[151,1436,1898,1958]" pageId="4" pageNumber="587">
<emphasis id="16E169556637151E7A97F8CC7F4CF85D" bold="true" box="[151,277,1898,1922]" pageId="4" pageNumber="587">FIGURE 4.</emphasis>
<taxonomicName id="E395CEC46637151E7B1DF8CC7D62F85D" authority="Li, Wang and Yang, 2016" authorityName="Li, Wang and Yang" authorityYear="2016" box="[285,827,1898,1923]" class="Insecta" family="Nemouridae" genus="Amphinemura" kingdom="Animalia" order="Plecoptera" pageId="4" pageNumber="587" phylum="Arthropoda" rank="species" species="bimaculata">
