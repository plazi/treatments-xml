<taxonomicName id="4C07E28A9F7E886E3462B12BFE1EBBBB" authorityName="O. F. Muller" authorityYear="1776" box="[367,442,370,396]" class="Ostracoda" family="Cyprididae" genus="Cypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="8" pageNumber="25" phylum="Arthropoda" rank="genus">
