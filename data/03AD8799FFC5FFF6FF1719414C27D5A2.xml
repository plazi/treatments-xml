B, 
<number id="3DCC24A8FFC2FFF4FCF8196D4AE6D122" box="[888,902,260,284]" pageId="12" pageNumber="435" value="3.0">3</number>
B, 
<number id="3DCC24A8FFC2FFF4FC26196D4AD6D122" box="[934,950,260,284]" pageId="12" pageNumber="435" value="4.0">4</number>
B, 
