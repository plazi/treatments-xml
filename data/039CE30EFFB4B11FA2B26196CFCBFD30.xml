<taxonomicName id="4C35299BFFBBB11FA70367E1CFCBFF06" box="[1337,1450,159,180]" class="Agaricomycetes" genus="Cotylidia" kingdom="Fungi" pageId="8" pageNumber="147" phylum="Basidiomycota" rank="species" species="undulata">
