<treatmentCitationGroup id="FFF9CD1FFFD5FF8CFF13FE618863FE32" box="[151,780,426,449]" pageId="96" pageNumber="97">
<treatmentCitation id="5E48CC20FFD5FF8CFF13FE618867FE32" author="Sasa, M." box="[151,776,426,449]" page="10" pageId="96" pageNumber="97" year="1979">
