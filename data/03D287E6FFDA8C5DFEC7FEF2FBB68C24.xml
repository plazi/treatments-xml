<number id="3DB324D7FFDA8C5FFBC1FEF5FB968C4F" box="[1038,1077,283,309]" pageId="1" pageNumber="2" value="5.0" valueMax="9.0" valueMin="1.0">1­9</number>
</figureCitation>
)
