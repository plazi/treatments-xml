<paragraph id="8BFD36B4FF9BFFCF97C0FD61FAFBFD3B" blockId="75.[151,1436,594,1051]" box="[199,1356,738,763]" pageId="75" pageNumber="76">
<emphasis id="B936EAA6FF9BFFCF97C0FD61FE58FD3B" bold="true" box="[199,495,738,763]" pageId="75" pageNumber="76">Geographic distribution.</emphasis>
<collectingRegion id="4986F856FF9BFFCF96F2FD60FDE7FD3A" box="[501,592,739,762]" country="Sri Lanka" name="Western" pageId="75" pageNumber="76">Western</collectingRegion>
