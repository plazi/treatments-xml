<taxonomicName id="8DEC4D30FF764FC1FC67F9F6423DFB55" authority="Mistshenko" authorityName="Mistshenko" box="[928,1348,1188,1213]" class="Insecta" family="Pamphagidae" genus="Paranocaracris" kingdom="Animalia" order="Orthoptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="subSpecies" species="rimansonae" subSpecies="ventosus">
