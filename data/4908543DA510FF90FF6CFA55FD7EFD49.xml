(Figs.: 
<number id="7769F70CA510FF92FF1DFA70FEF0FA24" box="[230,345,1452,1477]" pageId="36" pageNumber="537" value="20.55" valueMax="20.6" valueMin="20.5">20.5–20.6</number>
, 
<number id="7769F70CA510FF92FE9FFA71FE7DFA24" box="[356,468,1453,1477]" pageId="36" pageNumber="537" value="30.450000000000003" valueMax="30.8" valueMin="30.1">30.1–30.8</number>
