, HL 1.00– 
<number box="[1201,1251,1472,1496]" pageId="5" pageNumber="218" value="1.1">1.10</number>
, HW 
<number box="[1317,1432,1471,1496]" pageId="5" pageNumber="218" value="1.175" valueMax="1.25" valueMin="1.1">1.10–1.25</number>
, PL 
