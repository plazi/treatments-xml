species: 21) 
<taxonomicName box="[1097,1199,1965,1988]" class="Insecta" family="Lucanidae" genus="Leptinopterus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="41" phylum="Arthropoda" rank="species" species="tibialis">
<emphasis box="[1097,1199,1965,1988]" italics="true" pageId="9" pageNumber="41">L. tibialis</emphasis>
</taxonomicName>
