<number box="[682,698,1199,1225]" pageId="4" pageNumber="5" value="5.0">5</number>
(
<number box="[708,724,1199,1225]" pageId="4" pageNumber="5" value="5.0">5</number>
). Leg 
<date box="[803,863,1199,1225]" pageId="4" pageNumber="5" value="1933-02">
II 
<number box="[833,863,1199,1225]" pageId="4" pageNumber="5" value="33.0">33</number>
</date>
(
<number box="[880,954,1199,1225]" pageId="4" pageNumber="5" value="33.5" valueMax="35.0" valueMin="32.0">32­35</number>
); femur 
<number box="[1057,1086,1199,1225]" pageId="4" pageNumber="5" value="10.0">10</number>
