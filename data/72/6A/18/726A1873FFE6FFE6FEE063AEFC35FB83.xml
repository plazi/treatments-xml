<number box="[1199,1228,1438,1464]" pageId="4" pageNumber="5" value="23.0">23</number>
(
<number box="[1244,1314,1438,1464]" pageId="4" pageNumber="5" value="21.0" valueMax="23.0" valueMin="19.0">19­23</number>
) long; genital seta (
<emphasis box="[498,527,1479,1504]" italics="true" pageId="4" pageNumber="5">
<number box="[498,512,1479,1504]" pageId="4" pageNumber="5" value="3.0">3</number>
a
</emphasis>
) 
<number box="[550,579,1478,1504]" pageId="4" pageNumber="5" value="15.0">15</number>
(
<number box="[601,674,1478,1504]" pageId="4" pageNumber="5" value="13.5" valueMax="15.0" valueMin="12.0">12­15</number>
