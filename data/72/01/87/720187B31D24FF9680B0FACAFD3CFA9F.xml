</taxonomicName>
(
<taxonomicName authority="Panzer, 1794" authorityName="Panzer" authorityYear="1794" class="Insecta" family="Leiodidae" genus="Catops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" phylum="Arthropoda" rank="species" species="tristis">
<emphasis italics="true" pageId="9">C. tristis</emphasis>
(Panzer, 1794)
</taxonomicName>
and 
<taxonomicName authority="Kellner, 1846" authorityName="Kellner" authorityYear="1846" box="[440,745,620,645]" class="Insecta" family="Leiodidae" genus="Catops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" phylum="Arthropoda" rank="species" species="subfuscus">
<emphasis box="[440,581,620,644]" italics="true" pageId="9">C. subfuscus</emphasis>
