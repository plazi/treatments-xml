<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142751" class="Insecta" family="Formicidae" genus="Solenopsis" kingdom="Animalia" lsidName-HNS="Solenopsis transversalis Smith" order="Hymenoptera" pageId="7" pageNumber="74" phylum="Arthropoda" rank="species" species="transversalis">Solenopsis transversalis</taxonomicName>
<authority>.</authority>
