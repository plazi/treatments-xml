, 
<collectingCountry box="[1047,1142,188,213]" name="Ecuador" pageId="51" pageNumber="52">Ecuador</collectingCountry>
, French Guiana, 
<collectingCountry box="[1342,1432,188,213]" name="Guyana" pageId="51" pageNumber="52">Guyana</collectingCountry>
, 
<collectingCountry box="[151,243,224,247]" name="Panama" pageId="51" pageNumber="52">Panama</collectingCountry>
, 
<collectingCountry box="[254,359,224,248]" name="Paraguay" pageId="51" pageNumber="52">Paraguay</collectingCountry>
