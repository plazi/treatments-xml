</taxonomicName>
(
<collectingCountry box="[581,634,1513,1538]" name="Peru" pageId="69" pageNumber="70">Peru</collectingCountry>
and 
<collectingCountry box="[688,798,1513,1538]" name="Paraguay" pageId="69" pageNumber="70">Paraguay</collectingCountry>
