. 
<taxonomicName authorityName="Distant" authorityYear="1905" box="[305,566,1513,1538]" class="Insecta" family="Cicadidae" genus="Ahomana" kingdom="Animalia" order="Hemiptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="neotropicalis">
