<taxonomicName authorityName="De Man" authorityYear="1888" box="[1092,1261,989,1013]" class="Malacostraca" family="Diogenidae" genus="Clibanarius" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="35" phylum="Arthropoda" rank="species" species="merguiensis">
