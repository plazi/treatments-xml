<taxonomicName authorityName="Ramond (Blank" authorityYear="2002" box="[274,443,1197,1220]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="52" pageNumber="53" phylum="Tracheophyta" rank="species" species="uncinata">
