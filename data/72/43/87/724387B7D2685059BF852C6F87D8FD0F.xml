<location LSID="urn:lsid:plazi:treatment:724387B7D2685059BF852C6F87D8FD0F:FF35607AD2685059BB782CEC8696FE17" box="[1130,1234,470,491]" country="Democratic Republic of the Congo" name="east Africa" pageId="31" pageNumber="232">east Africa</location>
from 
<collectingCountry box="[1292,1395,470,491]" name="Democratic Republic of the Congo" pageId="31" pageNumber="232">DR Congo</collectingCountry>
and 
<collectingCountry box="[225,311,499,520]" name="Tanzania" pageId="31" pageNumber="232">Tanzania</collectingCountry>
to 
<collectingCountry box="[341,468,499,520]" name="Mozambique" pageId="31" pageNumber="232">Mozambique</collectingCountry>
and 
<collectingCountry box="[514,592,499,520]" name="Zambia" pageId="31" pageNumber="232">Zambia</collectingCountry>
)............................................... 
