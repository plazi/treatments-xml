<subSubSection pageId="8" pageNumber="9" type="materials_examined">
<paragraph blockId="8.[264,1324,284,1878]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[312,487,1244,1270]" pageId="8" pageNumber="9">
<typeStatus box="[312,371,1244,1270]" pageId="8" pageNumber="9">Type</typeStatus>
material
</emphasis>
. 
