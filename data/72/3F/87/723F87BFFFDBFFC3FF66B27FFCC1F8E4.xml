; shells in 
<quantity box="[520,638,1410,1434]" metricMagnitude="2" metricUnit="m" metricValue="3.685" metricValueMax="6.22" metricValueMin="1.15" pageId="44" pageNumber="259" unit="m" value="368.5" valueMax="622.0" valueMin="115.0">
