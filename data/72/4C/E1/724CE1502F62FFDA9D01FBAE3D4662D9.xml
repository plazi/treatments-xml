<quantity box="[769,936,1199,1225]" metricMagnitude="3" metricUnit="m" metricValue="1.5" metricValueMax="1.7" metricValueMin="1.3" pageId="5" pageNumber="282" unit="m" value="1500.0" valueMax="1700.0" valueMin="1300.0">1300–1700 m</quantity>
, 
<date box="[949,1071,1199,1225]" pageId="5" pageNumber="282" value="1997-06-03">
