<taxonomicName id="4C594D01FF85FF9BFF0BFBA46AFD2C0E" authorityName="Hampson" authorityYear="1898" box="[151,250,1031,1052]" class="Insecta" family="Erebidae" genus="Pseudothyretes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="29" phylum="Arthropoda" rank="species" species="carnea">
<emphasis id="B92DEA90FF85FF9BFF0BFBA46AFD2C0E" box="[151,250,1031,1052]" italics="true" pageId="7" pageNumber="29">P. carnea</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C594D01FF85FF9BFE94FBA46B932C09" authorityName="Hampson" authorityYear="1901" box="[264,404,1030,1052]" class="Insecta" family="Erebidae" genus="Pseudothyretes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="29" phylum="Arthropoda" rank="species" species="erubescens">
<emphasis id="B92DEA90FF85FF9BFE94FBA46B932C09" box="[264,404,1030,1052]" italics="true" pageId="7" pageNumber="29">P. erubescens</emphasis>
</taxonomicName>
, 
