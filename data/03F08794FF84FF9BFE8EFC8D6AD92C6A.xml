<typeStatus id="54E28820FF84FF9AFF20FC6D6AF52BF1" box="[188,242,974,995]" pageId="6" pageNumber="28">Type</typeStatus>
species: 
<taxonomicName id="4C594D01FF84FF9AFECBFC6D68982BF6" authority="Dufrane, 1945" authorityName="Dufrane" authorityYear="1945" box="[343,671,974,996]" class="Insecta" family="Arctiidae" genus="Apisa" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="28" phylum="Arthropoda" rank="species" species="kivensis">
<emphasis id="B92DEA90FF84FF9AFECBFC6D6BF02BF1" box="[343,503,974,995]" italics="true" pageId="6" pageNumber="28">Apisa kivensis</emphasis>
<bibRefCitation id="EFC84B73FF84FF9AFE62FC6D68982BF6" author="Dufrane A" box="[510,671,974,996]" pageId="6" pageNumber="28" pagination="90 - 143" refId="ref28597" refString="Dufrane A. 1945. Lepidopteres du Kivu (3 e note). Bulletin et Annales de la Societe Entomologique de Belgique 81: 90 - 143." type="journal article" year="1945">Dufrane, 1945</bibRefCitation>
