<number id="2A4BED7DB650AB4DFEEEC0543703FBC2" box="[333,364,1105,1129]" pageId="6" pageNumber="107" value="14.0">14</number>
; Dc 
<number id="2A4BED7DB650AB4DFE03C05437D0FBC2" box="[416,447,1105,1129]" pageId="6" pageNumber="107" value="10.0">10</number>
; Pa not observed; Scts 
<number id="2A4BED7DB650AB4DFD61C05434BCFBC2" box="[706,723,1105,1129]" pageId="6" pageNumber="107" value="6.0">6</number>
