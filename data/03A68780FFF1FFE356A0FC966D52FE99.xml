<paragraph id="8BB03696FFF1FFE256A0FC4F6DE4FC5A" blockId="2.[151,1274,912,934]" box="[151,1274,912,934]" pageId="2" pageNumber="403">
<taxonomicName id="4C0F4D15FFF1FFE256A0FC4F68A0FC5A" authority="Bigot, 1880a: 63" authorityName="Bigot" authorityPageNumber="63" authorityYear="1880" box="[151,446,912,934]" class="Insecta" family="Empididae" genus="Enoplempis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="403" phylum="Arthropoda" rank="genus">
<emphasis id="B97BEA84FFF1FFE256A0FC4F6811FC5A" box="[151,271,912,934]" italics="true" pageId="2" pageNumber="403">Enoplempis</emphasis>
Bigot, 
<number id="3DC724B1FFF1FFE2576DFC4F688EFC5A" box="[346,400,912,934]" pageId="2" pageNumber="403" value="1880.0">1880</number>
a: 
<number id="3DC724B1FFF1FFE25792FC4F68A0FC5A" box="[421,446,912,934]" pageId="2" pageNumber="403" value="63.0">63</number>
</taxonomicName>
[
