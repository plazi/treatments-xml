<taxonomicName id="07C04D0FAD0EFFDAFC30C816FAC3FA65" authority="Jak." authorityName="Jak." box="[949,1357,1405,1430]" class="Insecta" family="Tingidae" genus="Copium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="471" phylum="Arthropoda" rank="variety" species="teucrii" variety="brevicorne">
