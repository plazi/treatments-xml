<paragraph id="765CCA2BFF8AB009FF20FDD8F261D2A7" blockId="21.[144,354,639,663]" box="[144,354,639,663]" pageId="21" pageNumber="2206">
<emphasis id="44971639FF8AB009FF20FDD8F261D2A7" box="[144,354,639,663]" italics="true" pageId="21" pageNumber="2206">Material examined</emphasis>
</paragraph>
<paragraph id="765CCA2BFF8AB009FF20FD00F065D2CF" blockId="21.[144,1104,679,767]" pageId="21" pageNumber="2206">
<materialsCitation id="C68BC076FF8AB009FF20FD00F065D2CF" collectingDate="1909-12-09" collectionCode="BMNH" country="Sri Lanka" location="Female" pageId="21" pageNumber="2206" specimenCount="1" typeStatus="holotype">
