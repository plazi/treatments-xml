and 
<taxonomicName id="4C230FBBFFCFA474FD4EF8A7FCD17817" box="[740,857,1831,1856]" class="Entognatha" family="Paronellidae" genus="Trogolaphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="ocellata" status="sp. nov.">
