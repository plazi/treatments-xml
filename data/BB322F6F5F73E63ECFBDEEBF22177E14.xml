<collectionCode id="558A06BC5F73E631CBC1ED80264A7B91" box="[1259,1356,1925,1951]" pageId="6" pageNumber="64">MNHW</collectionCode>
. 
<typeStatus id="EC2020DB5F73E631CA7DED8023CF7BC9" pageId="6" pageNumber="64">Holotype</typeStatus>
