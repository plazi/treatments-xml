, 
<collectingCountry id="F5CD764BFFAFFF8A8E03F948FE4E5A97" box="[383,476,1755,1780]" name="Jamaica" pageId="9" pageNumber="454">Jamaica</collectingCountry>
, 
<collectingCountry id="F5CD764BFFAFFF8A8E94F948FDB45A97" box="[488,550,1755,1780]" name="Cuba" pageId="9" pageNumber="454">Cuba</collectingCountry>
