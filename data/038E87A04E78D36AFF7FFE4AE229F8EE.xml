<taxonomicName id="4C274D354E78D36AFE80F98DE283F906" authority="Koehler, 1904" authorityName="Koehler" authorityYear="1904" box="[360,924,1649,1674]" class="Ophiuroidea" family="Gorgonocephalidae" genus="Asteroporpa" kingdom="Animalia" order="Euryalida" pageId="1" pageNumber="544" phylum="Echinodermata" rank="species" species="bellator" subGenus="Astrohelix">
<emphasis id="B953EAA44E78D36AFE80F98DE0ECF905" box="[360,499,1650,1673]" italics="true" pageId="1" pageNumber="544">Asteroporpa</emphasis>
(
<emphasis id="B953EAA44E78D36AFDECF98EE369F905" box="[516,630,1649,1673]" italics="true" pageId="1" pageNumber="544">Astrohelix</emphasis>
) 
<emphasis id="B953EAA44E78D36AFD60F98EE3FEF905" box="[648,737,1649,1673]" italics="true" pageId="1" pageNumber="544">bellator</emphasis>
(
<bibRefCitation id="EFB64B474E78D36AFD19F98EE28BF906" author="Koehler" box="[753,916,1649,1674]" pageId="1" pageNumber="544" refString="Koehler, R. (1904) Ophiures de l'Expedition du Siboga. Part I. Ophiures de Mer profonde. Siboga-Expeditie, 45 a, 1 - 176." type="book chapter" year="1904">Koehler, 1904</bibRefCitation>
)
</taxonomicName>
from the 
<location id="8EF8606D4E78D36AFBE5F98EE5E8F906" LSID="urn:lsid:plazi:treatment:038E87A04E78D36AFF7FFE4AE229F8EE:8EF8606D4E78D36AFBE5F98EE5E8F906" box="[1037,1271,1649,1674]" country="Fiji" county="northwestern Australia" municipality="western Japan" name="southern Philippines" pageId="1" pageNumber="544">southern Philippines</location>
, 
<quantity id="4CDF9B534E78D36AFAEBF98EE454F905" box="[1283,1355,1649,1674]" metricMagnitude="2" metricUnit="m" metricValue="2.75" pageId="1" pageNumber="544" unit="m" value="275.0">
<elevation id="000AD1854E78D36AFAEBF98EE454F905" box="[1283,1355,1649,1674]" metricMagnitude="2" metricUnit="m" metricValue="2.75" pageId="1" pageNumber="544" unit="m" value="275.0">275 m</elevation>
</quantity>
and 
<taxonomicName id="4C274D354E78D36AFA6CF98DE364F922" authority="Mortensen, 1933" authorityName="Mortensen" authorityYear="1933" class="Ophiuroidea" family="Gorgonocephalidae" genus="Asteroporpa" kingdom="Animalia" order="Euryalida" pageId="1" pageNumber="544" phylum="Echinodermata" rank="species" species="paucidens" subGenus="Astrohelix">
<emphasis id="B953EAA44E78D36AFA6CF98DE483F905" box="[1412,1436,1650,1673]" italics="true" pageId="1" pageNumber="544">A.</emphasis>
(
<emphasis id="B953EAA44E78D36AFF48F96AE00DF921" box="[160,274,1685,1709]" italics="true" pageId="1" pageNumber="544">Astrohelix</emphasis>
) 
<emphasis id="B953EAA44E78D36AFEC0F96AE086F921" box="[296,409,1685,1709]" italics="true" pageId="1" pageNumber="544">paucidens</emphasis>
(
<bibRefCitation id="EFB64B474E78D36AFE45F969E36CF922" author="Mortensen" box="[429,627,1686,1710]" pageId="1" pageNumber="544" refString="Mortensen, T. (1933) Studies of Indo-Pacific euryalids. Fidenskabelige Meddelelser fra Dansk Naturhistorisk Forening i KjObenhavn, 96, 1 - 75." type="journal article" year="1933">Mortensen, 1933</bibRefCitation>
)
</taxonomicName>
from 
<collectingMunicipality id="6BFCACCC4E78D36AFD23F969E26EF921" box="[715,881,1686,1709]" pageId="1" pageNumber="544">western Japan</collectingMunicipality>
, 
<elevation id="000AD1854E78D36AFC69F969E2D2F921" box="[897,973,1686,1710]" metricMagnitude="2" metricUnit="m" metricValue="1.9" pageId="1" pageNumber="544" unit="m" value="190.0">
<quantity id="4CDF9B534E78D36AFC69F969E2D2F921" box="[897,973,1686,1710]" metricMagnitude="2" metricUnit="m" metricValue="1.9" pageId="1" pageNumber="544" unit="m" value="190.0">190 m</quantity>
