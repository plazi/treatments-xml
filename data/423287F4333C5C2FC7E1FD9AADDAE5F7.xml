<taxonomicName id="0D9B4D61333C5C2FC7E1FD39AD69E595" authorityName="Erichson" authorityYear="1839" box="[178,302,674,695]" class="Insecta" family="Staphylinidae" genus="Scymbalium" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="983" phylum="Arthropoda" rank="genus">
