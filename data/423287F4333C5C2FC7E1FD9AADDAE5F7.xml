<taxonomicName id="0D9B4D61333C5C2FC5E2FD1EAF5EE5BB" box="[689,793,644,666]" class="Insecta" family="Staphylinidae" genus="Micrillus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="983" phylum="Arthropoda" rank="species" species="badius">
<emphasis id="F8EFEAF0333C5C2FC5E2FD1EAF5EE5BB" box="[689,793,644,666]" italics="true" pageId="6" pageNumber="983">M. badius</emphasis>
</taxonomicName>
