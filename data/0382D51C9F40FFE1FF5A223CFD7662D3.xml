<caption id="DF5434829F43FFE0FF5A2151FDC766D6" httpUri="https://zenodo.org/record/280888/files/figure.png" pageId="30" pageNumber="49" targetBox="[153,1436,193,1780]" targetPageId="30">
<paragraph id="8B94640A9F43FFE0FF5A2151FDC766D6" blockId="30.[151,1437,1801,2026]" pageId="30" pageNumber="49">
