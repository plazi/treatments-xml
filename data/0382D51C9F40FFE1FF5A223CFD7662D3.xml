<taxonomicName id="4C2B1F899F43FFE0FBF721D7FE8766FA" authority="El Beshbeeshy, 2011" authorityName="El Beshbeeshy" authorityYear="2011" class="Hydrozoa" family="Sertulariidae" genus="Symplectoscyphus" kingdom="Animalia" order="Leptothecata" pageId="30" pageNumber="49" phylum="Cnidaria" rank="species" species="paraglacialis">
