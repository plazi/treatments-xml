<collectingRegion id="E40CA6E7FFDAFFBDFCBC4E90FCD3FA03" box="[785,862,1474,1500]" country="Greece" name="Ipeiros" pageId="123" pageNumber="124">Epirus</collectingRegion>
and only above 
<quantity id="E130C5E0FFDAFFBDFB8D4E90FBF4FA03" box="[1056,1145,1474,1500]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="123" pageNumber="124" unit="m" value="1200.0">1200 m</quantity>
in 
<collectingRegion id="E40CA6E7FFDAFFBDFB334E90FA88FA03" box="[1182,1285,1474,1500]" country="Italy" name="Calabria" pageId="123" pageNumber="124">Calabria</collectingRegion>
