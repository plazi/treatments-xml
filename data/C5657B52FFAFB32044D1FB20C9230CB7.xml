<specimenCount id="5BCA01CDFFAFB3204791F93DCC110C27" box="[983,1047,1780,1804]" pageId="51" pageNumber="52" type="generic">
<quantity id="8A3467A1FFAFB3204791F93DCC150C27" box="[983,1043,1780,1804]" metricMagnitude="1" metricUnit="m" metricValue="5.5" pageId="51" pageNumber="52" unit="m" value="55.0">55 m</quantity>
.
</specimenCount>
</paragraph>
