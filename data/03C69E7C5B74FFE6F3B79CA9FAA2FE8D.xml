</taxonomicName>
; 19–20 
<taxonomicName id="4C6F54E95B71FFE7F16D9CF6FD14F94C" baseAuthorityName="Scopoli" baseAuthorityYear="1772" box="[589,698,1735,1757]" class="Insecta" family="Scutelleridae" genus="Pachycoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="448" phylum="Arthropoda" rank="species" species="torridus">
<emphasis id="B91BF3785B71FFE7F16D9CF6FDF7F94C" box="[589,601,1736,1757]" italics="true" pageId="4" pageNumber="448">P</emphasis>
