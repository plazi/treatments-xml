<paragraph id="F76A36E9FB39FFE3FF14EEE6625BFF6C" blockId="11.[151,516,151,210]" box="[151,516,151,177]" pageId="11" pageNumber="12">
<heading id="AC228185FB39FFE3FF14EEE6625BFF6C" bold="true" box="[151,516,151,177]" fontSize="11" level="1" pageId="11" pageNumber="12" reason="1">
<emphasis id="C5A1EAFBFB39FFE3FF14EEE6625BFF6C" bold="true" box="[151,516,151,177]" pageId="11" pageNumber="12">
<taxonomicName id="30D54D6AFB39FFE3FF14EEE661E4FF6C" box="[151,443,151,177]" class="Insecta" family="Coccinellidae" genus="Sticholotis" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="magnostriata" status="sp. n.">
<emphasis id="C5A1EAFBFB39FFE3FF14EEE661E4FF6C" bold="true" box="[151,443,151,177]" italics="true" pageId="11" pageNumber="12">Sticholotis magnostriata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="DE925780FB39FFE3FE41EEE6625BFF6C" box="[450,516,151,177]" pageId="11" pageNumber="12" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
