<taxonomicName id="4C46D9E9FFEFFFA95E330954FC5CFD30" authority="STAUDINGER, 1887" authorityName="STAUDINGER" authorityYear="1887" box="[368,943,1079,1101]" class="Insecta" family="Zygaenidae" genus="Zygaena" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="181" phylum="Arthropoda" rank="subSpecies" species="carniolica" subSpecies="albarracina">
