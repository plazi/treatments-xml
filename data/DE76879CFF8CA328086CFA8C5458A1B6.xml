<taxonomicName id="91DF4D09FF8BA32A0ABBF96C549EA5DB" box="[396,501,1686,1706]" class="Mammalia" family="Muridae" genus="Echiothrix" kingdom="Animalia" order="Rodentia" pageId="12" pageNumber="12" phylum="Chordata" rank="species" species="leucura">
