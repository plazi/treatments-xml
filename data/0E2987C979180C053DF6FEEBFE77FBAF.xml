<number id="304824F879180C053838FDA2FA30FDC2" box="[1369,1431,528,554]" pageId="40" pageNumber="41" value="1998.0">1998</number>
</date>
, pitfall trap (
<accessionNumber id="99D3AB3C79180C053C40FD84FE00FDB8" box="[289,423,566,592]" httpUri="http://www.ncbi.nlm.nih.gov/protein/HUJ15512" pageId="40" pageNumber="41">
<collectionCode id="E091AE1A79180C053C40FD84FEFCFDB8" LSID="urn:lsid:biocol.org:col:15399" box="[289,347,566,592]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15399" name="Hebrew University" pageId="40" pageNumber="41">HUJ</collectionCode>
<number id="304824F879180C053C3AFD84FE00FDB8" box="[347,423,566,592]" pageId="40" pageNumber="41" value="15512.0">15512</number>
</accessionNumber>
