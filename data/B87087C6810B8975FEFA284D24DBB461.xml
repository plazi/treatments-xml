<paragraph id="306636D0810B8975FECA2F3E24EEB2F2" blockId="11.[264,1324,1607,1873]" lastBlockId="12.[264,1322,284,470]" lastPageId="12" lastPageNumber="43" pageId="11" pageNumber="42">
Female total length 
<number id="861124F7810B8972FDD12F3E241DB5D5" box="[547,602,1647,1673]" pageId="11" pageNumber="42" value="8.91">8.91</number>
. Carapace 
<number id="861124F7810B8972FD292F3E2549B5D5" box="[731,782,1647,1673]" pageId="11" pageNumber="42" value="2.79">2.79</number>
long, 
<number id="861124F7810B8972FCA82F3E25CAB5D5" box="[858,909,1647,1673]" pageId="11" pageNumber="42" value="4.27">4.27</number>
wide; opisthosoma 
<number id="861124F7810B8972FB882F3E22E9B5D5" box="[1146,1198,1647,1673]" pageId="11" pageNumber="42" value="6.12">6.12</number>
