<taxonomicName id="4C3C949BFF9AFFA4FE3FF9727873F973" box="[459,623,1707,1732]" class="Insecta" family="Cicadidae" genus="Oncotympana" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="19" phylum="Arthropoda" rank="genus">
