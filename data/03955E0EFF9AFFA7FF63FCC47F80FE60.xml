<taxonomicName id="4C3C949BFF9AFFA4FBCBF9727F1CF973" box="[1087,1280,1706,1732]" class="Insecta" family="Cicadidae" genus="Pomponia" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="19" phylum="Arthropoda" rank="species" species="pallidiventris">
<emphasis id="B948330AFF9AFFA4FBCBF9727E48F973" box="[1087,1108,1707,1732]" italics="true" pageId="5" pageNumber="19">O</emphasis>
