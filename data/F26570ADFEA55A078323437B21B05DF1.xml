<taxonomicName id="9EECF9FF6AEEFF41D321B3254BDD802E" LSID="http://zoobank.org/D5C3928A-818E-493E-A423-5D6C43BB8582" authority="Sheng &amp; Sun" authorityName="Sheng &amp; Sun" authorityYear="2021" class="Insecta" family="Ichneumonidae" genus="Anisotacrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anisotacrus externus" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="externus" status="sp. nov.">Anisotacrus externus Sheng &amp; Sun</taxonomicName>
