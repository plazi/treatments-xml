<taxonomicName id="4C5489D5FF2218C0FE1F09AEFD3FF964" authorityName="Sack" authorityYear="1941" box="[431,655,1688,1713]" class="Insecta" family="Syrphidae" genus="Toxomerus" kingdom="Animalia" order="Diptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="species" species="incaicus">
