<emphasis id="B9465424286C774CFF35F8F1FDA5D0A5" bold="true" box="[183,550,1900,1921]" pageId="3" pageNumber="202">
<taxonomicName id="4C32F3B5286C774CFF35F8F1FE44D0A5" authority="Gordon &amp; Mawatari &amp; Kajihara, 2002" authorityName="Gordon &amp; Mawatari &amp; Kajihara" authorityYear="2002" box="[183,455,1900,1921]" class="Gymnolaemata" family="Eurystomellidae" genus="Eurystomella" kingdom="Animalia" order="Cheilostomatida" pageId="3" pageNumber="202" phylum="Bryozoa" rank="species" species="aupouria" status="sp. nov.">
<emphasis id="B9465424286C774CFF35F8F1FE44D0A5" bold="true" box="[183,455,1900,1921]" italics="true" pageId="3" pageNumber="202">Eurystomella aupouria</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A275E95F286C774CFE4CF8F1FDA5D0A5" box="[462,550,1900,1921]" pageId="3" pageNumber="202" rank="species">sp. nov.</taxonomicNameLabel>
