<taxonomicName id="4C5005E9FFB4DB7AFEC3F8BBFE2CFB27" box="[277,408,1876,1897]" class="Insecta" family="Coccinellidae" genus="Mada" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="444" phylum="Arthropoda" rank="species" species="hirsuta" status="sp. nov.">
<emphasis id="B924A278FFB4DB7AFEC3F8BBFE2CFB27" box="[277,408,1876,1897]" italics="true" pageId="2" pageNumber="444">Mada hirsuta</emphasis>
</taxonomicName>
