<taxonomicName id="4C674D72B329927E30DAFA29F050FA4D" authority="Krober, 1915b: 57" authorityName="Krober" authorityPageNumber="57" authorityYear="1915" box="[151,590,1477,1502]" class="Insecta" family="Conopidae" genus="Conops" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="523" phylum="Arthropoda" rank="species" species="longiventris">
