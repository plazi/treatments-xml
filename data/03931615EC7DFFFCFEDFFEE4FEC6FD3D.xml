of 
<taxonomicName id="4C3ADC80EC7DFFFCFE11FE21FDCFFE5C" box="[454,605,473,498]" class="Malacostraca" family="Phronimidae" genus="Phronima" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="bucephala">
