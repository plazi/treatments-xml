<paragraph id="9A278E55FF876A16F708FC5DFEB6FBC1" blockId="46.[90,620,977,1090]" pageId="46" pageNumber="46">
<treatmentCitationGroup id="BA88A97BFF876A16F708FC5DFEB6FBC1" pageId="46" pageNumber="46">
<treatmentCitation id="1B39A844FF876A16F708FC5DFF4DFB85" author="Markham, J. C." page="285" pageId="46" pageNumber="46" year="1992">
<taxonomicName id="5D98F5D6FF876A16F708FC5DFF4DFB85" authority="Markham, 1992 a: 283 - 285" authorityName="Markham" authorityPageNumber="283 - 285" authorityYear="1992" class="Malacostraca" family="Bopyridae" genus="Probopyria" kingdom="Animalia" order="Isopoda" pageId="46" pageNumber="46" phylum="Arthropoda" rank="species" species="elliptica">
<emphasis id="A8EC5247FF876A16F708FC5DFEAFFC6B" box="[90,322,977,1000]" italics="true" pageId="46" pageNumber="46">Probopyria elliptica</emphasis>
<bibRefCitation id="FE09F3A4FF876A16F602FC5EFF4DFB85" author="Markham, J. C." pageId="46" pageNumber="46" pagination="277 - 302" refId="ref54901" refString="Markham, J. C. 1992 a. Second list of additions to the Isopoda Bopyridae of Hong Kong. In B. Morton (editor), The marine flora and fauna of Hong Kong and southern China III. Proceedings of the Fourth International Marine Biological Workshop: the marine flora and fauna of Hong Kong and southern China, Hong Kong, 11 - 29 April 1989: 277 - 302. Hong Kong: Hong Kong University Press." type="book chapter" year="1992">Markham, 1992a: 283– 285</bibRefCitation>
</taxonomicName>
