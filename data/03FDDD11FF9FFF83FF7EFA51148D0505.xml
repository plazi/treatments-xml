<number id="3D9C7E20FF98FF82FE6CFCCB16CA04A4" box="[389,404,891,917]" pageId="36" pageNumber="37" value="3.0">3</number>
, cua 
<number id="3D9C7E20FF98FF82FE24FCCB168204A4" box="[461,476,891,917]" pageId="36" pageNumber="37" value="1.0">1</number>
