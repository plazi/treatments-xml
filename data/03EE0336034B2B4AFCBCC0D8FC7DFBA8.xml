<taxonomicName id="4C47C9A3034B2B4AFC32C1D6FB3EFC2B" box="[950,1176,930,953]" class="Liliopsida" family="Cyperaceae" genus="Cyperus" kingdom="Plantae" order="Poales" pageId="2" pageNumber="147" phylum="Tracheophyta" rank="species" species="microcristatus">Cyperus microcristatus</taxonomicName>
