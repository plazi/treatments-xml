</taxonomicName>
from 
<taxonomicName id="2C87B5D96F79FF9FFE50006EFD32FCB8" authority="barbatula" authorityName="barbatula (Linnaeus" authorityYear="1758" box="[498,643,774,789]" class="Actinopterygii" family="Nemacheilidae" genus="Barbatula" kingdom="Animalia" order="Cypriniformes" pageId="4" pageNumber="875" phylum="Chordata" rank="species" species="barbatula">
