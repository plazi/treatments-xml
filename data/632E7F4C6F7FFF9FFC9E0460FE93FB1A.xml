<taxonomicName id="2C87B5D96F79FF9FFF06006EFE74FCB8" authority="(Grimm, 1872)" authorityName="La Rue" authorityYear="1911" baseAuthorityName="Grimm" baseAuthorityYear="1872" box="[164,453,774,789]" class="Cestoda" family="Proteocephalidae" genus="Proteocephalus" kingdom="Animalia" order="Proteocephalidea" pageId="4" pageNumber="875" phylum="Platyhelminthes" rank="species" species="sagittus">
<emphasis id="D9F312486F79FF9FFF06006EFEFCFCB8" box="[164,333,774,789]" italics="true" pageId="4" pageNumber="875">Proteocephalus sagittus</emphasis>
