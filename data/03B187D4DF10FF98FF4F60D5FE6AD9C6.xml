<taxonomicName id="4C184D41DF10FF99FEC266E1FE05DFD4" box="[282,475,1709,1733]" class="Actinopterygii" family="Cyprinodontidae" genus="Aphanius" kingdom="Animalia" order="Cyprinodontiformes" pageId="12" pageNumber="433" phylum="Chordata" rank="species" species="fasciatus">
