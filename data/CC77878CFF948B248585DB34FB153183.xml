<taxonomicName id="83DE4D19FF938B24849EDEDFFD393183" authority=", Leontodon" authorityName="Leontodon" box="[396,652,224,248]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="12" pageNumber="213" phylum="Magnoliopsida" rank="genus">Euphorbia, Leontodon</taxonomicName>
