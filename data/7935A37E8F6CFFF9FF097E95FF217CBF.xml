. 
<collectingCountry id="898B52F88F6CFFF9FEF97BEFFBDE7FBB" box="[359,426,1446,1471]" name="China" pageId="1" pageNumber="181">China</collectingCountry>
(
<collectingRegion id="3358DC8A8F6CFFF9FE277BEFF8F17FBB" box="[441,645,1446,1471]" country="China" name="Shaanxi" pageId="1" pageNumber="181">Shaanxi Province</collectingRegion>
