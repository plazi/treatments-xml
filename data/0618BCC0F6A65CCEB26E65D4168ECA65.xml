<paragraph id="8F6058BBFB2532ED7A1DA85353D1DE9F" pageId="0" pageNumber="43">
<taxonomicName id="F24235B5A6DA1032845AE6978B697081" baseAuthorityName="Liskiewicz" baseAuthorityYear="1925" class="Clitellata" family="Erpobdellidae" genus="Erpobdella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erpobdella vilnensis" order="Arhynchobdellida" pageId="0" pageNumber="43" phylum="Annelida" rank="species" species="vilnensis">Erpobdella vilnensis</taxonomicName>
