<taxonomicName id="4C207C9BFFEAA800FEBDFA4FE679FA3E" authority="Liljeborg 1855" authorityName="Liljeborg" authorityYear="1855" box="[264,623,1446,1472]" class="Malacostraca" family="Isaeidae" genus="Gammaropsis" kingdom="Animalia" order="Amphipoda" pageId="32" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="B954DB0AFFEAA800FEBDFA4FE679FA3E" bold="true" box="[264,623,1446,1472]" pageId="32" pageNumber="33">
<emphasis id="B954DB0AFFEAA800FEBDFA4FE5A6FA3E" bold="true" box="[264,432,1446,1472]" italics="true" pageId="32" pageNumber="33">Gammaropsis</emphasis>
<bibRefCitation id="EFB17AE9FFEAA800FE02FA4FE679FA3E" author="Liljeborg" box="[439,623,1446,1472]" pageId="32" pageNumber="50" refString="Liljeborg, W. (1855) Ofversigt af de inom Skandinavien hittills arterna af slagtet Gammarus Fabr. Konliga Svenska Vetenskaps Akademiens Handlingar, 12, 443 - 460." type="journal article" year="1855">
Liljeborg 
<number id="3DE8153FFFEAA800FD81FA4FE679FA3E" box="[564,623,1446,1472]" pageId="32" pageNumber="33" value="1855.0">1855</number>
</bibRefCitation>
</emphasis>
