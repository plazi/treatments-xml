<quantity id="4CC6B59C2F63FFD90660FDB9E4E32C7F" box="[1087,1257,520,547]" metricMagnitude="3" metricUnit="m" metricValue="1.35" metricValueMax="1.5" metricValueMin="1.2" pageId="14" pageNumber="15" unit="m" value="1350.0" valueMax="1500.0" valueMin="1200.0">1200–1500 m</quantity>
