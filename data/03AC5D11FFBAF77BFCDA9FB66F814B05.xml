<taxonomicName id="4C059784FFA5F77BFD869B6E6FC8492A" baseAuthorityName="Gravenhorst" baseAuthorityYear="1829" box="[522,711,273,298]" class="Insecta" family="Ichneumonidae" genus="Erigorgus" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="species" species="villosus">Erigorgus villosus</taxonomicName>
