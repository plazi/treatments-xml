<taxonomicName id="1CB1FAD2FF7D64581ACFF9D2C4BEFF96" authority="Ashmead, 1904" authorityName="Ashmead" authorityYear="1904" box="[145,419,1652,1677]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="atemiensis">
<emphasis id="E9C55D43FF7D64581ACFF9D2C415FF97" box="[145,264,1653,1676]" italics="true" pageId="2" pageNumber="33">X. atemiensis</emphasis>
Ashmead, 1904
</taxonomicName>
and 
<taxonomicName id="1CB1FAD2FF7D64581B8EF9D2C7E5FF96" authority="Mata-Casanova, 2013" authorityName="Mata-Casanova" authorityYear="2013" box="[464,760,1652,1677]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="pilosa">
<emphasis id="E9C55D43FF7D64581B8EF9D2C700FF97" box="[464,541,1652,1676]" italics="true" pageId="2" pageNumber="33">X. pilosa</emphasis>
Mata-Casanova, 2013
</taxonomicName>
