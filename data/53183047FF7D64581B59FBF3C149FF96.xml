<taxonomicName id="1CB1FAD2FF7D64581BABF90DC773FFDA" authorityName="Ashmead" authorityYear="1904" box="[501,622,1706,1729]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="atemiensis">
<emphasis id="E9C55D43FF7D64581BABF90DC773FFDA" box="[501,622,1706,1729]" italics="true" pageId="2" pageNumber="33">X. atemiensis</emphasis>
</taxonomicName>
and 
<taxonomicName id="1CB1FAD2FF7D645818C0F90DC7F0FFDA" authorityName="Mata-Casanova" authorityYear="2013" box="[670,749,1705,1729]" class="Insecta" family="Figitidae" genus="Xyalaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="pilosa">
<emphasis id="E9C55D43FF7D645818C0F90DC7F0FFDA" box="[670,749,1705,1729]" italics="true" pageId="2" pageNumber="33">X. pilosa</emphasis>
