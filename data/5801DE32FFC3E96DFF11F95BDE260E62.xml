<quantity id="1750C2C1FFC3E961FED5F8ACDC900AA1" box="[339,496,1898,1922]" metricMagnitude="3" metricUnit="m" metricValue="1.4" metricValueMax="1.8" metricValueMin="1.0" pageId="23" pageNumber="532" unit="m" value="1400.0" valueMax="1800.0" valueMin="1000.0">1000–1800 m</quantity>
