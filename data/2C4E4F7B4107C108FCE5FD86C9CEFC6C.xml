<taxonomicName id="63E785EE4107C108FC12FC5AC946FEE5" authority="(Rathbun, 1909)" baseAuthorityName="Rathbun" baseAuthorityYear="1909" box="[918,1307,284,306]" class="Malacostraca" family="Macrophthalmidae" genus="Ilyograpsus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="365" phylum="Arthropoda" rank="species" species="paludicola">
