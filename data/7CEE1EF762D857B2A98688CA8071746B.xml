<taxonomicName id="26DA34E60BA94AF0394CEEDDDCB48570" class="Arachnida" family="Ochyroceratidae" genus="Ochyrocera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ochyrocera cruciata" order="Araneae" pageId="0" pageNumber="41" phylum="Arthropoda" rank="species" species="cruciata">
