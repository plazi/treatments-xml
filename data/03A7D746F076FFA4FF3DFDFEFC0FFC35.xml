<taxonomicName id="4C0E1DD3F076FFA4FC4BFC9AFA23FC11" authority="Handmann, 1882" authorityName="Handmann" authorityYear="1882" box="[993,1432,882,907]" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" pageId="4" pageNumber="457" phylum="Mollusca" rank="species" species="inermis">
