<bibRefCitation id="EF9F1BA1F076FFA4FB65FC9AFA23FC11" author="Handmann, R." box="[1231,1432,882,907]" pageId="4" pageNumber="457" refId="ref9738" refString="Handmann, R. (1882) Die Neogenablagerungen des osterreichisch-ungarischen Tertiarbeckens. Aschendorff, Munster, 71 pp." type="book" year="1882">Handmann, 1882</bibRefCitation>
</taxonomicName>
, making 
<taxonomicName id="4C0E1DD3F076FFA4FF58FC7EFD68FC34" authority="Brusina, 1897 a" authorityName="Brusina" authorityYear="1897" box="[242,723,918,943]" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" pageId="4" pageNumber="457" phylum="Mollusca" rank="subSpecies" species="klerici" subSpecies="inermis">
