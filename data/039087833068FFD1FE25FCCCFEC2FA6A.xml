(
<figureCitation id="13022A103068FFCCFD96FC93FD56FA61" box="[555,675,1505,1531]" captionStart="Figure 2" captionStartId="5.[160,225,1440,1463]" captionTargetBox="[162,1154,142,1408]" captionTargetId="figure-62@5.[162,1154,142,1408]" captionTargetPageId="5" captionText="Figure 2. Habitus of examined specimens: (a) Pseudocrangonyx spangsbergi sp. nov., holotype, female, 5.5 mm; (b) Pseudocrangonyx setosus sp. nov., holotype, female, 5.5 mm; (c) Pseudocrangonyx myaochanicus sp. nov., holotype, female, 3.2 mm; (d) Pseudocrangonyx borealis sp. nov., holotype, female, 4.5 mm; (e) Pseudocrangonyx camtschaticus Birstein, 1955, female, 7.5 mm." figureDoi="http://doi.org/10.5281/zenodo.5030645" httpUri="https://zenodo.org/record/5030645/files/figure.png" pageId="14" pageNumber="1772">Figures 2c</figureCitation>
, 
<figureCitation id="13022A103068FFCCFD0CFC93FD0CFA61" box="[689,761,1505,1531]" captionStart-0="Figure 11" captionStart-1="Figure 12" captionStart-2="Figure 13" captionStartId-0="16.[160,225,1559,1582]" captionStartId-1="18.[160,225,1641,1664]" captionStartId-2="19.[160,225,998,1021]" captionTargetBox-0="[162,1154,142,1528]" captionTargetBox-1="[162,1154,142,1610]" captionTargetBox-2="[162,1154,142,966]" captionTargetId-0="figure-9@16.[162,1154,142,1528]" captionTargetId-1="figure-9@18.[162,1154,142,1610]" captionTargetId-2="figure-176@19.[162,1154,142,966]" captionTargetPageId-0="16" captionTargetPageId-1="18" captionTargetPageId-2="19" captionText-0="Figure 11. Pseudocrangonyx myaochanicus sp. nov., holotype MNHN-IU-2019-3172, female, 3.2 mm: (a) antenna I; (b) antenna II; (c) upper lip; (d) lower lip; (e) maxilla I; (f) outer plate of maxilla I; (g) left mandible; (h) right mandible; (i) maxilla II; (j) maxilliped. Scale bars: 0.2 mm." captionText-1="Figure 12. Pseudocrangonyx myaochanicus sp. nov., holotype MNHN-IU-2019-3172, female, 3.2 mm: (a) gnathopod I; (b) gnathopod II; (c) pereopod III; (d) pereopod IV; (e) pereopod V; (f) pereopod VI; (g) pereopod VII. Scale bar: 0.2 mm." captionText-2="Figure 13. Pseudocrangonyx myaochanicus sp. nov., holotype MNHN-IU-2019-3172, female, 3.2 mm: (a) uropod I; (b) uropod II; (c) uropod III; (d) telson; (e) pleopod I; (f) pleopod II; (g) pleopod III; (h) epimera I–III. Scale bars: 0.2 mm." figureDoi-0="http://doi.org/10.5281/zenodo.5030681" figureDoi-1="http://doi.org/10.5281/zenodo.5030685" figureDoi-2="http://doi.org/10.5281/zenodo.5030691" httpUri-0="https://zenodo.org/record/5030681/files/figure.png" httpUri-1="https://zenodo.org/record/5030685/files/figure.png" httpUri-2="https://zenodo.org/record/5030691/files/figure.png" pageId="14" pageNumber="1772">11–13</figureCitation>
