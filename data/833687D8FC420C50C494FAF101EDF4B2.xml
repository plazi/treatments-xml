<typeStatus id="D424886CFC420C52C494FA3900AFF1D2" box="[151,200,1439,1460]" pageId="9" pageNumber="10">Type</typeStatus>
species: 
<taxonomicName id="CC9F4D4DFC420C52C525FA390378F1D2" authority="Obenberger, 1922" authorityName="Obenberger" authorityYear="1922" box="[294,799,1438,1461]" class="Insecta" family="Buprestidae" genus="Tetragonoschema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="patagonicum">
