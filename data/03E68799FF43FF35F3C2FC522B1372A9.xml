<taxonomicName id="4C4F4D0CFF43FF35F21DF9CB2C257271" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,557,1590,1614]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="gagnei">
