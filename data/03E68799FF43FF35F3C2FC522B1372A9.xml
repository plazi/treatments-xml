<taxonomicName id="4C4F4D0CFF43FF35F082F9CB2A4E7271" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[983,1094,1590,1614]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="crispa" status="sp. nov.">
