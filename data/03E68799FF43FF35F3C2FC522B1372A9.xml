<taxonomicName id="4C4F4D0CFF43FF35F13AF9A62CD1724C" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[623,729,1627,1651]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="species" species="doddi" status="sp. nov.">
