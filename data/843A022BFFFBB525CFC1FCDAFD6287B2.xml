<key id="7802E40CFFFAB525CF93FAA6FD6287B2" lastPageId="2" lastPageNumber="107" pageId="1" pageNumber="106">
<keyStep id="B767AB98FFFAB526CF93FAA6FB658772" pageId="1" pageNumber="106">
<paragraph id="0C2CB33DFFFAB526CF93FAA6FB658732" blockId="1.[685,1172,1283,1304]" box="[685,1172,1283,1304]" pageId="1" pageNumber="106">
<keyLead id="B7621008FFFAB526CF93FAA6FB658732" box="[685,1172,1283,1304]" pageId="1" pageNumber="106">1. Elytra smooth, without impressed striae ......2</keyLead>
</paragraph>
