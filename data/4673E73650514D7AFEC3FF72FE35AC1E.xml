<collectingCounty id="27042EAC50534D7AFE4FFDB2FDCEAC3E" box="[399,525,513,536]" pageId="7" pageNumber="42">Altogether</collectingCounty>
<specimenCount id="D8DC9DA950534D7AFDD9FDB2FD6FAC3E" box="[537,684,513,536]" pageId="7" pageNumber="42" type="female">158 females</specimenCount>
