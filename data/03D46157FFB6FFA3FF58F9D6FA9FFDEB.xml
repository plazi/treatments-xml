<collectingCountry id="F36A90D1FFB4FFA3FEA8FE43FE29FE07" box="[359,446,477,503]" name="Uganda" pageId="14" pageNumber="465">Uganda</collectingCountry>
(
<collectingRegion id="49B91EA3FFB4FFA3FE02FE43FDCAFE08" box="[461,605,477,504]" country="Uganda" name="Mpigi" pageId="14" pageNumber="465">Mpigi Distr.</collectingRegion>
