<taxonomicName id="4C274D5D9D19FF9EFEDEC9406F17F981" authority="(Myers, 1985)" baseAuthorityName="Myers" baseAuthorityYear="1985" box="[291,619,1548,1571]" class="Malacostraca" family="Ampithoidae" genus="Ampithoe" kingdom="Animalia" order="Amphipoda" pageId="10" pageNumber="163" phylum="Arthropoda" rank="species" species="kava">
<emphasis id="B953EACC9D19FF9EFEDEC9406CB9F981" box="[291,453,1548,1571]" italics="true" pageId="10" pageNumber="163">Ampithoe kava</emphasis>
