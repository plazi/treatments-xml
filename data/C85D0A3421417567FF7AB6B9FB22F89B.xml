<taxonomicName id="87F4C0A121417567FE03B36CFD92FAFB" box="[494,588,1346,1372]" class="Insecta" family="Leptoceridae" genus="Triplectides" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="minutus">minutus</taxonomicName>
