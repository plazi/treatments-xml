<paragraph id="AD732749A63C001AFF4AA4EDFC92FBBB" blockId="3.[151,1436,1021,2028]" box="[151,772,1021,1047]" pageId="3" pageNumber="36">
<emphasis id="9FB8FB5BA63C001AFF4AA4EDFD34FBBA" box="[151,674,1021,1046]" italics="true" pageId="3" pageNumber="36">
<typeStatus id="727799EBA63C001AFF4AA4EDFF58FBBA" box="[151,206,1021,1046]" pageId="3" pageNumber="36">Type</typeStatus>
