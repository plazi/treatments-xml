<subSubSection id="C32FF4D3FFC1FFCDDC96F9E3FDC27F17" box="[164,600,1268,1291]" pageId="64" pageNumber="65" type="nomenclature">
<paragraph id="8B8AA758FFC1FFCDDC96F9E3FDC27F17" blockId="64.[164,600,1268,1291]" box="[164,600,1268,1291]" pageId="64" pageNumber="65">
<heading id="D0C21034FFC1FFCDDC96F9E3FDC27F17" box="[164,600,1268,1291]" centered="true" fontSize="9" level="2" pageId="64" pageNumber="65" reason="2">
<taxonomicName id="4C35DCDBFFC1FFCDDC96F9E3FE5B7F16" box="[164,449,1268,1291]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="carinatus" status="new species">
<emphasis id="B9417B4AFFC1FFCDDC96F9E3FE5B7F16" bold="true" box="[164,449,1268,1291]" italics="true" pageId="64" pageNumber="65">Paramblynotus carinatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFC1FFCDDDE3F9E2FDC27F17" box="[465,600,1269,1291]" pageId="64" pageNumber="65" rank="species">new species</taxonomicNameLabel>
