<quantity id="4CEF9B25FF8FFFAB9B09FB01F9EC3508" box="[884,941,1267,1293]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="30" pageNumber="31" unit="m" value="10.0">10 m</quantity>
