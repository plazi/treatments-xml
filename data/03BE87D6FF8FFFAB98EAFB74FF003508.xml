<geoCoordinate id="EE235007FF8FFFAB9D62FB74FFD934A4" box="[1311,1432,1158,1185]" degrees="23" direction="north" minutes="56.8" orientation="latitude" pageId="30" pageNumber="31" precision="92" value="23.946667">23°56.8’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FF8FFFAB98BBFB58FB1334C0" box="[198,338,1194,1221]" degrees="110" direction="west" minutes="51.1" orientation="longitude" pageId="30" pageNumber="31" precision="92" value="-110.85167">110°51.1’W</geoCoordinate>
