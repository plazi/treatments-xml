<collectingCountry id="F364CEF8FFECFF940FF5B11BFC63C57F" box="[948,1012,527,549]" name="Brazil" pageId="37" pageNumber="624">Brazil</collectingCountry>
, 
<collectingCountry id="F364CEF8FFECFF940843B11BFF41C518" name="Ecuador" pageId="37" pageNumber="624">Ecuador</collectingCountry>
, 
