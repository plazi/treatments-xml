<subSubSection id="C3416563FFA1FFB3FE8AFEEAFDB76349" pageId="5" pageNumber="6" type="etymology">
<paragraph id="8BE436E8FFA1FFB3FE8AFEEAFDB76349" blockId="5.[264,1325,283,950]" pageId="5" pageNumber="6">
<emphasis id="B92FEAFAFFA1FFB3FE8AFEEAFE716321" bold="true" box="[312,456,283,309]" pageId="5" pageNumber="6">Etymology:</emphasis>
