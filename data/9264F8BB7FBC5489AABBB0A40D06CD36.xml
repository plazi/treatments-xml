<taxonomicName id="FDA1A88202A919167A0DB56B6D6F0F9B" authorityName="Yang &amp; Yang" authorityYear="1987" class="Insecta" family="Tipulidae" genus="Nephrotoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nephrotoma claviformis" order="Diptera" pageId="0" pageNumber="123" phylum="Arthropoda" rank="species" species="claviformis">Nephrotoma claviformis</taxonomicName>
