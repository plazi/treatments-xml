<taxonomicName id="4C2BDCE7FFD01D4A5405F891F84EF80C" authorityName="Brancucci" authorityYear="1981" box="[1032,1331,1914,1939]" class="Insecta" family="Cantharidae" genus="Macromalthinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="belemensis">
