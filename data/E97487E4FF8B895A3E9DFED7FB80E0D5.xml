<collectingCounty id="88034E7EFF8B895A3AB4FE46FA8AE4BB" box="[1214,1338,438,463]" pageId="20" pageNumber="491">Depository</collectingCounty>
<collectionCode id="07CCAE37FF8B895A3B48FE46FACCE4BB" box="[1346,1404,438,463]" country="Germany" httpUri="http://grbio.org/cool/jp8y-i5nv" name="Museum f�r Naturkunde" pageId="20" pageNumber="491">MfN</collectionCode>
. The 
<typeStatus id="BE668850FF8B895A3EC3FE2AFE9BE487" box="[201,299,474,499]" pageId="20" pageNumber="491">holotype</typeStatus>
