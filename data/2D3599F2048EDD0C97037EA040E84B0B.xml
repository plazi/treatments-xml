<taxonomicName id="D9CB911F194724B6CD9352F09368345C" class="Magnoliopsida" family="Caryophyllaceae" genus="Cerastium" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="809" phylum="Tracheophyta" rank="species" species="semidecandrum">
