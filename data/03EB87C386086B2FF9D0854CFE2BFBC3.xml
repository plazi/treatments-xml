<quantity id="4CBA9B3086096B2FFD9B8E41FD2DFDC6" box="[579,695,565,605]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="27" pageNumber="90" unit="km" value="1.8">1.8 km</quantity>
