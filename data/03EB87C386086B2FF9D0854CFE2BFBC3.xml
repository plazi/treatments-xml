<quantity id="4CBA9B3086096B2FFD7A8E10FC8EFD11" box="[674,788,612,650]" metricMagnitude="4" metricUnit="m" metricValue="1.3" pageId="27" pageNumber="90" unit="km" value="13.0">13 km</quantity>
