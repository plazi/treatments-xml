<quantity id="4CC69B3EFF83CD3AFC194540FBBB4D51" box="[963,1056,1852,1878]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="13" pageNumber="14" unit="mm" value="1.7">1.7 mm</quantity>
