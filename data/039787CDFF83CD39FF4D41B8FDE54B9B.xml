<quantity id="4CC69B3EFF83CD3AFB844540FB204D51" box="[1118,1211,1852,1878]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="13" pageNumber="14" unit="mm" value="1.8">1.8 mm</quantity>
