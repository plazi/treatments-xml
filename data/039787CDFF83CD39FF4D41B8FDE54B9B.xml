<quantity id="4CC69B3EFF83CD3AFCC14540FCE34D51" box="[795,888,1852,1878]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="13" pageNumber="14" unit="mm" value="1.7">1.7 mm</quantity>
