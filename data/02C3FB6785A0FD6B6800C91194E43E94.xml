<taxonomicName id="D4AA6A409973DD0CE5992E026263AF3F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:236917" class="Insecta" family="Formicidae" genus="Axinidris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Axinidris gabonica" order="Hymenoptera" pageNumber="558" phylum="Arthropoda" rank="species" species="gabonica">Axinidris gabonica</taxonomicName>
