<taxonomicName id="4C631BDD6F48E061FE80F88AFDE4F8B1" box="[292,536,1879,1901]" class="Arachnida" family="Trombiculidae" genus="Hypogastia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="15" pageNumber="498" phylum="Arthropoda" rank="species" species="stekolnikovi" status="sp. nov.">
