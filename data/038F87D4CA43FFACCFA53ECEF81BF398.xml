<subSubSection id="C33C6549CA43FFACC2423FB4F6ADFBB1" pageId="19" pageNumber="638" type="distribution">
<caption id="DF59664ACA43FFACC2423FB4F6ADFBB1" ID-DOI="http://doi.org/10.5281/zenodo.5714205" ID-Zenodo-Dep="5714205" httpUri="https://zenodo.org/record/5714205/files/figure.png" inLine="true" pageId="19" pageNumber="638" targetBox="[1471,2061,851,1264]" targetPageId="19">
<paragraph id="8B9936C2CA43FFACC2423FB4F6ADFBB1" blockId="19.[2085,2675,838,1264]" pageId="19" pageNumber="638">
<emphasis id="B952EAD0CA43FFACC2423FB4F750FC41" box="[2087,2262,956,989]" pageId="19" pageNumber="638">Distribution.</emphasis>
