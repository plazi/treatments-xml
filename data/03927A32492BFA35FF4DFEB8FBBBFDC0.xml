<taxonomicName id="4C3BB0A7492BFA35FF11FCF5FE02FC20" baseAuthorityName="Gruvel" baseAuthorityYear="1901" box="[151,397,872,898]" class="Maxillopoda" family="Heteralepadidae" genus="Heteralepas" kingdom="Animalia" order="Pedunculata" pageId="6" pageNumber="711" phylum="Arthropoda" rank="species" species="microstoma">
