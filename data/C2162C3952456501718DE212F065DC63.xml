<taxonomicName id="6ADD9CE4289E3BD5B1477B114DD25DEF" class="Insecta" family="Liviidae" genus="Psyllopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psyllopsis discrepans" order="Hemiptera" pageId="10" pageNumber="179" phylum="Arthropoda" rank="species" species="discrepans">Psyllopsis discrepans</taxonomicName>
