(
<taxonomicName id="20884D6EFFCDFFBC2B3E9B07FCF5FE82" authority="Lane, 1936" authorityName="Lane" authorityYear="1936" box="[555,890,261,287]" pageId="10" pageNumber="45" rank="variety" variety="leucotarsis">
var. 
<emphasis id="D5FCEAFFFFCDFFBC2B779B07FD6CFE83" box="[610,739,261,286]" italics="true" pageId="10" pageNumber="45">leucotarsis</emphasis>
<bibRefCitation id="83194B1CFFCDFFBC2BFB9B07FCF5FE82" author="Lane" box="[750,890,261,287]" pageId="10" pageNumber="47" refString="Lane, J. (1936) Notas sobre culicideos de Matto Grosso. Revista de Museu Paulista da Unversidade de Sao Paulo, 20, 173 - 206, 4 pls." type="journal article" year="1936">
Lane, 
<number id="514024CAFFCDFFBC2A2E9B07FCF5FE82" box="[827,890,261,287]" pageId="10" pageNumber="45" value="1936.0">1936</number>
</bibRefCitation>
</taxonomicName>
