<location id="E515606DFF06FF6058F3F99EFE826757" LSID="urn:lsid:plazi:treatment:686387A0FF06FF605889FA2BFD296637:E515606DFF06FF6058F3F99EFE826757" box="[237,305,1555,1582]" county="Napo River" municipality="America" name="South" pageId="203" pageNumber="204">South</location>
