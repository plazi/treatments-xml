<location id="E515606DFF06FF605C4DFA62FB0D6771" LSID="urn:lsid:plazi:treatment:686387A0FF06FF605889FA2BFD296637:E515606DFF06FF605C4DFA62FB0D6771" box="[1107,1214,1519,1544]" county="Napo River" municipality="America" name="Rio Napo" pageId="203" pageNumber="204">Rio Napo</location>
