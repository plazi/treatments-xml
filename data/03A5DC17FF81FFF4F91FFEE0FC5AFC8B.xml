<collectingCountry id="F31B2D91FF81FFF4F99CFD0AFBCBFD4B" box="[981,1060,672,696]" name="Angola" pageId="1" pageNumber="198">Angola</collectingCountry>
, 
<collectingCountry id="F31B2D91FF81FFF4FE7BFD0AFA66FD4B" box="[1074,1417,672,696]" name="Democratic Republic of the Congo" pageId="1" pageNumber="198">Democratic Republic of Congo</collectingCountry>
, 
<collectingCountry id="F31B2D91FF81FFF4F979FD6AFC17FD2B" box="[816,1016,704,728]" name="Equatorial Guinea" pageId="1" pageNumber="198">Equatorial Guinea</collectingCountry>
, 
<collectingCountry id="F31B2D91FF81FFF4FE4DFD6AFBA2FD2B" box="[1028,1101,704,728]" name="Ghana" pageId="1" pageNumber="198">Ghana</collectingCountry>
, 
<collectingCountry id="F31B2D91FF81FFF4FE13FD6AFB47FD2B" box="[1114,1192,704,728]" name="Liberia" pageId="1" pageNumber="198">Liberia</collectingCountry>
