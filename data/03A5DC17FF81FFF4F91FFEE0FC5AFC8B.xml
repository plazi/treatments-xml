<quantity id="4CF4C0E4FF81FFF4FE24FDCAFB41FD84" box="[1133,1198,608,632]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="1" pageNumber="198" unit="mm" value="5.0">5 mm</quantity>
