<collectingCountry id="F31B2D91FF81FFF4FEFDFD6AFAADFD2B" box="[1204,1346,704,728]" name="Mozambique" pageId="1" pageNumber="198">Mozambique</collectingCountry>
, 
<collectingCountry id="F31B2D91FF81FFF4FF07FD6AFC9CFD0B" name="South Africa" pageId="1" pageNumber="198">South Africa</collectingCountry>
and 
<collectingCountry id="F31B2D91FF81FFF4F9EBFD4AFC18FD0B" box="[930,1015,736,760]" name="Uganda" pageId="1" pageNumber="198">Uganda</collectingCountry>
, (
<bibRefCitation id="EF9D10F0FF81FFF4FE4CFD4AFB74FD0B" author="ZIELKE, E." box="[1029,1179,736,760]" pageId="1" pageNumber="198" pagination="1 - 199" refId="ref5276" refString="ZIELKE, E. 1971 a: Revision der Muscinae der athiopischen Region. - Series Entomologica 7: 1 - 199." type="journal article" year="1971">ZIELKE 1971a</bibRefCitation>
