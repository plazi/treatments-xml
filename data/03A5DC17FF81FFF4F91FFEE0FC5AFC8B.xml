<taxonomicName id="4C0C1682FF81FFF4FEF2FDCAFACBFD84" baseAuthorityName="BIGOT" baseAuthorityYear="1878" box="[1211,1316,608,631]" class="Insecta" family="Muscidae" genus="Curranosia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="198" phylum="Arthropoda" rank="species" species="gemma">
<emphasis id="B978B113FF81FFF4FEF2FDCAFACBFD84" box="[1211,1316,608,631]" italics="true" pageId="1" pageNumber="198">C. gemma</emphasis>
</taxonomicName>
