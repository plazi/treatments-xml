<taxonomicName id="90C30867D45BC906831F6F2D5626BE98" authorityName="Bourguignat" authorityYear="1853" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis saulcyi" order="Pteropoda" pageId="98" pageNumber="99" phylum="Mollusca" rank="species" species="saulcyi">
