<collectingCountry id="F32373C3D4157839FC8BFD128B63FD05" box="[894,1058,674,699]" name="Guyana" pageId="11" pageNumber="512">British Guiana</collectingCountry>
and 
<collectingCountry id="F32373C3D4157839FBADFD128BE8FD05" box="[1112,1193,674,699]" name="Guyana" pageId="11" pageNumber="512">Guiana</collectingCountry>
(
<bibRefCitation id="EFA54EA2D4157839FB4DFD128A15FD05" author="Metcalf" box="[1208,1364,674,699]" pageId="11" pageNumber="512" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
