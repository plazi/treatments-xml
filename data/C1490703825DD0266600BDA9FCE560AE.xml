<paragraph id="66E608D21CCB8A1CB719B499DB36DEC5" pageId="6" pageNumber="23" type="mainText">
<taxonomicName id="C55B460D1811BF4A983B69E8B235FDC5" authority="Bloch, 1786" authorityName="Bloch" authorityYear="1786" class="Actinopterygii" family="Pleuronectidae" genus="Macrurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pleuronectiformes" pageId="6" pageNumber="23" phylum="Chordata" rank="species" species="longirostris">Macrurus longirostris</taxonomicName>
