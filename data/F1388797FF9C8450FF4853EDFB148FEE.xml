<taxonomicName id="BE914D02FF9C8450FB855474FB2A8F39" authorityName="GonzáLez-Acun &amp; Palma" authorityYear="2021" box="[1114,1210,1303,1329]" class="Insecta" family="Philopteridae" genus="Brueelia" kingdom="Animalia" order="Psocodea" pageId="72" pageNumber="73" phylum="Arthropoda" rank="genus">
