<taxonomicName id="4C3816D00A208B2EFC92FA57FBEF62B9" authority="Hing, 2009" authorityName="Hing" authorityYear="2009" box="[879,1089,1433,1454]" class="Insecta" family="Tephritidae" genus="Ectopomyia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="26" phylum="Arthropoda" rank="species" species="hancocki" status="new species">
