<keyLead id="30BEC03FFF8D3965A0E7FA7CFA1CFA7F" pageId="3" pageNumber="504">
<number id="3D87712DFF8D3965A0E7FA7CFF23FA5D" box="[151,163,1453,1475]" pageId="3" pageNumber="504" value="4.0">4</number>
