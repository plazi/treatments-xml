<taxonomicName id="30AB2FA5FFD6FF886D3A46EB78EB6CDA" authorityName="Linnaeus" authorityYear="1758" box="[1093,1230,151,177]" class="Aves" family="Anatidae" genus="Anas" kingdom="Animalia" order="Anseriformes" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="crecca">
