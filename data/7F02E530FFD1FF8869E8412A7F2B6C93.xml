<taxonomicName id="30AB2FA5FFD6FF886C7B46EB7D056CBF" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Aves" family="Anatidae" genus="Spatula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anseriformes" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="querquedula">
