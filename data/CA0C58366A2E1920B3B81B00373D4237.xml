<quantity id="855D44C56A2A1920B16B18E036FA44EF" box="[580,687,223,247]" metricMagnitude="-4" metricUnit="m" metricValue="9.3" pageId="6" pageNumber="35" unit="mm" value="0.93">0.93 mm</quantity>
; 
<taxonomicName id="85A592A36A2A1920B19118DF376144EF" box="[702,820,224,247]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="6" pageNumber="35" phylum="Arthropoda" rank="genus">mesosoma</taxonomicName>
