<taxonomicName id="5EF85A95FFACFF80FAEAF8DCFA44CB22" box="[1372,1474,1857,1881]" class="Liliopsida" family="Commelinaceae" genus="Palisota" kingdom="Plantae" order="Commelinales" pageId="6" pageNumber="175" phylum="Tracheophyta" rank="species" species="mannii">
<emphasis id="AB8CFD04FFACFF80FAEAF8DCFA44CB22" bold="true" box="[1372,1474,1857,1881]" italics="true" pageId="6" pageNumber="175">P. mannii</emphasis>
