<collectingCountry id="E1EF6186FFADFF81FD81FA69FD2FCA74" box="[567,681,1527,1551]" name="Cameroon" pageId="7" pageNumber="176">Cameroon</collectingCountry>
, Bioko) .................................................................. 
<taxonomicName id="5EF85A95FFADFF81FDC5F989FD7ACA54" box="[627,764,1559,1583]" class="Liliopsida" family="Commelinaceae" genus="Palisota" kingdom="Plantae" order="Commelinales" pageId="7" pageNumber="176" phylum="Tracheophyta" rank="species" species="preussiana">
<emphasis id="AB8CFD04FFADFF81FDC5F989FD7ACA54" bold="true" box="[627,764,1559,1583]" italics="true" pageId="7" pageNumber="176">P. preussiana</emphasis>
