<keyLead id="22098223FFADFF81FFC7FDF2FD7ACE9F" pageId="7" pageNumber="176">
14. Leaves to 
<quantity id="5E008CF3FFADFF81FEA5FDF2FEFDCEFF" box="[275,379,619,644]" metricMagnitude="-1" metricUnit="m" metricValue="1.35" metricValueMax="1.5" metricValueMin="1.2" pageId="7" pageNumber="176" unit="cm" value="13.5" valueMax="15.0" valueMin="12.0">12 –15 cm</quantity>
