<subSubSection id="D9CB01A259DB37733C7FAB6B3BBAA80F" pageId="2" pageNumber="103" type="nomenclature">
<paragraph id="85A32EFD8F07E6A4D59C563C853E4E7E" pageId="2" pageNumber="103">
<taxonomicName id="D5E7E53A6F34A58081398BAE6134BE03" authority="Kryzhanovskij, 1976" authorityName="Kryzhanovskij" authorityYear="1976" class="Insecta" family="Histeridae" genus="Hemisaprinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemisaprinus" order="Coleoptera" pageId="2" pageNumber="103" phylum="Arthropoda" rank="genus">Hemisaprinus Kryzhanovskij, 1976</taxonomicName>
