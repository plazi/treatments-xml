, 
<quantity id="8154E9B02A7EFFF0F19D2D08DAF31469" box="[151,223,583,608]" metricMagnitude="2" metricUnit="m" metricValue="2.25" pageId="21" pageNumber="92" unit="m" value="225.0">225 m</quantity>
