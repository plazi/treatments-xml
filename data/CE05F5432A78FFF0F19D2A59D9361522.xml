0º 
<number id="F06456722A7EFFF0F5982D6BDE9D140B" box="[1170,1201,548,573]" pageId="21" pageNumber="92" value="51.0">51</number>
’ 
<number id="F06456722A7EFFF0F5B62D6BDEF5140B" box="[1212,1241,548,573]" pageId="21" pageNumber="92" value="55.0">55</number>
” N
</geoCoordinate>
, 
<geoCoordinate id="239822922A7EFFF0F4002D6BDFB4140A" box="[1290,1432,548,573]" direction="west" orientation="longitude" pageId="21" pageNumber="92" precision="15" value="-76.56917">
<number id="F06456722A7EFFF0F4002D6BDF09140B" box="[1290,1317,548,573]" pageId="21" pageNumber="92" value="76.0">76</number>
º 
