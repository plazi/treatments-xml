</taxonomicName>
, not 
<taxonomicName id="4C0BAAEB8766FF8DB77FFA68FA66FA2E" box="[1136,1418,1456,1480]" class="Chilopoda" family="Linotaeniidae" genus="Scolioplanes" kingdom="Animalia" order="Geophilomorpha" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="bothriopus">
<emphasis id="B97F0D7A8766FF8DB77FFA68FA66FA2E" box="[1136,1418,1456,1480]" italics="true" pageId="9" pageNumber="10">Scolioplanes bothriopus</emphasis>
</taxonomicName>
); 
<bibRefCitation id="EF9AAC998766FF8DB398FA0CFE8AFA0A" author="Chamberlin" box="[151,358,1492,1516]" pageId="9" pageNumber="10" refString="Chamberlin, R. V. (1912 a) The Geophiloidea of the Southeastern States. Bulletin of the Museum of Comparative Zoology, 54, 405 - 436." type="journal article" year="1912" yearSuffix="a">
Chamberlin 
<number id="3DC3C34F8766FF8DB22DFA0CFEB6FA0A" box="[290,346,1492,1516]" pageId="9" pageNumber="10" value="1912.0">1912</number>
a
</bibRefCitation>
(sub 
<taxonomicName id="4C0BAAEB8766FF8DB2ABFA0DFDB1FA0A" authority="C.L.Koch, 1847" authorityName="C.L.Koch" authorityYear="1847" box="[420,605,1492,1516]" class="Chilopoda" family="Linotaeniidae" genus="Linotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Geophilomorpha" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="fulva">
