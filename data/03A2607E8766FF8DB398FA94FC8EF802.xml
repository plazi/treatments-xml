</bibRefCitation>
(sub 
<taxonomicName id="4C0BAAEB8766FF8DB02EFA68FBCEFA2E" box="[801,1058,1456,1480]" class="Chilopoda" family="Linotaeniidae" genus="Scolioplanes" kingdom="Animalia" order="Geophilomorpha" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="robustus">
