<subSubSection id="C29E04CDD933FFAB073BF9ACFBB7F985" box="[113,1090,1536,1559]" pageId="9" pageNumber="114" type="etymology">
<paragraph id="8A3B5746D933FFAB073BF9ACFBB7F985" blockId="9.[113,1090,1248,1591]" box="[113,1090,1536,1559]" pageId="9" pageNumber="114">
<emphasis id="B8F08B54D933FFAB073BF9ACFF1BF985" bold="true" box="[113,238,1536,1559]" pageId="9" pageNumber="114">Etymology.</emphasis>
