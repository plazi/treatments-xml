<taxonomicName id="4B44DBE33443C6781F84440B89185A95" class="Magnoliopsida" family="Poaceae" genus="Zea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Zea mays" order="Poales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="mays">
