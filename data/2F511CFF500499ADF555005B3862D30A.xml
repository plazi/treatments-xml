<taxonomicName id="ECD8066F61EBF737D38A1F501EDE044D" class="Monocotyledoneae" family="Poaceae" genus="Panicum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Panicum miliaceum" order="Graminales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="miliaceum">
