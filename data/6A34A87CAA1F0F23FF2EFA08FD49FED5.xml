<subSubSection id="AA874AE1AA180F23FF7EFF0FFE1BFEF0" pageId="13" pageNumber="304" type="materials_examined">
<paragraph id="E222196AAA180F23FF7EFF0FFCCDFF65" blockId="13.[151,1437,152,935]" pageId="13" pageNumber="304">
<emphasis id="D0E9C578AA180F23FF7EFF0FFE08FF01" bold="true" box="[199,494,152,179]" pageId="13" pageNumber="304">Type material examined.</emphasis>
Male 
<typeStatus id="3D26A7C8AA180F23FD8DFF0EFD7CFF00" box="[564,666,152,178]" pageId="13" pageNumber="304">holotype</typeStatus>
