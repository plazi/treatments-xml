and 
<taxonomicName id="4C074D13FF9DD560FE6124FFFDF75B29" authorityName="Verrill" authorityYear="1899" box="[438,553,1512,1537]" class="Asteroidea" family="Goniasteridae" genus="Plinthaster" kingdom="Animalia" order="Valvatida" pageId="32" pageNumber="233" phylum="Echinodermata" rank="genus">
