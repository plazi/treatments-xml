A.
</emphasis>
<taxonomicName id="4C074D13FF9DD560FD9924FEFCD95B29" baseAuthorityName="Muller &amp; Troschel" baseAuthorityYear="1842" box="[590,775,1513,1537]" class="Asteroidea" family="Goniasteridae" genus="Peltaster" kingdom="Animalia" order="Valvatida" pageId="32" pageNumber="233" phylum="Echinodermata" rank="species" species="placenta">
