<number id="3DF424D0FFF6576188636D57FED6F9FF" box="[279,287,1589,1603]" pageId="11" pageNumber="30" value="3.0">3</number>
. 
<collectingCountry id="F32B7667FFF6576188446D47FEA2F983" box="[304,363,1573,1599]" name="India" pageId="11" pageNumber="30">Hind</collectingCountry>
