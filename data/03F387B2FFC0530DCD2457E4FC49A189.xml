<taxonomicName id="4C5A4D27FFC0530CCEE4553BFCE7A4BD" authorityName="Erichson" authorityYear="1837" box="[712,831,1641,1666]" class="Insecta" family="Staphylinidae" genus="Homalota" kingdom="Animalia" order="Coleoptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="sodalis">
<emphasis id="B92EEAB6FFC0530CCEE4553BFCE7A4BD" box="[712,831,1641,1666]" italics="true" pageId="64" pageNumber="65">H. sodalis</emphasis>
