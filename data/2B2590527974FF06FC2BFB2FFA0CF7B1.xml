<taxonomicName id="648C5AC77974FF04FCA6FB1AFBFBFB65" authority="Bleszynski, 1970: 20" authorityName="Bleszynski" authorityPageNumber="20" authorityYear="1970" box="[831,1134,1203,1224]" class="Insecta" family="Crambidae" genus="Aurotalis" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="genus">
