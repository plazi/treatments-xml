<collectingCountry id="F329764C595FFFFCFF51F8E832D4CB05" box="[151,222,1859,1885]" name="Brazil" pageId="51" pageNumber="52">Brazil</collectingCountry>
, 
<collectingCountry id="F329764C595FFFFCFF2CF8E83351CB05" box="[234,347,1859,1885]" name="Colombia" pageId="51" pageNumber="52">Colombia</collectingCountry>
