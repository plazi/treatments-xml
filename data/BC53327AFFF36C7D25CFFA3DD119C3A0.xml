and 
<taxonomicName id="F3FAF8EFFFF16C7D2160FBCED4F2C008" box="[1080,1172,1080,1104]" class="Insecta" family="Eulophidae" genus="Zagrammosoma" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="velerii">
