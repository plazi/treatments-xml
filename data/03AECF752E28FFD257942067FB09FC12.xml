<geoCoordinate id="EE3318A42E28FFD255772162FCC6FCC6" box="[628,775,801,827]" degrees="44" direction="west" minutes="41" orientation="longitude" pageId="14" pageNumber="586" precision="15" seconds="30" value="-44.69167">44°41’30”W</geoCoordinate>
), soil and leaf litter of highlands grasslands (Queiroz 2014); Serra dos Órgãos National Park (
<geoCoordinate id="EE3318A42E28FFD2551B2106FD61FCA2" box="[536,672,837,863]" degrees="22" direction="south" minutes="27" orientation="latitude" pageId="14" pageNumber="586" precision="15" seconds="39" value="-22.460835">22°27’39”S</geoCoordinate>
; 
<geoCoordinate id="EE3318A42E28FFD255AC2106FCF2FCA2" box="[687,819,837,863]" degrees="43" direction="west" minutes="1" orientation="longitude" pageId="14" pageNumber="586" precision="15" seconds="42" value="-43.02833">43°1’42”W</geoCoordinate>
), soil and leaf litter of highlands grasslands (Queiroz 2014); Restinga de Jurubatiba National Park (
<geoCoordinate id="EE3318A42E28FFD255A0212AFCEAFC7E" box="[675,811,873,899]" degrees="22" direction="south" minutes="17" orientation="latitude" pageId="14" pageNumber="586" precision="15" seconds="39" value="-22.294167">22°17’39”S</geoCoordinate>
; 
<geoCoordinate id="EE3318A42E28FFD2543A212BFC0DFC7E" box="[825,972,872,899]" degrees="41" direction="west" minutes="40" orientation="longitude" pageId="14" pageNumber="586" precision="15" seconds="53" value="-41.68139">41°40’53”W</geoCoordinate>
), dunes vegetation and supralittoral vegetation (Silveira 2018); 
<collectingRegion id="49C3B0812E28FFD256A621CEFE24FC5A" box="[421,485,909,935]" country="New Zealand" name="West Coast" pageId="14" pageNumber="586">Costa</collectingRegion>
do 
<collectingRegion id="49C3B0812E28FFD2551221CEFDB7FC5A" box="[529,630,909,935]" inferredBySuffix="true" pageId="14" pageNumber="586">Sol State</collectingRegion>
