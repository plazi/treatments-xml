<geoCoordinate id="EE3318A42E28FFD256E22162FDA8FCC6" box="[481,617,801,827]" degrees="22" direction="south" minutes="22" orientation="latitude" pageId="14" pageNumber="586" precision="15" seconds="46" value="-22.379444">22°22’46”S</geoCoordinate>
