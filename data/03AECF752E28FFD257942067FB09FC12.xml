<geoCoordinate id="EE3318A42E28FFD25537209BFD05FD0E" box="[564,708,728,755]" degrees="42" direction="west" minutes="51" orientation="longitude" pageId="14" pageNumber="586" precision="15" seconds="31" value="-42.85861">42º51’31”W</geoCoordinate>
