<geoCoordinate id="EE3318A42E28FFD2569E209BFDE2FD0E" box="[413,547,728,755]" degrees="22" direction="south" minutes="44" orientation="latitude" pageId="14" pageNumber="586" precision="15" seconds="50" value="-22.747223">22º44’50”S</geoCoordinate>
