, 
<collectingCountry id="12CA84AD3862FFB039A42E7EFCD6D336" box="[725,816,1362,1386]" name="Senegal" pageId="17" pageNumber="18">Senegal</collectingCountry>
, 
<collectingCountry id="12CA84AD3862FFB0384F2E7EFC35D336" box="[830,979,1362,1386]" name="Sierra Leone" pageId="17" pageNumber="18">Sierra Leone</collectingCountry>
, 
<collectingCountry id="12CA84AD3862FFB038912E7FFB8FD336" box="[992,1129,1362,1387]" name="Ivory Coast" pageId="17" pageNumber="18">Ivory Coast</collectingCountry>
; West Africa: 
<collectingCountry id="12CA84AD3862FFB03E7E2E7EFA83D336" box="[1295,1381,1362,1386]" name="Angola" pageId="17" pageNumber="18">Angola</collectingCountry>
; 
<quantity id="AD2569D83862FFB03E032E7EFF3BD3D3" metricMagnitude="1" metricUnit="m" metricValue="9.25" metricValueMax="11.5" metricValueMin="7.0" pageId="17" pageNumber="18" unit="m" value="92.5" valueMax="115.0" valueMin="70.0">70– 115 m</quantity>
and planktonic
</paragraph>
</subSubSection>
