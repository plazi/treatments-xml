(Wiegmann, 1836)
</taxonomicName>
and 
<taxonomicName id="2FFF4DEAFF98FFE4FF391E70872AB69D" authority="(Stimpson, 1871)" baseAuthorityName="Stimpson" baseAuthorityYear="1871" box="[160,638,1149,1174]" class="Malacostraca" family="Palaemonidae" genus="Palaemon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="814" phylum="Arthropoda" rank="species" species="pandaliformis">
