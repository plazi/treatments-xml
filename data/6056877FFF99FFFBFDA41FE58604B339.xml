<taxonomicName id="2FFF4DEAFF98FFE4FE371F9F86E8B7A0" authority="(de Man, 1896)" baseAuthorityName="de Man" baseAuthorityYear="1896" box="[430,956,1426,1451]" class="Malacostraca" family="Glyptograpsidae" genus="Platychirograpsus" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="814" phylum="Arthropoda" rank="species" species="spectabilis">
