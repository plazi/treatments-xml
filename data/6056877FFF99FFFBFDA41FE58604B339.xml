<taxonomicName id="2FFF4DEAFF98FFE4FC151C6F8408B495" authority="(Saussure, 1858)" baseAuthorityName="Saussure" baseAuthorityYear="1858" class="Malacostraca" family="Sesarmidae" genus="Armases" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="814" phylum="Arthropoda" rank="species" species="americanum">
<emphasis id="DA8BEA7BFF98FFE4FC151C6F81D7B470" box="[908,1155,1634,1659]" italics="true" pageId="15" pageNumber="814">Armases americanum</emphasis>
(Saussure, 1858)
</taxonomicName>
, and 
<taxonomicName id="2FFF4DEAFF98FFE4FE381C888786B495" authority="(Dana, 1852)" baseAuthorityName="Dana" baseAuthorityYear="1852" box="[417,722,1669,1694]" class="Malacostraca" family="Sesarmidae" genus="Armases" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="814" phylum="Arthropoda" rank="species" species="angustipes">
<emphasis id="DA8BEA7BFF98FFE4FE381C888762B495" box="[417,566,1669,1694]" italics="true" pageId="15" pageNumber="814">A. angustipes</emphasis>
(Dana, 1852)
</taxonomicName>
from St. Thomas; 
<taxonomicName id="2FFF4DEAFF98FFE4FC361C888435B4CB" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Malacostraca" family="Gecarcinidae" genus="Gecarcinus" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="814" phylum="Arthropoda" rank="species" species="ruricola">
