<taxonomicName id="2FFF4DEAFF98FFE4FF391C6F8480B470" authority="(Smith, 1870)" baseAuthorityName="Smith" baseAuthorityYear="1870" box="[160,468,1634,1659]" class="Malacostraca" family="Ocypodidae" genus="Minuca" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="814" phylum="Arthropoda" rank="species" species="mordax">
