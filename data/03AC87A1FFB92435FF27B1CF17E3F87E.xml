<emphasis id="B971EAA5FFB82435FB76B2751623F9EA" italics="true" pageId="13" pageNumber="14">Mulloidichthys flavolineatus</emphasis>
</taxonomicName>
(
<number id="3DCD2490FFB82435FEAAB2511603F9EA" box="[282,298,1589,1613]" pageId="13" pageNumber="14" value="2.0">2</number>
); and from the ITS 
<number id="3DCD2490FFB82435FE4EB2511525F9EA" box="[510,524,1589,1613]" pageId="13" pageNumber="14" value="2.0">2</number>
region from five specimens – 
<taxonomicName id="4C054D34FFB82435FCEEB25214E2F9EA" box="[862,971,1589,1613]" class="Actinopterygii" family="Mullidae" genus="Parupeneus" kingdom="Animalia" order="Perciformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="ciliatus">
<emphasis id="B971EAA5FFB82435FCEEB2521445F9EA" box="[862,876,1590,1613]" italics="true" pageId="13" pageNumber="14">P</emphasis>
. 
<emphasis id="B971EAA5FFB82435FCC9B25114E2F9EA" box="[889,971,1589,1613]" italics="true" pageId="13" pageNumber="14">ciliatus</emphasis>
</taxonomicName>
(
<number id="3DCD2490FFB82435FC6BB25114C2F9EA" box="[987,1003,1589,1613]" pageId="13" pageNumber="14" value="2.0">2</number>
), 
<taxonomicName id="4C054D34FFB82435FBB0B2511216F9EA" box="[1024,1343,1589,1613]" class="Actinopterygii" family="Mullidae" genus="Mulloidichthys" kingdom="Animalia" order="Perciformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="flavolineatus">
<emphasis id="B971EAA5FFB82435FBB0B2511216F9EA" box="[1024,1343,1589,1613]" italics="true" pageId="13" pageNumber="14">Mulloidichthys flavolineatus</emphasis>
