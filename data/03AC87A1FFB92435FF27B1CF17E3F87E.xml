(
<number id="3DCD2490FFB82435FAE0B2511275F9EA" box="[1360,1372,1589,1613]" pageId="13" pageNumber="14" value="1.0">1</number>
), and 
<taxonomicName id="4C054D34FFB82435FF27B23D16F8F9D6" box="[151,465,1625,1649]" class="Actinopterygii" family="Mullidae" genus="Mulloidichthys" kingdom="Animalia" order="Perciformes" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="vanicolensis">
