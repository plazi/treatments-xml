<taxonomicName id="4C4A4D7DFFBEC17A01FEA3153A4DCF75" authority="(Claus 1863)" baseAuthorityName="Claus" baseAuthorityYear="1863" box="[686,1340,578,605]" class="Hexanauplia" family="Laophontidae" genus="Paralaophonte" kingdom="Animalia" order="Harpacticoida" pageId="3" pageNumber="198" phylum="Arthropoda" rank="species" species="brevirostris" subGenus="Paralaophonte">
