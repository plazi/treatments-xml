Asia-Tropical, 
<collectingCountry id="0E8076617138FF9AFDFADE071850F8BA" box="[527,625,1893,1919]" name="Vietnam" pageId="3" pageNumber="496">Vietnam</collectingCountry>
, 
<collectingCountry id="0E8076617138FF9AFD89DE0718C3F8BA" box="[636,738,1893,1919]" name="Thailand" pageId="3" pageNumber="496">Thailand</collectingCountry>
, 
