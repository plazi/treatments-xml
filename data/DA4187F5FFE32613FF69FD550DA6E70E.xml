<quantity id="95109B06FFE42613FA1FFE950DBBE5AE" metricMagnitude="-3" metricUnit="m" metricValue="1.0" metricValueMax="1.1" metricValueMin="0.9" pageId="4" pageNumber="319" unit="mm" value="1.0" valueMax="1.1" valueMin="0.9">0.9- 1.1 mm</quantity>
