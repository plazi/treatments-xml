<subSubSection id="C30C92B79519FFDEFDB5FF5A7722FF6E" pageId="28" pageNumber="29" type="biology_ecology">
<paragraph id="8BA9C13C9519FFDEFDB5FF5A7722FF6E" blockId="28.[151,1436,150,286]" pageId="28" pageNumber="29">
<emphasis id="B9621D2E9519FFDEFDB5FF5A724CFE8B" bold="true" box="[199,289,222,248]" pageId="28" pageNumber="29">Biology</emphasis>
