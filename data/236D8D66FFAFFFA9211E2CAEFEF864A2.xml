<typeStatus id="747F82D2FFA8FFA925C72829FF4D6434" box="[193,242,594,615]" pageId="60" pageNumber="513">type</typeStatus>
came from 
<collectingCountry id="D3D37CE0FFA8FFA9247C2829FE4F643B" box="[378,496,594,616]" name="Argentina" pageId="60" pageNumber="513">Argentina</collectingCountry>
