<paragraph id="7E20368C9A18D31CFF1BFF05AE4FFF49" blockId="9.[151,634,222,245]" box="[151,634,222,245]" pageId="9" pageNumber="250">
<typeStatus id="A124882E9A18D31CFF1BFF05ACFDFF49" box="[151,200,223,244]" pageId="9" pageNumber="250">Type</typeStatus>
species: 
<taxonomicName id="B99F4D0F9A18D31CFEAAFF05AE42FF49" authority="Kallies, 1998" authorityName="Kallies" authorityYear="1998" box="[294,631,222,245]" class="Insecta" family="Brachodidae" genus="Paranigilgia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="250" phylum="Arthropoda" rank="species" species="aritai">
<emphasis id="4CEBEA9E9A18D31CFEAAFF05ADD0FF48" box="[294,485,223,245]" italics="true" pageId="9" pageNumber="250">Paranigilgia aritai</emphasis>
Kallies, 
<number id="C85724AB9A18D31CFDCDFF05AE42FF49" box="[577,631,223,244]" pageId="9" pageNumber="250" value="1998.0">1998</number>
