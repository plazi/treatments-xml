<taxonomicName id="2409DDD5EB43599EF7FE38858AF55FD6" class="Insecta" family="Braconidae" genus="Zelomorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zelomorpha mariyavladmirovnae" order="Hymenoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="mariyavladmirovnae">
