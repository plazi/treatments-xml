<paragraph id="AE0DE731FFD3FFC3C3F3E728FDA90A19" blockId="17.[151,535,1373,1399]" box="[151,535,1373,1399]" pageId="17" pageNumber="18">
<heading id="F545505DFFD3FFC3C3F3E728FDA90A19" bold="true" box="[151,535,1373,1399]" fontSize="11" level="1" pageId="17" pageNumber="18" reason="1">
<taxonomicName id="69B29CB2FFD3FFC3C3F3E728FDA90A19" authority="Calman 1907" authorityName="Calman" authorityYear="1907" box="[151,535,1373,1399]" class="Malacostraca" family="Bodotriidae" genus="Cyclaspis" kingdom="Animalia" order="Cumacea" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="elegans">
