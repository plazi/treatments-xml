<taxonomicName id="EFF4CDC0120295760EF6FB34FC08FAD1" box="[664,981,1268,1292]" class="Arachnida" family="Syringophilidae" genus="Syringophilopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="386" phylum="Arthropoda" rank="species" species="philemonis">
