<number id="9E3CA464120295760D2AFD4FFEB3FD7A" box="[324,366,655,679]" pageId="1" pageNumber="386" value="230.0">230</number>
(
<number id="9E3CA464120295760D12FD4FFE74FD7A" box="[380,425,655,679]" pageId="1" pageNumber="386" value="235.0">235</number>
