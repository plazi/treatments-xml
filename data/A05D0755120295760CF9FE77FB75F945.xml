<emphasis id="1A806A51120295760E9DFD50FCBFFD75" box="[755,866,656,680]" italics="true" pageId="1" pageNumber="386">
<taxonomicName id="EFF4CDC0120295760E9DFD50FC82FD75" box="[755,863,656,680]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="386" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
