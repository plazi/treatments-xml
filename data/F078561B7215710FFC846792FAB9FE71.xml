<quantity id="BF294AE87212710FFE8060D8F8F8FC31" box="[338,493,992,1016]" metricMagnitude="-4" metricUnit="m" metricValue="1.15" metricValueMax="1.2" metricValueMin="1.1" pageId="68" pageNumber="159" unit="mm" value="0.11499999999999999" valueMax="0.12" valueMin="0.11">0.11–0.12 mm</quantity>
, length of temples 
<quantity id="BF294AE87212710FFD1E60D8FA38FC31" box="[716,813,992,1016]" metricMagnitude="-5" metricUnit="m" metricValue="8.0" pageId="68" pageNumber="159" unit="mm" value="0.08">0.08 mm</quantity>
, pronotum width 
<quantity id="BF294AE87212710FFE5D6738FB3CFBD1" box="[399,553,1024,1048]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" metricValueMax="4.2" metricValueMin="4.0" pageId="68" pageNumber="159" unit="mm" value="0.41" valueMax="0.42" valueMin="0.4">0.40–0.42 mm</quantity>
