, pronotum width 
<quantity id="BF294AE87212710FFD406058FA38FCB1" box="[658,813,864,888]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" metricValueMax="5.1" metricValueMin="4.9" pageId="68" pageNumber="159" unit="mm" value="0.5" valueMax="0.51" valueMin="0.49">0.49–0.51 mm</quantity>
, pronotum length 
<quantity id="BF294AE87212710FFE7560B8FB12FC51" box="[423,519,896,920]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="68" pageNumber="159" unit="mm" value="0.35">0.35 mm</quantity>
, sutural length of elytra 
<quantity id="BF294AE87212710FFF066098F87AFC71" box="[212,367,928,952]" metricMagnitude="-4" metricUnit="m" metricValue="3.7" metricValueMax="3.8" metricValueMin="3.5999999999999996" pageId="68" pageNumber="159" unit="mm" value="0.37" valueMax="0.38" valueMin="0.36">0.36–0.38 mm</quantity>
, elytral width 
<quantity id="BF294AE87212710FFDFB6098FBD1FC71" box="[553,708,928,952]" metricMagnitude="-4" metricUnit="m" metricValue="6.25" metricValueMax="6.4" metricValueMin="6.1" pageId="68" pageNumber="159" unit="mm" value="0.625" valueMax="0.64" valueMin="0.61">0.61–0.64 mm</quantity>
. females: head width 
<quantity id="BF294AE87212710FFE8960F8F8E3FC11" box="[347,502,960,984]" metricMagnitude="-4" metricUnit="m" metricValue="3.75" metricValueMax="3.8" metricValueMin="3.7" pageId="68" pageNumber="159" unit="mm" value="0.375" valueMax="0.38" valueMin="0.37">0.37–0.38 mm</quantity>
, head length 
<quantity id="BF294AE87212710FFD4060F8FA38FC11" box="[658,813,960,984]" metricMagnitude="-4" metricUnit="m" metricValue="2.45" metricValueMax="2.5" metricValueMin="2.4" pageId="68" pageNumber="159" unit="mm" value="0.245" valueMax="0.25" valueMin="0.24">0.24–0.25 mm</quantity>
