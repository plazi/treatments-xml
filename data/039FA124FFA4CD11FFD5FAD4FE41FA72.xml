<quantity id="4CCEBDD7FFA4CD17FCC7FBE2FC74FBF4" box="[867,992,1046,1071]" metricMagnitude="-3" metricUnit="m" metricValue="1.2999999999999998" metricValueMax="1.4" metricValueMin="1.2" pageId="3" pageNumber="4" unit="mm" value="1.2999999999999998" valueMax="1.4" valueMin="1.2">1,2–1,4 mm</quantity>
