<quantity id="4CCEBDD7FFA4CD17FB29FB42FA84FB14" box="[1165,1296,1206,1231]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="2.1" metricValueMin="1.4" pageId="3" pageNumber="4" unit="mm" value="1.75" valueMax="2.1" valueMin="1.4">1,4 –2,1 mm</quantity>
