<paragraph id="8BBA9943FF8EFFB1E7FE7B3E6D062F46" blockId="5.[151,1056,534,587]" box="[151,1056,565,587]" pageId="5" pageNumber="126">
<treatmentCitationGroup id="AB15BE6DFF8EFFB1E7FE7B3E6D062F46" box="[151,1056,565,587]" pageId="5" pageNumber="126">
<emphasis id="B9714551FF8EFFB1E7FE7B3E68B12F46" box="[151,407,565,587]" italics="true" pageId="5" pageNumber="126">
<taxonomicName id="4C05E2C0FF8EFFB1E7FE7B3E68362F46" authorityName="Allman" authorityYear="1888" box="[151,272,565,587]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="5" pageNumber="126" phylum="Cnidaria" rank="genus">Staurotheca</taxonomicName>
