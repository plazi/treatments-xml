<specimenCode id="DBE69E8D79364A6B640B62DF1208FA69" box="[336,517,1476,1503]" collectionCode="MFLU" country="Thailand" httpUri="http://grbio.org/cool/672j-mk16" name="Mae Fah Laung University Herbarium" pageId="5" pageNumber="147">MFLU 21-0003</specimenCode>
