<materialsCitation id="3B283CAB79364A6B65E662DE1204FA69" ID-GBIF-Occurrence="3341328301" box="[189,521,1476,1503]" collectionCode="MFLU" pageId="5" pageNumber="147" specimenCode="MFLU 21-0003" specimenCount="1" typeStatus="holotype">
