<paragraph id="3D317EF001BC3AD71945CAFADD2DA8BC" pageNumber="63">
(= 
<taxonomicName id="1941F1BBCB141F7BD2C8B09D3D15074C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:137060" class="Insecta" family="Formicidae" genus="Crematogaster" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crematogaster pacifica Santschi" lsidName-HNS="Crematogaster pacifica Santschi" order="Hymenoptera" pageNumber="63" phylum="Arthropoda" rank="species" species="pacifica">Cr. pacifica Santschi</taxonomicName>
