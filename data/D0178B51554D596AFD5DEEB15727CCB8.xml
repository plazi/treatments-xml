<taxonomicName id="F2E7BFD16E0334D9AECD60E61FC049C2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:36398" class="Insecta" family="Formicidae" genus="Solenopsis" kingdom="Animalia" lsidName-HNS="Solenopsis wasmannii Emery" order="Hymenoptera" pageNumber="297" phylum="Arthropoda" rank="species" species="wasmanni">Solenopsis Wasmanni Emery</taxonomicName>
