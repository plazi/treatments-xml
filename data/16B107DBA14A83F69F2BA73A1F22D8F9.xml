<taxonomicName id="32BAE186037BAE9D6C7518B2FAED301C" class="Insecta" family="Bostrichidae" genus="Apate" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apate" order="Coleoptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="genus">Apate</taxonomicName>
