<taxonomicName id="21C214444BB3B8A513646826DB4E4AB0" class="Insecta" family="Bostrichidae" genus="Apatides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apatides" order="Coleoptera" pageId="160" pageNumber="161" phylum="Arthropoda" rank="genus">Apatides</taxonomicName>
