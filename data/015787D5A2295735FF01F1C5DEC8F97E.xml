<collectingRegion id="4B3AF821A2295735FB9CF16ADEF8FAF1" box="[1077,1191,1326,1350]" country="India" name="Karnataka" pageId="4" pageNumber="41">Karnataka</collectingRegion>
, 
<collectingRegion id="4B3AF821A2295735FB1BF16ADEA4FAF1" box="[1202,1275,1326,1350]" country="India" name="Kerala" pageId="4" pageNumber="41">Kerala</collectingRegion>
