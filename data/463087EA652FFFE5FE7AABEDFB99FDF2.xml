<taxonomicName id="09994D7F6528FFE4FEECAF16FDAFFACF" authority="(Ashmead)" baseAuthorityName="Ashmead" baseAuthorityYear="1900" box="[260,614,1301,1324]" class="Insecta" family="Signiphoridae" genus="Chartocerus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="684" phylum="Arthropoda" rank="species" species="australiensis">
