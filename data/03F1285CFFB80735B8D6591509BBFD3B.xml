<heading id="D0AF2E26FFB80731B8D659150F6EFBAA" bold="true" box="[151,569,1051,1078]" fontSize="11" level="1" pageId="3" pageNumber="60" reason="1">
<emphasis id="B92C4558FFB80731B8D659150F6EFBAA" bold="true" box="[151,569,1051,1078]" pageId="3" pageNumber="60">
<taxonomicName id="4C58E2C9FFB80731B8D659150C81FBA9" box="[151,470,1051,1077]" class="Insecta" family="Ropalomeridae" genus="Acrocephalomyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="torulosa" status="sp. nov.">
<emphasis id="B92C4558FFB80731B8D659150C81FBA9" bold="true" box="[151,470,1051,1077]" italics="true" pageId="3" pageNumber="60">Acrocephalomyia torulosa</emphasis>
</taxonomicName>
