<quantity id="4CD39B07FFD11A07CB6737DB47CBF95F" box="[811,977,1010,1034]" metricMagnitude="-4" metricUnit="m" metricValue="2.65" metricValueMax="2.8" metricValueMin="2.5" pageId="31" pageNumber="32" unit="mm" value="0.265" valueMax="0.28" valueMin="0.25">0.25–0.28 mm</quantity>
