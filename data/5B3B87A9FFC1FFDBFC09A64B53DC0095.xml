<collectionCode id="B583AE7AFFC2FFDAFBECA54D5691046A" box="[1138,1209,1605,1628]" collectionName="USA, California, San Francisco, California Academy of Sciences" pageId="54" pageNumber="219">CASC</collectionCode>
