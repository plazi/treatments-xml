<taxonomicName id="4C2C4D0C421BFFCD98D47D74AD3EF897" baseAuthorityName="Schmalfuss" baseAuthorityYear="1980" box="[929,1104,1803,1827]" class="Malacostraca" family="Scleropactidae" genus="Scleropactes" kingdom="Animalia" order="Isopoda" pageId="39" pageNumber="40" phylum="Arthropoda" rank="genus">
