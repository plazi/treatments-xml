<taxonomicName id="4C364D1DFFF8FFD0A821FABB10EC4539" box="[309,473,1351,1377]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="decidua">
Larix 
<taxonomicName id="4C364D1DFFF8FFD0A868FAB410EC4539" box="[380,473,1351,1377]" pageId="24" pageNumber="25" rank="species" species="decidua">decidua</taxonomicName>
</taxonomicName>
x 
<taxonomicName id="4C364D1DFFF8FFD0A8E0FAB4135C4539" box="[500,617,1351,1377]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="kaempferi">kaempferi</taxonomicName>
</emphasis>
, 
<emphasis id="B942EA8CFFF8FFD0AB6CFAB413E54539" box="[632,720,1351,1377]" italics="true" pageId="24" pageNumber="25">gmelini</emphasis>
, 
<taxonomicName id="4C364D1DFFF8FFD0ABCBFAB412614539" box="[735,852,1351,1377]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="kaempferi">
<emphasis id="B942EA8CFFF8FFD0ABCBFAB412614539" box="[735,852,1351,1377]" italics="true" pageId="24" pageNumber="25">kaempferi</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C364D1DFFF8FFD0AA77FAB412884539" box="[867,957,1351,1377]" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="sibirica">
<emphasis id="B942EA8CFFF8FFD0AA77FAB412884539" box="[867,957,1351,1377]" italics="true" pageId="24" pageNumber="25">sibirica</emphasis>
