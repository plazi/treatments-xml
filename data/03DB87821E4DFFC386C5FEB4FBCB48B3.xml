<taxonomicName id="4C724D171E4DFFC68706F800FE394885" authorityName="Salles &amp; Domínguez" authorityYear="2012" box="[340,449,1990,2013]" class="Insecta" family="Leptophlebiidae" genus="Ulmeritoides" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="araponga">
