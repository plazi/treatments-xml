<taxonomicName id="20F00789FFC55334FCFE6A2AFB7D2938" authority="Chen &amp; Yin 2003" authorityName="Chen &amp; Yin" authorityYear="2003" box="[825,1224,1302,1328]" class="Insecta" family="Bostrichidae" genus="Dysides" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="407" phylum="Arthropoda" rank="species" species="spineus">
