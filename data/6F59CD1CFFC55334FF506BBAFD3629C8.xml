<taxonomicName id="20F00789FFC55334FADE6A2AFF7A295D" authority="Kurz" authorityName="Kurz" class="Magnoliopsida" family="Dipterocarpaceae" genus="Parashorea" kingdom="Plantae" order="Malvales" pageId="18" pageNumber="407" phylum="Tracheophyta" rank="genus">
<emphasis id="D584A018FFC55334FADE6A2AFA292938" box="[1305,1436,1302,1328]" italics="true" pageId="18" pageNumber="407">Parashorea</emphasis>
Kurz
</taxonomicName>
(
<taxonomicName id="20F00789FFC55334FF276A06FE1D295D" box="[224,424,1338,1365]" class="Magnoliopsida" family="Dipterocarpaceae" kingdom="Plantae" order="Malvales" pageId="18" pageNumber="407" phylum="Tracheophyta" rank="family">Dipterocarpaceae</taxonomicName>
