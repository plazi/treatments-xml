<subSubSection id="C3616572FF9CE476A8B5514342BFFC6C" pageId="60" pageNumber="61" type="biology_ecology">
<paragraph id="8BC436F9FF9CE476A8B551434282FCA5" blockId="60.[151,1437,740,1016]" box="[199,897,812,837]" pageId="60" pageNumber="61">
