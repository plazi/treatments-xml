<taxonomicName id="4C7B4D7AFF9CE476A97F514340A1FCA4" authorityName="" authorityYear="1835" box="[269,418,812,837]" class="Magnoliopsida" family="Brassicaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Brassicales" pageId="60" pageNumber="61" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
