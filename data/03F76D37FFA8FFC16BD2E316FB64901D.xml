<taxonomicName id="4C5EA7A2FFA6FFC16E79E5E5FA1293F1" authorityName="Walcott" authorityYear="1916" box="[1340,1436,477,503]" class="Trilobita" family="Kingstoniidae" genus="Blountia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Ptychopariida" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
