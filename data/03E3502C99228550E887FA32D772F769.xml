60. 
<taxonomicName id="4C4A9AB999228550E8D1FA32D617F586" authority="(Pollet, 1990) (Dolichopodinae)" authorityName="Dolichopodinae" baseAuthorityName="Pollet" baseAuthorityYear="1990" box="[193,838,1484,1512]" class="Insecta" family="Dolichopodidae" genus="Gymnopternus" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="silvestris">
