Fig. 
<number id="3DA62495FF9A322513DDFAB0FE37FADF" box="[422,436,1352,1378]" pageId="3" pageNumber="24" value="1.0">1</number>
</figureCitation>
). 
<collectingCountry id="F3797622FF9A322513B6FAB0FDC2FADF" box="[461,577,1352,1378]" name="Argentina" pageId="3" pageNumber="24">Argentina</collectingCountry>
and 
<collectingCountry id="F3797622FF9A32251001FAB0FD3EFADF" box="[634,701,1352,1378]" name="Chile" pageId="3" pageNumber="24">Chile</collectingCountry>
