<taxonomicName id="3250A17FB0B1EFBB97A32DD29B71AAB1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28905" class="Insecta" family="Formicidae" genus="Echinopla" kingdom="Animalia" lsidName-HNS="Echinopla pallipes Smith" order="Hymenoptera" pageId="198" pageNumber="198" phylum="Arthropoda" rank="species" species="pallipes">Echinopla pallipes, Smith</taxonomicName>
