<quantity id="0407B587FFA27750FE60F4BEFDD242C7" box="[499,564,1933,1957]" metricMagnitude="-1" metricUnit="m" metricValue="2.6" pageId="8" pageNumber="297" unit="cm" value="26.0">26 cm</quantity>
