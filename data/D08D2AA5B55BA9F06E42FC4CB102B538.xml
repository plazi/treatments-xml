<td id="02A7516F259A925D1B03BD53E1321E07" align="left" pageId="null" pageNumber="300" valign="top">
4. 
<normalizedToken id="A4A51AC4741F66DD33025F02AF0A8078" originalValue="Blatthäutchen" pageId="null" pageNumber="300">Blatthaeutchen</normalizedToken>
