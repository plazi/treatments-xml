<taxonomicName id="692DF115FFEF50C5213624EF32FFEED4" LSID="FE665D19-000D-578E-3674-BDD31D8FFE61" authority="Schauberger, 1932" authorityName="Schauberger" authorityYear="1932" class="Insecta" family="Carabidae" genus="Chydaeus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Chydaeus andrewesi" order="Coleoptera" pageId="24" pageNumber="63" phylum="Arthropoda" rank="species" species="andrewesi">
