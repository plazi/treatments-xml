<taxonomicName id="A4CC7E89FF8FE4146AD6F939D408938B" authority="Cunha &amp; Wilson, 2006" authorityName="Cunha &amp; Wilson" authorityYear="2006" box="[403,617,1686,1709]" class="Malacostraca" family="Ischnomesidae" genus="Heteromesus" kingdom="Animalia" order="Isopoda" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="calcar" status="sp. nov.">
