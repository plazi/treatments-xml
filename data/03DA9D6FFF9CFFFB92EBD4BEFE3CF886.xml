<emphasis id="B907F06BFF9CFFFB92BBD724FEA1F8E5" bold="true" box="[199,352,1811,1836]" inLineHeading="true" pageId="14" pageNumber="301" reason="1">Distribution.</emphasis>
<taxonomicName id="4C7357FAFF9CFFFB931BD723FE21F8E5" authorityName="Navas" authorityYear="1914" box="[359,480,1811,1836]" class="Insecta" family="Ascalaphidae" genus="Deleproctophylla" kingdom="Animalia" order="Neuroptera" pageId="14" pageNumber="301" phylum="Arthropoda" rank="species" species="dusmeti">
<emphasis id="B907F06BFF9CFFFB931BD723FE21F8E5" box="[359,480,1811,1836]" italics="true" pageId="14" pageNumber="301">D. dusmeti</emphasis>
</taxonomicName>
