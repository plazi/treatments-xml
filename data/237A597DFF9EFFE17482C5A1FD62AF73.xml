1B00 042 ex: 
<collectingMethod id="7292907CFF9EFFE67482C7F6FEC2ADED" box="[151,379,1930,1956]" pageId="51" pageNumber="52">flight intercept trap</collectingMethod>
” (
<specimenCount id="BDD523E2FF9EFFE6758CC7F6FE05ADED" box="[409,444,1930,1956]" pageId="51" pageNumber="52" type="female">1♀</specimenCount>
, 
<collectionCode id="CDC270AEFF9EFFE675DFC7F6FDA6ADED" box="[458,543,1930,1956]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34264" name="University of Kansas - Biodiversity Institute" pageId="51" pageNumber="52" type="Museum">SEMC</collectionCode>
