<taxonomicName id="04AC166954BA8CCA29E8383C7AABD38E" class="Agaricomycetes" family="Meripilaceae" genus="Meripilus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Meripilus giganteus" order="Polyporales" pageId="35" pageNumber="36" phylum="Basidiomycota" rank="species" species="giganteus">Meripilus giganteus</taxonomicName>
