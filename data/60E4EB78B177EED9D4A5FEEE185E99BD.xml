<taxonomicName id="9D2E88C6A796DABA815C12E77D9BAC37" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pinus ponderosa" order="Pinales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="ponderosa">Pinus ponderosa</taxonomicName>
