<taxonomicName id="A1D40F9DBBF8967DB81B77EB90E11AFE" class="Agaricomycetes" family="Polyporaceae" genus="Polyporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Polyporus adustus" order="Polyporales" pageId="35" pageNumber="36" phylum="Basidiomycota" rank="species" species="adustus">Polyporus adustus</taxonomicName>
(IL), 
<taxonomicName id="5295E087D76739D7696F16AB20C4C335" class="Agaricomycetes" family="Fomitopsidaceae" genus="Laetiporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Laetiporus sulphureus" order="Polyporales" pageId="35" pageNumber="36" phylum="Basidiomycota" rank="species" species="sulphureus">Laetiporus sulphureus</taxonomicName>
(IL), under 
<taxonomicName id="EE47027ED924DA9F409EF500A49E05DC" class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pinus" order="Pinales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="genus">Pinus</taxonomicName>
