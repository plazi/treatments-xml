<taxonomicName id="8A8ED1656CE5DB4DB38F3A88FFBE6E76" class="Agaricomycetes" family="Pleurotaceae" genus="Pleurotus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Pleurotus ostreatus" order="Agaricales" pageId="35" pageNumber="36" phylum="Basidiomycota" rank="species" species="ostreatus">Pleurotus ostreatus</taxonomicName>
