<taxonomicName id="F81679C686889859D442FC1058A28A23" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus rubrus" order="Fagales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="species" species="rubrus">Quercus rubrus</taxonomicName>
