<collectingCountry id="F32A760BFF4CE3556F409BE1FE70FD44" box="[359,426,697,722]" name="China" pageId="72" pageNumber="73">China</collectingCountry>
(
<collectingRegion id="49F9F879FF4CE3556F9E9BE1FD8AFD44" box="[441,592,697,722]" country="China" name="Heilongjiang" pageId="72" pageNumber="73">Heilongjiang</collectingRegion>
, 
<collectingRegion id="49F9F879FF4CE3556C7C9BE1FD6AFD44" box="[603,688,697,722]" country="China" name="Beijing" pageId="72" pageNumber="73">Beijing</collectingRegion>
, 
<collectingRegion id="49F9F879FF4CE3556C9C9BE1FCDBFD44" box="[699,769,697,722]" country="China" name="Hebei" pageId="72" pageNumber="73">Hebei</collectingRegion>
, 
<collectingRegion id="49F9F879FF4CE3556D2B9BE1FCAEFD44" box="[780,884,697,722]" country="China" name="Liaoning" pageId="72" pageNumber="73">Liaoning</collectingRegion>
, 
<collectingRegion id="49F9F879FF4CE3556D589BE1FC28FD44" box="[895,1010,697,722]" country="China" name="Shandong" pageId="72" pageNumber="73">Shandong</collectingRegion>
