<taxonomicName id="B7FB81495EB82A42E9D9473B9470D488" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus albidosparsus" order="Coleoptera" pageId="5" pageNumber="102" phylum="Arthropoda" rank="species" species="albidosparsus">Trigonopterus albidosparsus</taxonomicName>
