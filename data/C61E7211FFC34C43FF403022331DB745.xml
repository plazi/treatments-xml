(
<bibRefCitation id="2A26BEF6FFC44C43FBC034EA36D9B4EE" author="Smreczynski, S." box="[1047,1265,1413,1440]" pageId="68" pageNumber="69" pagination="1 - 115" refId="ref113520" refString="Smreczynski, S. (1976) Ryjkowce-Curculionidae: Podrodzina Curculioninae. Klucze do Oznaczania Owadow Polski, 87 (XIX, 98 f), 1 - 115." type="journal article" year="1976">Smreczyński, 1976</bibRefCitation>
) probably con- cerns 
<taxonomicName id="89B7B884FFC44C43FF0E34C6334CB48D" authorityName="A. Hoffmann" authorityYear="1938" box="[217,356,1449,1475]" class="Insecta" family="Curculionidae" genus="Cionus" kingdom="Animalia" order="Coleoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="bremondi">
