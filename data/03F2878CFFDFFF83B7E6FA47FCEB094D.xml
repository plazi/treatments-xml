<taxonomicName id="4C5B4D19FFDEFF83B4F1FB82FC190B8E" box="[896,1015,1129,1155]" class="Reptilia" family="Emydidae" kingdom="Animalia" order="Testudines" pageId="19" pageNumber="20" phylum="Chordata" rank="family">Emydidae</taxonomicName>
