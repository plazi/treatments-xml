<subSubSection id="C370DCE4CF606F2CFF7CFF7ED2C2C5D6" pageId="10" pageNumber="27" type="etymology">
<paragraph id="8BD58F6FCF606F2CFF7CFF7ED2C2C5D6" blockId="10.[151,1438,151,402]" pageId="10" pageNumber="27">
<emphasis id="B91E537DCF606F2CFF7CFF7ED157C59B" bold="true" box="[197,354,151,177]" pageId="10" pageNumber="27">Etymology.</emphasis>
