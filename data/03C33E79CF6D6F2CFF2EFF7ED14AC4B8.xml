<taxonomicName id="4C6AF4ECCF606F2CFF2EFF54D10DC5FD" box="[151,312,189,215]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="27" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
