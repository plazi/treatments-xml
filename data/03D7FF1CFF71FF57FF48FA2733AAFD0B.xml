<taxonomicName id="4C7E3589FF71FF57FC2FF9F535F2FD43" authorityName="Olivier" authorityYear="1789" box="[935,1166,667,691]" class="Insecta" family="Melolonthidae" genus="Melolontha" kingdom="Animalia" order="Coleoptera" pageId="186" pageNumber="185" phylum="Arthropoda" rank="species" species="undata">
