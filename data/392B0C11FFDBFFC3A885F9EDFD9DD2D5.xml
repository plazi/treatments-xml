<taxonomicName id="7682C684FFDAFFC3A937F983FE1ED316" box="[293,486,1595,1617]" class="Insecta" family="Mantispidae" genus="Climaciella" kingdom="Animalia" order="Neuroptera" pageId="35" pageNumber="436" phylum="Arthropoda" rank="species" species="obtusa">
