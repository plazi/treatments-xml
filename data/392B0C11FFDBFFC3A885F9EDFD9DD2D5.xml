<number id="074AAF20FFDAFFC3A885F97DFF4BD399" box="[151,179,1733,1758]" pageId="35" pageNumber="436" value="50.0">50</number>
and 
<specimenCount id="A784768EFFDAFFC3A8FDF97EFEB8D399" box="[239,320,1734,1758]" pageId="35" pageNumber="436" type="generic">
<quantity id="767A10E2FFDAFFC3A8FDF97EFEC4D399" box="[239,316,1734,1758]" metricMagnitude="2" metricUnit="m" metricValue="4.0" pageId="35" pageNumber="436" unit="m" value="400.0">400 m</quantity>
.
</specimenCount>
