<taxonomicName id="BAD44D36FF5931BDEA6FF99BFE1D1023" authorityName="Heller" authorityYear="1878" box="[296,419,1544,1569]" class="Ascidiacea" family="Styelidae" genus="Polycarpa" kingdom="Animalia" order="Stolidobranchia" pageId="134" pageNumber="335" phylum="Chordata" rank="species" species="nigricans">
<emphasis id="4FA0EAA7FF5931BDEA6FF99BFE1D1023" box="[296,419,1544,1569]" italics="true" pageId="134" pageNumber="335">P. nigricans</emphasis>
</taxonomicName>
