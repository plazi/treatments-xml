<taxonomicName id="BAD44D36FF5931BDEBD0FA3BFE0E13C0" authority="Heller, 1878" authorityName="Heller" authorityYear="1878" box="[151,432,1448,1474]" class="Ascidiacea" family="Styelidae" genus="Polycarpa" kingdom="Animalia" order="Stolidobranchia" pageId="134" pageNumber="335" phylum="Chordata" rank="species" species="nigricans">
