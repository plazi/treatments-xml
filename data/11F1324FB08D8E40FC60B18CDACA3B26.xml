1. 
<taxonomicName id="FA7DCD4AC86CE5E4D21D284F445135BE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29107" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName="Formica integra Nylander" lsidName-HNS="Formica integra Nylander" order="Hymenoptera" pageNumber="701" phylum="Arthropoda" rank="species" species="integra">F. integra</taxonomicName>
