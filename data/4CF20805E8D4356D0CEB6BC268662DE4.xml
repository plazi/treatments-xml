<geoCoordinate id="8C0C8A77A501285528C3F061DA441546" direction="north" orientation="latitude" precision="5" value="36.3">36.3000°N</geoCoordinate>
, 
<geoCoordinate id="799C8C5D13B62B1FFFAB13B8AC87D579" direction="east" orientation="longitude" precision="5" value="59.5167">59.5167°E</geoCoordinate>
