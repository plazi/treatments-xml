<taxonomicName id="F6BEBE86263E504A2B972CDA92C66F0F" authority="Keifer, 1944" authorityName="Keifer" authorityYear="1944" class="Arachnida" family="Eriophyidae" genus="Aceria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aceria pulicaris" order="Trombidiformes" pageId="6" pageNumber="29" phylum="Arthropoda" rank="species" species="pulicaris">Aceria pulicaris</taxonomicName>
