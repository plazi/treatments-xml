<quantity id="4CEF9B25FFA3FF879B00FEB9F9963160" box="[893,983,331,357]" metricMagnitude="-1" metricUnit="m" metricValue="1.24" pageId="50" pageNumber="51" unit="cm" value="12.4">12.4 cm</quantity>
(
<quantity id="4CEF9B25FFA3FF879B9AFEB8FE523163" box="[999,1043,330,358]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="50" pageNumber="51" unit="in" value="5.0">5 in</quantity>
