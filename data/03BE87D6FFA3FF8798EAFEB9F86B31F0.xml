<quantity id="4CEF9B25FFA3FF879B98FE44FE0531D4" box="[997,1092,438,465]" metricMagnitude="3" metricUnit="m" metricValue="1.75" pageId="50" pageNumber="51" unit="m" value="1750.0">1,750 m</quantity>
(
<quantity id="4CEF9B25FFA3FF879C2BFE44FEF431D7" box="[1110,1205,438,466]" metricMagnitude="3" metricUnit="m" metricValue="1.749552" pageId="50" pageNumber="51" unit="ft" value="5740.0">5,740 ft</quantity>
) (
<bibRefCitation id="EF864B31FFA3FF879CB3FE45FFD031D4" author="Kamikawa, D. J." box="[1230,1425,439,465]" pageId="50" pageNumber="51" refId="ref289096" refString="Kamikawa, D. J. (2017) Survey fishes: an illustrated list of the fishes captured during the Northwest Fisheries Science Center's Fishery Resource Analysis and Monitoring Division's West Coast Surveys. NOAA Technical Memoradum, NMFS-NWFSC- 138." type="journal volume" year="2017">Kamikawa 2017</bibRefCitation>
