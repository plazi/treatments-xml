<taxonomicName id="4C174D43FFA3FF8799DDFE29F86231F1" box="[416,547,475,500]" class="Actinopterygii" family="Platytroctidae" genus="Mirorictus" kingdom="Animalia" order="Osmeriformes" pageId="50" pageNumber="51" phylum="Chordata" rank="species" species="taaningi">
