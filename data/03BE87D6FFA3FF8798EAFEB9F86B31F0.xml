<geoCoordinate id="EE235007FFA3FF879AC6FE9CF970318C" box="[699,817,366,393]" degrees="124" direction="west" minutes="52" orientation="longitude" pageId="50" pageNumber="51" precision="925" value="-124.86667">124°52’W</geoCoordinate>
