<geoCoordinate id="EE235007FFA3FF879A36FE9DF8EF318C" box="[587,686,367,393]" degrees="41" direction="north" minutes="17" orientation="latitude" pageId="50" pageNumber="51" precision="925" value="41.283333">41°17’N</geoCoordinate>
