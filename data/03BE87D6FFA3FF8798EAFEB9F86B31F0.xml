<geoCoordinate id="EE235007FFA3FF879A36FE9DF8EF318C" box="[587,686,367,393]" degrees="41" direction="north" minutes="17" orientation="latitude" pageId="50" pageNumber="51" precision="925" value="41.283333">41°17’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FFA3FF879AC6FE9CF970318C" box="[699,817,366,393]" degrees="124" direction="west" minutes="52" orientation="longitude" pageId="50" pageNumber="51" precision="925" value="-124.86667">124°52’W</geoCoordinate>
) (Personal communication: University of Washington, Burke Museum of Natural History and Culture Fish Collection, Seattle, Washington) to 
<collectingCountry id="F3007650FFA3FF879CCEFE61FEA831A8" box="[1203,1257,403,429]" name="Peru" pageId="50" pageNumber="51">Peru</collectingCountry>
