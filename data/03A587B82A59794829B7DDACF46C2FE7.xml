<collectingCountry id="F31B763E2A5979482BBDDCFDF5042FC3" box="[669,784,1977,2002]" name="Venezuela" pageId="6" pageNumber="7">Venezuela</collectingCountry>
and 
<collectingRegion id="49C8F84C2A5979482A66DCFEF58B2FC0" box="[838,927,1978,2001]" country="Panama" name="Panama" pageId="6" pageNumber="7">
<collectingCountry id="F31B763E2A5979482A66DCFEF58B2FC0" box="[838,927,1978,2001]" name="Panama" pageId="6" pageNumber="7">Panama</collectingCountry>
</collectingRegion>
