<typeStatus id="870385C85B680070FAA1FCF6FA477B58" box="[1390,1436,864,888]" pageId="100" pageNumber="336">type</typeStatus>
species 
<emphasis id="6ACCE7785B680070FC55FCE9FC347BB7" box="[922,1007,895,919]" italics="true" pageId="100" pageNumber="336">Medusa</emphasis>
<taxonomicName id="9FB840E95B680070FC34FCE9FAE67BB7" authority="Forsskal, 1775" authorityName="Forsskal" authorityYear="1775" box="[1019,1341,895,919]" class="Hydrozoa" family="Geryoniidae" genus="Geryonia" kingdom="Animalia" order="Limnomedusae" pageId="100" pageNumber="336" phylum="Cnidaria" rank="species" species="proboscidalis">
<emphasis id="6ACCE7785B680070FC34FCE9FB567BB7" box="[1019,1165,895,919]" italics="true" pageId="100" pageNumber="336">proboscidalis</emphasis>
<bibRefCitation id="3C29469B5B680070FB56FCE9FAE67BB7" author="Forsskal" box="[1177,1341,895,919]" firstAuthor="Forsskal" pageId="100" pageNumber="336" refId="ref68004" refString="Forsskal P. in: Niebuhr C. E. 1775. Descriptiones animalium avium, amphibiorium, piscium, insectorum, vermium; quae in itinere orientali observavit Petrus Forskal. Post mortem auctoris edidit Carsten Niebuhr. Molleri, KObenhavn, 164 pp. DOI: 10.5962 / bhl. title. 2154" type="book" year="1775">Forsskål, 1775</bibRefCitation>
</taxonomicName>
