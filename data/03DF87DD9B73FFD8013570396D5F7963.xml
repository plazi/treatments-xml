<taxonomicName id="4C764D489B72FFD801B473F16B2878B3" box="[393,552,724,750]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
