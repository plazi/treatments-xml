<taxonomicName id="73914D7AFFB6FFC7CDA613D2C1B4B6D1" authority="(Roelofs)" baseAuthorityName="Roelofs" box="[386,662,691,712]" class="Insecta" family="Curculionidae" genus="Curculio" kingdom="Animalia" order="Coleoptera" pageId="2" phylum="Arthropoda" rank="species" species="dentipes">
<emphasis id="86E5EAEBFFB6FFC7CDA613D2C111B6D1" bold="true" box="[386,563,691,712]" italics="true" pageId="2">Curculio dentipes</emphasis>
(Roelofs)
