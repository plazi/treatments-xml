<collectionCode id="1CA450D8237E3A7F8481B8A387AF9AB0" box="[695,755,1666,1690]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="47" pageNumber="2485" type="Museum">ZSM</collectionCode>
<specimenCode id="2A136066237E3A7F84CFB8A386229AB0" box="[761,894,1666,1690]" collectionCode="ZSM" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="47" pageNumber="2485" type="Museum">A20110052</specimenCode>
