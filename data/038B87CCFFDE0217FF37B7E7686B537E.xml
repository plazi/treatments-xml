<taxonomicName id="4C224D59FFDF0217FCC1B1196ED9537E" authorityName="Jałoszyński" authorityYear="2020" box="[865,987,1324,1349]" class="Insecta" family="Staphylinidae" genus="Venezolanoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="283" phylum="Arthropoda" rank="species" species="minimus">
