<paragraph id="5D60F851FF9DFFA3FF37F853FA79F876" blockId="21.[151,1436,1651,2025]" pageId="21" pageNumber="400">
<keyLead id="E62E5B64FF9DFFA3FF37F853FA79F876" pageId="21" pageNumber="400">
<number id="EB17EA76FF9DFFA3FF37F853FF41F853" box="[151,164,1974,1996]" pageId="21" pageNumber="400" value="2.0">2</number>
