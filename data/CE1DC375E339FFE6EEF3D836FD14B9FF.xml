<quantity id="814CDF86E339FFE7E82ED8D9FA4DBBE6" box="[1368,1432,953,972]" metricMagnitude="3" metricUnit="m" metricValue="2.23" pageId="6" pageNumber="207" unit="m" value="2230.0">2230 m</quantity>
