<geoCoordinate id="238014A4E339FFE7E969D8D9FB56BBE6" box="[1055,1155,953,972]" degrees="89" direction="east" minutes="36" orientation="longitude" pageId="6" pageNumber="207" precision="15" seconds="44" value="89.61222">89°36′44″E</geoCoordinate>
