<taxonomicName id="6AF94D70FFE149221947FE9EFB732087" authority="Tian &amp; Huang &amp; Li, 2021" authorityName="Tian &amp; Huang &amp; Li" authorityYear="2021" box="[1081,1265,1210,1235]" class="Insecta" family="Carabidae" genus="Xiangxius" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="530" phylum="Arthropoda" rank="species" species="jiangi" status="n. sp.">
