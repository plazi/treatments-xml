<paragraph id="8BDCFC16FF09BDF1FF23393AFD1F516F" blockId="153.[151,1437,153,359]" pageId="153" pageNumber="154">
<emphasis id="B9172004FF09BDF1FF23393AFEBD514B" bold="true" box="[199,317,297,323]" pageId="153" pageNumber="154">Remarks:</emphasis>
<taxonomicName id="4C638795FF09BDF1FEA2393AFD85514B" box="[326,517,297,323]" class="Insecta" family="Chrysomelidae" genus="Chaetocnema" kingdom="Animalia" order="Coleoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="eastafghanica">
