<subSubSection id="C3196557FFB8FFECFFAEFD8EFB3A4A73" pageId="83" pageNumber="82" type="description">
<paragraph id="8BBC36DCFFB8FFECFFAEFD8EFE634A3E" blockId="83.[113,666,276,610]" box="[113,384,525,546]" pageId="83" pageNumber="82">
<emphasis id="B977EACEFFB8FFECFFAEFD8EFEC04A3D" bold="true" box="[113,291,525,545]" pageId="83" pageNumber="82">Species group:</emphasis>
<taxonomicName id="4C034D5FFFB8FFECFEF5FD8EFE9A4A3E" baseAuthorityName="Dana" baseAuthorityYear="1846" box="[298,377,525,546]" class="Anthozoa" family="Acroporidae" genus="Acropora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="83" pageNumber="82" phylum="Cnidaria" rank="species" species="horrida">
