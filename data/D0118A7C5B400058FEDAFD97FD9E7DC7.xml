<taxonomicName id="9FB840E95B400058FEAAFD97FDAE7A39" authority="Lamouroux, 1812" authorityName="Lamouroux" authorityYear="1812" box="[357,629,513,537]" class="Hydrozoa" family="Campanulariidae" genus="Clytia" kingdom="Animalia" order="Leptothecata" pageId="76" pageNumber="312" phylum="Cnidaria" rank="genus">
