<heading id="D0F081FCFFA6D55BFF40205AFD9B5C4F" bold="true" box="[151,581,333,360]" fontSize="10" level="2" pageId="27" pageNumber="228" reason="2">
<taxonomicName id="4C074D13FFA6D55BFF40205AFD9B5C4F" authority="(Verrill 1899)" baseAuthorityName="Verrill" baseAuthorityYear="1899" box="[151,581,333,360]" class="Asteroidea" family="Goniasteridae" genus="Gilbertaster" kingdom="Animalia" order="Valvatida" pageId="27" pageNumber="228" phylum="Echinodermata" rank="species" species="caribaea">
