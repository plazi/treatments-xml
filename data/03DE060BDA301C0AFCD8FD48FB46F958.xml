<taxonomicName id="4C77CC9EDA311C0AFEA9FAA8FE00FA9F" authorityName="Irmler" authorityYear="2019" box="[269,428,1354,1375]" class="Insecta" family="Staphylinidae" genus="Eleusis" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="76" phylum="Arthropoda" rank="species" species="mazureki">
