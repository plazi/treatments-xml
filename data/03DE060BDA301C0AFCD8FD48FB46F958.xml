<taxonomicName id="4C77CC9EDA311C0AFCB0FA85FC1FFABC" authorityName="Irmler" authorityYear="2019" box="[788,947,1383,1404]" class="Insecta" family="Staphylinidae" genus="Eleusis" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="76" phylum="Arthropoda" rank="species" species="mazureki">
