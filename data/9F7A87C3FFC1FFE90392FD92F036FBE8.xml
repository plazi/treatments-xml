. 
<taxonomicName id="D0D34D56FFC1FFE902DEFB8FF3AEFB84" baseAuthorityName="Vachal" baseAuthorityYear="1895" box="[475,729,1053,1079]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="223" phylum="Arthropoda" rank="species" species="medinai">
