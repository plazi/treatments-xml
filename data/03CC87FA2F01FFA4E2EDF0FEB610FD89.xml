<quantity id="4C9D9B092F00FFA4E027F7CDB6E4FE45" box="[605,742,404,428]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" metricValueMax="2.4" metricValueMin="2.1" pageId="3" pageNumber="197" unit="mm" value="2.25" valueMax="2.4" valueMin="2.1">2.1–2.4 mm</quantity>
, females 
<quantity id="4C9D9B092F00FFA4E12AF7CDB7D8FE44" box="[848,986,404,429]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" metricValueMax="2.5" metricValueMin="2.3" pageId="3" pageNumber="197" unit="mm" value="2.4" valueMax="2.5" valueMin="2.3">2.3– 2.5mm</quantity>
