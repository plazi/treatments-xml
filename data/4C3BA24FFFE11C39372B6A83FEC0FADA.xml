<typeStatus id="1B29ADFBFFE11C39372B6D1EFE99FB98" box="[264,317,1060,1083]" pageId="1" pageNumber="30">Type</typeStatus>
Species. 
<taxonomicName id="039268DAFFE11C3937806D1EFC81FB98" authority="Meyrick, 1922" authorityName="Meyrick" authorityYear="1922" box="[419,805,1060,1083]" class="Insecta" family="Tortricidae" genus="Acroclita" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="30" phylum="Arthropoda" rank="species" species="trimelaena">
<emphasis id="F6E6CF4BFFE11C3937806D1EFDDBFB98" box="[419,639,1060,1083]" italics="true" pageId="1" pageNumber="30">Acroclita trimelaena</emphasis>
<bibRefCitation id="A0036EA8FFE11C3934A66D1EFC81FB98" author="Meyrick" box="[645,805,1060,1083]" pageId="1" pageNumber="35" refString="Meyrick, E. (1922) New Microlepidoptera. Zoologische Mededelingen, 7, 80 - 89." type="journal article" year="1922">
Meyrick, 
<number id="725A017EFFE11C3934CE6D1EFC81FB98" box="[749,805,1060,1083]" pageId="1" pageNumber="30" value="1922.0">1922</number>
</bibRefCitation>
