<subSubSection id="D5A98B7C4F59D9E86B48D2705C520543" pageId="6" pageNumber="103" type="typification">
<paragraph id="DE0785408D4C2F6475F33BB8D6ECBC81" pageId="6" pageNumber="103">Typification.</paragraph>
<paragraph id="1AFFDD3F77537F3268A77BACD58350B8" pageId="6" pageNumber="103">
BRAZIL. Santa Catarina, 
<normalizedToken id="236067DB1AFE499615E0350D5590D5D0" originalValue="Florianópolis">Florianopolis</normalizedToken>
, (
<geoCoordinate id="4B1F160222987AACE5D2D86D5ADE98C4" direction="south" orientation="latitude" precision="1" value="-27.4698">27°28'11.28&quot;S</geoCoordinate>
, 
<geoCoordinate id="4A03ADB91B95C1E36AAB5EC67D1A789B" direction="west" orientation="longitude" precision="1" value="-48.377632">48°22'39.48&quot;W</geoCoordinate>
