<geoCoordinate id="110591AF90245A8DE628A6E4F391C455" direction="south" orientation="latitude" precision="1" value="-9.822984">09°49'22.74&quot;S</geoCoordinate>
, 
<geoCoordinate id="B5B5AFB9A9AA79C6E6B60506BF8177B1" direction="west" orientation="longitude" precision="1" value="-58.2589">58°15'32.04&quot;W</geoCoordinate>
