<taxonomicName id="66AB4D7AFFAAFFBA8E5FFC37FF25A0F5" class="Amphibia" family="Ceratophryidae" genus="Ceratophrys" kingdom="Animalia" order="Anura" pageId="78" pageNumber="79" phylum="Chordata" rank="species" species="dorsata">
<emphasis id="93DFEAEBFFAAFFBA8E5FFC37FF25A0F5" italics="true" pageId="78" pageNumber="79">C. dorsata</emphasis>
</taxonomicName>
