<taxonomicName id="66AB4D7AFFAAFFBA8CAAFCD5FEF2A0AE" box="[129,257,889,912]" class="Amphibia" family="Ceratophryidae" genus="Ceratophrys" kingdom="Animalia" order="Anura" pageId="78" pageNumber="79" phylum="Chordata" rank="species" species="dorsata">
