species: 
<taxonomicName id="17D65BB74941EA8A8333A3F6F8735D33" class="Insecta" family="Dytiscidae" genus="Exocelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exocelina ferruginea" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="ferruginea">Exocelina ferruginea</taxonomicName>
(A), 
<taxonomicName id="E883B87CA23E279A4A5A8F6C3C5DD9FF" class="Insecta" family="Dytiscidae" genus="Exocelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exocelina punctipennis" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="punctipennis">Exocelina punctipennis</taxonomicName>
(B), 
<taxonomicName id="8D6C6B38F439A98C1AE499A40E5C64C1" class="Insecta" family="Dytiscidae" genus="Exocelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exocelina saltusholmesensis" order="Coleoptera" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="saltusholmesensis">Exocelina saltusholmesensis</taxonomicName>
