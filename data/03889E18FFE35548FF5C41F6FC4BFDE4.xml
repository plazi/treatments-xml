<taxonomicName id="4C21548DFFE15548FDEB40D3FDFDFE1B" box="[544,635,434,457]" class="Arachnida" family="Chthoniidae" genus="Ephippiochthonius" kingdom="Animalia" order="Pseudoscorpiones" pageId="94" pageNumber="95" phylum="Arthropoda" rank="species" species="sevai">
