<emphasis id="B95FEA9ED2236E5AFED26389727DFA45" bold="true" box="[264,865,1470,1496]" pageId="4" pageNumber="51">
<taxonomicName id="4C2B4D0FD2236E5AFED26389720DFA45" authority="Evans and Dessart" authorityName="Evans and Dessart" authorityYear="2005" box="[264,785,1470,1496]" class="Insecta" family="Ceraphronidae" genus="Aphanogmus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="51" phylum="Arthropoda" rank="species" species="inamicus" status="n. sp.">
<emphasis id="B95FEA9ED2236E5AFED263897303FA45" bold="true" box="[264,543,1470,1496]" italics="true" pageId="4" pageNumber="51">Aphanogmus inamicus</emphasis>
Evans and Dessart
</taxonomicName>
, 
<taxonomicNameLabel id="A26C57E5D2236E5AFCC46389727DFA45" box="[798,865,1470,1496]" pageId="4" pageNumber="51" rank="species">n. sp.</taxonomicNameLabel>
