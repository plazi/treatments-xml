<paragraph id="485136C2FF2EFF05FF07FB98640FCFE6" blockId="199.[151,1437,474,1185]" pageId="199" pageNumber="200">
<emphasis id="7A9AEAD0FF2EFF05FF07FB98659BCF1E" bold="true" box="[151,216,1087,1113]" pageId="199" pageNumber="200">Note:</emphasis>
although 
<taxonomicName id="8FEE4D41FF2EFF05FEC0FB9867C5CF1E" authorityName="Nuttall and Warburton" authorityYear="1908" box="[336,646,1087,1113]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="wellingtoni">
