<geoCoordinate id="827C6F89CC229960DE58F0F0A0127F8F" direction="north" orientation="latitude" precision="5" value="45.6741">45.6741°N</geoCoordinate>
, 
<geoCoordinate id="AAFAF2BA41C031497BF964C357FC55A2" direction="west" orientation="longitude" precision="5" value="-66.8661">66.8661°W</geoCoordinate>
