<taxonomicName id="4C364D20FFEC4700FD5DF972FCBEF96B" authority="Guerin" authorityName="Guerin" box="[722,884,1706,1728]" class="Insecta" family="Anthribidae" genus="Acanthopygus" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="139" phylum="Arthropoda" rank="species" species="griseus">
