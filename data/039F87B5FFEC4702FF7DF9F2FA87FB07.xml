<taxonomicName id="4C364D20FFEC4700FF5BF973FD86F914" authority="Guerin-Meneville 1841: 126" authorityName="Guerin-Meneville" authorityPageNumber="126" authorityYear="1841" box="[212,588,1705,1728]" class="Insecta" family="Curculionidae" genus="Elytrogonus" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="139" phylum="Arthropoda" rank="genus">
