<taxonomicName id="4C0E4D68580EFFE1FD2D13D3EB58FE19" box="[709,770,439,462]" class="Insecta" family="Aradidae" genus="Vietnamaptera" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="tertia" status="n. sp.">
