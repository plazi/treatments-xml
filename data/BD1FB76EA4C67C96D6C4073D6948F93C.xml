<normalizedToken id="AF9E4A92F28678BA561DB3073E10C4F5" originalValue="Blätter" pageId="null" pageNumber="574">Blaetter</normalizedToken>
nicht 
<normalizedToken id="EF7E36747A2AE7849D910201D4E2E6C1" originalValue="über" pageId="null" pageNumber="574">ueber</normalizedToken>
4 mm breit, auffallend 
<normalizedToken id="BE6AF45A937CC29B7F9350DF7728D5FD" originalValue="schmäler" pageId="null" pageNumber="574">schmaeler</normalizedToken>
als das breiteste Stengelblatt oder Hochblatt.
</td>
</tr>
<tr id="E0470BFE370C4337D62BF63197B4AF67" pageId="null" pageNumber="574">
<td id="890972AFBFF21BCE02549C48053F918D" align="left" pageId="null" pageNumber="574" valign="top">
2. 
<normalizedToken id="3755384912FD384070C17B762A05FB67" originalValue="Perigonblätter" pageId="null" pageNumber="574">Perigonblaetter</normalizedToken>
stumpf, 
<normalizedToken id="E2E2F172359D00D0E5E53297A1D55466" originalValue="außerseits" pageId="null" pageNumber="574">ausserseits</normalizedToken>
kahl; 
<normalizedToken id="F17308C89A72875A08D51FB79250CA9D" originalValue="grundständige" pageId="null" pageNumber="574">grundstaendige</normalizedToken>
<normalizedToken id="18A2F2DC6EC5C2774BADD0C44A1CF3C2" originalValue="Blätter" pageId="null" pageNumber="574">Blaetter</normalizedToken>
im Querschnitt 
<normalizedToken id="2DD31B14A163FE899FA862A7AD2FA62C" originalValue="halbkreisförmig" pageId="null" pageNumber="574">halbkreisfoermig</normalizedToken>
