<taxonomicName box="[567,690,584,609]" class="Insecta" family="Coenagrionidae" genus="Telebasis" kingdom="Animalia" order="Odonata" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="racenisi">
