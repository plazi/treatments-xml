<subSubSection pageId="9" pageNumber="120" type="type species">
<paragraph pageId="9" pageNumber="120">Type species.</paragraph>
<paragraph pageId="9" pageNumber="120">
<taxonomicName class="Insecta" family="Permulidae" genus="Permula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Permula acra" order="Grylloblattodea" pageId="9" pageNumber="120" phylum="Arthropoda" rank="species" species="acra">Permula acra</taxonomicName>
<normalizedToken originalValue="Kukalová">Kukalova</normalizedToken>
