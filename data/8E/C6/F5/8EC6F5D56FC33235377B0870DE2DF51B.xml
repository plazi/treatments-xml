<subSubSection pageId="6" pageNumber="91" type="nomenclature">
<paragraph pageId="6" pageNumber="91">
<taxonomicName LSID="http://zoobank.org/C65B4812-FEBA-4BF0-B70C-882B5E8BC7A0" class="Gastropoda" family="Moitessieriidae" genus="Paladilhiopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paladilhiopsis falniowskii" order="Littorinimorpha" pageId="6" pageNumber="91" phylum="Mollusca" rank="species" species="falniowskii">
