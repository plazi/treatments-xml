<taxonomicName baseAuthorityName="Comstock" baseAuthorityYear="1883" box="[151,385,1847,1872]" class="Insecta" family="Diaspididae" genus="Melanaspis" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="smilacis">
