<caption httpUri="https://zenodo.org/record/858455/files/figure.png" pageId="16" pageNumber="17" targetBox="[171,1419,216,1619]" targetPageId="16">
<paragraph blockId="16.[151,1436,1662,1776]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[151,346,1662,1684]" pageId="16" pageNumber="17">FIGURES 21–26.</emphasis>
<taxonomicName authorityName="Ashmead" authorityYear="1900" box="[359,574,1662,1684]" class="Insecta" family="Signiphoridae" genus="Signiphora" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="aspidioti">
