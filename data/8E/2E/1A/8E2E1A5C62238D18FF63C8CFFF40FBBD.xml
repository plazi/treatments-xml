); middle Danian (
<taxonomicName authority="Zone" authorityName="Zone" class="Echinoidea" family="Psychocidaridae" genus="Tylocidaris" kingdom="Animalia" order="Cidaroida" pageId="7" pageNumber="40" phylum="Echinodermata" rank="species" species="bruennichi">
