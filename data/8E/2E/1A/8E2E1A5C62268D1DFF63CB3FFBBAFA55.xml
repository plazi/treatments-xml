<subSubSection pageId="2" pageNumber="35" type="materials_examined">
<paragraph blockId="2.[151,1436,946,1452]" pageId="2" pageNumber="35">
<emphasis bold="true" box="[198,265,1026,1052]" pageId="2" pageNumber="35">
<typeStatus box="[198,257,1026,1052]" pageId="2" pageNumber="35">Type</typeStatus>
