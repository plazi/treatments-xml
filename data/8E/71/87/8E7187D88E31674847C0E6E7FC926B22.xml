Dautzenberg, 
<number box="[1003,1060,1065,1089]" pageId="14" pageNumber="15" value="1889.0">1889</number>
</bibRefCitation>
; 
<bibRefCitation author="Dautzenberg" box="[1082,1431,1065,1089]" pageId="14" pageNumber="15" refString="Dautzenberg, P. &amp; Fischer, H. (1896) Dragages effectues par l' Hirondelle et par le Princess-Alice, 1888 - 1895. Memoires de la Societe Zoologique de France, 9, 395 - 498, pls. 15 - 22." type="journal article" year="1896">
Dautzenberg &amp; Fischer, 
<number box="[1375,1431,1065,1089]" pageId="14" pageNumber="15" value="1896.0">1896</number>
</bibRefCitation>
; 
