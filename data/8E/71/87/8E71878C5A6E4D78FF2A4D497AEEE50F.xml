<paragraph blockId="88.[93,620,1440,1759]" pageId="88" pageNumber="88">
<materialsCitation accessionNumber="OON 15747" collectionCode="PBI" pageId="88" pageNumber="88" specimenCount="1" typeStatus="paratype">
Female (
<typeStatus box="[226,332,1440,1461]" pageId="88" pageNumber="88">paratype</typeStatus>
, 
<collectionCode box="[348,393,1440,1461]" pageId="88" pageNumber="88">PBI</collectionCode>
