<collectingRegion box="[1071,1299,404,429]" country="Argentina" name="Rio Negro" pageId="25" pageNumber="26">Rio Negro Province</collectingRegion>
) at altitudes about 
<specimenCount box="[220,298,440,464]" pageId="25" pageNumber="26" type="generic">
<quantity box="[220,294,440,464]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="25" pageNumber="26" unit="m" value="800.0">800 m</quantity>
