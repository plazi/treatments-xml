Figs 
<number box="[735,805,1518,1543]" pageId="16" pageNumber="95" value="45.0" valueMax="46.0" valueMin="44.0">44–46</number>
</figureCitation>
and 
<figureCitation box="[859,1030,1518,1543]" captionStart="FIGURES 70 – 78" captionStartId="17.[151,264,1954,1977]" captionTargetBox="[211,1398,202,1921]" captionTargetId="figure@17.[183,1403,193,1933]" captionTargetPageId="17" captionText="FIGURES 70 – 78. Pisacha baculiformis sp. nov. 70. adult, dorsal view; 71. adult, lateral view; 72. head and thorax, dorsal view; 73. frons and clypeus, ventral view. 74. male genitalia, left view; 75. aedeagus, dorsal view; 76. aedeagus, ventral view; 77. male anal tube, dorsal view; 78. aedeagus, lateral view;" httpUri="https://zenodo.org/record/250356/files/figure.png" pageId="16" pageNumber="95">
Figs 
<number box="[915,987,1518,1543]" pageId="16" pageNumber="95" value="75.76">75, 76</number>
, 
