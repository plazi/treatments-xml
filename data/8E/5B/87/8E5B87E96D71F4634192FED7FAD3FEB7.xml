(
<figureCitation box="[159,242,386,411]" captionStart="FIGURE 16" captionStartId="21.[151,250,1957,1979]" captionTargetBox="[340,1247,1294,1955]" captionTargetId="figure@21.[340,1248,1294,1967]" captionTargetPageId="21" captionText="FIGURE 16. Plumarius mikrommatus sp. nov., ♂. A, Head, frontal view; B, Head, ventral view; C, Head, lateral view; D, Genital capsule, ventral view. Scale bars = 0.5 mm." httpUri="https://zenodo.org/record/257593/files/figure.png" pageId="21" pageNumber="463">
Figs 
<number box="[215,242,386,411]" pageId="21" pageNumber="463" value="16.0">16</number>
</figureCitation>
<collectionCode box="[242,263,386,411]" pageId="21" pageNumber="463">A</collectionCode>
– 
