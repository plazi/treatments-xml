<taxonomicName class="Dicotyledoneae" family="Betulaceae" genus="Corylus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Corylus heterophylla" order="Fagales" pageId="0" pageNumber="19" phylum="Angiospermae" rank="species" species="heterophylla">
