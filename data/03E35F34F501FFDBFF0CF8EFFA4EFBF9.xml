</taxonomicName>
and 
<taxonomicName id="4C4A95A1F501FFDAFA86F8B3FE61F804" authority="Distant" authorityName="Distant" class="Insecta" family="Cicadellidae" genus="Mimotettix" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="hieroglyphicus">
