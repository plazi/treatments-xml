<heading id="D0D38183FFD7981C7120FEDCD52FFEFF" bold="true" box="[264,564,283,310]" fontSize="11" level="1" pageId="10" pageNumber="65" reason="1">
<emphasis id="B950EAFDFFD7981C7120FEDCD52FFEFF" bold="true" box="[264,564,283,310]" pageId="10" pageNumber="65">
<taxonomicName id="4C244D6CFFD7981C7120FEDCD6CBFEFC" box="[264,464,284,310]" class="Insecta" family="Cupedidae" genus="Priacma" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="65" phylum="Arthropoda" rank="species" species="renaria" status="sp. nov.">
<emphasis id="B950EAFDFFD7981C7120FEDCD6CBFEFC" bold="true" box="[264,464,284,310]" italics="true" pageId="10" pageNumber="65">Priacma renaria</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2635786FFD7981C71FFFEDBD52FFEFF" box="[471,564,283,309]" pageId="10" pageNumber="65" rank="species">sp. nov.</taxonomicNameLabel>
