<taxonomicName id="ABC589CE223DFA573F1CF8B92ACFF89A" authority="Brandt, 2007" authorityName="Brandt" authorityYear="2007" box="[291,502,1834,1853]" class="Malacostraca" family="Antarcturidae" genus="Fissarcturus" kingdom="Animalia" order="Isopoda" pageId="25" pageNumber="288" phylum="Arthropoda" rank="species" species="rossi" status="sp. nov.">
