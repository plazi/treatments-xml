<geoCoordinate id="D2A9D5F3AD37C289EED65022C3BA6535" direction="north" orientation="latitude" precision="555" value="71.31">71.31°N</geoCoordinate>
, 
<geoCoordinate id="D02D07E0683C469ECF8DA82533D1E962" direction="west" orientation="longitude" precision="555" value="-156.66">156.66°W</geoCoordinate>
, 30.VIII.1976, thick moss, some algae and lichens; ibidem, moss, 
<taxonomicName id="AA5106E97EED81C6528BA5E876F2A9D6" class="Magnoliopsida" family="Saxifragaceae" genus="Saxifraga" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Saxifraga" order="Saxifragales" pageId="21" pageNumber="52" phylum="Tracheophyta" rank="genus">Saxifraga</taxonomicName>
sp., 
<taxonomicName id="5EB9DC90CC2F5854011357C6152A9AC7" class="Magnoliopsida" family="Brassicaceae" genus="Cochlearia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cochlearia" order="Brassicales" pageId="21" pageNumber="52" phylum="Tracheophyta" rank="genus">Cochlearia</taxonomicName>
