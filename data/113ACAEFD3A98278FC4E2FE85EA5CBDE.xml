<taxonomicName id="810D14350ED24A54129C4EF8F503B058" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="20" pageNumber="51" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
sp., leg. S. Ma 
<pageBreakToken id="ABCFEAAFA4155AAE75DBF69268678D21" pageId="21" pageNumber="52" start="start">cLean</pageBreakToken>
; Alaska, Norton Bay, Inglutalik River, moist tundra, leg. A.F.; Alaska, Nunivak Island, Duchikthluk Bay, 
<geoCoordinate id="2B7DC50810EB132AB1321C563529B440" direction="north" orientation="latitude" precision="555" value="59.86">59.86°N</geoCoordinate>
, 
<geoCoordinate id="F5B75A7DA418A809420D5C2EED245FC6" direction="west" orientation="longitude" precision="555" value="-166.07">166.07°W</geoCoordinate>
, 19.IX.1976 tundra with 
<taxonomicName id="ED6A4B786E0CDE7DA301D00D05CDD347" class="Magnoliopsida" family="Ericaceae" genus="Empetrum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Empetrum" order="Ericales" pageId="21" pageNumber="52" phylum="Tracheophyta" rank="genus">Empetrum</taxonomicName>
sp., 
<taxonomicName id="EBFF14FE3C2A5337E0A5F1A12E3A2D5D" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="21" pageNumber="52" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
sp., 
<taxonomicName id="6CF8339D4B35A28B80090D7B5B4F065E" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vaccinium" order="Ericales" pageId="21" pageNumber="52" phylum="Tracheophyta" rank="genus">Vaccinium</taxonomicName>
