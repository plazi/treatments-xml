<taxonomicName id="D05DC555191886A0F2094075E391CDC2" baseAuthorityName="Broderip" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="48" pageNumber="49" phylum="Mollusca" rank="species" species="derelictus">
