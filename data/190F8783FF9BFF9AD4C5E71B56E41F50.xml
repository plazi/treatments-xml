<taxonomicName id="56A64D16FF9BFF9AD4C5E7E155CA1DCA" authority="Bagnall, 1924: 633" authorityName="Bagnall" authorityPageNumber="633" authorityYear="1924" box="[151,648,438,461]" class="Insecta" family="Thysanoptera (awaiting allocation)" genus="Phrasterothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="omercooperi">
<emphasis id="A3D2EA87FF9BFF9AD4C5E7E156FF1DCB" box="[151,445,438,461]" italics="true" pageId="71" pageNumber="72">Phrasterothrips omercooperi</emphasis>
