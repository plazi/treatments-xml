<taxonomicName id="4C494D14614DFE6AFB879F6B5EED2F0E" box="[1052,1179,1592,1616]" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="194" phylum="Arthropoda" rank="genus">
<emphasis id="B93DEA85614DFE6AFB879F6B5EED2F0E" box="[1052,1179,1592,1616]" italics="true" pageId="2" pageNumber="194">Carpophilus</emphasis>
</taxonomicName>
. A) 
<taxonomicName id="4C494D14614DFE6AFB519F6B5A8F2F2A" authority="Powell &amp; Hamilton" authorityName="Powell &amp; Hamilton" authorityYear="2019" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="194" phylum="Arthropoda" rank="species" species="jarijari" status="sp. nov.">
<emphasis id="B93DEA85614DFE6AFB519F6B5F442F0E" box="[1226,1330,1592,1616]" italics="true" pageId="2" pageNumber="194">C. jarijari</emphasis>
Powell &amp; Hamilton
</taxonomicName>
, 
<emphasis id="B93DEA85614DFE6AFE9F9F0F5B222F2A" bold="true" box="[260,340,1628,1652]" pageId="2" pageNumber="194">
<taxonomicNameLabel id="A20E57FE614DFE6AFE9F9F0F5B222F2A" box="[260,340,1628,1652]" pageId="2" pageNumber="194" rank="species">sp. nov.</taxonomicNameLabel>
