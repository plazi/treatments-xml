<taxonomicName id="4C494D14614DFE6AFE829D3658C32D23" authority="Powell &amp; Hamilton" authorityName="Powell &amp; Hamilton" authorityYear="2019" box="[281,693,1124,1149]" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="194" phylum="Arthropoda" rank="species" species="jarijari" status="sp. nov.">
<emphasis id="B93DEA85614DFE6AFE829D365B912D23" box="[281,487,1125,1149]" italics="true" pageId="2" pageNumber="194">Carpophilus jarijari</emphasis>
