<paragraph id="8B8136DBFF88CD31FF1C442FFE504C93" blockId="6.[151,1437,682,1723]" pageId="6" pageNumber="7">
<emphasis id="B94AEAC9FF88CD31FF1C442FFEA14C6A" bold="true" box="[198,314,1619,1645]" pageId="6" pageNumber="7">Affinities</emphasis>
