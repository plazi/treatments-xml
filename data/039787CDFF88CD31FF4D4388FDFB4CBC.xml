<quantity id="4CC69B3EFF88CD31FE1A47A3FD864FFE" box="[448,541,1503,1529]" metricMagnitude="-3" metricUnit="m" metricValue="6.3" pageId="6" pageNumber="7" unit="mm" value="6.3">6.3 mm</quantity>
; TL-H: 6.0 mm; HL: 
<quantity id="4CC69B3EFF88CD31FCC147A3FCE34FFE" box="[795,888,1503,1529]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="6" pageNumber="7" unit="mm" value="0.8">0.8 mm</quantity>
; HW: 
<quantity id="4CC69B3EFF88CD31FC1947A3FBBB4FFE" box="[963,1056,1503,1529]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="6" pageNumber="7" unit="mm" value="1.8">1.8 mm</quantity>
; LP: 
<quantity id="4CC69B3EFF88CD31FB8447A3FB204FFE" box="[1118,1211,1503,1529]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="6" pageNumber="7" unit="mm" value="1.1">1.1 mm</quantity>
