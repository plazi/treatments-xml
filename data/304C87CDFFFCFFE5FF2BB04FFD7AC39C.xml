<taxonomicName id="7FE54D58FFFDFFE5FD84B719FD3EC51F" baseAuthorityName="Newstead" baseAuthorityYear="1896" box="[568,687,592,617]" class="Insecta" family="Margarodidae" genus="Gigantococcus" kingdom="Animalia" order="Hemiptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="ewarti">
