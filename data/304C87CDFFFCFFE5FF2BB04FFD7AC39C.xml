<collectionCode id="DEF4AE1EFFFDFFE5FCC2B7E9FC50C5CC" box="[894,961,672,698]" country="Germany" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="59" pageNumber="60" type="Museum">ZMB</collectionCode>
