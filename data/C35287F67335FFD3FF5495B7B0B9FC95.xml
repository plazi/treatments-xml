<quantity id="8C039B057337FFD3FF19926DB02FFCFB" box="[255,316,799,821]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="2" pageNumber="91" unit="mm" value="5.0">5 mm</quantity>
