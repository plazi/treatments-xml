<taxonomicName id="B145C98CB43FD2B362AB216F9AE481FD" class="Hexapoda" family="Carabidae" genus="Sphodrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="723" pageNumber="724" phylum="Arthropoda" rank="genus">
