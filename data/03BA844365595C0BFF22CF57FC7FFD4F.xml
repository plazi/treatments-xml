(Mollusca, 
<taxonomicName id="4C134ED665595C0BFC25C97DFBDEFF79" authorityName="Linnaeus" authorityYear="1758" box="[992,1050,115,133]" class="Bivalvia" kingdom="Animalia" pageId="2" pageNumber="637" phylum="Mollusca" rank="class">Bivalvia</taxonomicName>
