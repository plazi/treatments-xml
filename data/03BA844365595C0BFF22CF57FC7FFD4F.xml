<taxonomicName id="4C134ED665595C0BFEF8CF99FDF9F953" authority="Kuroda, 1929" authorityName="Kuroda" authorityYear="1929" box="[317,573,1687,1711]" class="Bivalvia" family="Cardiidae" genus="Cardium" kingdom="Animalia" order="Cardiida" pageId="2" pageNumber="637" phylum="Mollusca" rank="species" species="eos">
