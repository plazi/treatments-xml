<taxonomicName id="9B844D64FFEA6243D797FB19FD3EFAC6" authority="(Adelung, 1902)" authorityName="Adelung" authorityYear="1902" baseAuthorityName="Adelung" baseAuthorityYear="1902" box="[193,693,1261,1286]" class="Insecta" family="Rhaphidophoridae" genus="Diestrammena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="asynamora">
<emphasis id="6EF0EAF5FFEA6243D797FB19FE7AFAC5" bold="true" box="[193,497,1261,1286]" italics="true" pageId="44" pageNumber="45">Diestrammena asynamora</emphasis>
