<paragraph id="2A02BA1C2307F7CE3E561123592ECC5A" pageId="16" pageNumber="53">
<taxonomicName id="944544F73A10D3B6730A72D5E8607C15" genus="Thyrisomidae" pageId="16" pageNumber="53" rank="genus">Thyrisomidae Grandjean</taxonomicName>
