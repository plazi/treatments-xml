<emphasis id="EED8EAD5C161FF9B9FE90C1BFEFCFEA8" bold="true" box="[175,274,300,321]" pageId="13" pageNumber="244">Diagnosis.</emphasis>
<taxonomicName id="1BAC4D44C161FF9B9E590C1AFE55FEAB" baseAuthorityName="Boheman" baseAuthorityYear="1854" box="[287,443,301,322]" class="Insecta" family="Chrysomelidae" genus="Dorynota" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="244" phylum="Arthropoda" rank="species" species="nodosa">
