<taxonomicName id="1BAC4D44C161FF9B9EA50C1AFD90FEAB" baseAuthorityName="Wagener" baseAuthorityYear="1881" box="[483,638,301,322]" class="Insecta" family="Chrysomelidae" genus="Dorynota" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="244" phylum="Arthropoda" rank="species" species="rufomarginata">
