<date id="A9496FD2FFF6FF90FD096FDEFCF2F815" box="[758,877,1945,1971]" pageId="3" pageNumber="41" value="1924-05-14" valueMax="1924-05-07" valueMin="1924-05-14">
<number id="6B3F5B35FFF6FF90FD096FDEFC88F815" box="[758,791,1945,1971]" pageId="3" pageNumber="41" value="24.0">24</number>
