<date id="A9496FD2FFF6FF90FBD16FDEFB23F815" box="[1070,1212,1945,1971]" pageId="3" pageNumber="41" value="1932-06-05" valueMax="1932-11-03" valueMin="1932-06-05">
<number id="6B3F5B35FFF6FF90FBD16FDEFBCFF815" box="[1070,1104,1945,1971]" pageId="3" pageNumber="41" value="32.0">32</number>
