<taxonomicName id="39F14D722E5FFFFFE066B61EFDB2835E" authority="Mo &amp; Wang &amp; Yang &amp; Li, 2020" authorityName="Mo &amp; Wang &amp; Yang &amp; Li" authorityYear="2020" box="[276,547,1865,1887]" class="Insecta" family="Nemouridae" genus="Sphaeronemoura" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="448" phylum="Arthropoda" rank="species" species="acutispina" status="sp. n.">
<emphasis id="CC85EAE32E5FFFFFE066B61EFDB2835E" box="[276,547,1865,1887]" italics="true" pageId="1" pageNumber="448">Sphaeronemoura acutispina</emphasis>
</taxonomicName>
<emphasis id="CC85EAE32E5FFFFFE35BB61EFDF1835F" bold="true" box="[553,608,1865,1886]" pageId="1" pageNumber="448">
<taxonomicNameLabel id="D7B657982E5FFFFFE35BB61EFDF1835F" box="[553,608,1865,1886]" pageId="1" pageNumber="448" rank="species">sp. n.</taxonomicNameLabel>
