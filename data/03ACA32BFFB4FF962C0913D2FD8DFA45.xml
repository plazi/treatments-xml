<taxonomicName id="4C0569BEFFB4FF952C5C13C9FBBBFB7C" authority="Bonelli, 1810" authorityName="Bonelli" authorityYear="1810" box="[862,1101,1215,1236]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="17" phylum="Arthropoda" rank="tribe" tribe="Harpalini">Harpalini Bonelli, 1810</taxonomicName>
