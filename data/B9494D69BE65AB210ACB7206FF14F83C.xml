where trawled in 
<quantity id="F618519ABE65AB210F667474FF10F83C" metricMagnitude="2" metricUnit="m" metricValue="3.31" metricValueMax="3.5799999999999996" metricValueMin="3.04" pageId="80" pageNumber="81" unit="m" value="331.0" valueMax="358.0" valueMin="304.0">
