<collectingRegion id="F324329DBE65AB2109F6746BFBD3F818" box="[938,1139,1903,1928]" country="Australia" name="Western Australia" pageId="80" pageNumber="81">
<collectingRegion id="F324329DBE65AB2109F6746BFBA5F817" box="[938,1029,1904,1927]" country="American Samoa" name="Western" pageId="80" pageNumber="81">Western</collectingRegion>
