<taxonomicName id="4C0A4D14FFCAFFA3FECEFAB5FD7AC34D" box="[297,687,1374,1396]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="tansachai" status="n. sp." subGenus="Strobilophila">
