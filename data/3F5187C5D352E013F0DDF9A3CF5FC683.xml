<taxonomicName id="70F84D50D352E011F0DDF9A3CEF2C354" authority="Siddiqi &amp; Shahina, 2004" authorityName="Siddiqi &amp; Shahina" authorityYear="2004" box="[151,729,1567,1594]" class="Chromadorea" family="Monhysteridae" genus="Geomonhystera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monhysterida" pageId="21" pageNumber="97" phylum="Nematoda" rank="species" species="dubia">
