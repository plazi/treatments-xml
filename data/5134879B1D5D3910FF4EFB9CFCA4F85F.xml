<taxonomicName id="1E9D4D0E1D5D3910FF4EF87EFE63F85E" box="[151,434,2013,2036]" class="Insecta" family="Geometridae" genus="Prasinocyma" kingdom="Animalia" order="Lepidoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="trematerrai" status="sp. n.">
<emphasis id="EBE9EA9F1D5D3910FF4EF87EFE63F85E" box="[151,434,2013,2036]" italics="true" pageId="32" pageNumber="33">Prasinocyma trematerrai</emphasis>
</taxonomicName>
<taxonomicNameLabel id="F0DA57E41D5D3910FE60F87FFE29F85F" box="[441,504,2012,2037]" pageId="32" pageNumber="33" rank="species">
<emphasis id="EBE9EA9F1D5D3910FE60F87FFE29F85F" bold="true" box="[441,504,2012,2037]" pageId="32" pageNumber="33">sp. n.</emphasis>
</taxonomicNameLabel>
, and 
<taxonomicName id="1E9D4D0E1D5D3910FDECF87EFD36F85F" box="[565,743,2012,2037]" class="Insecta" family="Geometridae" genus="Prasinocyma" kingdom="Animalia" order="Lepidoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="tricolorifrons">
<emphasis id="EBE9EA9F1D5D3910FDECF87EFD36F85F" box="[565,743,2012,2037]" italics="true" pageId="32" pageNumber="33">P. tricolorifrons</emphasis>
</taxonomicName>
(see above).
</paragraph>
</subSubSection>
