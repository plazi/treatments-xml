<number id="3DA52491FF8AFF80FEB74EC1FEC6FAA9" box="[310,383,941,967]" pageId="4" pageNumber="25" value="25.0" valueMax="33.0" valueMin="17.0">17–33</number>
(
<number id="3DA52491FF8AFF80FE0E4EC1FE7CFAA9" box="[399,453,941,967]" pageId="4" pageNumber="25" value="22.9">22.9</number>
