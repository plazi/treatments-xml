<date id="FFD31076FF8AFF80FE544EC1FD8DFAA9" box="[469,564,941,967]" pageId="4" pageNumber="25" value="1917-10-30">
x 
<number id="3DA52491FF8AFF80FE6A4EC1FD8DFAA9" box="[491,564,941,967]" pageId="4" pageNumber="25" value="23.5" valueMax="30.0" valueMin="17.0">17–30</number>
</date>
(
<number id="3DA52491FF8AFF80FDC54EC1FDC3FAA9" box="[580,634,941,967]" pageId="4" pageNumber="25" value="22.1">22.1</number>
