<caption id="E5C7618F1C77FFDEF5B98412FBA4F8BE" httpUri="https://zenodo.org/record/226925/files/figure.png" pageId="3" pageNumber="594" targetBox="[163,1422,203,1791]" targetPageId="3">
<paragraph id="B10731071C77FFDEF5B98412FBA4F8BE" blockId="3.[151,1436,1820,1874]" pageId="3" pageNumber="594">
