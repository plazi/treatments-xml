<emphasis id="83CCED151C76FFDFF46C85F1FDF7F8F8" box="[322,543,1790,1812]" italics="true" pageId="2" pageNumber="593">
<taxonomicName id="76B84A841C76FFDFF46C85F1FDF2F8F8" box="[322,538,1790,1812]" class="Insecta" family="Perlidae" genus="Anacroneuria" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="593" phylum="Arthropoda" rank="species" species="debilis">Anacroneuria debilis</taxonomicName>
