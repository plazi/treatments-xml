<quantity id="0F559B5FFFC17B7D02CF854D0CB3F9D7" box="[1181,1271,1598,1625]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="6" pageNumber="224" unit="mm" value="2.9">2.9 mm</quantity>
