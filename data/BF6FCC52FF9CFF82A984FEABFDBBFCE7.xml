<taxonomicName id="F0C606C7FF9CFF82A8D3FEE2FCC7FE73" authority="(Peters, 1861)" authorityName="Peters" authorityYear="1861" baseAuthorityName="Peters" baseAuthorityYear="1861" box="[448,808,366,391]" class="Reptilia" family="Elapidae" genus="Notechis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="14" pageNumber="491" phylum="Chordata" rank="species" species="scutatus">
