<taxonomicName id="65C84D0BFFFAFF9DA29DDDC132DC4DA2" authorityName="Zicsi" authorityYear="1970" box="[691,916,899,923]" class="Clitellata" family="Lumbricidae" genus="Dendrobaena" kingdom="Animalia" order="Crassiclitellata" pageId="7" pageNumber="521" phylum="Annelida" rank="species" species="osellai">
