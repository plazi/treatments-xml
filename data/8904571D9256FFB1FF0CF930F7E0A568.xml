<collectingCountry id="79BAA69B9250FFB1FD3CFA71F631A54C" box="[679,774,1518,1544]" name="Pakistan" pageId="5" pageNumber="430">Pakistan</collectingCountry>
and 
<collectingCountry id="79BAA69B9250FFB1FCA6FA71F69AA54C" box="[829,941,1518,1544]" name="Indonesia" pageId="5" pageNumber="430">Indonesia</collectingCountry>
