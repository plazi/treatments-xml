</taxonomicName>
(= 
<taxonomicName id="C6AD9D889250FFB1FEE5FA71F755A54D" authority="(Hardy))" baseAuthorityName="Hardy" box="[382,610,1518,1545]" class="Insecta" family="Tephritidae" genus="Ceratitella" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="430" phylum="Arthropoda" rank="species" species="asiatica">
<emphasis id="33D93A199250FFB1FEE5FA71F4CFA54C" box="[382,504,1518,1544]" italics="true" pageId="5" pageNumber="430">C. asiatica</emphasis>
