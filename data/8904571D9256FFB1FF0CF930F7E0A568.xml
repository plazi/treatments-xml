<taxonomicName id="C6AD9D889250FFB1FF59FA71F461A54C" box="[194,342,1518,1544]" class="Insecta" family="Tephritidae" genus="Ceratitella" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="430" phylum="Arthropoda" rank="species" species="tomentosa">
