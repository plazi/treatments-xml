<emphasis id="B9090C53FFA0FFB7FF58FA5DFEB6FA2C" bold="true" box="[151,289,1475,1500]" pageId="26" pageNumber="477">FIGURE 18.</emphasis>
<taxonomicName id="4C7DABC2FFA0FFB7FEE6FA5AFDD6FA2C" authority="Park" authorityName="Park" authorityYear="2020" box="[297,577,1475,1500]" class="Insecta" family="Lecithoceridae" genus="Thubdora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="477" phylum="Arthropoda" rank="species" species="trigonoides" status="sp. nov.">
<emphasis id="B9090C53FFA0FFB7FEE6FA5AFD9FFA2C" box="[297,520,1476,1500]" italics="true" pageId="26" pageNumber="477">Thubdora trigonoides</emphasis>
Park
</taxonomicName>
