<emphasis id="B92BEAF3FFE3FFC1FF233297FBF15F6F" bold="true" box="[151,1043,1522,1549]" pageId="0" pageNumber="341">
<taxonomicName id="4C5F4D62FFE3FFC1FF233297FCC35F6F" authority="Wasbauer, Cambra &amp; Anino" authorityName="Wasbauer, Cambra &amp; Anino" box="[151,801,1522,1549]" class="Insecta" family="Pompilidae" genus="Priochilus" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="341" phylum="Arthropoda" rank="species" species="chrysopygus" status="sp. nov.">
<emphasis id="B92BEAF3FFE3FFC1FF233297FE505F6E" bold="true" box="[151,434,1522,1548]" italics="true" pageId="0" pageNumber="341">Priochilus chrysopygus</emphasis>
Wasbauer, Cambra &amp; Añino
</taxonomicName>
, 
<taxonomicNameLabel id="A2185788FFE3FFC1FC9A3296FC6E5F6F" box="[814,908,1523,1549]" pageId="0" pageNumber="341" rank="species">sp. nov.</taxonomicNameLabel>
