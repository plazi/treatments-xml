<taxonomicName id="85ECF49856141C705F880AC5DE72CA7F" authorityName="Zhao &amp; Li" authorityYear="2017" class="Arachnida" family="Telemidae" genus="Pinelema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pinelema qingfengensis" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="qingfengensis">Pinelema qingfengensis</taxonomicName>
