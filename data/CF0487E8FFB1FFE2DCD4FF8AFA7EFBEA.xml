<collectingCountry id="3FBA766EFFB1FFE2DCD4FEC9FED2FC49" box="[192,268,930,954]" name="Egypt" pageId="15" pageNumber="14">Egypt</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB1FFE2DD0FFEC9FE8DFC4A" box="[283,339,930,953]" name="Iran" pageId="15" pageNumber="14">Iran</collectingCountry>
, Arabian Peninsula, 
<collectingCountry id="3FBA766EFFB1FFE2DE49FEC9FD10FC49" box="[605,718,930,954]" name="Pakistan" pageId="15" pageNumber="14">Pakistan</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB1FFE2DEC9FEC9FCF9FC49" box="[733,807,930,954]" name="Nepal" pageId="15" pageNumber="14">Nepal</collectingCountry>
and 
<collectingCountry id="3FBA766EFFB1FFE2DF7CFEC9FC6DFC49" box="[872,947,930,954]" name="China" pageId="15" pageNumber="14">China</collectingCountry>
(
<bibRefCitation id="233C4B0FFFB1FFE2DFD2FEC9FB47FC49" author="Balthasar, V." box="[966,1177,930,954]" pageId="15" pageNumber="14" refId="ref16187" refString="Balthasar, V. 1963 b. Monographie der Scarabaeidae und Aphodiidae der palaearktischen und orientalischen Region. Coleoptera: Lamellicornia. Band 2. Coprinae (Onitini, Oniticellini, Onthophagini). Verlag der Tschechoslowakischen Akademie der Wissenschaften; Prag. 627 p., 16 pls." type="book" year="1963">Balthasar 1963b</bibRefCitation>
