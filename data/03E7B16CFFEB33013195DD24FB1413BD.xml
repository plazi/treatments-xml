<collectingCountry id="F35940EAFFE83301306EDCBBFE5C13BD" box="[364,471,1969,1995]" name="Malaysia" pageId="10" pageNumber="11">Malaysia</collectingCountry>
(
<collectingRegion id="498ACE98FFE8330130E5DCBBFDBF13BD" box="[487,564,1969,1995]" country="Malaysia" name="Sabah" pageId="10" pageNumber="11">Sabah</collectingRegion>
), 
<collectingCountry id="F35940EAFFE83301334BDCBBFD3013BD" box="[585,699,1969,1995]" name="Indonesia" pageId="10" pageNumber="11">Indonesia</collectingCountry>
