<taxonomicName id="23C8758A9D071E5528EC3A84CAFBB119" authorityName="Kurr" authorityYear="1856" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="kleini" order="Pteropoda" pageId="142" pageNumber="143" phylum="Mollusca" rank="species" species="kleini">
