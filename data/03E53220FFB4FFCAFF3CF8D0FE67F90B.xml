<heading id="D0BB345AFFB4FFC9FF3CF8D0FCECF882" bold="true" box="[151,789,1792,1819]" fontSize="11" level="1" pageId="5" pageNumber="592" reason="1">
<taxonomicName id="4C4CF8B5FFB4FFC9FF3CF8D0FCECF882" authority="Lent &amp; Wygodzinsky, 1979" authorityName="Lent &amp; Wygodzinsky" authorityYear="1979" box="[151,789,1792,1819]" class="Insecta" family="Reduviidae" genus="Parabelminus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="592" phylum="Arthropoda" rank="species" species="yurupucu">
