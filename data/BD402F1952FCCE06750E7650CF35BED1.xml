<paragraph id="041D9FA43BA2231BA8B16AFB71099AA8" pageId="null" pageNumber="294">
<pageBreakToken id="0F889787CE59FE22D13BEB97D8544B54" pageId="null" pageNumber="294" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="F510C4BDA378DFF672E181E7DF4F0C05" authority="L." class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="294" phylum="Tracheophyta" rank="species" species="rubrum">
