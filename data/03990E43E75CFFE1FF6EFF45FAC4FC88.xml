</emphasis>
<taxonomicName id="4C30C4D6E75CFFE1FED3FEF8FCE9FEDB" authority="Komai &amp; Osawa, 2012" authorityName="Komai &amp; Osawa" authorityYear="2012" box="[298,819,353,377]" class="Malacostraca" family="Paguridae" genus="Kumepagurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="242" phylum="Arthropoda" rank="species" species="cavernicolus">
