<quantity id="F1759B46FF9BFFDC6C367ECD4E96FC10" box="[826,934,927,954]" metricMagnitude="-4" metricUnit="m" metricValue="2.1" pageId="8" pageNumber="9" unit="mm" value="0.21">0.21 mm</quantity>
.
</paragraph>
<paragraph id="363236A3FF9BFFDC6FB17EB34808FC50" blockId="8.[189,1336,992,1019]" box="[189,1336,992,1019]" pageId="8" pageNumber="9">
