<collectionCode id="3DC3AE5572416170FD39FD21FCB7FD50" box="[729,799,642,666]" country="United Kingdom" httpUri="http://grbio.org/cool/zvf2-zx2b" name="CABI Genetic Resource Collection" pageId="118" pageNumber="119">CABI</collectionCode>
