<taxonomicName id="6987F8D9FFE31A49FF4CFB148A81FB6A" authority="H. Perrier" authorityName="H. Perrier" box="[234,713,1187,1211]" class="Magnoliopsida" family="Bignoniaceae" form="microphylla" genus="Rhodocolea" kingdom="Plantae" order="Lamiales" pageId="8" pageNumber="265" phylum="Tracheophyta" rank="form" species="racemosa">
