<taxonomicName id="6DB9FD89CD2DFFF8CEE65EC3FCC3FDBA" class="Arachnida" family="Lycosidae" genus="Alopecosa" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="9" phylum="Arthropoda" rank="species" species="subhirsutella">
