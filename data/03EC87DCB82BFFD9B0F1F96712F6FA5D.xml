<taxonomicName id="4C454D49B835FFD9B3BFFF1A1242FF66" box="[985,1086,190,214]" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="17" pageNumber="468" phylum="Chordata" rank="species" species="chotta">
