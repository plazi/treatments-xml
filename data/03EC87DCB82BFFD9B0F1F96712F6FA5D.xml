<taxonomicName id="4C454D49B835FFD9B5E2FEEA16A1FE3B" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="17" pageNumber="468" phylum="Chordata" rank="species" species="chotta">
<emphasis id="B931EAD8B835FFD9B5E2FEEA16A1FE3B" italics="true" pageId="17" pageNumber="468">R. chotta</emphasis>
</taxonomicName>
