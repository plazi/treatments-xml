<taxonomicName id="4C454D49B835FFD9B572FF3C16A1FF66" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="17" pageNumber="468" phylum="Chordata" rank="species" species="chotta">
