<taxonomicName id="4C454D49B835FFD9B3A9FE321327FE1F" authority="Biju and Bossuyt 2009" authorityName="Biju and Bossuyt" authorityYear="2009" box="[975,1371,406,431]" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="17" pageNumber="468" phylum="Chordata" rank="species" species="chotta">
