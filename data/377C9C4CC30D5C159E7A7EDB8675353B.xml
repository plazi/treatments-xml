<paragraph id="14B94CF3AB888C78F0478B2D92D11672" pageNumber="308">
<taxonomicName id="AD1CAE8332DCAC1D053C43DAD718013C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138002" class="Insecta" family="Formicidae" genus="Dorymyrmex" kingdom="Animalia" lsidName-HNS="Dorymyrmex tener subsp. richteri Forel" order="Hymenoptera" pageNumber="308" phylum="Arthropoda" rank="subSpecies" species="tener" subSpecies="richteri">Dorymyrmex tener Mayr. subsp. Richteri</taxonomicName>
<taxonomicNameLabel id="C41144C8443258625D745C82AFDB0D0F" rank="species">n. sp.</taxonomicNameLabel>
</paragraph>
