<taxonomicName id="BDC6FBBD392FFFB5B0F10C76FDEE9EF9" box="[470,573,1652,1676]" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="miksici">
