<taxonomicName id="4C284D39FFD047119835FDE9FCC8060F" box="[529,770,512,536]" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="493" phylum="Arthropoda" rank="species" species="kareenae">
