<taxonomicName id="4C284D39FFD0471199CDFF71FBA004A7" box="[1001,1130,152,176]" class="Malacostraca" family="Galatheidae" genus="Galathodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="493" phylum="Arthropoda" rank="genus">
<emphasis id="B95CEAA8FFD0471199CDFF71FBA004A7" box="[1001,1130,152,176]" italics="true" pageId="3" pageNumber="493">Galathodes</emphasis>
</taxonomicName>
group” within 
<taxonomicName id="4C284D39FFD047119F3DFF71FE4804C3" authority="Ahyong et al. 2011" authorityName="Ahyong et al." authorityYear="2011" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="493" phylum="Arthropoda" rank="genus">
<emphasis id="B95CEAA8FFD047119F3DFF71FA5604A7" box="[1305,1436,152,176]" italics="true" pageId="3" pageNumber="493">Munidopsis</emphasis>
(Ahyong 
<emphasis id="B95CEAA8FFD047119B25FF54FEF304C3" box="[257,313,188,212]" italics="true" pageId="3" pageNumber="493">et al.</emphasis>
<number id="3DE0249DFFD047119B1BFF55FEB004C3" box="[319,378,188,212]" pageId="3" pageNumber="493" value="2011.0">2011</number>
)
</taxonomicName>
