<taxonomicName id="4C7B4D2391553B03FEF1FC2DFE4DFC2D" authorityName="Topsent" authorityYear="1901" box="[280,437,918,944]" class="Demospongiae" family="Cladorhizidae" genus="Asbestopluma" kingdom="Animalia" order="Poecilosclerida" pageId="27" pageNumber="28" phylum="Porifera" rank="genus">
