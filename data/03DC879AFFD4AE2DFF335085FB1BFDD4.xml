<emphasis id="B901EA9EFFD4AE29FF335085FCEAFF3A" bold="true" box="[151,800,151,177]" pageId="1" pageNumber="26">
<taxonomicName id="4C754D0FFFD4AE29FF335085FD1BFF3A" authority="Vinokurov et Luo" authorityName="Vinokurov et Luo" box="[151,721,151,177]" class="Insecta" family="Miridae" genus="Psallus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="26" phylum="Arthropoda" rank="species" species="jungaricus" status="sp. n." subGenus="Psallus">
<emphasis id="B901EA9EFFD4AE29FF335085FF25FF3A" bold="true" box="[151,239,151,177]" italics="true" pageId="1" pageNumber="26">Psallus</emphasis>
(
<emphasis id="B901EA9EFFD4AE29FEA45085FE92FF3A" bold="true" box="[256,344,151,177]" italics="true" pageId="1" pageNumber="26">Psallus</emphasis>
) 
<emphasis id="B901EA9EFFD4AE29FECC5085FE20FF3A" bold="true" box="[360,490,151,177]" italics="true" pageId="1" pageNumber="26">jungaricus</emphasis>
Vinokurov et Luo
</taxonomicName>
, 
<taxonomicNameLabel id="A23257E5FFD4AE29FD7A5085FCEAFF3A" box="[734,800,151,177]" pageId="1" pageNumber="26" rank="species">sp. n.</taxonomicNameLabel>
