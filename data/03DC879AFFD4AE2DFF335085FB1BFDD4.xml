(15, 16), 
<taxonomicName id="4C754D0FFFD1AE2CFCA95300FC4EFCAC" box="[781,900,785,807]" class="Insecta" family="Miridae" genus="Psallus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="29" phylum="Arthropoda" rank="species" species="galilaeus">
