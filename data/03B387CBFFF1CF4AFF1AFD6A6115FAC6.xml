<collectingCountry id="F30D764DFFF1CF4AFD6AFA9B6015FAE2" box="[743,813,1335,1360]" name="China" pageId="14" pageNumber="315">China</collectingCountry>
, 
<collectingCountry id="F30D764DFFF1CF4AFCB5FA9B604CFAE2" box="[824,884,1335,1360]" name="India" pageId="14" pageNumber="315">India</collectingCountry>
, 
<collectingCountry id="F30D764DFFF1CF4AFCF2FA9B60D7FAE2" box="[895,1007,1335,1360]" name="Indonesia" pageId="14" pageNumber="315">Indonesia</collectingCountry>
, 
<collectingCountry id="F30D764DFFF1CF4AFC74FA946750FAE2" box="[1017,1128,1336,1360]" name="Myanmar" pageId="14" pageNumber="315">Myanmar</collectingCountry>
