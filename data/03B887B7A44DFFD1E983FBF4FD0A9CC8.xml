<taxonomicName id="4C114D22A44DFFD1EA23F8E1FC799C58" baseAuthorityName="Jaderholm" baseAuthorityYear="1904" box="[823,962,1805,1830]" class="Hydrozoa" family="Sertulariidae" genus="Symplectoscyphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="35" pageNumber="36" phylum="Cnidaria" rank="species" species="glacialis">
