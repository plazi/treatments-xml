<heading id="D0A41579BA3FFFFAF8EEFCD289E1FC17" bold="true" box="[151,475,871,898]" fontSize="11" level="1" pageId="11" pageNumber="512" reason="1">
<emphasis id="B9277E07BA3FFFFAF8EEFCD289E1FC17" bold="true" box="[151,475,871,898]" pageId="11" pageNumber="512">
<taxonomicName id="4C53D996BA3FFFFAF8EEFCD2894CFC14" box="[151,374,871,897]" class="Insecta" family="Hybotidae" genus="Oropezella" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="512" phylum="Arthropoda" rank="species" species="clavata" status="sp. nov.">
<emphasis id="B9277E07BA3FFFFAF8EEFCD2894CFC14" bold="true" box="[151,374,871,897]" italics="true" pageId="11" pageNumber="512">Oropezella clavata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A214C37CBA3FFFFAF904FCDD89E1FC17" box="[381,475,872,898]" pageId="11" pageNumber="512" rank="species">sp. nov.</taxonomicNameLabel>
