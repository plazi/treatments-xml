<taxonomicName id="4C434D27FF9CFFC9FDD9FE20FC7F9B87" authority="Salmon 1964" authorityName="Salmon" authorityYear="1964" box="[616,980,438,463]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="34" pageNumber="335" phylum="Arthropoda" rank="species" species="yosii">
