<taxonomicName id="4C434D27FF9CFFC9FB37FE4CFAB39BBB" box="[1158,1304,474,499]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="34" pageNumber="335" phylum="Arthropoda" rank="genus">
