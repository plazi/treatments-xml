<taxonomicName id="19C3C6F2456383F7883B7F3370CAF4A3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26207" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus arboreus (Smith)" lsidName-HNS="Camponotus arboreus (Smith)" order="Hymenoptera" pageNumber="666" phylum="Arthropoda" rank="species" species="arboreus">C. arboreus</taxonomicName>
