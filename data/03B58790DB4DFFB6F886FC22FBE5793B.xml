<emphasis id="B968EA94DB4CFFA9FABEF803FD027DFA" bold="true" box="[687,711,1982,2004]" pageId="15" pageNumber="62">C.</emphasis>
<taxonomicName id="4C1C4D05DB4CFFA9FAC0F802FC2E7DFA" authorityName="Craig, Crampton and Albert" authorityYear="2017" box="[721,1003,1982,2004]" class="Actinopterygii" family="Gymnotidae" genus="Gymnotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gymnotiformes" pageId="15" pageNumber="62" phylum="Chordata" rank="subSpecies" species="carapo" subSpecies="australis">
<emphasis id="B968EA94DB4CFFA9FAC0F802FC2E7DFA" box="[721,1003,1982,2004]" italics="true" pageId="15" pageNumber="62">
<taxonomicName id="4C1C4D05DB4CFFA9FAC0F802FC4C7DFA" box="[721,905,1983,2004]" class="Actinopterygii" family="Gymnotidae" genus="Gymnotus" higherTaxonomySource="CoL" kingdom="Animalia" order="Gymnotiformes" pageId="15" pageNumber="62" phylum="Chordata" rank="species" species="carapo">
<taxonomicName id="4C1C4D05DB4CFFA9FAC0F802FCFD7DFA" box="[721,824,1983,2004]" class="Actinopterygii" family="Gymnotidae" genus="Gymnotus" higherTaxonomySource="CoL" kingdom="Animalia" order="Gymnotiformes" pageId="15" pageNumber="62" phylum="Chordata" rank="genus">Gymnotus</taxonomicName>
