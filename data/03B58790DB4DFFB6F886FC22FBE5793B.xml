<emphasis id="B968EA94DB4CFFA9FA0DF81CFD727D98" box="[540,695,1953,1974]" italics="true" pageId="15" pageNumber="62">
<taxonomicName id="4C1C4D05DB4CFFA9FA0DF81CFD467D98" box="[540,643,1953,1974]" class="Actinopterygii" family="Gymnotidae" genus="Gymnotus" higherTaxonomySource="CoL" kingdom="Animalia" order="Gymnotiformes" pageId="15" pageNumber="62" phylum="Chordata" rank="genus">Gymnotus</taxonomicName>
