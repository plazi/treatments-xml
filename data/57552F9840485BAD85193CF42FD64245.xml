<paragraph id="3E8F684A069DE6977961E52468AA17F3" pageId="0" pageNumber="89">
<taxonomicName id="0E41E40DEB155B16EEE7FB925C0AC29A" LSID="57552F98-4048-5BAD-8519-3CF42FD64245" authority="Quicke &amp; Shaw &amp; Inayatullah &amp; Butcher, 2020" authorityName="Quicke &amp; Shaw &amp; Inayatullah &amp; Butcher" authorityYear="2020" class="Insecta" family="Braconidae" genus="Vipio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Vipio carinatus" order="Hymenoptera" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="carinatus" status="sp. nov.">Vipio carinatus</taxonomicName>
<taxonomicNameLabel id="F74351E8C6D12E0B90B60950ADEFAEE3" pageId="0" pageNumber="89">sp. nov.</taxonomicNameLabel>
