</taxonomicName>
and 
<taxonomicName id="4C3BB59BFFFC605AFAE5FF2D6CB2D22B" authorityName="Pease" authorityYear="1871" class="Gastropoda" family="Dorididae" genus="Doriopsis" higherTaxonomySource="CoL" kingdom="Animalia" order="Nudibranchia" pageId="19" pageNumber="554" phylum="Mollusca" rank="species" species="scabra">
<emphasis id="B94F120AFFFC605AFAE5FF2D6CB2D22B" italics="true" pageId="19" pageNumber="554">Doriopsis scabra</emphasis>
</taxonomicName>
