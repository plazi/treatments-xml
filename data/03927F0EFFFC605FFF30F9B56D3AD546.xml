<taxonomicName id="4C3BB59BFFFC605AFCB4FEEE6C9ED208" authorityName="Cheeseman" authorityYear="1881" box="[819,1005,288,309]" class="Gastropoda" family="Dorididae" genus="Doris" kingdom="Animalia" order="Nudibranchia" pageId="19" pageNumber="554" phylum="Mollusca" rank="species" species="flabellifera">
