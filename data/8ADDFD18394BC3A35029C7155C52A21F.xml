<paragraph id="F8F09BFDE2C67C3996D9506B0A718108" pageId="582" pageNumber="583">
<taxonomicName id="4DAE278AF5B547FC3AC99627A41B2FAA" authorityName="Casey" authorityYear="1924" class="Hexapoda" family="Carabidae" genus="Celia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="582" pageNumber="583" phylum="Arthropoda" rank="species" species="winnipegensis">Celia winnipegensis</taxonomicName>
