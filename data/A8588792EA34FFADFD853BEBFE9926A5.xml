<taxonomicName id="E7F14D07EA34FFADFF3B3AB8FE7922E8" authority="Bourgoin &amp; Wang &amp; Nel, 2020" authorityName="Bourgoin &amp; Wang &amp; Nel" authorityYear="2020" box="[189,501,908,934]" class="Insecta" family="Issidae" genus="Cubicostissus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="palaeocaeni" status="sp. nov.">
