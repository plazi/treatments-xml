<taxonomicName id="6FE54D129512FFD0A4D0FE7CFCE6B793" box="[673,813,404,429]" class="Anthozoa" family="Pocilloporidae" genus="Pocillopora" kingdom="Animalia" order="Scleractinia" pageId="14" pageNumber="15" phylum="Cnidaria" rank="genus">
<emphasis id="9A91EA839512FFD0A4D0FE7CFCE6B793" box="[673,813,404,429]" italics="true" pageId="14" pageNumber="15">Pocillopora</emphasis>
