(
<figureCitation id="132D2A5DA306FFD4FF56F8BCFF1EF88C" box="[159,246,1820,1846]" captionStart="FIGURE 19" captionStartId="28.[151,250,1663,1687]" captionTargetBox="[182,1400,706,1629]" captionTargetId="figure-282@28.[151,1436,669,1640]" captionTargetPageId="28" captionText="FIGURE 19. Single cladogram obtained with setk.run and concavity 10.239258 (continued), showing the resolution of Psalmopoeinae clade. Absolute and relative Bremer support are in each node." figureDoi="http://doi.org/10.5281/zenodo.6253245" httpUri="https://zenodo.org/record/6253245/files/figure.png" pageId="48" pageNumber="49">Figs 19</figureCitation>
, 
<figureCitation id="132D2A5DA306FFD4FECBF8BCFEC8F88C" box="[258,288,1820,1846]" captionStart="FIGURES 21–23. 21" captionStartId="32.[151,264,1339,1363]" captionTargetBox="[151,1435,181,1315]" captionTargetId="figure-19@32.[151,1435,181,1315]" captionTargetPageId="32" captionText="FIGURES 21–23. 21, Map showing geographic records (dots) of Tapinauchenius species. 22, detail of the geographic records (dots) of T. sanctivincenti, T. latipes, T. polybotes and T. rasti. 23, detail of the geographic records (dots) of T. plumipes (red) and T. gretae n. sp. (black) in Central Amazonia, Brazil. Tapinauchenius plumipes (red), T. sanctivincenti (pink), T. latipes (green), T. brunneus (brown), T. cupreus (yellow), T. polybotes (purple), T. rasti (light blue), T. gretae n. sp. (black). MI = Mustique island; SL = Saint Lucia island; SV = Saint Vincent island; UI = Union island; TR = Trinidad; VE = Venezuela." figureDoi="http://doi.org/10.5281/zenodo.6253255" httpUri="https://zenodo.org/record/6253255/files/figure.png" pageId="48" pageNumber="49">21</figureCitation>
, 
<figureCitation id="132D2A5DA306FFD4FEE5F8BCFE6AF88C" box="[300,386,1820,1846]" captionStart-0="FIGURES 92–99" captionStart-1="FIGURES 100–102" captionStartId-0="50.[151,264,1926,1950]" captionStartId-1="51.[151,264,873,897]" captionTargetBox-0="[206,1380,201,1901]" captionTargetBox-1="[151,1435,181,849]" captionTargetId-0="figure-19@50.[206,1380,181,1901]" captionTargetId-1="figure-20@51.[151,1435,181,849]" captionTargetPageId-0="50" captionTargetPageId-1="51" captionText-0="FIGURES 92–99. Tapinauchenius cupreus. (92–98) male (MNRJ 7698). (92–95) right bulb. 92, retrolateral. 93, prolateral. 94, frontal. 95, dorsal. (96–98) left leg I tibial apophysis. 96, ventral. 97, prolateral. 98, retrolateral. (99) female, paratype (SMF 39015), spermathecae. Scale bars = 1 mm." captionText-1="FIGURES 100–102. Tapinauchenius cupreus, holotype male (SMF 39015). (100–101) right bulb. 100, retrolatera1. 101, prolateral. 102, carapace." figureDoi-0="http://doi.org/10.5281/zenodo.6253311" figureDoi-1="http://doi.org/10.5281/zenodo.6253315" httpUri-0="https://zenodo.org/record/6253311/files/figure.png" httpUri-1="https://zenodo.org/record/6253315/files/figure.png" pageId="48" pageNumber="49">92–102</figureCitation>
)
