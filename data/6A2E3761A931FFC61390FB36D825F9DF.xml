<quantity id="257F2B92A931FFC61614FAB5D934FA88" box="[1299,1367,1350,1375]" metricMagnitude="-2" metricUnit="m" metricValue="2.5" pageId="3" pageNumber="404" unit="cm" value="2.5">2.5 cm</quantity>
