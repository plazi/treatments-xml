<quantity id="257F2B92A931FFC613DBFA96DD42FAAB" box="[220,289,1381,1406]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" pageId="3" pageNumber="404" unit="cm" value="1.5">1.5 cm</quantity>
