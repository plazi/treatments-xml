<td id="76F2262AFFBB00182191FEE19DCDDC83" box="[444,491,368,392]" gridcol="1" gridrow="1" pageId="73" pageNumber="74">31.1</td>
<td id="76F2262AFFBB00182230FEE19E62DC83" box="[541,580,368,392]" gridcol="2" gridrow="1" pageId="73" pageNumber="74">11</td>
<td id="76F2262AFFBB0018225BFEE19E83DC83" box="[630,677,368,392]" gridcol="3" gridrow="1" pageId="73" pageNumber="74">7.05</td>
<td id="76F2262AFFBB001822FDFEE19ED9DC83" box="[720,767,368,392]" gridcol="4" gridrow="1" pageId="73" pageNumber="74">15.9</td>
<td id="76F2262AFFBB0018231CFEE19F74DC83" box="[817,850,368,392]" gridcol="5" gridrow="1" pageId="73" pageNumber="74">5.8</td>
