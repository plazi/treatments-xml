<td id="76F2262AFFBB001823C5FEE19819DC83" box="[1000,1087,368,392]" gridcol="7" gridrow="1" pageId="73" pageNumber="74">1.56</td>
<td id="76F2262AFFBB00182479FEE1988FDC83" box="[1108,1193,368,392]" gridcol="8" gridrow="1" pageId="73" pageNumber="74">0.51</td>
<td id="76F2262AFFBB001824E8FEE19923DC83" box="[1221,1285,368,392]" gridcol="9" gridrow="1" pageId="73" pageNumber="74">1.51</td>
