<collectingRegion id="8F0804A6FF20B3AF45C3FB6FC9F00F92" box="[389,502,1190,1214]" country="Japan" name="Hokkaido" pageId="188" pageNumber="189">Hokkaido</collectingRegion>
, 
<collectorName id="E039AF92FF20B3AF4640FB6FCA6F0F92" box="[518,617,1190,1214]" pageId="188" pageNumber="189">Kuril Is.</collectorName>
, 
<collectingRegion id="8F0804A6FF20B3AF463CFB6FCAFB0F92" box="[634,765,1190,1214]" country="Russia" name="Kamchatka" pageId="188" pageNumber="189">Kamchatka</collectingRegion>
; 
<collectorName id="E039AF92FF20B3AF474BFB6FCB8C0F92" box="[781,906,1190,1214]" pageId="188" pageNumber="189">Pribilof Is.</collectorName>
to 
<collectorName id="E039AF92FF20B3AF47F1FB6ECC230F93" box="[951,1061,1191,1215]" pageId="188" pageNumber="189">Monterey</collectorName>
, 
