<taxonomicName id="04894D41FFCFFFA5C2ECDD3F68F15C27" box="[447,716,1579,1602]" class="Insecta" family="Chironomidae" genus="Psectrotanypus" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="51" phylum="Arthropoda" rank="species" species="lateralis" status="sp. n.">
