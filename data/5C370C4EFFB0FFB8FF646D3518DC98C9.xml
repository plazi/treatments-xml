<taxonomicName id="139EC6DBFFB0FFB8FEDA6D7C18849BFE" authority="Smitt, 1873" authorityName="Smitt" authorityYear="1873" box="[297,653,1147,1170]" class="Gymnolaemata" family="Hippothoidae" genus="Hippothoa" kingdom="Animalia" order="Cheilostomatida" pageId="1" pageNumber="40" phylum="Bryozoa" rank="subSpecies" species="pes" subSpecies="anseris">
