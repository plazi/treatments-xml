<emphasis id="B93373189B36FFE2C2CE077DFF63F933" bold="true" box="[195,229,1661,1678]" pageId="4" pageNumber="331">A -C</emphasis>
, 
<taxonomicName id="4C47D4899B36FFE2C2E2077DFDA5F933" authority="Montagu, 1818" authorityName="Montagu" authorityYear="1818" box="[239,547,1661,1678]" class="Demospongiae" family="Polymastiidae" genus="Polymastia" kingdom="Animalia" order="Polymastiida" pageId="4" pageNumber="331" phylum="Porifera" rank="species" species="penicillus">
<emphasis id="B93373189B36FFE2C2E2077DFE1AF933" box="[239,412,1661,1678]" italics="true" pageId="4" pageNumber="331">Polymastia penicillus</emphasis>
Montagu, 1818
</taxonomicName>
; 
<emphasis id="B93373189B36FFE2C026077DFDBEF933" bold="true" box="[555,568,1661,1678]" pageId="4" pageNumber="331">A</emphasis>
, ectosomal tylostyles; 
<emphasis id="B93373189B36FFE2C0F5077DFC83F933" bold="true" box="[760,773,1661,1678]" pageId="4" pageNumber="331">B</emphasis>
, intermediary tylostyles; 
<emphasis id="B93373189B36FFE2C1DB077DFC62F933" bold="true" box="[982,996,1661,1678]" pageId="4" pageNumber="331">C</emphasis>
