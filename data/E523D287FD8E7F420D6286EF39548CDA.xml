<taxonomicName id="008FE41B0DFD210E89C06C441FDC0E2D" class="Magnoliopsida" family="Fabaceae" genus="Pterocarpus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Fabales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
