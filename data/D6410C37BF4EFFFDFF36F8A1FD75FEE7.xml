: 
<taxonomicName id="99E8C6A2BF4CFFF2FD0CF8B7FBE5F848" authority="Petersen, 1990" authorityName="Petersen" authorityYear="1990" box="[685,1053,1914,1936]" class="Hydrozoa" family="Tubulariidae" genus="Ralpharia" kingdom="Animalia" order="Anthoathecata" pageId="7" pageNumber="8" phylum="Cnidaria" rank="species" species="gorgoniae">
