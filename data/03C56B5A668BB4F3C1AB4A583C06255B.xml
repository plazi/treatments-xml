<taxonomicName id="4C6CA1CF668BB4F3C37548D93AC12290" baseAuthorityName="Smreczynski" baseAuthorityYear="1976" box="[585,731,937,962]" class="Magnoliopsida" family="Haloragaceae" genus="Myriophyllum" kingdom="Plantae" order="Saxifragales" pageId="222" pageNumber="223" phylum="Tracheophyta" rank="species" species="spicatum">
