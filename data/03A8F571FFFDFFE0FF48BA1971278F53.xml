<date id="FFBF62A7FFF2FFE0FB94BEEC71A38F03" box="[1099,1278,752,778]" pageId="8" pageNumber="27" value="1991-07-14" valueMax="1991-07-21" valueMin="1991-07-14">
<number id="3DC95640FFF2FFE0FB94BEEC71C58F03" box="[1099,1176,752,778]" pageId="8" pageNumber="27" value="17.5" valueMax="21.0" valueMin="14.0">14–21</number>
