<taxonomicName id="4C364D20FFC84724FB25FD9AFAD2FDF1" box="[1194,1304,578,602]" class="Magnoliopsida" family="Araliaceae" kingdom="Plantae" order="Apiales" pageId="12" pageNumber="111" phylum="Tracheophyta" rank="family">Araliaceae</taxonomicName>
