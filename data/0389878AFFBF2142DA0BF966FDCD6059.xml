<taxonomicName id="4C204D1FFFBF2143DC56F8D6FA146530" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[1377,1467,1856,1879]" class="Bacilli" family="Lactobacillaceae" genus="Limosilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="50" pageNumber="2832" phylum="Firmicutes" rank="species" species="mucosae">
