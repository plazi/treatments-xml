<subSubSection id="0D9C65118056FF8DFF3966ECCDDBF933" box="[199,1170,1671,1696]" pageId="15" pageNumber="368" type="etymology">
<paragraph id="4539369A8056FF8DFF3966ECCDDBF933" blockId="15.[151,1436,1527,1912]" box="[199,1170,1671,1696]" pageId="15" pageNumber="368">
<emphasis id="77F2EA888056FF8DFF3966ECC807F933" bold="true" box="[199,334,1671,1696]" pageId="15" pageNumber="368">Etymology.</emphasis>
