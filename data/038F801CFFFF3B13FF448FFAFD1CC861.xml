<subSubSection id="C33C6281FFFF3B13FF448C4DFE27CD8A" pageId="9" pageNumber="52" type="etymology">
<paragraph id="8B99310AFFFF3B13FF448C4DFE27CD8A" blockId="9.[151,1437,629,1887]" pageId="9" pageNumber="52">
<emphasis id="B952ED18FFFF3B13FF448C4DFE9FCDB1" bold="true" box="[151,291,629,655]" pageId="9" pageNumber="52">Etymology.</emphasis>
