<taxonomicName id="24C94D671B47FFF5FF34F972FE729780" baseAuthorityName="Hoffmann" baseAuthorityYear="1965" box="[151,437,1700,1726]" class="Insecta" family="Curculionidae" genus="Mecinus" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="paratychioides">
<emphasis id="D1BDEAF61B47FFF5FF34F972FE729780" bold="true" box="[151,437,1700,1726]" italics="true" pageId="35" pageNumber="36">Mecinus paratychioides</emphasis>
