<taxonomicName id="4C56A6AF8521FFC57B318E0CF473FAA5" box="[298,573,1304,1329]" class="Entognatha" family="Neanuridae" genus="Pseudachorutes" kingdom="Animalia" order="Collembola" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="dubius">
