<taxonomicName id="0BE04D0EFFA6FFB9B05EFD54FD23FFE7" box="[264,732,729,756]" class="Insecta" family="Xiphocentronidae" genus="Xiphocentron" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="chiapasi" status="new species" subGenus="Xiphocentron">
<emphasis id="FE94EA9FFFA6FFB9B05EFD54FE51FFE7" bold="true" box="[264,430,730,756]" italics="true" pageId="9" pageNumber="46">Xiphocentron</emphasis>
