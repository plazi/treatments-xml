<taxonomicName id="81AEF38AFFB52476FE0DBA24FBB7F993" authority="Takaoka &amp; Henry. A" authorityName="Takaoka &amp; Henry. A" authorityYear="2010" box="[481,1097,1566,1590]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="sachini" subGenus="Gomphostilbia">
