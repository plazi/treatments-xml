, HL 
<number id="3DD924C98D74FFDFA391F8C7FC97F8CA" box="[763,883,1834,1860]" pageId="6" pageNumber="41" value="1.205" valueMax="1.25" valueMin="1.16">1.16–1.25</number>
, HW, 
<number id="3DD924C98D74FFDFA2D5F8C7FBD3F8CA" box="[959,1079,1834,1860]" pageId="6" pageNumber="41" value="1.18" valueMax="1.23" valueMin="1.13">1.13–1.23</number>
, CI 
<number id="3DD924C98D74FFDFA500F8C7FB21F8CA" box="[1130,1221,1834,1860]" pageId="6" pageNumber="41" value="98.5" valueMax="101.0" valueMin="96.0">96–101</number>
, SL 
<number id="3DD924C98D74FFDFA597F8C7FA91F8CA" box="[1277,1397,1834,1860]" pageId="6" pageNumber="41" value="0.935" valueMax="0.97" valueMin="0.9">0.90–0.97</number>
, SI 
<number id="3DD924C98D74FFDFA1FDF8BFFF06F8E2" box="[151,226,1874,1900]" pageId="6" pageNumber="41" value="78.0" valueMax="81.0" valueMin="75.0">75–81</number>
, PW 
<number id="3DD924C98D74FFDFA048F8BFFE7EF8E2" box="[290,410,1874,1900]" pageId="6" pageNumber="41" value="0.96" valueMax="1.02" valueMin="0.9">0.90–1.02</number>
, WL 
<number id="3DD924C98D74FFDFA0B1F8BFFDB6F8E2" box="[475,594,1874,1900]" pageId="6" pageNumber="41" value="1.7799999999999998" valueMax="1.9" valueMin="1.66">1.66–1.90</number>
, maximum diameter of eye 
<number id="3DD924C98D74FFDFA2F0F8BFFBF7F8E2" box="[922,1043,1874,1900]" pageId="6" pageNumber="41" value="0.30000000000000004" valueMax="0.32" valueMin="0.28">0.28–0.32</number>
, OI 
<number id="3DD924C98D74FFDFA52EF8BFFB6AF8E2" box="[1092,1166,1874,1900]" pageId="6" pageNumber="41" value="25.5" valueMax="27.0" valueMin="24.0">24–27</number>
(
<number id="3DD924C98D74FFDFA5F7F8BFFB58F8E2" box="[1181,1212,1874,1900]" pageId="6" pageNumber="41" value="20.0">20</number>
