<taxonomicName id="4C05D4E0213A97730709BFEFFEDAFA47" authority="(Chamberlin, 1923)" baseAuthorityName="Chamberlin" baseAuthorityYear="1923" class="Diplopoda" family="Polyxenidae" genus="Macroxenodes" kingdom="Animalia" order="Polyxenida" pageId="14" pageNumber="843" phylum="Arthropoda" rank="species" species="bartschi">
