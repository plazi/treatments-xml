<taxonomicName id="4C414D38FFEF7A3EFEE7F8B0F8C8F8E3" authority="Mello-Leit" authorityName="Mello-Leit" box="[364,659,1855,1881]" class="Arachnida" family="Pholcidae" genus="Ibotyporanga" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="genus">
