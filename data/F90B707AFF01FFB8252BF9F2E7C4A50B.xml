<quantity id="B65A6C89FF0FFFB82439FCFCE7C0A50B" box="[568,657,770,797]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="9" pageNumber="601" unit="mm" value="3.2">3.2 mm</quantity>
