<geoCoordinate id="0A01BAB5EF17A739CD7C63B5685ABA64" degrees="08" direction="south" minutes="44" orientation="latitude" precision="925" value="-8.733334">08°44'S</geoCoordinate>
, 
<geoCoordinate id="7A45B532E946B327E85D63F9EDA32ADD" degrees="131" direction="east" minutes="05" orientation="longitude" precision="925" value="131.08333">131°05'E</geoCoordinate>
