[[ 
<taxonomicName id="45CB22213ED94B3E4E5F2014C35A2480" LSID-ZBK="urn:lsid:zoobank.org:act:1FE516F9-3A59-4F68-815F-713FCAC4727E" class="Elasmobranchii" family="Rajidae" genus="Dipturus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Dipturus Rafinesque 1810:16" order="Rajiformes" pageNumber="45" phylum="Chordata" rank="genus">Dipturus Rafinesque</taxonomicName>
