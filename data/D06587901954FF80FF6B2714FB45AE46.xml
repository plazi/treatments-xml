<taxonomicName id="9FCC4D05195AFF80FD7C2395FC23AFF6" authority="Evans, 1954" authorityName="Evans" authorityYear="1954" box="[640,1007,151,176]" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" kingdom="Animalia" order="Prostigmata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="callunae">
<emphasis id="6AB8EA94195AFF80FD7C2395FC96AFE9" box="[640,858,152,176]" italics="true" pageId="41" pageNumber="42">Stigmaeus callunae</emphasis>
<bibRefCitation id="3C5D4B77195AFF80FC9D2395FC23AFF6" author="Evans" box="[865,1007,151,175]" pageId="41" pageNumber="42" refString="Evans, G. O. (1954) Some new rare species of Acarina. Proceedings of the Zoological Society of London, 123, 793 - 811." type="journal article" year="1954">
Evans, 
<number id="EE0424A1195AFF80FC48239AFC23AFF6" box="[948,1007,151,175]" pageId="41" pageNumber="42" value="1954.0">1954</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="9FCC4D05195AFF80FC072395FAB7AFF6" authority="Liang &amp; Hu, 1987" authorityName="Liang &amp; Hu" authorityYear="1987" box="[1019,1403,151,176]" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" kingdom="Animalia" order="Prostigmata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="longisetosus">
