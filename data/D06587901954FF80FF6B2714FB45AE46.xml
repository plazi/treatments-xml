<bibRefCitation id="3C5D4B77195AFF80FB55239AFAB7AFF6" author="Liang" box="[1193,1403,151,175]" pageId="41" pageNumber="42" refString="Liang, L. &amp; Hu, C. (1987) Three new stigmaeid mites of the genus Stigmaeus (Acarina: Stigmaedae). Entomotaxonomia, 9, 307 - 311." type="journal article" year="1987">
Liang &amp; Hu, 
<number id="EE0424A1195AFF80FABC239AFAB7AFF6" box="[1344,1403,151,175]" pageId="41" pageNumber="42" value="1987.0">1987</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="9FCC4D05195AFF80FA7B2395FE33AF8D" authority="Liang &amp; Hu, 1987" authorityName="Liang &amp; Hu" authorityYear="1987" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" kingdom="Animalia" order="Prostigmata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="macroposbus">
<emphasis id="6AB8EA94195AFF80FA7B2395FEE0AF8D" italics="true" pageId="41" pageNumber="42">S. macroposbus</emphasis>
<bibRefCitation id="3C5D4B77195AFF80FECE23B1FE33AF8D" author="Liang" box="[306,511,188,212]" pageId="41" pageNumber="42" refString="Liang, L. &amp; Hu, C. (1987) Three new stigmaeid mites of the genus Stigmaeus (Acarina: Stigmaedae). Entomotaxonomia, 9, 307 - 311." type="journal article" year="1987">
Liang &amp; Hu, 
<number id="EE0424A1195AFF80FE3B23B1FE33AF8D" box="[455,511,188,212]" pageId="41" pageNumber="42" value="1987.0">1987</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="9FCC4D05195AFF80FDCA23B0FCA8AF8D" authority="Kuznetsov, 1978" authorityName="Kuznetsov" authorityYear="1978" box="[566,868,188,212]" class="Arachnida" family="Stigmaeidae" genus="Stigmaeus" kingdom="Animalia" order="Prostigmata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="pilatus">
<emphasis id="6AB8EA94195AFF80FDCA23B0FD52AF8D" box="[566,670,188,212]" italics="true" pageId="41" pageNumber="42">S. pilatus</emphasis>
Kuznetsov, 
<number id="EE0424A1195AFF80FCD523B1FCA8AF8D" box="[809,868,188,212]" pageId="41" pageNumber="42" value="1978.0">1978</number>
</taxonomicName>
