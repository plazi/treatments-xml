<collectingCountry id="ED9FBFADF176FFF3DDFD738A2A0AF956" box="[1000,1069,1741,1764]" name="Sudan" pageId="10" pageNumber="11">Sudan</collectingCountry>
, 
<collectingCountry id="ED9FBFADF176FFF3DA2D738A2A5AF956" box="[1080,1149,1741,1764]" name="Kenya" pageId="10" pageNumber="11">Kenya</collectingCountry>
.......................................... 
<emphasis id="A7FC232FF176FFF3DED273B72AB6F8B5" box="[199,1169,1776,1799]" italics="true" pageId="10" pageNumber="11">.................................................................................................................................................</emphasis>
<taxonomicName id="528884BEF176FFF3DA8773B72BBBF8B5" authority="Orchymont" authorityName="Orchymont" box="[1170,1436,1776,1799]" class="Insecta" family="Hydraenidae" genus="Parhydraena" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
<emphasis id="A7FC232FF176FFF3DA8773B72B3BF8B5" box="[1170,1308,1776,1799]" italics="true" pageId="10" pageNumber="11">Parhydraena</emphasis>
