<taxonomicName id="4C740FA26A1FFFAF537B3C0806E2FB11" authority="Macgowan, 2021" authorityName="MacGowan" authorityYear="2021" box="[340,497,1141,1165]" class="Insecta" family="Lonchaeidae" genus="Lonchaea" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="350" phylum="Arthropoda" rank="species" species="efate" status="sp. nov.">
