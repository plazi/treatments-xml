<taxonomicName id="4C0A44CDFFABAB395B273A902200B693" authority="BROWNE, 1756" authorityName="BROWNE" authorityYear="1756" box="[1036,1290,452,477]" class="Nuda" family="Beroidae" genus="Beroe" kingdom="Animalia" order="Beroida" pageId="6" pageNumber="303" phylum="Ctenophora" rank="genus">
<emphasis id="B97EE35CFFABAB395B273A902350B692" box="[1036,1114,453,476]" italics="true" pageId="6" pageNumber="303">BEROE</emphasis>
<bibRefCitation id="EF9B42BFFFABAB395B4A3A912200B693" author="Browne P" box="[1121,1290,452,477]" pageId="6" pageNumber="303" refId="ref14791" refString="Browne P. 1756. The civil and natural history of Jamaica. London: printed for the author and sold by L. T. Osborne and J. Shipton, The author, 490." type="book" year="1756">BROWNE, 1756</bibRefCitation>
