<taxonomicName id="4C364D20FFBC4750FD43FB93FCFBFBC9" box="[716,817,1099,1122]" class="Insecta" family="Curculionidae" genus="Conopsis" kingdom="Animalia" order="Coleoptera" pageId="120" pageNumber="219" phylum="Arthropoda" rank="species" species="gibbosus">
