</taxonomicName>
and 
<emphasis id="B942EAB1FFBC4750FCE6FB93FC1BFBC9" box="[873,977,1099,1122]" italics="true" pageId="120" pageNumber="219">
<taxonomicName id="4C364D20FFBC4750FCE6FB93FC06FBC9" authorityName="Kuschel" authorityYear="2008" box="[873,972,1099,1122]" class="Insecta" family="Curculionidae" genus="Idastes" kingdom="Animalia" order="Coleoptera" pageId="120" pageNumber="219" phylum="Arthropoda" rank="species" species="grandis">I. grandis</taxonomicName>
