<number id="0E25A044FFF8FFD5B0C26FDBE3DFA1ED" box="[151,204,655,680]" pageId="6" pageNumber="107" value="0.65">0.65</number>
; ratio WPR 
<number id="0E25A044FFF8FFD5B1006FC4E24DA1ED" box="[341,350,656,680]" pageId="6" pageNumber="107" value="1.0">1</number>
/LPR 
