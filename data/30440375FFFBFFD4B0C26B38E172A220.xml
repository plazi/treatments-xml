<number id="0E25A044FFF8FFD5B1936FC4E2E9A1ED" box="[454,506,656,680]" pageId="6" pageNumber="107" value="1.9">1.90</number>
; ratio WPR 
<number id="0E25A044FFF8FFD5B2D76FC4E19CA1ED" box="[642,655,656,680]" pageId="6" pageNumber="107" value="2.0">2</number>
