<number id="0E25A044FFF8FFD5B5366FDBE684A1ED" box="[1379,1431,655,680]" pageId="6" pageNumber="107" value="0.75">0.75</number>
; ratio WPR 
<number id="0E25A044FFF8FFD5B1456FE0E20BA189" box="[272,280,692,716]" pageId="6" pageNumber="107" value="1.0">1</number>
/ 
