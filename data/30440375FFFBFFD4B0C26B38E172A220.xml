; ratio 
<collectionCode id="DEFC2AA6FFF8FFD5B4FC6FC4E7FDA1ED" box="[1193,1262,656,680]" pageId="6" pageNumber="107">LELY</collectionCode>
/ 
<collectionCode id="DEFC2AA6FFF8FFD5B4A36FC4E657A1ED" box="[1270,1348,656,680]" pageId="6" pageNumber="107">WELY</collectionCode>
