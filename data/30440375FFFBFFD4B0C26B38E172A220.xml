= 
<number id="0E25A044FFF8FFD5B1C76FE0E2D5A189" box="[402,454,692,716]" pageId="6" pageNumber="107" value="1.73">1.73</number>
; ratio WPR 
<number id="0E25A044FFF8FFD5B21E6FE0E140A189" box="[587,595,692,716]" pageId="6" pageNumber="107" value="1.0">1</number>
/ 
