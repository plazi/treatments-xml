<paragraph id="DC035A7B272CFF9CFF4DFF152D1DB971" blockId="6.[151,1437,151,572]" pageId="6" pageNumber="507">
<taxonomicName id="1BBC21F8272CFF9CFF4DFF152F9CB8A5" box="[151,245,152,176]" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="6" pageNumber="507" phylum="Chordata" rank="species" species="radiolus">
<emphasis id="EEC88669272CFF9CFF4DFF152F9CB8A5" box="[151,245,152,176]" italics="true" pageId="6" pageNumber="507">radiolus</emphasis>
</taxonomicName>
and 
<taxonomicName id="1BBC21F8272CFF9CFEEBFF152EE4B8A5" box="[305,397,152,176]" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" order="Siluriformes" pageId="6" pageNumber="507" phylum="Chordata" rank="species" species="saisii">
