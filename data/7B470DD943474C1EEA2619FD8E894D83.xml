<taxonomicName id="7F299A1E9730DEBCA261E176E94048D6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138802" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" lsidName="Ponera dulcis Forel" lsidName-HNS="Ponera dulcis Forel" order="Hymenoptera" pageNumber="53" phylum="Arthropoda" rank="species" species="dulcis">P. dulcis</taxonomicName>
