<taxonomicName id="80825D742D555287192059B1E045BFF4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:178402" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" lsidName="Ponera coarctata boerorum Forel" lsidName-HNS="Ponera coarctata boerorum Forel" order="Hymenoptera" pageNumber="53" phylum="Arthropoda" rank="species" species="boerorum">boerorum For.</taxonomicName>
