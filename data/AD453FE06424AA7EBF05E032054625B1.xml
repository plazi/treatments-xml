<taxonomicName id="0C0ED977F3809C270D301EE10D94022F" genus="N." lsidName="N. bankirai" pageId="31" pageNumber="82" rank="species" species="bankirai">N. bankirai</taxonomicName>
(Huber, 2017); 
<taxonomicName id="028CA17C45E2E32588640B9A546D3528" genus="N." lsidName="N. bidayuh" pageId="31" pageNumber="82" rank="species" species="bidayuh">N. bidayuh</taxonomicName>
