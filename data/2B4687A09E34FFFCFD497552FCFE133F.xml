<paragraph id="A35036B69E34FFFCFD2875B1FBB61443" blockId="48.[667,1193,372,526]" pageId="48" pageNumber="48">
<treatmentCitationGroup id="83FF11989E34FFFCFD2875B1FBB61443" pageId="48" pageNumber="48">
Now 
<taxonomicName id="64EF4D359E34FFFCFD6875B6FB8317F3" authority="gularis (Wagler, 1829)" authorityName="gularis (Wagler" authorityYear="1829" baseAuthorityName="Wagler" baseAuthorityYear="1829" box="[731,1141,426,446]" class="Aves" family="Icteridae" genus="Icterus" kingdom="Animalia" order="Passeriformes" pageId="48" pageNumber="48" phylum="Chordata" rank="subSpecies" species="gularis" subSpecies="gularis">
<emphasis id="919BEAA49E34FFFCFD6875B6FC3217F3" box="[731,964,426,446]" italics="true" pageId="48" pageNumber="48">Icterus gularis gularis</emphasis>
