<subSubSection id="91CC653BFFCC89391D18F7E484F45970" box="[828,1475,2076,2100]" pageId="10" pageNumber="359" type="etymology">
<paragraph id="D96936B0FFCC89391D18F7E484F45970" blockId="10.[828,1475,2076,2100]" box="[828,1475,2076,2100]" pageId="10" pageNumber="359">
<emphasis id="EBA2EAA2FFCC89391D18F7E4828F5970" bold="true" box="[828,952,2076,2100]" pageId="10" pageNumber="359">Etymology.</emphasis>
