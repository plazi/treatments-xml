<typeStatus id="93248802283DD00A177D3D59007B7553" box="[264,317,788,811]" pageId="2" pageNumber="3">Type</typeStatus>
species 
<taxonomicName id="8B9F4D23283DD00A17E23D5902447553" authority="Hodgson, 1910" authorityName="Hodgson" authorityYear="1910" box="[407,770,788,811]" class="Malacostraca" family="Munnopsidae" genus="Notopais" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="spicatus">
