.. 
<taxonomicName id="8B9F4D23283CD00B123438D905A370D5" box="[1089,1253,1684,1709]" class="Malacostraca" family="Munnopsidae" genus="Notopais" kingdom="Animalia" order="Isopoda" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="zealandica" status="n. sp.">N. zealandica</taxonomicName>
</emphasis>
<taxonomicNameLabel id="65D857C9283CD00B129938D9046D70D6" box="[1260,1323,1684,1710]" pageId="3" pageNumber="4" rank="species">n. sp.</taxonomicNameLabel>
</keyLead>
</paragraph>
</keyStep>
