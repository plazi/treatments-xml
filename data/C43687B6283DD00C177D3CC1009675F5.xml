<taxonomicName id="8B9F4D23283CD00B150B3D89059575A6" authority="Hodgson, 1902" authorityName="Hodgson" authorityYear="1902" box="[894,1235,964,990]" class="Malacostraca" family="Munnopsidae" genus="Notopais" kingdom="Animalia" order="Isopoda" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="spinosa" status="comb. nov.">
