<keyLead id="30BF1332FF8C8561FF4DB36A0580FE45" pageId="11" pageNumber="369">
<number id="3D86A220FF8C8561FF4DB36A00BFFEAB" box="[151,165,339,360]" pageId="11" pageNumber="369" value="2.0">2</number>
