<heading id="D0C5FB31FFB42A15C257F965FCA554F7" box="[383,864,1667,1691]" centered="true" fontSize="10" level="2" pageId="15" pageNumber="52" reason="2">
<taxonomicName id="4C3237DEFFB42A15C257F965FCA554F7" authority="Okubo, 1980" authorityName="Okubo" authorityYear="1980" box="[383,864,1667,1691]" class="Ostracoda" family="Candonidae" genus="Dolerocypria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="15" pageNumber="52" phylum="Arthropoda" rank="species" species="mukaishimensis">
