<collectionCode id="ED0BD7A1C908FFD1645F8BB8DC05FD23" box="[269,341,733,757]" collectionName="Belgium, Brussels, Institut Royal des Sciences Naturelles de Belgique" pageId="54" pageNumber="454">KBIN</collectionCode>
<specimenCode id="DBBCE71FC908FFD164328BB8DCE3FD23" box="[352,435,733,757]" pageId="54" pageNumber="454">SulO 17</specimenCode>
), pitfall trap, 
<collectingDate id="EFE0904CC908FFD167088BB8DFA3FD23" box="[602,755,733,757]" pageId="54" pageNumber="454" value="1985-10-10" valueMax="1985-10-24" valueMin="1985-10-10">
<date id="FFA469A4C908FFD167088BB8DFA3FD23" box="[602,755,733,757]" pageId="54" pageNumber="454" value="1985-10-10" valueMax="1985-10-24" valueMin="1985-10-10">10-24.x.1985</date>
</collectingDate>
, 
<collectorName id="26EF2AB2C908FFD166568BB8DEC2FD23" box="[772,914,733,757]" pageId="54" pageNumber="454">R. Bosmans</collectorName>
