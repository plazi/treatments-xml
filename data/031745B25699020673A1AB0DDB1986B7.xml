<taxonomicName id="B02E773728323E7122512B7C80473C71" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179323" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster emmae Forel" order="Hymenoptera" pageNumber="227" phylum="Arthropoda" rank="species" species="emmae" status="nov. sp.">CREMASTOGASTER EMMAE</taxonomicName>
