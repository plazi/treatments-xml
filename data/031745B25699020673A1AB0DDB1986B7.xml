<taxonomicName id="05E051AB1E1561BB8394B4DAAAB3B6DE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230977" authority="Agassiz, 1846" authorityName="Forel" authorityYear="1892" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster aberrans Forel" order="Hymenoptera" pageNumber="229" phylum="Arthropoda" rank="species" species="aberrans">C. aberrans</taxonomicName>
