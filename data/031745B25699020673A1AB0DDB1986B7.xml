<taxonomicName id="F795243BFFBBB9E5B5BDA6E09D311C13" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230977" authority="Agassiz, 1846" authorityName="Forel" authorityYear="1892" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster aberrans Forel" order="Hymenoptera" pageNumber="229" phylum="Arthropoda" rank="species" species="aberrans">C. aberrans</taxonomicName>
<normalizedToken id="08A6123A308FA5EEB4424A64E4EA5467" originalValue="n'était">n'etait</normalizedToken>
