<taxonomicName id="E2932FDA91CBC7A94CAAA781D7BEAAC2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180124" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster ranavalonae Forel" order="Hymenoptera" pageNumber="229" phylum="Arthropoda" rank="species" species="ranavalonae">C. Ranavalonae</taxonomicName>
