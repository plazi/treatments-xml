<taxonomicName id="6566825F719626B3287DF564DBC7C904" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:178358" authority="Agassiz, 1846" authorityName="Lund (Cremastogaster" authorityYear="1831" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster Lund" order="Hymenoptera" pageNumber="229" phylum="Arthropoda" rank="genus">Cremastogaster</taxonomicName>
