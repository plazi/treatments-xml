. 
<collectingCountry id="F31976783B740854AA4EFF4CFE7A4299" box="[358,464,223,249]" name="Australia" pageId="36" pageNumber="487">Australia</collectingCountry>
, 
<collectingRegion id="49CAF80A3B740854AAF2FF4CFD9D4299" box="[474,567,223,249]" country="Australia" name="Victoria" pageId="36" pageNumber="487">Victoria</collectingRegion>
