<taxonomicName id="4E24326E000E135BEB7740AECD946138" authorityName="Zhao &amp; Li" authorityYear="2017" class="Arachnida" family="Telemidae" genus="Pinelema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pinelema cunfengensis" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="cunfengensis">Pinelema cunfengensis</taxonomicName>
