[[ 
<taxonomicName id="6DC2FA6BAFFA03534065A3EEB2C19928" LSID-ZBK="urn:lsid:zoobank.org:act:B3F80107-BABB-495D-BFB7-721DCA10C4F8" class="Actinopterygii" family="Blenniidae" genus="Ecsenius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Ecsenius McCulloch 1923:121" order="Perciformes" pageNumber="3" phylum="Chordata" rank="genus">Ecsenius</taxonomicName>
