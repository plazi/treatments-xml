<typeStatus id="54918855D06F0302C5AF5F31FF1C910C" box="[151,201,1838,1859]" pageId="14" pageNumber="415">Type</typeStatus>
species: 
<taxonomicName id="4C2A4D74D06F0302C41E5F32FD24910C" authority="Gaud &amp; Atyeo, 1967" authorityName="Gaud &amp; Atyeo" authorityYear="1967" box="[294,753,1837,1859]" class="Arachnida" family="Psoroptoididae" genus="Temnalges" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="14" pageNumber="415" phylum="Arthropoda" rank="species" species="mesalgoides">
<emphasis id="B95EEAE5D06F0302C41E5F32FDC1910C" box="[294,532,1837,1859]" italics="true" pageId="14" pageNumber="415">Temnalges mesalgoides</emphasis>
<bibRefCitation id="EFBB4B06D06F0302C7235F32FD24910C" author="Gaud" box="[539,753,1837,1859]" pageId="14" pageNumber="415" refString="Gaud, J. &amp; Atyeo, W. T. (1967) Cinq genres nouveaux de la famille des Analgidae, Trouessart &amp; Megnin. Acarologia, 9, 435 - 446." type="journal article" year="1967">
Gaud &amp; Atyeo, 
<number id="3DE224D0D06F0302C7845F32FD24910C" box="[700,753,1837,1859]" pageId="14" pageNumber="415" value="1967.0">1967</number>
</bibRefCitation>
