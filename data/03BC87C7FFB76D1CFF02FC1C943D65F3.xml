<quantity id="4CED9B34FFB76D1CFE2FFBD594BF641C" box="[415,472,1084,1104]" metricMagnitude="2" metricUnit="m" metricValue="6.75" pageId="9" pageNumber="360" unit="m" value="675.0">675 m</quantity>
