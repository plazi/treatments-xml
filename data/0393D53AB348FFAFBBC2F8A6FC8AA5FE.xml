<taxonomicName id="4C3A1FAFB34FFFAFBAEFFE4BFD22A6A2" box="[442,673,476,500]" class="Magnoliopsida" family="Berberidaceae" genus="Berberis" kingdom="Plantae" order="Ranunculales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="grevilleana">
