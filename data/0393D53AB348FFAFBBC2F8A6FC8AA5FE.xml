</taxonomicName>
(
<taxonomicName id="4C3A1FAFB34FFFAFB9FAFE4BFCD0A6A3" box="[687,851,476,501]" class="Magnoliopsida" family="Berberidaceae" kingdom="Plantae" order="Ranunculales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="family">Berberidaceae</taxonomicName>
