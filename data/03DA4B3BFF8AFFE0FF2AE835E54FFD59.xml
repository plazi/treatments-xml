<collectingCountry id="F364BABDFF89FFE0FED1ECABE6C4FD59" box="[364,434,674,700]" name="China" pageId="2" pageNumber="39">China</collectingCountry>
(
<collectingRegion id="49B734CFFF89FFE0FE7CECABE55BFD59" box="[449,557,674,700]" country="China" name="Guizhou" pageId="2" pageNumber="39">Guizhou</collectingRegion>
).
</paragraph>
