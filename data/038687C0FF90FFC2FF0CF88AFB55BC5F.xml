<taxonomicName id="4C2F4D55FF91FFC1FB42FA72FA86BBE7" box="[1241,1313,1489,1511]" class="Insecta" family="Phlaeothripidae" genus="Hoplandrothrips" kingdom="Animalia" order="Thysanoptera" pageId="13" pageNumber="489" phylum="Arthropoda" rank="species" species="oreillyi" status="sp.n.">
