<paragraph id="0D78E4717B54B4EA70B6973F0645BC83" pageNumber="107">
<taxonomicName id="A629B7464592CBFBC11D5365C0054DE8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147277" authority="Wheeler" authorityName="Wheeler" class="Hexapoda" family="Formicidae" genus="Viticicola" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Viticicola Wheeler" order="Hymenoptera" pageNumber="107" phylum="Arthropoda" rank="genus">Viticicola</taxonomicName>
