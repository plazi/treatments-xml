<taxonomicName id="4C274185D808FF9CBEEDE1A12A0EFA11" authorityName="Gordon and Hanley" authorityYear="2017" box="[443,536,1476,1499]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="27" phylum="Arthropoda" rank="species" species="nina">
