<taxonomicName id="4C164D1E7909FFE4FEB9F7D7FDDEF994" baseAuthorityName="GRAVENHORST" baseAuthorityYear="1806" box="[355,526,1527,1548]" class="Insecta" family="Staphylinidae" genus="Nudobius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="479" phylum="Arthropoda" rank="species" species="lentus">
