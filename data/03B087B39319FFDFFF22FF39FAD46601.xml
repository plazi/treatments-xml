<taxonomicName id="4C194D269319FFDEFCB1FED2FBED64F9" authority="Lehman, 1979" authorityName="Lehman" authorityYear="1979" box="[772,1089,380,406]" class="Insecta" family="Chironomidae" genus="Nanocladius" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="49" phylum="Arthropoda" rank="species" species="ortsi" subGenus="Nanocladius">
