<taxonomicName id="CBF099855BB5051538B48C8E97BA3343" class="Hexanauplia" family="Cyclopinidae" genus="Mexiclopina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mexiclopina campechana" order="Cyclopoida" pageId="1" pageNumber="18" phylum="Arthropoda" rank="species" species="campechana">Mexiclopina campechana</taxonomicName>
