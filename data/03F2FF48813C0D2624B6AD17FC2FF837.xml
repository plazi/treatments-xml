<taxonomicName id="4C5B35DD81330D262128ABE9FAFEFF3E" box="[1289,1392,151,176]" class="Magnoliopsida" family="Theaceae" genus="Camellia" kingdom="Plantae" order="Ericales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="genus">
