<quantity id="691B19CD4E62B56A73CEFBC77465C43C" box="[710,813,1068,1093]" metricMagnitude="-2" metricUnit="m" metricValue="1.33" pageId="78" pageNumber="577" unit="mm" value="13.3">13.3 mm</quantity>
, cw 
