<taxonomicName id="4C32DAF7FF8BCB0146CA7345FDA3FBCB" baseAuthorityName="Strom" baseAuthorityYear="1768" box="[410,516,1035,1061]" class="Arachnida" family="Hygrobatidae" genus="Hygrobates" kingdom="Animalia" order="Trombidiformes" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="fluviatilis">
