<paragraph id="EA93C659C684F6C33A6D055B0177E20A" pageId="0" pageNumber="9042">
<taxonomicName id="18975A0D21D128717518F8278B067A06" class="Insecta" family="Ichneumonidae" genus="Polytribax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polytribax" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Polytribax</taxonomicName>
