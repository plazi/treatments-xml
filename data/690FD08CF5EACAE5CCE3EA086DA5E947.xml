<taxonomicName id="2764385C9FA0B4DCFC640EC0B274D561" genus="Gabuniina" lsidName="Gabuniina" order="Lepidoptera" pageId="0" pageNumber="9042" rank="order">Lepidoptera</taxonomicName>
