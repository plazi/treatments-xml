<number id="F4304B798F7B6034FC91FC42FCD9B09D" box="[840,854,974,998]" pageId="1" pageNumber="241" value="1.0">1</number>
species: 
<taxonomicName id="85F822DD8F7B6034FC64FC43FBA1B09D" box="[957,1070,974,998]" class="Malacostraca" family="Talitridae" genus="Minamitalitrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="241" phylum="Arthropoda" rank="species" species="zoltani" status="sp. nov.">
<emphasis id="708C854C8F7B6034FC64FC43FBA1B09D" box="[957,1070,974,998]" italics="true" pageId="1" pageNumber="241">M. zoltani</emphasis>
</taxonomicName>
