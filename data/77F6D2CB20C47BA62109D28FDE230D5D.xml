13. 
<taxonomicName id="29B36F54045AFCFD4C70F08B3BA0CFCF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:239301" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus nosibeensis" order="Hymenoptera" pageNumber="48" phylum="Arthropoda" rank="species" species="nosibeensis">CAMPONOTUS NOSIBEENSIS</taxonomicName>
