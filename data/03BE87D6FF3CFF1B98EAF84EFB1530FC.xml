<quantity id="4CEF9B25FF3FFF1B9B46FF48F93530D0" box="[827,884,186,213]" metricMagnitude="1" metricUnit="m" metricValue="2.5" pageId="174" pageNumber="175" unit="m" value="25.0">25 m</quantity>
