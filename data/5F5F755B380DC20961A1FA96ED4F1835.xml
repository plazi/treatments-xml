<treatmentCitationGroup id="F7E6E363380DC20961A1FA01EDD41BE1" box="[151,919,1439,1464]" pageId="11" pageNumber="531">
<taxonomicName id="10F6BFCE380DC20961A1FA01EC771BE1" authority="Lacordaire, 1869: 165" authorityName="Lacordaire" authorityPageNumber="165" authorityYear="1869" box="[151,564,1439,1464]" class="Insecta" family="Cerambycidae" genus="Aphylax" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="531" phylum="Arthropoda" rank="species" species="mentiens">
<emphasis id="E582185F380DC20961A1FA01EF081BE1" box="[151,331,1440,1464]" italics="true" pageId="11" pageNumber="531">Aphylax mentiens</emphasis>
<treatmentCitation id="5657E25C380DC2096067FA3EEC771BE1" author="Lacordaire, J. T." box="[337,564,1439,1464]" page="165" pageId="11" pageNumber="531" year="1869">
<bibRefCitation id="B367B9BC380DC2096067FA3EEC421BE1" author="Lacordaire, J. T." box="[337,513,1439,1464]" pageId="11" pageNumber="531" refId="ref6392" refString="Lacordaire, J. T. (1869) Histoire Naturelle des Insectes. Genera des Coleopteres, ou expose methodique et critique de tous les genres proposes jusqu'ici dans cet ordre d'insectes. Vol. 9 (1). Librairie Encyclopedique de Roret, Paris, 409 pp." type="book" year="1869">Lacordaire, 1869</bibRefCitation>
