<taxonomicName id="652F817FAA2B820C8498EDC696AE4162" LSID="8C20C01F-3982-0F74-6F8B-7D2110695CE3" authority="Roxb." authorityName="Roxb." class="Magnoliopsida" family="Fabaceae" genus="Butea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Butea superba" order="Fabales" pageId="79" pageNumber="80" phylum="Tracheophyta" rank="species" species="superba">Butea superba Roxb.</taxonomicName>
