<quantity id="283303E0FFF1F5291C4C39E0FC0C42BD" box="[813,931,1733,1757]" metricMagnitude="2" metricUnit="m" metricValue="6.25" metricValueMax="6.5" metricValueMin="6.0" pageId="32" pageNumber="161" unit="m" value="625.0" valueMax="650.0" valueMin="600.0">
