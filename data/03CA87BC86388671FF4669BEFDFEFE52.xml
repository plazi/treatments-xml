<taxonomicName id="4C634D29863A8671FF136984FE7DFE52" authority="Section" authorityName="Section" box="[194,431,392,418]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="22" phylum="Arthropoda" rank="order">Lepidoptera Section</taxonomicName>
at 
<collectionCode id="ED72AE6F863A8671FE026984FDFAFE52" LSID="urn:lsid:biocol.org:col:34613" box="[467,552,392,418]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="3" pageNumber="22">ZFMK</collectionCode>
.
