<typeStatus id="54D7C1CFFFDF3B25FCDAB8A9FC41FA0D" box="[839,907,1484,1510]" pageId="6" pageNumber="35">types</typeStatus>
of 
<taxonomicName id="4C6C04EEFFDF3B25FC24B8A9FA57FA0D" authority="Cabanis" authorityName="Cabanis" box="[953,1437,1484,1510]" class="Insecta" family="Curculionidae" genus="Mitrephorus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="ochraceiventris">
