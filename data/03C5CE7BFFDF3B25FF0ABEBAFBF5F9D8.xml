<number id="3DA46D4AFFDF3B25FE6FB8C0FDF8FA54" box="[498,562,1445,1471]" pageId="6" pageNumber="35" value="1875.0">1875</number>
: 
<number id="3DA46D4AFFDF3B25FDA3B8C0FDA6FA54" box="[574,620,1445,1471]" pageId="6" pageNumber="35" value="538.0">538</number>
)
</bibRefCitation>
