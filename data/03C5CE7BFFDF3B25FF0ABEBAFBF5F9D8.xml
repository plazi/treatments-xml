<collectionCode id="ED7DE7A8FFDF3B25FE7BB9D8FDECFB3C" box="[486,550,1213,1239]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="6" pageNumber="35">ZMB</collectionCode>
