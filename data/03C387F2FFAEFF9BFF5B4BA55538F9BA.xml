<taxonomicName id="4C6A4D67FFAEFF9BFB204A45531CFA2C" box="[1260,1380,1383,1408]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="genus">
