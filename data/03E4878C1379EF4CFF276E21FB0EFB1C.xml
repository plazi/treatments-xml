<taxonomicName id="4C4D4D191379EF4CFF276E21FDD4FCBC" authority="Pascoe, 1867" authorityName="Pascoe" authorityYear="1867" box="[151,535,795,822]" class="Insecta" family="Cerambycidae" genus="Oberea" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="578" phylum="Arthropoda" rank="species" species="gracillima">
<emphasis id="B939EA881379EF4CFF276E21FDD4FCBC" bold="true" box="[151,535,795,822]" pageId="1" pageNumber="578">
<emphasis id="B939EA881379EF4CFF276E21FEB1FCBF" bold="true" box="[151,370,795,821]" italics="true" pageId="1" pageNumber="578">Oberea gracillima</emphasis>
Pascoe, 
<number id="3D8524BD1379EF4CFE6C6E26FDD4FCBC" box="[476,535,796,822]" pageId="1" pageNumber="578" value="1867.0">1867</number>
</emphasis>
