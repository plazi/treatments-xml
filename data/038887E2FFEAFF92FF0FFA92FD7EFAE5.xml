<taxonomicName id="4C214D77FFEBFF90FE20FB56FDFFFB11" authorityName="Mengual &amp; Ghorpadé" authorityYear="2010" box="[419,525,1223,1245]" class="Insecta" family="Syrphidae" genus="Eosphaerophoria" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="56" phylum="Arthropoda" rank="species" species="adornata">
<emphasis id="B955EAE6FFEBFF90FE20FB56FDFFFB11" box="[419,525,1223,1245]" italics="true" pageId="17" pageNumber="56">E. adornata</emphasis>
</taxonomicName>
