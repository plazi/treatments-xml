<taxonomicName id="4C0CBEB8FFF5AE2AFD33AE09CEF3F8A3" authority="Jacobi" authorityName="Jacobi" box="[722,899,1884,1906]" class="Insecta" family="Cercopidae" genus="Simorhina" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="233" phylum="Arthropoda" rank="genus">
