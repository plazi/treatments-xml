<taxonomicName id="A5E04D707A68222D7F28F99D63EEF99E" class="Aves" family="Psittacidae" genus="Coriphilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psittaciformes" pageId="28" pageNumber="491" phylum="Chordata" rank="species" species="solitarius">Coriphilus solitarius</taxonomicName>
