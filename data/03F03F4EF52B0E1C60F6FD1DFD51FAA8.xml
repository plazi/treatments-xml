<collectionCode id="ED48169DF52B0E1C652AFB15FA72FB3C" LSID="urn:lsid:biocol.org:col:34802" box="[1355,1436,1159,1183]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34802" name="Natural History Museum of Los Angeles County" pageId="16" pageNumber="567">LACM</collectionCode>
