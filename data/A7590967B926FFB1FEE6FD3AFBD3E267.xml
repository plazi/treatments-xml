<taxonomicName id="E8F0C3F2B926FFB1FD62FD3AFC79E489" authority="Wolff, 1811" authorityName="Wolff" authorityYear="1811" box="[693,973,642,668]" class="Insecta" family="Reduviidae" genus="Empicoris" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="17" phylum="Arthropoda" rank="genus">
