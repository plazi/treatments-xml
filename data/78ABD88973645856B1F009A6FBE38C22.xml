Thwaites, 
<taxonomicName id="0833C3990721E7D48260779CA91ACE07" genus="C." higherTaxonomySource="GBIF,CoL" kingdom="Chromista" lsidName="C. rectangula" pageId="0" pageNumber="31" phylum="Ochrophyta" rank="species" species="rectangula">
