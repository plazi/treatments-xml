<taxonomicName id="96D253DBFFD7117EFF429E648425A9A8" authorityName="Osborn" authorityYear="1905" box="[164,370,1385,1418]" class="Reptilia" family="Tyrannosauridae" genus="Tyrannosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="1" phylum="Chordata" rank="species" species="rex">
