<subSubSection id="4CEE2A80FF827D4902FAFC4BAD5CF75C" pageId="13" type="description">
<paragraph id="044B790BFF827D4902FAFC4BAD5CF75C" blockId="13.[151,1436,753,1030]" pageId="13">
<emphasis id="3680A519FF827D4902FAFC4BAFEAF0B8" bold="true" box="[199,449,969,994]" pageId="13">Taxonomic remarks.</emphasis>
<taxonomicName id="C3F40288FF827D4903F1FC4BAC98F0BB" authorityName="Quiroga" authorityYear="2008" baseAuthorityName="Marcus" baseAuthorityYear="1952" box="[460,691,969,994]" class="Rhabditophora" family="Pseudocerotidae" genus="Phrikoceros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="13" phylum="Platyhelminthes" rank="species" species="mopsus">
