, 
<taxonomicName id="708AF8B44545A541708AA193DE0E0C82" class="Insecta" family="Crambidae" genus="Diatraea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diatraea magnifactella" order="Lepidoptera" pageId="110" pageNumber="111" phylum="Arthropoda" rank="species" species="magnifactella">Diatraea magnifactella</taxonomicName>
