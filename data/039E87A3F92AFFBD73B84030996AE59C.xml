<taxonomicName id="4C374D36F92AFFBD73B4400C9E64E44A" authority="Spencer, 1884, p. 42" authorityName="Spencer" authorityPageNumber="42" authorityYear="1884" class="Graptolithina" family="Cyclograptidae" genus="Cyclograptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tuboidea" pageId="23" pageNumber="328" phylum="Hemichordata" rank="species" species="rotadentatus">
