<taxonomicName id="4C6EEED6FFFBFFBD1BBD63572A6CBDEC" box="[1229,1335,368,392]" class="Insecta" family="Gerridae" genus="Gerris" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="43" phylum="Arthropoda" rank="species" species="costae">
