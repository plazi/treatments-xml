<taxonomicName id="4C734D5A0126DA46FA10F9463D7C2D51" authorityName="Clay" authorityYear="1937" class="Insecta" family="Philopteridae" genus="Strongylocotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="fimbriatus">
