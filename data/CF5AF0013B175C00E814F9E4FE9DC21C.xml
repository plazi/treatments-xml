<emphasis id="75879D053B145C00EA1FFECFFE33C72E" bold="true" box="[333,393,289,313]" pageId="14" pageNumber="813">
<collectingCountry id="3FE401873B145C00EA1FFECFFE3EC72E" box="[333,388,289,313]" name="Palau" pageId="14" pageNumber="813">Palau</collectingCountry>
