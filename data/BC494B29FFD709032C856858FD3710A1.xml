<taxonomicName id="F3E081BCFFD109022CEC684FFD821002" authorityName="Lyal" authorityYear="2000" box="[488,630,1565,1589]" class="Insecta" family="Curculionidae" genus="Alcidodes" kingdom="Animalia" order="Coleoptera" pageId="76" pageNumber="1817" phylum="Arthropoda" rank="species" species="walliorum">
