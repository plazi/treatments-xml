<taxonomicName id="F3E081BCFFD109022CA06BAFFDCB1022" authorityName="Lyal" authorityYear="2000" box="[420,575,1533,1557]" class="Insecta" family="Curculionidae" genus="Alcidodes" kingdom="Animalia" order="Coleoptera" pageId="76" pageNumber="1817" phylum="Arthropoda" rank="species" species="sterryorum">
