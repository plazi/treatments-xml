<taxonomicName id="F3E081BCFFD109022FAB684FFCEE1002" authorityName="Lyal" authorityYear="2000" box="[687,794,1565,1589]" class="Insecta" family="Curculionidae" genus="Alcidodes" kingdom="Animalia" order="Coleoptera" pageId="76" pageNumber="1817" phylum="Arthropoda" rank="species" species="fugitus">
<emphasis id="0694262DFFD109022FAB684FFD361002" box="[687,706,1565,1589]" italics="true" pageId="76" pageNumber="1817">A</emphasis>
