<emphasis id="0B9FEAAEFFB6707A3E83B7C2FDF2FE07" bold="true" box="[151,590,474,501]" pageId="33" pageNumber="254">
<taxonomicName id="FEEB4D3FFFB6707A3E83B7C2FE1BFE07" baseAuthorityName="Yosii" baseAuthorityYear="1966" box="[151,423,474,501]" class="Collembola" family="Orchesellidae" genus="Dicranocentrus" kingdom="Animalia" order="Entomobryomorpha" pageId="33" pageNumber="254" phylum="Arthropoda" rank="species" species="indicus">
<emphasis id="0B9FEAAEFFB6707A3E83B7C2FE1BFE07" bold="true" box="[151,423,474,501]" italics="true" pageId="33" pageNumber="254">Dicranocentrus indicus</emphasis>
</taxonomicName>
