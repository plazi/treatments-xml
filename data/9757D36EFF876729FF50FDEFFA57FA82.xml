<taxonomicName id="D8FE19FBFF876729FAD9FDA8FA4CFDFC" box="[1310,1415,615,636]" class="Liliopsida" family="Agavaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="1" pageNumber="58" phylum="Magnoliophyta" rank="family">Agavaceae</taxonomicName>
