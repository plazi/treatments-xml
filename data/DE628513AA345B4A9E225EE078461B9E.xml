<taxonomicName id="41ED6E8D5C8E904864DF6A4DFF4C5E5B" authorityName="Regimbart" authorityYear="1895" class="Insecta" family="Haliplidae" genus="Peltodytes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Peltodytes quadratus" order="Coleoptera" pageId="17" pageNumber="102" phylum="Arthropoda" rank="species" species="quadratus">
