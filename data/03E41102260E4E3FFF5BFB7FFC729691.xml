</taxonomicName>
(
<taxonomicName id="4C4DDB97260E4E3FFA97FAB6FF3C9561" class="Ostracoda" family="Cypridinidae" genus="Doloria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
<emphasis id="B9397C06260E4E3FFA97FAB6FF3C9561" italics="true" pageId="3" pageNumber="4">Doloria</emphasis>
</taxonomicName>
