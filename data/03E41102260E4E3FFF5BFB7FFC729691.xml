<taxonomicName id="4C4DDB97260E4E3FFEE7FACEFE769561" box="[299,498,1362,1387]" class="Ostracoda" family="Cypridinidae" genus="Doloria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="levis" subGenus="Doloria">
<emphasis id="B9397C06260E4E3FFEE7FACEFEC29561" box="[299,326,1364,1387]" italics="true" pageId="3" pageNumber="4">D.</emphasis>
(
<emphasis id="B9397C06260E4E3FFE99FAC9FE2A9561" box="[341,430,1363,1387]" italics="true" pageId="3" pageNumber="4">Doloria</emphasis>
