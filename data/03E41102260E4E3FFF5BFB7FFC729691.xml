<taxonomicName id="4C4DDB97260E4E3FFEDFFAB6FEE8954E" box="[275,364,1324,1348]" class="Ostracoda" family="Cypridinidae" genus="Doloria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
