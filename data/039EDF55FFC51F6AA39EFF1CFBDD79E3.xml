; 
<collectorName id="26C20B95FFC41F6AA0ADFE2DFAEB7B94" box="[932,1288,422,448]" pageId="9" pageNumber="41">Queen Sirikit Botanic Garden</collectorName>
, on the bark of 
<taxonomicName id="4C3715C0FFC41F6AA3B0FE46FDE37BB2" box="[185,512,461,486]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Dipterocarpus" kingdom="Plantae" order="Malvales" pageId="9" pageNumber="41" phylum="Tracheophyta" rank="species" species="obtusifolius">
