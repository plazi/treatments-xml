<taxonomicName id="95C0A9FBFFECFFECFC1BF9BA8C0486C2" authorityName="Trägårdh" authorityYear="1937" box="[953,1118,1655,1679]" class="Arachnida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="3" pageNumber="7" phylum="Arthropoda" rank="superFamily" superFamily="Fedrizzioidea">Fedrizzioidea</taxonomicName>
