<collectingCountry id="11C9A9D9C61D1B28FBEDF9D8FB34F8CB" box="[1094,1201,1790,1814]" name="Maldives" pageId="27" pageNumber="28">Maldives</collectingCountry>
, 
<collectingCountry id="11C9A9D9C61D1B28FB68F9D9FA83F8CB" box="[1219,1286,1791,1814]" name="Japan" pageId="27" pageNumber="28">Japan</collectingCountry>
