<taxonomicName id="4C3768B7FFB9FFECFEF49FCBFE4E4585" box="[275,391,152,177]" class="Insecta" family="Reduviidae" genus="Endochus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="43" phylum="Arthropoda" rank="genus">
