<taxonomicName id="4C3768B7FFB8FFEDFEC79880FE5042D8" box="[288,409,2003,2028]" class="Insecta" family="Reduviidae" genus="Endochus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="42" phylum="Arthropoda" rank="genus">
