<collectingCountry id="F35713C0FFD78B458209FDF3D4F7FDDD" box="[284,388,562,588]" name="Thailand" pageId="15" pageNumber="16">Thailand</collectingCountry>
(
<collectingRegion id="49849DB2FFD78B458280FDF3D7D4FDDD" box="[405,679,562,588]" country="Thailand" name="Nakhon Si Thammarat" pageId="15" pageNumber="16">Nakhon Si Thammarat</collectingRegion>
, 
<collectingRegion id="49849DB2FFD78B4581A1FDF2D632FDDD" box="[692,833,562,589]" country="Thailand" name="Surat Thani" pageId="15" pageNumber="16">Surat Thani</collectingRegion>
