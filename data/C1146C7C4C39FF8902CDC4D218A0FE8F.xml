<collectingCountry id="31AA9DFA4C37FF8903BDC13C1E61FE8E" box="[487,538,260,285]" name="Italy" pageId="8" pageNumber="559">Italy</collectingCountry>
(Balsamo &amp; Todaro 
<number id="FF75CF4D4C37FF89015CC13C1F3BFE8E" box="[774,832,260,285]" pageId="8" pageNumber="559" value="1995.0">1995</number>
) and 
<collectingCountry id="31AA9DFA4C37FF890124C13C1FACFE8E" box="[894,983,260,285]" name="Sweden" pageId="8" pageNumber="559">Sweden</collectingCountry>
(Kånneby 
<emphasis id="7BC901784C37FF890609C13D18FFFE8F" box="[1107,1156,260,284]" italics="true" pageId="8" pageNumber="559">et al</emphasis>
