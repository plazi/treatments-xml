<taxonomicName id="1E689AE6A9BED627E9A352FA70E6FDCA" LSID="CDCCFF29-FDDF-D76B-1D5E-29484C61DEB9" authority="Gleason" authorityName="Gleason" authorityYear="1941" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia chiriquensis" order="Myrtales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="chiriquensis">
<pageBreakToken id="A6C58841D79C3A30A67582A12E243C22" pageId="57" pageNumber="58" start="start">Conostegia</pageBreakToken>
chiriquensis Gleason
