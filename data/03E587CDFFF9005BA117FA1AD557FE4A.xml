<quantity id="4CB49B3EFFFE005AA237FA62D547FA33" box="[526,587,1426,1444]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="4" pageNumber="77" unit="mm" value="10.0">10 mm</quantity>
