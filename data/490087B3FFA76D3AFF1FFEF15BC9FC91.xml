, 
<collectingCountry id="B9BE7635FFA76D3AFF1FFD5D5B13FCFD" box="[151,277,745,770]" name="Philippines" pageId="26" pageNumber="127">Philippines</collectingCountry>
from 
<quantity id="06519B40FFA76D3AFED1FD5E5BA6FCFE" box="[345,416,746,770]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="26" pageNumber="127" unit="m" value="200.0">200 m</quantity>
