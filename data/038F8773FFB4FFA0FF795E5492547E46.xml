<heading id="D0D18109FFB4FFA1FF795E54938E7A23" bold="true" box="[151,927,1374,1400]" fontSize="11" level="1" pageId="4" pageNumber="305" reason="1">
<emphasis id="B952EA77FFB4FFA1FF795E54938E7A23" bold="true" box="[151,927,1374,1400]" pageId="4" pageNumber="305">
<taxonomicName id="4C264DE6FFB4FFA1FF795E5492167A23" authorityName="Vitzthum" authorityYear="1923" box="[151,519,1374,1400]" class="Arachnida" family="Parasitidae" genus="Holoparasitus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="hemisphaericus">
<emphasis id="B952EA77FFB4FFA1FF795E5492167A23" bold="true" box="[151,519,1374,1400]" italics="true" pageId="4" pageNumber="305">Holoparasitus hemisphaericus</emphasis>
