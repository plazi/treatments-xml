<paragraph id="65D0BD258F495345A118A8D2431BAC37" pageId="18" pageNumber="43">Taxonomical note.</paragraph>
<paragraph id="C883F1D232AD982CFA5020705CFB5024" pageId="18" pageNumber="43">
<taxonomicName id="9059627DCD31B91FFCBFCABFC321C37D" genus="Meteusarcus" lsidName="Meteusarcus armatus" pageId="18" pageNumber="43" rank="species" species="armatus">Meteusarcus armatus</taxonomicName>
