<taxonomicName id="CFD6FC8BFFBD7E7B6BFFEE04FE99FAD2" box="[236,364,1307,1332]" class="Arachnida" family="Eriophyidae" genus="Epitrimerus" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="lobatiae">
