<taxonomicName id="CFD6FC8BFFBD7E7B6835EFECFC52FAEA" box="[806,935,1267,1292]" class="Arachnida" family="Eriophyidae" genus="Epitrimerus" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="lobatiae">
<emphasis id="3AA25B1AFFBD7E7B6835EFECFC52FAEA" box="[806,935,1267,1292]" italics="true" pageId="9" pageNumber="10">E. lobatiae</emphasis>
</taxonomicName>
