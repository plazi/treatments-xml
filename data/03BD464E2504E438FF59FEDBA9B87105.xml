–SE, 
<quantity id="4CEC5ABD2504E438FD61FE93A8E972D6" box="[687,748,366,386]" metricMagnitude="4" metricUnit="m" metricValue="2.5" pageId="1" pageNumber="664" unit="km" value="25.0">25 km</quantity>
