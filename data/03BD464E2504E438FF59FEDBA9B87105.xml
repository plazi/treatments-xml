<quantity id="4CEC5ABD2504E438FC05FEABAE03723E" box="[971,1030,342,362]" metricMagnitude="4" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="664" unit="km" value="10.0">10 km</quantity>
