, height 
<quantity id="4CC59B3AFFEFFF811C485840FD0BFCB6" box="[594,760,866,890]" metricMagnitude="-4" metricUnit="m" metricValue="1.65" metricValueMax="2.1" metricValueMin="1.2" pageId="18" pageNumber="206" unit="mm" value="0.16499999999999998" valueMax="0.21" valueMin="0.12">0.12–0.21 mm</quantity>
