<paragraph id="752E3C5DFFB8FFBEFF23FE27FDDFFE4E" blockId="77.[151,598,476,503]" box="[151,598,476,503]" pageId="77" pageNumber="78">
<heading id="2E668B31FFB8FFBEFF23FE27FDDFFE4E" bold="true" box="[151,598,476,503]" fontSize="11" level="1" pageId="77" pageNumber="78" reason="1">
<emphasis id="47E5E04FFFB8FFBEFF23FE27FDDFFE4E" bold="true" box="[151,598,476,503]" pageId="77" pageNumber="78">
The 
<taxonomicName id="B29147DEFFB8FFBEFF7BFE27FE22FE4F" authorityName="Grall &amp; Jäger" authorityYear="2020" box="[207,427,476,503]" class="Arachnida" family="Sparassidae" genus="Sinopoda" kingdom="Animalia" order="Araneae" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="chiangmaiensis">
<emphasis id="47E5E04FFFB8FFBEFF7BFE27FE22FE4F" bold="true" box="[207,427,476,503]" italics="true" pageId="77" pageNumber="78">S. chiangmaiensis</emphasis>
</taxonomicName>
