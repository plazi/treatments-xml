; metatarsus + tarsus: I 
<number id="193D833CFFE1FFC6FCBA6DC6BAF8A503" box="[870,922,511,535]" pageId="7" pageNumber="593" value="0.76">0.76</number>
, II 
<number id="193D833CFFE1FFC6FC1A6DC6BA98A503" box="[966,1018,511,535]" pageId="7" pageNumber="593" value="0.68">0.68</number>
, III 
<number id="193D833CFFE1FFC6FBF26DC6BD00A503" box="[1070,1122,511,535]" pageId="7" pageNumber="593" value="0.99">0.99</number>
, IV 
<number id="193D833CFFE1FFC6FB446DC6BDAEA503" box="[1176,1228,511,535]" pageId="7" pageNumber="593" value="0.87">0.87</number>
