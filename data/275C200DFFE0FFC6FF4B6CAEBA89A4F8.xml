<taxonomicName id="68F5EA98FFE1FFC6FEEF6E1CBB3CA528" authority="Ruiz. Abdomen" authorityName="Ruiz. Abdomen" box="[307,606,548,572]" class="Arachnida" family="Salticidae" genus="Soesilarishius" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="593" phylum="Arthropoda" rank="species" species="lunatus">
