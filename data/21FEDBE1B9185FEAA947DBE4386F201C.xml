<taxonomicName id="690A0D31894F583F96EF942B74A8D2E1" authority="Jousseaume, 1900" authorityName="Jousseaume" authorityYear="1900" class="Gastropoda" family="Odontostomidae" genus="Bonnanius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bonnanius bouvieri" order="Stylommatophora" pageId="5" pageNumber="458" phylum="Mollusca" rank="species" species="bouvieri">
