<taxonomicName id="0516FF151F22FB738C971B05EFF96183" authority="Jousseaume, 1900" authorityName="Jousseaume" authorityYear="1900" class="Gastropoda" family="Odontostomidae" genus="Bonnanius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bonnanius bouvieri" order="Stylommatophora" pageId="5" pageNumber="458" phylum="Mollusca" rank="species" species="bouvieri">
