<taxonomicName id="4C0B4D2AFF5138089B64F8ADFE605B10" box="[365,498,1805,1830]" class="Magnoliopsida" family="Combretaceae" genus="Terminalia" kingdom="Plantae" order="Myrtales" pageId="8" pageNumber="47" phylum="Tracheophyta" rank="genus">
<emphasis id="B97FEABBFF5138089B64F8ADFE605B10" box="[365,498,1805,1830]" italics="true" pageId="8" pageNumber="47">Terminalia</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="4C0B4D2AFF5138089826F8ADFD775B11" box="[559,741,1805,1831]" class="Magnoliopsida" family="Combretaceae" kingdom="Plantae" order="Myrtales" pageId="8" pageNumber="47" phylum="Tracheophyta" rank="family">Combretaceae</taxonomicName>
), collected by Guo-Quan Wang. 
<emphasis id="B97FEABBFF5138089E73F8ADFB6F5B11" bold="true" box="[1146,1277,1805,1831]" pageId="8" pageNumber="47">
<typeStatus id="54B0880BFF5138089E73F8ADFB6F5B11" box="[1146,1277,1805,1831]" pageId="8" pageNumber="47">Paratypes</typeStatus>
</emphasis>
, 
<specimenCount id="9D0DFD20FF5138089F07F8ADFA165B11" box="[1294,1412,1805,1831]" pageId="8" pageNumber="47" type="female">
<number id="3DC3248EFF5138089F07F8ADFA8F5B11" box="[1294,1309,1805,1831]" pageId="8" pageNumber="47" value="6.0">6</number>
females
</specimenCount>
<specimenCount id="9D0DFD20FF5138089F84F8ADFF4C5B7A" pageId="8" pageNumber="47" type="male">
<number id="3DC3248EFF5138089F84F8ADFA0E5B11" box="[1421,1436,1805,1831]" pageId="8" pageNumber="47" value="2.0">2</number>
males
</specimenCount>
, were mounted on 
<number id="3DC3248EFF5138089BB5F892FE595B7A" box="[444,459,1842,1868]" pageId="8" pageNumber="47" value="8.0">8</number>
