(
<taxonomicName id="3AF296762176ABAF1A87DCBEF81F5EE2" class="Magnoliopsida" family="Proteaceae" genus="Protea" kingdom="Plantae" lsidName="" order="Proteales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="family">Proteaceae</taxonomicName>
