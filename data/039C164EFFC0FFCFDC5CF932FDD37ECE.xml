<subSubSection id="C32FF4D3FFC0FFCCDC5CF932FDD37E20" box="[110,585,1061,1084]" pageId="65" pageNumber="66" type="nomenclature">
<paragraph id="8B8AA758FFC0FFCCDC5CF932FDD37E20" blockId="65.[110,585,1061,1084]" box="[110,585,1061,1084]" pageId="65" pageNumber="66">
<heading id="D0C21034FFC0FFCCDC5CF932FDD37E20" box="[110,585,1061,1084]" centered="true" fontSize="9" level="2" pageId="65" pageNumber="66" reason="2">
<taxonomicName id="4C35DCDBFFC0FFCCDC5CF932FE287E20" box="[110,434,1061,1084]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="immaculatus" status="new species">
<emphasis id="B9417B4AFFC0FFCCDC5CF932FE287E20" bold="true" box="[110,434,1061,1084]" italics="true" pageId="65" pageNumber="66">Paramblynotus immaculatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFC0FFCCDDF0F931FDD37E20" box="[450,585,1062,1084]" pageId="65" pageNumber="66" rank="species">new species</taxonomicNameLabel>
