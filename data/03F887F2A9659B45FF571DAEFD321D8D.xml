<taxonomicName id="4C514D67A9659B46FDB51E2EFCB41B06" baseAuthorityName="Womersley" baseAuthorityYear="1960" box="[629,853,1602,1628]" class="Arachnida" family="Laelapidae" genus="Ololaelaps" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="356" phylum="Arthropoda" rank="species" species="obovata">
