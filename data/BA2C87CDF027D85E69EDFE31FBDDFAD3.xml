<quantity id="F57D9B3EF027D85E6881FDB7FE29FDBB" box="[463,508,544,564]" metricMagnitude="3" metricUnit="m" metricValue="5.0" pageId="62" pageNumber="445" unit="km" value="5.0">5 km</quantity>
