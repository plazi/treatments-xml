<quantity id="291D9B4795485B599BA4D0B3FD3C05A4" box="[652,761,1691,1715]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.2" metricValueMin="0.8" pageId="3" pageNumber="57" unit="mm" value="10.0" valueMax="12.0" valueMin="8.0">8–12 mm</quantity>
