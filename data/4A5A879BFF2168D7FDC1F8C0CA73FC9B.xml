<geoCoordinate id="A7C7504AFF2E68D7FAFDFE4BCA66FE37" box="[1290,1393,419,447]" degrees="71" direction="south" minutes="22" orientation="latitude" pageId="168" pageNumber="169" precision="925" value="-71.36667">71°22ʹ S</geoCoordinate>
