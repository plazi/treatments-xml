<taxonomicName id="4C3307D0FF1AFFE5FED4F832FD88F82B" authority="Yau &amp; Marshall, 2021" authorityName="Yau &amp; Marshall" authorityYear="2021" box="[340,587,1944,1968]" class="Insecta" family="Sphaeroceridae" genus="Stipulosina" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="264" phylum="Arthropoda" rank="species" species="amphibarba" status="sp. n.">
