<taxonomicName id="14BBE292FFAEFFA3FA54B6D5DC4F2030" authorityName="Koshantschikov. These" authorityYear="1894" class="Insecta" family="Aphodiidae" genus="Aphodius" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="variicolor">
