<taxonomicName id="14BBE292FFAEFFA3FEB5B6D5DC832012" box="[356,457,1770,1793]" class="Insecta" family="Aphodiidae" genus="Aphodius" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="genus">
