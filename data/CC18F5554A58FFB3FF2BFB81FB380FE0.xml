<number id="F27956644A58FFB0FEEFFAADFE360864" box="[339,452,1328,1352]" pageId="25" pageNumber="176" value="1.125" valueMax="1.13" valueMin="1.12">1.12–1.13</number>
; pronotal length/width ratio: 
<number id="F27956644A58FFB0FCB6FAADFCC90864" box="[778,827,1328,1352]" pageId="25" pageNumber="176" value="0.41">0.41</number>
