(figs 
<number id="3D9424B3E137FFA0DB4EFA2FFE721093" box="[325,399,1479,1505]" pageId="9" pageNumber="10" value="18.5" valueMax="21.0" valueMin="16.0">16–21</number>
)
</paragraph>
