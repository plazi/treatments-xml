<date id="FFB9102FFFC4417A4768FE6FFC53FE5F" box="[787,907,404,428]" pageId="4" pageNumber="233" value="2011" valueMax="2013">
<year id="FFAF77BCFFC4417A4768FE6FFC93FE5F" box="[787,843,404,428]" pageId="4" pageNumber="233">2011</year>
-2013
</date>
and 
<pagination id="B2CEC81BFFC4417A47BEFE6FFBE5FE5E" box="[965,1085,404,429]" pageId="4" pageNumber="233" type="pagination">
