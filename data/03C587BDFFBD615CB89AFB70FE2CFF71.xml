<taxonomicName id="4C6C4D28FFBE615CB89AFF41FE38FF14" authority="Burlini, 1969" authorityName="Burlini" authorityYear="1969" box="[151,414,151,176]" class="Insecta" family="Chrysomelidae" genus="Cerodens" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="335" phylum="Arthropoda" rank="genus">
