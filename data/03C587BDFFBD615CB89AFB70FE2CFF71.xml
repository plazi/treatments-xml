<taxonomicName id="4C6C4D28FFBE615CBCE6FF41FA3AFF14" box="[1259,1436,152,176]" class="Insecta" family="Chrysomelidae" genus="Cryptocephalus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="335" phylum="Arthropoda" rank="genus" sensu="stricto">
<emphasis id="B918EAB9FFBE615CBCE6FF41FA3AFF14" box="[1259,1436,152,176]" italics="true" pageId="2" pageNumber="335">Cryptocephalus</emphasis>
