and 
<taxonomicName id="DAEF4D63FF964C5723F75887FDE32DC3" authorityName="Zhang &amp; Rasnitsyn" authorityYear="2006" box="[479,595,1460,1484]" class="Insecta" family="Anaxyelidae" genus="Brachysyntexis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="334" phylum="Arthropoda" rank="species" species="robusta">
