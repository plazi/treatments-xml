the proclined 3r-m; from 
<taxonomicName id="DAEF4D63FF964C5722FE58EAFEDB2DFF" baseAuthorityName="Rasnitsyn" baseAuthorityYear="1968" box="[214,363,1496,1520]" class="Insecta" family="Anaxyelidae" genus="Brachysyntexis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="334" phylum="Arthropoda" rank="species" species="brachyura">
<emphasis id="2F9BEAF2FF964C5722FE58EAFEDB2DFF" box="[214,363,1496,1520]" italics="true" pageId="2" pageNumber="334">B. brachyura</emphasis>
</taxonomicName>
, 
<emphasis id="2F9BEAF2FF964C57235458EAFE632DFF" box="[380,467,1497,1520]" italics="true" pageId="2" pageNumber="334">B. nova</emphasis>
, and 
<taxonomicName id="DAEF4D63FF964C57203D58EAFD3A2DFF" authorityName="Zhang &amp; Rasnitsyn" authorityYear="2006" box="[533,650,1496,1520]" class="Insecta" family="Anaxyelidae" genus="Brachysyntexis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="334" phylum="Arthropoda" rank="species" species="robusta">
