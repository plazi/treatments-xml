Verhoeff, 
<number id="A76A24CD347C322CFE6C7A9FFBF778E5" box="[440,500,1217,1242]" pageId="25" pageNumber="359" value="1905.0">1905</number>
: 
<number id="A76A24CD347C322CFE2B7A9FF82F78E5" box="[511,556,1217,1242]" pageId="25" pageNumber="359" value="505.0">505</number>
</taxonomicName>
. 
<typeStatus id="CE198848347C322CFDE37A9CF86C78E5" box="[567,623,1218,1242]" pageId="25" pageNumber="359">Type</typeStatus>
species: 
<taxonomicName id="D6A24D69347C322CFD017A9FFE7778E5" authority="Berlese, 1888" authorityName="Berlese" authorityYear="1888" box="[725,1140,1217,1242]" class="Diplopoda" family="Julidae" genus="Mesoiulus" kingdom="Animalia" order="Julida" pageId="25" pageNumber="359" phylum="Arthropoda" rank="species" species="vulnerarius">
<emphasis id="23D6EAF8347C322CFD017A9FF9D378E5" box="[725,976,1217,1242]" italics="true" pageId="25" pageNumber="359">Mesoiulus vulnerarius</emphasis>
