<taxonomicName id="74B44D111368FFE4FC11611DFB51FDF7" authority="Pic, 1906" authorityName="Pic" authorityYear="1906" box="[977,1225,540,564]" class="Insecta" family="Curculionidae" genus="Ptinus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="30" phylum="Arthropoda" rank="species" species="natalensis">
