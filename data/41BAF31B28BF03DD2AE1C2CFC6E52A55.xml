<taxonomicName id="C31565EF3520906F15EA34412A6D27E9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:250510" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica rubra scabrinodis var. schencki Viereck" order="Hymenoptera" pageNumber="56" phylum="Arthropoda" rank="variety" species="rubra" subSpecies="scabrinodis" variety="schencki">Myrmica rubra scabrinodis var. schencki Emery</taxonomicName>
