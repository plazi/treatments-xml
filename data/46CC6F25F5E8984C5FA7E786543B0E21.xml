<normalizedToken id="A5927B4383DA5A4A857E043CAFE9950B" originalValue="überschneiden">ueberschneiden</normalizedToken>
sich: 
<taxonomicName id="49BB8E83D74FF8B8AA30F317194B8983" class="Arachnida" family="Phthiracaridae" genus="Steganacarus" kingdom="Animalia" order="Sarcoptiformes" pageNumber="128" phylum="Arthropoda" rank="species" species="magnus">St. magnus</taxonomicName>
