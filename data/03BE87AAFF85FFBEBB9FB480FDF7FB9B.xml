<taxonomicName id="4C174D3FFF82FFBFB9ADB6C6FCFAFE66" authorityName="Schuh &amp; P. Pedraza" authorityYear="2010" box="[701,872,477,500]" class="Insecta" family="Miridae" genus="Wallabicoris" kingdom="Animalia" order="Hemiptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="newcastelii">
