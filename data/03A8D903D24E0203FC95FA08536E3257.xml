<taxonomicName id="4C011396D24E0200FC02F942546135D0" authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[939,1379,1718,1742]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="daqingensis">
