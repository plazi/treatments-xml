<taxonomicName id="4C8C27DE49BC75E6DDC318EBB9FE68BE" authority="Koch, 1835" authorityName="Koch" authorityYear="1835" class="Arachnida" family="Phthiracaridae" genus="Hoplophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageNumber="125" phylum="Arthropoda" rank="species" species="stricula">Hoplophora stricula C.L. Koch</taxonomicName>
