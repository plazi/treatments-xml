<taxonomicName id="4C55E087FF9EFFB01AAEF924FCAAF97C" authority="Moritz &amp; Wesener, 2021" authorityName="Moritz &amp; Wesener" authorityYear="2021" box="[649,835,1717,1743]" class="Diplopoda" family="Pseudonannolenidae" genus="Kachincambala" kingdom="Animalia" order="Spirostreptida" pageId="16" pageNumber="38" phylum="Arthropoda" rank="genus" status="gen. nov.">
