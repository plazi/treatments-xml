<paragraph id="159966A7B6D9207D6436DB7AC116D22D" pageId="0" pageNumber="57889">
<taxonomicName id="B933A8074BD2888D5CEF8DDA9249E6CF" LSID="0A1FF58C-422F-532A-81EF-F88300FB1747" authority="Gravenhorst, 1806" authorityName="Gravenhorst" authorityYear="1806" class="Insecta" family="Staphylinidae" genus="Omalium" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Omalium caesum" order="Coleoptera" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="caesum">Omalium caesum Gravenhorst, 1806</taxonomicName>
