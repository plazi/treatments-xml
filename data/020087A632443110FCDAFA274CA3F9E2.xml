<taxonomicName id="4DA94D3332453110FB87FA674B4CFA22" class="Pisces (awaiting allocation)" family="Pisces (awaiting allocation)" genus="Garralepis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pisces (awaiting allocation)" pageId="3" pageNumber="276" phylum="Chordata" rank="species" species="simplex">
