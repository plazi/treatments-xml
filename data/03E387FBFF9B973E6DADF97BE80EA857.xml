<typeStatus id="54F1884FFF9B97326DADF8B5ED97AD46" box="[151,204,1835,1858]" pageId="23" pageNumber="24">Type</typeStatus>
species: 
<taxonomicName id="4C4A4D6EFF9B97326C17F8B5EFF1AD46" authority="Hendel 1914: 68" authorityName="Hendel" authorityPageNumber="68" authorityYear="1914" box="[301,682,1835,1858]" class="Insecta" family="Tephritidae" genus="Euribia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="m-nigrum">
