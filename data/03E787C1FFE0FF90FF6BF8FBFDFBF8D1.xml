<number id="3D8624F0FFE0FF92FCA5F879FCA8F843" box="[857,865,2008,2022]" pageId="9" pageNumber="10" value="2.0">2</number>
+ 
<number id="3D8624F0FFE0FF92FC97F879FCBBF843" box="[875,882,2008,2022]" pageId="9" pageNumber="10" value="3.0">3</number>
