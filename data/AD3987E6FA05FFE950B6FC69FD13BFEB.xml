<collectionCode id="4381AE35FA05FFE650E6FBB1FEB0BA56" box="[199,285,1106,1132]" country="New Zealand" httpUri="http://grbio.org/cool/n6z3-qvv0" name="Canterbury Museum" pageId="55" pageNumber="56">CMNZ</collectionCode>
