<collectingCountry id="B8C2763EFFA4B400FF2FF94CFC28F958" box="[862,959,1671,1695]" name="Norway" pageId="13" pageNumber="271">Norway</collectingCountry>
maple (
<taxonomicName id="07D54D2DFFA4B400F86EF94CFB46F959" authorityName="C.Linnaeus" authorityYear="1753" box="[1055,1233,1670,1694]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="13" pageNumber="271" phylum="Tracheophyta" rank="species" species="platanoides">
