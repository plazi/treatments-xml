<taxonomicName id="97F34D5C9F2F6B0178A1FDE0515BB8ED" authorityName="Dispons" authorityYear="1968" box="[218,584,628,654]" class="Insecta" family="Reduviidae" genus="Rhinocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="179" phylum="Arthropoda" rank="subSpecies" species="erythropus" subSpecies="propinquus">
