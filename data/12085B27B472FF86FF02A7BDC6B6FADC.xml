<collectingCountry id="E2B6AAA1B473FF86FEF2A347C4FCFBD2" box="[359,426,1122,1147]" name="China" pageId="7" pageNumber="597">China</collectingCountry>
(
<collectingRegion id="586524D3B473FF86FE2CA347C714FBD2" box="[441,578,1122,1147]" country="China" name="Guangdong" pageId="7" pageNumber="597">Guangdong</collectingRegion>
