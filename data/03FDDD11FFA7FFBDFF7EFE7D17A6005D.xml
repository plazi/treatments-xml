<quantity id="4CACC1E2FFA7FFBDFBB9F9F413EB016F" box="[1104,1205,1604,1630]" metricMagnitude="-2" metricUnit="m" metricValue="1.08" pageId="27" pageNumber="28" unit="mm" value="10.8">10.8mm</quantity>
