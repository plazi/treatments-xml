<taxonomicName id="4C684D024347C72CFE01FC26FD7301AA" authorityName="Tokunaga, Nakamura &amp; Saigusa" authorityYear="2009" box="[421,690,943,968]" class="Insecta" family="Ptychopteridae" genus="Bittacomorphella" kingdom="Animalia" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="esakii">
