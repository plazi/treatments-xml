<taxonomicName id="4C12E69AFF914E07D7D0F098FE443473" authorityName="McCoy" authorityYear="1844" box="[409,498,754,780]" class="Ostracoda" family="Bairdiidae" genus="Bairdia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
