<subSubSection id="CD916CF7FFC8FFE5FF1DEF5E8D0EE016" box="[199,1365,330,355]" pageId="11" pageNumber="27" type="diagnosis">
<paragraph id="85343F7CFFC8FFE5FF1DEF5E8D0EE016" blockId="11.[151,1436,258,355]" box="[199,1365,330,355]" pageId="11" pageNumber="27">
