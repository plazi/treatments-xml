<quantity id="EA0AA9D0FFB2FF80FC2F233E0D15DDFC" box="[963,1043,1655,1679]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="6" pageNumber="61" unit="mm" value="20.0">20 mm</quantity>
