<taxonomicName id="D4E94D07E75A01181C28F927FC91F93C" authorityName="Hamilton" authorityYear="2010" box="[615,788,1721,1745]" class="Insecta" family="Attelabidae" genus="Temnocerus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="herediensis">
<emphasis id="219DEA96E75A01181C28F927FC91F93C" box="[615,788,1721,1745]" italics="true" pageId="23" pageNumber="22">T. herediensis</emphasis>
