<taxonomicName id="5787F900FFF7B98CB5AE26DC6BEE0CB3" class="Insecta" family="Aphididae" genus="Rhopalosiphum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhopalosiphum rufiabdominale" order="Hemiptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="species" species="rufiabdominale">Rhopalosiphum rufiabdominale</taxonomicName>
