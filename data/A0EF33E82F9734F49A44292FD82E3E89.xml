<taxonomicName id="D0BCCD286BD64F693394CB6FFA4A986C" class="Insecta" family="Aphididae" genus="Rhopalosiphum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhopalosiphum arundinariae" order="Hemiptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="species" species="arundinariae">Rhopalosiphum arundinariae</taxonomicName>
