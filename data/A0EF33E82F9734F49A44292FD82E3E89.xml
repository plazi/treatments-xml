<taxonomicName id="4DBB4FA97715088FA84726CCC8C67BD0" lsidName="" pageId="5" pageNumber="64" rank="subFamily" subFamily="Aphidinae">Aphidinae</taxonomicName>
<taxonomicName id="26D8716A6A066863C32801C6155F24CB" lsidName="" pageId="5" pageNumber="64" rank="tribe" tribe="Aphidini">Aphidini</taxonomicName>
