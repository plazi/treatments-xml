<taxonomicName id="A610AFC1B21E60925CC9BD93E496DA32" class="Insecta" family="Aphididae" genus="Sitobion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sitobion bambusicola" order="Hemiptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="species" species="bambusicola">Sitobion bambusicola</taxonomicName>
