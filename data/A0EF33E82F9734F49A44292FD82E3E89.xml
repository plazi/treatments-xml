<taxonomicName id="9FE4C0D5E4CA5C8A41DE26641A8C6134" class="Insecta" family="Aphididae" genus="Melanaphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Melanaphis sacchari" order="Hemiptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="species" species="sacchari">Melanaphis sacchari</taxonomicName>
