<taxonomicName id="00D0AA210DBFC8DE0A632D618143CA2D" class="Pezizomycetes" family="Ascobolaceae" genus="Anserina" kingdom="Fungi" order="Pezizales" pageId="null" pageNumber="389" phylum="Ascomycota" rank="genus" sensu="stricto">Anserina</taxonomicName>
