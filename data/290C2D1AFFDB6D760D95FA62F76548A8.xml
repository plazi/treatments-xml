<taxonomicName id="66A5E78FFFDB6D760F49FA19F53B490A" authority="Oberthur, 1879" authorityName="Oberthur" authorityYear="1879" box="[587,879,1527,1549]" class="Insecta" family="Papilionidae" genus="Graphium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="36" pageNumber="429" phylum="Arthropoda" rank="species" species="mandarinus">
