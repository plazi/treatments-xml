<taxonomicName id="27CA4D35FEC8FEAE5965FA46FDD7649C" authorityName="Rondani" authorityYear="1868" box="[379,612,1483,1509]" class="Insecta" family="Syrphidae" genus="Paragus" kingdom="Animalia" order="Diptera" pageId="261" pageNumber="262" phylum="Arthropoda" rank="species" species="excalceatus">
