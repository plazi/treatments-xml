</taxonomicName>
from “ 
<taxonomicName id="27CA4D35FEC8FEAE5AACFA46FCB6649C" box="[690,773,1483,1509]" class="Mammalia" family="Bovidae" genus="Etruria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Artiodactyla" pageId="261" pageNumber="262" phylum="Chordata" rank="genus">Etruria</taxonomicName>
