<taxonomicName id="8CA575CAFF8DFB4C2D04F905DD661E86" authority="Li &amp; Chen &amp; Yang, 2020" authorityName="Li &amp; Chen &amp; Yang" authorityYear="2020" box="[349,569,1729,1753]" class="Insecta" family="Lauxaniidae" genus="Noeetomima" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="514" phylum="Arthropoda" rank="species" species="zhangae" status="sp. nov.">
