<taxonomicName id="4C27E3B77E56FFDAFDF972C1DDC2FDFF" authority="Bell, 1942" authorityName="Bell" authorityYear="1942" box="[518,732,491,516]" class="Insecta" family="Hesperiidae" genus="Viridina" kingdom="Animalia" order="Lepidoptera" pageId="36" pageNumber="35" phylum="Arthropoda" rank="species" species="viridis">
