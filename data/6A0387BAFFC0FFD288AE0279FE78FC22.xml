<taxonomicName id="25AA4D2FFFC2FFD28BE102BEFDB8FD63" authorityName="Mehes" authorityYear="1911" box="[402,516,723,749]" class="Ostracoda" family="Healdiidae" genus="Hungarella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="11" pageNumber="258" phylum="Arthropoda" rank="genus">
<emphasis id="D0DEEABEFFC2FFD28BE102BEFDB8FD63" box="[402,516,723,749]" italics="true" pageId="11" pageNumber="258">Hungarella</emphasis>
</taxonomicName>
