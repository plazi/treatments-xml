<taxonomicName id="25AA4D2FFFC2FFD2883E01BBFE1EFD9E" authority="(Blake, 1876)" baseAuthorityName="Blake" baseAuthorityYear="1876" class="Ostracoda" family="Healdiidae" genus="Hungarella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="11" pageNumber="258" phylum="Arthropoda" rank="species" species="elongata">
