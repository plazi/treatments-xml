<taxonomicName id="33CE36FA2D62FFF9FED1FA67EE8B7406" authorityName="Fibiger" authorityYear="2010" box="[366,467,795,820]" class="Insecta" family="Erebidae" genus="Disca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="anser">
<emphasis id="C6BA916B2D62FFF9FED1FA67EE8B7406" box="[366,467,795,820]" italics="true" pageId="81" pageNumber="82">D. anser</emphasis>
</taxonomicName>
