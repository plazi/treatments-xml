<collectingCountry id="8CD90DE92D62FFF9FD77FA66EC6E7406" box="[712,822,794,820]" name="Malaysia" pageId="81" pageNumber="82">Malaysia</collectingCountry>
, near 
<collectingRegion id="360A839B2D62FFF9FCC4FA66ECBE7406" box="[891,998,794,820]" country="Malaysia" name="Selangor" pageId="81" pageNumber="82">Selangor</collectingRegion>
