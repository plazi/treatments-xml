<paragraph id="C337332811EF63490FBB635F2D5C5C04" pageId="3" pageNumber="50">
<taxonomicName id="A9B70397F6631B7E297313D337CB19F0" class="Insecta" family="Staphylinidae" genus="Togpelenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Togpelenys gigantea" order="Coleoptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="gigantea">Togpelenys gigantea</taxonomicName>
