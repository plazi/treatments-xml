synonymized 
<taxonomicName id="E0F64D56DEF5FF69B205C557FC26903A" authorityName="Bamber" authorityYear="2000" box="[781,902,1315,1338]" class="Malacostraca" family="Kalliapseudidae" genus="Tanapseudes" kingdom="Animalia" order="Tanaidacea" pageId="158" pageNumber="159" phylum="Arthropoda" rank="species" species="sinensis">
