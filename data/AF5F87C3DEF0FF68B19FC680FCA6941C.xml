<taxonomicName id="E0F64D56DEF5FF69B2C1C557FBF7903A" authorityName="Bacescu" authorityYear="1978" box="[969,1111,1315,1338]" class="Malacostraca" family="Kalliapseudidae" genus="Tanapseudes" kingdom="Animalia" order="Tanaidacea" pageId="158" pageNumber="159" phylum="Arthropoda" rank="species" species="ormuzana">
