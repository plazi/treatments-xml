<emphasis id="29E4F938CE2D0216696DF967625F36FE" box="[1081,1151,1752,1776]" italics="true" pageId="23" pageNumber="104">
<taxonomicName id="DC905EA9CE2D0216696DF967625C36FE" authorityName="Curtis" authorityYear="1825" box="[1081,1148,1752,1776]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="104" phylum="Arthropoda" rank="genus">Agrilus</taxonomicName>
