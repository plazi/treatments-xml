<taxonomicName id="CFC194A10672600DF3CABFD38312717D" class="Insecta" family="Staphylinidae" genus="Phloeopora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phloeopora" order="Coleoptera" pageId="111" pageNumber="112" phylum="Arthropoda" rank="genus">Phloeopora</taxonomicName>
