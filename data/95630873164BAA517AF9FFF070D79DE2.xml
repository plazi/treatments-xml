<taxonomicName id="676C6D0DF688F379B586EB23AEF56D8B" class="Insecta" family="Staphylinidae" genus="Phloeopora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phloeopora" order="Coleoptera" pageId="111" pageNumber="112" phylum="Arthropoda" rank="genus">Phloeopora</taxonomicName>
