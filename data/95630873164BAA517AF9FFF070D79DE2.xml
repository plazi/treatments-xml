<taxonomicName id="78FCB85F9BE3FC4DFD565A07B31CE3E3" class="Insecta" family="Staphylinidae" genus="Haploglossa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haploglossa" order="Coleoptera" pageId="111" pageNumber="112" phylum="Arthropoda" rank="genus">Haploglossa</taxonomicName>
