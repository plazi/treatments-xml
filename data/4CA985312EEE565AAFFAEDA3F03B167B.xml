<paragraph id="9B7B03A30BD69DE367E81AB5E9DF97EC" pageId="0" pageNumber="127">Etymology.</paragraph>
<paragraph id="0EF07B64106C6CC25D76D297596A6E46" pageId="0" pageNumber="127">
<taxonomicName id="E82C632CE16DEDEE8E5957A7517ABEB5" authorityName="Hong Liu" authorityYear="2020" class="Magnoliopsida" family="Saxifragaceae" genus="Chrysosplenium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Chrysosplenium zhouzhiense" order="Saxifragales" pageId="0" pageNumber="127" phylum="Tracheophyta" rank="species" species="zhouzhiense">
