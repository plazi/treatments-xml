<taxonomicName id="4C184D17FFEC9F2A17B9DC19D65A0F26" authority="Volynkin, 2019" authorityName="Volynkin" authorityYear="2019" box="[466,591,296,320]" class="Insecta" family="Erebidae" genus="Lophilema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="119" phylum="Arthropoda" rank="species" species="bilobata" status="sp. nov.">
