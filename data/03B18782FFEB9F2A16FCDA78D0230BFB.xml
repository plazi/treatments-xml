<taxonomicName id="4C184D17FFEC9F2A156BDC7DD7700F02" authority="Volynkin, 2019" authorityName="Volynkin" authorityYear="2019" box="[768,869,332,356]" class="Insecta" family="Erebidae" genus="Lophilema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="119" phylum="Arthropoda" rank="species" species="mbulu" status="sp. nov.">
