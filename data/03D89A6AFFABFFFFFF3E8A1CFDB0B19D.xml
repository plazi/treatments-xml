<taxonomicName id="4C7150FFFFABFFFFFC688A54FB19B779" box="[990,1274,860,886]" class="Magnoliopsida" family="Fabaceae" genus="Peltophorum" kingdom="Plantae" order="Fabales" pageId="4" pageNumber="286" phylum="Tracheophyta" rank="species" species="vogelianum">
<emphasis id="B905F76EFFABFFFFFC688A54FB19B779" box="[990,1274,860,886]" italics="true" pageId="4" pageNumber="286">Peltophorum vogelianum</emphasis>
</taxonomicName>
(
<taxonomicName id="4C7150FFFFABFFFFFABC8A54FA91B779" box="[1290,1394,860,886]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="4" pageNumber="286" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
