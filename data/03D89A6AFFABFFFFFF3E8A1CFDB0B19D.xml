</paragraph>
<paragraph id="8BCE2B7CFFABFFFFFF0B8A88FDE3B795" blockId="4.[136,1452,824,1427]" box="[189,512,896,922]" pageId="4" pageNumber="286">
<materialsCitation id="3B192121FFABFFFFFF0B8A88FDE3B795" ID-GBIF-Occurrence="3450199301" box="[189,512,896,922]" collectionCode="MFLU" pageId="4" pageNumber="286" specimenCode="MFLU 21-0113" specimenCount="1" typeStatus="holotype">
