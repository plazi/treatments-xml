<heading id="6F7CC558170FA920989DFA6C7B56FA3D" bold="true" box="[151,789,1447,1473]" fontSize="11" level="1" pageId="62" pageNumber="63" reason="1">
<emphasis id="06FFAE26170FA920989DFA6C7B56FA3D" bold="true" box="[151,789,1447,1473]" pageId="62" pageNumber="63">
<taxonomicName id="F38B09B7170FA920989DFA6C7A3BFA3D" authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,632,1447,1473]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="gekilara" status="new species">
<emphasis id="06FFAE26170FA920989DFA6C79DCFA3D" bold="true" box="[151,415,1447,1473]" italics="true" pageId="62" pageNumber="63">Hydropsyche gekilara</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D170FA9209A8FFA6C7B56FA3D" box="[645,789,1447,1473]" pageId="62" pageNumber="63" rank="species">new species</taxonomicNameLabel>
