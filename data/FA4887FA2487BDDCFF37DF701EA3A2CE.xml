<taxonomicName id="B5E14D6F2487BDDEFF64DF731998A24D" authority="Klotzsch (1847: 452)" authorityName="Klotzsch" authorityPageNumber="452" authorityYear="1847" box="[196,679,305,332]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="153" pageNumber="154" phylum="Tracheophyta" rank="species" species="undata">
<emphasis id="4095EAFE2487BDDEFF64DF731AA5A24D" bold="true" box="[196,410,305,332]" italics="true" pageId="153" pageNumber="154">Geonoma undata</emphasis>
<bibRefCitation id="16704B1D2487BDDEFE03DF701998A24D" author="Klotzsch, J." box="[419,679,306,332]" journalOrPublisher="Linnaea" pageId="153" pageNumber="154" pagination="446 - 456" part="20" refId="ref123212" refString="Klotzsch, J. (1847) Palmae Juss., Brown, Mart., Endl., Kunth. Linnaea 20: 446 - 456." title="Palmae Juss., Brown, Mart., Endl., Kunth" type="journal article" year="1847">Klotzsch (1847: 452)</bibRefCitation>
