<taxonomicName id="96854D45CF4A1D3B6DD5BAF0FB3DF867" baseAuthorityName="MacCallum" baseAuthorityYear="1917" box="[937,1227,2001,2027]" class="Monogenea" family="Diclidophoridae" genus="Neoheterobothrium" kingdom="Animalia" order="Mazocraeidea" pageId="45" pageNumber="546" phylum="Platyhelminthes" rank="species" species="cynoscioni">
