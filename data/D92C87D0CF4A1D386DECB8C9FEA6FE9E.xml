<taxonomicName id="96854D45CF491D386C1DBDF9FCF1FF7D" baseAuthorityName="Hargis" baseAuthorityYear="1955" box="[609,775,215,241]" class="Monogenea" family="Diclidophoridae" genus="Hargicotyle" kingdom="Animalia" order="Mazocraeidea" pageId="46" pageNumber="547" phylum="Platyhelminthes" rank="species" species="louisianensis">
