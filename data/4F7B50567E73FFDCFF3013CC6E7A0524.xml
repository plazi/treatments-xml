<paragraph id="C76DE1407E71FFE3FF30167D6C360195" blockId="31.[151,1436,1907,2034]" pageId="31" pageNumber="32">
<emphasis id="F5A63D527E71FFE3FF30167D6E6E01EC" bold="true" box="[151,298,1907,1931]" pageId="31" pageNumber="32">FIGURE 13.</emphasis>
<taxonomicName id="00D29AC37E71FFE3FE95167D6EB101ED" authority="Published, 2009" authorityName="Published" authorityYear="2009" box="[306,501,1907,1930]" class="Malacostraca" family="Epialtidae" genus="Rochinia" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="ahyongi" status="sp. nov.">
<emphasis id="F5A63D527E71FFE3FE95167D6EB101ED" box="[306,501,1907,1930]" italics="true" pageId="31" pageNumber="32">Rochinia ahyongi</emphasis>
</taxonomicName>
<emphasis id="F5A63D527E71FFE3FE5B167D6D1001EC" bold="true" box="[508,596,1907,1931]" pageId="31" pageNumber="32">
<taxonomicNameLabel id="EE9580297E71FFE3FE5B167D6D1001EC" box="[508,596,1907,1931]" pageId="31" pageNumber="32" rank="species">sp. nov.</taxonomicNameLabel>
