<taxonomicName id="4C464D06FFF1FFF6EE66F94C55C1D2C8" box="[328,480,1764,1789]" class="Insecta" family="Cicadellidae" genus="Dusuna" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="577" phylum="Arthropoda" rank="species" species="anacantha">
