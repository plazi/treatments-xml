<quantity id="4CC366C14927FA39FEF7FEF5FE45FE20" box="[369,458,359,386]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="10" pageNumber="715" unit="m" value="1000.0">1000 m</quantity>
