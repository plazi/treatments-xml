<taxonomicName id="4C3BB0A74927FA39FD88FE15FEBBFE63" authority="(Gruvel, 1901)" baseAuthorityName="Gruvel" baseAuthorityYear="1901" class="Maxillopoda" family="Heteralepadidae" genus="Heteralepas" kingdom="Animalia" order="Pedunculata" pageId="10" pageNumber="715" phylum="Arthropoda" rank="species" species="microstoma">
