<taxonomicName id="4C3BB0A74927FA39FF56FE15FD8FFE03" authority="(Darwin, 1852)" baseAuthorityName="Darwin" baseAuthorityYear="1852" box="[208,512,392,418]" class="Maxillopoda" family="Heteralepadidae" genus="Heteralepas" kingdom="Animalia" order="Pedunculata" pageId="10" pageNumber="715" phylum="Arthropoda" rank="species" species="cornuta">
