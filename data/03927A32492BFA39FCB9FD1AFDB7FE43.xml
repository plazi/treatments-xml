found at 
<quantity id="4CC366C14927FA39FD99FF75FDFAFEA0" box="[543,629,231,258]" metricMagnitude="3" metricUnit="m" metricValue="2.235" pageId="10" pageNumber="715" unit="m" value="2235.0">2235 m</quantity>
