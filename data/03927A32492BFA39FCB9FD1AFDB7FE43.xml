<taxonomicName id="4C3BB0A74927FA39FEEEFE35FDC3FE63" authority="(Pilsbry, 1907)" baseAuthorityName="Pilsbry" baseAuthorityYear="1907" box="[360,588,424,450]" class="Maxillopoda" family="Heteralepadidae" genus="Heteralepas" kingdom="Animalia" order="Pedunculata" pageId="10" pageNumber="715" phylum="Arthropoda" rank="species" species="rex">
<emphasis id="B94F17364927FA39FEEEFE35FE26FE60" box="[360,425,424,449]" italics="true" pageId="10" pageNumber="715">H. rex</emphasis>
