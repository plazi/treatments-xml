<taxonomicName id="4C3BB0A74927FA39FE84FEB5FDE9FEE3" authority="(Gruvel, 1900)" baseAuthorityName="Gruvel" baseAuthorityYear="1900" box="[258,614,296,322]" class="Maxillopoda" family="Heteralepadidae" genus="Heteralepas" kingdom="Animalia" order="Pedunculata" pageId="10" pageNumber="715" phylum="Arthropoda" rank="species" species="lankesteri">
