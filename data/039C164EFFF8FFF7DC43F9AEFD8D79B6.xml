<paragraph id="8B8AA758FFF8FFF4DC43F9AEFDDC7ECC" blockId="121.[113,582,1209,1232]" box="[113,582,1209,1232]" pageId="121" pageNumber="122">
<heading id="D0C21034FFF8FFF4DC43F9AEFDDC7ECC" box="[113,582,1209,1232]" centered="true" fontSize="9" level="2" pageId="121" pageNumber="122" reason="2">
<taxonomicName id="4C35DCDBFFF8FFF4DC43F9AEFE347ECC" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[113,430,1209,1232]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="121" pageNumber="122" phylum="Arthropoda" rank="species" species="kitrinocarus" status="new species">
<emphasis id="B9417B4AFFF8FFF4DC43F9AEFE347ECC" bold="true" box="[113,430,1209,1232]" italics="true" pageId="121" pageNumber="122">Paramblynotus kitrinocarus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFF8FFF4DD8DF9ADFDDC7ECC" box="[447,582,1210,1232]" pageId="121" pageNumber="122" rank="species">new species</taxonomicNameLabel>
