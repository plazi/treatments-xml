<paragraph id="E3CC7A45C72A1EEFDC5B4E7D6916238B" pageId="null" pageNumber="278">
<taxonomicName id="CFF5A1E5605E659FC6A07888FB604877" authority="Hornschuch" class="Magnoliopsida" family="Saxifragaceae" genus="Saxifraga" kingdom="Plantae" order="Saxifragales" pageId="null" pageNumber="278" phylum="Tracheophyta" rank="species" species="rudolphiana">
Saxifraga 
<normalizedToken id="125226A1526440AC0CB6A8AC3F65BDEC" originalValue="Rudolphiána" pageId="null" pageNumber="278">Rudolphiana</normalizedToken>
Hornschuch
</taxonomicName>
