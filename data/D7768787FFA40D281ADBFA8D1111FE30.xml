<collectingCountry id="27C87601FFA40D2B1E60F9F31085F968" box="[1068,1178,1647,1673]" name="Suriname" pageId="25" pageNumber="476">Suriname</collectingCountry>
, 
<collectingCountry id="27C87601FFA40D2B1EE4F9F310F0F968" box="[1192,1263,1647,1673]" name="Brazil" pageId="25" pageNumber="476">Brazil</collectingCountry>
, 
<collectingCountry id="27C87601FFA40D2B1EB2F9F3114CF968" box="[1278,1363,1647,1673]" name="Bolivia" pageId="25" pageNumber="476">Bolivia</collectingCountry>
, 
