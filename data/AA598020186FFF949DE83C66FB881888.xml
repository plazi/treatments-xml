<subSubSection id="6AEA62BD186FFF959DE83C18FAFA1C48" box="[151,1333,1636,1661]" pageId="2" pageNumber="3" type="materials_examined">
<paragraph id="224F3136186FFF959DE83C18FAFA1C48" blockId="2.[151,1333,1636,1661]" box="[151,1333,1636,1661]" pageId="2" pageNumber="3">
<emphasis id="1084ED24186FFF959DE83C18FEE41C48" bold="true" box="[151,299,1636,1660]" pageId="2" pageNumber="3">
<typeStatus id="FD4B8F94186FFF959DE83C18FF001C48" box="[151,207,1636,1660]" pageId="2" pageNumber="3">Type</typeStatus>
