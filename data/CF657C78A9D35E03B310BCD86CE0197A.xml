<taxonomicName id="AD878377B4E767202D8EA6B81922CAE8" class="Insecta" family="Nolidae" genus="Iscadia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Iscadia" order="Lepidoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="genus">
