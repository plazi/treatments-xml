<paragraph id="8BCCCD0AFFD8686EC17117AD9261FC11" blockId="1.[264,786,913,939]" box="[264,786,913,939]" pageId="1" pageNumber="46">
<heading id="D0847A66FFD8686EC17117AD9261FC11" bold="true" box="[264,786,913,939]" fontSize="11" level="1" pageId="1" pageNumber="46" reason="1">
<emphasis id="B9071118FFD8686EC17117AD9261FC11" bold="true" box="[264,786,913,939]" pageId="1" pageNumber="46">
<taxonomicName id="4C73B689FFD8686EC17117AD90D8FC11" box="[264,427,913,939]" class="Insecta" family="Chrysomelidae" genus="Discomorpha" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="46" phylum="Arthropoda" rank="genus">
<emphasis id="B9071118FFD8686EC17117AD90D8FC11" bold="true" box="[264,427,913,939]" italics="true" pageId="1" pageNumber="46">Discomorpha</emphasis>
</taxonomicName>
(s. str.) 
<taxonomicName id="4C73B689FFD8686EC27717AD93DEFC11" box="[526,685,913,939]" class="Insecta" family="Chrysomelidae" genus="Discomorpha" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="46" phylum="Arthropoda" rank="species" species="howdenorum" status="sp. nov.">
<emphasis id="B9071118FFD8686EC27717AD93DEFC11" bold="true" box="[526,685,913,939]" italics="true" pageId="1" pageNumber="46">howdenorum</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A234AC63FFD8686EC2CC17AD9261FC11" box="[693,786,913,939]" pageId="1" pageNumber="46" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
