<number id="3DEB24BD0A7A9E04FDDDE627FD129379" box="[579,653,350,374]" pageId="19" pageNumber="86" value="27.5" valueMax="28.0" valueMin="27.0">27–28</number>
.VI. 
<number id="3DEB24BD0A7A9E04FD2DE627FD729379" box="[691,749,350,374]" pageId="19" pageNumber="86" value="1989.0">1989</number>
