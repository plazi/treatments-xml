<taxonomicName id="20DD54C7EC9B1EA000A38A3CC4338B5C" class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="634" phylum="Tracheophyta" rank="genus">Orchis</taxonomicName>
