<paragraph id="8BE036B1FFA7F858FF01BC46431EFB8F" blockId="95.[192,794,1059,1148]" box="[192,789,1059,1083]" pageId="95" pageNumber="94">
<heading id="D0A881DDFFA7F858FF01BC46431EFB8F" bold="true" box="[192,789,1059,1083]" fontSize="10" level="2" pageId="95" pageNumber="94" reason="3">
<emphasis id="B92BEAA3FFA7F858FF01BC46431EFB8F" bold="true" box="[192,789,1059,1083]" pageId="95" pageNumber="94">
<taxonomicName id="4C5F4D32FFA7F858FF01BC464233FB8F" authority="Blahnik &amp; Holzenthal, 2017" authorityName="Blahnik &amp; Holzenthal" authorityYear="2017" box="[192,568,1059,1083]" class="Magnoliopsida" family="Apocynaceae" genus="Mortoniella" kingdom="Plantae" order="Gentianales" pageId="95" pageNumber="94" phylum="Tracheophyta" rank="subGenus" status="new subgenus" subGenus="Nanotrichia">
<emphasis id="B92BEAA3FFA7F858FF01BC464163FB8F" bold="true" box="[192,360,1059,1083]" italics="true" pageId="95" pageNumber="94">Mortoniella</emphasis>
(
<emphasis id="B92BEAA3FFA7F858FEBCBC464226FB8F" bold="true" box="[381,557,1059,1083]" italics="true" pageId="95" pageNumber="94">Nanotrichia</emphasis>
)
</taxonomicName>
, 
<taxonomicNameLabel id="A21857D8FFA7F858FD8BBC46431EFB8F" box="[586,789,1059,1083]" pageId="95" pageNumber="94" rank="subGenus">new subgenus</taxonomicNameLabel>
