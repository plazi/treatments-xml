— 
<collectingCountry id="FE8007CBFFB48507FEEEFB04FE52FA88" box="[320,413,1274,1300]" name="Ecuador" pageId="19" pageNumber="230">Ecuador</collectingCountry>
and 
<collectingCountry id="FE8007CBFFB48507FE7DFB05FDC5FA89" box="[467,522,1275,1301]" name="Peru" pageId="19" pageNumber="230">Peru</collectingCountry>
