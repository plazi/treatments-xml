<taxonomicName id="ED864D075D02FF95FEE1FEB2A4FF0ABC" authorityName="Taverne &amp; Capasso" authorityYear="2015" box="[256,453,358,385]" class="Actinopterygii" family="Gladiopycnodontidae" genus="Acrorhinichthys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="20" pageNumber="21" phylum="Chordata" rank="genus" status="gen. nov.">
