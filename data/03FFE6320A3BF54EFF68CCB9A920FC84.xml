<number id="3D9E45030A3BF54EFF68CE69AAE9FDF1" box="[151,221,583,608]" pageId="62" pageNumber="63" value="15917.0">15917</number>
(
<number id="3D9E45030A3BF54EFF10CE66AACFFDF1" box="[239,251,584,608]" pageId="62" pageNumber="63" value="1.0">1</number>
spm., old number 
<number id="3D9E45030A3BF54EFE32CE69A82BFDF1" box="[461,543,583,608]" pageId="62" pageNumber="63" value="61044.0">61.044</number>
