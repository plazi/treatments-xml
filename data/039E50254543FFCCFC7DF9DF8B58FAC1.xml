</taxonomicName>
(
<taxonomicName id="4C379AB04542FFCCFF79FB1B89B2FB01" box="[158,244,1214,1237]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="236" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
