<paragraph id="32771A7D3FAA39BE10CDE6E916068582" pageId="0" pageNumber="1">
<taxonomicName id="F21EDAC155E143B1634C700507A881B8" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon alejandromarini" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="alejandromarini">
