<emphasis id="FE9A8B272744FFA94044D0B8FB7DFC7D" box="[1059,1164,924,948]" italics="true" pageId="2" pageNumber="473">S. kalfatak</emphasis>
</taxonomicName>
and 
<taxonomicName id="0BEE2CB62744FFA946C2D093FCF1FC07" authorityName="Ryan" authorityYear="1982" box="[677,768,950,974]" class="Actinopterygii" family="Gobiidae" genus="Stiphodon" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="473" phylum="Chordata" rank="species" species="astilbos">
<emphasis id="FE9A8B272744FFA946C2D093FCF1FC07" box="[677,768,950,974]" italics="true" pageId="2" pageNumber="473">S. astilbos</emphasis>
</taxonomicName>
