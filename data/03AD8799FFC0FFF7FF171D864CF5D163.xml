<paragraph id="8BBB368FFFC0FFF6FF171E144CF9D6DC" blockId="14.[151,1436,1661,1832]" pageId="14" pageNumber="437">
<keyLead id="30F595BAFFC0FFF6FF171E144CF9D6DC" pageId="14" pageNumber="437">
<keyStep id="30F02E2AFFC0FFF6FF171E144CF9D6DC" pageId="14" pageNumber="437">
<number id="3DCC24A8FFC0FFF6FF171E1449C4D6AD" box="[151,164,1661,1683]" pageId="14" pageNumber="437" value="1.0">1</number>
