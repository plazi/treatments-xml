[
<collectionCode id="5EDEAE0BFFA466485EDF97373BCFFBF0" box="[1417,1436,1099,1123]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="2" pageNumber="209" type="Herbarium">G</collectionCode>
0017 4076], 
<collectionCode id="5EDEAE0BFFA4664858C297173DF4FB10" box="[916,935,1131,1155]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="2" pageNumber="209" type="Herbarium">G</collectionCode>
[
<collectionCode id="5EDEAE0BFFA4664858E197173D99FB10" box="[951,970,1131,1155]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="2" pageNumber="209" type="Herbarium">G</collectionCode>
00381009]) y 
<emphasis id="0ABBEADCFFA466485F3297173AADFB10" box="[1124,1278,1131,1155]" italics="true" pageId="2" pageNumber="209">Fiebrig 4156a</emphasis>
(
<collectionCode id="5EDEAE0BFFA466485E5997173B71FB10" box="[1295,1314,1131,1155]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="2" pageNumber="209" type="Herbarium">G</collectionCode>
[
