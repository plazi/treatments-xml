<subSubSection id="ABB7D33A8C8B3722518C470FBCEEBBE8" lastPageId="21" lastPageNumber="78" pageId="20" pageNumber="77" type="materials_examined">
<paragraph id="21D909DE282D3DFF4EA7AC57F50AA887" pageId="20" pageNumber="77">Material examined.</paragraph>
<paragraph id="FD96989A7D3F64D5832DA80FE444E661" lastPageId="21" lastPageNumber="78" pageId="20" pageNumber="77">
