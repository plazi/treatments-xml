and of 
<taxonomicName id="7ED84D58FFD97727BF83A0AE9AF2888E" authority="Rudolphi, 1819" authorityName="Rudolphi" authorityYear="1819" box="[420,924,654,679]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="53" pageNumber="54" phylum="Platyhelminthes" rank="species" species="vanelli" subGenus="Haematetrophus">
