<taxonomicName id="EDAC4D58FFAFFFBA920CFB492DE2D186" authority="Simon, 1910" authorityName="Simon" authorityYear="1910" box="[889,1242,1157,1182]" class="Arachnida" family="Miturgidae" genus="Cheiramiona" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="87" phylum="Arthropoda" rank="species" species="simplicitarse">
