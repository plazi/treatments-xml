<typeStatus id="54813FACFFF7FF8CC04DFDB9FE17185E" box="[337,446,544,570]" pageId="39" pageNumber="40">Holotype</typeStatus>
male, 
<typeStatus id="54813FACFFF7FF8CC311FDB9FDDB185E" box="[525,626,544,570]" pageId="39" pageNumber="40">paratype</typeStatus>
male, 
<collectingCountry id="F32DC19EFFF7FF8CC3DDFDB9FCEB185E" box="[705,834,544,570]" name="Eritrea" pageId="39" pageNumber="40">ERITREA</collectingCountry>
