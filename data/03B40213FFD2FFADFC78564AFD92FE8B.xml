<paragraph id="8BA2B305FFD2FF93FCCD556FFB86FD5F" blockId="31.[806,1421,619,672]" pageId="31">
<emphasis id="B9696F17FFD2FF93FCCD556FFC16FD7F" box="[806,932,619,640]" italics="true" pageId="31">Etymology:</emphasis>
After 
<taxonomicName id="4C1DC886FFD2FF93FBE15568FBE9FD7E" box="[1034,1115,620,641]" class="Insecta" family="Crabronidae" genus="Gonius" kingdom="Animalia" order="Hymenoptera" pageId="31" phylum="Arthropoda" rank="genus">
<emphasis id="B9696F17FFD2FF93FBE15568FBE9FD7E" box="[1034,1115,620,641]" italics="true" pageId="31">Gonius</emphasis>
</taxonomicName>
, meaning ‘angled’; and 
<taxonomicName id="4C1DC886FFD2FF93FCCD558EFCDFFD60" box="[806,877,650,671]" class="Actinopterygii" family="Pholidae" genus="Pholis" kingdom="Animalia" order="Perciformes" pageId="31" phylum="Chordata" rank="genus">
