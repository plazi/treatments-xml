<emphasis id="DDB1DD49FFD0FFBBFF32F86BFA7B36A4" bold="true" box="[151,295,1961,1985]" pageId="66" pageNumber="67">FIGURE 33.</emphasis>
<taxonomicName id="28C57AD8FFD0FFBBFE8BF86BFAB336A5" authority="Edgar, 2008" authorityName="Edgar" authorityYear="2008" box="[302,495,1961,1984]" class="Malacostraca" family="Tanaididae" genus="Zeuxo" kingdom="Animalia" order="Tanaidacea" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="odohertyae" status="n. sp.">
<emphasis id="DDB1DD49FFD0FFBBFE8BF86BFAB336A5" box="[302,495,1961,1984]" italics="true" pageId="66" pageNumber="67">Zeuxo odohertyae</emphasis>
</taxonomicName>
<emphasis id="DDB1DD49FFD0FFBBFE53F86BF96E36A4" bold="true" box="[502,562,1961,1985]" pageId="66" pageNumber="67">
<taxonomicNameLabel id="C6826032FFD0FFBBFE53F86BF96E36A4" box="[502,562,1961,1985]" pageId="66" pageNumber="67" rank="species">n. sp.</taxonomicNameLabel>
