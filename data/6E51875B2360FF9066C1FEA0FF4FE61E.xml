x 
<number id="5030246A2360FF9065C3F9D5FC42E637" box="[917,990,1595,1621]" pageId="1" pageNumber="64" value="18.5" valueMax="23.0" valueMin="14.0">14–23</number>
</date>
(
<number id="5030246A2360FF9065A6F9D5FB8CE637" box="[1008,1040,1595,1621]" pageId="1" pageNumber="64" value="18.0">18</number>
). Excretory vesicle 
<number id="5030246A2360FF9062AAF9D5FAC8E637" box="[1276,1364,1595,1621]" pageId="1" pageNumber="64" value="112.5" valueMax="138.0" valueMin="87.0">87–138</number>
(
<number id="5030246A2360FF906333F9D5FA0FE637" box="[1381,1427,1595,1621]" pageId="1" pageNumber="64" value="103.0">103</number>
