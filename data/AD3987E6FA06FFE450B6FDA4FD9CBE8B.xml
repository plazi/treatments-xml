<taxonomicName id="E2904D73FA06FFE55577FCF9FE39BD6D" authority="Hutton, 1879" authorityName="Hutton, Dunedin, Purchased" authorityYear="1879" class="Gastropoda" family="Limacidae" genus="Limax" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="molestus">
