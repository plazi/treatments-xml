<taxonomicName id="E2904D73FA06FFE5530CFBBDFC4DBA4D" authorityName="Hutton, Dunedin, Purchased" authorityYear="1879" box="[813,992,1117,1143]" class="Gastropoda" family="Limacidae" genus="Limax" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="molestus">
