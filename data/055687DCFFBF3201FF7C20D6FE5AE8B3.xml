<taxonomicName id="4AFF4D49FFBC3200FD11205BFCB6EC0C" authority="Is." authorityName="Is." box="[762,859,1628,1650]" class="Entognatha" family="Paronellidae" genus="Salina" kingdom="Animalia" order="Collembola" pageId="22" pageNumber="173" phylum="Arthropoda" rank="genus">Salina Is.</taxonomicName>
