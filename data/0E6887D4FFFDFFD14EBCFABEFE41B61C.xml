<taxonomicName id="41C14D41FFFCFFD24D57F9FDFB8EB440" authority="Sinclair &amp; Shamshev, 2021" authorityName="Sinclair &amp; Shamshev" authorityYear="2021" box="[892,1042,1589,1613]" class="Insecta" genus="Iteaphila" kingdom="Animalia" order="Diptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="longiphallus" status="sp. nov.">
