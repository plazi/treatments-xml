(
<figureCitation id="130E2A3B4E05FFC397800034FF6847F8" box="[159,251,618,643]" captionStart="FIGURES 10 – 18" captionStartId="11.[151,264,1953,1976]" captionTargetBox="[162,1426,202,1924]" captionTargetId="figure@11.[155,1431,193,1932]" captionTargetPageId="11" captionText="FIGURES 10 – 18. Habitus. 10 Amydetes caetite sp. nov.. 11 Amydetes lucernula sp. nov.. 12 Amydetes plaumanni sp. nov.. 13 Amydetes bellorum sp. nov.. 14 Amydetes itatiaia sp. nov.. 15 Amydetes vivianii sp. nov.. 16 Amydetes marajoara sp. nov.. 17 Amydetes vagalume sp. nov.. 18 Amydetes luzecu sp. nov. Scale. 2 mm." httpUri="https://zenodo.org/record/252105/files/figure.png" pageId="34" pageNumber="235">
Figs. 
<number id="3DFD24994E05FFC397C10035FF6847F8" box="[222,251,619,643]" pageId="34" pageNumber="235" value="11.33">11</number>
</figureCitation>
, 
<figureCitation id="130E2A3B4E05FFC396180035FEB647F8" box="[263,293,619,643]" captionStart="FIGURES 23 – 40" captionStartId="13.[151,264,1758,1781]" captionTargetBox="[160,1429,204,1726]" captionTargetId="figure@13.[151,1436,193,1737]" captionTargetPageId="13" captionText="FIGURES 23 – 40. Frons shape drawings 23 Amydetes agnita. 24 A. apicalis. 25 A. discissa. 26 A. fastigiata. 27 A. fucata. 28 A. lucioloides. 29 Amydetes solaris sp. nov.. 30 Amydetes goiana sp. nov.. 31 Amydetes manezinha sp. nov.. 32 Amydetes caetite sp. nov.. 33 Amydetes lucernula sp. nov.. 34 Amydetes plaumanni sp. nov.. 35 Amydetes bellorum sp. nov.. 36 Amydetes itatiaia sp. nov.. 37 Amydetes vivianii sp. nov.. 38 Amydetes marajoara sp. nov.. 39 Amydetes vagalume sp. nov.. 40 Amydetes luzecu sp. nov. Scale. 250 µm. DBA = distance between antennifers antennifers. DBAS = distance between antennal sockets." httpUri="https://zenodo.org/record/252107/files/figure.png" pageId="34" pageNumber="235">33</figureCitation>
, 
<number id="3DFD24994E05FFC3962E0034FEEA47F8" box="[305,377,618,643]" pageId="34" pageNumber="235" value="52.71">52, 71</number>
, 89, 116, 135, 188– 
<number id="3DFD24994E05FFC395460035FD1147F8" box="[601,642,619,643]" pageId="34" pageNumber="235" value="191.0">191</number>
)
