<collectingCountry id="29B1760EFFB4B041FB3CF8B740A0F8D4" box="[1160,1217,1855,1878]" name="India" pageId="1" pageNumber="270">India</collectingCountry>
, 
<elevation id="DA8BD1ADFFB4B041FB65F8B7412FF8D4" box="[1233,1358,1855,1878]" metricMagnitude="2" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="1" pageNumber="270" unit="m" value="275.0" valueMax="300.0" valueMin="250.0">
<quantity id="965E9B7BFFB4B041FB65F8B7412FF8D4" box="[1233,1358,1855,1878]" metricMagnitude="2" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="1" pageNumber="270" unit="m" value="275.0" valueMax="300.0" valueMin="250.0">250–300 m</quantity>
