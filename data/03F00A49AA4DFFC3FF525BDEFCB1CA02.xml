<emphasis id="B92D674DAA4DFFC2FF525CBAFEDFCB83" bold="true" box="[151,281,1978,2000]" pageId="12" pageNumber="313">FIGURE 10</emphasis>
. 
<taxonomicName id="4C59C0DCAA4DFFC2FEE05CBAFDA0CB9C" box="[293,614,1978,2000]" class="Arachnida" family="Harpirhynchidae" genus="Perharpyrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="12" pageNumber="313" phylum="Arthropoda" rank="species" species="caprimulgus" status="sp. nov.">
