<quantity id="CC5F6281FFCFFF83FD784A2F6903FE69" box="[653,797,367,392]" metricMagnitude="-2" metricUnit="m" metricValue="1.31" metricValueMax="1.6199999999999999" metricValueMin="1.0" pageId="6" pageNumber="361" unit="mm" value="13.1" valueMax="16.2" valueMin="10.0">10–16.2 mm</quantity>
, n= 33; male forewing: 
<quantity id="CC5F6281FFCFFF83FBDC4A306EB5FE69" box="[1065,1195,368,392]" metricMagnitude="-3" metricUnit="m" metricValue="9.95" metricValueMax="10.9" metricValueMin="9.0" pageId="6" pageNumber="361" unit="mm" value="9.95" valueMax="10.9" valueMin="9.0">9–10.9 mm</quantity>
