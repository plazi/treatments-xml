, 
<taxonomicName id="4C7DC6A0FFCFFFD49984D5D7005EAF88" box="[457,575,1676,1699]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="12" pageNumber="4" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
, lateral view; 
<number id="3DB5AF04FFCFFFD49A98D5D70091AF8F" box="[725,752,1676,1700]" pageId="12" pageNumber="13" value="18.0">
