<taxonomicName id="4C6DBBE3FFB58D027B6C065CFD0F930F" authority="Scudder, 1890: 164" authorityName="Scudder" authorityPageNumber="164" authorityYear="1890" box="[297,687,284,306]" class="Insecta" family="Osmylidae" genus="Osmylus" kingdom="Animalia" order="Neuroptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="requietus">
