<taxonomicName id="4C2F4D37FF80FFFCA363B4D7D682FD20" authority="(Uvarov, 1942)" baseAuthorityName="Uvarov" baseAuthorityYear="1942" box="[855,1253,720,747]" class="Insecta" family="Baissogryllidae" genus="Chorthippus" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="3201" phylum="Arthropoda" rank="species" species="indus">
<emphasis id="B95BEAA6FF80FFFCA363B4D7D649FD21" bold="true" box="[855,1070,720,747]" italics="true" pageId="11" pageNumber="3201">Chorthippus indus</emphasis>
