<taxonomicName id="4C354D7E1B79FFAE2972F8D6A3A6C077" authority="Irfan &amp; Wang &amp; Zhao &amp; Zhang, 2022" authorityName="Irfan &amp; Wang &amp; Zhao &amp; Zhang" authorityYear="2022" box="[283,483,1824,1847]" class="Arachnida" family="Pimoidae" genus="Pimoa" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="324" phylum="Arthropoda" rank="species" species="yajiangensis" status="n. sp.">
