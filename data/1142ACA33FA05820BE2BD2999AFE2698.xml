14. 
<taxonomicName id="9DD9E7C2666171137FA50AD1FA27303C" LSID="1142ACA3-3FA0-5820-BE2B-D2999AFE2698" class="Insecta" family="Baetidae" genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis rimba" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="rimba">Labiobaetis rimba</taxonomicName>
