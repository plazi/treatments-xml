of 
<taxonomicName id="9BEF3AD6FFFEFFD68FCCFCA4FA110323" authority="Boettger, 1902" authorityName="Boettger" authorityYear="1902" box="[969,1432,777,803]" class="Gastropoda" family="Mitridae" genus="Mitra" kingdom="Animalia" order="Neogastropoda" pageId="11" pageNumber="12" phylum="Mollusca" rank="species" species="annae" subGenus="Costellaria">
