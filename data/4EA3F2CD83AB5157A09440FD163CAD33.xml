<taxonomicName id="3AE258B51D113E7281C4D1BF921FDD49" authorityName="Liston &amp; Prous" authorityYear="2020" class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="melagonia" order="Hymenoptera" pageId="0" pageNumber="127" phylum="Arthropoda" rank="species" species="melagonia">
