<caption id="DF2D1FA8FFF4FF86BF55FB7A7B4CFAD2" ID-DOI="http://doi.org/10.5281/zenodo.4891329" ID-Zenodo-Dep="4891329" httpUri="https://zenodo.org/record/4891329/files/figure.png" pageId="3" pageNumber="235" startId="3.[272,315,1255,1274]" targetBox="[252,1079,275,1234]" targetPageId="3">
<paragraph id="8BED4F20FFF4FF86BF55FB7A7B4CFAD2" blockId="3.[249,1081,1255,1298]" pageId="3" pageNumber="235">
<emphasis id="B9269332FFF4FF86BF55FB7A7B7FFB3A" bold="true" box="[272,370,1255,1274]" pageId="3" pageNumber="235">Figs. 2–5.</emphasis>
<emphasis id="B9269332FFF4FF86BFCFFB7A7883FB3A" box="[394,654,1255,1274]" italics="true" pageId="3" pageNumber="235">
<taxonomicName id="4C5234A3FFF4FF86BFCFFB7A7887FB3A" authorityName="Leschen &amp; Carlton" authorityYear="2000" box="[394,650,1255,1274]" class="Insecta" family="Endomychidae" genus="Micropsephodes" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="235" phylum="Arthropoda" rank="species" species="lundgreni">Micropsephodes lundgreni</taxonomicName>
