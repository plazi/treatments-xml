<taxonomicName id="EFE48BFB5201FF94FB52DCEFFE91FDD5" authority="Simon, 1897" authorityName="Simon" authorityYear="1897" class="Arachnida" family="Corinnidae" genus="Castianeira" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="556" phylum="Arthropoda" rank="species" species="zetes">
