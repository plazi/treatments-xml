<paragraph id="B03536B033742618FF7DFEBCBF072CF3" blockId="3.[160,1156,291,421]" pageId="3" pageNumber="2314">
<materialsCitation id="00E23CED33742618FF7DFEBCBF072CF3" ID-GBIF-Occurrence="2846789301" collectionCode="SMMG" country="Germany" location="Senckenberg Naturhistorische Sammlungen Dresden" pageId="3" pageNumber="2314" specimenCount="1" stateProvince="Sachsen" typeStatus="holotype">
