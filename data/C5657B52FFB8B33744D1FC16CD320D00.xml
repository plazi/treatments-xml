<quantity id="8A3467A1FFB8B33746F2FA6ECB000E93" box="[692,774,1447,1471]" metricMagnitude="-2" metricUnit="m" metricValue="5.5" pageId="36" pageNumber="37" unit="mm" value="55.0">55 mm</quantity>
