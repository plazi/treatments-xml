<taxonomicName id="19DABAC8FF98FFBFC3A6A0C7D7E1FF36" box="[988,1163,231,255]" class="Liliopsida" family="Arecaceae" genus="Calamus" kingdom="Plantae" order="Arecales" pageId="2" pageNumber="129" phylum="Tracheophyta" rank="species" species="nambariensis">
