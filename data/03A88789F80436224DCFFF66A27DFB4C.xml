<taxonomicName id="4C014D1CF80136224EEEFB94A76CFB90" authorityName="Marsh" authorityYear="2002" box="[950,1096,1125,1148]" class="Insecta" family="Braconidae" genus="Pedinotus" kingdom="Animalia" order="Hymenoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="variegatus">
<emphasis id="B975EA8DF80136224EEEFB94A76CFB90" box="[950,1096,1125,1148]" italics="true" pageId="44" pageNumber="45">P. variegatus</emphasis>
