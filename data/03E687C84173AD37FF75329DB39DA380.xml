<taxonomicName id="4C4F4D5D4172AD37FDD0312AB23DA50F" authority="(PANZ.)" baseAuthorityName="PANZ." box="[535,804,514,536]" class="Insecta" family="Ichneumonidae" genus="Hepiopelmus" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="634" phylum="Arthropoda" rank="species" species="variegatorius">
<emphasis id="B93BEACC4172AD37FDD0312AB20CA501" bold="true" box="[535,789,514,536]" pageId="37" pageNumber="634">
