<taxonomicName id="4C4F4D5D4172AD37FE57312AB0E7A50F" box="[400,510,514,536]" class="Insecta" family="Noctuidae" kingdom="Animalia" order="Lepidoptera" pageId="37" pageNumber="634" phylum="Arthropoda" rank="family">Noctuidae</taxonomicName>
