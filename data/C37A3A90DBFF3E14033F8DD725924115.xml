<taxonomicName id="7DC028A484726192C80A97052CF4918A" authorityName="Stephens" authorityYear="1828" class="Insecta" family="Carabidae" genus="Bembidium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="392" pageNumber="393" phylum="Arthropoda" rank="species" species="concinnum">
<emphasis id="C7956C366DEB6044E505483955B87101" italics="true" pageId="392" pageNumber="393">Bembidium concinnum</emphasis>
</taxonomicName>
