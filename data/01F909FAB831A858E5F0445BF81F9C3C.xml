<taxonomicName id="1A5E26562F9E9EF6F7DE31857DA1C15F" authority="Schmidt, 1852" authorityName="Schmidt" authorityYear="1852" class="Insecta" family="Leiodidae" genus="Leptoderus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Leptoderus" order="Coleoptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="genus">Leptoderus</taxonomicName>
