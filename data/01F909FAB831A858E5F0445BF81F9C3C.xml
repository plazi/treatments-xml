<taxonomicName id="06FA3E8C87C05D7AB99E3548A74493FE" class="Insecta" family="Leiodidae" genus="Leptodirus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptodirus" order="Coleoptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="genus">Leptodirus</taxonomicName>
