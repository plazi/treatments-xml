<taxonomicName id="4C4F4D12FFFE991AC05C6B40FAD2F912" box="[1105,1291,1652,1678]" class="Arachnida" family="Mimetidae" genus="Mimetus" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="undetermined">
