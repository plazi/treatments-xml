<taxonomicName id="3C39FE450E1697C87010FAEDFA828BF7" authority="Kishii, 1955" authorityName="Kishii" authorityYear="1955" class="Insecta" family="Elateridae" genus="Scutellathous" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Scutellathous spinosus" order="Coleoptera" pageId="0" pageNumber="85" phylum="Arthropoda" rank="species" species="spinosus">Scutellathous spinosus</taxonomicName>
