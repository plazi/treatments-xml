<taxonomicName id="DA37B4C82A450F25E2A93EA120E54EDF" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia" order="Zingiberales" pageId="50" pageNumber="51" phylum="Tracheophyta" rank="genus">Heliconia</taxonomicName>
sp. (
<taxonomicName id="8662A80C1FD12BAB7C1D2E56755FE490" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="50" pageNumber="51" rank="genus">Heliconiaceae</taxonomicName>
) (
<bibRefCitation id="C3EF6F8C7BF56188082D38FE3C0033FE" pageId="50" pageNumber="51">Staines 1996</bibRefCitation>
).
</paragraph>
</subSubSection>
