<paragraph id="5263B95196D6AB7255CE53C5E16669AA" pageNumber="1">
<taxonomicName id="8E1ACF25E28A153CEA0C8D5D0EF365EA" class="Arachnida" family="Nothridae" genus="Nothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageNumber="1" phylum="Arthropoda" rank="species" species="rostratus">Nothrus rostratus</taxonomicName>
