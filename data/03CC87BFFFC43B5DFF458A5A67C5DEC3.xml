<taxonomicName id="4C654D2AFFC43B5EFF458B93631ADC9B" box="[151,323,935,958]" class="Insecta" family="Mutillidae" genus="Taiwanomyrme" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="589" phylum="Arthropoda" rank="genus">
