<collectionCode id="ED74AE6CFFC43B5EFD168BB7615CDCBE" box="[708,773,899,923]" pageId="1" pageNumber="589">ICZN</collectionCode>
<number id="3DAD248EFFC43B5EFCC18BB76117DCBE" box="[787,846,899,923]" pageId="1" pageNumber="589" value="1999.0">1999</number>
; 
<bibRefCitation id="EFF44B58FFC43B5EFCB28BB6662FDCBE" author="Lelej" box="[864,1142,898,923]" pageId="1" pageNumber="589" refString="Lelej, A. S. &amp; Brothers, D. J. (2008) The genus-group names of Mutillidae (Hymenoptera) and their type species, with a new genus, new name, new synonymies, new combinations and lectotypifications. Zootaxa, 1889, 1 - 79." type="journal article" year="2008">
Lelej &amp; Brothers 
<number id="3DAD248EFFC43B5EFBEB8BB7662FDCBE" box="[1081,1142,899,923]" pageId="1" pageNumber="589" value="2008.0">2008</number>
</bibRefCitation>
