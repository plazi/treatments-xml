<taxonomicName id="73A5E5F03B2C4E1B7D753BAE5181FA66" box="[329,507,1445,1470]" class="Arachnida" family="Miturgidae" genus="Cheiracanthium" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="329" phylum="Arthropoda" rank="species" species="mayombense" status="sp. nov.">
<emphasis id="86D142613B2C4E1B7D753BAE5181FA66" box="[329,507,1445,1470]" italics="true" pageId="8" pageNumber="329">C. mayombense</emphasis>
</taxonomicName>
<taxonomicNameLabel id="9DE2FF1A3B2C4E1B7E3F3BAD5227FA66" box="[515,605,1445,1470]" pageId="8" pageNumber="329" rank="species">
<emphasis id="86D142613B2C4E1B7E3F3BAD5227FA66" bold="true" box="[515,605,1445,1470]" pageId="8" pageNumber="329">sp. nov.</emphasis>
</taxonomicNameLabel>
females are most similar to 
<taxonomicName id="73A5E5F03B2C4E1B7FA33BAE543AFA66" box="[927,1088,1445,1470]" class="Arachnida" family="Miturgidae" genus="Cheiracanthium" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="329" phylum="Arthropoda" rank="species" species="halophilum">
<emphasis id="86D142613B2C4E1B7FA33BAE543AFA66" box="[927,1088,1445,1470]" italics="true" pageId="8" pageNumber="329">C. halophilum</emphasis>
</taxonomicName>
, 
<taxonomicName id="73A5E5F03B2C4E1B78733BAE5492FA66" box="[1103,1256,1445,1470]" class="Arachnida" family="Miturgidae" genus="Cheiracanthium" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="329" phylum="Arthropoda" rank="species" species="ghanaense" status="sp. nov.">
<emphasis id="86D142613B2C4E1B78733BAE5492FA66" box="[1103,1256,1445,1470]" italics="true" pageId="8" pageNumber="329">C. ghanaense</emphasis>
</taxonomicName>
<taxonomicNameLabel id="9DE2FF1A3B2C4E1B78CC3BAD5530FA66" box="[1264,1354,1445,1470]" pageId="8" pageNumber="329" rank="species">
<emphasis id="86D142613B2C4E1B78CC3BAD5530FA66" bold="true" box="[1264,1354,1445,1470]" pageId="8" pageNumber="329">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="73A5E5F03B2C4E1B79BE3BAE5168FA3A" class="Arachnida" family="Miturgidae" genus="Cheiracanthium" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="329" phylum="Arthropoda" rank="species" species="kakumense" status="sp. nov.">
<emphasis id="86D142613B2C4E1B79BE3BAE5168FA3A" italics="true" pageId="8" pageNumber="329">C. kakumense</emphasis>
</taxonomicName>
<taxonomicNameLabel id="9DE2FF1A3B2C4E1B7D253BC15108FA3A" box="[281,370,1481,1506]" pageId="8" pageNumber="329" rank="species">
<emphasis id="86D142613B2C4E1B7D253BC15108FA3A" bold="true" box="[281,370,1481,1506]" pageId="8" pageNumber="329">sp. nov.</emphasis>
</taxonomicNameLabel>
