<collectionCode id="ED68D8F2FFFDFF82FAE3F995FA6EF937" box="[1352,1436,1659,1683]" country="Spain" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35275" name="Museo Nacional de Ciencias Naturales" pageId="10" pageNumber="56">MNCN</collectionCode>
