<date id="FF99101A296DFFCFFBD7E565FB654ECF" box="[1025,1192,885,914]" pageId="45" pageNumber="44" value="2011-04-06">
<number id="3DEF24FD296DFFCFFBD7E565FBDC4ECF" box="[1025,1041,885,914]" pageId="45" pageNumber="44" value="6.0">6</number>
