- 
<number id="3DEF24FD296DFFCFFC0CE541FC214E33" box="[986,1004,849,878]" pageId="45" pageNumber="44" value="2.0">2</number>
), 
<collectingCountry id="F330764A296DFFCFFBC3E541FBB34E33" box="[1045,1150,849,878]" name="China" pageId="45" pageNumber="44">CHINA</collectingCountry>
, Hainan Island, Dacheng, 
<taxonomicName id="4C274D59296DFFCFFC92E565FC5F4ECF" box="[836,914,885,914]" class="Arachnida" family="Uloboridae" genus="Octonoba" kingdom="Animalia" order="Araneae" pageId="45" pageNumber="44" phylum="Arthropoda" rank="species" species="xihua">Xihua</taxonomicName>
