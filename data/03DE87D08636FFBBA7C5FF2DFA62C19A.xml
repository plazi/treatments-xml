<taxonomicName id="4C774D458636FFBBA662FBC5FE21C1D2" authorityName="Huddleston" authorityYear="1980" box="[304,479,1150,1174]" class="Insecta" family="Braconidae" genus="Meteorus" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="eadyi">
