<taxonomicName id="3E255C2F9F511798E36C65F021A0FE8C" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora alata" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="alata">Passiflora alata</taxonomicName>
