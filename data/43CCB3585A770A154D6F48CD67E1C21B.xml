<taxonomicName id="2ABDF71E0A2AFD9D616FA85FCD2AD604" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora nitida" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="nitida">Passiflora nitida</taxonomicName>
Kunth, 
<taxonomicName id="4D19BDB5B0744F8E9EC6653275F02193" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora coccinea" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="coccinea">Passiflora coccinea</taxonomicName>
Aublet, 
<taxonomicName id="7E1CC46C67804A22FD53432071DF0E72" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Passiflora gibertii" order="Malpighiales" pageId="0" pageNumber="13067" phylum="Tracheophyta" rank="species" species="gibertii">Passiflora gibertii</taxonomicName>
