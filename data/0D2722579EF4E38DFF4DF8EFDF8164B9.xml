<taxonomicName id="428EE8C29EF7E38DFEEAF8DEDAE1646D" authorityName="Quicke &amp; Butcher" authorityYear="2012" box="[304,477,1831,1853]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="occimaculatus" status="sp. nov.">
<emphasis id="B7FA4F539EF7E38DFEEAF8DEDAE1646D" box="[304,477,1831,1853]" italics="true" pageId="146" pageNumber="147">A. occimaculatus</emphasis>
</taxonomicName>
<emphasis id="B7FA4F539EF7E38DFE3EF8D1D908646D" bold="true" box="[484,564,1832,1853]" pageId="146" pageNumber="147">
<taxonomicNameLabel id="ACC9F2289EF7E38DFE3EF8D1D908646D" box="[484,564,1832,1853]" pageId="146" pageNumber="147" rank="species">sp. nov.</taxonomicNameLabel>
