<taxonomicName id="AB6A3CED12B9A5AC7BA0F211D3407D6B" authorityName="N. Snow" authorityYear="2012" class="Magnoliopsida" family="Myrtaceae" genus="Rhodamnia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Myrtales" pageId="6" pageNumber="37" phylum="Tracheophyta" rank="species" species="makumak">
<emphasis id="5DA6657BAFA149AB8C99FC9652D1D4EA" italics="true" pageId="6" pageNumber="37">Rhodamnia makumak</emphasis>
</taxonomicName>
</td>
</tr>
<tr id="351DF62CD94C1AB76DD8616A52D40E1D" pageId="6" pageNumber="37">
