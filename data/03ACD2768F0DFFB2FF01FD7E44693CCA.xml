<paragraph id="8BBA63608F0DFFB0FFD9FD4B421C3C53" blockId="89.[119,648,755,830]" pageId="89" pageNumber="90">
<taxonomicName id="4C0518E38F0DFFB0FFD9FD4B41DE3C66" authority="Brignoli, 1981: 97" authorityName="Brignoli" authorityPageNumber="97" authorityYear="1981" box="[119,456,755,776]" class="Arachnida" family="Pholcidae" genus="Gertschiola" kingdom="Animalia" order="Araneae" pageId="89" pageNumber="90" phylum="Arthropoda" rank="genus">
