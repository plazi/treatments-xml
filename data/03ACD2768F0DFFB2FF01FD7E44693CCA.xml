<taxonomicName id="4C0518E38F0DFFB0FE3DFCB641913C53" authority="Mello-Leitao, 1941" authorityName="Mello-Leitao" authorityYear="1941" class="Arachnida" family="Pholcidae" genus="Physocyclus" kingdom="Animalia" order="Araneae" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="macrostylus">
