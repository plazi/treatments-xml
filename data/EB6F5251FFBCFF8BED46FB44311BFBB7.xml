<taxonomicName id="A4C698C4FFBDFF94EE18F8A531DFF8A7" authority="(Hincks, 1866)" baseAuthorityName="Hincks" baseAuthorityYear="1866" box="[316,691,1859,1886]" class="Hydrozoa" family="Campanulariidae" genus="Clytia" kingdom="Animalia" order="Leptothecata" pageId="15" pageNumber="64" phylum="Cnidaria" rank="species" species="gigantea">
