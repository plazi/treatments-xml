<taxonomicName id="A4C698C4FFBDFF94EBCFF88032C8F85A" authority="Nutting, 1904" authorityName="Nutting" authorityYear="1904" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="15" pageNumber="64" phylum="Cnidaria" rank="species" species="areyi">
