<taxonomicName id="4C7A4D21FF85FFD7FE1AFADCB016DD0A" box="[406,536,1383,1404]" class="Insecta" family="Baetidae" genus="Baetis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="16" pageNumber="121" phylum="Arthropoda" rank="species" species="vernus">
