<taxonomicName id="4C7A4D21FF85FFD7FB28FAB4B71ADD52" box="[1188,1300,1295,1316]" class="Insecta" family="Baetidae" genus="Baetis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="16" pageNumber="121" phylum="Arthropoda" rank="species" species="alius">
