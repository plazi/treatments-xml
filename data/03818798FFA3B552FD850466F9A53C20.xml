<taxonomicName id="4C284D0DFFA2B55DFC910045F89A3ECB" authorityName="Loginova" authorityYear="1976" box="[803,914,1652,1674]" class="Insecta" family="Psyllidae" genus="Arytainilla" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="435" phylum="Arthropoda" rank="species" species="dividens">
<emphasis id="B95CEA9CFFA2B55DFC910045F89A3ECB" box="[803,914,1652,1674]" italics="true" pageId="39" pageNumber="435">A. dividens</emphasis>
</taxonomicName>
(scale bars=50 
<emphasis id="B95CEA9CFFA2B55DFB8B0045FF4E3EC8" bold="true" box="[1081,1094,1653,1674]" italics="true" pageId="39" pageNumber="435">m</emphasis>
