<taxonomicName id="4C284D0DFFA2B55DFB8A006AFFE63E2D" authorityName="Percy Csiro" authorityYear="2003" baseAuthorityName="Percy Csiro" baseAuthorityYear="2003" box="[1080,1262,1626,1647]" class="Insecta" family="Psyllidae" genus="Arytinnis" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="435" phylum="Arthropoda" rank="species" species="modica">
