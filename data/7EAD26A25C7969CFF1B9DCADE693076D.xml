<taxonomicName id="C42218FFA2DF0B08F072C7214652C10C" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Abies religiosa" order="Pinales" pageId="37" pageNumber="38" phylum="Tracheophyta" rank="species" species="religiosa">Abies religiosa</taxonomicName>
