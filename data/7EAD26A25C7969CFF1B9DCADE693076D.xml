<taxonomicName id="D35A94CE4C40614B4A768FD64E780882" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Abies religiosa" order="Pinales" pageId="37" pageNumber="38" phylum="Tracheophyta" rank="species" species="religiosa">Abies religiosa</taxonomicName>
