<taxonomicName id="77A34D497A15FF85D902FDDEB56FFDEB" authority="Chamberlin and Mulaik, 1942" authorityName="Chamberlin and Mulaik" authorityYear="1942" box="[587,1132,521,549]" class="Arachnida" family="Opilioacaridae" genus="Neocarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opilioacarida" pageId="3" pageNumber="507" phylum="Arthropoda" rank="genus">
