, 
<taxonomicName id="4C0F4D457B180366EC7EFEDDFD21FE75" box="[577,743,367,392]" class="Magnoliopsida" family="Thymelaeaceae" genus="Banksia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="96" pageNumber="97" phylum="Tracheophyta" rank="species" species="integrifolia">B. integrifolia</taxonomicName>
, 
<taxonomicName id="4C0F4D457B180366ECC7FEDDFC54FE75" box="[760,914,369,392]" class="Magnoliopsida" family="Thymelaeaceae" genus="Banksia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="96" pageNumber="97" phylum="Tracheophyta" rank="species" species="marginata">B. marginata</taxonomicName>
, 
<taxonomicName id="4C0F4D457B180366ED9BFEDDFBD5FE75" box="[932,1043,369,392]" class="Magnoliopsida" family="Thymelaeaceae" genus="Banksia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="96" pageNumber="97" phylum="Tracheophyta" rank="species" species="ornata">B. ornata</taxonomicName>
</emphasis>
(
<taxonomicName id="4C0F4D457B180366EA18FEDCFB61FE75" authorityName="" authorityYear="1789" box="[1063,1191,368,392]" class="Magnoliopsida" family="Proteaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Proteales" pageId="96" pageNumber="97" phylum="Magnoliopsida" rank="family">Proteaceae</taxonomicName>
