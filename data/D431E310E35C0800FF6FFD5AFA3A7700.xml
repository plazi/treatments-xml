<emphasis id="6EEC8E14E35C0800FD1AFBE1FCDA7762" box="[738,892,1062,1083]" italics="true" pageId="53" pageNumber="54">
ro, la, ex, c 
<number id="EA504021E35C0800FCB7FBE1FCFD7762" box="[847,859,1062,1083]" pageId="53" pageNumber="54" value="1.0">1</number>
, c 
