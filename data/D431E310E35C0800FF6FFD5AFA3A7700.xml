<keyLead id="E769F133E35C0800FF6FFC76FA3A70BA" pageId="53" pageNumber="54">
<number id="EA504021E35C0800FF6FFC76FF03709F" box="[151,165,945,966]" pageId="53" pageNumber="54" value="3.0">3</number>
