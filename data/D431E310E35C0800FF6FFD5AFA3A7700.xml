<taxonomicName id="9B982985E35C0800FE8AFCB1FE4670D2" box="[370,480,886,907]" class="Arachnida" family="Tydeidae" genus="Paralorryia" kingdom="Animalia" order="Prostigmata" pageId="53" pageNumber="54" phylum="Arthropoda" rank="genus">Paralorryia</taxonomicName>
