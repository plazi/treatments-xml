<taxonomicName id="4C174D43FF58FF7C98EAFC5BF8DA33C1" authority="(Meek &amp; Pierson, 1895)" baseAuthorityName="Meek &amp; Pierson" baseAuthorityYear="1895" box="[151,667,937,964]" class="Actinopterygii" family="Isodiametridae" genus="Rimicola" kingdom="Animalia" order="Acoela" pageId="201" pageNumber="202" phylum="Xenacoelomorpha" rank="species" species="muscarum">
<emphasis id="B963EAD2FF58FF7C98EAFC5BFBC033C1" bold="true" box="[151,385,937,964]" italics="true" pageId="201" pageNumber="202">Rimicola muscarum</emphasis>
