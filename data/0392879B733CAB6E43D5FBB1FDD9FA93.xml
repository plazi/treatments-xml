<taxonomicName id="4C3B4D0E733CAB6E405DFB1CFBA1FB7F" authority="(Hurd 1979)" authorityName="Hurd" authorityYear="1979" baseAuthorityName="Hurd" baseAuthorityYear="1979" box="[799,1077,1228,1253]" class="Magnoliopsida" family="Nymphaeaceae" genus="Nymphaea" kingdom="Plantae" order="Nymphaeales" pageId="73" pageNumber="74" phylum="Tracheophyta" rank="genus">
