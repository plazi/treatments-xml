<taxonomicName id="4C3B4D0E733CAB6E4259FB3FFE0CFA93" box="[283,408,1263,1288]" class="Liliopsida" family="Pontederiaceae" genus="Pontederia" kingdom="Plantae" order="Commelinales" pageId="73" pageNumber="74" phylum="Tracheophyta" rank="genus">
