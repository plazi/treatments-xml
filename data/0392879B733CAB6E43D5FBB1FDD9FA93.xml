<taxonomicName id="4C3B4D0E733CAB6E4138FB1CFD4BFB7E" box="[634,735,1228,1253]" class="Magnoliopsida" family="Nelumbonaceae" genus="Nelumbo" kingdom="Plantae" order="Proteales" pageId="73" pageNumber="74" phylum="Tracheophyta" rank="genus">
