<taxonomicName id="4C1C4D0F754E6C5893F1FD35215BAAF0" authority="(ER. MARCUS, 1955)" baseAuthorityName="ER. MARCUS" baseAuthorityYear="1955" box="[947,1307,689,713]" class="Gastropoda" family="Dorididae" genus="Doris" kingdom="Animalia" order="Nudibranchia" pageId="25" pageNumber="560" phylum="Mollusca" rank="species" species="ilo">
