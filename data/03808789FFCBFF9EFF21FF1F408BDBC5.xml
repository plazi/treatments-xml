<taxonomicName id="4C294D1CFFCAFF91FEA1F827408CDD8A" authority="Tikader &amp; Malhotra, 1980" authorityName="Tikader &amp; Malhotra" authorityYear="1980" box="[279,698,1966,1990]" class="Arachnida" family="Lycosidae" genus="Arctosa" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="496" phylum="Arthropoda" rank="species" species="indica">
