<taxonomicName id="A6934F80CB353122FF03F3BF69CD0050" authority="(Cav.) Steud." authorityName="Steud." baseAuthorityName="Cav." box="[142,503,1266,1291]" class="Liliopsida" family="Poaceae" genus="Phragmites" kingdom="Plantae" order="Poales" pageId="11" pageNumber="120" phylum="Tracheophyta" rank="species" species="australis">
