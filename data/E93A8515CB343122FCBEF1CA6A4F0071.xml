<emphasis id="53E7E811CB353122FEC1F25E69F80070" box="[332,450,1298,1322]" italics="true" pageId="11" pageNumber="120">
<taxonomicName id="A6934F80CB353122FEC1F25E699A0070" box="[332,416,1298,1322]" class="Liliopsida" family="Poaceae" genus="Arundo" kingdom="Plantae" order="Poales" pageId="11" pageNumber="120" phylum="Tracheophyta" rank="species" species="plinii">A. plinii</taxonomicName>
