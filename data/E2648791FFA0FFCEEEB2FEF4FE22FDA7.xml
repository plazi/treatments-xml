<taxonomicName id="ADCD4D04FFA0FFCEEED7FDB8FEBDFD83" authorityName="Mitchell" authorityYear="1838" box="[242,320,512,538]" class="Insecta" family="Eumenidae" genus="Abispa" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="579" phylum="Arthropoda" rank="genus">
