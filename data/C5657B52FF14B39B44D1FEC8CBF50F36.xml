<taxonomicName id="8ACCB1C7FF14B39B47DAFBCACBEA0F37" authorityName="Kensley" authorityYear="1996" box="[924,1004,1027,1051]" class="Malacostraca" family="Axiidae" genus="Eiconaxius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="136" pageNumber="137" phylum="Arthropoda" rank="species" species="baja">
