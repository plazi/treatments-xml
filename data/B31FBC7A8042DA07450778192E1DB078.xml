, 
<collectingRegion id="F972C38E8041DA0746CB7BA62F64B6CC" box="[763,848,743,767]" country="Turkey" name="Yozgat" pageId="18" pageNumber="1681">Yozgat</collectingRegion>
, 
<collectingCountry id="43A14DFC8041DA07476D7BA62F86B6CC" box="[861,946,743,767]" name="Turkey" pageId="18" pageNumber="1681">Turkey</collectingCountry>
