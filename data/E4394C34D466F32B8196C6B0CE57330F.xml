, and 
<taxonomicName id="AB9086A1D464F32B828BC73BCE28330F" authority="Boulenger, 1890" authorityName="Boulenger" authorityYear="1890" box="[906,1341,511,536]" class="Amphibia" family="Rhacophoridae" genus="Polypedates" kingdom="Animalia" order="Anura" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="colletti">
