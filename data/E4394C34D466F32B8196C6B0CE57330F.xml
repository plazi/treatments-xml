(Boulenger, 
<number id="DA58EF05D464F32B8095C4C4CAC5330F" box="[404,464,511,535]" pageId="15" pageNumber="16" value="1882.0">1882</number>
)
</taxonomicName>
, 
<taxonomicName id="AB9086A1D464F32B80E2C73BC85A330F" authority="Dring, 1987" authorityName="Dring" authorityYear="1987" box="[483,847,511,536]" class="Amphibia" family="Rhacophoridae" genus="Philautus" kingdom="Animalia" order="Anura" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="kerangae">
