<number id="DA58EF05D464F32B81CAC4E7CA1330EC" box="[203,262,476,500]" pageId="15" pageNumber="16" value="1858.0">1858</number>
)
</taxonomicName>
, 
<taxonomicName id="AB9086A1D464F32B801EC4E7C9C930EC" authority="Peters, 1867" authorityName="Peters" authorityYear="1867" box="[287,732,476,500]" class="Amphibia" family="Microhylidae" genus="Metaphrynella" kingdom="Animalia" order="Anura" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="sundana">
<emphasis id="5EE42130D464F32B801EC4E7C93B30EC" box="[287,558,476,500]" italics="true" pageId="15" pageNumber="16">Metaphrynella sundana</emphasis>
(Peters, 
<number id="DA58EF05D464F32B8398C4E7C9C130EC" box="[665,724,476,500]" pageId="15" pageNumber="16" value="1867.0">1867</number>
)
</taxonomicName>
, 
<taxonomicName id="AB9086A1D464F32B83EDC4E7CFF130EC" authority="Das &amp; Haas, 2010" authorityName="Das &amp; Haas" authorityYear="2010" box="[748,1252,476,500]" class="Amphibia" family="Microhylidae" genus="Microhyla" kingdom="Animalia" order="Anura" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="nepenthicola">
<emphasis id="5EE42130D464F32B83EDC4E7C8E930EC" box="[748,1020,476,500]" italics="true" pageId="15" pageNumber="16">Microhyla nepenthicola</emphasis>
Das &amp; Haas, 
<number id="DA58EF05D464F32B85A8C4E7CFF130EC" box="[1193,1252,476,500]" pageId="15" pageNumber="16" value="2010.0">2010</number>
</taxonomicName>
, 
<taxonomicName id="AB9086A1D464F32B85F2C4E7CAC2330F" authority="Boulenger, 1882" authorityName="Boulenger" authorityYear="1882" class="Amphibia" family="Bufonidae" genus="Pelophryne" kingdom="Animalia" order="Anura" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="guentheri">
<emphasis id="5EE42130D464F32B85F2C4E7CE6130EC" box="[1267,1396,476,500]" italics="true" pageId="15" pageNumber="16">Pelophryne</emphasis>
