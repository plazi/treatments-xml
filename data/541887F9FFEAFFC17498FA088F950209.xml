<paragraph id="DC0E36EFFFEAFFDE7498F9A388110725" blockId="15.[178,1085,1504,1574]" box="[178,1075,1554,1574]" pageId="15" pageNumber="378">
<typeStatus id="030A884DFFEAFFDE7498F9A38D290725" box="[178,267,1554,1574]" pageId="15" pageNumber="378">Paratypus</typeStatus>
