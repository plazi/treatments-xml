<quantity id="A86A1599FFBBFFA1FCE9F9B31652F9DB" box="[814,881,1622,1646]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="36" pageNumber="237" unit="mm" value="4.0">4 mm</quantity>
