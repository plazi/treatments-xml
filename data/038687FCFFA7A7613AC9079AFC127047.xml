<taxonomicName id="4C2F4D69FFA7A7613AC9079AFCCE77B7" authorityName="Stechow" authorityYear="1919" box="[699,820,410,434]" class="Hydrozoa" family="Kirchenpaueriidae" genus="Oswaldella" kingdom="Animalia" order="Leptothecata" pageId="51" pageNumber="855" phylum="Cnidaria" rank="genus">
