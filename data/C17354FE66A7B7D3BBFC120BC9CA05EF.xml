<taxonomicName id="68CA53D556FF48F973B1AD842B1EEFFE" authority="Konow, 1902" authorityName="Konow" authorityYear="1902" class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristiphora (Pristiphora) denudata" order="Hymenoptera" pageId="0" pageNumber="1168" phylum="Arthropoda" rank="species" species="denudata" subGenus="Pristiphora">Pristiphora (Pristiphora) denudata Konow, 1902</taxonomicName>
