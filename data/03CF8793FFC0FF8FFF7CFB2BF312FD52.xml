, 
<quantity id="4C9E9B60FFC0FF8CFED1FA23F391FA61" box="[325,401,1445,1469]" metricMagnitude="3" metricUnit="m" metricValue="2.02" pageId="5" pageNumber="64" unit="m" value="2020.0">
<elevation id="004BD1B6FFC0FF8CFED1FA23F391FA61" box="[325,401,1445,1469]" metricMagnitude="3" metricUnit="m" metricValue="2.02" pageId="5" pageNumber="64" unit="m" value="2020.0">2020 m</elevation>
