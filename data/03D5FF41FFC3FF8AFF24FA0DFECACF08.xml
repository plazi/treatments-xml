<taxonomicName id="4C7C35D4FFC3FF8AFEF0FA63FDDECCCF" authorityName="JARRIGE" authorityYear="1949" box="[358,626,1504,1526]" class="Insecta" family="Staphylinidae" genus="Pseudobium" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="412" phylum="Arthropoda" rank="species" species="peyerimhoffi">
