<typeStatus box="[121,174,215,238]" pageId="24" pageNumber="309">Type</typeStatus>
species 
<taxonomicName authority="Gray, 2010" authorityName="Gray" authorityYear="2010" box="[264,459,215,238]" class="Arachnida" family="Atracidae" genus="Illawarra" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="309" phylum="Arthropoda" rank="species" species="wisharti" status="new species">
