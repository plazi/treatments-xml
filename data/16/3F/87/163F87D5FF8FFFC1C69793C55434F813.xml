<taxonomicName authorityName="Gray" authorityYear="2010" box="[901,1003,1878,1901]" class="Arachnida" family="Atracidae" genus="Hadronyche" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="100" pageNumber="385" phylum="Arthropoda" rank="species" species="raveni">
