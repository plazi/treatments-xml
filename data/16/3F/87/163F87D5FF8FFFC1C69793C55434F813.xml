<taxonomicName authorityName="Gray" authorityYear="2010" box="[1058,1253,1878,1901]" class="Arachnida" family="Atracidae" genus="Hadronyche" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="100" pageNumber="385" phylum="Arthropoda" rank="species" species="lamingtonensis">
