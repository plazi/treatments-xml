<taxonomicName box="[470,614,1762,1785]" class="Insecta" family="Empididae" genus="Macrostomus" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="61" phylum="Arthropoda" rank="genus">Macrostomus</taxonomicName>
<taxonomicName box="[623,703,1762,1785]" pageId="12" pageNumber="61" rank="species" species="tarsalis" status="n. sp.">tarsalis</taxonomicName>
</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[719,780,1762,1785]" pageId="12" pageNumber="61" rank="species">n. sp.</taxonomicNameLabel>
