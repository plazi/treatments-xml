with labels: “ 
<collectingCountry box="[514,612,1234,1258]" name="Morocco" pageId="3" pageNumber="404">MAROC</collectingCountry>
70 MA 70: 85, Moyen Atlas Nord-Est, 
<collectorName pageId="3" pageNumber="404">J. Bou Iblane</collectorName>
, 
<quantity box="[281,417,1265,1289]" metricMagnitude="3" metricUnit="m" metricValue="2.25" metricValueMax="2.5" metricValueMin="2.0" pageId="3" pageNumber="404" unit="m" value="2250.0" valueMax="2500.0" valueMin="2000.0">
