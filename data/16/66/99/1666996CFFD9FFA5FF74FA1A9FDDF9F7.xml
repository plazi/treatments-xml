<collectingCountry box="[759,811,1525,1547]" name="India" pageId="3" pageNumber="1020">India</collectingCountry>
and 
<collectingCountry box="[865,925,1525,1547]" name="Nepal" pageId="3" pageNumber="1020">Nepal</collectingCountry>
