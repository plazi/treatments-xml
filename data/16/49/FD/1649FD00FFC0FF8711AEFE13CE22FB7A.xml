and 
<emphasis box="[991,1019,917,940]" italics="true" pageId="1" pageNumber="132">an</emphasis>
<emphasis box="[1019,1027,931,944]" italics="true" pageId="1" pageNumber="132">
<number box="[1019,1027,931,944]" pageId="1" pageNumber="132" value="2.0">2</number>
