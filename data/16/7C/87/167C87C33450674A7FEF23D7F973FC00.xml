) 
<emphasis box="[230,351,853,876]" italics="true" pageId="1" pageNumber="57">exspinosus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[365,583,852,876]" class="Arachnida" family="Damaeidae" genus="Damaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="57" phylum="Arthropoda" rank="species" species="wulongensis" subGenus="Tectodamaeus">
<emphasis box="[365,393,853,876]" italics="true" pageId="1" pageNumber="57">D.</emphasis>
