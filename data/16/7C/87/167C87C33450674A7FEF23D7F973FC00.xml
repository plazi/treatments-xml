<emphasis box="[782,809,853,876]" italics="true" pageId="1" pageNumber="57">D.</emphasis>
(
<emphasis box="[818,838,853,876]" italics="true" pageId="1" pageNumber="57">T.</emphasis>
) 
<emphasis box="[861,974,852,876]" italics="true" pageId="1" pageNumber="57">brevisetus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[988,1116,852,876]" class="Arachnida" family="Damaeidae" genus="Damaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="57" phylum="Arthropoda" rank="species" species="yaoi" subGenus="Tectodamaeus">
