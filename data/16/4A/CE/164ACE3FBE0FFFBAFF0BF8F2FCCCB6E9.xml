<taxonomicName authority="Da Silva Brum, 1975" authorityName="Da Silva Brum" authorityYear="1975" box="[687,1142,692,716]" class="Malacostraca" family="Artesiidae" genus="Spelaeogammarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
