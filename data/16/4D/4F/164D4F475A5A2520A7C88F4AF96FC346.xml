<emphasis bold="true" box="[151,237,401,426]" pageId="6" pageNumber="396">Length</emphasis>
(n = 1). Body 
<quantity box="[401,504,402,426]" metricMagnitude="-2" metricUnit="m" metricValue="1.34" pageId="6" pageNumber="396" unit="mm" value="13.4">13.4 mm</quantity>
; wing 
