</taxonomicName>
, 
<taxonomicName authority="Roder" authorityName="Roder" box="[508,858,2007,2032]" class="Insecta" family="Syrphidae" genus="Palpada" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="395" phylum="Arthropoda" rank="species" species="monticola">
