</taxonomicName>
, 
<taxonomicName authority="Hippa" authorityName="Hippa" box="[151,495,2007,2032]" class="Insecta" family="Syrphidae" genus="Meropidia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="395" phylum="Arthropoda" rank="species" species="neurostigma">
<emphasis box="[151,415,2007,2032]" italics="true" pageId="5" pageNumber="395">Meropidia neurostigma</emphasis>
