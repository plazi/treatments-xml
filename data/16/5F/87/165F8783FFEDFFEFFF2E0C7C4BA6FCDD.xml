Total length 
<quantity box="[494,630,1874,1898]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" metricValueMax="2.9" metricValueMin="2.7" pageId="1" pageNumber="403" unit="mm" value="2.8" valueMax="2.9" valueMin="2.7">2.7–2.9 mm</quantity>
(
<typeStatus box="[645,749,1874,1898]" pageId="1" pageNumber="403">holotype</typeStatus>
: 
<quantity box="[760,851,1874,1898]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="1" pageNumber="403" unit="mm" value="2.8">2.8 mm</quantity>
), total width 
<quantity box="[999,1135,1874,1898]" metricMagnitude="-3" metricUnit="m" metricValue="1.45" metricValueMax="1.5" metricValueMin="1.4" pageId="1" pageNumber="403" unit="mm" value="1.45" valueMax="1.5" valueMin="1.4">1.4–1.5 mm</quantity>
(
