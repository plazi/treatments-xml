(
<collectingRegion box="[381,468,540,564]" country="China" name="Yunnan" pageId="16" pageNumber="731">Yunnan</collectingRegion>
), 
<collectingCountry box="[487,541,540,563]" name="Laos" pageId="16" pageNumber="731">Laos</collectingCountry>
