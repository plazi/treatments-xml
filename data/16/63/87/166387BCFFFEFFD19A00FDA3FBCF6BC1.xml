<collectingCountry box="[841,895,792,815]" name="Laos" pageId="8" pageNumber="723">Laos</collectingCountry>
, 
<collectingCountry box="[906,1004,791,815]" name="Thailand" pageId="8" pageNumber="723">Thailand</collectingCountry>
