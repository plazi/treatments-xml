<taxonomicName baseAuthorityName="Broderip" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="53" pageNumber="54" phylum="Mollusca" rank="species" species="erosus">
