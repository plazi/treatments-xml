(
<emphasis bold="true" box="[610,743,1502,1523]" pageId="18" pageNumber="119">34, 36-37, 39</emphasis>
: holotype; 
<emphasis bold="true" box="[858,920,1502,1523]" pageId="18" pageNumber="119">35, 38</emphasis>
