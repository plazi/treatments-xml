<subSubSection pageId="3" pageNumber="494" type="materials_examined">
<paragraph blockId="3.[151,1437,609,1752]" pageId="3" pageNumber="494">
<emphasis bold="true" box="[199,426,1689,1716]" pageId="3" pageNumber="494">Material examined</emphasis>
. 
<materialsCitation ID-GBIF-Occurrence="2273363941" collectingDate="2016-01" collectionCode="MZSP" country="Peru" location="Iquitos-Nauta" pageId="3" pageNumber="494" specimenCount="1" specimenCount-male="1" stateProvince="Loreto">
<collectingCountry box="[440,512,1690,1716]" name="Peru" pageId="3" pageNumber="494">PERU</collectingCountry>
(
<emphasis bold="true" box="[529,764,1690,1716]" pageId="3" pageNumber="494">new country record</emphasis>
), 
<collectingRegion box="[787,861,1691,1716]" country="Peru" name="Loreto" pageId="3" pageNumber="494">
<emphasis box="[787,861,1691,1716]" italics="true" pageId="3" pageNumber="494">Loreto</emphasis>
