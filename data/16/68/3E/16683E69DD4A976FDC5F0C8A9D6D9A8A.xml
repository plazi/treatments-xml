<taxonomicName class="Insecta" family="Liviidae" genus="Notophorina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Notophorina fusca" order="Hemiptera" pageId="8" pageNumber="99" phylum="Arthropoda" rank="species" species="fusca">Notophorina fusca</taxonomicName>
