<normalizedToken originalValue="Aramanaí">Aramanai</normalizedToken>
para Pindobal, 
<normalizedToken originalValue="próximo">proximo</normalizedToken>
a Fazenda 
<normalizedToken originalValue="São">Sao</normalizedToken>
<normalizedToken originalValue="Sebastião">Sebastiao</normalizedToken>
, 41 m a. s. l., 
<geoCoordinate degrees="2" direction="south" minutes="38" orientation="latitude" precision="1" seconds="24.7" value="-2.6401944">2°38'24.7&quot;S</geoCoordinate>
, 
<geoCoordinate degrees="54" direction="west" minutes="59" orientation="longitude" precision="1" seconds="06.6" value="-54.98517">54°59'06.6&quot;W</geoCoordinate>
