<taxonomicName authority="Huber, 2018" authorityName="Huber" authorityYear="2018" box="[1026,1141,1851,1872]" class="Arachnida" family="Pholcidae" genus="Mesabolivar" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="yucuma" status="sp. n.">
