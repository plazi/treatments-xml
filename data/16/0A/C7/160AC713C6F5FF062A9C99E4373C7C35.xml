state (
<collectingCountry box="[808,879,1340,1365]" name="Brazil" pageId="131" pageNumber="132">Brazil</collectingCountry>
) only (
<figureCitation box="[959,1058,1340,1365]" captionStart="FIGURE 741" captionStartId="171.[151,250,1963,1985]" captionTargetBox="[151,1436,1091,1942]" captionTargetId="figure@171.[151,1436,1091,1942]" captionTargetPageId="171" captionText="FIGURE 741. Known distribution of the Carapoia paraguaensis group." httpUri="https://zenodo.org/record/1202717/files/figure.png" pageId="131" pageNumber="132">Fig. 741</figureCitation>
).
</paragraph>
