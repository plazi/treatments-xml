<quantity id="4CF79B33FF8EFFA7C1F9FC8CFC22FC85" box="[896,948,853,873]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="97" pageNumber="252" unit="m" value="300.0">300m</quantity>
