<quantity id="4CF79B33FF8EFFA7C0FFFCE2FD2BFCA3" box="[646,701,827,847]" metricMagnitude="2" metricUnit="m" metricValue="2.3" pageId="97" pageNumber="252" unit="m" value="230.0">230m</quantity>
