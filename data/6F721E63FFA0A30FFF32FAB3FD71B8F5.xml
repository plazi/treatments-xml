<location id="E204F9AEFFA0A30FFB83F9C3FBD7B929" LSID="urn:lsid:plazi:treatment:6F721E63FFA0A30FFF32FAB3FD71B8F5:E204F9AEFFA0A30FFB83F9C3FBD7B929" box="[1062,1141,1546,1572]" country="Ukraine" county="Rovno Region" municipality="Vladimirets District" name="Klesov" pageId="4" pageNumber="598" stateProvince="Rivne">Klesov</location>
(Pugach quarry), 
<collectingCountry id="9FCCEFE5FFA0A30FFAE7F9C3FA3EB929" box="[1346,1436,1546,1572]" name="Ukraine" pageId="4" pageNumber="598">Ukraine</collectingCountry>
(
<collectionCode id="81CA37B0FFA0A30FFF3AF9E7FF41B945" box="[159,227,1582,1608]" country="Ukraine" httpUri="http://grbio.org/cool/c442-9tdv" name="Schmaulhausen Institute of Zoology" pageId="4" pageNumber="598">SIZK</collectionCode>
); “ 
<taxonomicName id="20DBD4F6FFA0A30FFEA6F9E7FE71B944" authority="Rovno" authorityName="Rovno" box="[259,467,1582,1609]" class="Insecta" family="Staphylinidae" genus="Baltostigus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="598" phylum="Arthropoda" rank="genus">
