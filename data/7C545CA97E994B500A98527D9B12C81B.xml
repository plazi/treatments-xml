<taxonomicName id="5C288AFC59B13751459A8734283A8690" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31539" class="Insecta" family="Formicidae" genus="Monomorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Monomorium destructor (Jerdon)" order="Hymenoptera" pageNumber="134" phylum="Arthropoda" rank="species" species="destructor">M. destructor</taxonomicName>
