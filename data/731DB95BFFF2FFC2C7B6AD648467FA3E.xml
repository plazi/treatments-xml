<paragraph id="FB0B084DFFF2FFC2C6D7AC798467FA3E" blockId="6.[213,1169,1369,1489]" pageId="6" pageNumber="917">
<taxonomicName id="3CB473CEFFF2FFC2C6D7AC79878CFA9D" baseAuthorityName="Grant &amp; McCulloch" baseAuthorityYear="1906" box="[213,390,1370,1394]" class="Malacostraca" family="Paguridae" genus="Pagurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="917" phylum="Arthropoda" rank="species" species="hedleyi">
