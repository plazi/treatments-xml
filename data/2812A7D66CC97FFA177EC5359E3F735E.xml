<paragraph id="45BAEC6986FD87B8C5B66D06FE720E82" pageId="2" pageNumber="3">
<taxonomicName id="001EFCF374B3E94234103E03A988097D" class="Diplopoda" family="Paradoxosomatidae" genus="Boreohesperus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Boreohesperus capensis" order="Polydesmida" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="capensis">Boreohesperus capensis</taxonomicName>
