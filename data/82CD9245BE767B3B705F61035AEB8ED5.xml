<taxonomicName id="5C7AFAA75797C79DCD9C0178BBB3D77E" class="Gastropoda" family="Dendronotidae" genus="Dendronotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dendronotus kamchaticus" order="Nudibranchia" pageId="8" pageNumber="27" phylum="Mollusca" rank="species" species="kamchaticus">Dendronotus kamchaticus</taxonomicName>
