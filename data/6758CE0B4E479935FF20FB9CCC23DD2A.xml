<taxonomicName id="28F1049E4E479935FBE7FA95CC3BDD2A" box="[1104,1289,1331,1356]" class="Insecta" family="Calliphoridae" genus="Phormia" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="regina">
