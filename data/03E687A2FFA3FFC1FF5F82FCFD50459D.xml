; AW= 
<quantity id="4CB79B51FFA0FFC0FF1285DEFEBC4671" box="[218,307,510,537]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="30" pageNumber="481" unit="mm" value="0.8">0.8 mm</quantity>
