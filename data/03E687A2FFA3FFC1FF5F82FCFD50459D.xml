<materialsCitation id="3B273CE9FFA1FFC1FDC48597FCBB45B9" ID-GBIF-Occurrence="3041480405" box="[524,820,439,465]" country="Brazil" location="Alagoas" pageId="31" pageNumber="482" specimenCount="1" stateProvince="Alagoas">
