, HF = 
<number id="3DFEAC6CFF97FFAB7E3EC15CFC092B3A" box="[847,919,260,284]" pageId="13" pageNumber="14" value="21.0" valueMax="23.0" valueMin="19.0">19–23</number>
, E = 
<number id="3DFEAC6CFF97FFAB7EA0C15CFB842B3A" box="[977,1050,260,284]" pageId="13" pageNumber="14" value="19.0" valueMax="20.0" valueMin="18.0">18–20</number>
