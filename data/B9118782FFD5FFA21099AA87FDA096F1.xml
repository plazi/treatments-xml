(
<emphasis id="03CCEA86FFD7FFA211C3AE8EFDFE97C9" bold="true" box="[461,595,1182,1206]" pageId="31" pageNumber="32">new record</emphasis>
), 
<collectingCountry id="49AF7604FFD7FFA21262AE8EFD6C97C9" box="[620,705,1182,1206]" name="Finland" pageId="31" pageNumber="32">Finland</collectingCountry>
(
<emphasis id="03CCEA86FFD7FFA212DDAE8EFCF797C9" bold="true" box="[723,858,1182,1206]" pageId="31" pageNumber="32">new record</emphasis>
