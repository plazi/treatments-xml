<taxonomicName id="DCF8599862BF0023542CC684D6C07028" baseAuthorityName="Mousson" baseAuthorityYear="1869" class="Gastropoda" family="Bothriembryontidae" genus="Placostylus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="22" pageNumber="23" phylum="Mollusca" rank="subSpecies" species="fibratus" subSpecies="ouveanus">
