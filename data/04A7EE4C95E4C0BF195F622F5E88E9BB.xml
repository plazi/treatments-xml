37. 
<taxonomicName id="8023094719D3ABD93F8C52F96B3CD19D" LSID="urn:lsid:zoobank.org:act:F0B9CC05-E2E0-4B7F-8A82-8246B16F69FE" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus imitatus" order="Coleoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="imitatus">Trigonopterus imitatus Riedel</taxonomicName>
<taxonomicNameLabel id="C953878CE6BEB8B811D28D1792D47485" pageId="33" pageNumber="34">sp. n.</taxonomicNameLabel>
