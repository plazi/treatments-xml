<taxonomicName id="4C180ABDFF9E2310684E23BED60A64A7" box="[324,539,476,500]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="38" phylum="Arthropoda" rank="species" species="minutulus">
