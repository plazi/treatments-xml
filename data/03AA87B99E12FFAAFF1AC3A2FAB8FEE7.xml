<taxonomicName id="4C034D2C9E12FFA6FD72C2A4FB83F9D6" authority="Westwood, 1874" authorityName="Westwood" authorityYear="1874" box="[767,1087,1968,1993]" class="Arachnida" family="Stylocellidae" genus="Stylocellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="23" pageNumber="324" phylum="Arthropoda" rank="genus">
<emphasis id="B977EABD9E12FFA6FD72C2A4FCCBF9D6" box="[767,887,1968,1993]" italics="true" pageId="23" pageNumber="324">Stylocellus</emphasis>
