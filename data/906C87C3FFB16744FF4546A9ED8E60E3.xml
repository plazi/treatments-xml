<taxonomicName id="DFC54D56FFB26745FF184063EF5E64C1" box="[232,333,1400,1421]" class="Crustacea (awaiting allocation)" family="Crustacea (awaiting allocation)" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea (awaiting allocation)" pageId="82" pageNumber="351" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
