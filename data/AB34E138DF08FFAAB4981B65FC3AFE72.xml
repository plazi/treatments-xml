<emphasis id="11E98C3CDF09FFAAB4C81E86FE85FF44" bold="true" box="[199,367,186,209]" pageId="3" pageNumber="300">
<typeStatus id="FC26EE8CDF09FFAAB4C81E86FF15FF44" box="[199,255,186,209]" pageId="3" pageNumber="300">Type</typeStatus>
material.
</emphasis>
<typeStatus id="FC26EE8CDF09FFAAB5771E86FE31FF44" box="[376,475,186,209]" pageId="3" pageNumber="300">Holotype</typeStatus>
<emphasis id="11E98C3CDF09FFAAB5EA1E85FE13FF44" box="[485,505,185,209]" italics="true" pageId="3" pageNumber="300">♂</emphasis>
, 
<emphasis id="11E98C3CDF09FFAAB6061E86FD11FF44" bold="true" box="[521,763,186,209]" pageId="3" pageNumber="300">
<collectingCountry id="5B8A10BEDF09FFAAB6061E86FDB8FF44" box="[521,594,186,209]" name="China" pageId="3" pageNumber="300">China</collectingCountry>
: Hainan Prov.
</emphasis>
