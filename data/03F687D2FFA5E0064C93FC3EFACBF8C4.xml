<quantity id="4CA79B21FFA4E0064E6EF918FD3DF91C" box="[618,759,1673,1699]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" metricValueMax="1.2" metricValueMin="0.9" pageId="5" pageNumber="308" unit="mm" value="1.05" valueMax="1.2" valueMin="0.9">0.9–1.2 mm</quantity>
