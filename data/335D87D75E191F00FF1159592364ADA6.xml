<taxonomicName id="7CF44D425E1A1F00FF11583B27ABAC0D" box="[151,253,1571,1594]" class="Arachnida" family="Salticidae" genus="Cucudeta" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">Cucudeta</taxonomicName>
