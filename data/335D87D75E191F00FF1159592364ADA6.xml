<taxonomicName id="7CF44D425E1A1F00FB155BA323ADAFE5" box="[1171,1275,1467,1490]" class="Arachnida" family="Salticidae" genus="Cucudeta" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">Cucudeta</taxonomicName>
