<taxonomicName id="01E691AAFFCF876CFED7C991FDBEF4D8" baseAuthorityName="Jeannel" baseAuthorityYear="1936" box="[309,617,1701,1727]" class="Insecta" family="Leiodidae" genus="Parapaulipalpina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="filicornis">
