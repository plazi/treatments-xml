<taxonomicName id="4C13DDB7D878964C7C36FF21FE46FF78" authorityName="Nielson" authorityYear="1982" box="[344,455,224,245]" class="Insecta" family="Cicadellidae" genus="Taharana" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="11" phylum="Arthropoda" rank="genus">
<emphasis id="B9677A26D878964C7C36FF21FE46FF78" box="[344,455,224,245]" italics="true" pageId="12" pageNumber="11">Taharana</emphasis>
