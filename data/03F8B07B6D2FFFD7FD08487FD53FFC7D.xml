<taxonomicName id="4C517AEE6D2FFFD4FD084805D744F9F9" authorityName="Kuwert" authorityYear="1890" box="[707,850,1628,1649]" class="Insecta" family="Hydrophilidae" genus="Berosus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="267" phylum="Arthropoda" rank="species" species="rubiginosus">
