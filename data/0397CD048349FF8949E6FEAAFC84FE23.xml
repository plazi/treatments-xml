<taxonomicName id="4C3E07918349FF8949CFFE5BFEECFE23" authorityName="Smith &amp; Beaver &amp; Cognato" authorityYear="2018" baseAuthorityName="Nobuchi" baseAuthorityYear="1981" box="[190,326,469,494]" class="Insecta" family="Curculionidae" genus="Cyclorhipidion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="395" phylum="Arthropoda" rank="species" species="bispinum">
<emphasis id="B94AA0008349FF8949CFFE5BFEECFE23" box="[190,326,469,494]" italics="true" pageId="4" pageNumber="395">C. bispinum</emphasis>
</taxonomicName>
