<geoCoordinate id="A39BA156608D514906DF29DE86ADFED8" degrees="25.385" direction="south" orientation="latitude" precision="55" value="-25.385">25.385°S</geoCoordinate>
, 
<geoCoordinate id="A93BD4C86C34FBF986FFED1FDA8867C1" degrees="57.621" direction="west" orientation="longitude" precision="55" value="-57.621">57.621°W</geoCoordinate>
