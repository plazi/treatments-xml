<taxonomicName id="7BDAFB237E478335E658F150DFDD2DD0" baseAuthorityName="Guppy" baseAuthorityYear="1866" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="163" pageNumber="164" phylum="Mollusca" rank="species" species="imperfectus" subGenus="Mesembrinus">
