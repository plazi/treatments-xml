(
<emphasis id="BB8257476E1A3F69FDE75AF4FD15515B" box="[593,694,504,526]" italics="true" pageId="20" pageNumber="221">Maculedo</emphasis>
) 
<emphasis id="BB8257476E1A3F69FD735AF5FCA4515B" box="[709,775,505,526]" italics="true" pageId="20" pageNumber="221">vestita</emphasis>
André, 
<number id="3F3E99726E1A3F69FCEF5AF4FC285158" box="[857,907,504,525]" pageId="20" pageNumber="221" value="1881.0">1881</number>
</taxonomicName>
