<taxonomicName id="4C354D1DFFB0217AFEDF8849FCA7768E" authority="(Millar, 1962)" authorityName="Millar" authorityYear="1962" baseAuthorityName="Millar" baseAuthorityYear="1962" box="[276,787,1894,1921]" class="Ascidiacea" family="Didemnidae" genus="Polysyncraton" kingdom="Animalia" order="Enterogona" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="magnilarvum">
