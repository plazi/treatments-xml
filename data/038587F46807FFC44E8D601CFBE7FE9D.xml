<taxonomicName id="4C2C4D616807FFC54D876057FAEAF811" authority="Van Duzee, 1929" authorityName="Van Duzee" authorityYear="1929" box="[925,1303,1912,1937]" class="Insecta" family="Pentatomidae" genus="Kyrtalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="546" phylum="Arthropoda" rank="species" species="mackiei">
