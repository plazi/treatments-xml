<taxonomicName id="4C1CC38A665FFFA04B7EFE7DD3D5978B" authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[326,433,420,444]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="89" pageNumber="901" phylum="Arthropoda" rank="species" species="tejaswii" status="sp. n.">
