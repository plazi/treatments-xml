<taxonomicName id="17FA4D65FFA6FFB11000FB5BFA43B2A5" box="[1288,1433,1205,1231]" class="Actinopterygii" kingdom="Animalia" order="Gadiformes" pageId="39" pageNumber="40" phylum="Chordata" rank="order">Gadiformes</taxonomicName>
