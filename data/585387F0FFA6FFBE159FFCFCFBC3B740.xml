<taxonomicName id="17FA4D65FFA6FFB1161AFAC3FC57B32C" box="[786,909,1325,1350]" class="Actinopterygii" family="Pleuronectidae" genus="Atheresthes" kingdom="Animalia" order="Pleuronectiformes" pageId="39" pageNumber="40" phylum="Chordata" rank="species" species="stomias">
