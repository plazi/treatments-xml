<paragraph id="8BB736F1FF9CFFF906AADF44FDD9F9B1" blockId="5.[151,630,1585,1612]" box="[151,630,1585,1612]" pageId="5" pageNumber="301">
<heading id="D0FF819DFF9CFFF906AADF44FDD9F9B1" bold="true" box="[151,630,1585,1612]" fontSize="11" level="1" pageId="5" pageNumber="301" reason="1">
<emphasis id="B97CEAE3FF9CFFF906AADF44FDD9F9B1" bold="true" box="[151,630,1585,1612]" pageId="5" pageNumber="301">
Species group 
<taxonomicName id="4C084D72FF9CFFF90771DF47FDD9F9B1" authority="(Haan, 1843)" baseAuthorityName="Haan" baseAuthorityYear="1843" box="[332,630,1585,1612]" class="Insecta" family="Baissogryllidae" genus="Locusta" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="301" phylum="Arthropoda" rank="species" species="niponensis">
<emphasis id="B97CEAE3FF9CFFF90771DF47FE61F9B6" bold="true" box="[332,462,1586,1612]" italics="true" pageId="5" pageNumber="301">niponensis</emphasis>
(Haan, 1843)
</taxonomicName>
</emphasis>
</heading>
</paragraph>
