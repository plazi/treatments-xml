<number id="3DEC24AAA672FFF78FE9C3EEFC9597DA" box="[151,165,1176,1202]" pageId="1" pageNumber="56" value="2.0">2</number>
BL 
<quantity id="4CDC9B68A672FFF78F8DC3EEFDB397DA" box="[243,387,1176,1202]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" metricValueMax="4.3" metricValueMin="3.5" pageId="1" pageNumber="56" unit="mm" value="3.9" valueMax="4.3" valueMin="3.5">3.5–4.3 mm</quantity>
