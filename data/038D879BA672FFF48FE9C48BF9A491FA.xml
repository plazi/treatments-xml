<quantity id="4CDC9B68A672FFF78E19C2AEFD95969A" box="[359,421,1496,1522]" metricMagnitude="0" metricUnit="m" metricValue="1.8034" pageId="1" pageNumber="56" unit="in" value="71.0">71 in</quantity>
