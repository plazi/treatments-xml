<quantity id="4CDC9B68A672FFF78E57C216FDB79612" box="[297,391,1376,1402]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" pageId="1" pageNumber="56" unit="mm" value="9.5">9.5 mm</quantity>
