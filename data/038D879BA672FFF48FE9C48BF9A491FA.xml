<quantity id="4CDC9B68A672FFF78B00C2C6F91996A2" box="[1150,1321,1456,1482]" metricMagnitude="-3" metricUnit="m" metricValue="2.1849999999999996" metricValueMax="2.32" metricValueMin="2.05" pageId="1" pageNumber="56" unit="mm" value="2.1849999999999996" valueMax="2.32" valueMin="2.05">2.05–2.32 mm</quantity>
