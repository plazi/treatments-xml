<quantity id="F6769B70FFE848295AE5FA9CFB79A664" box="[1214,1271,1371,1395]" metricMagnitude="1" metricUnit="m" metricValue="1.3" pageId="5" pageNumber="149" unit="m" value="13.0">13 m</quantity>
