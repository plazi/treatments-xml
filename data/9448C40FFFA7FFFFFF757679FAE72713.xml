<taxonomicName id="DBE10E9AFFA0FFFFFF0B765AFE6E2450" authority="Turcz." authorityName="Turcz." box="[190,480,354,378]" class="Magnoliopsida" family="Asteraceae" genus="Symphyopappus" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="3" phylum="Tracheophyta" rank="genus">
<emphasis id="2E95A90BFFA0FFFFFF0B765AFE062453" box="[190,392,354,378]" italics="true" pageId="4" pageNumber="3">Symphyopappus</emphasis>
Turcz.
</taxonomicName>
(
<taxonomicName id="DBE10E9AFFA0FFFFFE45765AFD052453" box="[496,651,354,378]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="3" phylum="Tracheophyta" rank="tribe" tribe="Eupatorieae">Eupatorieae</taxonomicName>
