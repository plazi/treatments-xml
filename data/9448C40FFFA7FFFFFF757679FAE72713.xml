<taxonomicName id="DBE10E9AFFA7FFF8FE00705AFB572253" authority="Pineda y Barros, HOLOTIPO" authorityName="Pineda y Barros, HOLOTIPO" authorityYear="2017" box="[437,1241,1890,1914]" class="Insecta" family="Buprestidae" genus="Conognatha" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="caparaoensis" subGenus="Pithiscus">
