<collectingCountry id="64F63589FFA7FFF8FD7E707AFCBB2273" box="[715,821,1858,1882]" name="Brazil" pageId="3" pageNumber="2">BRASIL</collectingCountry>
, 2400 msnm, 
<date id="685F53D9FFA7FFF8FC6B7079FBE82270" box="[990,1126,1857,1881]" pageId="3" pageNumber="2" value="2015-11-28">28.XI.2015</date>
