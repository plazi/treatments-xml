<subSubSection id="D48C652EFFDFFF99FF5B9E5DFF66FAC0" box="[194,250,1345,1383]" pageId="19" pageNumber="324" type="multiple">25.</subSubSection>
<subSubSection id="D48C652EFFDFFF99FE909E5DFD72FAC0" box="[265,750,1345,1383]" pageId="19" pageNumber="324" type="vernacular_name">Alexander’s Cusimanse</subSubSection>
<subSubSection id="D48C652EFFDFFF99FC9B9E5DFB00FAC0" box="[770,1180,1345,1383]" pageId="19" pageNumber="324" type="nomenclature">
