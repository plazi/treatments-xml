<paragraph id="595E3815423CB0EF047EBBE659F3DBF8" pageId="59" pageNumber="60">
<taxonomicName id="9834D0D131F6B8398E0193C831801BD9" class="Insecta" family="Histeridae" genus="Saprinodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Saprinodes" order="Coleoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="genus">
