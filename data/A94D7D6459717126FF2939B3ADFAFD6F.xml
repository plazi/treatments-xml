<taxonomicName id="E6E4B7F159707126FF713D99A803FE6C" box="[207,339,440,463]" class="Insecta" family="Sphaeroceridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="306" phylum="Arthropoda" rank="order">Heteroptera</taxonomicName>
