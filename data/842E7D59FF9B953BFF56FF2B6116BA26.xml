<collectingCountry id="74908CDFFF9B953BFD34FCB76667BA27" box="[757,863,834,860]" name="Morocco" pageId="4" pageNumber="133">Morocco</collectingCountry>
, 
<collectingCountry id="74908CDFFF9B953BFCA9FCB76687BA27" box="[872,959,834,860]" name="Algeria" pageId="4" pageNumber="133">Algeria</collectingCountry>
, 
<collectingCountry id="74908CDFFF9B953BFC0BFCB7611ABA26" box="[970,1058,834,861]" name="Tunisia" pageId="4" pageNumber="133">Tunisia</collectingCountry>
).
</paragraph>
