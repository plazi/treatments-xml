<taxonomicName id="4D1B08CD50822DF4BDB25C907E4986AF" class="Insecta" family="Eulophidae" genus="Ichneumon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ichneumon" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Ichneumon</taxonomicName>
