<taxonomicName id="4C404D53FFE3FFC8FBFBA5D7CF5163B6" authority="Brunnich" authorityName="Brunnich" class="Aves" family="Scolopacidae" genus="Lymnocryptes" kingdom="Animalia" order="Charadriiformes" pageId="2" pageNumber="62" phylum="Chordata" rank="species" species="minimus">
