<quantity id="4C8F1AF8DA361C0DFC20FC22FC7AFC17" box="[900,982,960,984]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="2" pageNumber="73" unit="m" value="2500.0">2500 m</quantity>
