<taxonomicName id="4C784D24FFADFFC2FED0FB9024FDF110" authority="Budde-Lund, 1908" authorityName="Budde-Lund" authorityYear="1908" box="[351,681,1057,1081]" class="Malacostraca" family="Philosciidae" genus="Aphiloscia" kingdom="Animalia" order="Isopoda" pageId="12" pageNumber="468" phylum="Arthropoda" rank="genus">
