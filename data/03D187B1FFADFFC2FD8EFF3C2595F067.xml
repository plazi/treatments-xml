<taxonomicName id="4C784D24FFADFFC2FCB3FAA52592F005" authorityName="Cardoso, Campos-Filho &amp; Araujo" authorityYear="2018" box="[828,966,1300,1324]" class="Malacostraca" family="Pudeoniscidae" genus="Brasiloniscus" kingdom="Animalia" order="Isopoda" pageId="12" pageNumber="468" phylum="Arthropoda" rank="genus">
