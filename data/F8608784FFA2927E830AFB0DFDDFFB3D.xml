<taxonomicName id="B7C94D11FFA1927E8184FC22FE4BFC7D" authorityName="H. Adams &amp; A. Adams" authorityYear="1852" box="[421,486,939,965]" class="Gastropoda" family="Iravadiidae" genus="Hyala" kingdom="Animalia" order="Littorinimorpha" pageId="2" pageNumber="239" phylum="Mollusca" rank="genus">
<emphasis id="42BDEA80FFA1927E8184FC22FE4BFC7D" box="[421,486,939,965]" italics="true" pageId="2" pageNumber="239">Hyala</emphasis>
</taxonomicName>
H. &amp; A. Adams, 1852, 
<taxonomicName id="B7C94D11FFA1927E80FFFC43FE81FC5C" authorityName="H. Adams &amp; A. Adams" authorityYear="1852" box="[222,300,970,996]" class="Gastropoda" family="Iravadiidae" genus="Ceratia" kingdom="Animalia" order="Littorinimorpha" pageId="2" pageNumber="239" phylum="Mollusca" rank="genus">
