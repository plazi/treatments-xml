<taxonomicName id="B7C94D11FFA1927E8141FC62FD0AFBBD" authority="Ponder, 1984" authorityName="Ponder" authorityYear="1984" box="[352,679,1003,1029]" class="Gastropoda" family="Iravadiidae" genus="Acliceratia" kingdom="Animalia" order="Littorinimorpha" pageId="2" pageNumber="239" phylum="Mollusca" rank="genus">
