<taxonomicName id="26CA4D50EC74C452FDDD2417FD686727" box="[525,663,301,327]" class="Insecta" family="Formicidae" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="family">Formicidae</taxonomicName>
: 
<taxonomicName id="26CA4D50EC74C452FD722417FC736727" authorityName="M.R.Smith" authorityYear="1952" box="[674,908,301,327]" pageId="33" pageNumber="45" rank="subFamily" subFamily="Pseudomyrmecinae">Pseudomyrmecinae</taxonomicName>
: 
<taxonomicName id="26CA4D50EC74C452FC462417FB8D6727" box="[918,1138,301,327]" class="Insecta" family="Formicidae" genus="Pseudomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="undetermined">
