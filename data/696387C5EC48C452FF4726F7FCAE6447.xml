<taxonomicName id="26CA4D50EC74C452FCFD2432FC626742" authorityName="de Jussieu" authorityYear="1789" box="[813,925,264,290]" class="Liliopsida" family="Liliaceae" kingdom="Plantae" order="Liliales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="family">Liliaceae</taxonomicName>
: 
<taxonomicName id="26CA4D50EC74C452FC782432FB7D6741" box="[936,1154,264,289]" class="Liliopsida" family="Asteliaceae" genus="Astelia" kingdom="Plantae" order="Asparagales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="pterocarpa">
<emphasis id="D3BEEAC1EC74C452FC782432FB7D6741" box="[936,1154,264,289]" italics="true" pageId="33" pageNumber="34">Astelia pterocarpa</emphasis>
