<collectingCountry id="F33A3A9DA46CC94297782AA1FE5DFA2B" box="[358,426,1508,1534]" name="Brazil" pageId="30" pageNumber="31">Brazil</collectingCountry>
(
<collectingRegion id="49E9B4EFA46CC94297A62AA1FDC5FA2B" box="[440,562,1508,1534]" country="Venezuela" name="Amazonas" pageId="30" pageNumber="31">Amazonas</collectingRegion>
