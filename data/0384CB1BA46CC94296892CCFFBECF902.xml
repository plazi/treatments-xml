); 
<collectingCountry id="F33A3A9DA46CC94294B32AA1FCD3FA2B" box="[685,804,1508,1534]" name="Venezuela" pageId="30" pageNumber="31">Venezuela</collectingCountry>
. 
<taxonomicName id="4C2D018EA46CC94295322AA1FC23FA2B" authorityName=", Menezes &amp; Ale-Rocha" authorityYear="2016" box="[812,980,1508,1534]" class="Insecta" family="Hybotidae" genus="Syneches" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="exilis">
