designate the unique known specimen of 
<taxonomicName id="4C37F8EDFFA0FFEFDF8F25A30A1891BD" box="[706,832,152,175]" class="Insecta" family="Baetidae" genus="Callibaetis" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="503" phylum="Arthropoda" rank="species" species="camposi">
<emphasis id="B9435F7CFFA0FFEFDF8F25A30A1891BD" box="[706,832,152,175]" italics="true" pageId="3" pageNumber="503">C. camposi</emphasis>
</taxonomicName>
as the 
<typeStatus id="548C3DCCFFA0FFEFDEC325AC0AA191A2" box="[910,1017,151,176]" pageId="3" pageNumber="503">lectotype</typeStatus>
. 
<emphasis id="B9435F7CFFA0FFEFDD8A258708E991C7" bold="true" box="[199,433,188,213]" pageId="3" pageNumber="503">
<location id="8EE8D5B5FFA0FFEFDD8A2587087791C7" LSID="urn:lsid:plazi:treatment:039E3278FFA2FFEFDDDA21F50B17900F:8EE8D5B5FFA0FFEFDD8A2587087791C7" box="[199,303,188,213]" country="Ecuador" county="Campos" municipality="Guayaquil" name="Material" pageId="3" pageNumber="503">Material</location>
examined.
</emphasis>
<taxonomicName id="4C37F8EDFFA0FFEFDCF425870BC191C6" box="[441,665,188,212]" class="Insecta" family="Baetidae" genus="Callibaetis" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="503" phylum="Arthropoda" rank="species" species="camposi">
<emphasis id="B9435F7CFFA0FFEFDCF425870BC191C6" box="[441,665,188,212]" italics="true" pageId="3" pageNumber="503">Callibaetis camposi</emphasis>
</taxonomicName>
, female imago (
<typeStatus id="548C3DCCFFA0FFEFDE1F25870AE291C7" box="[850,954,188,213]" pageId="3" pageNumber="503">lectotype</typeStatus>
