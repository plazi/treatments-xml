<collectingDate id="EFCD5C46FFA0FFEFDC5425E4083B91EA" box="[281,355,223,248]" pageId="3" pageNumber="503" value="1930-01">
<date id="FF89A5AEFFA0FFEFDC5425E4083B91EA" box="[281,355,223,248]" pageId="3" pageNumber="503" value="1930-01">i.1930</date>
</collectingDate>
, 
<collectingCounty id="62E9FBE2FFA0FFEFDC2325DB089291EA" box="[366,458,224,248]" pageId="3" pageNumber="503">Campos</collectingCounty>
leg, n° 75–5308, 
<collectionCode id="ED261BABFFA0FFEFDFC225DB0B9791E5" box="[655,719,224,247]" country="Indonesia" httpUri="http://grbio.org/cool/b9gz-izvg" name="Museum Zoologicum Bogoriense" pageId="3" pageNumber="503">MZB</collectionCode>
