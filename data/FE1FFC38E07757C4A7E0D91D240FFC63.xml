Israel • 1 spcm; Ashqelon; 
<geoCoordinate id="F910B756961B2819584DC77ABBA00AA4" degrees="31.6868" direction="north" orientation="latitude" precision="5" value="31.6868">31.6868°N</geoCoordinate>
, 
<geoCoordinate id="DAA08BD1FCA381CD47CF6ADCA82E9A68" degrees="34.5516" direction="east" orientation="longitude" precision="5" value="34.5516">34.5516°E</geoCoordinate>
