<paragraph id="8BB036D6FFB3FF9AC2EEFEDAFB85FEDD" blockId="92.[151,1059,259,305]" pageId="92" pageNumber="247">
<collectingRegion id="49CBF834FFB3FF9AC2EEFEDAFF7DFEFB" box="[151,235,259,279]" country="Austria" name="Salzburg" pageId="92" pageNumber="247">Salzburg</collectingRegion>
