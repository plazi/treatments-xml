<taxonomicName id="4C547783FFDE6C58FC2EB9E1FB310EF4" authority="Munari, 2004" authorityName="Munari" authorityYear="2004" box="[951,1161,205,228]" class="Insecta" family="Canacidae" genus="Thitena" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="50" phylum="Arthropoda" rank="species" species="cadaverina" status="n.sp.">
