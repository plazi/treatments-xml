Ramos-Porto (
<number id="DC791733FFF9517EFD55FE564EF6C8F2" box="[689,752,472,498]" pageId="10" pageNumber="11" value="1986.0">1986</number>
), 
<bibRefCitation id="0E2078E5FFF9517EFCE1FE564874C8F2" author="Ramos-Porto" box="[773,1138,472,498]" pageId="10" pageNumber="43" refString="Ramos-Porto, M. &amp; Coelho, P. A. (1990) Sinopse dos crustaceos decapodos brasileiros (familia Palaemonidae). Anais da Socidade Nordestina de Zoologia, 3, 93 - 111." type="journal article" year="1990">
Ramos-Porto &amp; Coelho (
<number id="DC791733FFF9517EFBCEFE56486FC8F2" box="[1066,1129,472,498]" pageId="10" pageNumber="11" value="1990.0">1990</number>
)
</bibRefCitation>
, 
<bibRefCitation id="0E2078E5FFF9517EFB9AFE56493FC8F2" author="Cardoso" box="[1150,1337,472,498]" pageId="10" pageNumber="39" refString="Cardoso, I. (2006) Caridea (Crustacea, Decapoda) collected on the Brazilian (13 o / 22 o S) continental shelf and slope. Zootaxa, 1364, 1 - 44." type="journal article" year="2006">
Cardoso (
<number id="DC791733FFF9517EFB0BFE564936C8F2" box="[1263,1328,472,498]" pageId="10" pageNumber="11" value="2006.0">2006</number>
)
</bibRefCitation>
.
</paragraph>
