<subSubSection id="18A76570783A8A50A0917E657747D263" box="[199,1395,330,355]" pageId="20" pageNumber="89" type="diagnosis">
<paragraph id="500236FB783A8A50A0917E657747D263" blockId="20.[151,1437,258,355]" box="[199,1395,330,355]" pageId="20" pageNumber="89">
<emphasis id="62C9EAE9783A8A50A0917E65730ED263" bold="true" box="[199,314,330,355]" pageId="20" pageNumber="89">Diagnosis</emphasis>
