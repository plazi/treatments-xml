<taxonomicName id="4C2D1FC8E362475BFB4563E5F981FE3C" box="[1158,1250,469,493]" class="Arachnida" family="Phytoseiidae" genus="Phytoseius" kingdom="Animalia" order="Mesostigmata" pageId="25" pageNumber="227" phylum="Arthropoda" rank="species" species="horridus">
