<taxonomicName id="4C2D1FC8E362475BFDF363C7FEB9FDDE" authority="(Chant and McMurtry 2007)" baseAuthorityName="Chant and McMurtry" baseAuthorityYear="2007" box="[560,986,503,528]" class="Arachnida" family="Phytoseiidae" genus="Phytoseius" kingdom="Animalia" order="Mesostigmata" pageId="25" pageNumber="227" phylum="Arthropoda" rank="genus">
