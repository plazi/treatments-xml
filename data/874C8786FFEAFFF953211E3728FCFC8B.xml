<geoCoordinate id="6AD15057FFECFFF950C0193228CFFEBB" box="[144,230,335,357]" degrees="51" direction="east" minutes="25" orientation="longitude" pageId="12" pageNumber="545" precision="925" value="51.416668">51°25’E</geoCoordinate>
, (
<collectorName id="A2105346FFECFFF950AB192D29E7FEBB" box="[251,462,335,357]" pageId="12" pageNumber="545">M.S. Abdel-Dayem</collectorName>
leg.), 3w, (
<collectionCode id="69F4AE55FFECFFF9521A192D2AB3FEB8" box="[586,666,336,358]" pageId="12" pageNumber="545">KSMA</collectionCode>
)
