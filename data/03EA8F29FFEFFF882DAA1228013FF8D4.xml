<taxonomicName id="4C4345BCFFEFFF882FD811830023FD1C" box="[741,829,694,719]" class="Insecta" family="Arctiidae" genus="Lyclene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="245" phylum="Arthropoda" rank="species" species="clara">
