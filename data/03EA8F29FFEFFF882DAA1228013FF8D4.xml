<taxonomicName id="4C4345BCFFEFFF88286111820682FD1C" box="[1372,1436,694,719]" class="Insecta" family="Arctiidae" genus="Miltochrista" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="245" phylum="Arthropoda" rank="species" species="tibeta">
