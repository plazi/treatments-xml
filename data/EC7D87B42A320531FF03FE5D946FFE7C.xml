<taxonomicName id="A3D44D212A320530FF03FE5D9625FE33" box="[151,355,429,455]" class="Insecta" family="Cecidomyiidae" genus="Zadbimyia" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="dubia" status="sp. n.">
<emphasis id="56A0EAB02A320530FF03FE5D9625FE33" bold="true" box="[151,355,429,455]" italics="true" pageId="26" pageNumber="27">Zadbimyia dubia</emphasis>
