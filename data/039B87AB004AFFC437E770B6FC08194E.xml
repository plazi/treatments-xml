<taxonomicName id="4C324D3E0048FFC435357776FD4D1851" box="[581,716,584,608]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="anfracta">
<emphasis id="B946EAAF0048FFC435357776FD4D1851" box="[581,716,584,608]" italics="true" pageId="19" pageNumber="120">M. anfracta</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C324D3E0048FFC435AD7776FCDA1851" box="[733,859,585,608]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="spinosa">
<emphasis id="B946EAAF0048FFC435AD7776FCDA1851" box="[733,859,585,608]" italics="true" pageId="19" pageNumber="120">M. spinosa</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C324D3E0048FFC4341C7776FC7F1851" box="[876,1022,584,608]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="bihamata">
<emphasis id="B946EAAF0048FFC4341C7776FC7F1851" box="[876,1022,584,608]" italics="true" pageId="19" pageNumber="120">M. bihamata</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C324D3E0048FFC4337F7776FB151851" box="[1039,1172,584,608]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="caribica">
<emphasis id="B946EAAF0048FFC4337F7776FB151851" box="[1039,1172,584,608]" italics="true" pageId="19" pageNumber="120">M. caribica</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C324D3E0048FFC433D57776FF5F18B5" authority="Jaschhof &amp; Hippa" authorityName="Jaschhof &amp; Hippa" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="incisa">
<emphasis id="B946EAAF0048FFC433D57776FA911851" box="[1189,1296,585,608]" italics="true" pageId="19" pageNumber="120">M. incisa</emphasis>
Jaschhof &amp; Hippa
</taxonomicName>
, 
<taxonomicName id="4C324D3E0048FFC437997752FE0718B5" box="[233,390,620,644]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="120" phylum="Arthropoda" rank="species" species="penicillata">
