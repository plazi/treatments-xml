<location id="8EDAF9B8213B977202D3B88AFD96FD72" LSID="urn:lsid:plazi:treatment:03AC1E75213B977203D8BBF4FB58FD82:8EDAF9B8213B977202D3B88AFD96FD72" box="[388,576,680,704]" country="Brazil" name="Musee de Zoologie" pageId="15" pageNumber="844" stateProvince="Sao Paulo">
