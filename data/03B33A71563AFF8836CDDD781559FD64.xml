(mm, 
<collectionCode id="ED0B13A2563AFF8936B5DEB315A3FD65" box="[436,454,696,720]" country="Netherlands" lsid="urn:lsid:biocol.org:col:15678" name="Nationaal Herbarium Nederland, Leiden University branch" pageId="14" pageNumber="3188" type="Herbarium">L</collectionCode>
