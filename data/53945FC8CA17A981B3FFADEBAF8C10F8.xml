<paragraph id="C3DCBE2F3C94756CB55DB42FF1C6B2A2" pageId="0" pageNumber="125">
<taxonomicName id="BCD21D20D2BF0B44E0F4B36D4FDFC618" class="Actinopterygii" family="Haemulidae" genus="Haemulon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haemulon plumieri" order="Perciformes" pageId="0" pageNumber="125" phylum="Chordata" rank="species" species="plumieri">Haemulon plumieri</taxonomicName>
