<taxonomicName id="79A85EF1FFEEB613B571F9F0FD4BF94A" authorityName="Leech" authorityYear="1890" box="[532,681,1658,1681]" class="Insecta" family="Nymphalidae" genus="Faunis" kingdom="Animalia" order="Lepidoptera" pageId="20" pageNumber="506" phylum="Arthropoda" rank="species" species="aerope">
