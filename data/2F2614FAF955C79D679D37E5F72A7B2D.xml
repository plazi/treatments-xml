</subSubSection>
<subSubSection id="05D28F7D9F309B4EE9F596403F3B761D" pageId="0" pageNumber="1" type="reference_group">
<paragraph id="36163D262AB1CF46C3204EDBA52C946F" pageId="0" pageNumber="1">
<taxonomicName id="FF1CE9B6E3381F1E87D0E3957BA32267" authorityName="Ferrari" authorityYear="1867" class="Insecta" family="Curculionidae" genus="Anisandrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anisandrus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Anisandrus</taxonomicName>
