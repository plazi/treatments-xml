<taxonomicName id="4C2901BE047C152FFB404D01FAC6FCB6" authorityName="Deshayes" authorityYear="1863" box="[1123,1325,808,834]" class="Gastropoda" family="Triphoridae" genus="Triphoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ptenoglossa" pageId="117" pageNumber="118" phylum="Mollusca" rank="species" species="mirifica">
<emphasis id="B95DA62F047C152FFB404D01FAC6FCB6" box="[1123,1325,808,834]" italics="true" pageId="117" pageNumber="118">Triphoris mirifica</emphasis>
</taxonomicName>
. 
