<taxonomicName id="4C02EBFAFFA0F656FEC4F9797E47F901" baseAuthorityName="Brook" baseAuthorityYear="1893" box="[292,529,1704,1723]" class="Anthozoa" family="Acroporidae" genus="Isopora" kingdom="Animalia" order="Scleractinia" pageId="79" pageNumber="756" phylum="Cnidaria" rank="species" species="brueggemanni">
