<taxonomicName id="1B910F80EA84C7D4E723BBB362649243" authority="" authorityName="" class="Insecta" family="Coccinellidae" genus="Coccinelloidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Coccinelloidea" order="Hemiptera" pageId="2" pageNumber="39" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
