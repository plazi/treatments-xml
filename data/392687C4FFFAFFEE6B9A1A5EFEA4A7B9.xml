<taxonomicName id="768F4D51FFFAFFEE6AA5184EFD37A761" box="[424,669,1846,1870]" class="Insecta" family="Nitidulidae" genus="Brachypeplus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="109" phylum="Arthropoda" rank="species" species="glaber">
