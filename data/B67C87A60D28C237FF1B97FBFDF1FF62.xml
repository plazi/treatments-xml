<number id="881D24970D28C236FB329539FB54FDDA" box="[1214,1259,597,623]" pageId="12" pageNumber="13" value="121.0">121</number>
° 
<number id="881D24970D28C236FB799539FAA9FDDA" box="[1269,1302,597,623]" pageId="12" pageNumber="13" value="22.0">22</number>
