spec. det. C.A. Schulz, 
<number id="881D24970D28C236FCD39516FC22FD21" box="[863,925,634,660]" pageId="12" pageNumber="13" value="2007.0">2007</number>
] (
<collectionCode id="58C4AE750D28C236FC3F9516FBAFFD21" LSID="urn:lsid:biocol.org:col:34975" box="[947,1040,634,660]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34975" name="Instituut voor Taxonomische Zoologie, Zoologisch Museum" pageId="12" pageNumber="13">ZMAN</collectionCode>
