, 
<collectingRegion id="49BEF80DFFF6FF9DFF43FD4CD01319B2" box="[212,299,649,674]" country="Mexico" name="Mexico" pageId="14" pageNumber="345">
<collectingCountry id="F36D767FFFF6FF9DFF43FD4CD01319B2" box="[212,299,649,674]" name="Mexico" pageId="14" pageNumber="345">Mexico</collectingCountry>
</collectingRegion>
