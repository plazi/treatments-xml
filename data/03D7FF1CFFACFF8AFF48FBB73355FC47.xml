<taxonomicName id="4C7E3589FFACFF8AFB03F86534DCFCD3" authorityName="Laporte" authorityYear="1840" box="[1163,1440,779,803]" class="Insecta" family="Scarabaeidae" genus="Oxysternon" kingdom="Animalia" order="Coleoptera" pageId="103" pageNumber="102" phylum="Arthropoda" rank="species" species="spiniferum">
