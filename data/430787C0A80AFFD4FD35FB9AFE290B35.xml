<collectorName id="665B5300A80AFFD5FC8BF979FCC30FDB" box="[798,894,1708,1729]" pageId="18" pageNumber="18">January</collectorName>
</materialsCitation>
; 
<materialsCitation id="7BC63C8BA80AFFD5FC18F979FCA50FC5" collectionCode="AMNH" pageId="18" pageNumber="18" specimenCode="AMNH 518573" specimenCount="1" typeStatus="holotype">
<emphasis id="F9DAEAC4A80AFFD5FC18F979FBF90FDB" bold="true" box="[909,1092,1708,1729]" pageId="18" pageNumber="18">
