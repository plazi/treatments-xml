<collectionCode id="ADBFAE13A80BFFD4FE05FE9EFE50087A" box="[400,493,331,352]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="19" pageNumber="19">AMNH</collectionCode>
