<specimenCode id="9B089EADA80AFFD5FC71F95AFB1F0FBE" box="[996,1186,1679,1700]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="18" pageNumber="18">AMNH 518572</specimenCode>
</emphasis>
