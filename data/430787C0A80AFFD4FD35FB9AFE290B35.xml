<location id="CE71600DA80BFFD4FF15FE14FF4708CD" LSID="urn:lsid:plazi:treatment:430787C0A80AFFD4FD35FB9AFE290B35:CE71600DA80BFFD4FF15FE14FF4708CD" box="[128,250,449,471]" county="Kny-Scheerer Co." municipality="City" name="According" pageId="19" pageNumber="19" stateProvince="Valle">According</location>
