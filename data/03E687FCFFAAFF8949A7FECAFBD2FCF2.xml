<quantity id="4CB79B0FFFAAFF8949FEFE47FDD0FE0D" box="[506,622,420,444]" metricMagnitude="-2" metricUnit="m" metricValue="2.327" pageId="10" pageNumber="875" unit="mm" value="23.27">23.27 mm</quantity>
