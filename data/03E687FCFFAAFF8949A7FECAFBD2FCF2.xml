<quantity id="4CB79B0FFFAAFF8948C4FE47FEF2FE0D" box="[192,332,419,444]" metricMagnitude="-2" metricUnit="m" metricValue="2.325" metricValueMax="2.55" metricValueMin="2.1" pageId="10" pageNumber="875" unit="mm" value="23.25" valueMax="25.5" valueMin="21.0">
