18.5 
<emphasis id="B93BEAF8FFAAFF8949E2FE62FE4DFE28" box="[486,499,385,409]" italics="true" pageId="10" pageNumber="875">–</emphasis>
21 mm
</quantity>
[mean (
<emphasis id="B93BEAF8FFAAFF894AB6FE62FD7EFE28" box="[690,704,385,409]" italics="true" pageId="10" pageNumber="875">n</emphasis>
= 6) 
<quantity id="4CB79B0FFFAAFF894B0AFE62FC38FE28" box="[782,902,385,409]" metricMagnitude="-2" metricUnit="m" metricValue="1.9899999999999998" pageId="10" pageNumber="875" unit="mm" value="19.9">19.90 mm</quantity>
