<taxonomicName id="4C764D0DFFAA1D26FED5FA81FDB74AE1" authority="(Olivier, 1811)" baseAuthorityName="Olivier" baseAuthorityYear="1811" box="[381,623,1405,1422]" class="Insecta" family="Scarabaeidae" genus="Bubas" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="772" phylum="Arthropoda" rank="species" species="bubalus">
