<quantity id="4CAD9B58904C6A1E548AC881D66287EA" box="[236,337,1761,1785]" metricMagnitude="-4" metricUnit="m" metricValue="1.6" pageId="4" unit="mm" value="0.16">0.16 mm</quantity>
