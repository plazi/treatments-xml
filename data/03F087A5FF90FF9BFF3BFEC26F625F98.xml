<materialsCitation id="3B313CEEFF90FF9BFE1DFA626F6E5F98" ID-GBIF-Occurrence="3332264301" collectingDate="10 diciembre del 2016" collectingMethod="en trampa UV" collectorName="G. Lankin" country="Chile" county="Provincia de Elqui" location="Pan de Azucar" pageId="3" pageNumber="2" specimenCount="1" specimenCount-male="1" stateProvince="Region de Coquimbo" typeStatus="holotype">
