<subSubSection id="C312CE85432DC158CCD11C5DB2FAFDEA" box="[197,1436,538,564]" pageId="11" pageNumber="12" type="etymology">
<paragraph id="8BB79D0E432DC158CCD11C5DB2FAFDEA" blockId="11.[151,1437,152,758]" box="[197,1436,538,564]" pageId="11" pageNumber="12">
<emphasis id="B97C411C432DC158CCD11C5DB637FDEA" bold="true" box="[197,337,538,564]" pageId="11" pageNumber="12">Etymology.</emphasis>
