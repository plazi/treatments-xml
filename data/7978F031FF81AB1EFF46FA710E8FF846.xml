<quantity id="3629ECC2FF81AB1EFCDBF9E60D17F9B6" box="[778,835,1545,1569]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="10" pageNumber="51" unit="m" value="10.0">10 m</quantity>
