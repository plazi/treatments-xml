<taxonomicName id="36D13AA4FF81AB1EFC3DF9E60A82F9B6" box="[1004,1238,1545,1569]" class="Anthozoa" family="Cerianthidae" genus="Cerianthus" kingdom="Animalia" order="Ceriantharia" pageId="10" pageNumber="51" phylum="Cnidaria" rank="species" species="filiformis">
<emphasis id="C3A59D35FF81AB1EFC3DF9E60A82F9B6" box="[1004,1238,1545,1569]" italics="true" pageId="10" pageNumber="51">Cerianthus filiformis</emphasis>
</taxonomicName>
, 
<specimenCount id="E7D78AAEFF81AB1EFB35F9E60B32F9B6" box="[1252,1382,1545,1569]" pageId="10" pageNumber="51" type="generic">
