</taxonomicName>
, and 
<quantity id="A61A9B23FFF2FF8F796FC0FCB81C0F1F" box="[1313,1395,1261,1287]" metricMagnitude="-2" metricUnit="m" metricValue="9.0" pageId="19" pageNumber="20" unit="mm" value="90.0">90 mm</quantity>
SL in 
<taxonomicName id="A6E24D45FFF2FF8F7CFBC109BC260F29" authorityName="Last &amp; Gledhill Csiro" authorityYear="2009" box="[181,329,1304,1329]" class="Actinopterygii" family="Brachionichthyidae" genus="Thymichthys" kingdom="Animalia" order="Lophiiformes" pageId="19" pageNumber="20" phylum="Chordata" rank="genus">
