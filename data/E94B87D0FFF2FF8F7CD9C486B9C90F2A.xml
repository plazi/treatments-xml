</taxonomicName>
, 
<quantity id="A61A9B23FFF2FF8F7FF7C0FCB9640F1F" box="[953,1035,1261,1287]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" pageId="19" pageNumber="20" unit="mm" value="45.0">45 mm</quantity>
