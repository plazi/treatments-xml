<taxonomicName id="A6E24D45FFF2FF8F7F63C0FCBEC50F1E" authorityName="Last &amp; Gledhill Csiro" authorityYear="2009" box="[813,938,1261,1286]" class="Actinopterygii" family="Brachionichthyidae" genus="Pezichthys" kingdom="Animalia" order="Lophiiformes" pageId="19" pageNumber="20" phylum="Chordata" rank="genus">
