<taxonomicName id="4C06E0B6FFBAF878FF3DD352FE93FDC9" authorityName="Jack" authorityYear="1962" box="[251,370,585,611]" class="Arachnida" family="Pterygosomatidae" genus="Pterygosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="caucasica">
