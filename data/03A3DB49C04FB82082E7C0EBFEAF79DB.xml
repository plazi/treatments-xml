<taxonomicName id="4C0A11DCC04FB82082E7C0EBFD4978AA" authority="Matsubara, 1936" authorityName="Matsubara" authorityYear="1936" box="[151,744,1228,1254]" class="Elasmobranchii" family="Pseudocarchariidae" genus="Pseudocarcharias" kingdom="Animalia" order="Lamniformes" pageId="10" pageNumber="266" phylum="Chordata" rank="species" species="kamoharai">
<emphasis id="B97EB64DC04FB82082E7C0EBFD4978AA" bold="true" box="[151,744,1228,1254]" pageId="10" pageNumber="266">
<emphasis id="B97EB64DC04FB82082E7C0EBFE5B78AA" bold="true" box="[151,506,1228,1254]" italics="true" pageId="10" pageNumber="266">Pseudocarcharias kamoharai</emphasis>
(Matsubara, 
<number id="3DC27878C04FB82080D4C0EBFD4178AA" box="[676,736,1228,1254]" pageId="10" pageNumber="266" value="1936.0">1936</number>
