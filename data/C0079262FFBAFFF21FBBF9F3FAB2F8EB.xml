<taxonomicName id="8FAE58F7FFBCFFF21BA0F943FAD4F8EA" authorityName="F. Solari" authorityYear="1955" box="[1164,1320,1772,1797]" class="Insecta" family="Curculionidae" genus="Dolichomeira" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="genus">
<emphasis id="7ADAFF66FFBCFFF21BA0F943FAD4F8EA" box="[1164,1320,1772,1797]" italics="true" pageId="92" pageNumber="93">Dolichomeira</emphasis>
