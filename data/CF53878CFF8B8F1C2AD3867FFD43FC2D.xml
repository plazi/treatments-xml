<collectingCountry id="3FED760AFF8B8F1C28F08681FCDAFE3A" box="[692,789,436,457]" name="Turkey" pageId="30" pageNumber="761">TURKEY</collectingCountry>
</emphasis>
: 1♂, 
<specimenCount id="51FCFD13FF8B8F1C291A8681FC45FE3A" box="[862,906,436,457]" pageId="30" pageNumber="761" type="female">2♀♀</specimenCount>
, 
<collectingRegion id="853EF878FF8B8F1C29E48681FC27FE39" box="[928,1000,436,458]" country="Turkey" name="Istanbul" pageId="30" pageNumber="761">Istanbul</collectingRegion>
, road Yalova-Orhangazi, 
<date id="3344105AFF8B8F1C2B3486F9FE02FE11" box="[368,461,460,482]" pageId="30" pageNumber="761" value="1976-05-11">11.V.1976</date>
