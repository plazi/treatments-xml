<taxonomicName id="34C135A018100B308071EB90C7EB864E" class="Gastropoda" family="Tateidae" genus="Leiorhagium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leiorhagium neteae" order="Littorinimorpha" pageId="2" pageNumber="65" phylum="Mollusca" rank="species" species="neteae">Leiorhagium neteae</taxonomicName>
