<collectingCountry id="12A64584FFD75150FC6DFB6C4FE0CDFC" box="[905,998,1250,1276]" name="Senegal" pageId="36" pageNumber="37">Senegal</collectingCountry>
to 
<collectingCountry id="12A64584FFD75150FBEAFB6C4862CDFC" box="[1038,1124,1250,1276]" name="Angola" pageId="36" pageNumber="37">Angola</collectingCountry>
(
