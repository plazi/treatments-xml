<quantity id="BC54E1DFC229E81BFD271CB0BC12F9FA" box="[752,863,1525,1549]" metricMagnitude="2" metricUnit="m" metricValue="4.5" metricValueMax="6.0" metricValueMin="3.0" pageId="39" pageNumber="564" unit="m" value="450.0" valueMax="600.0" valueMin="300.0">300-600 m</quantity>
