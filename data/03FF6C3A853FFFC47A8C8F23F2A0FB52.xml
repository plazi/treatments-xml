<number id="3D9ECF0B8520FFC479198BF1F560FF6B" box="[770,814,229,255]" pageId="16" pageNumber="17" value="3.8">3, 8</number>
and 
<number id="3D9ECF0B8520FFC4797C8BF1F538FF6B" box="[871,886,229,255]" pageId="16" pageNumber="17" value="8.0">8</number>
setae, subcoxae 
<number id="3D9ECF0B8520FFC47E278BF1F205FF6B" box="[1084,1099,229,255]" pageId="16" pageNumber="17" value="2.0">2</number>
