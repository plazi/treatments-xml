<taxonomicName id="1E94CDD5FFE7D4325570F897FE4643ED" authority="Peña Cantero, 2020" authorityName="Peña Cantero" authorityYear="2020" box="[283,485,1904,1928]" class="Hydrozoa" family="Lafoeidae" genus="Zygophylax" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="399" phylum="Cnidaria" rank="species" species="medeae" status="sp. nov.">
<emphasis id="EBE06A44FFE7D4325570F897FE4643ED" box="[283,485,1904,1928]" italics="true" pageId="10" pageNumber="399">Zygophylax medeae</emphasis>
</taxonomicName>
