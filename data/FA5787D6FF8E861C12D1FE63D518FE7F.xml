<taxonomicName id="B5FE4D43FF8D861A13EDF841D5B7F842" authorityName="Xue &amp; Li &amp; Li" authorityYear="2017" box="[427,658,1971,1993]" class="Insecta" family="Hesperiidae" genus="Coladenia" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="123" phylum="Arthropoda" rank="species" species="simianensis">
