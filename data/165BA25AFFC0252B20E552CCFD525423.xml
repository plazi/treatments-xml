<heading id="C505A420FFC0252B20E552CCFE4853A0" bold="true" box="[151,504,465,491]" fontSize="11" level="1" pageId="7" pageNumber="524" reason="1">
<emphasis id="AC86CF5EFFC0252B20E552CCFE4853A0" bold="true" box="[151,504,465,491]" pageId="7" pageNumber="524">
<emphasis id="AC86CF5EFFC0252B20E552CCFE2353A0" bold="true" box="[151,403,465,491]" italics="true" pageId="7" pageNumber="524">
<taxonomicName id="59F268CFFFC0252B20E552CCFE3D53A0" authority="Limeira-De-Oliveira &amp; Marques &amp; Reis &amp; Rafael, 2017" authorityName="Limeira-De-Oliveira &amp; Marques &amp; Reis &amp; Rafael" authorityYear="2017" box="[151,397,465,491]" class="Insecta" family="Odiniidae" genus="Inpauema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="524" phylum="Arthropoda" rank="species" species="catarinae" status="sp. nov.">Inpauema catarinae</taxonomicName>
