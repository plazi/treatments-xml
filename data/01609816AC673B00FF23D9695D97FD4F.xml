<collectionCode id="EFD8B1C5AC673B00FBD0D9895857FE41" box="[1025,1044,460,480]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="43" pageNumber="436" type="Museum">V</collectionCode>
