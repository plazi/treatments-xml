<materialsCitation id="39A1235DAC673B00FE18D9895882FE41" box="[457,1217,460,480]" collectingDate="1951-08-31" collectingDateMax="1957-03-30" collectingDateMin="1951-08-31" collectionCode="V" collectorName="de Nzo &amp; Ziela, U." location="Nimba" pageId="43" pageNumber="436" specimenCount="1" stateProvince="Nimba">
