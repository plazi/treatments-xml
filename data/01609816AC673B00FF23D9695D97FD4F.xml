<collectorName id="243C4CD6AC673B00FD81D9895ED2FE41" box="[592,657,460,480]" pageId="43" pageNumber="436">de Nzo</collectorName>
et Véla, savane, 
<date id="FD770FC0AC673B00FCFAD9895FE1FE41" box="[811,930,460,480]" pageId="43" pageNumber="436" value="1951-08-31">
<collectingDate id="ED33F628AC673B00FCFAD9895FE1FE41" box="[811,930,460,480]" pageId="43" pageNumber="436" value="1951-08-31">31.VIII.1951</collectingDate>
