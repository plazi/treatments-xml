[
<taxonomicName id="E2D54D0E1C0DFFAE5D9DF9A7FD54F9E9" authority="Smith 1852" authorityName="Smith" authorityYear="1852" box="[466,766,1552,1579]" class="Insecta" family="Vespidae" genus="Vespa" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="54" phylum="Arthropoda" rank="species" species="ducalis">
