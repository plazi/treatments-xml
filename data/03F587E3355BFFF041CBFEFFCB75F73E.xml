<number id="3D9424D2355BFFF04182FCDFCF16F15C" box="[222,264,816,841]" pageId="52" pageNumber="53" value="306.0">306</number>
(n= 
<number id="3D9424D2355BFFF04060FCDFCF58F15D" box="[316,326,816,840]" pageId="52" pageNumber="53" value="1.0">1</number>
