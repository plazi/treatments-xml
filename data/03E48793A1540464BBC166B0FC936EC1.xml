</accessionNumber>
, body in alcohol, skull extracted [in 
<date id="FFF31045A1540467B8D36770FB9268FB" box="[901,1028,1918,1944]" pageId="21" pageNumber="22" value="2006-06">
June 
<number id="3D8524A2A1540467B8956770FB9268FB" box="[963,1028,1918,1944]" pageId="21" pageNumber="22" value="2006.0">2006</number>
</date>
]; 
<accessionNumber id="941EAB66A1540467BF416770FB3D68FB" box="[1047,1195,1918,1944]" httpUri="http://www.uniprot.org/uniprot/ZMB83310" pageId="21" pageNumber="22">
<collectionCode id="ED5CAE40A1540467BF416770FBC168FB" box="[1047,1111,1918,1944]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="21" pageNumber="22">ZMB</collectionCode>
<number id="3D8524A2A1540467BF096770FB3D68FB" box="[1119,1195,1918,1944]" pageId="21" pageNumber="22" value="83310.0">83310</number>
</accessionNumber>
