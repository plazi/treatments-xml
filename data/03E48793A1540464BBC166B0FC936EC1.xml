<paragraph id="8BF23685A1540467BB906770FA0A68A3" blockId="21.[151,1436,1878,1984]" pageId="21" pageNumber="22">
<typeStatus id="54F68827A1540467BB906770FEA068FB" box="[198,310,1918,1944]" pageId="21" pageNumber="22">Syntypes</typeStatus>
