<elevation id="8BDCD187FFD58A33F9C9FD49F911DA6C" box="[1541,1684,744,783]" metricMagnitude="3" metricUnit="m" metricValue="1.22" pageId="26" pageNumber="305" unit="m" value="1220.0">1220 m</elevation>
</quantity>
