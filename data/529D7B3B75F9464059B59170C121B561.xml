<td id="B057E1318A9D6AB62C4FCA1448229C55" align="left" pageId="null" pageNumber="474" valign="top">
2. Behaarung 
<normalizedToken id="EE4B33456DBD3322A52F9070E9C92A43" originalValue="weiß" pageId="null" pageNumber="474">weiss</normalizedToken>
; 
<normalizedToken id="E79E1812826372834D8397F802568B28" originalValue="Hüllblattspitzen" pageId="null" pageNumber="474">Huellblattspitzen</normalizedToken>
