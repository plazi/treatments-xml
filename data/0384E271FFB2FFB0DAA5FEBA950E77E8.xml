<quantity id="4CD5FE82FFB2FFB0DE99FA77925B7480" box="[1195,1281,1426,1452]" metricMagnitude="3" metricUnit="m" metricValue="1.9" pageId="6" pageNumber="39" unit="m" value="1900.0">1900m</quantity>
