<collectingCountry id="F31B763E2A7B796A2A11DFCDF5B02CB3" box="[817,932,1161,1186]" name="Argentina" pageId="36" pageNumber="37">Argentina</collectingCountry>
, 
<collectingCountry id="F31B763E2A7B796A2A96DFCDF21F2CB3" box="[950,1035,1161,1186]" name="Bolivia" pageId="36" pageNumber="37">Bolivia</collectingCountry>
, 
<collectingCountry id="F31B763E2A7B796A2D3EDFCDF2712CB3" box="[1054,1125,1161,1186]" name="Brazil" pageId="36" pageNumber="37">Brazil</collectingCountry>
, 
<collectingCountry id="F31B763E2A7B796A2D57DFCDF2C22CB3" box="[1143,1238,1161,1186]" name="Ecuador" pageId="36" pageNumber="37">Ecuador</collectingCountry>
, 
<collectingCountry id="F31B763E2A7B796A2DC8DFCDF38C2CB3" box="[1256,1432,1161,1186]" name="French Guiana" pageId="36" pageNumber="37">French Guiana</collectingCountry>
, 
<collectingRegion id="49C8F84C2A7B796A29B7DFEAF6E72CD4" box="[151,243,1198,1221]" country="Panama" name="Panama" pageId="36" pageNumber="37">
<collectingCountry id="F31B763E2A7B796A29B7DFEAF6E72CD4" box="[151,243,1198,1221]" name="Panama" pageId="36" pageNumber="37">Panama</collectingCountry>
</collectingRegion>
, and 
<collectingCountry id="F31B763E2A7B796A280FDFEAF7772CD4" box="[303,355,1198,1221]" name="Peru" pageId="36" pageNumber="37">Peru</collectingCountry>
(
<bibRefCitation id="EF9D4B5F2A7B796A2852DFE9F40F2CD7" box="[370,539,1197,1222]" pageId="36" pageNumber="37" refId="ref62259" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii + 1 - 585." type="journal article">Metcalf 1963a</bibRefCitation>
