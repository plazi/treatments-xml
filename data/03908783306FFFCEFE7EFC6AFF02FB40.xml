(
<figureCitation id="13022A10306FFFCBFD8DFC48FD59FACE" box="[560,684,1338,1364]" captionStart="Figure 2" captionStartId="5.[160,225,1440,1463]" captionTargetBox="[162,1154,142,1408]" captionTargetId="figure-62@5.[162,1154,142,1408]" captionTargetPageId="5" captionText="Figure 2. Habitus of examined specimens: (a) Pseudocrangonyx spangsbergi sp. nov., holotype, female, 5.5 mm; (b) Pseudocrangonyx setosus sp. nov., holotype, female, 5.5 mm; (c) Pseudocrangonyx myaochanicus sp. nov., holotype, female, 3.2 mm; (d) Pseudocrangonyx borealis sp. nov., holotype, female, 4.5 mm; (e) Pseudocrangonyx camtschaticus Birstein, 1955, female, 7.5 mm." figureDoi="http://doi.org/10.5281/zenodo.5030645" httpUri="https://zenodo.org/record/5030645/files/figure.png" pageId="9" pageNumber="1767">Figures 2b</figureCitation>
, 
<figureCitation id="13022A10306FFFCBFD07FC48FD01FACE" box="[698,756,1338,1364]" captionStart-0="Figure 7" captionStart-1="Figure 8" captionStart-2="Figure 9" captionStart-3="Figure 10" captionStartId-0="11.[160,225,1054,1077]" captionStartId-1="13.[160,225,1560,1583]" captionStartId-2="14.[160,225,935,958]" captionStartId-3="15.[160,225,1496,1519]" captionTargetBox-0="[162,1154,142,1023]" captionTargetBox-1="[162,1154,142,1528]" captionTargetBox-2="[162,1154,142,903]" captionTargetBox-3="[163,1154,142,1464]" captionTargetId-0="figure-147@11.[162,1154,142,1023]" captionTargetId-1="figure-9@13.[162,1154,142,1529]" captionTargetId-2="figure-116@14.[162,1154,142,904]" captionTargetId-3="figure-64@15.[162,1154,142,1464]" captionTargetPageId-0="11" captionTargetPageId-1="13" captionTargetPageId-2="14" captionTargetPageId-3="15" captionText-0="Figure 7. Pseudocrangonyx setosus sp. nov., holotype MNHN-IU-2019-3171, female, 5.5 mm: (a) antenna I; (b) antenna II; (c) right mandible; (d) left mandible; (e) maxilla I; (f) outer plate of maxilla I; (g) lower lip; (h) maxilliped. Scale bars: 0.2 mm." captionText-1="Figure 8. Pseudocrangonyx setosus sp. nov., holotype MNHN-IU-2019-3171, female, 5.5 mm: (a) gnathopod I; (b) gnathopod II; (c) pereopod III; (d) pereopod IV; (e) pereopod V; (f) pereopod VI; (g) pereopod VII. Scale bar: 0.2 mm." captionText-2="Figure 9. Pseudocrangonyx setosus sp. nov., holotype MNHN-IU-2019-3171, female, 5.5 mm: (a) pleopod I; (b) pleopod II; (c) pleopod III; (d) epimera I–III; (e) uropod I; (f) uropod II; (g) uropod III; (h) telson. Scale bar: 0.2 mm." captionText-3="Figure 10. Pseudocrangonyx setosus sp. nov., paratype 11/10-DAS, male, 3.5 mm: (a) antenna II; (b) gnathopod I; (c) gnathopod II; (d) uropod I; (e) uropod II; (f) telson. Scale bars: 0.2 mm." figureDoi-0="http://doi.org/10.5281/zenodo.5030665" figureDoi-1="http://doi.org/10.5281/zenodo.5030669" figureDoi-2="http://doi.org/10.5281/zenodo.5030673" figureDoi-3="http://doi.org/10.5281/zenodo.5030677" httpUri-0="https://zenodo.org/record/5030665/files/figure.png" httpUri-1="https://zenodo.org/record/5030669/files/figure.png" httpUri-2="https://zenodo.org/record/5030673/files/figure.png" httpUri-3="https://zenodo.org/record/5030677/files/figure.png" pageId="9" pageNumber="1767">7–10</figureCitation>
