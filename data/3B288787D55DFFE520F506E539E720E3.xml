<paragraph id="B33E3691D55DFFE520F506E539EA2589" blockId="25.[151,435,645,704]" box="[151,435,645,671]" pageId="25" pageNumber="282">
<heading id="E87681FDD55DFFE520F506E539EA2589" bold="true" box="[151,435,645,671]" fontSize="11" level="1" pageId="25" pageNumber="282" reason="1">
<emphasis id="81F5EA83D55DFFE520F506E539EA2589" bold="true" box="[151,435,645,671]" pageId="25" pageNumber="282">
<taxonomicName id="74814D12D55DFFE520F506E539332589" box="[151,362,645,671]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="282" phylum="Arthropoda" rank="species" species="unisetata" status="sp. n.">
<emphasis id="81F5EA83D55DFFE520F506E539332589" bold="true" box="[151,362,645,671]" italics="true" pageId="25" pageNumber="282">Manota unisetata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="9AC657F8D55DFFE5211306E539EA2589" box="[369,435,645,671]" pageId="25" pageNumber="282" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
