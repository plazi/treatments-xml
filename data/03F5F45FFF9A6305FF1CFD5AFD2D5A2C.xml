<taxonomicName id="4C5C3ECAFF9A6308FEF2F8A1FDA95CD7" authority="Muona, 2022" authorityName="Muona" authorityYear="2022" box="[302,516,1900,1926]" class="Insecta" family="Eucnemidae" genus="Thambus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="woodruffi" status="n. sp.">
