<collectionCode id="25806A939165FFB46C82BDA52C5250C3" box="[224,288,1186,1210]" country="Canada" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="11" pageNumber="10">CNC</collectionCode>
