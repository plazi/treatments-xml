, male: * 
<collectorName id="EE6497809165FFB46E09BD852EE950E3" box="[619,923,1154,1178]" pageId="11" pageNumber="10">Cataloochee Divide,N.C.</collectorName>
/ 
<date id="372FD4969165FFB46FD6BD85292950E3" box="[948,1115,1154,1178]" pageId="11" pageNumber="10" value="1940-06-10">
<collectingDate id="276B2D7E9165FFB46FD6BD85292950E3" box="[948,1115,1154,1178]" pageId="11" pageNumber="10" value="1940-06-10">June 10 1940</collectingDate>
