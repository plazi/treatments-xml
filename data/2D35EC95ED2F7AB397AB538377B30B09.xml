<taxonomicName id="9A848111138DB746C0989DE08F2FD45D" class="Insecta" family="Zygaenidae" genus="Sinica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinica" order="Lepidoptera" pageId="1" pageNumber="42" phylum="Arthropoda" rank="genus">Sinica</taxonomicName>
