; 
<taxonomicName id="438A4D63FFBDFF96FDB58654FBCFFBF2" authority="Brown &amp; Hodkinson, 1988" authorityName="Brown &amp; Hodkinson" authorityYear="1988" box="[566,1056,1084,1111]" class="Insecta" family="Psyllidae" genus="Limbopsylla" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="166" phylum="Arthropoda" rank="genus">
<emphasis id="B6FEEAF2FFBDFF96FDB58654FD28FBF2" box="[566,711,1084,1110]" italics="true" pageId="29" pageNumber="166">Limbopsylla</emphasis>
<bibRefCitation id="E01B4B11FFBDFF96FD518655FBCFFBF2" author="Brown R. G. &amp; Hodkinson I. D." box="[722,1056,1084,1111]" pageId="29" pageNumber="166" refId="ref20845" refString="Brown R. G. &amp; Hodkinson I. D. 1988. Taxonomy and Ecology of the jumping Plant-Lice of Panama (Homoptera: Psylloidea). E. J. Brill / Scandinavian Science Press Ltd, Leiden." type="book" year="1988">Brown &amp; Hodkinson, 1988</bibRefCitation>
</taxonomicName>
; 
<taxonomicName id="438A4D63FFBDFF96FBB38654FF17FBDE" authority="Hollis &amp; Martin, 1993" authorityName="Hollis &amp; Martin" authorityYear="1993" class="Insecta" family="Psyllidae" genus="Padaukia" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="166" phylum="Arthropoda" rank="genus">
<emphasis id="B6FEEAF2FFBDFF96FBB38654FB4FFBF2" box="[1072,1184,1084,1110]" italics="true" pageId="29" pageNumber="166">Padaukia</emphasis>
Hollis &amp; Martin, 1993
</taxonomicName>
(syn. 
<taxonomicName id="438A4D63FFBDFF96FEC28608FD03FBDE" authority="Heslop-Harrison" authorityName="Heslop-Harrison" box="[321,748,1120,1146]" genus="Peltapaurocephala" kingdom="Animalia" pageId="29" pageNumber="166" rank="genus">
<emphasis id="B6FEEAF2FFBDFF96FEC28608FDCEFBDE" box="[321,545,1120,1146]" italics="true" pageId="29" pageNumber="166">Peltapaurocephala</emphasis>
Heslop-Harrison
</taxonomicName>
nomen nudum, no description); * 
<taxonomicName id="438A4D63FFBDFF96FBF88608FF17FB3A" authority="Tuthill, 1945" authorityName="Tuthill" authorityYear="1945" class="Insecta" family="Psyllidae" genus="Platycorypha" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="166" phylum="Arthropoda" rank="genus">
<emphasis id="B6FEEAF2FFBDFF96FBF88608FAF6FBDE" box="[1147,1305,1120,1146]" italics="true" pageId="29" pageNumber="166">Platycorypha</emphasis>
Tuthill, 1945
</taxonomicName>
(syn. 
<taxonomicName id="438A4D63FFBDFF96FEC386EBFE52FB39" authority="Caldwell, 1948" authorityName="Caldwell" authorityYear="1948" box="[320,445,1155,1181]" class="Insecta" family="Psyllidae" genus="Neopsyllia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="166" phylum="Arthropoda" rank="genus">
