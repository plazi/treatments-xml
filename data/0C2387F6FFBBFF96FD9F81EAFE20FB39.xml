<taxonomicName id="438A4D63FFBDFF96FF3E8654FDC8FBF2" authority="Hodkinson, 1991" authorityName="Hodkinson" authorityYear="1991" box="[189,551,1084,1110]" class="Insecta" family="Calophyidae" genus="Allophorina" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="166" phylum="Arthropoda" rank="genus">
