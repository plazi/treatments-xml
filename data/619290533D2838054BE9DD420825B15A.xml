<taxonomicName id="EBB97581F81E7C8E1D1DF8D03481DF4A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181812" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole obscurior Forel" lsidName-HNS="Pheidole obscurior Forel" order="Hymenoptera" pageNumber="314" phylum="Arthropoda" rank="species" species="obscurior">obscurior</taxonomicName>
