<paragraph id="433636F95E2AFFDED6E7FF10FCB6FF7A" blockId="7.[151,1436,155,915]" box="[199,871,155,180]" pageId="7" pageNumber="432">
<emphasis id="71FDEAEB5E2AFFDED6E7FF10FE9DFF7D" bold="true" box="[199,332,155,180]" pageId="7" pageNumber="432">Size range:</emphasis>
Disc diameter 
<quantity id="84719B1C5E2AFFDED7D6FF10FDBBFF7A" box="[502,618,155,180]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="4.0" metricValueMin="1.5" pageId="7" pageNumber="432" unit="mm" value="2.75" valueMax="4.0" valueMin="1.5">1.5–4 mm</quantity>
, arm length 
<quantity id="84719B1C5E2AFFDED4D5FF10FCB2FF7A" box="[757,867,155,180]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" metricValueMax="13.0" metricValueMin="6.0" pageId="7" pageNumber="432" unit="mm" value="9.5" valueMax="13.0" valueMin="6.0">6–13 mm</quantity>
