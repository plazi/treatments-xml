<taxonomicName id="4C2C4D0C4222FFF49A6F7F0EAB04FA3F" authority="VANDEL, 1972" authorityName="Vandel" authorityYear="1972" box="[282,618,1393,1417]" class="Malacostraca" family="Scleropactidae" genus="Colomboscia" kingdom="Animalia" order="Isopoda" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
<emphasis id="B958EA9D4222FFF49A6F7F0EA8D3FA3E" box="[282,445,1393,1417]" italics="true" pageId="30" pageNumber="31">COLOMBOSCIA</emphasis>
<bibRefCitation id="EFBD4B7E4222FFF49AB07F0EAB04FA3F" author="Vandel A" box="[453,618,1393,1417]" pageId="30" pageNumber="31" pagination="147 - 172" refId="ref84162" refString="Vandel A. 1972. Les isopodes terrestres de la Colombie. Studies on the Neotropical Fauna 7: 147 - 172." type="journal article" year="1972">VANDEL, 1972</bibRefCitation>
