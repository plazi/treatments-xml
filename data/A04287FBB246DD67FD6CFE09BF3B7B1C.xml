<materialsCitation id="98833CB0B246DD66FC3CFE54BA457816" box="[933,1121,497,518]" collectionCode="MCZ" location="Colorado" pageId="6" pageNumber="239" specimenCode="MCZ 1" specimenCount="2" stateProvince="Colorado" typeStatus="lectotype">LeConte 1876: 287</materialsCitation>
