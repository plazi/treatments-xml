Same data as 
<typeStatus id="54C88878C30DB923FDA92C3BB9E0F8E7" box="[600,706,1863,1889]" pageId="1" pageNumber="46">holotype</typeStatus>
, 
<specimenCount id="9D75FD53C30DB923FD3C2C3BB802F8E7" box="[717,800,1863,1889]" pageId="1" pageNumber="46" type="male">
<number id="3DBB24FDC30DB923FD3C2C3BB9F9F8E7" box="[717,731,1863,1889]" pageId="1" pageNumber="46" value="1.0">1</number>
