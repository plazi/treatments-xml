<specimenCode id="DBB39EE7E848FFEA7E06FEA7FDB6FEBC" box="[377,585,260,287]" collectionCode="NHML" country="Libya" httpUri="http://grbio.org/cool/49nc-28y0" name="Natural History Museum, Tripoli" pageId="10" pageNumber="411">NHML010661561</specimenCode>
and 
<specimenCode id="DBB39EE7E848FFEA7DF8FEA7FCA7FEBC" box="[647,856,260,287]" collectionCode="NHML" country="Libya" httpUri="http://grbio.org/cool/49nc-28y0" name="Natural History Museum, Tripoli" pageId="10" pageNumber="411">NHML010661563</specimenCode>
