Peterson (
<taxonomicName id="7B5A8D2EBB4E7BAB2794193DE2466E04" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="153" pageNumber="154" rank="genus">Heliconiaceae</taxonomicName>
) (
<bibRefCitation id="F528860A1F6499713049891A5AF002B6" author="Frank, JH" journalOrPublisher="Insecta Mundi" pageId="287" pageNumber="288" pagination="1 - 12" title="Natural history of four species of Platydracus Thomson (Coleoptera: Staphylinidae) in Heliconia bourgaeana Peterson (Zingiberales: Heliconiaceae) flower bracts." volume="0258" year="2012">
Frank and 
<normalizedToken id="5E20C3E1B1DBD480C8E4ACAE95F0EF2B" originalValue="Morón">Moron</normalizedToken>
2012
</bibRefCitation>
).
</paragraph>
</subSubSection>
