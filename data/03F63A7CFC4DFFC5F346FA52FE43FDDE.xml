<collectingCountry id="F348CBFAFC4DFFC4F3E8FA22FDACFA0D" box="[506,585,1492,1512]" name="Thailand" pageId="12" pageNumber="1021">Thailand</collectingCountry>
, Jaeson NP, 
<geoCoordinate id="EE6BEDADFC4DFFC4F0D9FA22FCFDFA0D" box="[715,792,1492,1512]" degrees="18" direction="north" minutes="46" orientation="latitude" pageId="12" pageNumber="1021" precision="925" value="18.766666">18°46‘N</geoCoordinate>
