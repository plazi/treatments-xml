<quantity id="4CA7268FFC4DFFC4F193FA22FC50FA0D" box="[897,949,1492,1512]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="12" pageNumber="1021" unit="m" value="500.0">500m</quantity>
