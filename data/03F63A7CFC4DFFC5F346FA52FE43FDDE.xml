<geoCoordinate id="EE6BEDADFC4DFFC4F135FA22FC97FA0D" box="[807,882,1492,1512]" degrees="99" direction="east" minutes="28" orientation="longitude" pageId="12" pageNumber="1021" precision="925" value="99.46667">99°28‘E</geoCoordinate>
