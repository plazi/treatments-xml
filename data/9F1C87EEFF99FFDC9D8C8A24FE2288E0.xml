<taxonomicName id="D0B54D7BFF9BFFDC9CC88C4AFC2F8C47" authority="Hartmann-Schroder, 1974" authorityName="Hartmann-Schroder" authorityYear="1974" box="[432,931,538,562]" class="Polychaeta" family="Syllidae" genus="Exogone" kingdom="Animalia" order="Phyllodocida" pageId="26" pageNumber="749" phylum="Annelida" rank="species" species="parahomoseta">
