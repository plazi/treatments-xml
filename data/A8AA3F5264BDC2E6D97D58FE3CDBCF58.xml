<taxonomicName id="B3A7EC33A3F59E97BE5590D1FBBA81A3" class="Insecta" family="Braconidae" genus="Alysia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Alysia" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Alysia</taxonomicName>
