<taxonomicName id="538FF5C3AA49FFC3A76830CCFD74F824" authorityName="Rougemont" authorityYear="2017" box="[597,681,1973,1997]" class="Insecta" family="Staphylinidae" genus="Lesteva" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="alesi">
