<taxonomicName id="4C50C9DC086BFFA8FF57FEE4FE87DCDA" authorityName="Mordvilko" authorityYear="1914" box="[151,321,368,395]" class="Insecta" family="Aphididae" genus="Acyrthosiphon" kingdom="Animalia" order="Aphidomorpha" pageId="8" pageNumber="95" phylum="Arthropoda" rank="genus">
