<taxonomicName id="4BD808B0407DFF98FEB2AA28FD099756" authority="Ueckermann, 1992: 151" authorityName="Ueckermann" authorityPageNumber="151" authorityYear="1992" box="[264,752,1113,1136]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="insulatus">
