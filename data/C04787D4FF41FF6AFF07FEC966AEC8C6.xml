<collectingRegion id="8A2AF820FF41FF6AFAF7FCE460DFC81A" box="[1383,1436,835,861]" country="Guinea-Bissau" name="Bafata" pageId="168" pageNumber="169">Bhat</collectingRegion>
(1968b)
</bibRefCitation>
, 
<taxonomicName id="8FEE4D41FF41FF6AFF6EFCC06758C8C6" authorityName="Dhanda and Bhat" authorityYear="1968" box="[254,539,871,897]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="indoflava">
<emphasis id="7A9AEAD0FF41FF6AFF6EFCC06758C8C6" box="[254,539,871,897]" italics="true" pageId="168" pageNumber="169">Haemaphysalis indoflava</emphasis>
