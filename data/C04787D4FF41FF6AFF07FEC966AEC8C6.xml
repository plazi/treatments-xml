<taxonomicName id="8FEE4D41FF41FF6AFCF2FCB861C1C87E" authorityName="Dhanda and Bhat" authorityYear="1968" box="[866,1154,799,825]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="indoflava">
