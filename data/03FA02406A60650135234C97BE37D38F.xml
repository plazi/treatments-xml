<taxonomicName id="4C53C8D56A60650C31534B62BDF3D6EE" authorityName="McAlpine" authorityYear="2007" class="Insecta" family="Heleomyzidae" genus="Borboroides" kingdom="Animalia" order="Diptera" pageId="37" pageNumber="180" phylum="Arthropoda" rank="species" species="staniochi">
