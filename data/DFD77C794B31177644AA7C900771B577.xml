<taxonomicName id="BB1D6F292E7C21D42AA285C529366BF2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142148" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName="Pheidole arenicola Forel" lsidName-HNS="Pheidole arenicola Forel" order="Hymenoptera" pageNumber="9" phylum="Arthropoda" rank="species" species="arenicola">arenicola</taxonomicName>
