<typeStatus id="54BA0BEAF713FFD951D3F220FD266E38" box="[665,723,1042,1068]" pageId="9" pageNumber="134">Type</typeStatus>
locality: Commonwealth Bay (
<collectingCountry id="F316F5D8F713FFD95706F223FB386E38" box="[1100,1229,1041,1068]" name="Antarctica" pageId="9" pageNumber="134">Antarctica</collectingCountry>
), 325 fathoms (
<quantity id="4CF918ADF713FFD9538DF206FEE26E5B" box="[199,279,1076,1103]" metricMagnitude="2" metricUnit="m" metricValue="5.9399999999999995" pageId="9" pageNumber="134" unit="m" value="594.0">594 m</quantity>
