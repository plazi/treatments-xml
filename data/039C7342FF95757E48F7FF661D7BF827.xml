<taxonomicName id="4C35B9D7FF95757E48F7FDAB1EA2FDB8" box="[151,243,602,627]" class="Malacostraca" family="Anarthruridae" genus="Anarthruropsis" kingdom="Animalia" order="Tanaidacea" pageId="3" pageNumber="64" phylum="Arthropoda" rank="species" species="langi">
