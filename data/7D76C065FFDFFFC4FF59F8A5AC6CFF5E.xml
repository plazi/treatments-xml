<subSubSection id="BDC522F8FFDAFFC4FF08FF7FAC6CFF5E" pageId="6" pageNumber="57" type="etymology">
<paragraph id="F5607173FFDAFFC4FF08FF7FAC6CFF5E" blockId="6.[151,1436,151,216]" pageId="6" pageNumber="57">
<emphasis id="C7ABAD61FFDAFFC4FF08FF7FA97DFF37" bold="true" box="[198,331,151,177]" pageId="6" pageNumber="57">Etymology</emphasis>
