<bibRefCitation id="477A366CA9B922E32D92984109FFF6FF" author="Atanassov, N" journalOrPublisher="Bulletin de l'Institut et Musee de Zoologie" pageId="42" pageNumber="43" pagination="77 - 104" title="[Investigation on the systematic and ecology of ants (Formicidae, Hymenoptera) from Petrich region (SW Bulgaria)]." volume="15" year="1964">Atanassov 1964</bibRefCitation>
(as 
<taxonomicName id="78B9F08633D00148839CCA0A55739D64" class="Insecta" family="Formicidae" genus="Myrmecocystus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmecocystus cursor var. aenescens" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="variety" species="cursor" variety="aenescens">Myrmecocystus cursor var. aenescens</taxonomicName>
