<taxonomicName id="0058084B6546CEDEE7F3B70105612F88" class="Insecta" family="Formicidae" genus="Myrmecocystus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmecocystus cursor" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="cursor">Myrmecocystus cursor</taxonomicName>
