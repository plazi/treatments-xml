<taxonomicName id="D8F54FFFA3F24D54E07147C46E7670E1" class="Insecta" family="Formicidae" genus="Myrmecocystus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmecocystus cursor" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="cursor">Myrmecocystus cursor</taxonomicName>
