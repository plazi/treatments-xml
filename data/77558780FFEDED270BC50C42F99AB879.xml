<date id="8B421056FFEDED2708790D91F9A8BF8A" box="[811,937,916,940]" pageId="6" pageNumber="26" value="1973-01-19">
<number id="493424B1FFEDED2708790D91F96DBF8A" box="[811,876,916,940]" pageId="6" pageNumber="26" value="19.01">19.01</number>
