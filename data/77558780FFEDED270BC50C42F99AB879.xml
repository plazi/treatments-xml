<number id="493424B1FFEDED270BCD0D91FAD0BF8A" box="[159,209,916,940]" pageId="6" pageNumber="26" value="9.45">9.45</number>
x 
<quantity id="38049B73FFEDED270BBF0D91FB62BF8A" box="[237,355,916,940]" metricMagnitude="-2" metricUnit="m" metricValue="1.6179999999999999" pageId="6" pageNumber="26" unit="mm" value="16.18">16.18 mm</quantity>
), Tanjung Namatatumi, Seram, 
<collectingCountry id="87EB7606FFEDED2709950D91F920BF8A" box="[711,801,916,940]" name="Indonesia" pageId="6" pageNumber="26">Maluku</collectingCountry>
