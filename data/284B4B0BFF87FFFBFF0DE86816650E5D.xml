</taxonomicName>
(
<taxonomicName id="67E2819EFF87FFFBFCAFE9EB177601BD" box="[821,1025,2013,2037]" class="Branchiopoda" family="Macrothricidae" genus="Macrothrix" kingdom="Animalia" order="Diplostraca" pageId="13" pageNumber="636" phylum="Arthropoda" rank="species" species="rosea-triserialis">
