<collectingCountry id="D8F5BA8DFF87FFFBFF0BE9281392017D" box="[145,229,1821,1845]" name="Mexico" pageId="13" pageNumber="636">Mexico</collectingCountry>
, 
<collectingCountry id="D8F5BA8DFF87FFFBFF68E9281223017D" box="[242,340,1821,1845]" name="Australia" pageId="13" pageNumber="636">Australia</collectingCountry>
