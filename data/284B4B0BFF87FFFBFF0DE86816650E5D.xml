<taxonomicName id="67E2819EFF87FFFBFAD6E98B16DC019D" baseAuthorityName="Jurine" baseAuthorityYear="1820" box="[1356,1451,1982,2005]" class="Branchiopoda" family="Macrothricidae" genus="Macrothrix" kingdom="Animalia" order="Diplostraca" pageId="13" pageNumber="636" phylum="Arthropoda" rank="species" species="rosea">
