<taxonomicName id="4C6AE58D9626FFD7EABAFC10FEBE20AF" authorityName="Al-Qahtanii Mansouri Al-Quraishyi Abdel-Bakii" authorityYear="2015" box="[160,267,989,1013]" class="Myxosporea" family="Ceratomyxidae" genus="Ceratomyxa" kingdom="Animalia" order="Bivalvulida" pageId="5" pageNumber="545" phylum="Myxozoa" rank="species" species="arabica">
