<taxonomicName id="4C6AE58D9627FFD6EB15F9A0FD9D25DE" box="[271,552,1645,1668]" class="Myxosporea" family="Ceratomyxidae" genus="Ceratomyxa" kingdom="Animalia" order="Bivalvulida" pageId="4" pageNumber="544" phylum="Myxozoa" rank="species" species="ghannouchensis" status="n. sp.">
<emphasis id="B91E421C9627FFD6EB15F9A0FD9D25DE" box="[271,552,1645,1668]" italics="true" pageId="4" pageNumber="544">Ceratomyxa ghannouchensis</emphasis>
