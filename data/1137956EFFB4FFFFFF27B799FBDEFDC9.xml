; 10, 
<taxonomicName id="5E9E5FFBFFB7FFFEFD7BB60DFC17F898" authorityName="Boucek" authorityYear="1963" box="[715,899,1816,1840]" class="Insecta" family="Pteromalidae" genus="Spalangia" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="63" phylum="Arthropoda" rank="species" species="gemina">
