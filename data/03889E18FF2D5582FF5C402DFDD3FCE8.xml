<geoCoordinate id="EE1549C9FF2D5584FD6E4298FCB7FBC0" box="[677,817,1017,1042]" direction="west" orientation="longitude" pageId="146" pageNumber="147" precision="15" value="-0.17722222">00°10′38″W</geoCoordinate>
; 
<elevation id="000CC83DFF2D5584FCF0429BFC38FBC0" box="[827,958,1017,1042]" metricMagnitude="2" metricUnit="m" metricValue="4.279999999999999" pageId="146" pageNumber="147" unit="m" value="428.0">
<quantity id="4CD982EBFF2D5584FCF0429BFC04FBC3" box="[827,898,1018,1042]" metricMagnitude="2" metricUnit="m" metricValue="4.279999999999999" pageId="146" pageNumber="147" unit="m" value="428.0">428 m</quantity>
a.s.l.
</elevation>
), date unknown, leg. 
