<taxonomicName id="9DC850D6FFB52A2E4D05A4F6FB65FF6E" box="[1097,1176,151,176]" class="Magnoliopsida" family="Lamiaceae" genus="Nepeta" kingdom="Plantae" order="Lamiales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="genus">
<emphasis id="68BCF747FFB52A2E4D05A4F6FB65FF6E" box="[1097,1176,151,176]" italics="true" pageId="76" pageNumber="77">Nepeta</emphasis>
