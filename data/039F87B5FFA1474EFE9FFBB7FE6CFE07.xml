<taxonomicName id="4C364D20FFA1474DFEFAFB68FD57FB6F" authority="Boisduval." authorityName="Boisduval." box="[373,669,1198,1221]" class="Insecta" family="Curculionidae" genus="Orthorhinus" kingdom="Animalia" order="Coleoptera" pageId="101" pageNumber="200" phylum="Arthropoda" rank="species" species="aethiops">
<emphasis id="B942EAB1FFA1474DFEFAFB68FDF9FB6E" box="[373,563,1200,1221]" italics="true" pageId="101" pageNumber="200">Orthorhinus aethiops</emphasis>
