<taxonomicName id="D1AA4D2AFFF3FFBB019E6EDEFDA3F82A" authority="Poppius, 1909" authorityName="Poppius" authorityYear="1909" box="[151,512,1944,1970]" class="Insecta" family="Miridae" genus="Euchilofulvius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="439" phylum="Arthropoda" rank="genus">
