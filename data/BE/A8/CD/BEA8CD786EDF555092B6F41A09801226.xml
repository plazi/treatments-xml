<taxonomicName class="Magnoliopsida" family="Lamiaceae" genus="Salvia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Salvia" order="Lamiales" pageId="0" pageNumber="21" phylum="Tracheophyta" rank="genus">
