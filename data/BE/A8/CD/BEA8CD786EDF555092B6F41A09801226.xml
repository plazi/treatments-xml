<taxonomicName class="Liliopsida" family="Poaceae" genus="Bouteloua" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Bouteloua uniflora" order="Poales" pageId="0" pageNumber="21" phylum="Tracheophyta" rank="species" species="uniflora">
