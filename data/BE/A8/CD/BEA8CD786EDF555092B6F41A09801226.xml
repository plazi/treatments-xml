<emphasis italics="true" pageId="0" pageNumber="21">Berberis</emphasis>
</taxonomicName>
sp., 
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Brickellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Brickellia" order="Asterales" pageId="0" pageNumber="21" phylum="Tracheophyta" rank="genus">
