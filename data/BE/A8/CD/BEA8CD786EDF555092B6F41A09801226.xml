</taxonomicName>
sp., 
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Prunus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Prunus" order="Rosales" pageId="0" pageNumber="21" phylum="Tracheophyta" rank="genus">
