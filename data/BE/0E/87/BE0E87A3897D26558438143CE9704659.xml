<taxonomicName authority="GOPP." authorityName="GOPP." box="[204,447,1399,1420]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="pyramidalis">
