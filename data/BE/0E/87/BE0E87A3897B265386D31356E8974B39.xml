<taxonomicName authority="ETTINGSH." authorityName="ETTINGSH." box="[918,1218,657,678]" class="Magnoliopsida" family="Magnoliaceae" genus="Liriodendron" kingdom="Plantae" order="Magnoliales" pageId="3" pageNumber="295" phylum="Tracheophyta" rank="species" species="haueri">
