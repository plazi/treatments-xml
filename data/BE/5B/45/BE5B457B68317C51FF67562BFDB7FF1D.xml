<number box="[572,631,1638,1664]" pageId="3" pageNumber="34" value="2.1">2, 10</number>
on coxae 
<number box="[753,856,1638,1664]" pageId="3" pageNumber="34" value="8.56" valueMax="14.0" valueMin="3.12">3, 12–14</number>
