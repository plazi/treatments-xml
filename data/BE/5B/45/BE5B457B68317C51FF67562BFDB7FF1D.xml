<number box="[631,765,1598,1624]" pageId="3" pageNumber="34" value="1272.0" valueMax="1525.0" valueMin="1019.0">
<date box="[631,765,1598,1624]" pageId="3" pageNumber="34" value="1019" valueMax="1525">1019–1525</date>
