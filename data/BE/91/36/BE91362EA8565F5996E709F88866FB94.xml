<taxonomicName genus="P." lsidName="P. oculospinum" pageId="0" pageNumber="145" rank="species" species="oculospinum">
<emphasis italics="true" pageId="0" pageNumber="145">P. oculospinum</emphasis>
</taxonomicName>
