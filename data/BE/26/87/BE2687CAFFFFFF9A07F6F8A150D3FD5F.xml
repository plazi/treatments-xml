<taxonomicName authority="Pascoe, 1857" authorityName="Pascoe" authorityYear="1857" box="[377,668,722,747]" class="Insecta" family="Cerambycidae" genus="Golsinda" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="corallina">
<emphasis box="[377,511,723,747]" italics="true" pageId="1" pageNumber="64">G. corallina</emphasis>
Pascoe, 
<number box="[609,668,722,746]" pageId="1" pageNumber="64" value="1857.0">1857</number>
</taxonomicName>
, subsequently designated by 
<bibRefCitation author="Pascoe" box="[994,1156,722,746]" pageId="1" pageNumber="64" refString="Pascoe, F. P. (1862) Notices of new or little-known genera and species of Coleoptera. Part III. Journal of Entomology, 1, 319 - 370, 2 pls." type="journal article" year="1862">
Pascoe (
<number box="[1087,1148,722,746]" pageId="1" pageNumber="64" value="1862.0">1862</number>
)
