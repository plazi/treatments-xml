<taxonomicName class="Arachnida" family="Liphistiidae" genus="Ganthela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ganthela cipingensis" order="Araneae" pageId="6" pageNumber="127" phylum="Arthropoda" rank="species" species="cipingensis">Ganthela cipingensis</taxonomicName>
