<collectingCountry box="[787,931,1089,1115]" name="Madagascar" pageId="20" pageNumber="21">Madagascar</collectingCountry>
, 
<collectingCountry box="[945,1002,1089,1115]" name="Peru" pageId="20" pageNumber="21">Peru</collectingCountry>
, 
<collectingCountry box="[1016,1218,1089,1115]" name="Solomon Islands" pageId="20" pageNumber="21">Solomon Islands</collectingCountry>
, 
<collectingCountry box="[1232,1346,1089,1115]" name="Indonesia" pageId="20" pageNumber="21">Indonesia</collectingCountry>
