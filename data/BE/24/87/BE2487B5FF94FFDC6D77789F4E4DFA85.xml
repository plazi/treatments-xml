<subSubSection pageId="7" pageNumber="8" type="diagnosis">
<paragraph blockId="7.[189,310,1621,1647]" box="[189,310,1621,1647]" pageId="7" pageNumber="8">
<heading bold="true" box="[189,310,1621,1647]" fontSize="11" level="3" pageId="7" pageNumber="8" reason="3">
