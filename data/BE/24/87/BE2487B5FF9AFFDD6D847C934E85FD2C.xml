<taxonomicName authority="Darby, 2017" authorityName="Darby" authorityYear="2017" box="[730,940,449,476]" class="Insecta" family="Ptiliidae" genus="Erro" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
