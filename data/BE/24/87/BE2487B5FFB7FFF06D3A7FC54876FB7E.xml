<subSubSection pageId="36" pageNumber="37" type="diagnosis">
<paragraph blockId="36.[189,310,763,789]" box="[189,310,763,789]" pageId="36" pageNumber="37">
<heading bold="true" box="[189,310,763,789]" fontSize="11" level="3" pageId="36" pageNumber="37" reason="3">
