<subSubSection box="[151,598,1900,1927]" pageId="0" pageNumber="423" type="nomenclature">
<paragraph blockId="0.[151,598,1900,1927]" box="[151,598,1900,1927]" pageId="0" pageNumber="423">
<heading bold="true" box="[151,598,1900,1927]" fontSize="11" level="1" pageId="0" pageNumber="423" reason="1">
<taxonomicName authority="Norman &amp; Stebbing, 1886" authorityName="Norman &amp; Stebbing" authorityYear="1886" box="[151,598,1900,1927]" class="Malacostraca" family="Anthuridae" genus="Cyathura" kingdom="Animalia" order="Isopoda" pageId="0" pageNumber="423" phylum="Arthropoda" rank="genus">
