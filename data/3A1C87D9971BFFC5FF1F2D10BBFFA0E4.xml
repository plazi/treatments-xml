<taxonomicName id="75B54D4C971BFFD8FF1F28D7B859A1EB" authorityName="Golovatch" authorityYear="1992" box="[151,317,1950,1975]" class="Diplopoda" family="Paradoxosomatidae" genus="Martensosoma" kingdom="Animalia" order="Polydesmida" pageId="14" pageNumber="499" phylum="Arthropoda" rank="genus">Martensosoma</taxonomicName>
