<taxonomicName id="75B54D4C971BFFD8FBDE28D4BDBEA1EB" baseAuthorityName="Golovatch" baseAuthorityYear="1992" box="[1110,1242,1949,1975]" class="Diplopoda" family="Paradoxosomatidae" genus="Delarthrum" kingdom="Animalia" order="Polydesmida" pageId="14" pageNumber="499" phylum="Arthropoda" rank="genus">Delarthrum</taxonomicName>
