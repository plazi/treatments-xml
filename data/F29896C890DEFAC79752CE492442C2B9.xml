<paragraph id="B456A617EE788F2EDF66AAEF39DAB27E" pageId="null" pageNumber="505">
<taxonomicName id="D103DDDCDAFD4E43AAC8F52C0A6AC478" authority="L." class="Magnoliopsida" family="Fabaceae" genus="Ononis" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="505" phylum="Tracheophyta" rank="species" species="spinosa">
<pageBreakToken id="58A40CCD16B388B382462E046DEA8425" pageId="null" pageNumber="505">Ononis</pageBreakToken>
