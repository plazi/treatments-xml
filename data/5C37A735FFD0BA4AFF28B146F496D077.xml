<collectionCode id="B28F8EE6FFD0BA4BFCCDB0CAF4DED6DE" LSID="urn:lsid:biocol.org:col:34940" box="[882,968,1974,1997]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34940" name="Delaware Museum of Natural History" pageId="42" pageNumber="43">DMNH</collectionCode>
