<collectionCode id="B28F8EE6FFD0BA4BFE6BB0CAF53CD6DE" LSID="urn:lsid:biocol.org:col:34940" box="[468,554,1974,1997]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34940" name="Delaware Museum of Natural History" pageId="42" pageNumber="43">DMNH</collectionCode>
