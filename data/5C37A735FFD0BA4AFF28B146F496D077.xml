<taxonomicName id="139E6DA0FFD0BA4BFC70B0EEF373D6BA" authority="Scopoli, 1777" authorityName="Scopoli" authorityYear="1777" box="[975,1125,1938,1961]" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="semipictus">
