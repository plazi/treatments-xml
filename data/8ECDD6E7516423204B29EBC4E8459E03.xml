40. 
<taxonomicName id="8E361066E69C9073BC1C57CC2DD6664C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136046" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica rufoglauca Jerdon" lsidName-HNS="Formica rufoglauca Jerdon" order="Hymenoptera" pageNumber="124" phylum="Arthropoda" rank="species" species="rufoglauca">Formica rufoglauca</taxonomicName>
