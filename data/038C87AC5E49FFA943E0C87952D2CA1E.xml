<emphasis id="B951EAA85E49FFA943E0C8795087CC06" bold="true" box="[151,511,594,620]" pageId="1" pageNumber="2">
Family 
<taxonomicName id="4C254D395E49FFA94381C8795087CC06" authority="Huxley, 1879" authorityName="Huxley" authorityYear="1879" box="[246,511,594,620]" class="Insecta" family="Axiidae" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="family">
Axiidae Huxley, 
<number id="3DED249D5E49FFA942B3C8795087CC06" box="[452,511,594,620]" pageId="1" pageNumber="2" value="1879.0">1879</number>
</taxonomicName>
