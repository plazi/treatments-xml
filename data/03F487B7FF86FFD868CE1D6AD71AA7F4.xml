<specimenCount id="9D5BFD28FF86FFDC6DD91CA2D713A7ED" pageId="33" pageNumber="434" type="generic">
<number id="3D952486FF86FFDC6DD91CA2D383A7C8" box="[1408,1436,1967,1992]" pageId="33" pageNumber="434" value="16.0">16</number>
