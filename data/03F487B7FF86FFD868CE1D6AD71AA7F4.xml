<date id="FFE31061FF86FFDC6B171CBDD20CA7C8" box="[846,1043,1968,1992]" pageId="33" pageNumber="434" value="1999-01-13">
<number id="3D952486FF86FFDC6B171CBDD575A7C8" box="[846,874,1968,1992]" pageId="33" pageNumber="434" value="13.0">13</number>
