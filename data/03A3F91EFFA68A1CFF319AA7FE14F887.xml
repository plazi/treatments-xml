<taxonomicName id="4C0A338BFFA68A1CFB2A9BEAFB7CFC7A" box="[1164,1272,896,920]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="573" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
