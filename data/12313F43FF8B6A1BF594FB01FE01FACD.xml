<taxonomicName id="5D98F5D6FF8A6A1BF68FFE4EFED4FE76" authority="Kingsley, 1878" authorityName="Kingsley" authorityYear="1878" class="Malacostraca" family="Hippolytidae" genus="Thor" kingdom="Animalia" order="Decapoda" pageId="35" pageNumber="35" phylum="Arthropoda" rank="species" species="floridanus">
