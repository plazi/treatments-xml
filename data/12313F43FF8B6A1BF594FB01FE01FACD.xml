<taxonomicName id="5D98F5D6FF8A6A1BF7E0FE4FFEA9FE5A" box="[178,324,450,473]" class="Malacostraca" family="Bopyridae" genus="Bopyrinella" kingdom="Animalia" order="Isopoda" pageId="35" pageNumber="35" phylum="Arthropoda" rank="species" species="antillensis">
