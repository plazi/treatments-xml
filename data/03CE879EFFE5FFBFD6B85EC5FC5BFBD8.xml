in 
<collectionCode id="ED76AE4DFFE5FFBFD7885EB7F8F6FCD5" box="[423,511,764,787]" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="5" pageNumber="24">NHMW</collectionCode>
