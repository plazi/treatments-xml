<emphasis id="6FD4EAAE0108DC39F9C1A429FE5A2AAE" bold="true" box="[199,460,619,644]" italics="true" pageId="12" pageNumber="59">
<taxonomicName id="9AA04D3F0108DC39F9C1A429FEE72AAE" box="[199,369,619,644]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="59" phylum="Arthropoda" rank="genus">Crematogaster</taxonomicName>
