<quantity id="4C8DCEC3FFC88740FDD5FE2283B57CDF" box="[636,757,474,499]" metricMagnitude="-2" metricUnit="m" metricValue="7.65" metricValueMax="9.8" metricValueMin="5.5" pageId="10" pageNumber="477" unit="mm" value="76.5" valueMax="98.0" valueMin="55.0">55–98 mm</quantity>
