species with 
<taxonomicName id="6F8674CE09228C52FAE1837BF588FE4C" authorityName="Chamberlin &amp; Ivie" authorityYear="1943" box="[1308,1427,368,392]" class="Arachnida" family="Linyphiidae" genus="Arcuphantes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="509" phylum="Arthropoda" rank="species" species="arcuatulus">
