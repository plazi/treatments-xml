<collectionCode id="B5E1AE6CFFF82D3656C4FA9442AEF350" LSID="urn:lsid:biocol.org:col:34700" box="[151,213,1344,1370]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="10" pageNumber="11">CAS</collectionCode>
