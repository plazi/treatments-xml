<taxonomicName id="239C147F0297D6B7502E2E675B1D42F6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142831" class="Insecta" family="Formicidae" genus="Platythyrea" kingdom="Animalia" lsidName="Platythyrea lamellosa subsp. suturalis Forel" lsidName-HNS="Platythyrea lamellosa subsp. suturalis Forel" order="Hymenoptera" pageNumber="64" phylum="Arthropoda" rank="subSpecies" species="lamellosa" subSpecies="suturalis">Platythyrea lamellosa Rog. subsp. suturalis</taxonomicName>
<taxonomicNameLabel id="FC690B426F9B4B3EF81D9C82B8A9BF5F" rank="subSpecies">n. subsp.</taxonomicNameLabel>
