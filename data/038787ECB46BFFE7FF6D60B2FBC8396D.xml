<quantity id="4CD69B1FB469FFE7FE4064B9FDBD3CF3" box="[442,573,152,175]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" metricValueMax="3.0" metricValueMin="2.0" pageId="2" pageNumber="73" unit="mm" value="0.25" valueMax="0.3" valueMin="0.2">0.2–0.3 mm</quantity>
, membrane without setae. VR 
<number id="3DE624DDB469FFE7FC7964B9FC713CF3" box="[899,1009,152,175]" pageId="2" pageNumber="73" value="1.44" valueMax="1.48" valueMin="1.4">1.40–1.48</number>
