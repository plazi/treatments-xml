7. 
<taxonomicName id="84954D02C3188707FDF219ABFD3FFF36" authorityName="Tendeiro" authorityYear="1984" box="[515,668,137,161]" class="Insecta" family="Philopteridae" genus="Columbicola" kingdom="Animalia" order="Psocodea" pageId="34" pageNumber="3577" phylum="Arthropoda" rank="species" species="effeminatus">
