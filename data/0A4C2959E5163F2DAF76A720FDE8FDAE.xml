thick (n = 
<number id="342D8A68E5103F2CAFFFA348FE37FA06" box="[385,399,1396,1422]" pageId="4" pageNumber="5" value="1.0">1</number>
); male, ca. 
<number id="342D8A68E5103F2CAC64A348FD86FA06" box="[538,574,1396,1422]" pageId="4" pageNumber="5" value="0.7">0.7</number>
