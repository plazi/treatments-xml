<taxonomicName id="F5A14D257F247D0629E56D08B643F98B" authority="GUERRA-GARCÍA, 2003" authorityName="GUERRA-GARCÍA" authorityYear="2003" box="[405,611,1546,1567]" class="Malacostraca" family="Caprellidae" genus="Deutella" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="1067" phylum="Arthropoda" rank="species" species="margaritae" status="new species">
