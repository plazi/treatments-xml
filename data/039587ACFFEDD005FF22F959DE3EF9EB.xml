<number id="3DF4249DFFECD005FE60FF4FD857FF1B" box="[469,541,227,251]" pageId="9" pageNumber="31" value="26.5" valueMax="30.0" valueMin="23.0">23–30</number>
% (
<number id="3DF4249DFFECD005FDF7FF4FD82AFF1B" box="[578,608,227,251]" pageId="9" pageNumber="31" value="26.0">26</number>
