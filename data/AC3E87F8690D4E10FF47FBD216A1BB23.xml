<taxonomicName id="E3974D6D690D4E10FED3F91116CBBB57" box="[259,387,1773,1797]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="capitata">A. capitata</taxonomicName>
