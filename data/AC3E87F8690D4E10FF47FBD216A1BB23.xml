<emphasis id="16E3EAFC690D4E10FA54F93512D4BB57" italics="true" pageId="3" pageNumber="43">
<taxonomicName id="E3974D6D690D4E10FA54F93517BFBB57" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="alluaudi">A. alluaudi</taxonomicName>
