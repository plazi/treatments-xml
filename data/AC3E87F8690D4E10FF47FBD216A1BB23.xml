<taxonomicName id="E3974D6D690D4E10FC6FF9591365BAEF" box="[959,1069,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="hystrix">A. hystrix</taxonomicName>
, 
<taxonomicName id="E3974D6D690D4E10FBE8F95913ECBAEF" box="[1080,1188,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="majori">A. majori</taxonomicName>
, 
<taxonomicName id="E3974D6D690D4E10FB60F9591258BAEF" box="[1200,1296,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="remyi">A. remyi</taxonomicName>
, 
<taxonomicName id="E3974D6D690D4E10FACBF95912D4BAEF" box="[1307,1436,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="succinea" status="sp. nov.">A. succinea</taxonomicName>
</emphasis>
<taxonomicNameLabel id="0DD05787690D4E10FF47F93617B9BAB3" box="[151,241,1738,1762]" pageId="3" pageNumber="43" rank="species">
<emphasis id="16E3EAFC690D4E10FF47F93617A3BAB0" bold="true" box="[151,235,1738,1762]" pageId="3" pageNumber="43">sp. nov</emphasis>
.
</taxonomicNameLabel>
, 
<taxonomicName id="E3974D6D690D4E10FED3F9351633BAB3" box="[259,379,1737,1761]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="venator">
<emphasis id="16E3EAFC690D4E10FED3F9351633BAB3" box="[259,379,1737,1761]" italics="true" pageId="3" pageNumber="43">A. venator</emphasis>
</taxonomicName>
