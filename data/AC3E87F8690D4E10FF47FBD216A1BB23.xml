<emphasis id="16E3EAFC690D4E10FDCEF95912D4BAEF" box="[542,1436,1701,1725]" italics="true" pageId="3" pageNumber="43">
<taxonomicName id="E3974D6D690D4E10FDCEF95915D8BAEF" box="[542,656,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="armata">A. armata</taxonomicName>
, 
<taxonomicName id="E3974D6D690D4E10FD4BF9591451BAEF" box="[667,793,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="capitata">A. capitata</taxonomicName>
, 
<taxonomicName id="E3974D6D690D4E10FCF4F95914FDBAEF" box="[804,949,1701,1725]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="flavispina">A. flavispina</taxonomicName>
