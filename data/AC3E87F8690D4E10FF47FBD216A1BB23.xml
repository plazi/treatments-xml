<taxonomicName id="E3974D6D690D4E10FAD2F9A11225BA27" box="[1282,1389,1629,1653]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="majori">
<emphasis id="16E3EAFC690D4E10FAD2F9A11225BA27" box="[1282,1389,1629,1653]" italics="true" pageId="3" pageNumber="43">A. majori</emphasis>
</taxonomicName>
