<taxonomicName id="E3974D6D690D4E10FE7AF8C91562BB1F" box="[426,554,1845,1869]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="alluaudi">A. alluaudi</taxonomicName>
