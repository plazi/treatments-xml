<taxonomicName id="E3974D6D690D4E10FE5FF911154CBB57" box="[399,516,1773,1797]" class="Arachnida" family="Triaenonychidae" genus="Triacumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="cowani">A. cowani</taxonomicName>
