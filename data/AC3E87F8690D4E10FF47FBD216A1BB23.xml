<taxonomicName id="E3974D6D690D4E10FBD8F8C913C3BB1F" box="[1032,1163,1845,1869]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="succinea" status="sp. nov.">
