<taxonomicName id="E3974D6D690D4E10FAECF91112D4BB57" box="[1340,1436,1773,1797]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="remyi">A. remyi</taxonomicName>
