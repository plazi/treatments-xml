<taxonomicName id="E3974D6D690D4E10FC68F911136FBB57" box="[952,1063,1773,1797]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="hystrix">A. hystrix</taxonomicName>
