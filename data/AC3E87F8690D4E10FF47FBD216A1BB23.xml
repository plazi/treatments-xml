<taxonomicName id="E3974D6D690D4E10FF16F8ED1600BB7B" box="[198,328,1809,1833]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="succinea" status="sp. nov.">
