<taxonomicName id="E3974D6D690D4E10FD7FF911146DBB57" box="[687,805,1773,1797]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="hispida">A. hispida</taxonomicName>
, 
<taxonomicName id="E3974D6D690D4E10FCE2F91114E2BB57" box="[818,938,1773,1797]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="horrida">A. horrida</taxonomicName>
