<taxonomicName id="E3974D6D690D4E10FF47F97D164BBACB" box="[151,259,1665,1689]" class="Arachnida" family="Triaenonychidae" genus="Acumontia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="43" phylum="Arthropoda" rank="species" species="roberti">
<emphasis id="16E3EAFC690D4E10FF47F97D164BBACB" box="[151,259,1665,1689]" italics="true" pageId="3" pageNumber="43">A. roberti</emphasis>
</taxonomicName>
