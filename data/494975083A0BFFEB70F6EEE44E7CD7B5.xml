<taxonomicName id="06E0BF9D3A0BFFE87173E8344EFBD24B" authorityName="Ahti &amp; Sipman" authorityYear="2013" box="[274,417,1828,1849]" class="Lecanoromycetes" family="Cladoniaceae" genus="Cladonia" kingdom="Fungi" order="Lecanorales" pageId="9" pageNumber="34" phylum="Ascomycota" rank="species" species="recta">
