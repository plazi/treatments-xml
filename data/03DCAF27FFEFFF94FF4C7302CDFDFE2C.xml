<paragraph id="8BCA1E31FFEFFF96FF4C7344CEC8F954" blockId="1.[151,651,1654,1677]" box="[151,651,1654,1677]" pageId="1" pageNumber="44">
<emphasis id="B901C223FFEFFF96FF4C7344CEC8F954" bold="true" box="[151,651,1654,1677]" pageId="1" pageNumber="44">
<typeStatus id="54CEA093FFEFFF96FF4C7344CC89F954" box="[151,202,1655,1676]" pageId="1" pageNumber="44">Type</typeStatus>
species 
<emphasis id="B901C223FFEFFF96FEC7734BCDAAF954" bold="true" box="[284,489,1654,1677]" italics="true" pageId="1" pageNumber="44">
— 
<taxonomicName id="4C7565B2FFEFFF96FEE87345CDAAF954" authority="Smirnov, 1989" authorityName="Smirnov" authorityYear="1989" box="[307,489,1654,1676]" class="Branchiopoda" family="Chydoridae" genus="Biapertura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="willisi">Biapertura willisi</taxonomicName>
