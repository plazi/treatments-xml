<paragraph id="F620369BFF5FFFD3FF12FD976347FDA8" blockId="15.[189,966,576,641]" box="[234,348,576,602]" pageId="15" pageNumber="16">
<heading id="AD6881F7FF5FFFD3FF12FD976347FDA8" bold="true" box="[234,348,576,602]" fontSize="11" level="2" pageId="15" pageNumber="16" reason="6">
