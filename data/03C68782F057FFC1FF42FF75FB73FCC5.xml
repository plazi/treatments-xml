<number id="3DA724B3F057FFC2FB79F88AFB3BF850" box="[1196,1245,1895,1920]" pageId="53" pageNumber="54" value="10.5">10.5</number>
± 
<number id="3DA724B3F057FFC2FB38F88AFAF4F850" box="[1261,1298,1895,1920]" pageId="53" pageNumber="54" value="0.8">0.8</number>
