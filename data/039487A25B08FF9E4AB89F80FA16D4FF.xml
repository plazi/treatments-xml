<subSubSection id="C327653F5B08FF9E4AE89E46FE28D4D7" lastPageId="14" lastPageNumber="15" pageId="13" pageNumber="14" type="description">
<paragraph id="8B8236B45B08FF9D4AE89E46FB5FD372" blockId="13.[151,1437,1229,2016]" box="[199,1243,1349,1376]" pageId="13" pageNumber="14">
<emphasis id="B949EAA65B08FF9D4AE89E46FEDBD34D" bold="true" box="[199,351,1349,1375]" pageId="13" pageNumber="14">Description.</emphasis>
Total length, 
<quantity id="4CC59B515B08FF9D482E9E45FDE9D372" box="[513,621,1350,1376]" metricMagnitude="-3" metricUnit="m" metricValue="5.58" pageId="13" pageNumber="14" unit="mm" value="5.58">5.58 mm</quantity>
