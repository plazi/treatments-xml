<number id="3DE7248A9625FFCFFACABA486329F9E8" box="[1296,1366,1626,1650]" pageId="6" pageNumber="64" value="315.5">315.5</number>
<emphasis id="B95BEABF9625FFCFFA8CBA49631BF9E9" bold="true" box="[1366,1380,1627,1651]" pageId="6" pageNumber="64">–</emphasis>
<number id="3DE7248A9625FFCFFABEBA4863E7F9E8" box="[1380,1432,1626,1650]" pageId="6" pageNumber="64" value="16.5">16.5</number>
