<geoCoordinate id="EE6C58CA3818FFC0A788FB05FCB05D89" box="[763,826,1247,1268]" degrees="42, 8" direction="north" orientation="latitude" pageId="54" pageNumber="1067" precision="5555" value="42.8">42,8 N</geoCoordinate>
