<taxonomicName id="4C394D52FFABFF82E0434E0031346AF4" authority="Tschitscherine, 1897" authorityName="Tschitscherine" authorityYear="1897" box="[600,1053,1397,1422]" class="Insecta" family="Carabidae" genus="Blennidus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="jelskii">
