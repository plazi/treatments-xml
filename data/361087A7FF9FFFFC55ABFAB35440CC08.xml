<collectingCountry id="C6AE7621FF9FFFFA545BF83C51BDCAA6" box="[359,471,1945,1971]" name="Indonesia" pageId="90" pageNumber="91">Indonesia</collectingCountry>
, depth 
<quantity id="79419B54FF9FFFFA5714F83C52EACAA1" box="[552,640,1945,1972]" metricMagnitude="1" metricUnit="m" metricValue="2.6" metricValueMax="5.2" metricValueMin="0.0" pageId="90" pageNumber="91" unit="m" value="26.0" valueMax="52.0" valueMin="0.0">
