<paragraph id="8BE7890F5A3CFF8B46FBFED3FE33FE16" blockId="8.[151,1436,222,422]" pageId="8" pageNumber="319">
<materialsCitation id="3B3083525A3CFF8B46FBFED3FE33FE16" ID-GBIF-Occurrence="1290990126" location="Guangxi" pageId="8" pageNumber="319" specimenCount="1" stateProvince="Guangxi">
