<collectionCode id="ED76AE780C2DF8006DA89C74FEC8706E" box="[200,291,1635,1659]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="61" pageNumber="60">AMNH</collectionCode>
