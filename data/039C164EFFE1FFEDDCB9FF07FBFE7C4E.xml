<taxonomicName id="4C35DCDBFFE1FFEDDCB9FF07FE40783B" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[139,474,528,551]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="miltocephalus" status="new species">
<emphasis id="B9417B4AFFE1FFEDDCB9FF07FE40783B" bold="true" box="[139,474,528,551]" italics="true" pageId="96" pageNumber="97">Paramblynotus miltocephalus</emphasis>
</taxonomicName>
, 
