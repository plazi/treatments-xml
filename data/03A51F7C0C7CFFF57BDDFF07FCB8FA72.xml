<quantity id="4CF4038F0C7CFFF67F4DFE10FBF8FE21" box="[1031,1117,397,420]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="1" pageNumber="434" unit="mm" value="4.5">4.5 mm</quantity>
