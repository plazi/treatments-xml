<taxonomicName id="4C3B4D5BFFCB2A35FB9DF9A5FB75F90B" baseAuthorityName="Retana-Salazar and Soto-Rodriguez" baseAuthorityYear="2007" box="[1100,1155,1647,1672]" class="Insecta" family="Phlaeothripidae" genus="Haplothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="1403" phylum="Arthropoda" rank="species" species="saidi">
<emphasis id="B94FEACAFFCB2A35FB9DF9A5FB75F90B" bold="true" box="[1100,1155,1647,1672]" italics="true" pageId="15" pageNumber="1403">saidi</emphasis>
