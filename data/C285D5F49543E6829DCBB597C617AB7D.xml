<paragraph id="268C3F1055EE17BDB749BF0DCBA78557" pageId="0" pageNumber="1">
66. 
<taxonomicName id="4DFBB644E4792251B29FCECB770C46F8" LSID="http://zoobank.org/6ED58BAB-BF9B-4E92-869C-5446BD7A45E3" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus procurtus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="procurtus">Trigonopterus procurtus Riedel</taxonomicName>
<taxonomicNameLabel id="9E7738D1CF843B7A5695B6F0519281F4" pageId="0" pageNumber="1">sp. n.</taxonomicNameLabel>
