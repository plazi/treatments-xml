<treatmentCitationGroup id="AB4811E4FF8A2768B510DAC227DEFE1C" pageId="5" pageNumber="6">
<taxonomicName id="4C584D49FF8A2768B510DAC224D0FE7F" box="[264,405,399,422]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
