<heading id="CF100C28FFA9493BEC0EFF7E64A412AF" bold="true" box="[889,1415,190,214]" centered="true" fontSize="10" level="2" pageId="6" pageNumber="533" reason="2">
<taxonomicName id="53E7C0C7FFA9493BEC0EFF7E643612AF" authority="(Boulenger)" baseAuthorityName="Boulenger" baseAuthorityYear="1887" box="[889,1301,190,214]" class="Amphibia" family="Ranidae" genus="Meristogenys" kingdom="Animalia" order="Anura" pageId="6" pageNumber="533" phylum="Chordata" rank="species" species="whiteheadi">
<emphasis id="A6936756FFA9493BEC0EFF7E65AF12AC" bold="true" box="[889,1164,190,214]" italics="true" pageId="6" pageNumber="533">Meristogenys whiteheadi</emphasis>
(Boulenger)
</taxonomicName>
(
<taxonomicName id="53E7C0C7FFA9493BEA52FF7E64A312AF" authorityName="Batsch" authorityYear="1796" box="[1317,1408,190,213]" class="Amphibia" family="Ranidae" kingdom="Animalia" order="Anura" pageId="6" pageNumber="533" phylum="Chordata" rank="family">Ranidae</taxonomicName>
)
</heading>
