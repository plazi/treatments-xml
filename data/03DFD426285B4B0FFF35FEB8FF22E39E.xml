<taxonomicName id="4C761EB3285B4B0FFD65FB96FC9FE366" box="[711,840,1089,1114]" class="Insecta" family="Cecidomyiidae" genus="Gynapteromyia" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="220" phylum="Arthropoda" rank="species" species="temburong">Temburong</taxonomicName>
