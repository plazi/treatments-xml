<geoCoordinate id="EE60506BFFA12E6666FBF945D094CD23" box="[659,731,1788,1812]" degrees="139" direction="east" orientation="longitude" pageId="77" pageNumber="78" precision="55555" value="139.0">139°E</geoCoordinate>
