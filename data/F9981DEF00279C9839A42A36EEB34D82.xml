<paragraph id="5C2A6D620A8BF8D09AFEF7F9EA098AB7" pageId="0" pageNumber="1">
<emphasis id="4F98898472FB2C7F1C27AF3D7D086C3E" bold="true" italics="true" pageId="0" pageNumber="1">Holotype</emphasis>
<taxonomicName id="71D84A11B5CB3396A7E44BB37887CEB7" authorityName="Eggers" authorityYear="1923" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus fornicatior" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="fornicatior">
<emphasis id="3CF0C39713832DAD5A7DAC61B3003FAE" italics="true" pageId="0" pageNumber="1">Xyleborus fornicatior</emphasis>
