<emphasis id="1B4C7055431CD5ED12AA0B5108625DA4" bold="true" italics="true" pageId="0" pageNumber="1">Lectotype</emphasis>
<taxonomicName id="D85562E8886DE519C02BAA8954BEFDED" authorityName="Schedl" authorityYear="1951" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus schultzei" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="schultzei">
