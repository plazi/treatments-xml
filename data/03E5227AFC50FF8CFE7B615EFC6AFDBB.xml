<taxonomicName id="4C4CE8EFFC53FF8DFE4D61F1FDE5FC3E" authorityName="Pelsue" authorityYear="2001" box="[449,592,953,972]" class="Insecta" family="Curculionidae" genus="Shigizo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="183" phylum="Arthropoda" rank="species" species="obrieni" status="n. sp.">
