<collectingCountry id="89AD7610FFD5F559FE4B5AB7662DFCDE" box="[429,543,765,791]" name="Colombia" pageId="10" pageNumber="161">Colombia</collectingCountry>
. A 
<typeStatus id="2E018822FFD5F559FDA45AB766ABFCDE" box="[578,665,765,791]" pageId="10" pageNumber="161">syntype</typeStatus>
