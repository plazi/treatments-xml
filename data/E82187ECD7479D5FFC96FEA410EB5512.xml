<collectionCode id="0699AE3FD7479D5FFC68FD9C10425462" box="[980,1030,537,561]" pageId="3" pageNumber="794">SVL</collectionCode>
<quantity id="A7709B1FD7479D5FFBB7FD9C10105462" box="[1035,1108,537,561]" metricMagnitude="-2" metricUnit="m" metricValue="6.8" pageId="3" pageNumber="794" unit="mm" value="68.0">68 mm</quantity>
