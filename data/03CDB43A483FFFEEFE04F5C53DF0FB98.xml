<taxonomicNameLabel id="A2236445483CFFEEFE39F3233C1DFC9B" box="[442,533,871,894]" pageId="2" pageNumber="2277" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="4C647EAF483CFFEEFDD0F3233CEDFC9B" authority="Park &amp; Cho, 2015" authorityName="Park &amp; Cho" authorityYear="2015" box="[595,741,870,894]" class="Malacostraca" family="Parabathynellidae" genus="Nipponbathynella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Bathynellacea" pageId="2" pageNumber="2277" phylum="Arthropoda" rank="species" species="shigaensis" status="sp. nov.">
