<taxonomicName id="4C12E9ADE36DE21EFF1FA4425B48FAD5" authority="DeVries, 2019" authorityName="DeVries" authorityYear="2019" box="[190,324,1281,1305]" class="Gastropoda" family="Turritellidae" genus="Turritella" kingdom="Animalia" pageId="37" pageNumber="1569" phylum="Mollusca" rank="species" species="capistrata" status="sp. nov.">
