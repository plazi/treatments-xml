<taxonomicName id="549BF2EF8303B26CF70BA1B1FBAFA297" authority=", Tyee Spring, Skamania Co., WA" authorityName="Tyee Spring, Skamania Co., WA" box="[574,1075,1717,1739]" class="Insecta" family="Perlodidae" genus="Isoperla" kingdom="Animalia" order="Plecoptera" pageId="20" pageNumber="54" phylum="Arthropoda" rank="species" species="rainiera">
