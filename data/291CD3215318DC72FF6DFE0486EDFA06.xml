<quantity id="664DCFD25318DC72FD9EFCE381B5FCA5" box="[612,728,793,820]" metricMagnitude="-2" metricUnit="m" metricValue="1.6219999999999999" pageId="15" pageNumber="466" unit="mm" value="16.22">16.22 mm</quantity>
, HW= 
<quantity id="664DCFD25318DC72FCD0FCE380FDFCA5" box="[810,912,793,820]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="15" pageNumber="466" unit="mm" value="3.1">3.10 mm</quantity>
, PL= 
<quantity id="664DCFD25318DC72FC29FCE38757FCA5" box="[979,1082,793,820]" metricMagnitude="-3" metricUnit="m" metricValue="3.32" pageId="15" pageNumber="466" unit="mm" value="3.32">3.32 mm</quantity>
, PW= 
<quantity id="664DCFD25318DC72FB7CFCE38780FCA5" box="[1158,1261,793,820]" metricMagnitude="-3" metricUnit="m" metricValue="3.83" pageId="15" pageNumber="466" unit="mm" value="3.83">3.83 mm</quantity>
, EL= 
<quantity id="664DCFD25318DC72FACBFCE386F5FCA5" box="[1329,1432,793,820]" metricMagnitude="-3" metricUnit="m" metricValue="9.52" pageId="15" pageNumber="466" unit="mm" value="9.52">9.52 mm</quantity>
, EW= 
<quantity id="664DCFD25318DC72FF20FCC7822CFCC9" box="[218,321,829,856]" metricMagnitude="-3" metricUnit="m" metricValue="3.08" pageId="15" pageNumber="466" unit="mm" value="3.08">3.08 mm</quantity>
