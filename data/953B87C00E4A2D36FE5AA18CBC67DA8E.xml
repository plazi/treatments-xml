<subSubSection id="5588655D0E4B2D36FF19A5B7BC67DA8E" pageId="13" pageNumber="1269" type="etymology">
<paragraph id="1D2D36D60E4B2D36FF19A5B7BF38DAC1" blockId="13.[211,1171,1180,1275]" box="[211,331,1180,1204]" pageId="13" pageNumber="1269">
<heading id="466581BA0E4B2D36FF19A5B7BF38DAC1" box="[211,331,1180,1204]" fontSize="10" level="3" pageId="13" pageNumber="1269" reason="5">
