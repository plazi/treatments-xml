<taxonomicName id="4C084D7C2D6CB46DFCCA10DEFBB0CBC6" authorityName="Hardy and Kaneshiro" authorityYear="1968" box="[770,902,187,213]" class="Agaricomycetes" family="Psathyrellaceae" genus="Drosophila" kingdom="Fungi" order="Agaricales" pageId="29" pageNumber="30" phylum="Basidiomycota" rank="species" species="orphnopeza">
