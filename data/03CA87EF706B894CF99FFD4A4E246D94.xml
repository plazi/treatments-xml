<taxonomicName id="4C634D7A706B894CFC13FBAA4AC66D00" box="[1307,1436,1054,1078]" class="Gastropoda" family="Areneidae" genus="Cinysca" higherTaxonomySource="CoL" kingdom="Animalia" order="Trochida" pageId="73" pageNumber="74" phylum="Mollusca" rank="species" species="forticostata">
