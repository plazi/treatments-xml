<keyLead id="AE4D95B53262273FD0B7FE53FA14FE40" pageId="5" pageNumber="276">
4. Leaves linear-lanceolate, 
<quantity id="D2449B653262273FD1F6FE53FDFFFE5F" box="[457,576,428,450]" metricMagnitude="-2" metricUnit="m" metricValue="1.225" metricValueMax="1.5" metricValueMin="0.95" pageId="5" pageNumber="276" unit="cm" value="1.225" valueMax="1.5" valueMin="0.95">0.95–1.5 cm</quantity>
