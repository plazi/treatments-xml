<quantity id="D2449B653262273FD184FDA7FE43FDF0" box="[443,508,600,622]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" pageId="5" pageNumber="276" unit="cm" value="1.5">1.5 cm</quantity>
wide; flowers pale yellow, lip pure white; dorsal sepal 
<quantity id="D2449B653262273FD42EFDA6FBEBFDF0" box="[1041,1108,601,622]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" pageId="5" pageNumber="276" unit="mm" value="12.0">12 mm</quantity>
long; lip 
<quantity id="D2449B653262273FD48DFDA6FB4AFDF0" box="[1202,1269,601,622]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="5" pageNumber="276" unit="mm" value="13.0">13 mm</quantity>
long, midlobe 
<quantity id="D2449B653262273FD5B8FDA6FF47FD15" metricMagnitude="-3" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="5" pageNumber="276" unit="mm" value="8.5" valueMax="9.0" valueMin="8.0">8–9 mm</quantity>
wide ..................................................................................................................................................................... 
