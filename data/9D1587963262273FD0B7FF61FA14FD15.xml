<taxonomicName id="D2BC4D033262273FD53EFE36FA14FE40" box="[1281,1451,456,478]" class="Liliopsida" family="Orchidaceae" genus="Dendrobium" kingdom="Plantae" order="Asparagales" pageId="5" pageNumber="276" phylum="Tracheophyta" rank="species" species="hollandianum">
<emphasis id="27C8EA923262273FD53EFE36FA14FE40" bold="true" box="[1281,1451,456,478]" italics="true" pageId="5" pageNumber="276">D. hollandianum</emphasis>
