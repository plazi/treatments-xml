(
<quantity id="4CBEE1EDE27FFFF8D293F9C6FBCBF9D2" box="[1009,1099,1629,1653]" metricMagnitude="0" metricUnit="m" metricValue="8.0" metricValueMax="12.0" metricValueMin="4.0" pageId="12" pageNumber="621" unit="m" value="8.0" valueMax="12.0" valueMin="4.0">4–12 m</quantity>
