<taxonomicName id="EAC14C584E4B731A2E722E705432866B" LSID-ZBK="urn:lsid:zoobank.org:act:266E0DDA-BB13-4ED8-9787-0D32AD2D14CD" class="Actinopterygii" family="Liparidae" genus="Psednos" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Psednos steini Chernova 2001:502" order="Scorpaeniformes" pageNumber="20" phylum="Chordata" rank="species" species="steini">Psednos steini Chernova 2001</taxonomicName>
