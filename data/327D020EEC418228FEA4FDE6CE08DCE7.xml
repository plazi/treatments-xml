<taxonomicName id="7DD4C89BEC41822AFFBAFD93CDD9DFD3" box="[91,193,626,650]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
