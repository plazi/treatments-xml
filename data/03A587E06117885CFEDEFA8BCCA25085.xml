<taxonomicName id="4C0C4D756116885BFE4AFBEECF295728" authority="Ruta &amp; Yoshitomi &amp; Klausnitzer, 2013" authorityName="Ruta &amp; Yoshitomi &amp; Klausnitzer" authorityYear="2013" box="[454,606,1100,1118]" class="Insecta" family="Scirtidae" genus="Dermestocyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="264" phylum="Arthropoda" rank="species" species="apiciconcavus" status="sp. nov.">
<emphasis id="B978EAE46116885BFE4AFBEECF295728" box="[454,606,1100,1118]" italics="true" pageId="11" pageNumber="264">D. apiciconcavus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24B579F6116885BFDEAFBE9CFDA5728" box="[614,685,1099,1118]" pageId="11" pageNumber="264" rank="species">sp. nov.</taxonomicNameLabel>
(44–47), 
<taxonomicName id="4C0C4D756116885BFC89FBEECEB05728" authority="(Pic, 1918)" baseAuthorityName="Pic" baseAuthorityYear="1918" box="[773,967,1099,1118]" class="Insecta" family="Scirtidae" genus="Cyphon" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="264" phylum="Arthropoda" rank="species" species="drianti">
<emphasis id="B978EAE46116885BFC89FBEECE2A5728" box="[773,861,1100,1118]" italics="true" pageId="11" pageNumber="264">D. drianti</emphasis>
(
<bibRefCitation id="EF9D4B076116885BFCE7FBE9CEB65728" author="PIC M." box="[875,961,1099,1118]" pageId="11" pageNumber="264" pagination="7 - 24" refId="ref15342" refString="PIC M. 1918: Nouveautes diverses. Melange Exotico-Entomologiques 29: 7 - 24." type="journal article" year="1918">Pic, 1918</bibRefCitation>
