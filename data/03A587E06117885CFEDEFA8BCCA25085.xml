<quantity id="4CF49B136111885CFDEFFCD9CFB550E5" box="[611,706,891,915]" metricMagnitude="-3" metricUnit="m" metricValue="3.05" pageId="12" pageNumber="265" unit="mm" value="3.05">3.05 mm</quantity>
&amp; 
