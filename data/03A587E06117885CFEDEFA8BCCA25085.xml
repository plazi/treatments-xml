<caption id="DF73667E6116885BFFFDFBE9CC5D57E5" ID-DOI="http://doi.org/10.5281/zenodo.5740405" ID-Zenodo-Dep="5740405" httpUri="https://zenodo.org/record/5740405/files/figure.png" pageId="11" pageNumber="264" startId="11.[113,153,1099,1118]" targetBox="[113,1088,219,1091]" targetPageId="11">
<paragraph id="8BB336F66116885BFFFDFBE9CC5D57E5" blockId="11.[113,1088,1098,1171]" pageId="11" pageNumber="264">
