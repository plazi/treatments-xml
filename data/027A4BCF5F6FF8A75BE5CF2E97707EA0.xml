<paragraph id="41116CAF5476D9FAC52514D510BF1BF3" pageId="12" pageNumber="151">
<taxonomicName id="C6CF44469CFE09BF3E435F8C7EFD6EC6" authority="Guenée, 1839" authorityName="Guenée" authorityYear="1839" class="Insecta" family="Noctuidae" genus="Taeniocampa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Taeniocampa perforata" order="Lepidoptera" pageId="12" pageNumber="151" phylum="Arthropoda" rank="species" species="perforata">Taeniocampa perforata</taxonomicName>
