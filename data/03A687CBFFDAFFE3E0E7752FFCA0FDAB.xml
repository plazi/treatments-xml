<collectingCountry id="F318764DFFDDFFE3E1177040FE05FDAB" box="[359,454,583,609]" name="Vietnam" pageId="29" pageNumber="30">Vietnam</collectingCountry>
(
<collectingRegion id="49CBF83FFFDDFFE3E1A47040FDB4FDAB" box="[468,631,583,609]" country="Vietnam" name="Phu Tho" pageId="29" pageNumber="30">Phu Tho Prov.</collectingRegion>
and 
<collectingRegion id="49CBF83FFFDDFFE3E2DD7040FC9FFDAB" box="[685,860,583,609]" country="Vietnam" name="Lang Son" pageId="29" pageNumber="30">Lang Son Prov.</collectingRegion>
)
</paragraph>
</subSubSection>
