5. 
<taxonomicName id="0774FCD2FCCE45A07C657F88F76D91D0" LSID="FBCA8D4B-5BCA-5053-B84C-F1399D3AC9DE" authority="Gagnep., Fl. Indo-Chine [Lecomte et al.]. 3: 1046 (1933 a); Bull. Soc. Bot. France 80: 77 (1933 b)" authorityName="Gagnep., Fl. Indo-Chine [Lecomte et al.]. 3: 1046 (1933 a); Bull. Soc. Bot. France 80: 77" authorityYear="1933" class="Magnoliopsida" family="Oleaceae" genus="Jasminum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Jasminum lang" order="Lamiales" pageId="0" pageNumber="55" phylum="Tracheophyta" rank="species" species="lang">Jasminum lang Gagnep., Fl. Indo-Chine [Lecomte et al.]. 3: 1046 (1933a); Bull. Soc. Bot. France 80: 77 (1933b)</taxonomicName>
