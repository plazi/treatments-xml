75. 
<taxonomicName id="A3C469CCF5483F294B14E67EFCC6C828" authority="Usinger, 1946" authorityName="Usinger" authorityYear="1946" box="[372,802,2326,2368]" class="Insecta" family="Miridae" genus="Orthotylellus" kingdom="Animalia" order="Hemiptera" pageId="68" pageNumber="79" phylum="Arthropoda" rank="species" species="rufescens" status="new species">Orthotylellus rufescens</taxonomicName>
, 
<taxonomicNameLabel id="4D837326F5483F29495BE67EFBF0C828" box="[827,1044,2326,2368]" pageId="68" pageNumber="79" rank="species">new species</taxonomicNameLabel>
