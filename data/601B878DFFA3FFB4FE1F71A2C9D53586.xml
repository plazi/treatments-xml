<taxonomicName id="2FB24D18FFA2FFB4FD9D70DBC9D53635" authority="Austini Wharton and Dangerfieldi 1993" authorityName="Austini Wharton and Dangerfieldi" authorityYear="1993" box="[586,1155,1268,1292]" class="Insecta" family="Braconidae" genus="Cercobarcon" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="555" phylum="Arthropoda" rank="species" species="niger">
