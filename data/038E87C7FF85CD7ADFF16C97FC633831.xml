<collectingRegion id="49E3F833FF82CD7ADE0B6998FE483A99" box="[365,434,1432,1458]" country="Italy" name="Sicilia" pageId="52" pageNumber="53">Sicily</collectingRegion>
(
<collectingCountry id="F3307641FF82CD7ADEA46998FE073A99" box="[450,509,1432,1458]" name="Italy" pageId="52" pageNumber="53">Italy</collectingCountry>
