<taxonomicName id="76A24D7FFF96FF98FD14ABEEFCD4FD21" authorityName=", Thorell" authorityYear="1890" box="[729,882,729,755]" class="Arachnida" family="Tetragnathidae" genus="Tetragnatha" kingdom="Animalia" order="Araneae" pageId="30" pageNumber="73" phylum="Arthropoda" rank="species" species="chauliodus">
