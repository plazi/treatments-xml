<heading id="F76AE037C651FFD1FF5F1B2FFB271961" bold="true" box="[151,1235,1572,1597]" fontSize="10" level="2" pageId="5" pageNumber="260" reason="2">
<emphasis id="9EE98B49C651FFD1FF5F1B2FFB271961" bold="true" box="[151,1235,1572,1597]" pageId="5" pageNumber="260">
<taxonomicName id="6B9D2CD8C651FFD1FF5F1B2FFDF21961" authority="Berest, 1991" authorityName="Berest" authorityYear="1991" box="[151,518,1572,1597]" class="Insecta" family="Cecidomyiidae" genus="Peromyia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="260" phylum="Arthropoda" rank="species" species="podolica">
<emphasis id="9EE98B49C651FFD1FF5F1B2FFE9A1961" bold="true" box="[151,366,1572,1597]" italics="true" pageId="5" pageNumber="260">Peromyia podolica</emphasis>
Berest, 
<number id="1A55457CC651FFD1FE071B2FFDF21961" box="[463,518,1572,1597]" pageId="5" pageNumber="260" value="1991.0">1991</number>
</taxonomicName>
â€”new junior synonym of 
<taxonomicName id="6B9D2CD8C651FFD1FCF41B2FFB271961" authority="Kieffer, 1901" authorityName="Kieffer" authorityYear="1901" box="[828,1235,1572,1597]" class="Insecta" family="Cecidomyiidae" genus="Peromyia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="260" phylum="Arthropoda" rank="species" species="fungicola">
<emphasis id="9EE98B49C651FFD1FCF41B2FFBD41961" bold="true" box="[828,1056,1572,1597]" italics="true" pageId="5" pageNumber="260">Peromyia fungicola</emphasis>
(Kieffer, 
<number id="1A55457CC651FFD1FB5A1B2FFB3F1961" box="[1170,1227,1572,1597]" pageId="5" pageNumber="260" value="1901.0">1901</number>
)
</taxonomicName>
</emphasis>
