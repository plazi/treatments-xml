<quantity id="654D9B46FE774C8CFE59D875FE147C18" box="[424,475,1798,1822]" metricMagnitude="-1" metricUnit="m" metricValue="7.62" pageId="481" pageNumber="482" unit="in" value="30.0">30 in</quantity>
