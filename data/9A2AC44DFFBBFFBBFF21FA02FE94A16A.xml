<heading id="4974C237FFBBFFBAFF21FA02FB3EA6E7" bold="true" box="[151,1104,1431,1457]" fontSize="11" level="1" pageId="18" pageNumber="19" reason="1">
<emphasis id="20F7A949FFBBFFBAFF21FA02FB3EA6E7" bold="true" box="[151,1104,1431,1457]" pageId="18" pageNumber="19">
Validity of 
<taxonomicName id="D5830ED8FFBBFFBAFE96FA02FD1AA6E7" authority="Poll 1944" authorityName="Poll" authorityYear="1944" box="[288,628,1431,1457]" class="Actinopterygii" family="Mochokidae" genus="Chiloglanis" kingdom="Animalia" order="Siluriformes" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="pojeri">
<emphasis id="20F7A949FFBBFFBAFE96FA02FE94A6E7" bold="true" box="[288,506,1431,1457]" italics="true" pageId="18" pageNumber="19">Chiloglanis pojeri</emphasis>
<bibRefCitation id="761208AAFFBBFFBAFDB6FA02FD1AA6E7" author="Poll" box="[512,628,1431,1457]" pageId="18" pageNumber="19" refString="Poll, M. (1944) Descriptions de poissons nouveaux recueillis dans la region d'Albertville (Congo belge) par le Dr. G. Pojer. Bulletin du Musee Royal d'Histoire Naturelle de Belgique, 20, 1 - 12." type="journal article" year="1944">
Poll 
<number id="A44B677CFFBBFFBAFD8FFA02FD1AA6E7" box="[569,628,1431,1457]" pageId="18" pageNumber="19" value="1944.0">1944</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="D5830ED8FFBBFFBAFD07FA02FB3EA6E7" authority="Poll 1952" authorityName="Poll" authorityYear="1952" box="[689,1104,1431,1457]" class="Actinopterygii" family="Mochokidae" genus="Chiloglanis" kingdom="Animalia" order="Siluriformes" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="micropogon">
<emphasis id="20F7A949FFBBFFBAFD07FA02FCB8A6E7" bold="true" box="[689,982,1431,1457]" italics="true" pageId="18" pageNumber="19">Chiloglanis micropogon</emphasis>
<bibRefCitation id="761208AAFFBBFFBAFC6BFA02FB3EA6E7" author="Poll" box="[989,1104,1431,1457]" pageId="18" pageNumber="19" refString="Poll, M. (1952) Poissons de rivieres de la region des lacs Tanganika et Kivu recuellis par G. Marlier. Revue de Zoologie et de Botanique Africaines, 46, 3 - 4, 221 - 236." type="journal article" year="1952">
Poll 
<number id="A44B677CFFBBFFBAFBA3FA02FB3EA6E7" box="[1045,1104,1431,1457]" pageId="18" pageNumber="19" value="1952.0">1952</number>
