<taxonomicName id="FDE0848EFFA5B257F578FF37FD80A419" authority="Polejaeff, 1883" authorityName="Polejaeff" authorityYear="1883" box="[305,612,251,275]" class="Calcarea" family="Grantiidae" genus="Ute" kingdom="Animalia" order="Leucosolenida" pageId="30" pageNumber="233" phylum="Porifera" rank="species" species="argentea">
