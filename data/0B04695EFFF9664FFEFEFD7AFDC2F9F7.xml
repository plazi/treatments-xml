<taxonomicName id="ECF8FF64644D99108C15660B4D605BF5" authority="Baroni Urbani et de Andrade, 2003" authorityName="Baroni Urbani" authorityYear="2003" box="[255,538,1481,1503]" class="Insecta" family="Formicidae" genus="Proceratium" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="143" phylum="Arthropoda" rank="species" species="gibberum">
