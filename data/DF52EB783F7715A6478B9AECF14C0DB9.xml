<taxonomicName id="EE9AC28D279601CE82A883FDF6DE8968" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143167" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica exercita Walker" lsidName-HNS="Formica exercita Walker" order="Hymenoptera" pageNumber="370" phylum="Arthropoda" rank="species" species="exercita">Formica exercita</taxonomicName>
