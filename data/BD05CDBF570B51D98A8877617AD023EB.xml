Live 
<taxonomicName id="B8BB4F8F66795DCAE2F12B77A89DBFE0" authority="Cumming et al., 2021" authorityName="Cumming &amp; Bank &amp; Bresseel &amp; Constant &amp; Tirant &amp; Dong &amp; Sonet &amp; Bradler" authorityYear="2021" baseAuthorityName="Cumming &amp; Le Tirant" baseAuthorityYear="2020" class="Insecta" family="Phylliidae" genus="Cryptophyllium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptophyllium oyae" order="Phasmatodea" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="oyae">
