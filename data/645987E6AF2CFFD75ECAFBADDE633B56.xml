<collectingCountry id="94E77660AF2CFFD75A7BFB5ED96A3B22" box="[1062,1144,1263,1287]" name="Greece" pageId="13" pageNumber="14">Greece</collectingCountry>
, 
<collectingCountry id="94E77660AF2CFFD75AD9FB5FD9C63B22" box="[1156,1236,1262,1287]" name="Turkey" pageId="13" pageNumber="14">Turkey</collectingCountry>
