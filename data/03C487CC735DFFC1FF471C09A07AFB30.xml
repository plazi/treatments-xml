<collectingRegion id="49A9F838735DFFC1FB691DCEA770FC52" box="[1262,1362,942,968]" country="Bangladesh" name="Sylhet" pageId="27" pageNumber="26">SYLHET</collectingRegion>
<collectionCode id="ED7CAE1F735DFFC1FADC1DCFA321FC70" pageId="27" pageNumber="26">DIVI- SION</collectionCode>
