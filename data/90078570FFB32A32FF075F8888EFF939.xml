<quantity id="DF569983FFB32A32FBD059338DBBFA3D" box="[1088,1226,1398,1423]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="4.5" metricValueMin="3.5" pageId="22" pageNumber="23" unit="mm" value="4.0" valueMax="4.5" valueMin="3.5">3.5–4.5 mm</quantity>
