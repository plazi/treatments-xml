<taxonomicName id="9CEC4D62FFFB2F6BFDFFC458FCFC5F14" box="[603,792,1350,1374]" class="Magnoliopsida" family="Anacardiaceae" genus="Pistacia" kingdom="Plantae" order="Sapindales" pageId="5" pageNumber="923" phylum="Tracheophyta" rank="species" species="lentiscus">
