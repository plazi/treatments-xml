<taxonomicName id="9CEC4D62FFFB2F6BFBE7C43BFAA15F71" box="[1091,1349,1316,1339]" class="Pinopsida" family="Cupressaceae" genus="Cupressus" kingdom="Plantae" order="Pinales" pageId="5" pageNumber="923" phylum="Tracheophyta" rank="species" species="sempervirens">
