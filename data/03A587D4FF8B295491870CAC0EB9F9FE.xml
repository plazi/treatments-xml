<taxonomicName id="4C0C4D41FF8B295492F70CE3089FFF67" box="[999,1177,216,241]" class="Aves" family="Scolopacidae" genus="Tringa" kingdom="Animalia" order="Charadriiformes" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="totanus">
