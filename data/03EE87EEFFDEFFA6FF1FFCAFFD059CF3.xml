<specimenCount id="9D41FD71FFDEFFA6FE95FBAAFE069D37" box="[285,414,1122,1146]" pageId="22" pageNumber="23" type="generic">1 specimen</specimenCount>
; ANT XV #142: 
<specimenCount id="9D41FD71FFDEFFA6FDE9FBAAFD7B9D37" box="[609,739,1122,1146]" pageId="22" pageNumber="23" type="generic">1 specimen</specimenCount>
