(
<figureCitation id="137C2A6AFFE238332CCAF3ACFEEDFB1F" box="[159,273,1215,1239]" captionStart="FIGURE 2. a – d" captionStartId="8.[151,250,1574,1597]" captionTargetBox="[185,1407,567,1535]" captionTargetId="figure@8.[172,1415,555,1552]" captionTargetPageId="8" captionText="FIGURE 2. a – d. Kapsa (Rigida) furcata Cao &amp; Zhang sp. nov.; e – h. Kapsa (Rigida? imminuta Yang &amp; Zhang sp. nov.; i – l. Kapsa (Rigida? megaprocessa Cao &amp; Zhang sp. nov.; m – p. Kapsa (Rigida) serrata Cao &amp; Zhang sp. nov." httpUri="https://zenodo.org/record/222872/files/figure.png" pageId="16" pageNumber="133">
Figs 
<number id="3D8F24C8FFE238332C82F3ACFF19FB1F" box="[215,229,1215,1239]" pageId="16" pageNumber="133" value="2.0">2</number>
a–d
</figureCitation>
, 
<figureCitation id="137C2A6AFFE238332D49F3ACFEC5FB1F" box="[284,313,1215,1239]" captionStart="FIGURE 10" captionStartId="17.[151,250,1159,1182]" captionTargetBox="[192,1392,189,1136]" captionTargetId="figure@17.[176,1410,171,1150]" captionTargetPageId="17" captionText="FIGURE 10. Kapsa (Rigida) furcata Cao &amp; Zhang sp. nov. a. forewing; b. hind wing; c. abdominal apodemes; d. genital capsule; e. pygofer dorsal appendage and anal tube appendage, lateral view; f. subgenital plate, paramere and connective, dorsal view; g. subgenital plate, dorsal view; h. paramere, lateral view; i. connective; j. aedeagus, lateral view; k. aedeagus, ventral view." httpUri="https://zenodo.org/record/222880/files/figure.png" pageId="16" pageNumber="133">
<number id="3D8F24C8FFE238332D49F3ACFEC5FB1F" box="[284,313,1215,1239]" pageId="16" pageNumber="133" value="10.0">10</number>
