<taxonomicName id="8E506B50E76E2168C9FA725930DF28E4" class="Insecta" family="Ichneumonidae" genus="Syrphoctonus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Syrphoctonus" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Syrphoctonus</taxonomicName>
