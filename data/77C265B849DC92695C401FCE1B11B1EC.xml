<paragraph id="49590A3215D0FEAF918C23EA8B2E27C1" pageId="0" pageNumber="9042">
<taxonomicName id="B02F254E646E9C01BECB5986583E9B45" authority="Forster, 1869" authorityName="Forster" authorityYear="1869" class="Insecta" family="Hymenoptera" genus="Homotropus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Homotropus" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Homotropus</taxonomicName>
