<taxonomicName id="4C024D41CE53FF81FE2E96ECE84CF984" authorityName="DANIEL &amp; DANIEL" authorityYear="1898" box="[444,628,1526,1544]" class="Insecta" family="Curculionidae" genus="Otiorhynchus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="55" phylum="Arthropoda" rank="species" species="gredleri">
