<subSubSection id="A39CDEADD4B307CF1547DAC8CF3DE65A" pageId="2" pageNumber="47" type="nomenclature">
<paragraph id="957C8EACF58269D37822A2310274E4FB" pageId="2" pageNumber="47">
Genus 
<taxonomicName id="AC105062097D95AF4336B11A611688E3" authority="Verhoeff, 1930" authorityName="Verhoeff" authorityYear="1930" class="Malacostraca" family="Agnaridae" genus="Mongoloniscus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mongoloniscus" order="Isopoda" pageId="2" pageNumber="47" phylum="Arthropoda" rank="genus">Mongoloniscus Verhoeff, 1930</taxonomicName>
