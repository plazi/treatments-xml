<quantity id="4C894D90E774FFBF0F980FFBFCA5FB74" box="[702,860,1254,1279]" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="1.75" metricValueMin="1.5" pageId="7" pageNumber="368" unit="m" value="1625.0" valueMax="1750.0" valueMin="1500.0">1500–1750 m</quantity>
