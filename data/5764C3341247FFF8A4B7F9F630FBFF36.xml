<number id="690560051246FFFBA56EF96435A8F92E" box="[334,385,1716,1741]" pageId="5" pageNumber="196" value="2.26">2.26</number>
, f 
<number id="690560051246FFFBA5BCF9643581F92E" box="[412,424,1716,1741]" pageId="5" pageNumber="196" value="1.0">1</number>
