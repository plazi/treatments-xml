<quantity id="1835DFC71246FFFBA04EF96430FCF92F" box="[1134,1237,1716,1741]" metricMagnitude="-3" metricUnit="m" metricValue="1.96" pageId="5" pageNumber="196" unit="mm" value="1.96">1.96 mm</quantity>
