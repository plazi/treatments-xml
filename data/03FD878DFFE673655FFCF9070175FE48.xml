<emphasis id="B920EA89FFE073645DC5F9750163F92D" box="[686,857,1760,1783]" italics="true" pageId="12" pageNumber="13">
<taxonomicName id="4C544D18FFE073645DC5F97500C3F92D" box="[686,761,1760,1783]" class="Gymnolaemata" family="Beaniidae" genus="Beania" higherTaxonomySource="CoL" kingdom="Animalia" order="Cheilostomatida" pageId="12" pageNumber="13" phylum="Bryozoa" rank="genus">Beania</taxonomicName>
