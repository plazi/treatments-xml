<taxonomicName id="700F9258A814A3173A2A085C8DD7BE6A" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus rameus" order="Lecanorales" pageId="2" pageNumber="107" phylum="Ascomycota" rank="species" species="rameus">Xyleborus rameus</taxonomicName>
