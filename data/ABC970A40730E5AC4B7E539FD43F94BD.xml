<taxonomicName id="63E850B886AA20B1DF711B12645318FC" authority="Hulcr &amp; Cognato, 2010" authorityName="Hulcr &amp; Cognato" authorityYear="2010" class="Insecta" family="Curculionidae" genus="Planiculus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Planiculus bicolor" order="Coleoptera" pageId="2" pageNumber="107" phylum="Arthropoda" rank="species" species="bicolor">Planiculus bicolor</taxonomicName>
