<taxonomicName id="0384E9D4225E4BBD40A442E23770F3E7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:228755" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Crematogaster (Sphaerocrema) amita caffra Santschi" order="Hymenoptera" pageNumber="94" phylum="Arthropoda" rank="variety" species="kneri" subGenus="Sphaerocrema" subSpecies="amita" variety="caffra">Crematogaster (Sphaerocrema) kneri Mayr st. amita Forel v. caffra</taxonomicName>
a. var. = 
<taxonomicName id="890D112CFA62EE461D07AC7B8CBE943D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28266" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Crematogaster kneri Mayr" order="Hymenoptera" pageNumber="94" phylum="Arthropoda" rank="species" species="kneri">C. kneri Arnold</taxonomicName>
