No. 
<number id="232824E38F1DFFBF33CEAE47B051E127" box="[483,545,1890,1916]" pageId="12" pageNumber="13" value="1450.0">1450</number>
. 
<typeStatus id="4A5B88668F1DFFBF3015AE47B0DAE127" box="[568,682,1890,1916]" pageId="12" pageNumber="13">Paratypes</typeStatus>
