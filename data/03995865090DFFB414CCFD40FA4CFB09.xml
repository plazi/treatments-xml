<collectingCountry id="F327A9E3090DFFB41528FB8BFE3FFBE1" box="[371,482,1025,1051]" name="Argentina" pageId="44" pageNumber="45">Argentina</collectingCountry>
(south to 
<collectingRegion id="49F42791090DFFB4160CFB88FD32FBE6" box="[599,751,1025,1052]" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="44" pageNumber="45">Buenos Aires</collectingRegion>
