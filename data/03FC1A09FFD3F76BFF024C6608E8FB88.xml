species: 
<taxonomicName id="4C55D09CFFD3F76BFEA54CC60B9FFC48" authority="Fabricius 1775" authorityName="Fabricius" authorityYear="1775" box="[359,726,931,955]" class="Insecta" family="Geotrupidae" genus="Geotrupes" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="11" phylum="Arthropoda" rank="species" species="pan">
