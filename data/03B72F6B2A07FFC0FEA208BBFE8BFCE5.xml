<taxonomicName id="4C1EE5FE2A07FFC7FE5408BBFD4AFA65" authority="Gee &amp; Mu, 2000" authorityName="Gee &amp; Mu" authorityYear="2000" box="[426,649,1412,1436]" class="Maxillopoda" family="Cletodidae" genus="Neoacrenhydrosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="3" pageNumber="810" phylum="Arthropoda" rank="genus" status="gen. nov.">
