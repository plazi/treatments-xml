<quantity id="4CD99B620A1D2F1CFE25734F8F8BFAE8" box="[422,490,1382,1406]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="5" pageNumber="2276" unit="mm" value="8.0">8 mm</quantity>
