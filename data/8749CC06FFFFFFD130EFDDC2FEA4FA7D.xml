<taxonomicName id="C8E00693FFF9FFD132D1DB2DFCFDFA99" box="[681,833,1379,1403]" class="Insecta" family="Formicidae" genus="Cardiocondyla" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="353" phylum="Arthropoda" rank="species" species="compressa">
