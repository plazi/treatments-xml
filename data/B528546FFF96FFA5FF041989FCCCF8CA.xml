<taxonomicName id="FA819EFAFF96FFA5FE091834FDF9F8CB" authorityName="Stephani" authorityYear="1913" baseAuthorityName="Spruce" box="[389,559,1814,1840]" class="Jungermanniopsida" family="Lejeuneaceae" genus="Prionolejeunea" kingdom="Plantae" order="Porellales" pageId="24" pageNumber="225" phylum="Marchantiophyta" rank="species" species="macrocardia">
<emphasis id="0FF5396BFF96FFA5FE091834FDF9F8CB" box="[389,559,1814,1840]" italics="true" pageId="24" pageNumber="225">P. macrocardia</emphasis>
</taxonomicName>
