<subSubSection id="C32665172D71FFD2FF66FD284E80FD4A" box="[199,1267,655,680]" pageId="2" pageNumber="283" type="etymology">
<paragraph id="8B83369C2D71FFD2FF66FD284E80FD4A" blockId="2.[151,1436,151,896]" box="[199,1267,655,680]" pageId="2" pageNumber="283">
<emphasis id="B948EA8E2D71FFD2FF66FD284B3EFD45" box="[199,333,655,680]" inLineHeading="true" pageId="2" pageNumber="283" reason="1">
<emphasis id="B948EA8E2D71FFD2FF66FD284B35FD4A" bold="true" box="[199,326,655,680]" pageId="2" pageNumber="283">Etymology</emphasis>
.
</emphasis>
