<taxonomicName id="01E691AAFFBF871CFF0FCB94FE18F6DD" authority="Panzer" authorityName="Panzer" box="[237,463,1184,1210]" class="Magnoliopsida" family="Apocynaceae" genus="Anisotoma" kingdom="Plantae" order="Gentianales" pageId="68" pageNumber="69" phylum="Tracheophyta" rank="genus">
