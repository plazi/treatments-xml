<taxonomicName id="D180DCD20521B06612E4FE2E01AE7E41" authority="Rasnitsyn, 1969" authorityName=": Rasnitsyn" authorityYear="1969" box="[1162,1436,505,527]" class="Insecta" family="Anaxyelidae" genus="Urosyntexis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="344" phylum="Arthropoda" rank="genus">
