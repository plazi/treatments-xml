<taxonomicName id="AFF34D24FFBEB17613D37DCEFC934CC1" authorityName="Lowry &amp; Berents" authorityYear="1996" box="[699,828,1681,1707]" class="Malacostraca" family="Ischyroceridae" genus="Notopoma" kingdom="Animalia" order="Amphipoda" pageId="24" pageNumber="99" phylum="Arthropoda" rank="species" species="moorea">
