<subSubSection id="C30F651DFFC8FF96FF38739CFCBEFD19" box="[199,832,700,725]" pageId="3" pageNumber="292" type="etymology">
<paragraph id="8BAA3696FFC8FF96FF38739CFCBEFD19" blockId="3.[151,1436,151,905]" box="[199,832,700,725]" pageId="3" pageNumber="292">
<emphasis id="B961EA84FFC8FF96FF38739CFEB0FD19" bold="true" box="[199,334,700,725]" inLineHeading="true" pageId="3" pageNumber="292" reason="1">Etymology.</emphasis>
