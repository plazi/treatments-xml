<taxonomicName id="4C614D32A82FFFD5FD6D6647FB8AE665" authority="Van Soest &amp; De Voogd, 2015" authorityName="Van Soest &amp; De Voogd" authorityYear="2015" box="[644,1146,1346,1371]" class="Calcarea" family="Clathrinidae" genus="Ernstia" kingdom="Animalia" order="Clathrinida" pageId="24" pageNumber="325" phylum="Porifera" rank="species" species="klautauae">
