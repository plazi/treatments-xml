<taxonomicName id="A5909E367A0434F864425511C7CE5A47" class="Polychaeta" family="Onuphidae" genus="Onuphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Onuphis conchylega" order="Eunicida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="conchylega">Onuphis conchylega</taxonomicName>
