<taxonomicName id="239738B3FFA1790AFD16F8E4FB30F899" authority="Caceres-Chamizo et al. 2017" authorityName="Caceres-Chamizo et al." authorityYear="2017" box="[706,1152,1834,1859]" class="Gymnolaemata" family="Exechonellidae" genus="Exechonella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="4" pageNumber="159" phylum="Bryozoa" rank="species" species="vieirai">
