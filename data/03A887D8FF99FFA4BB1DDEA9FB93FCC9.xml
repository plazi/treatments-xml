<taxonomicName id="4C014D4DFF99FFA6B854D8B2FE23F936" authorityName="Huber" authorityYear="2000" box="[331,471,1672,1693]" class="Arachnida" family="Pholcidae" genus="Tupigea" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="297" phylum="Arthropoda" rank="species" species="maza">
