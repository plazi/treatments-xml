<taxonomicName id="95804D40FFFFDF491206FA66378E0234" box="[262,391,1360,1383]" class="Insecta" family="Cantharidae" genus="Micropodabrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="67" phylum="Arthropoda" rank="species" species="wittmeri">
