<number id="143D030EFFD3FFBEFF50A71614AAF802" box="[151,164,1910,1931]" pageId="22" pageNumber="383" value="5.0">5</number>
(
<number id="143D030EFFD3FFBEFF6CA71614B7F802" box="[171,185,1910,1931]" pageId="22" pageNumber="383" value="4.0">4</number>
