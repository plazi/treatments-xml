<taxonomicName id="4C0A6090FFBC4340FB18AEACFAEA59D0" box="[1157,1301,1159,1184]" class="Malacostraca" family="Talitridae" genus="Notorchestia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="562" phylum="Arthropoda" rank="genus">
