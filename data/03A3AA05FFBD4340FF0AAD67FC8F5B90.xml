. 
<taxonomicName id="4C0A6090FFBC4340FE4AAEACFD9159D0" box="[471,622,1159,1184]" class="Malacostraca" family="Talitridae" genus="Mexorchestia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="562" phylum="Arthropoda" rank="genus">
