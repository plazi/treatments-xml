<collectingRegion id="49828DA6F3097F6E16E0A79FFCD7F9BA" box="[712,804,1516,1541]" country="Angola" name="Malanje" pageId="13" pageNumber="214">Malanje</collectingRegion>
and 
<collectingRegion id="49828DA6F3097F6E1772A79EFC11F9BA" box="[858,994,1516,1541]" country="Angola" name="Kwanza Sul" pageId="13" pageNumber="214">
Kwanza 
<collectingRegion id="49828DA6F3097F6E1795A79FFC11F9BA" box="[957,994,1516,1541]" country="Tanzania" name="Kusini Unguja" pageId="13" pageNumber="214">Sul</collectingRegion>
</collectingRegion>
Provinces.
