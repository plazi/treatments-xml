<paragraph id="8BB6ED732F0C9549AA0FF8FDE4FCF8D6" blockId="10.[826,1441,1820,1903]" pageId="10" pageNumber="48">
<taxonomicName id="4C0996F02F0C9549AA0FF8FDE4DBF8E9" authority="Issel, 1866" authorityName="Issel" authorityYear="1866" box="[826,1158,1850,1872]" class="Gastropoda" family="Bithyniidae" genus="Bithynia" kingdom="Animalia" order="Littorinimorpha" pageId="10" pageNumber="48" phylum="Mollusca" rank="species" species="lucensis">
