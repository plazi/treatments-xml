<heading id="D0B2F839EE3EFF9030DFFA77E94CC4FB" bold="true" box="[151,575,1495,1521]" fontSize="11" level="1" pageId="9" pageNumber="52" reason="1">
<emphasis id="B9319347EE3EFF9030DFFA77E94CC4FB" bold="true" box="[151,575,1495,1521]" pageId="9" pageNumber="52">
<taxonomicName id="4C4534D6EE3EFF9030DFFA77EAD2C4FB" box="[151,417,1495,1521]" class="Insecta" family="Psychomyiidae" genus="Paduniella" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="52" phylum="Arthropoda" rank="species" species="sabarmata" status="new species">
<emphasis id="B9319347EE3EFF9030DFFA77EAD2C4FB" bold="true" box="[151,417,1495,1521]" italics="true" pageId="9" pageNumber="52">Paduniella sabarmata</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A2022E3CEE3EFF9031E7FA77E94CC4FB" box="[431,575,1495,1521]" pageId="9" pageNumber="52" rank="species">new species</taxonomicNameLabel>
</emphasis>
