<taxonomicName id="438A4D63FFA6FF8DFF3E80BDFE12FD54" authority="Kieffer, 1905" authorityName="Kieffer" authorityYear="1905" box="[189,509,725,752]" class="Insecta" family="Calophyidae" genus="Cecidopsylla" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="143" phylum="Arthropoda" rank="genus">
<emphasis id="B6FEEAF2FFA6FF8DFF3E80BDFEB9FD4B" box="[189,342,725,751]" italics="true" pageId="6" pageNumber="143">Cecidopsylla</emphasis>
