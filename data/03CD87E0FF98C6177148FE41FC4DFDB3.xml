<taxonomicName id="4C644D75FF98C6177670FE21FB78FE32" authority="Forel" authorityName="Forel" authorityYear="1892" box="[1031,1220,464,488]" class="Insecta" family="Formicidae" genus="Cardiocondyla" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="248" phylum="Arthropoda" rank="species" species="stambuloffii">Stambuloffii Forel</taxonomicName>
