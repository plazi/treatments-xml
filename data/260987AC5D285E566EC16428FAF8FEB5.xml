<treatmentCitationGroup id="8EB011945D285E556EC16472FBCAF840" pageId="1" pageNumber="60">
<taxonomicName id="69A04D395D285E556EC16472FDB4F8AD" authority="Simon, 1903: 38" authorityName="Simon" authorityPageNumber="38" authorityYear="1903" box="[151,563,1872,1896]" class="Arachnida" family="Pisauridae" genus="Hypsithylla" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="60" phylum="Arthropoda" rank="species" species="linearis">
<emphasis id="9CD4EAA85D285E556EC16472FEE9F8A2" box="[151,366,1873,1896]" italics="true" pageId="1" pageNumber="60">Hypsithylla linearis</emphasis>
<treatmentCitation id="2F0110AB5D285E556F2E6473FDB4F8AD" author="Simon" box="[376,563,1872,1895]" page="38" pageId="1" pageNumber="60" year="1903">
<bibRefCitation id="CA314B4B5D285E556F2E6473FD8FF8AD" author="Simon" box="[376,520,1872,1895]" pageId="1" pageNumber="60" refString="Simon, E. (1903) Descriptions d'arachnides nouveaux. Annales de la Societe Entomologique de Belgique, 47, 21 - 39." type="journal article" year="1903">
Simon, 
<number id="1868249D5D285E556F996473FD8FF8AD" box="[463,520,1872,1895]" pageId="1" pageNumber="60" value="1903.0">1903</number>
</bibRefCitation>
: 
<number id="1868249D5D285E556C406473FDB4F8AD" box="[534,563,1872,1895]" pageId="1" pageNumber="60" value="38.0">38</number>
</treatmentCitation>
</taxonomicName>
, immature female 
<typeStatus id="711B88185D285E556D506473FCE4F8AD" box="[774,867,1872,1895]" pageId="1" pageNumber="60">holotype</typeStatus>
from Antongil, 
<collectingCountry id="D6B7762A5D285E556A4B6473FB27F8AD" box="[1053,1184,1872,1895]" name="Madagascar" pageId="1" pageNumber="60">Madagascar</collectingCountry>
, 
