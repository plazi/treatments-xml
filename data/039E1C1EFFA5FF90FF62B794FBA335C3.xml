<heading id="D0C01A64FFA5FF90FF62B794FD8D34E3" bold="true" box="[151,546,1728,1755]" fontSize="11" level="1" pageId="4" pageNumber="105" reason="1">
<taxonomicName id="4C37D68BFFA5FF90FF62B794FD8D34E3" authority="Bartsch, 1985" authorityName="Bartsch" authorityYear="1985" box="[151,546,1728,1755]" class="Arachnida" family="Halacaridae" genus="Atelopsalis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="4" pageNumber="105" phylum="Arthropoda" rank="species" species="pacifica">
<emphasis id="B943711AFFA5FF90FF62B794FD8D34E3" bold="true" box="[151,546,1728,1755]" pageId="4" pageNumber="105">
<emphasis id="B943711AFFA5FF90FF62B794FED734E2" bold="true" box="[151,376,1728,1755]" italics="true" pageId="4" pageNumber="105">Atelopsalis pacifica</emphasis>
<bibRefCitation id="EFA6D0F9FFA5FF90FE8AB795FD8D34E3" author="Bartsch, I." box="[383,546,1729,1755]" pageId="4" pageNumber="105" pagination="21 - 35" refId="ref12934" refString="Bartsch, I. (1985) Halacaridae (Acari) from the Strangford Narrows and the Irish Sea. Proceedings of the Royal Irish Academy, Section B, 85, 21 - 35." type="book chapter" year="1985">Bartsch, 1985</bibRefCitation>
</emphasis>
</taxonomicName>
