<taxonomicName id="17B04D74FF97FF8621F05EF7FBCBFA4E" authorityName="Bravard" authorityYear="1828" box="[904,1062,1482,1503]" class="Mammalia" family="Cainotheriidae" genus="Cainotherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Artiodactyla" pageId="4" pageNumber="149" phylum="Chordata" rank="genus">
