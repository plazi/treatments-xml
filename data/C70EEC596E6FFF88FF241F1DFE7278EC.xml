<geoCoordinate id="2A933B886E6FFF88FBA11EFDFB327F8C" box="[1042,1179,908,933]" direction="east" orientation="longitude" pageId="74" pageNumber="75" precision="5" value="177.1698">
<number id="F96F4F686E6FFF88FBA11EFDFBD67F8C" box="[1042,1151,908,933]" pageId="74" pageNumber="75" value="177.1698">177.1698</number>
°E
</geoCoordinate>
, 
<quantity id="885FF0AA6E6FFF88FB161EFDFB477F8D" box="[1189,1262,908,933]" metricMagnitude="2" metricUnit="m" metricValue="3.76" pageId="74" pageNumber="75" unit="m" value="376.0">376 m</quantity>
