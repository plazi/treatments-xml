<taxonomicName id="4FE00387FF84AE27FF32FC603DD2F0D9" authority="Schedl, 1965" authorityName="Schedl" authorityYear="1965" box="[146,511,921,940]" class="Insecta" family="Curculionidae" genus="Macrocryphalus" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="oblongus">
