<taxonomicName id="6A5338242BAC100D61218F1EA84BAE27" authority="Marshall in André, 1896" authorityName="Marshall in André" authorityYear="1896" class="Insecta" family="Braconidae" genus="DYSCRITUS" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="DYSCRITUS" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">DYSCRITUS</taxonomicName>
