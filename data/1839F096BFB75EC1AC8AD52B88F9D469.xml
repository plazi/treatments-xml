<taxonomicName id="153E94107C666BB61C85269B25AEEEAD" authorityName="Hance" authorityYear="1869" class="Magnoliopsida" family="Lamiaceae" genus="Dracocephalum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dracocephalum rupestre" order="Lamiales" pageId="0" pageNumber="117" phylum="Tracheophyta" rank="species" species="rupestre">
<emphasis id="E3A465647296820B11335F644B43AAA4" italics="true" pageId="0" pageNumber="117">Dracocephalum rupestre</emphasis>
</taxonomicName>
Hance (
<taxonomicName id="29AA834E67E9F6D0BDFA82401A54486F" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Lamiaceae" genus="Dracocephalum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Lamiales" pageId="0" pageNumber="117" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
) and 
<taxonomicName id="6ECC69FC0035F22CDA6AA152009D04B5" authorityName="Regel" authorityYear="1876" class="Magnoliopsida" family="Papaveraceae" genus="Meconopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Meconopsis quintuplinervia" order="Ranunculales" pageId="0" pageNumber="117" phylum="Tracheophyta" rank="species" species="quintuplinervia">
