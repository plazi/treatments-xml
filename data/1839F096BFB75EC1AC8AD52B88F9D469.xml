<taxonomicName id="AD97CCE3ACDC31F2AA9C2DF3B9C5C6B9" authorityName="Hance" authorityYear="1878" class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Chrysanthemum oreastrum" order="Asterales" pageId="0" pageNumber="117" phylum="Tracheophyta" rank="species" species="oreastrum">
<emphasis id="22572670F360A0D26FF27B2403FB5576" italics="true" pageId="0" pageNumber="117">Chrysanthemum oreastrum</emphasis>
</taxonomicName>
Hance (
<taxonomicName id="CF513C932E40D1661093D733FF04EF20" class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="0" pageNumber="117" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
