<number id="3D9C24E1E118054E33FAFD3FB719FD0B" box="[440,519,692,716]" pageId="11" pageNumber="12" value="46.851">46.851</number>
S
</geoCoordinate>
, 
<number id="3D9C24E1E118054E306EFD3FB74BFD0B" box="[556,597,692,716]" pageId="11" pageNumber="12" value="174.0">174</number>
º 
<geoCoordinate id="EE605001E118054E301CFD3FB7D9FD0B" box="[606,711,692,716]" direction="east" orientation="longitude" pageId="11" pageNumber="12" precision="55" value="16.877">
<number id="3D9C24E1E118054E301CFD3FB7B2FD0B" box="[606,684,692,716]" pageId="11" pageNumber="12" value="16.877">16.877</number>
E
