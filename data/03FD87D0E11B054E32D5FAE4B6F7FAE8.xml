depth from natural podocarp forest (
<taxonomicName id="4C544D45E118054E368FFD1BB04AFD6F" box="[1229,1364,656,680]" class="Pinopsida" family="Podocarpaceae" genus="Podocarpus" kingdom="Plantae" order="Pinales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="genus">
