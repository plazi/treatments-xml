<subSubSection id="C365BE9665773348FF333FEA6086FDF8" box="[151,690,510,535]" pageId="4" pageNumber="5" type="diagnosis">
<paragraph id="8BC0ED1D65773348FF333FEA6086FDF8" blockId="4.[151,1437,510,1363]" box="[151,690,510,535]" pageId="4" pageNumber="5">
<emphasis id="B90B310F65773348FF333FEA6325FDF8" bold="true" box="[151,273,510,535]" pageId="4" pageNumber="5">Diagnosis.</emphasis>
