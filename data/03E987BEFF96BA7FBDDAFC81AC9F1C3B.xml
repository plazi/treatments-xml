<taxonomicName id="4C404D2BFF96BA7FB988FBADAC6F1C1F" box="[1221,1398,1025,1050]" class="Actinopterygii" family="Muraenidae" genus="Uropterygius" kingdom="Animalia" order="Anguilliformes" pageId="24" pageNumber="164" phylum="Chordata" rank="species" species="alboguttatus">
