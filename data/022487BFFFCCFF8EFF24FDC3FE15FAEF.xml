<quantity id="4D759B4CFFCCFF8EFD29FCFEFCA2FCDB" box="[666,788,799,825]" metricMagnitude="0" metricUnit="m" metricValue="8.6995" metricValueMax="8.8646" metricValueMin="8.5344" pageId="17" pageNumber="218" unit="in" value="342.5" valueMax="349.0" valueMin="336.0">336-349 in</quantity>
female, 
