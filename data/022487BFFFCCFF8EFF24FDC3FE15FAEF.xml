female, 
<quantity id="4D759B4CFFCCFF8EFF24FCFEFEA0FCDB" box="[151,278,799,825]" metricMagnitude="1" metricUnit="m" metricValue="1.02362" metricValueMax="1.07696" metricValueMin="0.97028" pageId="17" pageNumber="218" unit="in" value="403.0" valueMax="424.0" valueMin="382.0">382–424 in</quantity>
male, 
<quantity id="4D759B4CFFCCFF8EFED2FCFEFE1EFCDB" box="[353,424,799,825]" metricMagnitude="0" metricUnit="m" metricValue="9.4996" pageId="17" pageNumber="218" unit="in" value="374.0">374 in</quantity>
DN. 
<taxonomicName id="4D8D4D2AFFCCFF8EFE57FCFEFDFAFCDB" authorityName="Ausserer" authorityYear="1871" box="[484,588,799,825]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
