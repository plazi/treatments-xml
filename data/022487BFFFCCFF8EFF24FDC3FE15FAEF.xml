male, 
<quantity id="4D759B4CFFCCFF8EFB8DFCFEFB33FCDB" box="[1086,1157,799,825]" metricMagnitude="0" metricUnit="m" metricValue="7.5691999999999995" pageId="17" pageNumber="218" unit="in" value="298.0">298 in</quantity>
DN. 
<taxonomicName id="4D8D4D2AFFCCFF8EFB71FCFEFAE6FCDB" authorityName="Krefft" authorityYear="1871" box="[1218,1360,799,825]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="17" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
