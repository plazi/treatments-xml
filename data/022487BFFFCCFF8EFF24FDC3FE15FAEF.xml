<quantity id="4D759B4CFFCCFF8EFF24FCA2FEA1FCBF" box="[151,279,835,861]" metricMagnitude="0" metricUnit="m" metricValue="5.3213" metricValueMax="5.7658" metricValueMin="4.8768" pageId="17" pageNumber="218" unit="in" value="209.5" valueMax="227.0" valueMin="192.0">192–227 in</quantity>
female, 
<quantity id="4D759B4CFFCCFF8EFEC9FCA2FE74FCBF" box="[378,450,835,861]" metricMagnitude="0" metricUnit="m" metricValue="5.1308" pageId="17" pageNumber="218" unit="in" value="202.0">202 in</quantity>
male, 
<quantity id="4D759B4CFFCCFF8EFDA3FCA2FDEEFCBF" box="[528,600,835,861]" metricMagnitude="0" metricUnit="m" metricValue="4.1402" pageId="17" pageNumber="218" unit="in" value="163.0">163 in</quantity>
DN. Ratio of 
<taxonomicName id="4D8D4D2AFFCCFF8EFD48FCA2FC34FCBF" authorityName="Krefft" authorityYear="1871" box="[763,898,835,861]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="17" pageNumber="203" phylum="Nematoda" rank="genus">gnathosoma</taxonomicName>
