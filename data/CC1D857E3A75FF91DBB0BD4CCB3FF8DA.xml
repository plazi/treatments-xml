<number id="F27C264F3A75FF91DA48BD93C9BCFC1D" box="[367,422,961,983]" pageId="1" pageNumber="152" value="2014.0">2014</number>
</bibRefCitation>
: 
<number id="F27C264F3A75FF91DA88BD93C9C2FC1D" box="[431,472,961,983]" pageId="1" pageNumber="152" value="153.0">153</number>
</treatmentCitation>
. 
<typeStatus id="9B0F8ACA3A75FF91DAC5BD90CA0EFC1D" box="[482,532,962,983]" pageId="1" pageNumber="152">Type</typeStatus>
species: 
<taxonomicName id="83B44FEB3A75FF91D956BD93CBCEFC1D" authority="Gahan, 1893" authorityName="Gahan" authorityYear="1893" box="[625,980,961,983]" class="Insecta" family="Cerambycidae" genus="Euseboides" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="152" phylum="Arthropoda" rank="species" species="plagiatus">
<emphasis id="76C0E87A3A75FF91D956BD93CB53FC1D" box="[625,841,961,983]" italics="true" pageId="1" pageNumber="152">Euseboides plagiatus</emphasis>
<bibRefCitation id="202549993A75FF91D868BD93CBCEFC1D" author="Gahan" box="[847,980,961,983]" pageId="1" pageNumber="152" refString="Gahan C. J. (1893) Descriptions of some new Longicorn Coleoptera from the Indian Region. The Annals and Magazine of Natural History, Series 6, 11 (65), 377 - 390. http: // dx. doi. org / 10.1080 / 00222939308677542" type="journal article" year="1893">
Gahan, 
<number id="F27C264F3A75FF91D8B9BD93CBCEFC1D" box="[926,980,961,983]" pageId="1" pageNumber="152" value="1893.0">1893</number>
