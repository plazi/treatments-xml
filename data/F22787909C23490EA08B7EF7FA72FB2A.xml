<keyLead id="C17F95B39C23490EA08B7871FA75FBD8" pageId="29" pageNumber="30">
<number id="CC4624A19C23490EA08B7871FF4DFBFA" box="[151,163,1028,1050]" pageId="29" pageNumber="30" value="3.0">3</number>
