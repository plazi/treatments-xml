<taxonomicName id="AFD9A5961240FFCDFD60F96EFBD9F97D" authority="(Monterosato, 1875)" authorityName="Monterosato" authorityYear="1875" baseAuthorityName="Monterosato" baseAuthorityYear="1875" box="[675,1148,1695,1720]" class="Gastropoda" family="Conidae" genus="Mangelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="58" pageNumber="59" phylum="Mollusca" rank="species" species="indistincta">
