<taxonomicName id="62B07BBE9E3A66790C69FEFC4163AA62" authorityName="Parriwi Park" baseAuthorityName="Stanisic" baseAuthorityYear="2010" box="[833,1036,260,284]" class="Gastropoda" family="Charopidae" genus="Planorbacochlea" kingdom="Animalia" order="Stylommatophora" pageId="49" pageNumber="50" phylum="Mollusca" rank="species" species="hawkesburyana">
