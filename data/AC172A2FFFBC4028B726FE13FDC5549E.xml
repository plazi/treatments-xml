<taxonomicName id="E3BEE0BAFFBE4028B65FFE29FED35680" authority="Wagner, 1839" authorityName="Wagner" authorityYear="1839" class="Mammalia" family="Cercopithecidae" genus="Mesopithecus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="7" pageNumber="500" phylum="Chordata" rank="species" species="pentelicus">
