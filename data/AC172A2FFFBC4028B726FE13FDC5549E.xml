, 
<taxonomicName id="E3BEE0BAFFBC4028B02AF907FD8F5621" authority="(Roth &amp; Wagner, 1855)" baseAuthorityName="Roth &amp; Wagner" baseAuthorityYear="1855" class="Mammalia" family="Equidae" genus="Cremohipparion" higherTaxonomySource="GBIF" kingdom="Animalia" lastPageId="7" lastPageNumber="500" order="Perissodactyla" pageId="5" pageNumber="498" phylum="Chordata" rank="species" species="mediterraneum">
