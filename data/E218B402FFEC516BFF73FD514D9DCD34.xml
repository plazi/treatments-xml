<quantity id="AD49A8F1FFEC516BFD4CFC444EE2CAE4" box="[680,740,970,996]" metricMagnitude="1" metricUnit="m" metricValue="7.5" pageId="31" pageNumber="32" unit="m" value="75.0">75 m</quantity>
(
<bibRefCitation id="0E2078E5FFEC516BFD17FC444F7FCAE4" author="Melo" box="[755,889,970,996]" pageId="31" pageNumber="42" refString="Melo, G. A. S. (1996) Manual de identificacao dos Brachyura (caranguejos e siris) do litoral Brasileiro. Editora Pleiade, Sao Paulo, 603 pp." type="book" year="1996">
