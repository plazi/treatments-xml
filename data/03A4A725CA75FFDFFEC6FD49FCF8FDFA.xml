<taxonomicName id="4C0D6DB0CA7BFFDFFE79FDA9FD29FDC1" authorityName="Jameson and Wada" authorityYear="2004" box="[439,668,564,589]" class="Insecta" family="Dynastidae" genus="Peltonotus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="silvanus">
