<taxonomicName id="898E73EFD9F2B33EFE8720F8AC8D9C69" LSID-ZBK="urn:lsid:zoobank.org:act:4DA48F0B-C37A-4FEC-B3D3-69FEDD2F9E6B" class="Actinopterygii" family="Zoarcidae" genus="Oidiphorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Oidiphorus laevifasciatus Torno, Tomo &amp; Marschoff 1977:4" order="Perciformes" pageNumber="55" phylum="Chordata" rank="species" species="laevifasciatus">Oidiphorus laevifasciatus Torno, Tomo &amp; Marschoff, 1977</taxonomicName>
