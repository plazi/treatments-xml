<taxonomicName id="4C4C54BAFFEE88014430BAA2C1B15905" authority="Rivera-Quiroz &amp; Álvarez-Padilla, 2019" authorityName="Rivera-Quiroz &amp; Álvarez-Padilla" authorityYear="2019" box="[335,511,761,785]" class="Arachnida" family="Anyphaenidae" genus="Wulfila" kingdom="Animalia" order="Araneae" pageId="7" pageNumber="276" phylum="Arthropoda" rank="species" species="xilitlensis" status="spec. nov.">
<emphasis id="B938F32BFFEE88014430BAA2C1B15905" box="[335,511,761,785]" italics="true" pageId="7" pageNumber="276">Wulfila xilitlensis</emphasis>
</taxonomicName>
<emphasis id="B938F32BFFEE88014779BAA2C2235905" bold="true" box="[518,621,761,785]" pageId="7" pageNumber="276">
