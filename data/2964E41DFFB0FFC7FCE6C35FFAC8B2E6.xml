</taxonomicName>
and 
<taxonomicName id="66CD2E88FFB3FFC7FCE3C1F3FB51B2C5" authority="Hertwig, 1882" authorityName="Hertwig" authorityYear="1882" box="[876,1157,197,219]" class="Anthozoa" family="Halcampoididae" genus="Scytophorus" kingdom="Animalia" order="Actiniaria" pageId="10" pageNumber="1402" phylum="Cnidaria" rank="species" species="striatus">
<emphasis id="93B98919FFB3FFC7FCE3C1F3FC0BB2C4" box="[876,991,197,219]" italics="true" pageId="10" pageNumber="1402">S. striatus</emphasis>
<bibRefCitation id="C55C28FAFFB3FFC7FC6AC1F3FB51B2C5" author="Hertwig R" box="[997,1157,197,219]" pageId="10" pageNumber="1402" refId="ref16441" refString="Hertwig R. 1882. Die actinien der challenger expedition. Jena: Gustav Fischer." type="book" year="1882">Hertwig, 1882</bibRefCitation>
