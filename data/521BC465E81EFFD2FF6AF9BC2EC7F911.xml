<taxonomicName id="1DB20EF0E81EFFD2FF6AF9BC281BF9E2" authority="(Saussure, 1861)" baseAuthorityName="Saussure" baseAuthorityYear="1861" box="[192,596,1565,1589]" class="Insecta" family="Tetrigidae" genus="Cota" kingdom="Animalia" order="Orthoptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="species" species="bispina">
