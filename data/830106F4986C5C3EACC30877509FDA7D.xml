</taxonomicName>
(Boulenger, 1904); 
<taxonomicName id="4742556AB40900452E7BC12117A7683C" genus="G." lsidName="G. capsus" pageId="0" pageNumber="21" rank="species" species="capsus">
<emphasis id="EE6CD47189F62636F75DD7A5827CCF7F" italics="true" pageId="0" pageNumber="21">G. capsus</emphasis>
</taxonomicName>
(Das, Min, Hsu, Hertwig &amp; Haas, 2014); 
<taxonomicName id="99DF584E1D397D28E2B3AEA9E9A09381" genus="G." lsidName="G. flavus" pageId="0" pageNumber="21" rank="species" species="flavus">
<emphasis id="A14FBB6962D246CD4040291CA3DB2ACC" italics="true" pageId="0" pageNumber="21">G. flavus</emphasis>
</taxonomicName>
(Kiew, 1984); 
<taxonomicName id="827DB3F532DA699271CBCFD405CE0B2D" genus="G." lsidName="G. guttulatus" pageId="0" pageNumber="21" rank="species" species="guttulatus">
<emphasis id="D193713515BF900BD50BBF7BD18E45DF" italics="true" pageId="0" pageNumber="21">G. guttulatus</emphasis>
</taxonomicName>
(Blyth, 1856); 
<taxonomicName id="55A721C6C5696A52FFF801DDBFDE7ABE" genus="G." lsidName="G. minutus" pageId="0" pageNumber="21" rank="species" species="minutus">
<emphasis id="83686CD9A897B2D1F45041207F07BB5D" italics="true" pageId="0" pageNumber="21">G. minutus</emphasis>
</taxonomicName>
