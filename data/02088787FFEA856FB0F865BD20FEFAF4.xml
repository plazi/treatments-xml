<taxonomicName id="4DA14D12FFEA856FB2CB62FE204AFAC8" box="[676,794,1271,1295]" class="Sphagnopsida" family="Sphagnaceae" genus="Sphagnum" kingdom="Plantae" order="Sphagnales" pageId="6" pageNumber="233" phylum="Bryophyta" rank="genus">
