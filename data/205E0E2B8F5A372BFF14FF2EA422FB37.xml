<taxonomicName id="6FF7C4BE8F5D372BFDA9FBF6A5A6FBC8" authorityName="Loktionov" authorityYear="2020" box="[554,700,1089,1115]" class="Insecta" family="Pompilidae" genus="Dipogon" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="298" phylum="Arthropoda" rank="species" species="himalayensis">
