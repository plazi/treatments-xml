<taxonomicName id="4C194D78FFEFFFBFFCA2FB90FB8EDB39" box="[876,1067,1092,1115]" class="Demospongiae" family="Acarnidae" genus="Iophon" kingdom="Animalia" order="Poecilosclerida" pageId="38" pageNumber="503" phylum="Porifera" rank="subSpecies" species="situ" subSpecies="appearance">
