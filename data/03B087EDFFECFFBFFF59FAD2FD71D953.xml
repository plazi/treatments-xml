<taxonomicName id="4C194D78FFEFFFBFFEE2FB97FCE5DB3E" authority="(Ridley &amp; Dendy, 1886)" authorityName="Ridley &amp; Dendy" authorityYear="1886" baseAuthorityName="Ridley &amp; Dendy" baseAuthorityYear="1886" box="[300,832,1091,1116]" class="Demospongiae" family="Suberitidae" genus="Homaxinella" kingdom="Animalia" order="Hadromerida" pageId="38" pageNumber="499" phylum="Porifera" rank="species" species="balfourensis">
