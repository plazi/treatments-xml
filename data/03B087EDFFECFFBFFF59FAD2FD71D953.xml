<taxonomicName id="4C194D78FFEFFFBFFDE6FB06FBBBDB8E" authority="Ridley &amp; Dendy, 1886" authorityName="Ridley &amp; Dendy" authorityYear="1886" box="[552,1054,1234,1260]" class="Demospongiae" family="Axinellidae" genus="Axinella" kingdom="Animalia" order="Halichondrida" pageId="38" pageNumber="499" phylum="Porifera" rank="species" species="balfourensis">
