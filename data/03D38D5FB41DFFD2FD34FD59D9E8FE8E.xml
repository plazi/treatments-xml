<taxonomicName id="4C7A47CAB41CFFD2FE07FEBCDAC4FECF" authorityName="Milne-Edwards &amp; Haime" authorityYear="1848" box="[502,646,279,305]" class="Anthozoa" family="Lobophylliidae" genus="Acanthastraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="11" phylum="Cnidaria" rank="genus">
