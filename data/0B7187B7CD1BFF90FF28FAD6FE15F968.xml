<taxonomicName id="44D84D22CD1BFF90FD60F9AEFCBFF984" box="[735,864,1577,1602]" class="Insecta" family="Gomphidae" genus="Burmagomphus" kingdom="Animalia" order="Odonata" pageId="14" phylum="Arthropoda" rank="species" species="asahinai">
