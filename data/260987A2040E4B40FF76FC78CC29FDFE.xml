<taxonomicName id="69A04D37040E4B42FF76FC78CC02FB96" box="[151,306,1006,1032]" class="Insecta" family="Cerambycidae" genus="Cotycicuiara" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="221" phylum="Arthropoda" rank="genus">Cotycicuiara</taxonomicName>
