<taxonomicName id="4C24AD95FF26568D106E0154AD40B652" authority="Duperre &amp; Paquin 2007" authorityName="Duperre &amp; Paquin" authorityYear="2007" box="[373,843,1870,1893]" class="Arachnida" family="Linyphiidae" genus="Agyneta" kingdom="Animalia" order="Araneae" pageId="154" pageNumber="155" phylum="Arthropoda" rank="species" species="sheffordiana">
