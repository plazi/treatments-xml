<taxonomicName id="1E6E08FB32DC3681464AC8ED333C592B" class="Magnoliopsida" family="Myrtaceae" genus="Psidium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Psidium guajava" order="Myrtales" pageId="18" pageNumber="117" phylum="Tracheophyta" rank="species" species="guajava">Psidium guajava</taxonomicName>
