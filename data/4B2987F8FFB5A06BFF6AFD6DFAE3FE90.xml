<taxonomicName id="04804D6DFFB5A06DFF6AF84BFE82F826" box="[151,271,1938,1963]" class="Magnoliopsida" family="Araliaceae" kingdom="Plantae" order="Apiales" pageId="3" pageNumber="354" phylum="Tracheophyta" rank="family">Araliaceae</taxonomicName>
