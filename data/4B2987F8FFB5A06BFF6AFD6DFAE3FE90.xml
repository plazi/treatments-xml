<taxonomicName id="04804D6DFFB2A06AFE3AF8A9FDD2F80A" authorityName=": Kirkaldy" authorityYear="1909" box="[455,607,1904,1927]" class="Insecta" family="Acanthosomatidae" genus="Elasmostethus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="355" phylum="Arthropoda" rank="genus">
