<specimenCode id="DB894E090D3BCB02290FFA14FB59FA2B" box="[1035,1199,1447,1473]" collectionCode="IZBE" country="Estonia" httpUri="http://grbio.org/cool/9bph-kj5p" name="Institute of Zoology and Botany" pageId="5" pageNumber="266">IZBE0210186</specimenCode>
