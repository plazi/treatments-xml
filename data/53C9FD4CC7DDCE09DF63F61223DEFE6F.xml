<taxonomicName id="300CA7FA398A0D970676C0DE3B0624CC" LSID-ZBK="urn:lsid:zoobank.org:act:3EF9A59B-2473-44CF-BB0F-3846968A6AB8" authority="Weber, 1913" authorityName="Weber" authorityYear="1913" class="Actinopterygii" family="Ariidae" genus="Doiichthys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-ZBK="Doiichthys Weber 1913:532" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Doiichthys</taxonomicName>
