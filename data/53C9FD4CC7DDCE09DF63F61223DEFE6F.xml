<taxonomicName id="2803E951BE3CE1D20069C6B216F55F5D" LSID-ZBK="urn:lsid:zoobank.org:act:F589893C-76A9-4820-A8B9-B794837B3FB9" class="Actinopterygii" family="Ariidae" genus="Nedystoma" kingdom="Animalia" lsidName-ZBK="Nedystoma Ogilby 1898:32" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Nedystoma</taxonomicName>
