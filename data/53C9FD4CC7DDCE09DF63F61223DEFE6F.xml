<taxonomicName id="2653A339EBC729B6847E950D8C8A6E84" LSID-ZBK="urn:lsid:zoobank.org:act:57F8F227-2C96-4E3F-B4D1-3CDE8E5DE40F" class="Actinopterygii" family="Ariidae" genus="Cephalocassis" kingdom="Animalia" lsidName-ZBK="Cephalocassis Bleeker 1857:473" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Cephalocassis</taxonomicName>
