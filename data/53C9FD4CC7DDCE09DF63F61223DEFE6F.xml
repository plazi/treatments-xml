<taxonomicName id="8DC4C6CB858C0940D3A4D50C24244506" LSID-ZBK="urn:lsid:zoobank.org:act:1AE6B6D0-01A8-43E7-9505-E6855D1FC99A" class="Actinopterygii" family="Ariidae" genus="Plicofollis" kingdom="Animalia" lsidName-ZBK="Plicofollis Kailola 2004:141" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Plicofollis</taxonomicName>
