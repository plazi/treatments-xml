<taxonomicName id="6AC528DF2564DD5180C4E8E5F16686FF" LSID-ZBK="urn:lsid:zoobank.org:act:F589893C-76A9-4820-A8B9-B794837B3FB9" class="Actinopterygii" family="Ariidae" genus="Nedystoma" kingdom="Animalia" lsidName-ZBK="Nedystoma Ogilby 1898:32" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Nedystoma</taxonomicName>
and 
<taxonomicName id="A169F479293DAF784C87E8D09BAF93A3" LSID-ZBK="urn:lsid:zoobank.org:act:5BD940A7-0AA1-4F57-B0A5-9D295A651224" class="Actinopterygii" family="Ariidae" genus="Nemapteryx" kingdom="Animalia" lsidName-ZBK="Nemapteryx Ogilby 1908:3, 10" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Nemapteryx</taxonomicName>
