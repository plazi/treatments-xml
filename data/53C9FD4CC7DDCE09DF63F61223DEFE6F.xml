, 
<taxonomicName id="8D3DB3DA548FD39002487EAAF388B6CB" LSID-ZBK="urn:lsid:zoobank.org:act:CC7B154F-0B6B-4A8C-A4DD-2EBF83B9C00A" class="Actinopterygii" family="Ariidae" genus="Genidens" kingdom="Animalia" lsidName-ZBK="Genidens Castelnau 1855:33" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Genidens</taxonomicName>
