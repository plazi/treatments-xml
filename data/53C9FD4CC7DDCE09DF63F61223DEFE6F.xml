<taxonomicName id="3A553FCD25D3F23121F2C33B30E0A9F3" LSID-ZBK="urn:lsid:zoobank.org:act:7EFF0E2C-627F-4985-9DCA-5178DA9034F3" class="Actinopterygii" family="Ariidae" genus="Osteogeneiosus" kingdom="Animalia" lsidName-ZBK="Osteogeneiosus Bleeker 1846:173" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Osteogeneiosus</taxonomicName>
