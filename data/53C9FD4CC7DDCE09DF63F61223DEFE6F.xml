<taxonomicName id="A2C841F318FABD8D9671E0E5362CF1B0" LSID-ZBK="urn:lsid:zoobank.org:act:4A7D104C-F38B-4AFB-8C4A-A3838C172BB0" class="Actinopterygii" family="Ariidae" genus="Brustiarius" kingdom="Animalia" lsidName-ZBK="Brustiarius Arius Herre 1935:388" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Brustiarius</taxonomicName>
