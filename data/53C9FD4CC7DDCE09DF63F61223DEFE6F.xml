, 
<taxonomicName id="A4AF5292988BB7A8A9448C78BE92283F" LSID-ZBK="urn:lsid:zoobank.org:act:7EFF0E2C-627F-4985-9DCA-5178DA9034F3" class="Actinopterygii" family="Ariidae" genus="Osteogeneiosus" kingdom="Animalia" lsidName-ZBK="Osteogeneiosus Bleeker 1846:173" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="genus">Osteogeneiosus</taxonomicName>
