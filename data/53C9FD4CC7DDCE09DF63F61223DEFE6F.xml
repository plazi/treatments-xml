<taxonomicName id="DAA79F5CEF511DB52F77AD0F8EC03872" LSID-ZBK="urn:lsid:zoobank.org:act:F10D90A5-B4F6-4567-A366-398C2B738FB4" class="Actinopterygii" family="Ariidae" genus="Arius" kingdom="Animalia" lsidName-ZBK="Arius hypophthalmus Steindachner 1877:581 [31]" order="Siluriformes" pageNumber="38" phylum="Chordata" rank="species" species="hypophthalmus">Arius hypophthalmus</taxonomicName>
