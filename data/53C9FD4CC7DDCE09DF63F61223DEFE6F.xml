and 
<taxonomicName id="AD98FE515E6C83E9C664DC9C7B13EB29" LSID-ZBK="urn:lsid:zoobank.org:act:F589893C-76A9-4820-A8B9-B794837B3FB9" class="Actinopterygii" family="Ariidae" genus="Nedystoma" kingdom="Animalia" lsidName-ZBK="Nedystoma Ogilby 1898:32" order="Siluriformes" pageNumber="39" phylum="Chordata" rank="genus">Nedystoma</taxonomicName>
