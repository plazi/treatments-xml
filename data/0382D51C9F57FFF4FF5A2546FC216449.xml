<number id="3DE3762D9F57FFF4FDE825D1FD89629D" box="[549,578,905,929]" pageId="10" pageNumber="29" value="29.0">29</number>
.xii. 
<number id="3DE3762D9F57FFF4FDA725D1FD63629D" box="[618,680,905,929]" pageId="10" pageNumber="29" value="2010.0">2010</number>
</date>
, S07 (
<quantity id="4CD3C9EF9F57FFF4FD2225D1FCD5629D" box="[751,798,905,929]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="10" pageNumber="29" unit="m" value="5.0">5 m</quantity>
