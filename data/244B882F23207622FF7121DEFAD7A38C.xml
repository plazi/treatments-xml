<number id="1A2A2B1E23207622FAA620E4FAB5A1E1" box="[1344,1402,429,454]" pageId="14" pageNumber="167" value="2004.0">2004</number>
</date>
, P. Razafindraibe (
<collectionCode id="CAF3A1FC23207622FED92099FE59A1CF" box="[319,406,464,488]" httpUri="http://grbio.org/cool/82c5-17ij" name="Zoologisches Institut und Zoologisches Museum, Universitat Hamburg" pageId="14" pageNumber="167">ZMUH</collectionCode>
); 
<specimenCount id="BAE4F2B023207622FE4F2099FE3AA1CF" box="[425,501,463,488]" pageId="14" pageNumber="167" type="male">
<number id="1A2A2B1E23207622FE4F2099FE79A1CF" box="[425,438,464,488]" pageId="14" pageNumber="167" value="1.0">1</number>
male
</specimenCount>
, Tolagnaro, Mandena, marecage, 
<date id="D85C1FF923207622FC802086FC13A1CF" box="[870,988,463,488]" pageId="14" pageNumber="167" value="2004-02-12">
<number id="1A2A2B1E23207622FC802086FC4FA1CF" box="[870,896,463,488]" pageId="14" pageNumber="167" value="12.0">12</number>
-ii- 
<number id="1A2A2B1E23207622FC472086FC13A1CF" box="[929,988,463,488]" pageId="14" pageNumber="167" value="2004.0">2004</number>
