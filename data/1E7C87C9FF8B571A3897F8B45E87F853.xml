<taxonomicName id="51D54D5CFF8B571A3BECF8CB5A5DF80B" authority="Richardson 1839" authorityName="Richardson" authorityYear="1839" box="[1004,1401,1910,1934]" class="Actinopterygii" family="Triglidae" genus="Trigla" kingdom="Animalia" order="Scorpaeniformes" pageId="0" pageNumber="53" phylum="Chordata" rank="species" species="polommata">
