<taxonomicName id="51D54D5CFF8B571A3AD4F8275C10F837" authority="Jordan &amp; Starks, 1907" authorityName="Jordan &amp; Starks" authorityYear="1907" box="[724,820,1946,1970]" class="Actinopterygii" family="Triglidae" genus="Otohime" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scorpaeniformes" pageId="0" pageNumber="53" phylum="Chordata" rank="genus">
