<number id="3DA53520BF72FFDFFAE51249FAF60B70" box="[1282,1312,1599,1625]" pageId="1" pageNumber="2" value="15.0">15</number>
° 42.44'S
</geoCoordinate>
/ 
<geoCoordinate id="EE5941C0BF72FFDFFE9C1211FDC70BA8" box="[379,529,1639,1665]" direction="west" orientation="longitude" pageId="1" pageNumber="2" precision="9" value="-52.340668">
<number id="3DA53520BF72FFDFFE9C1211FE4C0BA8" box="[379,410,1639,1665]" pageId="1" pageNumber="2" value="52.0">52</number>
° 20.44'W
</geoCoordinate>
, 
<date id="FFD301C7BF72FFDFFDF81211FD7F0BA8" box="[543,681,1639,1665]" pageId="1" pageNumber="2" value="2003-06-04">
04/VI/ 
<number id="3DA53520BF72FFDFFD891211FD7F0BA8" box="[622,681,1639,1665]" pageId="1" pageNumber="2" value="2003.0">2003</number>
