<taxonomicName id="4C17D0C3F64EDE2814D0FC52FDF570BF" box="[490,561,898,921]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="ardens">
<emphasis id="B9637752F64EDE2814D0FC52FDF570BF" box="[490,561,898,921]" italics="true" pageId="1" pageNumber="66">ardens</emphasis>
</taxonomicName>
group (
<bibRefCitation id="EF86D6B1F64EDE2817BEFC52FC8E70BF" author="Linsenmaier" box="[644,842,898,921]" pageId="1" pageNumber="66" refString="Linsenmaier, W. (1959) Revision der Familie Chrysididae. Mitteilungen der Schweizerischen Entomologischen Gesellschaft, 32, 1 - 240." type="journal article" year="1959">Linsenmaier 1959</bibRefCitation>
, 
<bibRefCitation id="EF86D6B1F64EDE28166EFC52FC4870BF" author="Linsenmaier" box="[852,908,898,921]" pageId="1" pageNumber="66" refString="Linsenmaier, W. (1997) Altes und Neues von den Chrysididen. Entomofauna, 30, 245 - 300." type="journal article" year="1997">1997</bibRefCitation>
