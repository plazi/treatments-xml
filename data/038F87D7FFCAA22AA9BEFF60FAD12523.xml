<emphasis id="B952EAD3FFCAA22DA9BEF88FFEFD25D9" bold="true" box="[151,285,1912,1936]" pageId="27" pageNumber="28">FIGURE 5.</emphasis>
<taxonomicName id="4C264D42FFCAA22DA801F88EFDF725D9" authority="Cabezas, 2010" authorityName="Cabezas" authorityYear="2010" box="[296,535,1913,1936]" class="Malacostraca" family="Munididae" genus="Paramunida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="marionis" status="n. sp.">
<emphasis id="B952EAD3FFCAA22DA801F88EFDF725D9" box="[296,535,1913,1936]" italics="true" pageId="27" pageNumber="28">Paramunida marionis</emphasis>
</taxonomicName>
<emphasis id="B952EAD3FFCAA22DAB08F88FFD8125D9" bold="true" box="[545,609,1912,1936]" pageId="27" pageNumber="28">
<taxonomicNameLabel id="A26157A8FFCAA22DAB08F88FFD8125D9" box="[545,609,1912,1936]" pageId="27" pageNumber="28" rank="species">n. sp.</taxonomicNameLabel>
