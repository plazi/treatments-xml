<taxonomicName id="4C744D35FFD7FFD5FD0DFE97D85BFEC5" authority="(Sharp, 1882)" baseAuthorityName="Sharp" baseAuthorityYear="1882" class="Insecta" family="Dytiscidae" genus="Copelatus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="257" phylum="Arthropoda" rank="species" species="gestroi">
