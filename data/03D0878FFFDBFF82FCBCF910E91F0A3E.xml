<taxonomicName id="4C794D1AFFDBFF84FD37FDA1E97B0EB6" authorityName="W. F. Kirby" authorityYear="1900" box="[699,818,1689,1712]" class="Insecta" family="Halictidae" genus="Pseudapis" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
