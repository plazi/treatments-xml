Additional material examined — 
<materialsCitation id="C4F83CC6FFC58548F16EFD912AC93331" collectingDate="2004-10-01" collectingDateMax="2012-10-28" collectingDateMin="2004-10-01" country="Singapore" county="One" latitude="1.4166666" location="One" longLatPrecision="1308" longitude="103.933334" municipality="St John's Island" pageId="10" pageNumber="156" specimenCount="1" specimenCount-male="1">
<location id="714F6040FFC58548F16EFD912A85324E" LSID="urn:lsid:plazi:treatment:FC39878DFFC58548F39DFE3F2BD5372E:714F6040FFC58548F16EFD912A85324E" box="[620,669,592,616]" country="Singapore" county="One" latitude="1.4166666" longLatPrecision="1308" longitude="103.933334" municipality="St John's Island" name="One" pageId="10" pageNumber="156">One</location>
female (IB), 
<collectingCountry id="0C87760BFFC58548F3E4FDB2294732AD" box="[230,351,627,651]" name="Singapore" pageId="10" pageNumber="156">Singapore</collectingCountry>
, 
<collectingMunicipality id="944BACE1FFC58548F27FFDB22A4A32AD" box="[381,594,627,651]" pageId="10" pageNumber="156">St John’s Island</collectingMunicipality>
, from dead coral block, 
<date id="002E105BFFC58548F240FD5729E43288" box="[322,508,662,686]" pageId="10" pageNumber="156" value="2004-10-01">
<collectingDate id="106AE9B3FFC58548F240FD5729E43288" box="[322,508,662,686]" pageId="10" pageNumber="156" value="2004-10-01">01 Oct. 2004</collectingDate>
</date>
. 
<collectingCounty id="9D4E4E17FFC58548F127FD572A4E3288" box="[549,598,662,686]" pageId="10" pageNumber="156">One</collectingCounty>
female, 
<specimenCount id="6296FD12FFC58548F1CDFD5628C032F7" pageId="10" pageNumber="156" type="male">one male</specimenCount>
(ZRC.ARA.1383), Pulau Ubin, OBS, 
<geoCoordinate id="11A4505CFFC58548F1A1FD782AED32F7" box="[675,757,697,721]" degrees="1" direction="north" minutes="25" orientation="latitude" pageId="10" pageNumber="156" precision="925" value="1.4166666">1°25’N</geoCoordinate>
, 
<geoCoordinate id="11A4505CFFC58548F39DFD1D291D32D2" box="[159,261,732,756]" degrees="103" direction="east" minutes="56" orientation="longitude" pageId="10" pageNumber="156" precision="925" value="103.933334">103°56’E</geoCoordinate>
, midtidal filamentous algae on rock, 
<date id="002E105BFFC58548F1DDFD1D29083331" pageId="10" pageNumber="156" value="2012-10-28">
