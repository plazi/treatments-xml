<taxonomicName id="4C514D1EFFA0FFF5F42FF9E64584F9F9" box="[338,478,1561,1584]" class="Insecta" family="Formicidae" genus="Hypoponera" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
