<taxonomicName id="4C514D1EFFA0FFF5F5EAFF6845B2FF7B" authority="Santschi, 1938" authorityName="Santschi" authorityYear="1938" box="[151,488,151,178]" class="Insecta" family="Formicidae" genus="Hypoponera" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis id="B925EA8FFFA0FFF5F5EAFF6845B2FF7B" bold="true" box="[151,488,151,178]" pageId="33" pageNumber="34">
<emphasis id="B925EA8FFFA0FFF5F5EAFF684577FF78" bold="true" box="[151,301,151,177]" italics="true" pageId="33" pageNumber="34">Hypoponera</emphasis>
<bibRefCitation id="EFC04B6CFFA0FFF5F449FF6745B2FF7B" author="Santschi" box="[308,488,152,178]" pageId="33" pageNumber="34" refString="Santschi, F. (1938) Notes sur quelques Ponera. Bulletin dela Societe Entomologique de France, 43, 78 - 80." type="journal article" year="1938">
Santschi, 
<number id="3D9924BAFFA0FFF5F4D0FF6745B2FF7B" box="[429,488,152,178]" pageId="33" pageNumber="34" value="1938.0">1938</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
