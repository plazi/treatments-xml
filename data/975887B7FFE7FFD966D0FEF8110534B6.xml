<taxonomicName id="D8F14D22FFE7FFD96774F999108D3476" authorityName="Santos-Silva &amp; Heffern &amp; Matsuda" authorityYear="2010" box="[356,754,1603,1627]" class="Insecta" family="Cerambycidae" genus="Melanesiandra" kingdom="Animalia" order="Coleoptera" pageId="50" pageNumber="47" phylum="Arthropoda" rank="species" species="bougainvillensis">
