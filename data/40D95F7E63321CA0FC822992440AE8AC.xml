<geoCoordinate id="14160BF1A1D2C239E7C8936C5B0E6088" direction="north" orientation="latitude" precision="1" value="13.689651">13°41'22.74&quot;N</geoCoordinate>
, 
<geoCoordinate id="3499B103A6DF18D5F9E88BD48B4BDFC0" direction="east" orientation="longitude" precision="1" value="120.8368">120°50'12.46&quot;E</geoCoordinate>
