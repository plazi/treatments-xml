<quantity id="4CE4EAD84712FF80E390F9AAA7B499F1" box="[389,442,1663,1687]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="6" pageNumber="836" unit="m" value="15.0">15 m</quantity>
