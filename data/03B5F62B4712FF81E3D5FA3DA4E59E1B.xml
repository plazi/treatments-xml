<quantity id="4CE4EAD84712FF80E3D2F98AA40A9911" box="[455,516,1631,1655]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="6" pageNumber="836" unit="mm" value="8.0">8 mm</quantity>
