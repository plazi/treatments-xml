<taxonomicName id="E6E4B7F159557103FCFC3CB8AC4EFF0C" authority="Walley, 1936" authorityName="Walley" authorityYear="1936" box="[834,1310,151,176]" class="Insecta" family="Corixidae" genus="Hesperocorixa" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="333" phylum="Arthropoda" rank="species" species="scabricula">
