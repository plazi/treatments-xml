<paragraph id="8BC73B35C862187F5BAC2AB29562FB92" blockId="32.[718,1266,1760,1848]" lastBlockId="33.[94,643,959,1048]" lastPageId="33" lastPageNumber="88" pageId="32" pageNumber="87">
<emphasis id="B90CE727C862187E5BAC2AB29460F97D" box="[718,810,1760,1784]" italics="true" pageId="32" pageNumber="87">Hypodigm</emphasis>
