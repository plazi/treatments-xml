<taxonomicName id="4C3B4D76B00C9851F23E563DFC43F8CF" authority="(Halbert, 1923)" baseAuthorityName="Halbert" baseAuthorityYear="1923" box="[552,924,1856,1880]" class="Arachnida" family="Ascidae" genus="Cheiroseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="478" phylum="Arthropoda" rank="species" species="curtipes">
