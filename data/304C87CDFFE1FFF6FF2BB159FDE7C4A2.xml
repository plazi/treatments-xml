, 
<location id="BD3A6000FFEEFFF6FE42B429FDE2C60C" LSID="urn:lsid:plazi:treatment:304C87CDFFE1FFF6FF2BB159FDE7C4A2:BD3A6000FFEEFFF6FE42B429FDE2C60C" box="[510,627,352,378]" country="United States of America" name="Key West" pageId="40" pageNumber="41" stateProvince="Florida">Key West</location>
, ex rose, 
<collectingDate id="DC1FE9F3FFEEFFF6FD5DB429FB85C60C" box="[737,1044,352,378]" isEnumeration="true" pageId="40" pageNumber="41">
<date id="CC5B101BFFEEFFF6FD5DB429FCCCC60C" box="[737,861,352,378]" pageId="40" pageNumber="41" value="1890-04-16">16.iv.1890</date>
