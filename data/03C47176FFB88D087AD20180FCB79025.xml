<paragraph id="8BD2C060FFB88D0F7AD2006AFBA9957D" blockId="59.[151,1033,1834,1856]" box="[151,1033,1834,1856]" pageId="59" pageNumber="60">
<emphasis id="B9191C72FFB88D0F7AD2006AFEBC957D" bold="true" box="[151,284,1834,1856]" pageId="59" pageNumber="60">
<typeStatus id="54D67EC2FFB88D0F7AD2006AFF6C957D" box="[151,204,1834,1856]" pageId="59" pageNumber="60">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="4C6DBBE3FFB88D0F7B6C006AFCA1957D" authority="Makarkin, 1994: 286" authorityName="Makarkin" authorityPageNumber="286" authorityYear="1994" box="[297,769,1834,1856]" class="Insecta" family="Osmylidae" genus="Imanosmylus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Neuroptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="ussuriensis">
<emphasis id="B9191C72FFB88D0F7B6C006AFD86957D" box="[297,550,1834,1856]" italics="true" pageId="59" pageNumber="60">Imanosmylus ussuriensis</emphasis>
<bibRefCitation id="EFFCBD91FFB88D0F7869006AFD71957D" author="Makarkin" box="[556,721,1834,1856]" firstAuthor="Makarkin" pageId="59" pageNumber="60" pagination="283 - 292" refId="ref50630" refString="Makarkin, V. N. (1994) Upper Cretaceous Neuroptera from Russia and Kazakhstan. Annales de la Societ Entomologique de France, 30, 283 - 292." type="journal article" year="1994">Makarkin, 1994</bibRefCitation>
