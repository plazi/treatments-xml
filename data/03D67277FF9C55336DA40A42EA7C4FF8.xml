<quantity id="4C876E84FF9C55316F8D0A94E86D48A1" box="[702,786,1425,1449]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="0" pageNumber="198" unit="mm" value="2.8">2.8 mm</quantity>
, and of body 
<quantity id="4C876E84FF9C55316E900A94E88748A1" box="[931,1016,1425,1449]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="0" pageNumber="198" unit="mm" value="3.4">3.4 mm</quantity>
