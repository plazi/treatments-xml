, 
<geoCoordinate id="EE2C504CFFA1DE5CFC05FAFC7C9BFAF3" box="[917,1020,1324,1350]" direction="west" orientation="longitude" pageId="22" pageNumber="23" precision="925" value="-65.96667">
<number id="3DD024ACFFA1DE5CFC05FAFC7CD4FAF3" box="[917,947,1324,1350]" pageId="22" pageNumber="23" value="65.0">65</number>
° 
<number id="3DD024ACFFA1DE5CFC2CFAFC7CBDFAF3" box="[956,986,1324,1350]" pageId="22" pageNumber="23" value="58.0">58</number>
'W
</geoCoordinate>
, 
<quantity id="4CE09B6EFFA1DE5CFB9AFAFC7B00FAF3" box="[1034,1127,1324,1350]" metricMagnitude="3" metricUnit="m" metricValue="1.8" pageId="22" pageNumber="23" unit="m" value="1800.0">1800 m</quantity>
, 
<date id="FFA6104BFFA1DE5CFBE4FAFC7A41FAF3" box="[1140,1318,1324,1350]" pageId="22" pageNumber="23" value="1985-01-30" valueMax="1985-02-10" valueMin="1985-01-30">
<number id="3DD024ACFFA1DE5CFBE4FAFC7BF4FAF3" box="[1140,1171,1324,1350]" pageId="22" pageNumber="23" value="30.0">30</number>
.i– 
<number id="3DD024ACFFA1DE5CFB3CFAFC7BACFAF3" box="[1196,1227,1324,1350]" pageId="22" pageNumber="23" value="10.0">10</number>
.ii. 
<number id="3DD024ACFFA1DE5CFB75FAFC7A41FAF3" box="[1253,1318,1324,1350]" pageId="22" pageNumber="23" value="1985.0">1985</number>
