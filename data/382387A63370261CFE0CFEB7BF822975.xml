<subSubSection id="F890653B3370261CFF7DFD9DBA412F90" pageId="7" pageNumber="2318" type="nomenclature">
<paragraph id="B03536B03370261CFF7DFD9DBF642FD6" blockId="7.[160,1155,614,710]" box="[160,362,614,640]" pageId="7" pageNumber="2318">
<heading id="EB7D81DC3370261CFF7DFD9DBF642FD6" box="[160,362,614,640]" fontSize="11" level="3" pageId="7" pageNumber="2318" reason="5">
<emphasis id="82FEEAA23370261CFF7DFD9DBF642FD6" bold="true" box="[160,362,614,640]" inLineHeading="true" italics="true" pageId="7" pageNumber="2318" reason="3">
<taxonomicName id="778A4D333370261CFF7DFD9DBEC32FD6" authorityName="Diakonoff" authorityYear="1982" box="[160,205,614,640]" class="Insecta" family="Tortricidae" genus="Age" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="2318" phylum="Arthropoda" rank="genus">Age</taxonomicName>
