<taxonomicName id="4C2B4D777E55FFDAFB39F94E4B9638EA" authorityName="Beklemischev" authorityYear="1927" box="[1227,1421,1759,1781]" class="Rhabditophora" family="Trigonostomidae" genus="Proxenetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabdocoela" pageId="18" pageNumber="289" phylum="Platyhelminthes" rank="species" species="lictor">Proxenetes lictor</taxonomicName>
with 
