<emphasis id="9FA6E03CFFE9FFD4FF3EFB03E810FB0A" bold="true" box="[151,267,1231,1257]" pageId="56" pageNumber="57">Remarks.</emphasis>
<taxonomicName id="6AD247ADFFE9FFD4FEBDFB03E8DBFB0A" authorityName="Rolston" authorityYear="1980" box="[276,448,1231,1257]" class="Insecta" family="Pentatomidae" genus="Elanela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="hevera">
