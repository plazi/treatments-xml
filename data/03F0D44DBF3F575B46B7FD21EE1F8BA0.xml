</taxonomicName>
and 
<taxonomicName id="4C591ED8BF3F575B47F5FCB2EE6B8B58" box="[469,590,836,861]" class="Aves" family="Threskiornithidae" genus="Bostrychia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pelecaniformes" pageId="6" pageNumber="227" phylum="Chordata" rank="genus">
