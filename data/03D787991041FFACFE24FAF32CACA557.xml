<taxonomicName id="4C7E4D0C1042FFADFD52FA452A22A046" authority="Westring, 1874" authorityName="Westring" authorityYear="1874" box="[729,1042,1515,1540]" class="Arachnida" family="Gnaphosidae" genus="Poecilochroa" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="1330" phylum="Arthropoda" rank="genus">
