<number id="F90D7413B91EFFA5FDF2FA42FD9310D1" box="[571,588,1456,1482]" pageId="18" pageNumber="46" value="9.0">9</number>
</treatmentCitation>
</taxonomicName>
; a junior secondary homonym of 
<taxonomicName id="88C51DB7B91EFFA5FC1EFA42FB6110D2" box="[983,1214,1456,1481]" class="Insecta" family="Lycidae" genus="Cautires" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="46" phylum="Arthropoda" rank="species" species="reticulatus">
