<subSubSection id="BB2CCCED34F2F462CC0284E5CC8413D8" pageId="0" pageNumber="80219" type="reference_group">
<paragraph id="02A1FD411784A1ACC96ED37D9838A60C" pageId="0" pageNumber="80219">
<taxonomicName id="01D5AD4F42588D8A0863FB81826C1089" authority="Yang &amp; Li &amp; Tong &amp; Bian, 2022" authorityName="Yang &amp; Li &amp; Tong &amp; Bian" authorityYear="2022" class="Arachnida" genus="Yueleptoneta" kingdom="Animalia" lsidName="Yueleptoneta" order="Araneae" pageId="0" pageNumber="80219" phylum="Arthropoda" rank="genus" status="gen. n.">Yueleptoneta</taxonomicName>
<taxonomicName id="671F938631303C0D8840CF228EEBA473" authority="Yang &amp; Li &amp; Tong &amp; Bian, 2022" authorityName="Yang &amp; Li &amp; Tong &amp; Bian" authorityYear="2022" class="Arachnida" genus="Yueleptoneta" kingdom="Animalia" lsidName="Yueleptoneta dongxing" order="Araneae" pageId="0" pageNumber="80219" phylum="Arthropoda" rank="species" species="dongxing" status="gen. n.">Yueleptoneta dongxing</taxonomicName>
