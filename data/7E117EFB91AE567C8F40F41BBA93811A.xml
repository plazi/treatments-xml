<paragraph id="70A0AADDE6E6E1A8C9AFDC6207519823" pageId="0" pageNumber="1">
<taxonomicName id="2750E2AF80EDF7EB844166E20E5D6127" class="Insecta" family="Ericaceae" genus="Pyrrhalta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyrrhalta gressitti" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gressitti">
