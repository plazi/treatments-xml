<taxonomicName id="BC913C7C7483BFFD61FA8724DCFF7418" class="Dicotyledoneae" family="Rosaceae" genus="P." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="P. houjayi" order="Rosales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="houjayi">
