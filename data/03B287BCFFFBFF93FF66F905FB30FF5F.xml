<paragraph id="8BA436AAFFF8FF92FF36F8DEFB4AF9A1" blockId="10.[151,1437,1478,2046]" pageId="10">
<emphasis id="B96FEAB8FFF8FF92FF36F8DEFED5FA69" bold="true" box="[199,353,1478,1505]" pageId="10">Distribution.</emphasis>
<taxonomicName id="4C1B4D29FFF8FF92FE9BF8D0FDDAFA69" authorityName="Guichenot" authorityYear="1863" box="[362,622,1479,1505]" class="Actinopterygii" family="Serranidae" genus="Liopropoma" kingdom="Animalia" order="Perciformes" pageId="10" phylum="Chordata" rank="species" species="lunulatum">
