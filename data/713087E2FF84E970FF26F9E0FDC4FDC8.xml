<taxonomicName id="3E994D77FF85E977FD5BF9E3FC59F9B8" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[746,941,1590,1612]" class="Insecta" family="Braconidae" genus="Charmon" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="586" phylum="Arthropoda" rank="species" species="extensor">
<emphasis id="CBEDEAE6FF85E977FD5BF9E3FC59F9B8" box="[746,941,1590,1612]" italics="true" pageId="3" pageNumber="586">
