<collectingCountry id="9E887625A84BD179066CFF77FC459FE6" box="[797,996,192,218]" name="Solomon Islands" pageId="46" pageNumber="47">Solomon Islands</collectingCountry>
, between 399 and 
<quantity id="21679B50A84BD17901CFFF77FABB9FE6" box="[1214,1306,192,218]" metricMagnitude="3" metricUnit="m" metricValue="1.2029999999999998" pageId="46" pageNumber="47" unit="m" value="1203.0">1203 m</quantity>
