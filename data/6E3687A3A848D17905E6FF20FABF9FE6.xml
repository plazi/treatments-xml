<collectingCountry id="9E887625A84BD179041FFF77FE669FE6" box="[366,455,192,218]" name="Taiwan" pageId="46" pageNumber="47">Taiwan</collectingCountry>
, 
<collectingCountry id="9E887625A84BD17904A2FF77FDE49FE6" box="[467,581,192,218]" name="Indonesia" pageId="46" pageNumber="47">Indonesia</collectingCountry>
