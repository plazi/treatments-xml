<quantity id="17466ABAFF9BD929E0B07CC7FB362086" box="[1173,1210,973,995]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="5" pageNumber="406" unit="in" value="2.0">2 in</quantity>
female................................................................................... 
<emphasis id="E2CA1B4DFF9BD929E01A7CE0FA10209A" bold="true" box="[1087,1436,1002,1024]" pageId="5" pageNumber="406">
<taxonomicName id="17BEBCDCFF9BD929E01A7CE0FAC9209A" authority="Shima &amp; Zhang" authorityName="Shima &amp; Zhang &amp; Tachi" authorityYear="2022" box="[1087,1349,1002,1024]" class="Insecta" family="Tachinidae" genus="Phyllomya" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="406" phylum="Arthropoda" rank="species" species="sichuana" status="sp. nov.">
