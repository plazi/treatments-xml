<taxonomicName id="0FE14D23FF9A244FAB79FAAB1879FA85" authority="Geld" authorityName="Geld" box="[605,872,1303,1328]" class="Secernentea" family="Pratylenchidae" genus="Apratylenchoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tylenchida" pageId="15" pageNumber="194" phylum="Nematoda" rank="species" species="cappadocicum">
