<collectingCountry id="8E87F883FF94414DFDCC2674488FFEFA" box="[615,707,324,350]" name="Pakistan" pageId="14" pageNumber="415">Pakistan</collectingCountry>
(
<collectingRegion id="345476F1FF94414DFD7826744933FEFA" box="[723,895,324,350]" country="Pakistan" name="Sindh" pageId="14" pageNumber="415">Sindh province</collectingRegion>
