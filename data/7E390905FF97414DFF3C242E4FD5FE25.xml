<taxonomicName id="3190C390FF94414DFC2026114E51FE9F" box="[907,1053,289,315]" class="Insecta" family="Ascalaphidae" kingdom="Animalia" order="Neuroptera" pageId="14" pageNumber="415" phylum="Arthropoda" rank="family">Ascalaphidae</taxonomicName>
