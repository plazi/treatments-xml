<quantity id="1509A8D0FFC23117FEA0CC8124A9F923" box="[279,400,1733,1757]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="2" pageNumber="36" unit="mm" value="16.0" valueMax="17.0" valueMin="15.0">15–17 mm</quantity>
