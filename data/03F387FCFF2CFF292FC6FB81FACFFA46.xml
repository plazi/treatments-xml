<taxonomicName id="4C5A4D69FF2CFF292934FAEAFBB8FAB2" authorityName="FIEBER, WITH" authorityYear="1858" class="Insecta" family="Miridae" genus="Plagiognathus" kingdom="Animalia" order="Hemiptera" pageId="248" pageNumber="249" phylum="Arthropoda" rank="genus">Plagiognathus</taxonomicName>
