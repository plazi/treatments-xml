<paragraph id="3566A505E21AB66554B5FCD8C08C1230" blockId="39.[164,779,813,1324]" pageId="39" pageNumber="762">
<taxonomicName id="F2D9DE86E21AB66554B5FCD8C08A147C" box="[164,415,843,865]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="barrientosi">
