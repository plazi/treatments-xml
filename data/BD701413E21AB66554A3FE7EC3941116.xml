<taxonomicName id="F2D9DE86E21AB665564BFBB6C3CC132A" baseAuthorityName="Pavesi" baseAuthorityYear="1873" box="[602,729,1057,1079]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="fuesslini">
<emphasis id="07AD7917E21AB665564BFBB6C3CC132A" box="[602,729,1057,1079]" italics="true" pageId="39" pageNumber="762">E. fuesslini</emphasis>
</taxonomicName>
