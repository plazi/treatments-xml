<taxonomicName id="F2D9DE86E21AB66554E6FBB6C07A132A" baseAuthorityName="SIMON" baseAuthorityYear="1870" box="[247,367,1057,1079]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="feminea">
<emphasis id="07AD7917E21AB66554E6FBB6C07A132A" box="[247,367,1057,1079]" italics="true" pageId="39" pageNumber="762">E. feminea</emphasis>
</taxonomicName>
, 
<taxonomicName id="F2D9DE86E21AB6655595FBB6C318132B" box="[388,525,1058,1079]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="incognita">
<emphasis id="07AD7917E21AB6655595FBB6C318132B" box="[388,525,1058,1079]" italics="true" pageId="39" pageNumber="762">E. incognita</emphasis>
</taxonomicName>
