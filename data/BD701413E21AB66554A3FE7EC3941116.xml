<taxonomicName id="F2D9DE86E21AB66554EFFBEAC09D138F" box="[254,392,1150,1171]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="incognita">
<emphasis id="07AD7917E21AB66554EFFBEAC09D138F" box="[254,392,1150,1171]" italics="true" pageId="39" pageNumber="762">E. incognita</emphasis>
</taxonomicName>
, all other 
<taxonomicName id="F2D9DE86E21AB665560AFBEAC399138F" authority="Bolzern &amp; Burckhardt &amp; Hänggi, 2013" authorityName="Bolzern &amp; Burckhardt &amp; Hänggi" authorityYear="2013" box="[539,652,1150,1171]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="genus" status="gen. nov.">
