<taxonomicName id="F2D9DE86E21AB6655621FC3CC3BD14A0" baseAuthorityName="SIMON" baseAuthorityYear="1870" box="[560,680,935,957]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="feminea">
<emphasis id="07AD7917E21AB6655621FC3CC3BD14A0" box="[560,680,935,957]" italics="true" pageId="39" pageNumber="762">E. feminea</emphasis>
</taxonomicName>
, 
<taxonomicName id="F2D9DE86E21AB66556AAFC3CC0AD14C0" authority="sensu Machado" authorityName="Machado" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="bucculenta">
<emphasis id="07AD7917E21AB66556AAFC3CC05714C7" italics="true" pageId="39" pageNumber="762">E. bucculenta sensu</emphasis>
Machado
</taxonomicName>
, and 
<taxonomicName id="F2D9DE86E21AB6655611FC52C39C14C7" box="[512,649,966,987]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="species" species="incognita">
<emphasis id="07AD7917E21AB6655611FC52C39C14C7" box="[512,649,966,987]" italics="true" pageId="39" pageNumber="762">E. incognita</emphasis>
</taxonomicName>
; absent in all other 
<taxonomicName id="F2D9DE86E21AB6655505FC71C09014E6" authority="Bolzern &amp; Burckhardt &amp; Hänggi, 2013" authorityName="Bolzern &amp; Burckhardt &amp; Hänggi" authorityYear="2013" box="[276,389,997,1018]" class="Arachnida" family="Agelenidae" genus="Eratigena" kingdom="Animalia" order="Araneae" pageId="39" pageNumber="762" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="07AD7917E21AB6655505FC71C09014E6" box="[276,389,997,1018]" italics="true" pageId="39" pageNumber="762">Eratigena</emphasis>
</taxonomicName>
<taxonomicNameLabel id="1C9EC46CE21AB6655581FC71C0ED14E6" box="[400,504,997,1018]" pageId="39" pageNumber="762" rank="genus">gen. nov.</taxonomicNameLabel>
