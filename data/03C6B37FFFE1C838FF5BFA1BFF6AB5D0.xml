<taxonomicName id="4C6F79EAFFE1C839FEEFF8AAFD4CB394" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[291,710,1914,1941]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="161" phylum="Arthropoda" rank="species" species="locusta">
