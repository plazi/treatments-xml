; 
<taxonomicName id="4C6F79EAFFE1C839FD1FF8AAFBE8B394" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[723,1122,1914,1941]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="161" phylum="Arthropoda" rank="species" species="pulex">
<emphasis id="B91BDE7BFFE1C839FD1FF8AAFC1CB395" box="[723,918,1914,1941]" italics="true" pageId="10" pageNumber="161">Gammarus pulex</emphasis>
(Linnaeus, 1758)
</taxonomicName>
, 
<taxonomicName id="4C6F79EAFFE1C839FBA3F8AAFA9FB395" box="[1135,1301,1915,1941]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="161" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="B91BDE7BFFE1C839FBA3F8AAFA9FB395" box="[1135,1301,1915,1941]" italics="true" pageId="10" pageNumber="161">Gammarus sp.</emphasis>
