<taxonomicName id="4BD808B04065FF80FEB2AC88FBE39036" authority="Ueckermann &amp; Loots, 1988: 85" authorityName="Ueckermann &amp; Loots" authorityPageNumber="85" authorityYear="1988" box="[264,1050,761,784]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="kangwanensis" subGenus="Amblyseius">
