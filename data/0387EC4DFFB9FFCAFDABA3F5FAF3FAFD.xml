(
<bibRefCitation id="EFBF20AAFFBBFFCAFB27A3A7FAEBFAA6" author="Becker T." box="[1231,1390,1266,1292]" pageId="18" pageNumber="19" pagination="1 - 247" refId="ref8238" refString="Becker T. 1922. Dipterologische Studien. Dolichopodidae der indo-australischen Region. Capita Zoologica 1 (4): 1 - 247." type="journal article" year="1922">Becker, 1922</bibRefCitation>
)
</taxonomicName>
</paragraph>
