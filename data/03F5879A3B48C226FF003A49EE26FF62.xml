<taxonomicName id="4C5C4D0F3B46C227FE1339AEEF7EF9A9" box="[388,602,1532,1555]" class="Arachnida" family="Agelenidae" genus="Coelotes" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="suthepicus">
