<taxonomicName id="4C5C4D0F3B46C227FE1339AEECC5F9A9" box="[388,481,1532,1555]" class="Arachnida" family="Agelenidae" genus="Coelotes" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">Coelotes</taxonomicName>
suthepicus
</emphasis>
