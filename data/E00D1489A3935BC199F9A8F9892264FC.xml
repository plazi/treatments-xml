<paragraph id="843C2A1FE552C98D8E60743F094A6708" pageId="0" pageNumber="1">
<taxonomicName id="2F47A0A1A3D63E98C5941331DBD720DD" class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dianthus tergestinus" order="Caryophyllales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="tergestinus">Dianthus tergestinus</taxonomicName>
