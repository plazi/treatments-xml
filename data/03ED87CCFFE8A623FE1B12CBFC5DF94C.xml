<taxonomicName id="4C444D59FFE8A628FE1B12CBFDDBFA1F" authority="Burckhardt &amp; Basset, 2000" authorityName="Burckhardt &amp; Basset" authorityYear="2000" box="[434,622,1462,1486]" class="Insecta" family="Psyllidae" genus="Calophya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="72" phylum="Arthropoda" rank="species" species="andina" status="sp. n.">
