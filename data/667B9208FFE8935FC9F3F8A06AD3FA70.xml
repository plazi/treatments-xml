<taxonomicName id="29D2589DFFEA935FCB54FB326B1FFB77" box="[559,790,1155,1182]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="19" pageNumber="40" phylum="Tracheophyta" rank="species" species="mandshuricum">
