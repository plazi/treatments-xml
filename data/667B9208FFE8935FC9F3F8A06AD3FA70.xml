<taxonomicName id="29D2589DFFEA935FC84EFB1169EEFB2B" box="[309,487,1191,1217]" class="Magnoliopsida" family="Ulmaceae" genus="Ulmus" kingdom="Plantae" order="Rosales" pageId="19" pageNumber="40" phylum="Tracheophyta" rank="species" species="japonica">
