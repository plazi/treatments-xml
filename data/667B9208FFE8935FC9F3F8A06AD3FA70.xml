<taxonomicName id="29D2589DFFEA935FCD52FB356D33FB77" box="[1065,1338,1155,1181]" class="Magnoliopsida" family="Araliaceae" genus="Kalopanax" kingdom="Plantae" order="Apiales" pageId="19" pageNumber="40" phylum="Tracheophyta" rank="species" species="septemlobus">
