<taxonomicName id="29D2589DFFEA935FCA52FB356C1EFB77" box="[809,1047,1155,1181]" class="Magnoliopsida" family="Salicaceae" genus="Chosenia" kingdom="Plantae" order="Malpighiales" pageId="19" pageNumber="40" phylum="Tracheophyta" rank="species" species="arbutifolia">
