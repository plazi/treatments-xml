</taxonomicName>
, 
<taxonomicName id="29D2589DFFEA935FCC35FB3268F7FB2B" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="19" pageNumber="40" phylum="Tracheophyta" rank="species" species="mongolica">
<emphasis id="DCA6FF0CFFEA935FCC35FB3268F7FB2B" italics="true" pageId="19" pageNumber="40">Quercus mongolica</emphasis>
