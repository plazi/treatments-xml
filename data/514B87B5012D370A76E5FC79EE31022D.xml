(
<figureCitation id="41D92A26012D370A76EDFC45ED6D07B0" box="[159,315,946,973]" captionStart-0="FIGURES 150–152" captionStart-1="FIGURES 153, 154" captionStart-2="FIGURES 155, 156" captionStartId-0="120.[151,264,1933,1957]" captionStartId-1="121.[151,264,1933,1957]" captionStartId-2="122.[151,264,1933,1957]" captionTargetBox-0="[151,1435,181,1909]" captionTargetBox-1="[151,1435,181,1909]" captionTargetBox-2="[151,1435,181,1909]" captionTargetId-0="figure-18@120.[151,1435,181,1909]" captionTargetId-1="figure-20@121.[151,1435,181,1909]" captionTargetId-2="figure-18@122.[151,1435,181,1909]" captionTargetPageId-0="120" captionTargetPageId-1="121" captionTargetPageId-2="122" captionText-0="FIGURES 150–152. Gonatocerus sp. 150, head, anterior; 151, antenna; 152, wings. Scale bars: 150 = 100 μm; 151, 152 = 200 μm." captionText-1="FIGURES 153, 154. Gonatocerus sp. 153, mesosoma, dorsal; 154, mesosoma, lateral. Scale bars = 100 μm." captionText-2="FIGURES 155, 156. Gonatocerus spp. 155a, metasoma, dorsal; 155b, genitalia seen through gaster; 156, metasoma, lateral. Scale bars = 200 μm." figureDoi-0="http://doi.org/10.5281/zenodo.5503185" figureDoi-1="http://doi.org/10.5281/zenodo.5503189" figureDoi-2="http://doi.org/10.5281/zenodo.5503191" httpUri-0="https://zenodo.org/record/5503185/files/figure.png" httpUri-1="https://zenodo.org/record/5503189/files/figure.png" httpUri-2="https://zenodo.org/record/5503191/files/figure.png" pageId="29" pageNumber="30">Figs 150–156</figureCitation>
)
</paragraph>
<paragraph id="D95D36A3012D370A76E5FC00E8390072" blockId="29.[151,1135,1015,1039]" box="[151,1135,1015,1039]" pageId="29" pageNumber="30">
<taxonomicName id="1EE24D20012D370A76E5FC00ED9F0072" authority="Nees, 1834: 192" authorityName="Nees" authorityPageNumber="192" authorityYear="1834" box="[151,457,1015,1039]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis id="EB96EAB1012D370A76E5FC00ED4C0073" box="[151,282,1015,1038]" italics="true" pageId="29" pageNumber="30">Gonatocerus</emphasis>
Nees, 1834: 192
</taxonomicName>
