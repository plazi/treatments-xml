<taxonomicName id="B295A8EFFF92FFB2FD28FA88FB80FAAE" box="[864,1047,1343,1368]" class="Insecta" family="Aleyrodidae" genus="Metaleurodicus" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="genus">
