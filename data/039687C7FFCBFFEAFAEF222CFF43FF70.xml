<taxonomicName id="4C3F4D52FFCBFFE9FB342620FD9DFAE3" box="[332,567,1323,1348]" class="Gymnolaemata" family="Microporellidae" genus="Fenestrulina" kingdom="Animalia" order="Cheilostomatida" pageId="25" pageNumber="26" phylum="Bryozoa" rank="species" species="ampla">
