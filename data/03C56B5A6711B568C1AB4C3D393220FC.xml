<taxonomicName id="4C6CA1CF6710B568C2BB4A3C3BEE2032" baseAuthorityName="Cholokava" baseAuthorityYear="2008" box="[903,1012,332,355]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="325" pageNumber="326" phylum="Tracheophyta" rank="genus">
