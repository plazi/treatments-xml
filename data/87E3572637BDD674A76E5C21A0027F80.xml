<taxonomicName id="9328246E9C055344ED6EDADB9656C468" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139215" genus="Acantholepis" kingdom="Plantae" lsidName="Acantholepis capensis Mayr" lsidName-HNS="Acantholepis capensis Mayr" pageNumber="449" rank="species" species="capensis">capensis</taxonomicName>
