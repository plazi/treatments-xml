<taxonomicName id="55283658A3F718D3CE81B2C62ED40109" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139215" genus="Acantholepis" kingdom="Plantae" lsidName="Acantholepis capensis Mayr" lsidName-HNS="Acantholepis capensis Mayr" pageNumber="449" rank="species" species="capensis">capensis</taxonomicName>
