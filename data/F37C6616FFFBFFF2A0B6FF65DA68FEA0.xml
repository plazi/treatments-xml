<taxonomicName id="BCD5AC83FFFBFFF3A0B6FF65DA4AFF74" authorityName="Holland" authorityYear="1896" box="[151,262,151,177]" class="Insecta" family="Hesperiidae" genus="Artitropa" kingdom="Animalia" order="Lepidoptera" pageId="42" pageNumber="343" phylum="Arthropoda" rank="genus">
<emphasis id="49A10B12FFFBFFF3A0B6FF65DA4AFF74" bold="true" box="[151,262,151,177]" italics="true" pageId="42" pageNumber="343">Artitropa</emphasis>
