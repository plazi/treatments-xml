<number id="3DC824FBF45C5257FAAD731EFA0CFABD" box="[1379,1412,1373,1397]" pageId="1" pageNumber="55" value="49.0">49</number>
’S
</geoCoordinate>
; 
<geoCoordinate id="EE34501BF45C5257FF5973C2FF77FA51" box="[151,255,1409,1433]" direction="west" orientation="longitude" pageId="1" pageNumber="55" precision="925" value="-70.15">
<number id="3DC824FBF45C5257FF5973C2FF3EFA51" box="[151,182,1409,1433]" pageId="1" pageNumber="55" value="70.0">70</number>
