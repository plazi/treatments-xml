<taxonomicName id="D15B3607F0437D4102A6C15E6A80D450" authority="Plaisance &amp; Kritsky, 2004" authorityName="Plaisance &amp; Kritsky" authorityYear="2004" class="Monogenea" family="Ancyrocephalidae" genus="Euryhaliotrematoides" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Euryhaliotrematoides" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="genus">Euryhaliotrematoides</taxonomicName>
