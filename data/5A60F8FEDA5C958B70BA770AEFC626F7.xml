. 
<taxonomicName id="54E30AB9346834D7A0D566B9EB6BBC02" class="Monogenea" family="Ancyrocephalidae" genus="Haliotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haliotrema annulocirrus" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="annulocirrus">Haliotrema annulocirrus</taxonomicName>
<bibRefCitation id="341ECAAE98C306445ADE611AA7CFF818" author="Yamaguti, S" journalOrPublisher="University of Hawaii, Honolulu" pageId="19" pageNumber="20" title="Monogenetic trematodes of Hawaiian fishes." url="https://doi.org/10.2307/3277429" year="1968">Yamaguti 1968</bibRefCitation>
, 
<taxonomicName id="D66EF0AAB20C768EBF23FDC6065F2546" authority="Plaisance &amp; Kritsky, 2004" authorityName="Plaisance &amp; Kritsky" authorityYear="2004" class="Monogenea" family="Ancyrocephalidae" genus="Euryhaliotrematoides" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Euryhaliotrematoides annulocirrus" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="annulocirrus">Euryhaliotrematoides annulocirrus</taxonomicName>
(
<bibRefCitation id="F9535A11511800073870543DD5FDF696" author="Yamaguti, S" journalOrPublisher="University of Hawaii, Honolulu" pageId="19" pageNumber="20" title="Monogenetic trematodes of Hawaiian fishes." url="https://doi.org/10.2307/3277429" year="1968">Yamaguti 1968</bibRefCitation>
) 
<bibRefCitation id="457A5A4C456EA3F5007D4DBCA128B0AC" author="Plaisance, L" journalOrPublisher="The Journal of Parasitology" pageId="17" pageNumber="18" pagination="328 - 341" title="Dactylogyrids (Platyhelminthes: Monogenoidea) parasitizing butterflyfishes (Teleostei: Chaetodontidae) from the coral reefs of Palau, Moorea, Wallis, New Caledonia and Australia: species of Euryhaliotrematoides gen. n. and Aliatrema gen. n." url="https://doi.org/10.1645/ge-3257" volume="90" year="2004">Plaisance and Kritsky 2004</bibRefCitation>
, 
<taxonomicName id="8BFA6F8B19DAA54A8DA00B27CC568300" authority="Mizelle &amp; Price, 1964" authorityName="Mizelle &amp; Price" authorityYear="1964" class="Monogenoidea" family="Dactylogyridae" genus="Parahaliotrema" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Parahaliotrema affinis" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="affinis">Parahaliotrema affinis</taxonomicName>
<bibRefCitation id="8107251CD49774A896621404D277948B" author="Mizelle, JD" journalOrPublisher="American Midland Naturalist" pageId="17" pageNumber="18" pagination="417 - 428" title="Studies on Monogenetic Trematodes. XL. New Species from Marine and Freshwater Fishes." url="https://doi.org/10.2307/2423787" volume="82" year="1969">Mizelle and Kritsky 1969</bibRefCitation>
, 
<taxonomicName id="C9B43B5605C6694999770A7E17BA84B3" class="Monogenea" family="Ancyrocephalidae" genus="Haliotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haliotrema affinis" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="affinis">Haliotrema affinis</taxonomicName>
