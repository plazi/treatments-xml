<taxonomicName id="BFBD34B8FFBC4A31FF2FB872FD8FFAB1" authority="Bembe, 2007" authorityName="Bembe" authorityYear="2007" box="[151,532,1309,1335]" class="Insecta" family="Apidae" genus="Euglossa" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="milenae">
<emphasis id="4AC99329FFBC4A31FF2FB872FD8FFAB1" bold="true" box="[151,532,1309,1335]" pageId="7" pageNumber="62">
