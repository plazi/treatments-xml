♂: 
<emphasis id="B912EA9BC65DFFDEFD0839E6FD24F902" box="[737,765,1779,1800]" italics="true" pageId="23" pageNumber="354">(a)</emphasis>
dorsal; 
<emphasis id="B912EA9BC65DFFDEFCA739E6FCB3F902" box="[846,874,1779,1800]" italics="true" pageId="23" pageNumber="354">(b)</emphasis>
lateral; 
<emphasis id="B912EA9BC65DFFDEFC5239E6FC0FF902" box="[955,982,1779,1800]" italics="true" pageId="23" pageNumber="354">(c)</emphasis>
flagellum; 
<emphasis id="B912EA9BC65DFFDEFBAD39E6FBB9F902" box="[1092,1120,1779,1800]" italics="true" pageId="23" pageNumber="354">(d)</emphasis>
wing; 
<emphasis id="B912EA9BC65DFFDEFB4E39E6FB1BF902" box="[1191,1218,1779,1800]" italics="true" pageId="23" pageNumber="354">(e)</emphasis>
head, dorsal; 
<emphasis id="B912EA9BC65DFFDEFAA539E6FABAF902" box="[1356,1379,1779,1800]" italics="true" pageId="23" pageNumber="354">(f)</emphasis>
