<geoCoordinate id="217D341D0A06579D8949E71A898FFD32" degrees="14.8715" direction="north" orientation="latitude" precision="5" value="14.8715">14.8715</geoCoordinate>
<geoCoordinate id="9C38F59BA1C47C1D5CE0F9A3E5899907" degrees="87.9821" direction="west" orientation="longitude" precision="5" value="-87.9821">-87.9821</geoCoordinate>
