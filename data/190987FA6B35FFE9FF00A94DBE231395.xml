<taxonomicName id="56A04D6F6B35FFE9FD27ABBABCBD13FD" authority="(Walker)" baseAuthorityName="Walker" baseAuthorityYear="1870" box="[688,952,1944,1970]" class="Insecta" family="Baissogryllidae" genus="Aiolopus" kingdom="Animalia" order="Orthoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="simulatrix">
