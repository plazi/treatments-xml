<taxonomicName id="FC4C720B77FB0FEE14F37278DD11B8EA" class="Magnoliopsida" family="Asteraceae" genus="Inula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="525" phylum="Tracheophyta" rank="genus">Inula</taxonomicName>
