<taxonomicName id="032DB68896B03374E75C519AD7F37EC8" class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta subtilis" order="Coleoptera" pageId="20" pageNumber="123" phylum="Arthropoda" rank="species" species="subtilis">Atheta subtilis</taxonomicName>
