<paragraph id="3648AC0D7F4B6916AAB55B7E17A5C4A7" pageId="0" pageNumber="1">
<taxonomicName id="282D61E1F4886E17B383AF1272CBB07B" class="Insecta" family="Noctuidae" genus="Pararcte" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pararcte schneideriana" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="schneideriana">
<emphasis id="B0EF9001AD117007BD50071ABBCC3314" italics="true" pageId="0" pageNumber="1">Pararcte schneideriana</emphasis>
</taxonomicName>
Stoll (
<taxonomicName id="3F0F99799C9BA4B10B80726C00D60BEC" family="Noctuidae" lsidName="" pageId="0" pageNumber="1" rank="family">Noctuidae</taxonomicName>
: 
<taxonomicName id="12F12598B1970C7DF96D5A12D9E61667" lsidName="" pageId="0" pageNumber="1" rank="subfamily" subfamily="Catocalinae">Catocalinae</taxonomicName>
