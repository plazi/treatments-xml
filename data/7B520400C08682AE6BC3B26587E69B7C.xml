<taxonomicName id="4EEC84D9D48422D27C7FB1D9920392AC" class="Magnoliopsida" family="Urticaceae" genus="Cecropia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cecropia peltata" order="Rosales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="peltata">
