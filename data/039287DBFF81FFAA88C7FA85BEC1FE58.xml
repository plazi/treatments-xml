<emphasis id="B94FEADFFF86FFA889E6F8BFBF2AFAAA" box="[502,613,1262,1283]" italics="true" pageId="4" pageNumber="3">Microtityus</emphasis>
(
<emphasis id="B94FEADFFF86FFA88A64F8BCBFBEFAAA" box="[628,753,1261,1283]" italics="true" pageId="4" pageNumber="3">Parvabsonus</emphasis>
) 
<emphasis id="B94FEADFFF86FFA88B11F8BCBE22FAAA" box="[769,877,1261,1283]" italics="true" pageId="4" pageNumber="3">borincanus</emphasis>
</taxonomicName>
, 
<emphasis id="B94FEADFFF86FFA88B6BF8BCBEFDFAAB" bold="true" box="[891,946,1261,1282]" pageId="4" pageNumber="3">
<taxonomicNameLabel id="A27C57A4FF86FFA88B6BF8BCBEFDFAAB" box="[891,946,1261,1282]" pageId="4" pageNumber="3" rank="species">sp. n.</taxonomicNameLabel>
