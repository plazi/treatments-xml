<taxonomicName id="B89C4D4D08341705FE8A4F60FE7DE3D0" box="[262,420,1126,1152]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="46" phylum="Arthropoda" rank="genus">
