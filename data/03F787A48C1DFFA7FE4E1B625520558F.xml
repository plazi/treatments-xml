<location id="8E8160698C1DFFA0FD7E1B9E54A65097" LSID="urn:lsid:plazi:treatment:03F787A48C1DFFA7FE4E1B625520558F:8E8160698C1DFFA0FD7E1B9E54A65097" box="[724,905,1189,1213]" country="Oman" name="Wadi Al Fara" pageId="2" pageNumber="2834">Wadi Al Fara</location>
, 
<collectorName id="26AB53648C1DFFA0FC351B9E530D5097" box="[927,1058,1189,1213]" pageId="2" pageNumber="2834">Rustaq, B.</collectorName>
-Rh. pump in gravel, probe at 
<quantity id="4CA69B578C1DFFA0FE631BFE552350F7" box="[457,524,1221,1245]" metricMagnitude="-1" metricUnit="m" metricValue="6.0" pageId="2" pageNumber="2834" unit="cm" value="60.0">60 cm</quantity>
; UTM 0545970/2595453 (23‡28'06̎N, 57‡27'00̎E), 28.0‡C, 1.134 mS cm 
<superScript id="7C2B9BFA8C1DFFA0FE231BE4568B50C5" attach="left" box="[393,420,1247,1263]" fontSize="7" pageId="2" pageNumber="2834">21</superScript>
