<taxonomicName id="49B85AC1FF91332DB3A86F2FAE1F0957" box="[948,1103,283,309]" class="Insecta" family="Philopteridae" genus="Anaticola" kingdom="Animalia" order="Psocodea" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="BCCCFD50FF91332DB3A86F2FAE760957" box="[948,1062,284,309]" italics="true" pageId="19" pageNumber="20">Anaticola</emphasis>
