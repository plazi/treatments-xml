<taxonomicName id="49B85AC1FF91332DB3F06EC1AE910969" authorityName="Linnaeus" authorityYear="1758" box="[1004,1217,242,267]" class="Aves" family="Anatidae" genus="Anas" kingdom="Animalia" order="Anseriformes" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="querquedula">
