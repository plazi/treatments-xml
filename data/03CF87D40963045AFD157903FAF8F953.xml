<paragraph id="8BD936C20960045AFF2D7D04FAF8F953" blockId="14.[189,1399,1611,1720]" pageId="14" pageNumber="15">
<collectingCountry id="F37176520960045AFF2D7D04FEF8F97F" box="[189,282,1658,1684]" name="Sweden" pageId="14" pageNumber="15">Sweden</collectingCountry>
(Småland, Öland, 
<collectingRegion id="49A2F8200960045AFE657D06FD6EF97F" box="[501,652,1656,1684]" country="Sweden" name="Ostergotland" pageId="14" pageNumber="15">Östergötland</collectingRegion>
), 
<collectingCountry id="F37176520960045AFD347D04FCF3F97F" box="[676,785,1658,1684]" name="Germany" pageId="14" pageNumber="15">Germany</collectingCountry>
(
<collectingRegion id="49A2F8200960045AFC8F7D04FB85F97E" box="[799,1127,1658,1685]" country="Germany" name="Mecklenburg-Vorpommern" pageId="14" pageNumber="15">Mecklenburg-Vorpommern</collectingRegion>
