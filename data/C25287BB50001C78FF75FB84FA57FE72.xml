</paragraph>
<paragraph id="4A4436AD50001C76FF75FB11FBACFB6A" blockId="7.[151,1137,1216,1239]" box="[151,1137,1216,1239]" pageId="7" pageNumber="256">
<taxonomicName id="8DFB4D2E50001C76FF75FB11FE7EFB6B" authority="Blake 1934:53" authorityName="Blake" authorityPageNumber="53" authorityYear="1934" box="[151,419,1216,1238]" class="Insecta" family="Chrysomelidae" genus="Argopistes" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="256" phylum="Arthropoda" rank="species" species="rubicundus">
