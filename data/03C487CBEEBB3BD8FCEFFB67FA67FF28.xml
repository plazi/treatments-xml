<subSubSection id="C3776556EEBB3BDFFC9AFA74FB1CFA37" pageId="490" pageNumber="491" type="etymology">
<paragraph id="8BD236DDEEBB3BDFFC9AFA74FB1CFA37" blockId="490.[810,1452,1295,2071]" pageId="490" pageNumber="491">
<emphasis id="B919EACFEEBB3BDFFC9AFA74FC25FA15" bold="true" box="[863,992,1466,1492]" pageId="490" pageNumber="491">Etymology</emphasis>
