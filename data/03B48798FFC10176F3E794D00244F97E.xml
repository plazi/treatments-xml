<subSubSection id="C3076505FFC10176F3B7918C0347FA1D" box="[199,1298,1419,1444]" pageId="1" pageNumber="290" type="etymology">
<paragraph id="8BA2368EFFC10176F3B7918C0347FA1D" blockId="1.[151,1437,316,1728]" box="[199,1298,1419,1444]" pageId="1" pageNumber="290">
<emphasis id="B969EA9CFFC10176F3B7918C0712FA1A" bold="true" box="[199,327,1419,1444]" pageId="1" pageNumber="290">Etymology</emphasis>
