<quantity id="4CE6E0BDC03A6E76FE63FEA06D55E176" box="[398,575,289,313]" metricMagnitude="-1" metricUnit="m" metricValue="9.95" metricValueMax="10.4" metricValueMin="9.5" pageId="7" pageNumber="332" unit="mm" value="995.0" valueMax="1040.0" valueMin="950.0">950 to 1040 mm</quantity>
