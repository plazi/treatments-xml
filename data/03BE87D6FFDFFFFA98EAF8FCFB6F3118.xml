<taxonomicName id="4C174D43FFDFFFFB98DFF8FCF87C372C" authority="(Peters, 1874)" baseAuthorityName="Peters" baseAuthorityYear="1874" box="[162,573,1806,1833]" class="Actinopterygii" family="Gadidae" genus="Arctogadus" kingdom="Animalia" order="Gadiformes" pageId="78" pageNumber="79" phylum="Chordata" rank="species" species="glacialis">
<emphasis id="B963EAD2FFDFFFFB98DFF8FCFBD3372C" bold="true" box="[162,402,1806,1833]" italics="true" pageId="78" pageNumber="79">Arctogadus glacialis</emphasis>
(Peters, 1874)
