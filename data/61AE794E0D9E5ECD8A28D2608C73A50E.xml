<taxonomicName id="3D029A02DFC553C87EF05B9A57EF6786" authorityName="Kieffer" authorityYear="1916" family="Scelionidae" genus="Plagioscelio" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Plagioscelio rufescens" order="Hymenoptera" pageId="0" pageNumber="575" rank="species" species="rufescens">Plagioscelio rufescens</taxonomicName>
