<taxonomicName id="C6EC53B096D89613901B834FAB88788F" authorityName="Lahey" authorityYear="2021" class="Insecta" family="Scelionidae" genus="Phoenoteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phoenoteleia sanma" order="Hymenoptera" pageId="0" pageNumber="575" phylum="Arthropoda" rank="species" species="sanma">
