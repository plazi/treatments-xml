<taxonomicName id="4C204D72FFBDFFEAFCB9FBD0D7D5FB95" box="[192,459,1123,1147]" class="Magnoliopsida" family="Rubiaceae" genus="Mussaenda" kingdom="Plantae" order="Gentianales" pageId="10" pageNumber="9" phylum="Tracheophyta" rank="species" species="philippica">
