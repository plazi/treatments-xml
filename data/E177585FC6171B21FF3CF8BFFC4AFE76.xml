<taxonomicName id="AEDE92CAC6141B21FCB5FEB3FC4DFE71" baseAuthorityName="Stimpson" baseAuthorityYear="1860" box="[798,968,404,428]" class="Malacostraca" family="Upogebiidae" genus="Upogebia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="carinicauda">
