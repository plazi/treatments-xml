<taxonomicName id="4C414D16FFA8FFACFD6CFE63FC07FE59" box="[738,906,437,462]" class="Elasmobranchii" family="Scyliorhinidae" genus="Atelomycterus" kingdom="Animalia" order="Carcharhiniformes" pageId="5" pageNumber="24" phylum="Chordata" rank="genus">
