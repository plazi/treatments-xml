<taxonomicName id="4C414D16FFA8FFACFBBAFE63FB30FE59" box="[1076,1213,437,462]" class="Elasmobranchii" family="Scyliorhinidae" genus="Atelomycterus" kingdom="Animalia" order="Carcharhiniformes" pageId="5" pageNumber="24" phylum="Chordata" rank="species" species="fasciatus">
