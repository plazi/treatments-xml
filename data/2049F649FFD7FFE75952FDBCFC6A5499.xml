<paragraph id="A85F475FFFD7FFE65BBEFD05FDED5333" blockId="20.[188,1399,764,825]" pageId="20" pageNumber="54">
<taxonomicName id="6FE03CDCFFD7FFE65BBEFD05FE35531C" box="[189,480,764,790]" class="Insecta" family="Curculionidae" genus="Smicronyx" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="54" phylum="Arthropoda" rank="species" species="paucisquamis" status="sp. nov.">
