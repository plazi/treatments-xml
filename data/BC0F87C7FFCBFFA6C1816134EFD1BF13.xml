<emphasis id="06D2EAC3FFCBFFA6C1816134EF2DB80E" bold="true" box="[151,788,592,614]" pageId="1" pageNumber="678">
<taxonomicName id="F3A64D52FFCBFFA6C1816134EE01B80E" authority="(MEYRICK 1914)" baseAuthorityName="MEYRICK" baseAuthorityYear="1914" box="[151,568,592,614]" class="Insecta" family="Choreutidae" genus="Prochoreutis" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="678" phylum="Arthropoda" rank="species" species="holotoxa">
