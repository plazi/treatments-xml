<collectionCode id="ED47CFE10A19F56CFED5CEF6AB46FD61" LSID="urn:lsid:biocol.org:col:34784" box="[298,370,728,752]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34784" name="Academy of Natural Sciences of Philadelphia" pageId="28" pageNumber="29">ANSP</collectionCode>
