<quantity id="1C0888A5970AFFDCFE6FF8A3FE21B86A" box="[397,479,1838,1862]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="93" pageNumber="94" unit="mm" value="13.0">13 mm</quantity>
