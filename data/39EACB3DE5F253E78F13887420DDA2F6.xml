<taxonomicName id="2FB0B16F9D8BBCF6FD2FBF5ED21C5772" class="Hexapoda" family="Disteniidae" genus="Distenia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="5" pageNumber="82" phylum="Arthropoda" rank="subSpecies" species="gracilis" subSpecies="yakushimana">Distenia gracilis yakushimana</taxonomicName>
