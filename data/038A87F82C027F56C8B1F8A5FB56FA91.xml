<taxonomicName id="4C234D6D2C037F51C94EF9ABFDC3F951" box="[360,603,1644,1667]" class="Entognatha" family="Isotomidae" genus="Subisotoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="pomorskii" status="sp. nov.">
