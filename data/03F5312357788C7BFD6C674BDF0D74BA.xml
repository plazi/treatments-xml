<typeStatus id="54E73E9757788C7FFECC6706DD2676C0" box="[317,366,1642,1664]" pageId="1" pageNumber="6">Type</typeStatus>
species: 
<taxonomicName id="4C5CFBB657788C7FFE376706DEBE76C0" authority="EWING, 1909" authorityName="EWING" authorityYear="1909" box="[454,758,1642,1664]" class="Arachnida" family="Phenopelopidae" genus="Pelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="6" phylum="Arthropoda" rank="species" species="bifurcatus">
<emphasis id="B9285C2757788C7FFE376706DE2776C0" box="[454,623,1642,1664]" italics="true" pageId="1" pageNumber="6">Pelops bifurcatus</emphasis>
<bibRefCitation id="EFCDFDC457788C7FFD846706DEBE76C0" author="EWING, H. E." box="[629,758,1642,1664]" pageId="1" pageNumber="6" pagination="116 - 136" refId="ref4243" refString="EWING, H. E. (1909) New American Oribatoidea. J. N. Y. Ent. Soc. 17: 116 - 136." type="journal article" year="1909">EWING, 1909</bibRefCitation>
</taxonomicName>
(= 
<taxonomicName id="4C5CFBB657788C7FFCE96706D81C76C0" authority="HALL, 1911" authorityName="HALL" authorityYear="1911" box="[792,1108,1642,1664]" class="Arachnida" family="Punctoribatidae" genus="Pelopsis" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="6" phylum="Arthropoda" rank="species" species="nudiuscula">
