<taxonomicName id="4C5CFBB6577C8C7BFB906561DD33740A" authorityName="Shaldybina" authorityYear="1973" class="Arachnida" family="Punctoribatidae" genus="Zachvatkinibates" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis id="B9285C27577C8C7BFB906561DD33740A" italics="true" pageId="5" pageNumber="10">Zachvatkinibates</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C5CFBB6577C8C7BFE7E655CDE7C740A" authorityName="Berlese" authorityYear="1908" box="[399,564,1072,1098]" class="Arachnida" family="Punctoribatidae" genus="Punctoribates" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis id="B9285C27577C8C7BFE7E655CDE7C740A" box="[399,564,1072,1098]" italics="true" pageId="5" pageNumber="10">Punctoribates</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C5CFBB6577C8C7BFD87655CDF69740A" authorityName="Hull" authorityYear="1916" box="[630,801,1072,1098]" class="Arachnida" family="Punctoribatidae" genus="Minunthozetes" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis id="B9285C27577C8C7BFD87655CDF69740A" box="[630,801,1072,1098]" italics="true" pageId="5" pageNumber="10">Minunthozetes</emphasis>
</taxonomicName>
