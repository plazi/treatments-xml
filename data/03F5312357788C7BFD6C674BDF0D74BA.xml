<taxonomicName id="4C5CFBB6577C8C7BFD55658CDF0974BA" baseAuthorityName="GRANDJEAN" baseAuthorityYear="1954" box="[676,833,1248,1274]" family="Mycobatidae" pageId="5" pageNumber="10" rank="family">Mycobatidae</taxonomicName>
.
