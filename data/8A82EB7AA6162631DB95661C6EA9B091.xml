<pageBreakToken id="83D637E00B677A8A21A29BA18C8467B8" pageId="null" pageNumber="853" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="0E0B55350A1525CA9D8B58B59CA3BAE0" authority="L." class="Magnoliopsida" family="Apiaceae" genus="Heracleum" kingdom="Plantae" order="Apiales" pageId="null" pageNumber="853" phylum="Tracheophyta" rank="species" species="sphondylium">
