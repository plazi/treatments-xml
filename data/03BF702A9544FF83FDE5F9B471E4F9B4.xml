<taxonomicName id="4C16BABF9544FF83FF5DF80D7035F9D1" authorityName="Ritchie &amp; Shorthouse" authorityYear="1987" box="[559,856,1929,1955]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="mesoamericanus">
