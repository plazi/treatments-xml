<quantity id="6F0DC883FFC6FFA9D0BF2A471EE9F9AF" box="[671,740,1564,1584]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="54" pageNumber="1731" unit="m" value="600.0">600 m</quantity>
