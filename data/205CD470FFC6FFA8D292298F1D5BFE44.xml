<paragraph id="A84A6566FFC6FFA9D2922A5F1D55F9D7" blockId="54.[178,1085,1540,1608]" pageId="54" pageNumber="1731">
<collectingRegion id="6A31AB84FFC6FFA9D2922A5F1D0BF987" box="[178,262,1540,1560]" country="Austria" name="Salzburg" pageId="54" pageNumber="1731">Salzburg</collectingRegion>
