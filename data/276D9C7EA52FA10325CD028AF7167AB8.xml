<taxonomicName id="EFD484FE700B8B53B9D7FFFEAD5B3998" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29522" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera occidentalis (Bernard)" order="Hymenoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="occidentalis">occidentalis</taxonomicName>
