<collectingRegion id="49FE2BCA7A554A7CFF86F8E3DAFAF8D4" box="[126,217,1827,1847]" country="Denmark" name="Sjaelland" pageId="8" pageNumber="1054">Sjaelland</collectingRegion>
(
<collectingRegion id="49FE2BCA7A554A7CFF1DF8E3DB15F8D4" box="[229,310,1827,1847]" country="Denmark" name="Sjaelland" pageId="8" pageNumber="1054">Selandia</collectingRegion>
