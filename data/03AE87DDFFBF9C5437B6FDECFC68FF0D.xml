<collectingCountry id="F310765BFFBF9C57360CF8CCFE6EF88D" box="[378,442,1891,1915]" name="India" pageId="53" pageNumber="52">India</collectingCountry>
(
<collectingRegion id="49C3F829FFBF9C5736BFF8CCFDB2F88D" box="[457,614,1891,1915]" country="India" name="West Bengal" pageId="53" pageNumber="52">West Bengal</collectingRegion>
).
</paragraph>
