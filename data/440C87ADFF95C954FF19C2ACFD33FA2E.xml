</taxonomicName>
(
<taxonomicName id="0BA54D38FF92C954FD75C5F9FC17FB06" box="[763,902,1236,1260]" class="Clitellata" family="Octochaetidae" genus="Dichogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="4" pageNumber="395" phylum="Annelida" rank="genus">
