Species 30: 
<taxonomicName id="4C174D1A0232441CFDEBFC32FC78E61E" authorityName="Meuffels &amp; Grootaert" authorityYear="2004" box="[624,910,1376,1400]" class="Insecta" family="Dolichopodidae" genus="Teuchophorus" kingdom="Animalia" order="Diptera" pageId="54" pageNumber="196" phylum="Arthropoda" rank="species" species="ctenomerus" status="sp. nov.">
<emphasis id="B963EA8B0232441CFDEBFC32FC78E61E" bold="true" box="[624,910,1376,1400]" italics="true" pageId="54" pageNumber="196">Teuchophorus ctenomerus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25057F00232441CFC0CFC33FC04E61E" box="[919,1010,1377,1400]" pageId="54" pageNumber="196" rank="species">sp. nov.</taxonomicNameLabel>
