</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C0AA5C6CA695A66F37CFB59FBD76D9E" authority="Nutting, 1911" authorityName="Nutting" authorityYear="1911" box="[775,1083,1168,1194]" class="Anthozoa" family="Acanthogorgiidae" genus="Acanthogorgia" kingdom="Animalia" order="Alcyonacea" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="turgida">
<emphasis id="B97E0257CA695A66F37CFB59FC696D9D" box="[775,901,1168,1193]" italics="true" pageId="8" pageNumber="9">A. turgida</emphasis>
<bibRefCitation id="EF9BA3B4CA695A66F3F6FB59FBD76D9E" author="Nutting" box="[909,1083,1168,1194]" pageId="8" pageNumber="51" refString="Nutting, C. C. (1911) The Gorgonacea of the Siboga Expedition VIII. The Scleraxonia. Siboga-Expeditie Monograph 13 b 5, 1 - 62." type="journal article" year="1911">
Nutting, 
<number id="3DC2CC62CA695A66F387FB59FBD76D9E" box="[1020,1083,1168,1194]" pageId="8" pageNumber="9" value="1911.0">1911</number>
</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="4C0AA5C6CA695A66F406FB59FAEC6D9D" box="[1149,1280,1168,1193]" class="Anthozoa" family="Acanthogorgiidae" genus="Acanthogorgia" kingdom="Animalia" order="Alcyonacea" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="dofleini">
<emphasis id="B97E0257CA695A66F406FB59FAEC6D9D" box="[1149,1280,1168,1193]" italics="true" pageId="8" pageNumber="9">A. dofleini</emphasis>
</taxonomicName>
<taxonomicName id="4C0AA5C6CA695A66F573FB59FE826DE4" authority="Aurivillius, 1931" authorityName="Aurivillius" authorityYear="1931" pageId="8" pageNumber="9" rank="variety" variety="spinosa">
