<number id="38202489FFF2FF92549DFA56AE4E6746" box="[511,540,1531,1557]" pageId="11" pageNumber="58" value="27.0">27</number>
(
<number id="38202489FFF2FF92574FFA56AE2F6746" box="[557,637,1531,1557]" pageId="11" pageNumber="58" value="26.0" valueMax="28.0" valueMin="24.0">24–28</number>
; n = 
<number id="38202489FFF2FF9257DBFA56AE986746" box="[697,714,1531,1557]" pageId="11" pageNumber="58" value="5.0">5</number>
); outer length 
<number id="38202489FFF2FF92561BFA56AFC56746" box="[889,919,1531,1557]" pageId="11" pageNumber="58" value="30.0">30</number>
(
<number id="38202489FFF2FF9256CAFA56AFA46746" box="[936,1014,1531,1557]" pageId="11" pageNumber="58" value="30.5" valueMax="32.0" valueMin="29.0">29–32</number>
; n = 
<number id="38202489FFF2FF925151FA56A8166746" box="[1075,1092,1531,1557]" pageId="11" pageNumber="58" value="5.0">5</number>
); inner root 
<number id="38202489FFF2FF9251BBFA56A8A56746" box="[1241,1271,1531,1557]" pageId="11" pageNumber="58" value="10.0">10</number>
(
<number id="38202489FFF2FF92506AFA56A9146746" box="[1288,1350,1531,1557]" pageId="11" pageNumber="58" value="10.0" valueMax="11.0" valueMin="9.0">9–11</number>
; n = 
<number id="38202489FFF2FF9250E1FA56A9C06746" box="[1411,1426,1531,1557]" pageId="11" pageNumber="58" value="5.0">5</number>
) long; outer root 
<number id="38202489FFF2FF925431F98FAD30676F" box="[339,354,1570,1596]" pageId="11" pageNumber="58" value="9.0">9</number>
(
<number id="38202489FFF2FF925413F98FADE2676F" box="[369,432,1570,1596]" pageId="11" pageNumber="58" value="9.5" valueMax="10.0" valueMin="9.0">9–10</number>
; n = 
<number id="38202489FFF2FF92548AF98FADA5676F" box="[488,503,1570,1596]" pageId="11" pageNumber="58" value="5.0">5</number>
) long; shaft straight, with relatively wide proximal part; point 
<number id="38202489FFF2FF9251B3F98FA8BC676F" box="[1233,1262,1570,1596]" pageId="11" pageNumber="58" value="12.0">12</number>
(
<number id="38202489FFF2FF92519CF98FA919676F" box="[1278,1355,1570,1596]" pageId="11" pageNumber="58" value="12.5" valueMax="13.0" valueMin="12.0">12–13</number>
; n = 
<number id="38202489FFF2FF9250E6F98FA9C1676F" box="[1412,1427,1570,1596]" pageId="11" pageNumber="58" value="5.0">5</number>
) long. One bar (dorsal): 
<number id="38202489FFF2FF9254CEF9E4AD9B6730" box="[428,457,1609,1635]" pageId="11" pageNumber="58" value="25.0">25</number>
(
<number id="38202489FFF2FF9254B8F9E4AE7A6730" box="[474,552,1609,1635]" pageId="11" pageNumber="58" value="25.0" valueMax="27.0" valueMin="23.0">23–27</number>
; n = 
<number id="38202489FFF2FF925706F9E4AE266730" box="[612,628,1609,1635]" pageId="11" pageNumber="58" value="4.0">4</number>
) long, 
<number id="38202489FFF2FF9257AAF9E4AE856730" box="[712,727,1609,1635]" pageId="11" pageNumber="58" value="4.0">4</number>
(
<number id="38202489FFF2FF925785F9E4AF456730" box="[743,791,1609,1635]" pageId="11" pageNumber="58" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
; n = 
<number id="38202489FFF2FF925631F9E4AF316730" box="[851,867,1609,1635]" pageId="11" pageNumber="58" value="4.0">4</number>
) wide, with swollen medial part. Hooks 
<number id="38202489FFF2FF925027F9E4A9066730" box="[1349,1364,1609,1635]" pageId="11" pageNumber="58" value="7.0">7</number>
pairs, dissimilar in size; hook lengths (n = 
<number id="38202489FFF2FF925732F9DDAE3067D9" box="[592,610,1648,1674]" pageId="11" pageNumber="58" value="2.0">2</number>
): pair I = 
<number id="38202489FFF2FF9257BEF9DDAF7867D9" box="[732,810,1648,1674]" pageId="11" pageNumber="58" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
; pairs II–VII = 
<number id="38202489FFF2FF925685F9DDA86667D9" box="[999,1076,1648,1674]" pageId="11" pageNumber="58" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
. Needles (
<number id="38202489FFF2FF9251D6F9DDA89367D9" box="[1204,1217,1648,1674]" pageId="11" pageNumber="58" value="1.0">1</number>
