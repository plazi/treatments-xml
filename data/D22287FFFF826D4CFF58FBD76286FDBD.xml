(about 
<geoCoordinate id="3FBF502EFF826D40FC27FD7A6545FAED" box="[1000,1067,1304,1326]" degrees="1" direction="south" minutes="44" orientation="latitude" pageId="55" pageNumber="640" precision="925" value="-1.7333333">1°44’S</geoCoordinate>
, 
<geoCoordinate id="3FBF502EFF826D40FF58FD536078FA84" box="[151,278,1329,1351]" degrees="101" direction="east" minutes="15" orientation="longitude" pageId="55" pageNumber="640" precision="15" seconds="30" value="101.25833">101°15’30”E</geoCoordinate>
), 
<quantity id="9D739B0CFF826D40FEE4FD5360C2FA84" box="[299,428,1329,1351]" metricMagnitude="3" metricUnit="m" metricValue="1.89" metricValueMax="1.98" metricValueMin="1.8" pageId="55" pageNumber="640" unit="m" value="1890.0" valueMax="1980.0" valueMin="1800.0">
<elevation id="D1A6D1DAFF826D40FEE4FD5360C2FA84" box="[299,428,1329,1351]" metricMagnitude="3" metricUnit="m" metricValue="1.89" metricValueMax="1.98" metricValueMin="1.8" pageId="55" pageNumber="640" unit="m" value="1890.0" valueMax="1980.0" valueMin="1800.0">1800-1980 m</elevation>
