<taxonomicName id="385253D3D47AB7A7FE0453C5298A3EA7" LSID="http://zoobank.org/307964FF-DEC2-410A-9CDF-8CA41D5D924E" class="Arachnida" family="Nemesiidae" genus="Raveniola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Raveniola rugosa" order="Araneae" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="rugosa">Raveniola rugosa</taxonomicName>
