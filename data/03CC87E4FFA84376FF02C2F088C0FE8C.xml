<taxonomicName id="4C654D71FFA94376FEC6C5BC89E5FF68" box="[339,476,223,248]" class="Insecta" family="Latridiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="family">Lathridiidae</taxonomicName>
