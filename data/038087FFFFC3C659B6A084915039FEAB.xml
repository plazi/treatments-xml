</subSubSection>
<caption id="DF566661FFC2C65CB6A0837951B3F874" ID-DOI="http://doi.org/10.5281/zenodo.4563746" ID-Zenodo-Dep="4563746" httpUri="https://zenodo.org/record/4563746/files/figure.png" pageId="11" pageNumber="162" startId="11.[151,250,1919,1943]" targetBox="[155,1426,190,1887]" targetPageId="11">
<paragraph id="8B9636E9FFC2C65CB6A0837951B3F874" blockId="11.[151,1436,1919,1979]" pageId="11" pageNumber="162">
