<subSubSection id="C33F6534D000FFACFF4277C1E7B1FC8C" box="[199,1208,764,789]" pageId="3" pageNumber="278" type="etymology">
<paragraph id="8B9A36BFD000FFACFF4277C1E7B1FC8C" blockId="3.[151,1436,151,1149]" box="[199,1208,764,789]" pageId="3" pageNumber="278">
<emphasis id="B951EAADD000FFACFF4277C1E247FC8D" bold="true" box="[199,334,764,789]" pageId="3" pageNumber="278">Etymology.</emphasis>
Latin, 
<taxonomicName id="4C254D3CD000FFACFE1877C1E117FC8D" box="[413,542,764,789]" class="Insecta" family="Cerambycidae" genus="Mephritus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="278" phylum="Arthropoda" rank="species" species="punctulatus">punctulatus</taxonomicName>
