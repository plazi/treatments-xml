<subSubSection id="04DF41D227864DC67DA76F7201D78BA7" pageId="1" pageNumber="108" type="nomenclature">
<paragraph id="F33FD2A2C8F7CA37C0D16DBA3ADB842F" pageId="1" pageNumber="108">
<taxonomicName id="1C953B3A9527B5651704B3B73C2262E5" authority="Y. Yang &amp; X. Yang" authorityName="Motschoulsky" authorityYear="1858" class="Insecta" family="Cantharidae" genus="Themus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Themus (Themus) dimorphus" order="Coleoptera" pageId="1" pageNumber="108" phylum="Arthropoda" rank="species" species="dimorphus" subGenus="Themus">Themus (Themus) dimorphus Y. Yang &amp; X. Yang</taxonomicName>
<taxonomicNameLabel id="A43AA2CE3BE65FEFABB074F86449BD9A" pageId="1" pageNumber="108">sp. n.</taxonomicNameLabel>
Figs 1
<normalizedToken id="65D334B50E810A6B5ED46F9731BA149B" originalValue="A–B">A-B</normalizedToken>
, 2
<normalizedToken id="1B12EDA67D701C4778BB928D64796F69" originalValue="A–C">A-C</normalizedToken>
