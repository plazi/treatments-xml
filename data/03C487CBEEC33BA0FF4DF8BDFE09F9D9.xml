<th id="768C5DE3EEC4C5CAFD9CFCFDFCC1FC8C" box="[601,772,819,845]" gridcol="4" gridrow="0" pageId="405" pageNumber="406">Endopod</th>
</tr>
<tr id="355D349FEEC4C5CAFF4DFC9BFCC1FCB1" box="[136,772,853,880]" gridrow="1" pageId="405" pageNumber="406">
<th id="768C5DE3EEC4C5CAFF4DFC9BFF0DFCB1" box="[136,200,853,880]" gridcol="0" gridrow="1" pageId="405" pageNumber="406">Leg 1</th>
<td id="768C5DE3EEC4C5CAFECFFC9BFEEAFCB1" box="[266,303,853,880]" gridcol="1" gridrow="1" pageId="405" pageNumber="406">0-1</td>
<td id="768C5DE3EEC4C5CAFE8AFC9BFEB1FCB1" box="[335,372,853,880]" gridcol="2" gridrow="1" pageId="405" pageNumber="406">1-I</td>
<td id="768C5DE3EEC4C5CAFE59FC9BFD82FCB1" box="[412,583,853,880]" gridcol="3" gridrow="1" pageId="405" pageNumber="406">I-1; I-1; III, I, 4</td>
<td id="768C5DE3EEC4C5CAFD9CFC9BFCC1FCB1" box="[601,772,853,880]" gridcol="4" gridrow="1" pageId="405" pageNumber="406">0-1; 0-1; 1, 2, 3</td>
