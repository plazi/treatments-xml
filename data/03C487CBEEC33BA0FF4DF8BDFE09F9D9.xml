<td id="768C5DE3EEC4C5CAFECFFC57FEEAFC75" box="[266,303,921,948]" gridcol="1" gridrow="3" pageId="405" pageNumber="406">0-1</td>
<td id="768C5DE3EEC4C5CAFE8AFC57FEB1FC75" box="[335,372,921,948]" gridcol="2" gridrow="3" pageId="405" pageNumber="406">1-0</td>
<td id="768C5DE3EEC4C5CAFE59FC57FD82FC75" box="[412,583,921,948]" gridcol="3" gridrow="3" pageId="405" pageNumber="406">1-1; 1-1; 2, 1, 5</td>
<td id="768C5DE3EEC4C5CAFD9CFC57FCC1FC75" box="[601,772,921,948]" gridcol="4" gridrow="3" pageId="405" pageNumber="406">0-1; 0-1; 1, 2, 2</td>
</tr>
