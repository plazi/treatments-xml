<td id="768C5DE3EEC4C5CAFE8AFCB9FEB1FC53" box="[335,372,887,914]" gridcol="2" gridrow="2" pageId="405" pageNumber="406">1-0</td>
<td id="768C5DE3EEC4C5CAFE59FCB9FD82FC53" box="[412,583,887,914]" gridcol="3" gridrow="2" pageId="405" pageNumber="406">1-1; 1-1; 3, 1, 5</td>
