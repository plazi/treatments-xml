<taxonomicName id="03FD4D09B667CD0F5832F8B4FE10A32A" authority="Sanborn, 2020" authorityName="Sanborn" authorityYear="2020" box="[283,485,1875,1899]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="bitorquata" status="n. sp.">
<emphasis id="F689EA98B667CD0F5832F8B4FE10A32A" box="[283,485,1875,1899]" italics="true" pageId="58" pageNumber="59">Carineta bitorquata</emphasis>
