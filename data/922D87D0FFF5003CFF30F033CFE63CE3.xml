<taxonomicName id="DD844D45FFF5003CFC51F2C8C81D3C87" authority="Verhoeff, 1910" authorityName="Verhoeff" authorityYear="1910" box="[1014,1368,919,944]" class="Diplopoda" family="Glomeridae" genus="Hyleoglomeris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Glomerida" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
