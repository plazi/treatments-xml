Known from 
<collectingCountry id="F8B0760D1C6C7713FD894B162A3E7DB5" box="[522,619,1128,1154]" name="Ethiopia" pageId="12" pageNumber="13">Ethiopia</collectingCountry>
(
<bibRefCitation id="E4364B6C1C6C7713FDF94B162B467DB5" author="Krabbe" box="[634,787,1128,1154]" pageId="12" pageNumber="13" refString="Krabbe, E. (1982) Systematik der Spirostreptidae (Diplopoda, Spirostreptomorpha). Abhandlungen des Naturwissenschaftlichen Vereins in Hamburg. Verlag Paul Parey. Hamburg, 476 pp." type="book" year="1982">
Krabbe 
<number id="366F24BA1C6C7713FD5B4B162B467DB5" box="[728,787,1128,1154]" pageId="12" pageNumber="13" value="1982.0">1982</number>
</bibRefCitation>
