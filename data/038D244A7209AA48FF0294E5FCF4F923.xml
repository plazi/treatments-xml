<taxonomicName id="4C24EEDF7209AA48FC6492CCFBFEFA67" box="[1009,1062,1428,1451]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="18" pageNumber="549" phylum="Tracheophyta" rank="genus">
