<taxonomicName id="B7E94D65FFA4A51C1A08F95EAC1CFBD2" authorityName="Lacaita" authorityYear="1911" box="[312,578,1057,1083]" class="Magnoliopsida" family="Fabaceae" genus="Onobrychis" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="242" phylum="Tracheophyta" rank="species" species="tenoreana">Onobrychis tenoreana</taxonomicName>
