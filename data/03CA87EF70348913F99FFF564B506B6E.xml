<taxonomicName id="4C634D7A70348913FB87FDF44B5C6B6E" authority="Linnaeus, 1771" authorityName="Linnaeus" authorityYear="1771" box="[655,1030,576,601]" class="Gastropoda" family="Patellidae" genus="Patella" kingdom="Animalia" order="Pezizales" pageId="22" pageNumber="23" phylum="Mollusca" rank="species" species="noachina">
<emphasis id="B917EAEB70348913FB87FDF44C156B6F" box="[655,847,576,601]" italics="true" pageId="22" pageNumber="23">Patella noachina</emphasis>
Linnaeus, 
<number id="3DAB24DE70348913FAC3FDF44B5C6B6E" box="[971,1030,576,600]" pageId="22" pageNumber="23" value="1771.0">1771</number>
