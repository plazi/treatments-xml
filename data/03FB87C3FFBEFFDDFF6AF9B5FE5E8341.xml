<taxonomicName id="4C524D56FFBEFFDDFEDBF9D1FD4C82A9" authority="Ingrisch, 2018" authorityName="Ingrisch" authorityYear="2018" box="[294,648,1654,1678]" class="Insecta" family="Gryllacrididae" genus="Siamgryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="44" pageNumber="245" phylum="Arthropoda" rank="species" species="rufa">
