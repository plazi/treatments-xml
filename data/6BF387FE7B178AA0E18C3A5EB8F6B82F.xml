<taxonomicName id="F210890DD10A709ACA55EBA487F363EF" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis fasciata" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="fasciata">Chrysis fasciata</taxonomicName>
