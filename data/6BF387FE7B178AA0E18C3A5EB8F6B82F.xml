<taxonomicName id="0503CA78B5B7680DD079EAD7D6A25C76" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis fasciata" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="fasciata">Chrysis fasciata</taxonomicName>
