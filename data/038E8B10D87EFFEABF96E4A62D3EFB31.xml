<collectionCode id="ED36A2C3D87EFFEABE08E78629AFFC31" box="[350,441,995,1019]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="106" pageNumber="105" type="Museum">USNM</collectionCode>
