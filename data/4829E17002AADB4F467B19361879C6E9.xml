<taxonomicName id="192D6CB650741BFCDDE4F6E60E2951D7" class="Actinopterygii" family="Rhamphichthyidae" genus="Iracema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Iracema" order="Gymnotiformes" pageId="1" pageNumber="2" phylum="Chordata" rank="genus">Iracema</taxonomicName>
