<taxonomicName id="4CEACE81DE1A40630032F93D7846555B" authority="Gahan" authorityName="Gahan" box="[322,560,848,874]" class="Insecta" family="Cerambycidae" genus="Glenea" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="subsimilis">
