<taxonomicName id="64AF136D942D2CB722B6C5208CE8EF38" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Calendula" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="485" phylum="Tracheophyta" rank="species" species="officinalis">
