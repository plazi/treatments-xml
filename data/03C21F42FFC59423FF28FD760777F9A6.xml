<collectionCode id="ED7A3691FFC09423FD81FA0304DFF9DB" box="[574,653,1520,1544]" pageId="12" pageNumber="139">NASU</collectionCode>
, 
<collectingRegion id="49AF60B6FFC09423FD27FA1C049CF9DB" box="[664,718,1519,1544]" country="Ukraine" name="L'viv" pageId="12" pageNumber="139">Lviv</collectingRegion>
