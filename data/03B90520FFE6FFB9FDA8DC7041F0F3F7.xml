<paragraph id="8BAFB436FFE6FFB4FF7BDF7542A6F314" blockId="7.[213,908,574,598]" box="[213,908,574,598]" pageId="7" pageNumber="2879">
<materialsCitation id="3B78BE6BFFE6FFB4FF7BDF7542A2F314" box="[213,904,574,598]" collectingDate="2006-05-20" county="From Xiniu Cave" elevation="1242" latitude="25.05" location="Anlong county" longLatPrecision="1248" longitude="105.48333" municipality="Dushan town" pageId="7" pageNumber="2879" specimenCount="1" specimenCount-female="1" stateProvince="Guizhou" typeStatus="paratype">
