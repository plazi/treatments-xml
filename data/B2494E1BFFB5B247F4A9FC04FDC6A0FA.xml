SPECIES
</collectionCode>
. — 
<taxonomicName id="FDE0848EFFB5B247F56AFBCEFD82A111" authority="Haeckel, 1872" authorityName="Haeckel" authorityYear="1872" box="[291,614,1026,1051]" class="Calcarea" family="Leucosoleniidae" genus="Ascyssa" kingdom="Animalia" order="Leucosolenida" pageId="14" pageNumber="217" phylum="Porifera" rank="species" species="troglodytes">
