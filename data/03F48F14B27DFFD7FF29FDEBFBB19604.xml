<emphasis id="B929E210B27CFFD7FB18FEB8FA8B9620" box="[1190,1327,295,320]" italics="true" pageId="7" pageNumber="283">
<taxonomicName id="4C5D4581B27CFFD7FB18FEB8FA8F9620" authorityName="Sasa" authorityYear="1961" box="[1190,1323,295,320]" class="Arachnida" family="Pygmephoridae" genus="Bakerdania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="283" phylum="Arthropoda" rank="genus">Bakerdania</taxonomicName>
