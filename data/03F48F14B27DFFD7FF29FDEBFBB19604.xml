<taxonomicName id="4C5D4581B27CFFD7FC32FED3FBAA9605" authorityName="Sasa" authorityYear="1961" box="[908,1038,332,357]" class="Arachnida" family="Pygmephoridae" genus="Bakerdania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="283" phylum="Arthropoda" rank="genus">
<emphasis id="B929E210B27CFFD7FC32FED3FBAA9605" box="[908,1038,332,357]" italics="true" pageId="7" pageNumber="283">Bakerdania</emphasis>
</taxonomicName>
