<taxonomicName id="B18A16AE34EEF67B7F5AA665800B1AE6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34906" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName="Polyrhachis jacksoniana Roger" lsidName-HNS="Polyrhachis jacksoniana Roger" order="Hymenoptera" pageNumber="158" phylum="Arthropoda" rank="species" species="jacksoniana">Polyrhachis jacksoniana</taxonomicName>
