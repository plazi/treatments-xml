<taxonomicName id="27CA4D35FF6FFF0959D7FE72FD326361" authority="Macq." authorityName="Macq." box="[457,641,510,536]" class="Insecta" family="Muscidae" genus="Mesembrina" kingdom="Animalia" order="Diptera" pageId="162" pageNumber="163" phylum="Arthropoda" rank="species" species="mystacea">
