<taxonomicName id="27CA4D35FF6FFF095BAEFE57FAE6608D" authority="Rondani, 1862" authorityName="Rondani" authorityYear="1862" box="[944,1365,474,500]" class="Insecta" family="Muscidae" genus="Mesembrina" kingdom="Animalia" order="Diptera" pageId="162" pageNumber="163" phylum="Arthropoda" rank="species" species="matutina">
