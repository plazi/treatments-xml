<collectionCode id="C09253CEFF47E0D8FE2D6605FD89F886" box="[496,576,1831,1857]" country="Costa Rica" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="151" pageNumber="152">INBio</collectionCode>
