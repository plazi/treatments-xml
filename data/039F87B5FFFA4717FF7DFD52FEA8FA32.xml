</taxonomicName>
<taxonomicNameLabel id="A27157CAFFFB4717FD5AFED1FC8EFE8A" box="[725,836,265,289]" pageId="63" pageNumber="162" rank="species">comb nov.</taxonomicNameLabel>
(from 
<taxonomicName id="4C364D20FFFB4717FC02FED3FC33FE89" authorityName="Marshall" authorityYear="1956" box="[909,1017,267,290]" class="Insecta" family="Curculionidae" genus="Platysimus" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="162" phylum="Arthropoda" rank="genus">
<emphasis id="B942EAB1FFFB4717FC02FED3FC33FE89" box="[909,1017,267,290]" italics="true" pageId="63" pageNumber="162">Platysimus</emphasis>
