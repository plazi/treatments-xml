<taxonomicName id="36D84D4EFFDFFF942C92190AD218E269" baseAuthorityName="Walker" baseAuthorityYear="1866" box="[767,869,1363,1389]" class="Insecta" family="Staphylinidae" genus="Xenodusa" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="105" phylum="Arthropoda" rank="species" species="reflexa">
<emphasis id="C3ACEADFFFDFFF942C92190AD218E269" box="[767,869,1363,1389]" italics="true" pageId="20" pageNumber="105">X. reflexa</emphasis>
</taxonomicName>
