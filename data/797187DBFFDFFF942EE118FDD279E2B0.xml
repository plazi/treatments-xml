<taxonomicName id="36D84D4EFFDFFF942F4419C4D346E2B0" authority="Mayer" authorityName="Mayer" authorityYear="1886" box="[297,571,1434,1460]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="105" phylum="Arthropoda" rank="species" species="subpolita">
