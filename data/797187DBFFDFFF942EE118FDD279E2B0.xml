<taxonomicName id="36D84D4EFFDFFF942C951929D190E2B0" authority="Wheeler" authorityName="Wheeler" authorityYear="1910" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="105" phylum="Arthropoda" rank="subSpecies" species="herculeanus" subSpecies="modoc">
