<taxonomicName id="36D84D4EFFDFFF942DA1190AD002E295" authority="(Fitch)" baseAuthorityName="Fitch" baseAuthorityYear="1855" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="105" phylum="Arthropoda" rank="species" species="noveboracensis">
<emphasis id="C3ACEADFFFDFFF942DA1190AD05AE294" italics="true" pageId="20" pageNumber="105">Camponotus noveboracensis</emphasis>
(Fitch)
</taxonomicName>
, 
<taxonomicName id="36D84D4EFFDFFF942FE01929D397E295" authority="(Smith)" baseAuthorityName="Smith" baseAuthorityYear="1858" box="[397,746,1398,1425]" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="105" phylum="Arthropoda" rank="species" species="laevigatus">
<emphasis id="C3ACEADFFFDFFF942FE01929D3FAE294" box="[397,647,1398,1425]" italics="true" pageId="20" pageNumber="105">Camponotus laevigatus</emphasis>
