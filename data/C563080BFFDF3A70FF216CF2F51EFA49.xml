<number id="FB02AB3AFFDF3A73FF216CF2F088F8F3" box="[151,167,1771,1797]" pageId="13" pageNumber="14" value="9.0">9</number>
. 
<taxonomicName id="8ACAC29EFFDF3A73FF026CF5F23DF8F3" authority="Nikitsky" authorityName="Nikitsky" box="[180,530,1771,1798]" class="Insecta" family="Derodontidae" genus="Laricobius" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="kovalevi">
<emphasis id="7FBE650FFFDF3A73FF026CF5F18EF8F0" bold="true" box="[180,417,1772,1798]" italics="true" pageId="13" pageNumber="14">Laricobius kovalevi</emphasis>
Nikitsky
</taxonomicName>
