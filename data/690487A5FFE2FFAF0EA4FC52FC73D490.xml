<taxonomicName id="26AD4D30FFE2FFAF0988F9FEFA9CD55E" authority="Keyserling &amp; Blasius, 1830" authorityName="Peters" authorityYear="1871" box="[1207,1369,1555,1581]" class="Mammalia" family="Vespertilionidae" genus="Vesperugo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="33" pageNumber="225" phylum="Chordata" rank="species" species="pulveratus">
