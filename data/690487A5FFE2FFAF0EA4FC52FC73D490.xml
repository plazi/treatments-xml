<taxonomicName id="26AD4D30FFE2FFAF09FEF8CDFAECD449" authorityName="Kolenati" authorityYear="1856" box="[1217,1321,1824,1850]" class="Mammalia" family="Vespertilionidae" genus="Hypsugo" kingdom="Animalia" order="Chiroptera" pageId="33" pageNumber="225" phylum="Chordata" rank="genus">
