<taxonomicName id="26AD4D30FFE2FFAF0ECAF9FEFB4FD55E" authorityName="Thomas" authorityYear="1916" box="[1013,1162,1555,1581]" class="Mammalia" family="Vespertilionidae" genus="Pipistrellus" kingdom="Animalia" order="Chiroptera" pageId="33" pageNumber="225" phylum="Chordata" rank="species" species="cadornae">
