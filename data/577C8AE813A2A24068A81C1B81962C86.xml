<paragraph id="4FCA0E25965F1475E9039F0B30D9A4B8" pageId="54" pageNumber="55">
<emphasis id="BA4EDE0AB426E251291A4359BE68E075" bold="true" pageId="54" pageNumber="55">Brazil</emphasis>
, 
<normalizedToken id="BDCCD843094359302F823848A66D7B9F" originalValue="Pará">Para</normalizedToken>
, 
<normalizedToken id="10F52B85180C4B9EF6635DB81A8304CE" originalValue="Belém">Belem</normalizedToken>
, 5m, 
<geoCoordinate id="EBCE0E2C0F822E248EB029543AFCF980" degrees="1.46" direction="south" orientation="latitude" precision="555" value="-1.46">1.46°S</geoCoordinate>
, 
<geoCoordinate id="80F491F1C6DAE1E9E10681A8A7D235B6" degrees="48.42" direction="west" orientation="longitude" precision="555" value="-48.42">48.42°W</geoCoordinate>
