<paragraph id="8BAB36EEC953FFE47E93FD501DF112D0" blockId="44.[151,1135,532,751]" box="[151,952,648,674]" pageId="44" pageNumber="45">
<materialsCitation id="3B7C3CB3C953FFE47E93FD501DE712D0" box="[151,942,648,674]" location="Eastern Atlantic" pageId="44" pageNumber="45" specimenCount="1" stateProvince="Hawaii">
