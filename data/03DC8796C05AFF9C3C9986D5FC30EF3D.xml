<taxonomicName id="4C754D03C05BFF9C3F98828FFBCDECFA" box="[918,1043,439,464]" class="Insecta" family="Noctuidae" genus="Atrovirensis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="214" phylum="Arthropoda" rank="species" species="furcatus">
