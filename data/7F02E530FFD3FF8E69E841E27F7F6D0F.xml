<taxonomicName id="30AB2FA5FFD0FF8E699147377DEC6D0F" baseAuthorityName="Giebel" baseAuthorityYear="1874" box="[238,457,331,357]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Psocodea" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="thoracica">
