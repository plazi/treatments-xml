<taxonomicName id="30AB2FA5FFD0FF8E6B41475B7F3C6D2B" baseAuthorityName="Giebel" baseAuthorityYear="1874" box="[574,793,295,321]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Psocodea" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="thoracica">
