<taxonomicName id="30AB2FA5FFD0FF8E6D3A477F788A6D77" box="[1093,1199,259,285]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Psocodea" pageId="34" pageNumber="35" phylum="Arthropoda" rank="genus">
