<taxonomicName id="4C0A4D5DFFDBFFECFF5B9A47A9FCC4D8" authorityName=", Thollesson &amp; Norenburg" authorityYear="2003" box="[236,489,880,906]" class="Enopla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monostilifera" pageId="3" pageNumber="154" phylum="Nemertea" rank="order">MONOSTILIFERA</taxonomicName>
