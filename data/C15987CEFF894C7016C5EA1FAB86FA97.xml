<taxonomicName id="8EF04D5BFF894C701774EAD4A92AFB2E" authority="Signoret." authorityName="Signoret." box="[294,621,1197,1221]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="33" phylum="Arthropoda" rank="species" species="aceris">
