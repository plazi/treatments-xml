<taxonomicName id="CDA04D5FAF49FFFA8121FCB0FF3CFCF7" authority="" authorityName="Cope" authorityYear="1876" box="[51,234,858,879]" class="Reptilia" family="Dinodontidae" genus="Laelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="3" pageNumber="343" phylum="Chordata" rank="species" species="hazenianus" status="sp. nov.">
<emphasis id="38D4EACEAF49FFFA8121FCB0FF3CFCF7" bold="true" box="[51,234,858,879]" pageId="3" pageNumber="343">Laelaps hazenianus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="23E757B5AF49FFFA81EAFCB4FEEAFCF5" box="[248,316,863,877]" pageId="3" pageNumber="343" rank="species">sp. nov.</taxonomicNameLabel>
</paragraph>
