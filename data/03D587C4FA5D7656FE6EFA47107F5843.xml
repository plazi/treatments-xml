<quantity id="4C849B37FA5D7656FE07F9CD17D15997" box="[836,925,1648,1672]" metricMagnitude="-1" metricUnit="m" metricValue="8.95" pageId="20" pageNumber="865" unit="cm" value="89.5">89.5 cm</quantity>
