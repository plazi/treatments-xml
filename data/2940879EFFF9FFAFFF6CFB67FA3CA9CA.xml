<quantity id="66119B6DFFF9FFAFFAFFFA23FAC8AAE7" box="[1284,1384,1409,1430]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="10.0" metricValueMin="7.0" pageId="5" pageNumber="6" unit="mm" value="0.85" valueMax="1.0" valueMin="0.7">0.7–1 mm</quantity>
)....................................................................................... 
