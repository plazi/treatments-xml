<taxonomicName id="4C6D4D13FFB7FFAC5148F7ABFF3BF7A0" authorityName="H. Milne Edwards" authorityYear="1852" box="[110,178,2091,2112]" class="Malacostraca" family="Macrophthalmidae" genus="Macrophthalmus" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="38" phylum="Arthropoda" rank="subGenus" subGenus="Euplax">
