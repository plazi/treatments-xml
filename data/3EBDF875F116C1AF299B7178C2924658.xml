<paragraph id="D74DDCB143A7688C01FB0B1C27CA56E6">
<taxonomicName id="2D1B25A5C26F588856CAD8A872891971" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" class="Arachnida" family="Oribatulidae" genus="Oribates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" phylum="Arthropoda" rank="species" species="fusifer">Oribates fusifer</taxonomicName>
