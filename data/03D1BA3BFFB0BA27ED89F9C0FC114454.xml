<heading id="D08FBC41FFB0BA20ED89F9C0FE7444F1" box="[239,462,1648,1672]" centered="true" fontSize="36" level="2" pageId="43" pageNumber="44" reason="8">
<taxonomicName id="4C7870AEFFB0BA20ED89F9C0FE7444F1" box="[239,462,1648,1672]" class="Reptilia" family="Troodontidae" genus="Archaeornithoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="43" pageNumber="44" phylum="Chordata" rank="genus">
<emphasis id="B90CD73FFFB0BA20ED89F9C0FE7444F1" box="[239,462,1648,1672]" italics="true" pageId="43" pageNumber="44">ARCHAEORNITHOIDES</emphasis>
</taxonomicName>
