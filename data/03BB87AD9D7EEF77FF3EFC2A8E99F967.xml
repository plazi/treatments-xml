<taxonomicName id="4C124D389D7EEF77FB23F9CB8E9EF9CF" authority="Sasakawa" authorityName="Sasakawa" authorityYear="2001" box="[1162,1435,1573,1594]" class="Insecta" family="Lauxaniidae" genus="Homoneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="512" phylum="Arthropoda" rank="species" species="conspicua" subGenus="Homoneura">
