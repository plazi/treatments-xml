<number id="96722494760F2051F83AF8B817444A1D" box="[263,300,1846,1868]" pageId="13" pageNumber="264" value="115.0">115</number>
(
<typeStatus id="FF018811760F2051F804F8B817054A1D" box="[313,365,1846,1868]" pageId="13" pageNumber="264">Type</typeStatus>
locality: 
<collectingCountry id="58AD7623760F2051F8F0F8B914674A1D" box="[461,527,1847,1868]" name="South Korea" pageId="13" pageNumber="264">Korea</collectingCountry>
