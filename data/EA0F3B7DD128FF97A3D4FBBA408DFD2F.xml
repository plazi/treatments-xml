<taxonomicName id="A5A6F1E8D128FF92A3D4FBBA4098FBD8" box="[151,399,1113,1139]" class="Polychaeta" family="Flabelligeridae" genus="Flabegraviera" kingdom="Animalia" order="Terebellida" pageId="1" pageNumber="478" phylum="Annelida" rank="species" species="fujiae" status="sp. nov.">
<emphasis id="50D25679D128FF92A3D4FBBA4098FBD8" bold="true" box="[151,399,1113,1139]" italics="true" pageId="1" pageNumber="478">Flabegraviera fujiae</emphasis>
