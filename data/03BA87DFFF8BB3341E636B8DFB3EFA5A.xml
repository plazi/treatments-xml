<taxonomicName id="4C134D4AFF8BB3341E636B8DFC78FD37" authority="Juarrero &amp; Martinez-Iglesias, 1997" authorityName="Juarrero &amp; Martinez-Iglesias" authorityYear="1997" box="[264,936,657,683]" class="Malacostraca" family="Laomediidae" genus="Espeleonaushonia" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
