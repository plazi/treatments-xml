<taxonomicName id="4C2F0DB1E263FFBD2604C7B8FAD4A0E6" box="[1052,1328,969,993]" class="Arachnida" family="Araneidae" genus="Cyrtophora" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="156" phylum="Arthropoda" rank="species" species="moluccensis">
