<taxonomicName id="4EFD4D42FF8D1836C6436B9E8BD76C89" authority="Povolny" authorityName="Povolny" box="[639,902,1413,1439]" class="Insecta" family="Gelechiidae" genus="Scrobipalpa" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="chitensis">
