<taxonomicName id="99D14D11FF97FF8EFE87083EFE6CFBC7" authorityName="Kontschán" authorityYear="2010" box="[331,439,1047,1071]" class="Arachnida" family="Uropodidae" genus="Depressorotunda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="1465" phylum="Arthropoda" rank="species" species="malayana">
<emphasis id="6CA5EA80FF97FF8EFE87083EFE6CFBC7" box="[331,439,1047,1071]" italics="true" pageId="5" pageNumber="1465">malayana</emphasis>
