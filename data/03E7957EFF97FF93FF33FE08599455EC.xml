<taxonomicName id="4C4E5FEBFF97FF9DFC54FD9D58B65797" authority="(Muller, 1776)" authorityName="Muller" authorityYear="1776" baseAuthorityName="Muller" baseAuthorityYear="1776" box="[1008,1436,618,643]" class="Aves" family="Fringillidae" genus="Zonotrichia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="6" pageNumber="239" phylum="Chordata" rank="species" species="capensis">
