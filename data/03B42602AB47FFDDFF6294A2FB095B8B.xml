<quantity id="4CE53AF1AB47FFDEFC3295EEFBBF5C8D" box="[967,1049,1977,2003]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="595" unit="mm" value="10.0">10 mm</quantity>
