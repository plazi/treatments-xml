<taxonomicName id="8DDA0E74AA323B6F984867EA72A599BF" class="Insecta" family="Anthribidae" genus="Mecocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecocerus rhombeus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="rhombeus">Mecocerus rhombeus</taxonomicName>
