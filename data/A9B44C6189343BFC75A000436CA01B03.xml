<taxonomicName id="CCD8295B125FD5CEA2F75E0D02E0C6A1" authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
, 
<taxonomicName id="94619B6FE3DFDAE9F9520A7142688E33" family="Anthribidae" lsidName="" pageId="0" pageNumber="1" rank="family">Anthribidae</taxonomicName>
