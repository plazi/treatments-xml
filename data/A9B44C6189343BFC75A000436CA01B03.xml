<taxonomicName id="923525EEAECBD2CA678BFD2B39936577" class="Insecta" family="Anthribidae" genus="Mecocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecocerus rhombeus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="rhombeus">Mecocerus rhombeus</taxonomicName>
