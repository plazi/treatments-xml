<paragraph id="D0523691A5583F748EF3F8AEFB84592C" blockId="64.[151,1120,1820,1872]" box="[151,1120,1850,1872]" pageId="64" pageNumber="65">
<typeStatus id="0F568833A5583F748EF3F8AEFF2D592C" box="[151,201,1851,1872]" pageId="64" pageNumber="65">Type</typeStatus>
species: 
<taxonomicName id="17ED4D12A5583F748F42F8AFFC81592C" authority="Skoracki, Bochkov and Wauthy, 2004" authorityName="Skoracki, Bochkov and Wauthy" authorityYear="2004" box="[294,869,1850,1872]" class="Arachnida" family="Syringophilidae" genus="Picobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="locustella">
