</taxonomicName>
(
<taxonomicName id="D9D3459899209F64A4A0FA2FFDB2FA2F" box="[439,541,1495,1516]" class="Magnoliopsida" family="Salicaceae" kingdom="Plantae" order="Malpighiales" pageId="6" pageNumber="392" phylum="Tracheophyta" rank="family">Salicaceae</taxonomicName>
