one record is from 
<taxonomicName id="D9D3459899209F64A5F7FA2FFE03FA2F" authority="Lacksch." authorityName="Lacksch." box="[224,428,1495,1516]" class="Magnoliopsida" family="Salicaceae" genus="Salix" kingdom="Plantae" order="Malpighiales" pageId="6" pageNumber="392" phylum="Tracheophyta" rank="species" species="rorida">
