<taxonomicName id="50122C87995F4E90EE164856BA062623" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pholcus andulau" order="Araneae" pageId="35" pageNumber="86" phylum="Arthropoda" rank="species" species="andulau">Pholcus andulau</taxonomicName>
