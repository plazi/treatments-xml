<taxonomicName id="28EFAA606436EDA051BDCBFAF0845157" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pholcus andulau" order="Araneae" pageId="35" pageNumber="86" phylum="Arthropoda" rank="species" species="andulau">Pholcus andulau</taxonomicName>
