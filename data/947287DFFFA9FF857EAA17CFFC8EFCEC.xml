<emphasis id="2EAFEADBFFAAFF857EFA10C2FEF5FD19" bold="true" box="[199,353,654,680]" pageId="11" pageNumber="491">Distribution.</emphasis>
<collectingCountry id="64CC7659FFAAFF857F5B10C3FE44FD18" box="[358,464,655,681]" name="Australia" pageId="11" pageNumber="491">Australia</collectingCountry>
, 
<collectingCountry id="64CC7659FFAAFF857FE610C3FDDFFD18" box="[475,587,655,681]" name="Indonesia" pageId="11" pageNumber="491">Indonesia</collectingCountry>
, 
<collectingCountry id="64CC7659FFAAFF857C6810C3FD2AFD18" box="[597,702,655,681]" name="Malaysia" pageId="11" pageNumber="491">Malaysia</collectingCountry>
, 
<collectingCountry id="64CC7659FFAAFF857CF710C3FCDFFD18" box="[714,843,655,681]" name="Philippines" pageId="11" pageNumber="491">Philippines</collectingCountry>
, 
<collectingCountry id="64CC7659FFAAFF857D6B10C3FC3FFD18" box="[854,939,655,681]" name="Taiwan" pageId="11" pageNumber="491">Taiwan</collectingCountry>
, 
<collectingCountry id="64CC7659FFAAFF857D8B10C3FC68FD18" box="[950,1020,655,681]" name="China" pageId="11" pageNumber="491">China</collectingCountry>
, 
