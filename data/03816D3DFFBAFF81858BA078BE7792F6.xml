<quantity id="4CD071CEFFBBFF8186DFA4F5B809969F" box="[963,1064,940,965]" metricMagnitude="-4" metricUnit="m" metricValue="6.6" pageId="109" pageNumber="110" unit="mm" value="0.66">0.66 mm</quantity>
