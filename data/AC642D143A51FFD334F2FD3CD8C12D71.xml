<number id="92058E253A51FFD334A2FA43DD8E2EC5" box="[199,213,1487,1511]" pageId="24" pageNumber="317" value="2.0">2</number>
specimens
</specimenCount>
of 
<number id="92058E253A51FFD335E1FA43DCFB2EC5" box="[388,416,1487,1511]" pageId="24" pageNumber="317" value="72.0">72</number>
and 
<quantity id="E33531E73A51FFD335BAFA43DF722EC4" box="[479,553,1487,1511]" metricMagnitude="-1" metricUnit="m" metricValue="8.2" pageId="24" pageNumber="317" unit="cm" value="82.0">82 cm</quantity>
