<number id="92058E253A51FFD337F6FA42DE892EC5" box="[915,978,1486,1511]" pageId="24" pageNumber="317" value="1683.0">1,683</number>
and 
<quantity id="E33531E73A51FFD33074FA42D9292EC4" box="[1041,1138,1486,1511]" metricMagnitude="3" metricUnit="m" metricValue="1.685" pageId="24" pageNumber="317" unit="m" value="1685.0">1,685 m</quantity>
