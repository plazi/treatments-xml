<collectingCountry id="F32373C3D4157839FC65FF6F8B40FF46" box="[912,1025,223,248]" name="Colombia" pageId="11" pageNumber="512">Colombia</collectingCountry>
, 
<collectingCountry id="F32373C3D4157839FBFAFF6F8BC3FF46" box="[1039,1154,223,248]" name="Venezuela" pageId="11" pageNumber="512">Venezuela</collectingCountry>
(
<bibRefCitation id="EFA54EA2D4157839FB66FF6F8A71FF46" author="Metcalf" box="[1171,1328,223,248]" pageId="11" pageNumber="512" refString="Metcalf, Z. P. (1963) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="journal article" year="1963">
Metcalf 
<number id="3DFC2174D4157839FB00FF6F8A71FF46" box="[1269,1328,223,248]" pageId="11" pageNumber="512" value="1963.0">1963</number>
</bibRefCitation>
; 
<bibRefCitation id="EFA54EA2D4157839FACAFF6F8F92FEA2" author="Sanborn" pageId="11" pageNumber="512" refString="Sanborn, A. F. (2013) Catalogue of the Cicadoidea (Hemiptera: Cicadoidea). With contributions to the bibliography by Martin H. Villet. Elsevier / Academic Press, San Diego, 1001 pp." type="book" year="2013">
Sanborn 
<number id="3DFC2174D4157839FF62FEB48F92FEA2" box="[151,211,260,284]" pageId="11" pageNumber="512" value="2013.0">2013</number>
</bibRefCitation>
), and 
<collectingCountry id="F32373C3D4157839FEE0FEB48EC4FEA3" box="[277,389,260,285]" name="Argentina" pageId="11" pageNumber="512">Argentina</collectingCountry>
(
<bibRefCitation id="EFA54EA2D4157839FE66FEB48DDCFEA2" author="Sanborn" box="[403,669,260,285]" pageId="11" pageNumber="512" refString="Sanborn, A. F. &amp; Heath, M. S. (2014) The cicadas of Argentina with new records, a new genus and fifteen new species (Hemiptera: Cicadoidea: Cicadidae). Zootaxa, 3883 (1), 1 - 94. http: // dx. doi. org / 10.11646 / zootaxa. 3883.1.1" type="journal article" year="2014">
Sanborn &amp; Heath 
