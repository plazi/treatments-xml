<typeStatus id="54F242EFFFA3FFA9FF21D19F6512FD2D" box="[199,258,657,682]" pageId="35" pageNumber="36">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54F242EFFFA3FFA9FE9AD19C6600FD2D" box="[380,528,658,682]" pageId="35" pageNumber="36">HOLOTYPE</typeStatus>
. ♂, N.E. 
<collectingCountry id="F35EBCDDFFA3FFA9FD9DD19C66DAFD2E" box="[635,714,658,681]" name="Myanmar" pageId="35" pageNumber="36">Burma</collectingCountry>
, Kambaiti, 
<quantity id="4CB151A8FFA3FFA9FCADD19C67B0FD2D" box="[843,928,657,682]" metricMagnitude="3" metricUnit="m" metricValue="2.1336" pageId="35" pageNumber="36" unit="ft" value="7000.0">7000 ft</quantity>
., 
<date id="FFF7DA8DFFA3FFA9FC56D19F6002FD2D" box="[944,1042,657,682]" pageId="35" pageNumber="36" value="1934-05-05">
<number id="3D81EE6AFFA3FFA9FC56D19F67AEFD2D" box="[944,958,657,682]" pageId="35" pageNumber="36" value="5.0">5</number>
.v. 
<number id="3D81EE6AFFA3FFA9FC32D19F6002FD2D" box="[980,1042,657,682]" pageId="35" pageNumber="36" value="1934.0">1934</number>
