<taxonomicName id="52AD6585E23CFFC3D914FEE8FC3C9527" box="[864,920,290,313]" class="Arachnida" family="Trechaleidae" genus="Enna" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="468" phylum="Arthropoda" rank="genus">
