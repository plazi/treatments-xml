<taxonomicName id="FF924D026964FFB7FED5F8A459C2F833" authority="Goldschmidt, 2008" authorityName="Goldschmidt" authorityYear="2008" box="[355,741,1906,1929]" class="Arachnida" family="Anisitsiellidae" genus="Nilotonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="hodivaventy" status="subgen. et sp. nov." subGenus="Telotaolana">
