<td id="76AFA258817300664390F88EB6BCF869" box="[409,510,1913,1939]" gridcol="1" gridrow="7" pageId="1" pageNumber="130">0.59–0.66</td>
<td id="76AFA25881730066405EF88EB5FFF869" box="[599,701,1913,1939]" gridcol="2" gridrow="7" pageId="1" pageNumber="130">3.29–4.28</td>
<td id="76AFA258817300664122F88EB4D3F869" box="[811,913,1913,1939]" gridcol="3" gridrow="7" pageId="1" pageNumber="130">2.91–3.18</td>
<td id="76AFA2588173006641F7F88EB326F869" box="[1022,1124,1913,1939]" gridcol="4" gridrow="7" pageId="1" pageNumber="130">1.56–2.22</td>
