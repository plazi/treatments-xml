<taxonomicName id="4C265D93FFC45451BC0D33B53D5F109E" box="[879,1072,729,753]" class="Insecta" family="Cetoniidae" genus="Gymnetis" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="582" phylum="Arthropoda" rank="species" species="undetermined">
