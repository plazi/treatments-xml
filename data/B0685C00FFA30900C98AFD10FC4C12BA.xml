<emphasis id="0AB53104FFA20900C98AFA96FED0101F" bold="true" box="[151,274,1367,1389]" pageId="9" pageNumber="244">FIGURE 9.</emphasis>
<taxonomicName id="FFC19695FFA20900C804FA96FDB6101F" authority="(Navas)" baseAuthorityName="Navas" baseAuthorityYear="1905" box="[281,628,1367,1389]" class="Insecta" family="Berothidae" genus="Isoscelipteron" kingdom="Animalia" order="Neuroptera" pageId="9" pageNumber="244" phylum="Arthropoda" rank="species" species="pectinatum">
