<taxonomicName id="4C54AFE81058057C8E12FB6FFCD800A5" authority="Krishna &amp; Grimaldi, 2009" authorityName="Krishna &amp; Grimaldi" authorityYear="2009" box="[544,775,1175,1196]" class="Insecta" family="Termitidae" genus="Parvitermes" kingdom="Animalia" order="Blattodea" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="longinasus" status="n. sp.">
