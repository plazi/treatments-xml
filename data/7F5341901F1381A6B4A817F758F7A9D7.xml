73. 
<taxonomicName id="67CA983D3C09944388A1F52E2197FE8A" LSID="http://zoobank.org/71216E34-1FD2-44B2-B50F-8CC0532AED3D" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus pseudosimulans" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pseudosimulans">Trigonopterus pseudosimulans Riedel</taxonomicName>
