<taxonomicName id="4C204D2BFF83FFA2FDCA474FFC2EFC88" box="[566,924,769,796]" class="Liliopsida" family="Musaceae" genus="Musa" kingdom="Plantae" order="Zingiberales" pageId="4" pageNumber="53" phylum="Tracheophyta" rank="subSpecies" species="velutina" subSpecies="markkuana">
<emphasis id="B954EABAFF83FFA2FDCA474FFD09FC88" box="[566,699,769,795]" italics="true" pageId="4" pageNumber="53">M. velutina</emphasis>
subsp. 
<emphasis id="B954EABAFF83FFA2FCE8474CFC2EFC88" box="[788,924,769,795]" italics="true" pageId="4" pageNumber="53">markkuana</emphasis>
