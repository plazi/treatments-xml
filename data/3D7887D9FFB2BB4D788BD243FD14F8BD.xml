<taxonomicName id="72D14D4CFFB2BB4D7D68D147FD9EF9CD" authority="(Stimpson, 1860)" baseAuthorityName="Stimpson" baseAuthorityYear="1860" class="Malacostraca" family="Palaemonidae" genus="Palaemon" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="523" phylum="Arthropoda" rank="species" species="pacificus">
