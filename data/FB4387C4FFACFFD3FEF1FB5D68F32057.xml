<collectionCode id="15FBAE17FFACFFD3FDF9F9026E5623D5" box="[540,604,1714,1738]" country="South Africa" httpUri="http://grbio.org/cool/mhew-9czz" name="South African Museum" pageId="12" pageNumber="258">SAM</collectionCode>
); Blackwood (
<collectionCode id="15FBAE17FFACFFD3FF64F9606CCA23F7" box="[129,192,1744,1768]" country="South Africa" httpUri="http://grbio.org/cool/mhew-9czz" name="South African Museum" pageId="12" pageNumber="258">SAM</collectionCode>
); Black Mountain (
<collectionCode id="15FBAE17FFACFFD3FE6BF9606DDF23F7" box="[398,469,1744,1768]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" lsid="urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="12" pageNumber="258">ANIC</collectionCode>
