<figureCitation id="1305DB91C761FFAD1CCCFE7430D984DC" box="[159,244,410,435]" captionStart="FIGURES 31 ‒ 40" captionStartId="8.[151,262,1775,1798]" captionTargetBox="[158,1427,204,1749]" captionTargetId="figure@8.[151,1436,193,1754]" captionTargetPageId="8" captionText="FIGURES 31 ‒ 40. Sinella copiosa sp. nov.: 31, Lateral process of labial palp; 32, chaetae on the ventral side of head; 33, dorsal cephalic chaetotaxy; 34, trochanteral organ; 35, hind claw; 36 – 38, ventral tube. 36, anterior face; 37, posterior face; 38, lateral flap; 39, manubrial plaque; 40, mucro." httpUri="https://zenodo.org/record/229122/files/figure.png" pageId="10" pageNumber="563">
Fig. 
<number id="3DF6D533C761FFAD1C86FE7430D984DC" box="[213,244,410,435]" pageId="10" pageNumber="563" value="36.0">36</number>
</figureCitation>
); posteriorly with 
<number id="3DF6D533C761FFAD1D99FE7531F584DD" box="[458,472,411,434]" pageId="10" pageNumber="563" value="7.0">7</number>
apical smooth and 
<number id="3DF6D533C761FFAD1EEAFE7432EA84DD" box="[697,711,410,434]" pageId="10" pageNumber="563" value="9.0">9</number>
distal ciliate chaetae (
<figureCitation id="1305DB91C761FFAD1F9AFE74340D84DD" box="[969,1056,410,435]" captionStart="FIGURES 31 ‒ 40" captionStartId="8.[151,262,1775,1798]" captionTargetBox="[158,1427,204,1749]" captionTargetId="figure@8.[151,1436,193,1754]" captionTargetPageId="8" captionText="FIGURES 31 ‒ 40. Sinella copiosa sp. nov.: 31, Lateral process of labial palp; 32, chaetae on the ventral side of head; 33, dorsal cephalic chaetotaxy; 34, trochanteral organ; 35, hind claw; 36 – 38, ventral tube. 36, anterior face; 37, posterior face; 38, lateral flap; 39, manubrial plaque; 40, mucro." httpUri="https://zenodo.org/record/229122/files/figure.png" pageId="10" pageNumber="563">
Fig. 
<number id="3DF6D533C761FFAD1852FE74340D84DD" box="[1025,1056,410,434]" pageId="10" pageNumber="563" value="37.0">37</number>
</figureCitation>
