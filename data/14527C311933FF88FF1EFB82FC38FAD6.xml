<taxonomicName id="5BFBB6A41933FF88FF1EFB19FE86FB1D" box="[178,365,1222,1244]" class="Magnoliopsida" family="Lamiaceae" genus="Satureja" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="334" phylum="Tracheophyta" rank="species" species="hortensis">
