, 
<number id="225424C26E1AAB56FD4D7D5D1ACAE06C" box="[672,727,1589,1615]" pageId="28" pageNumber="29" value="0.26">0.26</number>
, 
<number id="225424C26E1AAB56FD0E7D5D1B07E06C" box="[739,794,1589,1615]" pageId="28" pageNumber="29" value="1.84">1.84</number>
