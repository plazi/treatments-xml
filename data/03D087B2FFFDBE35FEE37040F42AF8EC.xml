<taxonomicName id="4C794D27FFFFBE35FBDB758DF385FC19" authorityName="Thiele" authorityYear="1925" box="[1064,1196,994,1018]" class="Gastropoda" family="Acteonidae" genus="Acteon" kingdom="Animalia" pageId="27" pageNumber="614" phylum="Mollusca" rank="species" species="laetus">
