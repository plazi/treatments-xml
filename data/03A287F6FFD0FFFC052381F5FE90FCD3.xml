<emphasis id="B97FEAF2FFD0FFF9052381F5FD0FF986" bold="true" box="[284,714,1533,1559]" pageId="11" pageNumber="1953">
<emphasis id="B97FEAF2FFD0FFF9052381F5FE45F986" bold="true" box="[284,384,1533,1559]" italics="true" pageId="11" pageNumber="1953">Stegana</emphasis>
(
<emphasis id="B97FEAF2FFD0FFF905AD81F5FDEDF986" bold="true" box="[402,552,1533,1559]" italics="true" pageId="11" pageNumber="1953">Oxyphortica</emphasis>
