/ 
<typeStatus id="54FD0DD585150A0DFD6CD459FCD3FA96" box="[728,876,1338,1364]" pageId="4" pageNumber="371">PARATYPE</typeStatus>
/ 
<taxonomicName id="4C46C8F485150A0DFC31D458FC4FFA97" box="[901,1008,1339,1365]" class="Insecta" family="Anthribidae" genus="Merarius" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="371" phylum="Arthropoda" rank="genus">
