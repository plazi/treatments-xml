<taxonomicName id="7ADFAFDC0E45FF85FD9E2C1BFD37E933" box="[637,716,1879,1902]" class="Aves" family="Anatidae" genus="Carina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anseriformes" pageId="4" pageNumber="5" phylum="Chordata" rank="genus">
