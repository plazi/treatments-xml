<subSubSection id="CB905FFEFFC3FFE7FF47F921FC3CB535" pageId="16" pageNumber="167" type="materials_examined">
<paragraph id="83350C75FFC3FFE7FF47F921FB37B48D" blockId="16.[151,1436,1694,1935]" pageId="16" pageNumber="167">
<emphasis id="B1FED067FFC3FFE7FF47F921FE10B4A9" bold="true" box="[199,436,1730,1755]" pageId="16" pageNumber="167">
