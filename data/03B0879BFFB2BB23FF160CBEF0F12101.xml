<taxonomicName id="4C194D0EFFB2BB20FB5C0E4EF0F12419" authority="(Stein)" authorityName="Stein" baseAuthorityName="Stein" baseAuthorityYear="1857" box="[1245,1436,1820,1842]" class="Insecta" family="Nabidae" genus="Alloeorhynchus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="306" phylum="Arthropoda" rank="species" species="trimacula">
<emphasis id="B96DEA9FFFB2BB20FB5C0E4EF0392419" box="[1245,1364,1820,1842]" italics="true" pageId="1" pageNumber="306">A. trimacula</emphasis>
