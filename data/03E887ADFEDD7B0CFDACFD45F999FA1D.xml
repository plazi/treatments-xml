<paragraph id="8BFE36BBFEDD7B0CFF36FA03F999FA1D" blockId="269.[189,962,1420,1447]" box="[189,962,1420,1447]" pageId="269" pageNumber="270">
Known from 
<typeStatus id="54FA8819FEDD7B0CFED0FA03FBD5FA1C" box="[347,398,1420,1446]" pageId="269" pageNumber="270">type</typeStatus>
locality only, in 
<taxonomicName id="4C414D38FEDD7B0CFDD8FA03F894FA1C" box="[595,719,1420,1446]" class="Arachnida" family="Pholcidae" genus="Venezuela" kingdom="Animalia" order="Araneae" pageId="269" pageNumber="312" phylum="Arthropoda" rank="genus">Venezuela</taxonomicName>
, 
<collectingRegion id="4985F859FEDD7B0CFD51FA03F974FA1C" box="[730,815,1420,1446]" country="Venezuela" name="Merida" pageId="269" pageNumber="270">Mérida</collectingRegion>
