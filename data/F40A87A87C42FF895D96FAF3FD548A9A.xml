<taxonomicName id="BBA34D3D7C42FF895949FABBFB058A09" box="[1096,1226,1375,1399]" class="Magnoliopsida" family="Sapotaceae" kingdom="Plantae" order="Ericales" pageId="8" pageNumber="34" phylum="Tracheophyta" rank="family">Sapotaceae</taxonomicName>
