<taxonomicName id="BBA34D3D7C42FF895C1FFA84FE228A06" box="[286,493,1376,1400]" class="Magnoliopsida" family="Anacardiaceae" genus="Schinus" kingdom="Plantae" order="Sapindales" pageId="8" pageNumber="34" phylum="Tracheophyta" rank="species" species="polygama">
<emphasis id="4ED7EAAC7C42FF895C1FFA84FE228A06" box="[286,493,1376,1400]" italics="true" pageId="8" pageNumber="34">Schinus polygama</emphasis>
</taxonomicName>
(
<taxonomicName id="BBA34D3D7C42FF895CFEFABBFD678A09" box="[511,680,1375,1399]" class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="8" pageNumber="34" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
) [recorded as 
<taxonomicName id="BBA34D3D7C42FF895E51FA84FBF98A06" box="[848,1078,1376,1400]" class="Insecta" family="Noctuidae" genus="Argania" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="34" phylum="Arthropoda" rank="species" species="sideroxylon">
