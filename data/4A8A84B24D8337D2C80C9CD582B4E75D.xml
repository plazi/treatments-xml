<taxonomicName id="7D012A54ACD9BE8AE01CB5D1783F76B8" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis patriarchalis" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="patriarchalis">Chrysis patriarchalis</taxonomicName>
