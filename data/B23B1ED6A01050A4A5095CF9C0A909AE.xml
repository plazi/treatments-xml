<taxonomicName id="1B7812A8341D5ABC445A6E3B0449CD86" genus="D." lsidName="D. (Dennyus) hirundinis" pageId="0" pageNumber="67927" rank="species" species="hirundinis" subGenus="Dennyus">D. (Dennyus) hirundinis</taxonomicName>
