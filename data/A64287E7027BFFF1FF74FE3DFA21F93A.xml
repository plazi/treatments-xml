<taxonomicName id="E9EB4D72027BFFF1FAEFFB5FFA27FAD0" box="[1292,1435,1269,1291]" class="Actinopterygii" family="Platycephalidae" genus="Platycephalus" kingdom="Animalia" order="Scorpaeniformes" pageId="3" pageNumber="154" phylum="Chordata" rank="species" species="grandispinis">
