<taxonomicName id="4C544D75FFD2F661FB481D38FAA4FEE8" authority="Prathapan &amp; Shameem, 2017" authorityName="Prathapan &amp; Shameem" authorityYear="2017" box="[1272,1370,310,331]" class="Insecta" family="Chrysomelidae" genus="Wallacea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="383" phylum="Arthropoda" rank="species" species="jarawa" status="n. sp.">
