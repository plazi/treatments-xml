<quantity id="840FCAA2FC79931EFE2BFD6A4AB0FD52" box="[594,680,717,741]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="13" pageNumber="66" unit="mm" value="2.5">2.5 mm</quantity>
