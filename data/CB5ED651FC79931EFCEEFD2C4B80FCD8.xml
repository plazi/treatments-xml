<paragraph id="43486747FC79931EFCEEFD6A4AA9FD52" blockId="13.[151,737,717,778]" box="[151,689,717,741]" pageId="13" pageNumber="66">
<taxonomicName id="84F71CC4FC79931EFCEEFD6A4953FD52" box="[151,331,717,741]" class="Malacostraca" family="Nannastacidae" genus="Procampylaspis" kingdom="Animalia" order="Cumacea" pageId="13" pageNumber="66" phylum="Arthropoda" rank="genus">
