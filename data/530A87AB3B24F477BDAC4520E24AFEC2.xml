<mcCollectionCode id="0E8E53323B24F471BEC04715E24AF8EF" box="[1019,1087,1835,1861]" country="0" httpUri="http://grbio.org/cool/fc7c-p25f" name="San Jose State University, Museum of Birds and Mammals" pageId="2" pageNumber="475">MBM</mcCollectionCode>
0 92292, 
<mcCollectionCode id="0E8E53323B24F471B9924715E298F8EF" box="[1193,1261,1835,1861]" country="0" httpUri="http://grbio.org/cool/fc7c-p25f" name="San Jose State University, Museum of Birds and Mammals" pageId="2" pageNumber="475">MBM</mcCollectionCode>
0 92315, 
<mcCollectionCode id="0E8E53323B24F471B8634715E3E9F8EF" box="[1368,1436,1835,1861]" country="0" httpUri="http://grbio.org/cool/fc7c-p25f" name="San Jose State University, Museum of Birds and Mammals" pageId="2" pageNumber="475">MBM</mcCollectionCode>
0 92318 and 
<mcCollectionCode id="0E8E53323B24F471BC194771E713F8C3" box="[290,358,1871,1897]" country="0" httpUri="http://grbio.org/cool/fc7c-p25f" name="San Jose State University, Museum of Birds and Mammals" pageId="2" pageNumber="475">MBM</mcCollectionCode>
188532 (
<collectingCountry id="A3B4762D3B24F471BCEB4770E418F8C3" box="[464,621,1870,1897]" name="China" pageId="2" pageNumber="475">Jiaozhou Bay</collectingCountry>
), 
<mcCollectionCode id="0E8E53323B24F471BFBA4771E4B0F8C3" box="[641,709,1871,1897]" country="0" httpUri="http://grbio.org/cool/fc7c-p25f" name="San Jose State University, Museum of Birds and Mammals" pageId="2" pageNumber="475">MBM</mcCollectionCode>
2843975 (Yangkou, Laoshan) and 
