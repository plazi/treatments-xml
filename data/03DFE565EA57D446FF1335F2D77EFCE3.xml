<taxonomicName id="4C762FF0EA50D446FF433417D454FCE2" authority="Conlan, 2021" authorityName="Conlan" authorityYear="2021" box="[199,304,799,824]" class="Malacostraca" family="Ischyroceridae" genus="Pleojassa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="moorei">
