<collectingCountry id="E49E760D1366541CFE13FF62746DE182" box="[439,523,224,248]" name="India" pageId="23" pageNumber="24">INDIA</collectingCountry>
: 
<collectingRegion id="5E4DF87F1366541CFDB1FF5D7483E182" box="[533,741,223,248]" country="India" name="Himachal Pradesh" pageId="23" pageNumber="24">Himachal Pradesh</collectingRegion>
, Kasauli, 
<quantity id="5B719B781366541CFCF0FF5D75C2E182" box="[852,932,223,248]" metricMagnitude="3" metricUnit="m" metricValue="1.85" pageId="23" pageNumber="24" unit="m" value="1850.0">1850m</quantity>
, 61 Km NE 
<collectingRegion id="5E4DF87F1366541CFB94FF5D72D0E182" box="[1072,1206,223,248]" country="India" name="Chandigarh" pageId="23" pageNumber="24">Chandigarh</collectingRegion>
, 
<date id="E837105D1366541CFB65FF5D735DE182" box="[1217,1339,223,248]" pageId="23" pageNumber="24" value="1985-09-12">12.ix.1985</date>
, C.W. &amp; L. O’Brien, Berl. Mus. 1992-118 (
<collectionCode id="FA98AE581366541CFDB3FE867417E066" box="[535,625,260,284]" pageId="23" pageNumber="24">BMNH</collectionCode>
