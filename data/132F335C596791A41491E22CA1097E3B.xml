<taxonomicName id="08A3438C779DB971B612E0B591CC15AF" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia subcrustulata" order="Myrtales" pageId="220" pageNumber="221" phylum="Tracheophyta" rank="species" species="subcrustulata">
