<subSubSection id="C3696520FFE3FF8FFF08FDBB3A331FD0" box="[199,1128,618,643]" pageId="11" pageNumber="192" type="etymology">
<paragraph id="8BCC36ABFFE3FF8FFF08FDBB3A331FD0" blockId="11.[151,1437,438,1435]" box="[199,1128,618,643]" pageId="11" pageNumber="192">
<emphasis id="B907EAB9FFE3FF8FFF08FDBB3F151FD0" bold="true" box="[199,334,618,643]" pageId="11" pageNumber="192">Etymology.</emphasis>
