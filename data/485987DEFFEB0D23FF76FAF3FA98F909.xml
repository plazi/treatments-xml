<taxonomicName id="07F04D4BFFEB0D23FF76FA39FDB4FA34" authority="Kussakin, 1982: 95" authorityName="Kussakin" authorityPageNumber="95" authorityYear="1982" box="[151,630,1469,1492]" class="Malacostraca" family="Ischnomesidae" genus="Bactromesus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="antarcticus">
