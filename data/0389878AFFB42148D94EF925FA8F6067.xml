<taxonomicName id="4C204D1FFFB42148D9DFF81AFD3F65C3" authority="Meroth et al. 2004" authorityName="Meroth" authorityYear="2004" box="[232,656,1932,1956]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="57" pageNumber="2839" phylum="Firmicutes" rank="species" species="spicheri">
