<paragraph id="8B83A141FFE0CA40FF3D15ACFB59FA6D" blockId="41.[151,1437,1194,1723]" box="[151,1229,1446,1471]" pageId="41" pageNumber="42">
<taxonomicName id="4C3CDAC2FFE0CA40FF3D15ACFCB3FA6D" authority="Delfinado, 1962: 47" authorityName="Delfinado" authorityPageNumber="47" authorityYear="1962" box="[151,807,1446,1471]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="philippinense" subGenus="Eusimulium">
