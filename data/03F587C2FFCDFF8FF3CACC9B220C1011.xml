<number id="3D9424F3FFCBFF8DF2C4CF0427B21434" box="[409,423,1817,1841]" pageId="21" pageNumber="157" value="1.0">1</number>
with 
<number id="3D9424F3FFCBFF8DF2B1CF0427EF1434" box="[492,506,1817,1841]" pageId="21" pageNumber="157" value="3.0">3</number>
