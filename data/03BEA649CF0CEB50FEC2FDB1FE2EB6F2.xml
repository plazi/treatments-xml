<taxonomicName id="4C176CDCCF0CEB51FE1DFC12FD84B6CC" box="[471,570,919,944]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromalus" kingdom="Animalia" order="Mesostigmata" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="athiasae">
