<taxonomicName id="4C4B4D45404B292D125DFC72F0C2FC23" authority="Boulard &amp; Martinelli, 1996" authorityName="Boulard &amp; Martinelli" authorityYear="1996" box="[311,742,988,1015]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="460" phylum="Arthropoda" rank="subTribe" subTribe="Guyalnina">
