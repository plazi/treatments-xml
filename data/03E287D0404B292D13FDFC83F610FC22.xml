<taxonomicName id="4C4B4D45404B292D100DFC16F1FDFC07" authorityName="Distant" authorityYear="1905" box="[871,985,952,978]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="460" phylum="Arthropoda" rank="subTribe" subTribe="Fidicinina">Fidicinina</taxonomicName>
