<taxonomicName id="4C6FB188FFD3FFA40F3F4F53638F7016" baseAuthorityName="Hamilton" baseAuthorityYear="1822" box="[561,701,1057,1077]" class="Actinopterygii" family="Channidae" genus="Channa" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="321" phylum="Chordata" rank="species" species="gachua">
