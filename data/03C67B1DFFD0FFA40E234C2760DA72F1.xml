<taxonomicName id="4C6FB188FFD3FFA408414F2E601670AE" baseAuthorityName="Herre" baseAuthorityYear="1940" class="Actinopterygii" family="Mastacembelidae" genus="Macrognathus" kingdom="Animalia" order="Synbranchiformes" pageId="6" pageNumber="321" phylum="Chordata" rank="species" species="keithi">
<emphasis id="B91B1619FFD3FFA408414F2E601670AE" italics="true" pageId="6" pageNumber="321">Macrognathus keithi</emphasis>
