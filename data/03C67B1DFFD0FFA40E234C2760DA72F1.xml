<taxonomicName id="4C6FB188FFD3FFA40DB34F4C60BF7071" baseAuthorityName="Valenciennes" baseAuthorityYear="1840" box="[189,397,1086,1106]" class="Actinopterygii" family="Bagridae" genus="Leiocassis" kingdom="Animalia" order="Siluriformes" pageId="6" pageNumber="321" phylum="Chordata" rank="species" species="poecilopterus">
