<taxonomicName id="E1984D7E2F2BFFD42DECE4069582FCE1" authority="(Agassiz)" baseAuthorityName="Agassiz" baseAuthorityYear="1843" box="[917,1208,808,830]" class="Actinopterygii" family="Latidae" genus="Eolates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="84" phylum="Chordata" rank="species" species="gracilis">
