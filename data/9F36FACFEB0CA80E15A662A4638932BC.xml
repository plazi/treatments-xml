<taxonomicName id="6D535E6EFCB9B1C77B087B2D9BA443C6" LSID="urn:lsid:zoobank.org:act:154511E2-E70E-4D78-BFBB-EA95A730ACC0" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus violaceus" order="Coleoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="violaceus">Trigonopterus violaceus Riedel</taxonomicName>
