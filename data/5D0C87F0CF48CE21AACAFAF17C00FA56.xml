<treatmentCitationGroup id="F5B511C8CF48CE22AACAFA657B87FA0F" box="[151,1002,1411,1433]" pageId="5" pageNumber="176">
<taxonomicName id="12A54D65CF48CE22AACAFA6579BCFA0E" box="[151,465,1411,1432]" class="Insecta" family="Cerambycidae" genus="Inermestoloides" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="176" phylum="Arthropoda" rank="species" species="praeapicealba">
