<taxonomicName id="AA8E4D15FFE2FFC6FF6A5AD9FE1CF8DE" box="[198,418,1781,1806]" class="Arachnida" family="Cheyletidae" genus="Cheyletus" kingdom="Animalia" order="Prostigmata" pageId="15" pageNumber="46" phylum="Arthropoda" rank="species" species="eruditus">
