<taxonomicName id="BAD14D50FFAE92309DA6FD9271CE3145" authority="Worthy, 2009" authorityName="Worthy" authorityYear="2009" box="[995,1230,611,632]" class="Aves" family="Anatidae" genus="Australotadorna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anseriformes" pageId="17" pageNumber="428" phylum="Chordata" rank="genus" status="GEN. NOV.">AUSTRALOTADORNA</taxonomicName>
