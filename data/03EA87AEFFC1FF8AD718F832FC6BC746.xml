<paragraph id="8BFC36B8FFC1FF8AD5DFF6DAFC6BC746" blockId="7.[360,2027,250,2662]" pageId="7" pageNumber="392">
Les 
<taxonomicName id="4C434D3BFFC1FF8AD65AF6D9FD36C449" box="[553,740,2373,2409]" class="Gastropoda" family="Myrrhinidae" genus="Ennoia" kingdom="Animalia" order="Nudibranchia" pageId="7" pageNumber="392" phylum="Mollusca" rank="genus">
