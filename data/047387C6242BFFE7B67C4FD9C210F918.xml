<taxonomicName id="4BDA4D53242BFFE7B0994C7DC4D8F9D8" authority="Chanabun &amp; Bantaowong &amp; Sutcharit &amp; Tongkerd &amp; James &amp; Panha, 2012" authorityName="Chanabun &amp; Bantaowong &amp; Sutcharit &amp; Tongkerd &amp; James &amp; Panha" authorityYear="2012" class="Clitellata" family="Glossoscolecidae" genus="Glyphidrilus" kingdom="Animalia" order="Haplotaxida" pageId="5" pageNumber="270" phylum="Annelida" rank="species" species="mekongensis" status="new species">
