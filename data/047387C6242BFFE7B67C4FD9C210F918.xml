<taxonomicName id="4BDA4D53242BFFE7B6C24C7DC39CF9F8" authorityName="Chen &amp; Xu" authorityYear="1977" box="[971,1264,1586,1609]" class="Clitellata" family="Glossoscolecidae" genus="Glyphidrilus" kingdom="Animalia" order="Haplotaxida" pageId="5" pageNumber="270" phylum="Annelida" rank="species" species="yunnanensis">
