<taxonomicName id="4C27B1A82607A83BFF4BFCCE9E4E6361" authority="Burmeister, 1838" authorityName="Burmeister" authorityYear="1838" box="[189,626,801,827]" class="Insecta" family="Ectobiidae" genus="Anaplecta" kingdom="Animalia" order="Blattodea" pageId="6" pageNumber="83" phylum="Arthropoda" rank="species" species="lateralis">
