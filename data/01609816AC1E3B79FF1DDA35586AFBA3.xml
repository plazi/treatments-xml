<paragraph id="89762900AC1E3B79FF1DDBE3593CFC1C" blockId="82.[173,1407,934,1026]" box="[204,1407,934,957]" pageId="82" pageNumber="475">
Ce 
<taxonomicName id="4EC95283AC1E3B79FF3DDBE35D1BFC1C" authorityName="Hyslop. Elles" authorityYear="1921" box="[236,344,934,957]" class="Insecta" family="Elateridae" genus="Propsephus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="82" pageNumber="475" phylum="Arthropoda" rank="genus">
