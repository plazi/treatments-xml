<taxonomicName id="A329C979345738710BB4067E21A4C464" class="Gastropoda" family="Unidentiidae" genus="Unidentia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Unidentia angelvaldesi" order="Nudibranchia" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="angelvaldesi">Unidentia angelvaldesi</taxonomicName>
