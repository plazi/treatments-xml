<taxonomicName id="2ACABDE0FFACE911FC14B3A8FB81FBAA" box="[955,1045,432,457]" class="Insecta" family="Staphylinidae" genus="Lomechusa" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="38" phylum="Arthropoda" rank="species" species="elegans">
