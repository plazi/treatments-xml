<subSubSection id="8B9F6556C55CFFB7FF528BF9D8C8F876" pageId="16" pageNumber="227" type="discussion">
<paragraph id="C33A36DDC55CFFB7FF528BF9DD3EF88C" blockId="16.[151,1437,258,2031]" pageId="16" pageNumber="227">
<emphasis id="F1F1EACFC55CFFB7FF528BF9DD24F98A" bold="true" box="[199,315,1530,1555]" pageId="16" pageNumber="227">Remarks.</emphasis>
<taxonomicName id="04854D5EC55CFFB7FED78BF9DEBEF98A" authority="Hugo-Coetzee, 2017" authorityName="Hugo-Coetzee" authorityYear="2017" box="[322,673,1530,1555]" class="Arachnida" family="Oppiidae" genus="Brachioppiella" kingdom="Animalia" order="Sarcoptiformes" pageId="16" pageNumber="227" phylum="Arthropoda" rank="species" species="ricknuttalli" status="sp. nov." subGenus="Gressittoppia">
