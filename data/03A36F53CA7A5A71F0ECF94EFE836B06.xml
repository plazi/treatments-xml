<taxonomicName id="4C0AA5C6CA7E5A71F437FE04FE936B38" authority="Grasshoff &amp; Alderslade, 1997" authorityName="Grasshoff &amp; Alderslade" authorityYear="1997" class="Anthozoa" family="Gorgoniidae" genus="Guaiagorgia" kingdom="Animalia" order="Alcyonacea" pageId="31" pageNumber="32" phylum="Cnidaria" rank="species" species="anas">
