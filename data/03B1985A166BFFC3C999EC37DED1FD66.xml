<taxonomicName id="4C1852CF166AFFC0C87AEFD2D82DF879" authority="Obando &amp; Gironza &amp; Panche &amp; Aldrete, 2020" authorityName="Obando &amp; Gironza &amp; Panche &amp; Aldrete" authorityYear="2020" box="[372,529,1920,1944]" class="Insecta" family="Ptiloneuridae" genus="Ptiloneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="41" pageNumber="442" phylum="Arthropoda" rank="species" species="yari" status="n. sp.">
<emphasis id="B96CF55E166AFFC0C87AEFD2D82DF879" box="[372,529,1920,1944]" italics="true" pageId="41" pageNumber="442">
<taxonomicName id="4C1852CF166AFFC0C87AEFD2DBDCF879" box="[372,480,1920,1944]" class="Insecta" family="Ptiloneuridae" genus="Ptiloneura" higherTaxonomySource="CoL" kingdom="Animalia" order="Psocodea" pageId="41" pageNumber="442" phylum="Arthropoda" rank="genus">Ptiloneura</taxonomicName>
