<taxonomicName id="CCCC4D74FF92FFDEFCD836E9F414FB0C" authorityName="Safian" authorityYear="2015" box="[794,856,1205,1229]" class="Insecta" family="Lycaenidae" genus="Parasiomera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="221" phylum="Arthropoda" rank="species" species="alfa">
