<taxonomicName id="4C1D4D13FFCA60025C2C7F3557A1C641" box="[589,652,1289,1314]" class="Arachnida" family="Thomisidae" genus="Cupa" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="42" phylum="Arthropoda" rank="genus">
