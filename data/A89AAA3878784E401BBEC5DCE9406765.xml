<taxonomicName id="1BDC3C65721FD7ABF923A7A44CB7C82D" class="Insecta" family="Corydiidae" genus="Paralatindia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paralatindia" order="Blattodea" pageId="2" pageNumber="29" phylum="Arthropoda" rank="genus">Paralatindia</taxonomicName>
