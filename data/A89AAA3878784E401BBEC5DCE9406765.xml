<paragraph id="1F143B04ECA6CCE9385BABEBF35A63A0" pageId="2" pageNumber="29">
<taxonomicName id="BE6F8F043BF41276DC9848144DDF9DD2" class="Insecta" family="Corydiidae" genus="Latindia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Latindia" order="Blattodea" pageId="2" pageNumber="29" phylum="Arthropoda" rank="genus">Latindia</taxonomicName>
<normalizedToken id="B46C9E76782814E02BB3117E5E36AF23" originalValue="Stål">Stal</normalizedToken>
