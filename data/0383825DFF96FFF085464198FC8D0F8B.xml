<emphasis id="B95EEF59FFA9FFF0876740B3FE8B0ED1" box="[284,365,1219,1243]" italics="true" pageId="32" pageNumber="241">Thalassa</emphasis>
</taxonomicName>
, stn 
<quantity id="4CD29EAEFFA9FFF087DA40B3FDFE0ED3" box="[417,536,1218,1243]" metricMagnitude="2" metricUnit="m" metricValue="1.0748" pageId="32" pageNumber="241" unit="m" value="107.48">107, 480 m</quantity>
, drague Rallié, 
<date id="FF94158BFFA9FFF086EC40ADFEEA0EFF" box="[151,268,1245,1269]" pageId="32" pageNumber="241" value="1977-03-17">17.III.1977</date>
, 
<specimenCount id="9D2CF8C2FFA9FFF0876D40ADFE4F0EFF" box="[278,425,1245,1269]" pageId="32" pageNumber="241" type="generic">300 spécimens</specimenCount>
