<typeStatus id="549796CCFFF59122FF5538CDFECBFB57" box="[199,312,1236,1261]" pageId="11" pageNumber="222">Holotype</typeStatus>
: 
<collectingCountry id="F33B68FEFFF59122FEDB38CDFE1BFB57" box="[329,488,1236,1261]" name="Sri Lanka" pageId="11" pageNumber="222">
