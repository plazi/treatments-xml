<paragraph id="8B93286EFFF59122FF053DE6FDCCFDA0" blockId="11.[151,575,511,571]" box="[151,575,511,538]" pageId="11" pageNumber="222">
<heading id="D0DB9F02FFF59122FF053DE6FDCCFDA0" bold="true" box="[151,575,511,538]" fontSize="11" level="1" pageId="11" pageNumber="222" reason="1">
<emphasis id="B958F47CFFF59122FF053DE6FDCCFDA0" bold="true" box="[151,575,511,538]" pageId="11" pageNumber="222">
<taxonomicName id="4C2C53EDFFF59122FF053DE6FE33FDA3" box="[151,448,511,537]" class="Insecta" family="Clusiidae" genus="Sobarocephala" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="222" phylum="Arthropoda" rank="species" species="kapnikos" status="spec. nov.">
<emphasis id="B958F47CFFF59122FF053DE6FE33FDA3" bold="true" box="[151,448,511,537]" italics="true" pageId="11" pageNumber="222">Sobarocephala kapnikos</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26B4907FFF59122FE553E19FDCCFDA0" box="[455,575,512,538]" pageId="11" pageNumber="222" rank="species">spec. nov.</taxonomicNameLabel>
