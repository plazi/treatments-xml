<quantity id="DA7C18CAFFCAFFDBFE2FEFF4FDE2F8BE" box="[459,536,1772,1795]" metricMagnitude="-2" metricUnit="m" metricValue="4.5" pageId="64" pageNumber="295" unit="mm" value="45.0">45 mm</quantity>
