<geoCoordinate id="1C8122534D4631C36320F37C7696EE46" direction="north" orientation="latitude" precision="9" value="38.435165">38°26.11'N</geoCoordinate>
, 
<geoCoordinate id="35B8170AD2DE57D5B03FAB50B259CBD4" direction="west" orientation="longitude" precision="9" value="-122.61117">122°36.67'W</geoCoordinate>
