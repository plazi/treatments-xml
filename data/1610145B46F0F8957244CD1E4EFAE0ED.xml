<taxonomicName id="15B7E40F3F51F6684994A13452851AE5" authority="Dziedzicki, 1889" authorityName="Dziedzicki" authorityYear="1889" class="Insecta" family="Mycetophilidae" genus="Megophthalmidia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Megophthalmidia misericordia" order="Diptera" pageId="16" pageNumber="45" phylum="Arthropoda" rank="species" species="misericordia">Megophthalmidia misericordia</taxonomicName>
