</taxonomicName>
and 
<taxonomicName id="2EF86FE5FFDFFFBBFB02FBC8FED4FB55" authority="Veron &amp; Pichon, 1980" authorityName="Veron &amp; Pichon" authorityYear="1980" class="Anthozoa" family="Euphylliidae" genus="Euphyllia" kingdom="Animalia" order="Scleractinia" pageId="15" pageNumber="100" phylum="Cnidaria" rank="species" species="divisa">
