<specimenCode id="DBDC9EE3FF88FF80E9921728FC2C5576" box="[767,934,822,846]" collectionCode="FMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="8" pageNumber="8" type="Museum">FMNH 203324</specimenCode>
