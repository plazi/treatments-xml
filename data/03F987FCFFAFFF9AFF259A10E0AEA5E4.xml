<taxonomicName id="4C504D69FFAFFF9AFF259A58E375A426" authority="Hadzibejli, 1956: 515" authorityName="Hadzibejli" authorityPageNumber="515" authorityYear="1956" box="[151,545,1099,1122]" class="Insecta" family="Pseudococcidae" genus="Puto" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="caucasicus">
