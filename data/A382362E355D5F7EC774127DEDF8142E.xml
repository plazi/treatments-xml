<taxonomicName id="D16CCBADF0673266E10A892B2236E6EC" authority="Hulcr &amp; Cognato, 2009" authorityName="Hulcr &amp; Cognato" authorityYear="2009" class="Insecta" family="Curculionidae" genus="Diuncus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Diuncus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Diuncus</taxonomicName>
