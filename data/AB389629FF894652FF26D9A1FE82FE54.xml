<collectingRegion id="E155E9DDFF8C4652FCF3D9CBFC5DFF58" box="[861,942,158,184]" country="India" name="Assam" pageId="4" pageNumber="195">Assam</collectingRegion>
: 
<location id="264E71E4FF8C4652FC12D9CBFB3AFF59" LSID="urn:lsid:plazi:treatment:AB389629FF894652FF26D9A1FE82FE54:264E71E4FF8C4652FC12D9CBFB3AFF59" box="[956,1225,158,185]" country="India" name="Dulang Reserve Forest" pageId="4" pageNumber="195" stateProvince="Assam">Dulang Reserve Forest</location>
, 
