<taxonomicName id="D6A46FBCFFBFFFA6FEEF432E17C6FE6E" box="[318,415,368,392]" class="Insecta" family="Dynastidae" genus="Tomarus" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="484" phylum="Arthropoda" rank="species" species="fossor">
