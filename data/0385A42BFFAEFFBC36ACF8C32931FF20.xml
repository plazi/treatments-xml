<taxonomicName id="4C2C6EBEFFAEFFBA371DF8882A92FF2C" authority="Perkins, 2009: 47" authorityName="Perkins" authorityPageNumber="47" authorityYear="2009" box="[294,767,220,244]" class="Insecta" family="Hydraenidae" genus="Protozantaena" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="575" phylum="Arthropoda" rank="species" species="grebennikovi">
