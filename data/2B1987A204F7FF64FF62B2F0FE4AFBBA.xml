<taxonomicNameLabel id="8AF757DD04F6FF64FB33B1F7FAD4FC32" box="[1222,1288,912,938]" pageId="153" pageNumber="154" rank="species">sp. n.</taxonomicNameLabel>
, 
<taxonomicName id="64B04D3704F6FF64FAE3B1F7FEC9FC4A" authority="(Ogloblin)" authorityName="Ogloblin" baseAuthorityName="Ogloblin" baseAuthorityYear="1936" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="quirogai">
