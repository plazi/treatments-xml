<taxonomicName id="64B04D3704F6FF64FD42B1F7FCF6FC31" box="[695,810,912,937]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="barbos" status="sp. n.">
