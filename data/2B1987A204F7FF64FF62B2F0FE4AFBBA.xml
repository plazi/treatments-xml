<taxonomicNameLabel id="8AF757DD04F6FF64FC1AB1F7FBEEFC32" box="[1007,1074,912,938]" pageId="153" pageNumber="154" rank="species">sp. n.</taxonomicNameLabel>
, 
<taxonomicName id="64B04D3704F6FF64FBB5B1F7FB61FC31" box="[1088,1213,912,937]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="rakitovi" status="sp. n.">
