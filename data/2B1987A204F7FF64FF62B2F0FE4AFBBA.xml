<taxonomicName id="64B04D3704F6FF64FEA6B1DFFD57FC4A" authority="Triapitsyn" authorityName="Triapitsyn" box="[339,651,952,978]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="walkerjonesi">
