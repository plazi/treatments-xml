<taxonomicName id="E3E18191FF62FF6B1ED5F974FD10F8E5" authorityName="Timmermann &amp; Kuhlmann" authorityYear="2009" box="[418,665,1832,1856]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="136" pageNumber="137" phylum="Arthropoda" rank="species" species="drakensbergensis" subGenus="Chaetalictus">
(
<emphasis id="16952600FF62FF6B1EDDF975FE40F8E5" box="[426,457,1833,1856]" italics="true" pageId="136" pageNumber="137">Ch</emphasis>
.) 
<emphasis id="16952600FF62FF6B1E97F975FD10F8E5" box="[480,665,1833,1856]" italics="true" pageId="136" pageNumber="137">drakensbergensis</emphasis>
