<emphasis id="B939EABFFFC62B1AFF3CFA5C3B6A1AA0" bold="true" box="[151,573,1479,1505]" pageId="21" pageNumber="22">
<taxonomicName id="4C4D4D2EFFC62B1AFF3CFA5C38C81AA0" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[151,415,1479,1505]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="pulakita" status="new species">
<emphasis id="B939EABFFFC62B1AFF3CFA5C38C81AA0" bold="true" box="[151,415,1479,1505]" italics="true" pageId="21" pageNumber="22">Hydropsyche pulakita</emphasis>
</taxonomicName>
