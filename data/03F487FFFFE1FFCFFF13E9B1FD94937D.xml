<taxonomicName id="4C5D4D6AFFFEFFCFFC74EFE8FAE491F8" authority="(Dana, 1852)" baseAuthorityName="Dana" baseAuthorityYear="1852" box="[1008,1314,152,178]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="48" pageNumber="691" phylum="Arthropoda" rank="species" species="suluensis">
