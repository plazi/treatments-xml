<taxonomicName id="7DDCD8D42C43F32AD6ADF8D72865F883" authorityName=": Riemann-Zurneck" authorityYear="1986" box="[673,843,1813,1836]" class="Magnoliopsida" family="Asteraceae" genus="Monactis" kingdom="Plantae" order="Asterales" pageId="12" pageNumber="569" phylum="Tracheophyta" rank="species" species="vestita">Monactis vestita</taxonomicName>
