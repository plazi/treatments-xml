<taxonomicName id="4C7E3589FF7FFF59FE41FA0F33B2FE89" authorityName="Burmeister" authorityYear="1847" box="[457,718,353,377]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="180" pageNumber="179" phylum="Arthropoda" rank="species" species="putrida">
<emphasis id="B90A9218FF7FFF59FE41FA0F33B2FE89" box="[457,718,353,377]" italics="true" pageId="180" pageNumber="179">Cyclocephala putrida</emphasis>
</taxonomicName>
