</taxonomicName>
, 
<taxonomicName id="4C0C4D783205FFEF8125F9442866F95A" authority="(Bonvicino &amp; Weksler 1998)" baseAuthorityName="Bonvicino &amp; Weksler" baseAuthorityYear="1998" box="[603,1101,1667,1693]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="stramineus">
<emphasis id="B978EAE93205FFEF8125F9442ED1F95B" box="[603,762,1667,1693]" italics="true" pageId="58" pageNumber="59">O. stramineus</emphasis>
(
<bibRefCitation id="EF9D4B0A3205FFEF8074F944286EF95A" author="Bonvicino, C. R. &amp; Weksler, M." box="[778,1093,1667,1693]" pageId="58" pageNumber="59" pagination="90 - 103" refId="ref64575" refString="Bonvicino, C. R. &amp; Weksler, M. (1998) A new species of Oligoryzomys (Rodentia, Sigmodontinae) from northeastern and central Brazil. Zeitschrift fur Saugetierkunde, 63, 90 - 103." type="journal article" year="1998">Bonvicino &amp; Weksler 1998</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="4C0C4D783205FFEF8724F94429B3F959" authority="(Allen 1916)" baseAuthorityName="Allen" baseAuthorityYear="1916" box="[1114,1432,1667,1694]" class="Mammalia" family="Muridae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="utiaritensis">
<emphasis id="B978EAE93205FFEF8724F94428D7F95B" box="[1114,1276,1667,1693]" italics="true" pageId="58" pageNumber="59">O. utiaritensis</emphasis>
