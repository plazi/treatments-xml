, at 
<geoCoordinate id="D2FE5078FF21E28581C0F864D9C362E6" box="[1167,1229,1975,1999]" direction="south" orientation="latitude" pageId="204" pageNumber="205" precision="5555" value="-3.9">3.9°S</geoCoordinate>
