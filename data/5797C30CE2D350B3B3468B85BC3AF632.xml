<normalizedToken id="16C9E6638428C7D483B2C7A1AD145366" originalValue="OÖLM">OOELM</normalizedToken>
; 21.ii.1992, 1♂, leg. Warncke, 
<normalizedToken id="90573B9A7A59D6C7CA6ACBA0B8D6BC46" originalValue="OÖLM">OOELM</normalizedToken>
