<collectingRegion id="49F304CDFFD0FFF48D6BFD77FB8CFD03" box="[893,1055,665,686]" country="Thailand" name="Phuket" pageId="12" pageNumber="270">Phuket Province</collectingRegion>
: Palong (
<taxonomicName id="4C37B1ACFFD0FFF48A6CFD74FD7EFD64" authorityName="Lee, Yang, and Sato" authorityYear="1999" class="Insecta" family="Psephenidae" genus="Macroeubria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="270" phylum="Arthropoda" rank="species" species="diffusa">
<emphasis id="B943163DFFD0FFF48A6CFD74FD7EFD64" italics="true" pageId="12" pageNumber="270">M. diffusa</emphasis>
</taxonomicName>
); 
<collectingRegion id="49F304CDFFD0FFF48D1EFD5AFC05FD64" box="[776,918,692,713]" country="Thailand" name="Yala" pageId="12" pageNumber="270">Yala Province</collectingRegion>
