<taxonomicName id="4C37B1ACFFD0FFF48DEBFD5AFBC1FD64" authorityName="Lee, Yang, and Sato" authorityYear="1999" box="[1021,1106,691,713]" class="Insecta" family="Psephenidae" genus="Macroeubria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="270" phylum="Arthropoda" rank="species" species="fulva">
