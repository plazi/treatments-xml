<taxonomicName id="4C3BE2BFFFD1FFD3AA125EA51CADFE3F" authority="Haeckel, 1879: 125" authorityName="Haeckel" authorityPageNumber="125" authorityYear="1879" box="[1092,1436,412,437]" class="Hydrozoa" family="Halopterididae" genus="Tetranema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="55" pageNumber="56" phylum="Cnidaria" rank="genus">
