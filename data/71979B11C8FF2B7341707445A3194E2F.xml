<subSubSection id="F0B24AB7CD5AC9FB81C8B272FBDCB361" type="diagnosis">
<paragraph id="5DB2BEBE5E470A9A6B837F98A1945BB9" pageNumber="307">
diagnosis A member of the 
<taxonomicName id="2A51F6C44AF307DC57116CB7399108D7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33735" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole fallax Mayr" lsidName-HNS="Pheidole fallax Mayr" order="Hymenoptera" pageNumber="307" phylum="Arthropoda" rank="species" species="fallax">fallax</taxonomicName>
group, similar to 
<taxonomicName id="17C8C81153F7720E7BA6BE7A57B1D17B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33735" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole fallax Mayr" lsidName-HNS="Pheidole fallax Mayr" order="Hymenoptera" pageNumber="307" phylum="Arthropoda" rank="species" species="fallax">fallax</taxonomicName>
, 
<taxonomicName id="BFD4F00326303225311240729616A78F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181813" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole obscurithorax Forel" lsidName-HNS="Pheidole obscurithorax Forel" order="Hymenoptera" pageNumber="307" phylum="Arthropoda" rank="species" species="obscurithorax">obscurithorax</taxonomicName>
, 
<taxonomicName id="B2740C1FC461F7D0E9C728351D27306E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181822" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole puttemansi Forel" lsidName-HNS="Pheidole puttemansi Forel" order="Hymenoptera" pageNumber="307" phylum="Arthropoda" rank="species" species="puttemansi">puttemansi</taxonomicName>
, 
<taxonomicName id="D27CF889D7D4B02C002847636F1A764E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181823" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole roushae Wilson" lsidName-HNS="Pheidole roushae Wilson" order="Hymenoptera" pageNumber="307" phylum="Arthropoda" rank="species" species="roushae">roushae</taxonomicName>
