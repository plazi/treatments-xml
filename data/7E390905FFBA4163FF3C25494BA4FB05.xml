<taxonomicName id="3190C390FFBA4163FD2523524E0AFBD9" authority="Meszaros &amp; Abraham, 2005" authorityName="Meszaros &amp; Abraham" authorityYear="2005" box="[654,1094,1122,1150]" class="Insecta" family="Ascalaphidae" genus="Stylascalaphus" kingdom="Animalia" order="Neuroptera" pageId="32" pageNumber="433" phylum="Arthropoda" rank="species" species="fabiani">
