<number id="3DBA24C600269F2FFE08FD8F700FFDFB" box="[511,626,609,633]" pageId="12" pageNumber="113" value="0.63" valueMax="0.7" valueMin="0.56">0.56–0.70</number>
, HL 
<number id="3DBA24C600269F2FFD45FD8F7158FDFB" box="[690,805,609,633]" pageId="12" pageNumber="113" value="0.625" valueMax="0.67" valueMin="0.58">0.58–0.67</number>
