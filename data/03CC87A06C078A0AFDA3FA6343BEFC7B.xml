<taxonomicName id="4C654D356C008A0BFB81F84B4266F80D" authority="perarcta" authorityName="perarcta (Blanford" authorityYear="1865" box="[1145,1391,1931,1957]" class="Gastropoda" family="Plectopylidae" genus="Chersaecia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="subSpecies" species="perarcta" subSpecies="perarcta">
