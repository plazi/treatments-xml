<taxonomicName id="7B48F80E17D348789B4B4195EF6491AB" class="Insecta" family="Hydraenidae" genus="Ochthebius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ochthebius lobatus" order="Coleoptera" pageId="9" pageNumber="66" phylum="Arthropoda" rank="species" species="lobatus">Ochthebius lobatus</taxonomicName>
