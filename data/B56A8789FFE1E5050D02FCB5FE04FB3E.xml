<taxonomicName id="FAC34D1CFFE1E5050DBCFCB5FE5DFC6E" authority="MONTFORT, 1810" authorityName="MONTFORT" authorityYear="1810" box="[199,489,877,902]" class="Gastropoda" family="Ampullariidae" genus="Lanistes" kingdom="Animalia" order="Architaenioglossa" pageId="5" pageNumber="6" phylum="Mollusca" rank="genus" species="undetermined">
