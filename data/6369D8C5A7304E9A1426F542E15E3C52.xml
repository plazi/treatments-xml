<taxonomicName id="EF8AC530C410B909E0C7CC075044065D" authorityName="Michaelsen" authorityYear="1922" class="Clitellata" family="Almidae" genus="Glyphidrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Haplotaxida" pageId="16" pageNumber="17" phylum="Annelida" rank="species" species="buttikoferi">Glyphidrilus buttikoferi</taxonomicName>
