<number id="3DA124D6456EFFA2FC7FFDEAFB904A77" box="[980,1037,590,614]" pageId="25" pageNumber="526" value="1947.0">1947</number>
</bibRefCitation>
) and 
<figureCitation id="13522A74456EFFA2FBFBFDE9FB4E4A77" box="[1104,1235,589,614]" captionStart="FIGURES 71 – 75" captionStartId="26.[151,264,1910,1933]" captionTargetBox="[151,1436,193,1888]" captionTargetId="figure@26.[151,1436,193,1890]" captionTargetPageId="26" captionText="FIGURES 71 – 75. Metapone krombeini, holotype gyne; FIGURES 76 – 80. Worker, see text for dimensions." httpUri="https://zenodo.org/record/271335/files/figure.png" pageId="25" pageNumber="526">
