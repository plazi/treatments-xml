<caption id="DF7309A6FF9AFFCB86C0F8D45079F848" ID-DOI="http://doi.org/10.5281/zenodo.5737567" ID-Zenodo-Dep="5737567" httpUri="https://zenodo.org/record/5737567/files/figure.png" pageId="7" pageNumber="174" startId="7.[156,187,1894,1916]" targetBox="[155,1432,206,1853]" targetPageId="7">
