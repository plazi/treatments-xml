<paragraph id="853193419EC1E3BBFF4DF982DFCE65E0" blockId="164.[151,1436,1659,1712]" pageId="164" pageNumber="165">
<emphasis id="B7FA4F539EC1E3BBFF4DF982DA1365C2" bold="true" box="[151,303,1659,1682]" pageId="164" pageNumber="165">FIGURE 133.</emphasis>
<taxonomicName id="428EE8C29EC1E3BBFEEDF985DA9765C2" authorityName="Quicke &amp; Butcher" authorityYear="2012" box="[311,427,1660,1682]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="164" pageNumber="165" phylum="Arthropoda" rank="species" species="placidus" status="sp. nov.">
<emphasis id="B7FA4F539EC1E3BBFEEDF985DA9765C2" box="[311,427,1660,1682]" italics="true" pageId="164" pageNumber="165">A. placidus</emphasis>
</taxonomicName>
<emphasis id="B7FA4F539EC1E3BBFE6EF985D93A65C1" bold="true" box="[436,518,1660,1681]" pageId="164" pageNumber="165">
<taxonomicNameLabel id="ACC9F2289EC1E3BBFE6EF985D93A65C1" box="[436,518,1660,1681]" pageId="164" pageNumber="165" rank="species">sp. nov.</taxonomicNameLabel>
