<taxonomicName id="36DA2DC9E233FFCB72F6FA5D0772FA68" authority=": Lin &amp; Li, 2014: 37" authorityName="Lin &amp; Li" authorityPageNumber="37" authorityYear="2014" box="[151,516,1457,1479]" class="Arachnida" family="Tetrablemmidae" genus="Sinamma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="446" phylum="Arthropoda" rank="species" species="sanya">
