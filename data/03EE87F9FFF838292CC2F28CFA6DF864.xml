<emphasis id="B933EAFDFFF838292C92F087FE9EF865" bold="true" box="[199,354,1940,1965]" pageId="10" pageNumber="127">Distribution:</emphasis>
<collectingCountry id="F350767FFFF838292D3CF087FE5FF864" box="[361,419,1940,1964]" name="India" pageId="10" pageNumber="127">India</collectingCountry>
(Gulmarg, Mussoorie, Sikkim, Simla); 
<collectingCountry id="F350767FFFF838292F34F087FC58F864" box="[865,932,1940,1964]" name="Nepal" pageId="10" pageNumber="127">Nepal</collectingCountry>
(Katmandu Valley); 
<collectingCountry id="F350767FFFF8382928DAF087FB2EF864" box="[1167,1234,1940,1964]" name="China" pageId="10" pageNumber="127">China</collectingCountry>
