<heading id="D0BA81CCFFB3FFD0D5CB91E5FE50FD77" bold="true" box="[151,473,711,737]" fontSize="11" level="1" pageId="22" pageNumber="23" reason="1">
<emphasis id="B939EAB2FFB3FFD0D5CB91E5FE50FD77" bold="true" box="[151,473,711,737]" pageId="22" pageNumber="23">
<taxonomicName id="4C4D4D23FFB3FFD0D5CB91E5FE0EFD77" box="[151,391,711,737]" class="Reptilia" family="Gekkonidae" genus="Phelsuma" kingdom="Animalia" order="Squamata" pageId="22" pageNumber="23" phylum="Chordata" rank="species" species="laticauda">
<emphasis id="B939EAB2FFB3FFD0D5CB91E5FE0EFD77" bold="true" box="[151,391,711,737]" italics="true" pageId="22" pageNumber="23">Phelsuma laticauda</emphasis>
</taxonomicName>
