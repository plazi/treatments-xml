<emphasis id="B918EAF59149FFF8FF21BAC49773F907" bold="true" box="[199,345,1755,1780]" pageId="4" pageNumber="281">Description.</emphasis>
FEMALE (
<typeStatus id="54D788459149FFF8FE03BAC49463F907" box="[485,585,1755,1780]" pageId="4" pageNumber="281">holotype</typeStatus>
and 
<typeStatus id="54D788459149FFF8FD60BAC394C6F907" box="[646,748,1756,1780]" pageId="4" pageNumber="281">paratype</typeStatus>
). Body length 
<date id="FFD210279149FFF8FC7FBAC3923CF907" box="[921,1046,1756,1780]" pageId="4" pageNumber="281" value="1107" valueMax="1218">
<number id="3DA424C09149FFF8FC7FBAC3923CF907" box="[921,1046,1756,1780]" pageId="4" pageNumber="281" value="1162.5" valueMax="1218.0" valueMin="1107.0">1107–1218</number>
</date>
