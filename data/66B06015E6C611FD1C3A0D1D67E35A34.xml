<subSubSection id="56AEA018CEF01913C3E914A45F1836D0" type="nomenclature">
<paragraph id="8AD6792780FC88A8453532FF7B586D60" pageNumber="50">
<taxonomicName id="B1201BEB35DB2A00DCAA06D94CA30628" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:229311" class="Insecta" family="Formicidae" genus="Mayriella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Mayriella occidua Shattuck" order="Hymenoptera" pageNumber="50" phylum="Arthropoda" rank="species" species="occidua">Mayriella occidua</taxonomicName>
