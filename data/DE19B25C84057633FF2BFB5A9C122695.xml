<taxonomicName id="91B078C984057631FF2BFAF59EA72141" authority="Cresson, 1865" authorityName="Cresson" authorityYear="1865" box="[151,472,1373,1395]" class="Insecta" family="Braconidae" genus="Agathis" kingdom="Animalia" order="Hymenoptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="vulgaris">
