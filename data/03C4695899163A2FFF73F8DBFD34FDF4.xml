. 
<taxonomicName id="4C6DA3CD99143A29FEC7F8B9FE3FF881" authorityName="Heatubun" authorityYear="2011" box="[294,437,1892,1915]" class="Liliopsida" family="Arecaceae" genus="Areca" kingdom="Plantae" order="Arecales" pageId="2" pageNumber="8" phylum="Tracheophyta" rank="species" species="bakeri">
