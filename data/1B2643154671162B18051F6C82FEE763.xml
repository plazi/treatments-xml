<taxonomicName id="D030EAEE7460B9580B268F437E59CE59" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:251853" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Crematogaster (Physocrema) sewardi" order="Hymenoptera" pageNumber="28" phylum="Arthropoda" rank="species" species="sewardi" subGenus="Physocrema">Crematogaster (Physocrema) sewardi Forel</taxonomicName>
