<collectingCountry id="95BA7641FF92FF80FF47FA7BFF11FA76" box="[160,224,1449,1474]" name="China" pageId="11" pageNumber="1142">China</collectingCountry>
(
<collectingRegion id="2F69F833FF92FF80FF09FA7BFEBBFA76" box="[238,330,1449,1474]" country="China" name="Yunnan" pageId="11" pageNumber="1142">Yunnan</collectingRegion>
