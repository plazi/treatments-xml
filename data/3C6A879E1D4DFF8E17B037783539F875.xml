<subSubSection id="FCD965031D42FF8E17E037FA3132F921" box="[199,1356,1720,1745]" pageId="24" pageNumber="25" type="etymology">
<paragraph id="B47C36881D42FF8E17E037FA3132F921" blockId="24.[151,1437,1540,1924]" box="[199,1356,1720,1745]" pageId="24" pageNumber="25">
<emphasis id="86B7EA9A1D42FF8E17E037FA3530F920" bold="true" box="[199,334,1721,1745]" inLineHeading="true" pageId="24" pageNumber="25" reason="1">Etymology.</emphasis>
