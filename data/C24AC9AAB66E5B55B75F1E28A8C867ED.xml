<taxonomicName id="63E7E723BB5F632F1BE473A984D79EA6" authorityName="Foerster" authorityYear="1856" class="Insecta" family="Scelionidae" genus="Hadronotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hadronotus" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="genus">
