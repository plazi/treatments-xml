x 
<quantity id="4CCE9B37FA00FFCDCF03BD24FB7DFBFC" box="[1083,1173,1087,1111]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="101" pageNumber="102" unit="mm" value="3.6">3.6 mm</quantity>
