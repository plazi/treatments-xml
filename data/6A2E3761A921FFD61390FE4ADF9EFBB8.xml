<specimenCount id="F4814DFEA921FFD612A0FCC7DDB6FC9B" box="[423,469,820,845]" pageId="19" pageNumber="420" type="generic">
<quantity id="257F2B92A921FFD612A0FCC7DDB2FC9B" box="[423,465,820,845]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="19" pageNumber="420" unit="m" value="5.0">5 m</quantity>
