<geoCoordinate id="E9B80368615D1B911A805A908119FE66" direction="north" orientation="latitude" precision="925" value="36.566666">36°34'N</geoCoordinate>
, 
<geoCoordinate id="23D33DE4AF50DF00CE08EBEBCAAD6C42" direction="east" orientation="longitude" precision="925" value="51.55">051°33'E</geoCoordinate>
