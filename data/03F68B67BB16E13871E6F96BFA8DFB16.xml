Entebbe, 
<date id="FFE11CB1BB10E13873DBFC3AFC84FC77" box="[682,854,908,933]" pageId="4" pageNumber="449" value="1972-12-20" valueMax="1972-12-31" valueMin="1972-12-20">
<number id="3D972856BB10E13873DBFC3AFD21FC77" box="[682,755,908,933]" pageId="4" pageNumber="449" value="25.5" valueMax="31.0" valueMin="20.0">20–31</number>
.xii. 
<number id="3D972856BB10E138726BFC3AFC84FC77" box="[794,854,908,933]" pageId="4" pageNumber="449" value="1972.0">1972</number>
