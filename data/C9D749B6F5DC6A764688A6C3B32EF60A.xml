<taxonomicName id="B945BF8118BFB699D82E1A3198B5BAE7" class="Insecta" family="Staphylinidae" genus="Holisus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Holisus" order="Coleoptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="genus">Holisus</taxonomicName>
