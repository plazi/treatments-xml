<emphasis id="EBEA23763F3DFF81143C1399F52FD8C5" box="[358,573,1976,1999]" italics="true" pageId="13" pageNumber="64">
“ 
<taxonomicName id="1E9E84E73F3DFF8114341399F6CDD8C5" box="[366,479,1976,1999]" class="Diplopoda" genus="Tidesmus" kingdom="Animalia" order="Polydesmida" pageId="13" pageNumber="64" phylum="Arthropoda" rank="genus">Tidesmus</taxonomicName>
” 
<taxonomicName id="1E9E84E73F3DFF8114B51399F52BD8C5" box="[495,569,1976,1999]" class="Diplopoda" genus="Tidesmus" kingdom="Animalia" order="Polydesmida" pageId="13" pageNumber="64" phylum="Arthropoda" rank="species" species="hubbsi">hubbsi</taxonomicName>
