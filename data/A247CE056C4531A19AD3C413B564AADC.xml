<taxonomicName id="C57292E2088638440CCFD8FC6CDBB6C0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142283" class="Insecta" family="Formicidae" genus="Pheidole" genus.bestMatchDistance="680" genus.bestMatchVote="2" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole javana var. pectinata Stitz" order="Hymenoptera" pageNumber="24" phylum="Arthropoda" rank="variety" species="javana" variety="pectinata">P. javana var. pectinata Stitz</taxonomicName>
