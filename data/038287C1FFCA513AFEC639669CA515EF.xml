<paragraph id="8B9436D7FFCA513BFEC639FD9EAB108B" blockId="8.[264,946,1143,1202]" box="[264,946,1178,1202]" pageId="8" pageNumber="9">
<treatmentCitationGroup id="AB3B11F9FFCA513BFEC639FD9EAB108B" box="[264,946,1178,1202]" pageId="8" pageNumber="9">
<taxonomicName id="4C2B4D54FFCA513BFEC639FD9FE21088" authority="Borgmeier, 1967: 207" authorityName="Borgmeier" authorityPageNumber="207" authorityYear="1967" box="[264,763,1178,1202]" class="Insecta" family="Phoridae" genus="Chonocephalus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="digitalis">
<emphasis id="B95FEAC5FFCA513BFEC639FD9F15108B" box="[264,524,1179,1202]" italics="true" pageId="8" pageNumber="9">Chonocephalus digitalis</emphasis>
<treatmentCitation id="0A8A10C6FFCA513BFDDD39FC9FE21088" author="Borgmeier" box="[531,763,1178,1201]" page="207" pageId="8" pageNumber="9" year="1967">
<bibRefCitation id="EFBA4B26FFCA513BFDDD39FC9FD01088" author="Borgmeier" box="[531,713,1178,1201]" pageId="8" pageNumber="34" refString="Borgmeier, T. (1967) New or little known phorid flies, with description of two new genera, Anais da Academia Brasileira de Ciencias, Rio de Janeiro, 39,195 - 210." type="book" year="1967">
Borgmeier, 
<number id="3DE324F0FFCA513BFD5E39FC9FD01088" box="[656,713,1178,1201]" pageId="8" pageNumber="9" value="1967.0">1967</number>
</bibRefCitation>
