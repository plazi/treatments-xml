<specimenCode id="FD63B105FF91FFD8FB14BF3EFAB1FCF6" box="[1260,1302,844,868]" collectionCode="II" pageId="11" pageNumber="12">II 2</specimenCode>
, 
<specimenCode id="FD63B105FF91FFD8FADBBF3EFA96FCF6" box="[1315,1329,844,868]" collectionCode="II" pageId="11" pageNumber="12">2</specimenCode>
</materialsCitation>
/ 
<materialsCitation id="1DAD1323FF91FFD8FAC2BF3EFE84FC15" collectionCode="III" pageId="11" pageNumber="12" specimenCode="III 1, 2" specimenCount="1">
0, 2/0, 1, genu 
<specimenCode id="FD63B105FF91FFD8FF2CBF02FEA2FC15" box="[212,261,879,903]" collectionCode="III" pageId="11" pageNumber="12">III 1</specimenCode>
, 
<specimenCode id="FD63B105FF91FFD8FEEBBF1DFE84FC15" box="[275,291,879,903]" collectionCode="III" pageId="11" pageNumber="12">2</specimenCode>
</materialsCitation>
