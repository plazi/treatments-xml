<taxonomicName id="4C6A4D42FFCE2524FE67EB25FDF9FAD9" box="[458,547,1323,1345]" class="Insecta" family="Limoniidae" genus="Rhipidia" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="228" phylum="Arthropoda" rank="genus">Rhipidia</taxonomicName>
