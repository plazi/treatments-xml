<paragraph id="8B9E0E7A395CFF9AFF78FC64FCD42FE4" blockId="20.[136,1059,920,977]" box="[136,819,952,977]" pageId="20" pageNumber="685">
<collectingRegion id="49E5C098395CFF9AFF78FC64FF0C2FE4" box="[136,235,952,976]" country="Hungary" name="Budapest" pageId="20" pageNumber="685">Budapest</collectingRegion>
, 
<collectingCountry id="F3364EEA395CFF9AFF09FC65FEB22FE5" box="[249,341,953,977]" name="Hungary" pageId="20" pageNumber="685">Hungary</collectingCountry>
