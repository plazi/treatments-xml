<td id="7701F447B7A2458D1ECEB23CD4EE56E5" align="left" pageId="null" pageNumber="817" valign="top">
2. 
<normalizedToken id="C8B38585436B0840AA69FFB9686BD215" originalValue="Blätter" pageId="null" pageNumber="817">Blaetter</normalizedToken>
