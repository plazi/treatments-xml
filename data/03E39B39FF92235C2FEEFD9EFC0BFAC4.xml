<heading id="D0BD9D43FF92235C2EF4FD74FED9FD2A" box="[211,348,685,708]" fontSize="10" level="3" pageId="7" pageNumber="191" reason="5">
<emphasis id="B93EF63DFF92235C2EF4FD74FED9FD2A" box="[211,348,685,708]" italics="true" pageId="7" pageNumber="191">
<typeStatus id="54F1948DFF92235C2EF4FD74FE8CFD2A" box="[211,265,685,708]" pageId="7" pageNumber="191">Type</typeStatus>
