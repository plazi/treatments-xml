<taxonomicName id="4C374D29B266FE87FE27FAA32B94FAE1" authority="and" authorityName="AND" box="[486,662,1283,1307]" class="Insecta" family="Scolytidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="288" pageNumber="1" phylum="Arthropoda" rank="family">Scolytidae and</taxonomicName>
the 
<taxonomicName id="4C374D29B266FE87FD0AFAA32A69FAE1" box="[715,875,1283,1307]" class="Insecta" family="Platypodidae" kingdom="Animalia" order="Coleoptera" pageId="288" pageNumber="287" phylum="Arthropoda" rank="family">Platypodidae</taxonomicName>
