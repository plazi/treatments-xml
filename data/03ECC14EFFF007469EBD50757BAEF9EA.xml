<emphasis id="B931AC4AFFF107469AE354237C6FFAE6" bold="true" box="[1225,1282,1306,1327]" pageId="9" pageNumber="525">n. sp.</emphasis>
</taxonomicNameLabel>
, 2— 
<taxonomicName id="4C450BDBFFF107469B1E5422785DFA86" authority="Attems, 1952" authorityName="Attems" authorityYear="1952" class="Diplopoda" family="Paradoxosomatidae" genus="Boreviulisoma" kingdom="Animalia" order="Polydesmida" pageId="9" pageNumber="525" phylum="Arthropoda" rank="species" species="badium">
<emphasis id="B931AC4AFFF107469B1E54227CF1FAF9" box="[1332,1436,1306,1328]" italics="true" pageId="9" pageNumber="525">B. badium</emphasis>
(Attems, 1952)
</taxonomicName>
and 3— 
<taxonomicName id="4C450BDBFFF107469FAD54037BDAFA87" authority="Brolemann, 1928" authorityName="Brolemann" authorityYear="1928" box="[391,695,1337,1359]" class="Diplopoda" family="Paradoxosomatidae" genus="Boreviulisoma" kingdom="Animalia" order="Polydesmida" pageId="9" pageNumber="525" phylum="Arthropoda" rank="species" species="liouvillei">
