<quantity id="8D6B9B35FFF7FF9CFBD3FB2B1885FB2D" box="[1109,1155,1211,1235]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="68" pageNumber="69" unit="m" value="1.0">1 m</quantity>
