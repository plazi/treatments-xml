<subSubSection id="C36F651EFFD48959F9A62330FBDEF942" pageId="1" pageNumber="190" type="etymology">
<paragraph id="8BCA3695FFD48959F9A62330FBDEF942" blockId="1.[151,1437,545,1686]" pageId="1" pageNumber="190">
<emphasis id="B901EA87FFD48959F9A62330FEACF9A6" bold="true" box="[199,337,1625,1650]" pageId="1" pageNumber="190">Etymology:</emphasis>
