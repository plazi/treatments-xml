<quantity id="4CF79B33FFD7FFFEC347FA2EFEE5F9E7" box="[318,371,1527,1547]" metricMagnitude="2" metricUnit="m" metricValue="4.7" pageId="56" pageNumber="211" unit="m" value="470.0">470m</quantity>
