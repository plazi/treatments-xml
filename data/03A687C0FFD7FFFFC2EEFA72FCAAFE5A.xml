<quantity id="4CF79B33FFD7FFFEC190F9CBFB88F9CA" box="[1001,1054,1554,1574]" metricMagnitude="2" metricUnit="m" metricValue="4.9" pageId="56" pageNumber="211" unit="m" value="490.0">490m</quantity>
