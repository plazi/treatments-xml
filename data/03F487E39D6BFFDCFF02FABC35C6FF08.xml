, 
<collectingCountry id="F34A76659D6BFFDCFDFEF8BA339DF8BD" box="[596,678,1876,1900]" name="Bolivia" pageId="23" pageNumber="674">Bolivia</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD17F8BA3239F8BD" box="[701,770,1876,1900]" name="Brazil" pageId="23" pageNumber="674">Brazil</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFF02F898302FF85F" box="[168,276,1910,1934]" name="Colombia" pageId="23" pageNumber="674">Colombia</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE8FF898309FF85F" box="[293,420,1910,1934]" name="Costa Rica" pageId="23" pageNumber="674">Costa Rica</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE1FF898332AF85F" box="[437,529,1910,1934]" name="Ecuador" pageId="23" pageNumber="674">Ecuador</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD88F89833A0F85F" box="[546,667,1910,1934]" name="Guatemala" pageId="23" pageNumber="674">Guatemala</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD06F8983238F85F" box="[684,771,1910,1934]" name="Guyana" pageId="23" pageNumber="674">Guyana</collectingCountry>
, French Guiana, 
<collectingCountry id="F34A76659D6BFFDCFEFCF87930F9F87E" box="[342,450,1943,1967]" name="Honduras" pageId="23" pageNumber="674">Honduras</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE67F879331AF87E" box="[461,545,1943,1967]" name="Mexico" pageId="23" pageNumber="674">Mexico</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD86F87933A4F87E" box="[556,671,1943,1967]" name="Nicaragua" pageId="23" pageNumber="674">Nicaragua</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD00F8793238F87F" box="[682,771,1943,1966]" name="Panama" pageId="23" pageNumber="674">Panama</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFF02F85731E6F801" box="[168,221,1977,2000]" name="Peru" pageId="23" pageNumber="674">Peru</collectingCountry>
