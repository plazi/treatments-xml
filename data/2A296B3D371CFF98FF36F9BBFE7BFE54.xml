, 
<taxonomicName id="6580A1A8371AFF99FB5BF9CAFEC0F93B" authority="Beier, 1953" authorityName="Beier" authorityYear="1953" class="Insecta" family="Mantidae" genus="Bolbella" kingdom="Animalia" order="Mantodea" pageId="4" pageNumber="57" phylum="Arthropoda" rank="species" species="brevis">
