, 
<taxonomicName id="6580A1A8371AFF99FEE3F929FD64F93B" authority="Kaltenbach, 1996" authorityName="Kaltenbach" authorityYear="1996" box="[322,691,1670,1694]" class="Insecta" family="Mantidae" genus="Bolbella" kingdom="Animalia" order="Mantodea" pageId="4" pageNumber="57" phylum="Arthropoda" rank="species" species="affinis">
