<taxonomicName id="F6A14D5CFFBFA613F131F8E70469A2F4" authority="Gutierrez, Toledo-Hernandez &amp; Noguera, 2020" authorityName="Gutierrez, Toledo-Hernandez &amp; Noguera" authorityYear="2020" class="Insecta" family="Cerambycidae" genus="Phrynidius" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="51" phylum="Arthropoda" rank="species" species="jonesi">
