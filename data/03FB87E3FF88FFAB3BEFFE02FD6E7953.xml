REMARKS: 
<taxonomicName id="4C524D76FF88FFAB3A64FCDDFE997F02" authority="Meigen, 1800" authorityName="Meigen" authorityYear="1800" box="[284,370,871,894]" class="Insecta" family="Sciaridae" genus="Lycoria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus">
