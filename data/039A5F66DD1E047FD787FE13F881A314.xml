<taxonomicName id="4C3395F3DD1E047FD3DFFBA2FF77A256" box="[1231,1343,1060,1085]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="lepidus">
