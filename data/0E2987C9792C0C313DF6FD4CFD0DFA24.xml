<typeStatus id="593B887D792C0C313CB1FBE4FD9FFB98" box="[464,568,1110,1136]" pageId="28" pageNumber="29">holotype</typeStatus>
+ 
<number id="304824F8792C0C313F3DFBE4FDCCFB98" box="[604,619,1110,1136]" pageId="28" pageNumber="29" value="7.0">7</number>
ɗɗ; 
<typeStatus id="593B887D792C0C313FCFFBE4FCB2FB98" box="[686,789,1110,1136]" pageId="28" pageNumber="29">holotype</typeStatus>
listed first): total length 5.0, 
<number id="304824F8792C0C313918FBE4FB73FB98" box="[1145,1236,1110,1136]" pageId="28" pageNumber="29" value="4.449999999999999" valueMax="5.1" valueMin="3.8">3.8–5.1</number>
