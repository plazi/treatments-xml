<collectionCode id="ED05AE28E62A7170FCA3FD8EBFAFCC27" LSID="urn:lsid:biocol.org:col:34871" box="[828,912,610,634]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="7" pageNumber="200">USNM</collectionCode>
