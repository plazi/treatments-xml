<taxonomicName id="B8B64D37FFB3FF9A6D9D9678FE405326" authority="nemesis" box="[151,432,513,538]" class="Insecta" family="Pieridae" genus="Lieinix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="20" pageNumber="221" phylum="Arthropoda" rank="subSpecies" species="nemesis" subSpecies="nemesis">
