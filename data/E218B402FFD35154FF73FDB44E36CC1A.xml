<taxonomicName id="ADB17E97FFD35154FCA3FB9E4836CD29" box="[839,1072,1040,1065]" class="Magnoliopsida" family="Rhizophoraceae" genus="Rhizophora" kingdom="Plantae" order="Malpighiales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="mangle">
