<quantity id="4CC53BB0EE43A17FFACC94D1FAC8F954" box="[1283,1355,1737,1757]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" pageId="6" pageNumber="313" unit="mm" value="0.25">0.25 mm</quantity>
