<taxonomicName id="4C6A4D67FF99FFACFF5B4E6E5720FEC9" box="[151,344,332,357]" class="Magnoliopsida" family="Caryophyllaceae" kingdom="Plantae" order="Caryophyllales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="family">Caryophyllaceae</taxonomicName>
