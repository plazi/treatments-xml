<quantity id="3859ED89B857E5696116FD1361DAFDFA" box="[624,694,651,677]" metricMagnitude="4" metricUnit="m" metricValue="1.6" pageId="117" pageNumber="118" unit="km" value="16.0">16 km</quantity>
S, 
<date id="8B1F66ACB857E56961BEFD13601EFDF9" box="[728,882,651,678]" pageId="117" pageNumber="118" value="1965-04-16">16 April 1965</date>
(
<collectionCode id="99B0D8A9B857E56960E6FD1460D8FDF9" box="[896,948,652,678]" pageId="117" pageNumber="118">RJL</collectionCode>
). PLATTE: 
<collectingCountry id="87B600FCB857E569675FFD1467C9FDF9" box="[1081,1189,652,678]" name="Guernsey" pageId="117" pageNumber="118">Guernsey</collectingCountry>
, 
<date id="8B1F66ACB857E56967D6FD14661AFDFA" box="[1200,1398,651,678]" pageId="117" pageNumber="118" value="1964-04-13" valueMax="1964-04-27" valueMin="1964-04-13">13–27 April 1964</date>
, 
<date id="8B1F66ACB857E56966E6FD14627FFD96" pageId="117" pageNumber="118" value="1964-04-30">30 April 1964</date>
, 
<date id="8B1F66ACB857E569627BFD3762CFFD96" box="[285,419,687,714]" pageId="117" pageNumber="118" value="1966-05-05">5 May 1966</date>
(
<collectionCode id="99B0D8A9B857E56962D7FD286289FD95" box="[433,485,688,714]" pageId="117" pageNumber="118">RJL</collectionCode>
); Wheatland, 
<date id="8B1F66ACB857E5696118FD286077FD96" box="[638,795,687,714]" pageId="117" pageNumber="118" value="1964-04-30">30 April 1964</date>
