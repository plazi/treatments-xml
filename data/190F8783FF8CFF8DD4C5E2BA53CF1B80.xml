<treatmentCitationGroup id="B1B611BBFF8CFF8DD4C5E30F55661968" box="[151,548,1368,1390]" pageId="80" pageNumber="81">
<taxonomicName id="56A64D16FF8CFF8DD4C5E30F55621968" authority="Hood, 1914: 50" authorityName="Hood" authorityPageNumber="50" authorityYear="1914" box="[151,544,1368,1390]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="signatus">
<emphasis id="A3D2EA87FF8CFF8DD4C5E30F563B1968" box="[151,377,1368,1390]" italics="true" pageId="80" pageNumber="81">Holopothrips signatus</emphasis>
<treatmentCitation id="10071084FF8CFF8DD52CE30F55621968" author="" box="[382,544,1368,1390]" page="50" pageId="80" pageNumber="81">
<bibRefCitation id="F5374B64FF8CFF8DD52CE30F56BB1968" box="[382,505,1368,1390]" pageId="80" pageNumber="81" refId="ref58692" refString="Hood, J. D. (1914) Two new Thysanoptera from Panama. Insecutor inscitiae menstruus, 2, 49 - 53. https: // doi. org / 10.5962 / bhl. part. 9581" type="journal article">Hood, 1914</bibRefCitation>
