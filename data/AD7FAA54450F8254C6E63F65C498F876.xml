<collectingCountry id="5DC15BD2450F8254C53F3D75C3CEF8E6" box="[846,988,1864,1888]" name="South Korea" pageId="23" pageNumber="24">South Korea</collectingCountry>
(
<bibRefCitation id="414766B3450F8254C59B3D75C4C3F8E6" author="Kim" box="[1002,1233,1864,1888]" pageId="23" pageNumber="24" refString="Kim, I. H. &amp; Chung, K. S. (1993) Water mites of the genus Aturus (Acarina: Aturidae) in Korea. Korean Journal of Zoology, 36, 329 - 341." type="journal article" year="1993">
Kim &amp; Chung 
<number id="931E0965450F8254C2E43D75C4C3F8E6" box="[1173,1233,1864,1888]" pageId="23" pageNumber="24" value="1993.0">1993</number>
</bibRefCitation>
