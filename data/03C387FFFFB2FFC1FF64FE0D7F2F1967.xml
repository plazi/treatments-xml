<elevation id="0047D1DAFFB2FFC0FBF8FD247EFC1A8A" box="[1035,1168,660,685]" metricMagnitude="2" metricUnit="m" metricValue="2.25" pageId="4" pageNumber="569" unit="m" value="225.0">
