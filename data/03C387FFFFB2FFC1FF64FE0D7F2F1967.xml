</elevation>
, 
<collectingDate id="EF90E9C1FFB2FFC0FB6DFD247F4E1A8A" box="[1182,1314,660,685]" pageId="4" pageNumber="569" value="2000-12-29">
<date id="FFD41029FFB2FFC0FB6DFD247F4E1A8A" box="[1182,1314,660,685]" pageId="4" pageNumber="569" value="2000-12-29">29.xii.2000</date>
