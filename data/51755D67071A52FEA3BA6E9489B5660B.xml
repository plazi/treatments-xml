<taxonomicName id="72575F07B9C29E68036001BE5269ED7E" class="Magnoliopsida" family="Fabaceae" genus="Desmodium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Desmodium tortuosum" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="tortuosum">
<emphasis id="57D0F0E6D0FA794A40F57B17F0E08AFA" italics="true" pageId="0" pageNumber="69">Desmodium tortuosum</emphasis>
</taxonomicName>
(
<taxonomicName id="6503D4FE588FF12CE15EFA5BD1206D32" class="Magnoliopsida" family="Fabaceae" genus="Desmodium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
