<taxonomicName id="B24DACFC1C93875ECD56F648369212C1" authorityName="Ellis &amp; G. Martin, Am. Nat. 16 (12): 1003" authorityYear="1882" class="Dothideomycetes" family="Mycosphaerellaceae" genus="Cercospora" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Cercospora canescens" order="Mycosphaerellales" pageId="0" pageNumber="69" phylum="Ascomycota" rank="species" species="canescens">Cercospora aff. canescens</taxonomicName>
