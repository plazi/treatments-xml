<taxonomicName id="4C057380FFAB7420FF2740BDFD69FA42" authority="Hickman, 1931" authorityName="Hickman" authorityYear="1931" box="[229,664,1504,1530]" class="Arachnida" family="Symphytognathidae" kingdom="Animalia" order="Araneae" pageId="52" pageNumber="61" phylum="Arthropoda" rank="family">Symphytognathidae Hickman, 1931</taxonomicName>
