; pronotum maximal width 
<number id="2B462494CF4FFFE27E02FEC8FDF87D49" box="[562,614,357,381]" pageId="5" pageNumber="62" value="1.3">1.30</number>
; elytral length 
<number id="2B462494CF4FFFE27F25FEC8FCD77D49" box="[789,841,357,381]" pageId="5" pageNumber="62" value="2.33">2.33</number>
; elytron width at base 
<number id="2B462494CF4FFFE27864FEC8FB167D49" box="[1108,1160,357,381]" pageId="5" pageNumber="62" value="0.53">0.53</number>
; elytron maximal width 
<number id="2B462494CF4FFFE27CA7FE24FF527D95" box="[151,204,393,417]" pageId="5" pageNumber="62" value="0.7">0.70</number>
