:— 
<taxonomicName id="4C004D77FFE5C8523651FA478964FA25" authorityName="W. T. Wang, Y. G. Wei &amp; A. K. Monro" authorityYear="2011" box="[367,635,1452,1477]" class="Magnoliopsida" family="Urticaceae" genus="Elatostema" kingdom="Plantae" order="Rosales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="species" species="multicaule">
