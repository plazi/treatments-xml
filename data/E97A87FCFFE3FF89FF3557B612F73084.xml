<paragraph id="616C36EAFFE3FF88FF3557EB10B83392" blockId="4.[151,778,671,732]" box="[151,435,706,732]" pageId="4" pageNumber="31">
<figureCitation id="F9E82A6FFFE3FF88FF3557EB11E23392" box="[151,233,706,732]" captionStart="FIGURES 1 – 6" captionStartId="2.[151,269,1817,1841]" captionTargetBox="[186,1403,219,1816]" captionTargetId="figure@2.[151,1436,193,1887]" captionTargetPageId="2" captionText="FIGURES 1 – 6. Labia. 1. Canthochilum brodzinskyorum; 2. Canthochilum magnum; 3. Canthochilum matthewsi; 4. Canthochilum mimicum; 5. Canthochilum nebulonemi; 6. Canthochilum platycnemis." httpUri="https://zenodo.org/record/181297/files/figure.png" pageId="4" pageNumber="31">
Figs. 
<number id="D71B24CDFFE3FF88FF7A57EB11E23392" box="[216,233,706,732]" pageId="4" pageNumber="31" value="2.8">2</number>
</figureCitation>
, 8, 
<number id="D71B24CDFFE3FF88FEB057EB10553392" box="[274,350,706,732]" pageId="4" pageNumber="31" value="14.2">14, 20</number>
, 
<number id="D71B24CDFFE3FF88FEC857EB10B83392" box="[362,435,706,732]" pageId="4" pageNumber="31" value="26.35">26, 35</number>
