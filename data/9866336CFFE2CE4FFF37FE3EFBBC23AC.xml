<quantity id="D7372F9FFFE2CE50FEE9F85EFE52258C" box="[329,435,1965,1991]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="14" pageNumber="29" unit="mm" value="0.5">0.50 mm</quantity>
