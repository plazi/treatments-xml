<taxonomicName id="4C6C04EEFFD43B2EFCFDBB98FBBDF8FD" box="[864,1143,1789,1814]" class="Aves" family="Parulidae" genus="Basileuterus" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="42" phylum="Chordata" rank="species" species="diachlorus">
