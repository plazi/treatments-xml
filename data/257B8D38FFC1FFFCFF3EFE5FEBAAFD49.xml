<taxonomicName id="6AD247ADFFC1FFFCFD9DFE7BEB60FE32" box="[564,635,439,465]" class="Insecta" family="Pentatomidae" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="73" phylum="Arthropoda" rank="subFamily" subFamily="Brazil">Brazil</taxonomicName>
