<taxonomicName id="4C314D33FF8F7440A47E78210B7EDB3A" class="Magnoliopsida" family="Vitaceae" genus="Ampelopsis" kingdom="Plantae" order="Vitales" pageId="37" pageNumber="38" phylum="Tracheophyta" rank="species" species="arborea">
