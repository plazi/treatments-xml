<taxonomicName id="4C314D33FF8F7440A2BD7BF90FF8D88A" authority="Michx. var. aestivalis" box="[1001,1436,472,498]" class="Magnoliopsida" family="Vitaceae" genus="Vitis" kingdom="Plantae" order="Vitales" pageId="37" pageNumber="38" phylum="Tracheophyta" rank="variety" species="aestivalis" variety="aestivalis">
