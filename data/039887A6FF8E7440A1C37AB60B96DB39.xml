<taxonomicName id="4C314D33FF8F7440A2997B910F7FD8B2" box="[973,1307,432,457]" class="Magnoliopsida" family="Vitaceae" genus="Parthenocissus" kingdom="Plantae" order="Vitales" pageId="37" pageNumber="38" phylum="Tracheophyta" rank="species" species="quinquefolia">
