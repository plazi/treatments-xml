<paragraph id="8BD9368A8B7AFFC9FE9FFDB7FBD1FD9D" blockId="52.[264,1032,483,550]" box="[312,1032,524,550]" pageId="52" pageNumber="53">
<taxonomicName id="4C664D098B7AFFC9FE9FFDB7FE34FD9E" authorityName="Sun" authorityYear="2003" box="[312,493,524,549]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="frontata">
