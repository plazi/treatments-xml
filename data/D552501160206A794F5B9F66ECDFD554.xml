<normalizedToken id="FA90682E502ED115EE292080C44931EA" originalValue="Brullé">Brulle</normalizedToken>
, 1832 | 
<taxonomicName id="E43DA3C4D88C4FD9952F7028E86401FF" class="Polychaeta" family="Amphinomidae" genus="Hermodice" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hermodice carunculata" order="Amphinomida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="carunculata">Hermodice carunculata</taxonomicName>
