<taxonomicName id="9945EF5AB76F7822232C0EFF51969A0E" LSID="http://zoobank.org/61EC2F6C-06C1-4C8A-B821-3A58E6A3C4B6" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus parkeri" order="Hymenoptera" pageId="260" pageNumber="261" phylum="Arthropoda" rank="species" species="parkeri">
<pageBreakToken id="D66E9338B00F812BE4A567CFE6A25AA8" pageId="260" pageNumber="261" start="start">Heterospilus</pageBreakToken>
parkeri Marsh
</taxonomicName>
<taxonomicNameLabel id="9692C2896E0DFEE0B9E6F6B15EFE9C01" pageId="260" pageNumber="261">sp. n.</taxonomicNameLabel>
