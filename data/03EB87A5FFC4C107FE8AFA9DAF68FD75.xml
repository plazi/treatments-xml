</taxonomicName>
. 29, 
<taxonomicName id="4C424D30FFC7C101FC08F963AF51F900" authority="(Casey)" authorityName="Gusarov" authorityYear="2002" baseAuthorityName="Casey" baseAuthorityYear="1910" box="[906,1122,1748,1772]" class="Insecta" family="Staphylinidae" genus="Tomoglossa" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="ornatella">
<taxonomicName id="4C424D30FFC7C101FC08F963AF3BF907" box="[906,1032,1749,1772]" genus="T." pageId="10" pageNumber="11" rank="species" species="ornatella">
<emphasis id="B936EAA1FFC7C101FC08F963AF3BF907" box="[906,1032,1749,1772]" italics="true" pageId="10" pageNumber="11">
T. 
<taxonomicName id="4C424D30FFC7C101FC24F963AF3BF907" authority="Casey" authorityName="Casey" box="[934,1032,1749,1772]" pageId="10" pageNumber="11" rank="species" species="ornatella">ornatella</taxonomicName>
</emphasis>
</taxonomicName>
(Casey)
</taxonomicName>
(lectotype); 30, 
<taxonomicName id="4C424D30FFC7C101FA95F963AAF8F8E5" authority="(Casey)" authorityName="Gusarov" authorityYear="2002" baseAuthorityName="Casey" baseAuthorityYear="1910" class="Insecta" family="Staphylinidae" genus="Tomoglossa" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="improvisa">
<taxonomicName id="4C424D30FFC7C101FA95F963AA42F8E4" genus="T." pageId="10" pageNumber="11" rank="species" species="improvisa">
<emphasis id="B936EAA1FFC7C101FA95F963AA42F8E4" italics="true" pageId="10" pageNumber="11">
T. 
<taxonomicName id="4C424D30FFC7C101FE8AF94EAA42F8E4" authority="Casey" authorityName="Casey" box="[264,369,1784,1807]" pageId="10" pageNumber="11" rank="species" species="improvisa">improvisa</taxonomicName>
</emphasis>
</taxonomicName>
(Casey)
</taxonomicName>
(lectotype); 31, 
<taxonomicName id="4C424D30FFC7C101FDFDF94EA80DF8E5" authority="(Casey)" authorityName="Gusarov" authorityYear="2002" baseAuthorityName="Casey" baseAuthorityYear="1910" box="[639,830,1783,1807]" class="Insecta" family="Staphylinidae" genus="Tomoglossa" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="decora">
<emphasis id="B936EAA1FFC7C101FDFDF94EA9D0F8E4" box="[639,739,1784,1807]" italics="true" pageId="10" pageNumber="11">
T. 
<taxonomicName id="4C424D30FFC7C101FD18F94EA9D0F8E4" authority="Casey" authorityName="Casey" box="[666,739,1784,1807]" pageId="10" pageNumber="11" rank="species" species="decora">decora</taxonomicName>
</emphasis>
(Casey)
</taxonomicName>
(Pennsylvania); 32, 
<taxonomicName id="4C424D30FFC7C101FB9FF94EAFCCF8E5" authority="Gusarov" authorityName="Gusarov" authorityYear="2002" box="[1053,1279,1783,1807]" class="Insecta" family="Staphylinidae" genus="Tomoglossa" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="arizonica" status="sp. nov.">
