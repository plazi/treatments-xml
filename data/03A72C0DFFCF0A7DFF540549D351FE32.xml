<taxonomicName id="4C0EE698FFCE0A7FFE97055FD20EF89E" authorityName="Pascoe" authorityYear="1857" box="[340,562,1813,1835]" class="Insecta" family="Cerambycidae" genus="Cereopsius" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="143" phylum="Arthropoda" rank="species" species="marmoreus">
