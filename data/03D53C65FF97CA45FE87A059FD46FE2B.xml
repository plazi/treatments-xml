&quot; 
<taxonomicName id="4C7CF6F0FF97CA45FE9BA059FDA3FEE7" authority="Fabr." authorityName="Fabr." authorityYear="1934" box="[276,533,283,309]" class="Insecta" family="Conopidae" genus="Occemya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="atra">Occemya atra Fabr.</taxonomicName>
