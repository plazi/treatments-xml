<taxonomicName id="6DFD27A9BF79FFC55ABEFC0037274F6D" box="[449,585,999,1025]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
