<taxonomicName id="6DFD27A9BF79FFC559BCFC0036A14F6C" box="[707,975,999,1024]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="lycopersicum">
<emphasis id="98898038BF79FFC559BCFC0036A14F6C" box="[707,975,999,1024]" italics="true" pageId="20" pageNumber="21">Solanum lycopersicum</emphasis>
</taxonomicName>
L., 
<taxonomicName id="6DFD27A9BF79FFC55F7CFC0031E54F6D" box="[1027,1163,999,1025]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
