<taxonomicName id="6DFD27A9BF79FFC55968FBE837E34F44" box="[535,653,1039,1064]" class="Liliopsida" family="Poaceae" genus="Zea" kingdom="Plantae" order="Poales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="mays">
<emphasis id="98898038BF79FFC55968FBE837E34F44" box="[535,653,1039,1064]" italics="true" pageId="20" pageNumber="21">Zea mays</emphasis>
