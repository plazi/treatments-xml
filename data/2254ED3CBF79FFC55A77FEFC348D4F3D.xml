<taxonomicName id="6DFD27A9BF79FFC559B5FBE836424F45" authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[714,812,1039,1065]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
