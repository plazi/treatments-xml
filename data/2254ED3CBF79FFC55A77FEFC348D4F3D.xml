<taxonomicName id="6DFD27A9BF79FFC55FDDFBE8304C4F45" box="[1186,1314,1039,1065]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
