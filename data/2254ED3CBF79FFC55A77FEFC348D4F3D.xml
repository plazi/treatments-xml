<taxonomicName id="6DFD27A9BF79FFC558A4FBE831E04F45" box="[987,1166,1039,1065]" class="Magnoliopsida" family="Malvaceae" genus="Gossypium" kingdom="Plantae" order="Malvales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="98898038BF79FFC558A4FBE831334F44" box="[987,1117,1039,1064]" italics="true" pageId="20" pageNumber="21">Gossypium</emphasis>
