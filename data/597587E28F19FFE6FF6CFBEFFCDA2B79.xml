[
<taxonomicName id="16DC4D778F19FFE6FBEEF9F3FB892A72" box="[1045,1126,1604,1628]" class="Insecta" family="Baetidae" genus="Cloeon" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="493" phylum="Arthropoda" rank="genus">
