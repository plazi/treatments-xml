[[ 
<taxonomicName id="890F8048EE799FC6D16B7BA686138048" LSID-ZBK="urn:lsid:zoobank.org:act:4210616A-20C6-439C-B592-B4D7E6DC8E61" class="Actinopterygii" family="Trichomycteridae" genus="Listrura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Listrura de Pinna 1988:3" order="Siluriformes" pageNumber="43" phylum="Chordata" rank="genus">Listrura de Pinna</taxonomicName>
