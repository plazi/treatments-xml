<taxonomicName id="1A1790D185DDFDF752268B9EA7349BDF" baseAuthorityName="Gray" baseAuthorityYear="1831" class="Reptilia" family="Geoemydidae" genus="Mauremys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Testudinata" pageId="30" pageNumber="49" phylum="Chordata" rank="species" species="reevesii">
<emphasis id="A782B2CE0FB3212A8EC3DD589AF880AC" italics="true" pageId="30" pageNumber="49">Mauremys reevesii</emphasis>
