st. 
<taxonomicName id="4C1EADC6FF9EFFCF76E7FF30FD29FF3F" authority="Forel, 1916: 453" authorityName="Forel" authorityPageNumber="453" authorityYear="1916" box="[393,662,222,244]" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="volkarti">
