<taxonomicName id="4C514D532673FFC0463DFCD6FA6AFCD8" box="[1298,1436,780,801]" class="Malacostraca" family="Xanthidae" genus="Glyptoxanthus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="34" phylum="Arthropoda" rank="species" species="meandricus">
