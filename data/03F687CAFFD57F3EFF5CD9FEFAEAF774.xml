<quantity id="4CA79B39FFD57F3FFD0FDBD4FCE9F38B" box="[708,870,1665,1692]" metricMagnitude="-3" metricUnit="m" metricValue="2.13" metricValueMax="2.21" metricValueMin="2.05" pageId="48" pageNumber="249" unit="mm" value="2.13" valueMax="2.21" valueMin="2.05">2.05–2.21 mm</quantity>
