<collectionCode id="DCA14C9FFF5AFF60355352162504F88A" box="[556,631,1820,1844]" country="Australia" lsid="urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="176" pageNumber="177">ANIC</collectionCode>
