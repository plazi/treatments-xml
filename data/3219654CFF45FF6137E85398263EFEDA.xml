<quantity id="7D4879BFFF5AFF60342053F224E9F8AE" box="[863,922,1783,1808]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="176" pageNumber="177" unit="km" value="4.0">4 km</quantity>
