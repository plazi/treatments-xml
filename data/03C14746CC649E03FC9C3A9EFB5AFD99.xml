<taxonomicName id="4C688DD3CC649E03FB80393EFA19FC5F" authority="De Vis, 1892" authorityName="De Vis" authorityYear="1892" box="[1073,1451,1554,1581]" class="Reptilia" family="Scincidae" genus="Mocoa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="623" phylum="Chordata" rank="species" species="caeruleocauda">
