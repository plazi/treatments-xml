<taxonomicName id="FFF34D4AA937FF9B0603F889CA44F8CF" baseAuthorityName="Walsingham" baseAuthorityYear="1889" box="[346,573,1821,1845]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="species" species="ledella">
