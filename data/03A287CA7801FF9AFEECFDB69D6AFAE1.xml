<taxonomicName id="4C0B4D5F7800FF9AFD1FFBE69F14FB82" box="[763,800,1124,1149]" class="Insecta" family="Carabidae" genus="Agra" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="not">
