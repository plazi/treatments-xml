<quantity id="4CEF9B25FF71FF559BBAFAC8FE513550" box="[967,1040,1338,1365]" metricMagnitude="-1" metricUnit="m" metricValue="2.5" pageId="224" pageNumber="225" unit="cm" value="25.0">25 cm</quantity>
