<taxonomicName id="2DDC4D7BFFF22A0BDAA9FE018E1BB20C" box="[1240,1324,511,536]" class="Insecta" family="Staphylinidae" genus="Paluma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="165" phylum="Arthropoda" rank="genus">Paluma</taxonomicName>
