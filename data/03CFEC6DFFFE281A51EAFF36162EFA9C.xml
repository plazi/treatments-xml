<taxonomicName id="4C6626F8FFFE281A53B0FECE14D5FEE0" authority="Scopoli, 1786" authorityName="Scopoli" authorityYear="1786" box="[717,994,293,315]" class="Insecta" family="Reduviidae" genus="Ploiaria" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="558" phylum="Arthropoda" rank="species" species="domestica">
