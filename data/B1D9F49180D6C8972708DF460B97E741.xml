<taxonomicName id="54FB347C0D5234D70D68FB4F05EB42F6" class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="631" phylum="Tracheophyta" rank="genus">Orchis</taxonomicName>
