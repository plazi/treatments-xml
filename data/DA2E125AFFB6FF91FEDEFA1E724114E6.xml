<taxonomicName id="9587D8CFFFB4FF91FD77F9FC75A61549" authorityName="Lindley" authorityYear="1854" box="[827,960,1580,1602]" class="Liliopsida" family="Orchidaceae" genus="Coelogyne" kingdom="Plantae" order="Asparagales" pageId="2" pageNumber="117" phylum="Tracheophyta" rank="species" species="corymbosa">
<emphasis id="60F37F5EFFB4FF91FD77F9FC75A61549" box="[827,960,1580,1602]" italics="true" pageId="2" pageNumber="117">C. corymbosa</emphasis>
