<taxonomicName id="4C011396D223026DFF4EFE0953AA310B" authority="Liu and Gu 2019" authorityName="Liu and Gu" authorityYear="2019" box="[231,680,509,533]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="44" pageNumber="2826" phylum="Firmicutes" rank="species" species="daowaiensis">
<emphasis id="B975B407D223026DFF4EFE0950F1310B" box="[231,499,509,533]" italics="true" pageId="44" pageNumber="2826">Lactobacillus daowaiensis</emphasis>
