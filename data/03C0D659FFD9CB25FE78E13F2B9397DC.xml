<location id="8EB63194FFD9CB25FFC5E1DF283396E6" LSID="urn:lsid:plazi:treatment:03C0D659FFD9CB25FE78E13F2B9397DC:8EB63194FFD9CB25FFC5E1DF283396E6" box="[125,294,1224,1248]" county="Ningshan County" name="Mount Taibai" pageId="12" pageNumber="2730">Mount Taibai</location>
