species:
</emphasis>
<taxonomicName id="72A0A9B7D437564EFEA0FFC7787DFA67" authority="Bezerra &amp; Pape &amp; Hauquier &amp; Vanreusel, 2021" authorityName="Bezerra &amp; Pape &amp; Hauquier &amp; Vanreusel" authorityYear="2021" box="[364,587,1483,1509]" class="Adenophorea" family="Desmoscolecidae" genus="Odetenema" kingdom="Animalia" order="Desmoscolecida" pageId="13" pageNumber="555" phylum="Nematoda" rank="species" species="gesarae" status="sp. nov.">
<emphasis id="87D40E26D437564EFEA0FFC7787DFA67" box="[364,587,1483,1509]" italics="true" pageId="13" pageNumber="555">Odetenema gesarae</emphasis>
</taxonomicName>
