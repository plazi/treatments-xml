<taxonomicName id="34E64D289538FFC4FBDD59A0FAD57AA6" authorityName="Willey" authorityYear="1905" box="[1109,1281,1966,1989]" class="Polychaeta" family="Serpulidae" genus="Serpula" kingdom="Animalia" order="Sabellida" pageId="46" pageNumber="47" phylum="Annelida" rank="species" species="watsoni">
