<collectingCountry id="47881CD2FFE6C2088FB7B1E8BD30709E" box="[1196,1284,1679,1703]" name="Mexico" pageId="14" pageNumber="40">Mexico</collectingCountry>
, 
<collectingCountry id="47881CD2FFE6C2088E14B1F7BDA3709E" box="[1295,1431,1679,1703]" name="Puerto Rico" pageId="14" pageNumber="40">Puerto Rico</collectingCountry>
