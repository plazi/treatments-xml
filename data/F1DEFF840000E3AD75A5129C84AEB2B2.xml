<taxonomicName id="D3BC2A7CD641455E4B3364FC7357FDA9" authority="Marseul, 1857" authorityName="Marseul" authorityYear="1857" lsidName="" pageId="89" pageNumber="90" rank="subFamily" subFamily="Trypanaeinae">Trypanaeinae Marseul, 1857</taxonomicName>
