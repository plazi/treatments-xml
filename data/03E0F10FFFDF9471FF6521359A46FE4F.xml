<subSubSection id="C3531392FFDE9471FF3526B29A46FE4F" pageId="3" pageNumber="298" type="discussion">
<paragraph id="8BF64019FFDE9471FF3526B29A46FE4F" blockId="3.[151,1437,151,464]" pageId="3" pageNumber="298">
<emphasis id="B93D9C0BFFDE9471FF3526B29F0CFE32" bold="true" box="[199,315,404,429]" pageId="3" pageNumber="298">Remarks.</emphasis>
<taxonomicName id="4C493B9AFFDE9471FEB326B29C5DFE33" authorityName=": Oku" authorityYear="1979" box="[321,618,404,428]" class="Insecta" family="Tortricidae" genus="Pseudacroclita" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="298" phylum="Arthropoda" rank="species" species="hapalaspis">
<emphasis id="B93D9C0BFFDE9471FEB326B29C5DFE33" box="[321,618,404,428]" italics="true" pageId="3" pageNumber="298">Pseudacroclita hapalaspis</emphasis>
</taxonomicName>
