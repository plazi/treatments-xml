<taxonomicName id="4C7326F2FF9AAC55FAD1FA55BFE81680" box="[1335,1429,1453,1476]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="62" phylum="Tracheophyta" rank="genus">
<emphasis id="B9078163FF9AAC55FAD1FA55BFE81680" box="[1335,1429,1453,1476]" italics="true" pageId="6" pageNumber="62">Quercus</emphasis>
</taxonomicName>
, SubgenuS 
<taxonomicName id="4C7326F2FF9AAC55FEECFA28BBB816AD" authority="Oersted, 1867" authorityName="Oersted" authorityYear="1867" box="[266,453,1488,1513]" class="Magnoliopsida" family="Fagaceae" genus="Cyclobalanopsis" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="62" phylum="Tracheophyta" rank="genus">
