<taxonomicName id="4C7326F2FF9AAC55FB1DFA29BF2416AC" box="[1275,1369,1489,1512]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="62" phylum="Tracheophyta" rank="genus">
