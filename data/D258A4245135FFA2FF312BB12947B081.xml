<quantity id="9D09B8D75135FFA3FD962BF92F09B7A7" box="[560,617,2011,2035]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="12" pageNumber="147" unit="mm" value="4.0">4mm</quantity>
