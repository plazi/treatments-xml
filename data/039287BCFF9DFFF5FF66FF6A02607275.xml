</taxonomicName>
, 
<taxonomicName id="4C3B4D29FF9DFFF5FC38FB58020473B1" box="[969,1135,1189,1214]" class="Malacostraca" family="Paguridae" genus="Pagurus" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="54" phylum="Arthropoda" rank="species" species="longimanus">
