<taxonomicName id="4C3B4D29FF9DFFF5FCACFB5805D373B1" box="[861,952,1189,1214]" class="Malacostraca" family="Paguridae" genus="Pagurus" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="54" phylum="Arthropoda" rank="species" species="exilis">
