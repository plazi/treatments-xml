<taxonomicName id="4C4E4D27F83CFFA5FC75FBFFFB30FBB8" authority="LATREILLE, 1804" authorityName="Latreille" authorityYear="1804" box="[959,1269,1112,1133]" class="Insecta" family="Syrphidae" genus="Paragus" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="535" phylum="Arthropoda" rank="genus">
