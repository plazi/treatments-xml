<location id="5B4E60C4FFB251445966ABFFF953FB58" LSID="urn:lsid:plazi:treatment:D6388709FFB251445E6DAC6EF952FA32:5B4E60C4FFB251445966ABFFF953FB58" box="[1597,1705,1189,1230]" country="Samoa" municipality="Tutuila" name="Upolu" pageId="66" pageNumber="139">Upolu</location>
