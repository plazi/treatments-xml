<collectorName id="F36453C9FFB251445D08AB2BFC97FB0D" box="[595,877,1137,1179]" pageId="66" pageNumber="139">L. A. Whitney</collectorName>
; and one, 
<collectingMunicipality id="BE4AAC65FFB251445B68AB2BFAF5FB0D" box="[1075,1295,1137,1179]" pageId="66" pageNumber="139">Aunuu Islet</collectingMunicipality>
, 
<collectingCounty id="B74F4E93FFB251445A71AB2BFA55FB0D" box="[1322,1455,1137,1179]" pageId="66" pageNumber="139">Tutuila</collectingCounty>
, 
<collectingCountry id="2686768FFFB251445A97AB2BF9B8FB0D" box="[1484,1602,1137,1179]" name="Samoa" pageId="66" pageNumber="139">Samoa</collectingCountry>
</materialsCitation>
, 
<materialsCitation id="EEF93C42FFB251445906AB2BFD64FA94" ID-GBIF-Occurrence="3334700301" collectingDate="1930-02" country="Samoa" location="Upolu" municipality="Tutuila" pageId="66" pageNumber="139" specimenCount="1">
<date id="2A2F10DFFFB251445906AB2BFE77FB58" pageId="66" pageNumber="139" value="1930-02">
<collectingDate id="3A6BE937FFB251445906AB2BFE77FB58" pageId="66" pageNumber="139" value="1930-02">Feb. 1930</collectingDate>
</date>
