<collectorName id="F36453C9FFB251445CDBAB67FB89FBFE" box="[896,1139,1085,1128]" pageId="66" pageNumber="139">C. M. Cooke</collectorName>
, Jr.; one, 
<location id="5B4E60C4FFB251445A1FAB67F95BFBFE" LSID="urn:lsid:plazi:treatment:D6388709FFB251445E6DAC6EF952FA32:5B4E60C4FFB251445A1FAB67F95BFBFE" box="[1348,1697,1085,1128]" country="Samoa" county="Tutuila" municipality="Aunuu Islet" name="Washington Island" pageId="66" pageNumber="139">Washington Island</location>
, 
<date id="2A2F10DFFFB251445E6DAB2BFDC3FB0D" box="[310,569,1137,1179]" pageId="66" pageNumber="139" value="1924-08-18">
<collectingDate id="3A6BE937FFB251445E6DAB2BFDC3FB0D" box="[310,569,1137,1179]" pageId="66" pageNumber="139" value="1924-08-18">Aug. 18, 1924</collectingDate>
</date>
