<typeStatus id="54FD75AFFFD2FFB60380FA2CFF3E4BA9" box="[168,226,1410,1436]" pageId="49" pageNumber="50">TYPE</typeStatus>
SPECIES: 
<emphasis id="B932171FFFD2FFB6027AFA2FFDA64BAE" box="[338,634,1409,1435]" italics="true" pageId="49" pageNumber="50">
<taxonomicName id="4C46B08EFFD2FFB6027AFA2FFDAB4BAE" authority="Vea &amp; Grimaldi, 2015" authorityName="Vea &amp; Grimaldi" authorityYear="2015" box="[338,631,1409,1435]" class="Insecta" family="Pseudococcidae" genus="Williamsicoccus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="megalops" status="n. sp.">Williamsicoccus megalops</taxonomicName>
