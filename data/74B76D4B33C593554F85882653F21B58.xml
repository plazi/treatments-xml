, 
<taxonomicName id="53F4C026E6D730167CFD5AF5B8C70B2E" baseAuthorityName="Angas" baseAuthorityYear="1879" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="175" pageNumber="176" phylum="Mollusca" rank="species" species="zhorquinensis" subGenus="Drymaeus">
