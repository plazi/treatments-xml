<typeStatus id="54F088423046FFB5090EFB8EFD58FB9A" box="[538,656,1088,1112]" pageId="16" pageNumber="17">Lectotype</typeStatus>
of 
<taxonomicName id="4C4B4D633046FFB509D4FBF1FBFBFB9A" authority="Arrow" authorityName="Arrow" box="[704,1075,1087,1112]" class="Insecta" family="Curculionidae" genus="Stenotarsus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="guatemalae">
