<taxonomicName id="4C5AD994FFC9430EFF40DE89FDC95F90" authority="Klautau et al., 1994" authorityName="Klautau" authorityYear="1994" box="[232,533,1187,1204]" class="Calcarea" family="Clathrinidae" genus="Clathrina" kingdom="Animalia" order="Clathrinida" pageId="5" pageNumber="400" phylum="Porifera" rank="species" species="aspina">
