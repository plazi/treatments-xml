<taxonomicName id="6183B088FFCDE052FD6C6707FD22F9F9" box="[689,747,1573,1598]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="ollae">
