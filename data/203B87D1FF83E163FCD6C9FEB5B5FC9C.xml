<subSubSection id="E088654CFF83E163FCB8CF27B5B5FC9C" lastPageId="22" lastPageNumber="253" pageId="21" pageNumber="252" type="discussion">
<paragraph id="A82D36C7FF83E163FCB8CF27B674FEDC" blockId="21.[804,1454,1828,1916]" lastBlockId="22.[139,789,282,370]" lastPageId="22" lastPageNumber="253" pageId="21" pageNumber="252">
<emphasis id="9AE6EAD5FF83E160FCB8CF27B181F892" box="[842,1253,1828,1852]" italics="true" pageId="21" pageNumber="252">
Species included. โ 
<taxonomicName id="6F924D44FF83E160FBE6CF27B181F892" box="[1044,1253,1828,1852]" class="Magnoliopsida" family="Salicaceae" genus="Homalium" kingdom="Plantae" order="Malpighiales" pageId="21" pageNumber="252" phylum="Tracheophyta" rank="species" species="boinense">Homalium boinense</taxonomicName>
