<taxonomicName id="189F20AB34F7D05EDC024095CACC7A05" baseAuthorityName="L. Pfeiffer" baseAuthorityYear="1856" class="Gastropoda" family="Bulimulidae" genus="Bulimulus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="coriaceus">
