<paragraph id="AF0592FC43165198E09AF4D0383126FD" pageId="10" pageNumber="11">
<taxonomicName id="D70860D9E142474FE0EE37390D0CEC92" authority="Rohr, 1792" authorityName="Rohr" authorityYear="1792" class="Magnoliopsida" family="Microteaceae" genus="Schollera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Schollera" order="Caryophyllales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="genus">Schollera</taxonomicName>
