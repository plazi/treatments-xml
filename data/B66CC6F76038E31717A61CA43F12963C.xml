<taxonomicName id="128A87B3B529DFE7C54826BD19A0B4F6" class="Magnoliopsida" family="Cannabaceae" genus="Aphananthe" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Caryophyllales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="family">Cannabaceae</taxonomicName>
