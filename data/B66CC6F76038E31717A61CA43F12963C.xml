<taxonomicName id="E859D12A4785520D3BB9DB4C71B58E70" class="Magnoliopsida" family="Microteaceae" genus="Microtea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Microtea celosioides" order="Caryophyllales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="celosioides">Microtea celosioides</taxonomicName>
