<quantity id="4CFF6AF87024FFB8FEC4F9BEC45F4AF6" box="[257,307,1654,1675]" metricMagnitude="1" metricUnit="m" metricValue="1.6" pageId="1" pageNumber="390" unit="m" value="16.0">16 m</quantity>
above sea level
</elevation>
, 
<collectorName id="26F2A2CB7024FFB8FE18F9BEC7E04AF6" box="[477,652,1654,1675]" pageId="1" pageNumber="390">Kuching Division</collectorName>
, 
<collectingCountry id="F310878D7024FFB8FD52F9BEC7804AF6" box="[663,748,1654,1675]" name="Malaysia" pageId="1" pageNumber="390">Sarawak</collectingCountry>
, coll. 
<collectorName id="26F2A2CB7024FFB8FF66F95BC4694AD5" box="[163,261,1683,1704]" pageId="1" pageNumber="390">P.K.L. Ng</collectorName>
et al., 
