<collectingCountry id="F3007650FFDFFFFB9B74FC21F90733E8" box="[777,838,979,1005]" name="Chile" pageId="78" pageNumber="79">Chile</collectingCountry>
(
<geoCoordinate id="EE235007FFDFFFFB9B25FC20F9F633E8" box="[856,951,978,1005]" degrees="21" direction="south" minutes="05" orientation="latitude" pageId="78" pageNumber="79" precision="925" value="-21.083334">21°05’S</geoCoordinate>
