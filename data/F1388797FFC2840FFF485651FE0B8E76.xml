<taxonomicName id="BE914D02FFC3840FFEA0527CFD208931" authorityName="Tendeiro" authorityYear="1988" box="[383,688,799,825]" class="Insecta" family="Menoponidae" genus="Colpocephalum" kingdom="Animalia" order="Psocodea" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="subzerafae">
