<taxonomicName id="4A9D0289C765FFDBFE486B8BFCB9FE2F" box="[491,866,402,426]" class="Arachnida" family="Tetranychidae" genus="Mixonychus" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="503" phylum="Arthropoda" rank="species" species="orissaensis" subGenus="Bakerina">
