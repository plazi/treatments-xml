<taxonomicName id="C8AE438AFF8BFFBDFDF1FBD214FB9563" box="[516,722,1098,1119]" class="Insecta" family="Scarabaeidae" genus="Deltochilum" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="131" phylum="Arthropoda" rank="subSpecies" species="acropyge" subSpecies="valgum">
