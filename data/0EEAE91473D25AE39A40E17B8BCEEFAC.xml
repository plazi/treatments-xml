<taxonomicName id="F028BC1E7E4155C05FC65B80D02FED50" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="44" pageNumber="45" phylum="Angiospermae" rank="species" species="shawii">
<emphasis id="0473DC0EC7E3CBD38E2F358343D79E35" italics="true" pageId="44" pageNumber="45">Lycium shawii</emphasis>
</taxonomicName>
