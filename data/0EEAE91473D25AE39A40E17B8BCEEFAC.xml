<taxonomicName id="D5F80C9AA598BF101527E008C836CF6E" authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="44" pageNumber="45" phylum="Angiospermae" rank="species" species="stricta">
