<taxonomicName id="471F2E03ACBBE10ADBCE2EA7D4B722F7" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="gerrardii">Acacia gerrardii</taxonomicName>
</emphasis>
