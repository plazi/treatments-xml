<taxonomicName id="C79CA26BBB23CFA59B2E876AD08164B8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136677" class="Insecta" family="Formicidae" genus="Cerapachys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cerapachys mayri var. brachynodus Forel" lsidName-HNS="Cerapachys mayri var. brachynodus Forel" order="Hymenoptera" phylum="Arthropoda" rank="variety" species="Mayri" species.completionEvidence="missing" species.completionRound="1" variety="brachynodus">Cerapachys Mayri Forel, var. brachynodus</taxonomicName>
