<normalizedToken id="8F1CD637C3E461A53D4EF175A1BEEE7C" originalValue="Pirenópolis">Pirenopolis</normalizedToken>
, 820 m, 
<geoCoordinate id="B750D58F0BD01E1FDEC1001307FC048E" direction="south" orientation="latitude" precision="925" value="-15.816667">15°49'S</geoCoordinate>
, 
<geoCoordinate id="7362B6BAAFB222F4CF7B944261562F09" direction="west" orientation="longitude" precision="925" value="-48.983334">48°59'W</geoCoordinate>
