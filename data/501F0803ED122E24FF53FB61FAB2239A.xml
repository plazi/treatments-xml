<treatment id="501F0803ED122E24FF53FB61FAB2239A" ID-DOI="http://doi.org/10.5281/zenodo.6131195" ID-GBIF-Taxon="119419679" ID-Zenodo-Dep="6131195" LSID="urn:lsid:plazi:treatment:501F0803ED122E24FF53FB61FAB2239A" httpUri="http://treatment.plazi.org/id/501F0803ED122E24FF53FB61FAB2239A" lastPageId="26" lastPageNumber="427" pageId="24" pageNumber="425">
