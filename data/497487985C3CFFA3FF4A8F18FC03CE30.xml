(mean (
<quantity id="06259B6B5C39FFA3FD6C8E21FC80CF73" box="[689,795,548,572]" metricMagnitude="-4" metricUnit="m" metricValue="3.1" pageId="4" pageNumber="67" unit="mm" value="0.31">0.31 mm</quantity>
