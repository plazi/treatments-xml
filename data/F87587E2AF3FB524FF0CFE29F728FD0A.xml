<collectionCode id="16CDAE31AF3FB524FBE6FDDBF73AFD9A" box="[1122,1145,591,615]" country="USA" lsid="urn:lsid:biocol.org:col:14792" name="Yale University" pageId="1" pageNumber="108" type="Herbarium">Y</collectionCode>
