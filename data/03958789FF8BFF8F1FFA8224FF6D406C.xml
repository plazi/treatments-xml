<number id="3DF424B8FF8BFF8E1D5586DFFDC547A4" box="[568,596,1724,1748]" pageId="4" pageNumber="61" value="10.0">10</number>
<emphasis id="B948EA8DFF8BFF8E1D3386DEFDFD47A4" box="[606,620,1725,1748]" italics="true" pageId="4" pageNumber="61">p</emphasis>
