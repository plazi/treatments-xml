<number id="3DF424B8FF8BFF8E1FBC86FBFF4E47C0" box="[209,223,1688,1712]" pageId="4" pageNumber="61" value="2.0">2</number>
long 
<emphasis id="B948EA8DFF8BFF8E1E4F86FAFEAC47C0" box="[290,317,1689,1712]" italics="true" pageId="4" pageNumber="61">av</emphasis>
on distal 
<number id="3DF424B8FF8BFF8E1EC286FBFE2B47C0" box="[431,442,1688,1712]" pageId="4" pageNumber="61" value="1.0">1</number>
/3, 3 or 
<number id="3DF424B8FF8BFF8E1D7486FBFDB647C0" box="[537,551,1688,1712]" pageId="4" pageNumber="61" value="4.0">4</number>
