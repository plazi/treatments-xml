<taxonomicName id="4C4D4D48C50DFFEAFF5CFE447842FE0E" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[151,623,424,451]" class="Insecta" family="Geometridae" genus="Cyclophora" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="17" phylum="Arthropoda" rank="species" species="punctaria">
