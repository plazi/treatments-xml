<taxonomicName baseAuthorityName="Fabricius" baseAuthorityYear="1798" class="Insecta" family="Staphylinidae" genus="Camponotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Camponotus sericeus" order="Coleoptera" pageId="0" pageNumber="155" phylum="Arthropoda" rank="species" species="sericeus">
