- 
<number box="[1399,1410,1214,1238]" pageId="29" pageNumber="180" value="1.0">1</number>
- 
<number box="[1420,1431,1214,1238]" pageId="29" pageNumber="180" value="1.0">1</number>
, trochanters 
<number box="[286,298,1250,1274]" pageId="29" pageNumber="180" value="1.0">1</number>
- 
<number box="[309,350,1250,1274]" pageId="29" pageNumber="180" value="1.5" valueMax="2.0" valueMin="1.0">1-2</number>
- 
<number box="[360,372,1250,1274]" pageId="29" pageNumber="180" value="1.0">1</number>
, femora 
<number box="[479,496,1250,1274]" pageId="29" pageNumber="180" value="4.0">4</number>
- 
<number box="[505,522,1250,1274]" pageId="29" pageNumber="180" value="4.0">4</number>
- 
<number box="[530,546,1250,1274]" pageId="29" pageNumber="180" value="2.0">2</number>
- 
<number box="[555,565,1250,1274]" pageId="29" pageNumber="180" value="1.0">1</number>
, genua 
<number box="[660,675,1250,1274]" pageId="29" pageNumber="180" value="2.0">2</number>
- 
<number box="[683,698,1250,1274]" pageId="29" pageNumber="180" value="2.0">2</number>
-0-0, tibiae 
<number box="[836,851,1249,1274]" pageId="29" pageNumber="180" value="5.0">5</number>
- 
<number box="[860,875,1249,1274]" pageId="29" pageNumber="180" value="5.0">5</number>
- 
<number box="[884,898,1249,1274]" pageId="29" pageNumber="180" value="3.0">3</number>
- 
<number box="[907,921,1249,1274]" pageId="29" pageNumber="180" value="3.0">3</number>
, and tarsi 
<number box="[1049,1063,1249,1274]" pageId="29" pageNumber="180" value="8.0">8</number>
(ω)- 
<number box="[1111,1125,1249,1274]" pageId="29" pageNumber="180" value="8.0">8</number>
(ω)- 
<number box="[1173,1187,1249,1274]" pageId="29" pageNumber="180" value="5.0">5</number>
- 
