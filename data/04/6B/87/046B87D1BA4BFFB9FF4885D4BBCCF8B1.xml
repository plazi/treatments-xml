L. (
<taxonomicName box="[1187,1316,1681,1706]" class="Magnoliopsida" family="Lythraceae" kingdom="Plantae" order="Myrtales" pageId="29" pageNumber="180" phylum="Tracheophyta" rank="family">Lythraceae</taxonomicName>
), Antalya, 
<collectingCountry box="[151,232,1717,1742]" name="Turkey" pageId="29" pageNumber="180">Turkey</collectingCountry>
, 
<date box="[243,391,1717,1742]" pageId="29" pageNumber="180" value="2000-07-26">
