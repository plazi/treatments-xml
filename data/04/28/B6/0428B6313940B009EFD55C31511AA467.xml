<paragraph pageNumber="260">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24882" class="Insecta" family="Formicidae" genus="Ocymyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Ocymyrmex Emery" order="Hymenoptera" pageNumber="260" phylum="Arthropoda" rank="genus">Ocymyrmex</taxonomicName>
