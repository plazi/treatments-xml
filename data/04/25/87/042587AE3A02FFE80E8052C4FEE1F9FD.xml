, 
<collectingRegion box="[537,627,1370,1394]" country="China" name="Yunnan" pageId="4" pageNumber="301">Yunnan</collectingRegion>
), 
<collectingCountry box="[646,744,1369,1394]" name="Vietnam" pageId="4" pageNumber="301">Vietnam</collectingCountry>
, 
<collectingCountry box="[755,858,1369,1394]" name="Malaysia" pageId="4" pageNumber="301">Malaysia</collectingCountry>
