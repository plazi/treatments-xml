and 
<taxonomicName authority="Andrewes." authorityName="Andrewes." box="[916,1177,1686,1711]" class="Insecta" family="Carabidae" genus="Physodera" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="301" phylum="Arthropoda" rank="species" species="chalceres">
<emphasis box="[916,1049,1686,1711]" italics="true" pageId="4" pageNumber="301">P. chalceres</emphasis>
