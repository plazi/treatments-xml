<taxonomicName class="Gastropoda" family="Ellobiidae" genus="Zospeum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zospeum simplex" order="Basommatophora" pageId="0" pageNumber="175" phylum="Mollusca" rank="species" species="simplex">
