<tr pageId="null" pageNumber="629">
<td align="left" pageId="null" pageNumber="629" valign="top">
12*. 
<normalizedToken originalValue="Kelchblätter" pageId="null" pageNumber="629">Kelchblaetter</normalizedToken>
