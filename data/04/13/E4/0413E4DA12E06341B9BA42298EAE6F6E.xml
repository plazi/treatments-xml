<normalizedToken originalValue="Blütenstand" pageId="null" pageNumber="629">Bluetenstand</normalizedToken>
meist 1 
<normalizedToken originalValue="blütig" pageId="null" pageNumber="629">bluetig</normalizedToken>
; 
<normalizedToken originalValue="Blütenstiele" pageId="null" pageNumber="629">Bluetenstiele</normalizedToken>
