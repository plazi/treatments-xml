<td align="left" pageId="null" pageNumber="629" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Geraniaceae" genus="Geranium" kingdom="Plantae" order="Geraniales" pageId="null" pageNumber="629" phylum="Tracheophyta" rank="species" species="sanguineum">
