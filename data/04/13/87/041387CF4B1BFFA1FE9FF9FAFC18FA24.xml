, 
<location LSID="urn:lsid:plazi:treatment:041387CF4B1BFFA1FE9FF9FAFC18FA24:896560024B1BFFA7FDAEF8DDFDCFF881" box="[569,620,1817,1843]" country="Panama" municipality="Length" name="Tole" pageId="26" pageNumber="27">Tolé</location>
