<collectingCountry box="[705,762,1444,1470]" name="Peru" pageId="56" pageNumber="57">Peru</collectingCountry>
, 
<collectingCountry box="[773,862,1444,1470]" name="Bolivia" pageId="56" pageNumber="57">Bolivia</collectingCountry>
, 
<collectingCountry box="[874,945,1444,1470]" name="Brazil" pageId="56" pageNumber="57">Brazil</collectingCountry>
(AM).
</paragraph>
