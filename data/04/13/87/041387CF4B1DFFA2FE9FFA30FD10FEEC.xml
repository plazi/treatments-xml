<emphasis bold="true" box="[940,967,1292,1316]" pageId="30" pageNumber="31">42</emphasis>
, male antenna; 
<taxonomicName authorityName="Zikan" authorityYear="1949" box="[1137,1274,1292,1315]" class="Insecta" family="Vespidae" genus="Mischocyttarus" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="funerulus">
<emphasis box="[1137,1159,1292,1315]" italics="true" pageId="30" pageNumber="31">M</emphasis>
. 
<emphasis box="[1174,1274,1292,1315]" italics="true" pageId="30" pageNumber="31">funerulus</emphasis>
</taxonomicName>
: 
<emphasis bold="true" box="[1289,1315,1292,1316]" pageId="30" pageNumber="31">40</emphasis>
, T1, 
<emphasis bold="true" box="[307,333,1326,1350]" pageId="30" pageNumber="31">41</emphasis>
