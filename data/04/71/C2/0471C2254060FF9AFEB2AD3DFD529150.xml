<paragraph blockId="15.[264,1021,924,982]" pageId="15" pageNumber="16">
<taxonomicName authority="Merwe &amp; Ryke, 1964: 273" authorityName="Merwe &amp; Ryke" authorityPageNumber="273" authorityYear="1964" box="[264,1014,924,947]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="16" phylum="Arthropoda" rank="variety" species="citri" subGenus="Typhlodromalus" variety="der">
