<subSubSection pageId="18" pageNumber="271" type="description">
<paragraph blockId="18.[151,934,918,940]" box="[151,934,918,940]" pageId="18" pageNumber="271">
(syn.: 
<emphasis box="[215,464,918,940]" italics="true" pageId="18" pageNumber="271">
<taxonomicName baseAuthorityName="Binney" baseAuthorityYear="1843" box="[215,368,918,940]" class="Gastropoda" family="Vertiginidae" genus="Vertigo" kingdom="Animalia" order="Stylommatophora" pageId="18" pageNumber="271" phylum="Mollusca" rank="species" species="gouldii">Vertigo gouldii</taxonomicName>
