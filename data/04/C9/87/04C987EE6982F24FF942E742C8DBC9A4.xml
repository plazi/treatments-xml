<treatmentCitation httpUri="http://dx.doi.org/10.5281/zenodo.15068">
<taxonomicName authority="Sw." authorityName="Sw." class="Magnoliopsida" family="Ochnaceae" genus="Ochna" higherTaxonomySource="CoL" kingdom="Plantae" order="Malpighiales" pageId="1" pageNumber="1123" phylum="Tracheophyta" rank="species" species="nitida">
