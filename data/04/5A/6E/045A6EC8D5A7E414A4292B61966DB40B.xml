<subSubSection type="reference_group">
<paragraph pageNumber="49">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:137899" class="Insecta" family="Formicidae" genus="Dorylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Dorylus (Anomma) kohli Wasmann" order="Hymenoptera" pageNumber="49" phylum="Arthropoda" rank="species" species="kohli" subGenus="Anomma">Dorylus (Anomma) kohli Wasmann</taxonomicName>
