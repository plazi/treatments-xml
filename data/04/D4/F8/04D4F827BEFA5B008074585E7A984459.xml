<taxonomicName LSID="04D4F827-BEFA-5B00-8074-585E7A984459" authority="Dognin" authorityName="Dognin" authorityYear="1891" class="Insecta" family="Noctuidae" genus="Gaujonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gaujonia" order="Lepidoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="genus">Gaujonia Dognin</taxonomicName>
