Genus 
<taxonomicName id="54954D3F0049FFBAFF64FB73FD8CFB05" authority="Hope, 1837" authorityName="Hope" authorityYear="1837" box="[238,552,1161,1188]" class="Insecta" family="Cetoniidae" genus="Dicronorhina" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="genus">
