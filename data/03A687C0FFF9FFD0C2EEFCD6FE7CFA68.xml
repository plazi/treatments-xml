<quantity id="4CF79B33FFF9FFD0C1E1FC6AFC58FC2B" box="[920,974,947,967]" metricMagnitude="2" metricUnit="m" metricValue="4.85" pageId="22" pageNumber="177" unit="m" value="485.0">485m</quantity>
