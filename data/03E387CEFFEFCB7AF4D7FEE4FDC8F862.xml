<quantity id="4CB29B3DFFEFCB7AF464FBE7FE36FBAA" box="[357,450,1032,1059]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="12" pageNumber="13" unit="mm" value="4.5">4.5 mm</quantity>
