<taxonomicName id="9DEB4D3EFFED8E1E7C4F71C96E4FFB39" authority="Baron-Szabo, 2008" authorityName="Baron-Szabo" authorityYear="2008" box="[295,624,1233,1256]" class="Anthozoa" family="Caryophylliidae" genus="Frescocyathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="53" pageNumber="54" phylum="Cnidaria" rank="species" species="nagagreboensis" status="n. sp.">
