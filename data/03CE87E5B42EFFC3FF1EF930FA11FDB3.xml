<heading id="D090819FB42EFFC4FF1EF930FCF4F969" bold="true" box="[151,892,1667,1693]" fontSize="11" level="1" pageId="3" pageNumber="269" reason="1">
<emphasis id="B913EAE1B42EFFC4FF1EF930FCF4F969" bold="true" box="[151,892,1667,1693]" pageId="3" pageNumber="269">
Key to 
<taxonomicName id="4C674D70B42EFFC4FF79F930FED2F969" authorityName="McLachlan" authorityYear="1866" box="[240,346,1667,1693]" class="Insecta" family="Ithonidae" genus="Rapisma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="269" phylum="Arthropoda" rank="genus">
