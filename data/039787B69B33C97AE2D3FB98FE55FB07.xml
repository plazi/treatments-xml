<taxonomicName id="4C3E4D239B33C97AE2BCFB98FEFBFBB3" authorityName="PUTHZ" authorityYear="1965" box="[221,334,1103,1125]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="823" phylum="Arthropoda" rank="species" species="fulgidus">
