<quantity id="2A271FE1991DFF9AEDC39FECDA63FB6B" box="[1246,1282,1217,1246]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="46" pageNumber="47" unit="in" value="1.0">1 in</quantity>
