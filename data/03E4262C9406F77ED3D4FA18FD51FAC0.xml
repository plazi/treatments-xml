<taxonomicName id="4C4DECB99407F77ED15FFB01FC99FB18" authority="Liu, 1940" authorityName="Liu" authorityYear="1940" box="[540,888,1199,1224]" class="Insecta" family="Cicadidae" genus="Dundubia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="116" phylum="Arthropoda" rank="species" species="bifasciata">
