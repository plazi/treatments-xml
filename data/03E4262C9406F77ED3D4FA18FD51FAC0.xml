<taxonomicName id="4C4DECB99407F77ED7C9FB1FFF39FB3C" authority="Distant, 1901" authorityName="Distant" authorityYear="1901" class="Insecta" family="Cicadidae" genus="Dundubia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="116" phylum="Arthropoda" rank="species" species="hainanensis">
