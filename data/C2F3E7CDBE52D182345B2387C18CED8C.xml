<paragraph id="D16D10CDC938523E04C846C847CF3004" pageId="20" pageNumber="77">
<taxonomicName id="3003701890B06066BA505B7F6E2B8125" class="Insecta" family="Noctuidae" genus="Trachea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trachea perfumosa" order="Lepidoptera" pageId="20" pageNumber="77" phylum="Arthropoda" rank="species" species="perfumosa">
