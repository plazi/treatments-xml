<number id="3DFC24E0FFA2AE15FA1213A0A2B907D2" box="[1414,1426,152,176]" pageId="7" pageNumber="351" value="3.0">3</number>
/ 
<date id="FF8A1007FFA2AE15FF031387A79207B5" box="[151,185,191,215]" pageId="7" pageNumber="351">
<number id="3DFC24E0FFA2AE15FF031387A78E07B5" box="[151,165,191,215]" pageId="7" pageNumber="351" value="4.0">4</number>
