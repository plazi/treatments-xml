<taxonomicName id="77FB1FBBFFC33B27FEE1F896B3B6A0B5" authority="Tan, Japir &amp; Chung" authorityName="Tan, Japir &amp; Chung" authorityYear="2021" box="[280,750,1918,1942]" class="Insecta" family="Mogoplistidae" genus="Cycloptiloides" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="413" phylum="Arthropoda" rank="species" species="bimaculata" status="sp. nov.">
