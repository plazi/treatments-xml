<heading id="F67C81EF310AFFDEA49CF99F5B3BBD4F" bold="true" box="[151,563,1576,1602]" fontSize="11" level="1" pageId="15" pageNumber="48" reason="1">
<taxonomicName id="6A8B4D00310AFFDEA49CF99F5B3BBD4F" authority="Bocourt 1870" authorityName="Bocourt" authorityYear="1870" box="[151,563,1576,1602]" class="Reptilia" family="Phrynosomatidae" genus="Phrynosoma" kingdom="Animalia" order="Squamata" pageId="15" pageNumber="48" phylum="Chordata" rank="species" species="taurus">
