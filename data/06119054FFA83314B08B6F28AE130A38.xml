<taxonomicName id="49B85AC1FFA83314B0C26C73A85E0A38" authority="Nitzsch" authorityName="Nitzsch" box="[222,526,576,602]" class="Aves" family="Laricolidae" genus="Laricola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Charadriiformes" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="minutus">
