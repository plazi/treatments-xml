<collectingCountry id="EEBD7674FFECFFC64558FD56919F77EC" box="[359,426,727,753]" name="China" pageId="6" pageNumber="81">China</collectingCountry>
(
<collectingRegion id="546EF806FFECFFC64586FD56922A77EF" box="[441,543,727,754]" country="China" name="Guizhou" pageId="6" pageNumber="81">Guizhou</collectingRegion>
