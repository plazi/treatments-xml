[[ 
<taxonomicName id="2950B5925572C42FE500C1E847F3DF29" LSID-ZBK="urn:lsid:zoobank.org:act:7520B3CD-59C7-4425-A30E-1336A0B2AADD" class="Actinopterygii" family="Loricariidae" genus="Hypostomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Hypostomus cochliodon Kner 1854:265 [15]" order="Siluriformes" pageNumber="4" phylum="Chordata" rank="species" species="cochliodon">Hypostomus cochliodon</taxonomicName>
