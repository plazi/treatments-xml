<taxonomicName id="8C1E8E1BC4E796947908FB02D7970E39" LSID="urn:lsid:zoobank.org:act:CEC41DF7-A6BB-4871-9D06-FABC3DDAC219" authority="Keifer, 1939" authorityName="Keifer" authorityYear="1939" class="Arachnida" family="Eriophyidae" genus="Gammaphytoptus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gammaphytoptus schimae" order="Trombidiformes" pageId="5" pageNumber="14" phylum="Arthropoda" rank="species" species="schimae">Gammaphytoptus schimae</taxonomicName>
