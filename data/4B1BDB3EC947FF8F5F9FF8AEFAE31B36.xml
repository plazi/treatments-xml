: 
<specimenCode id="9314C253C947FF8C5E1DF85FFE5A1D85" box="[277,435,1951,1975]" collectionCode="FMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="37" pageNumber="38" type="Museum">FMNH 59146</specimenCode>
