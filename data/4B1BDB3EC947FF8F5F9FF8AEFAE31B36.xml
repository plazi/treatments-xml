<location id="C66D3CF3C947FF8C5DFFF860FCD41D85" LSID="urn:lsid:plazi:treatment:4B1BDB3EC947FF8F5F9FF8AEFAE31B36:C66D3CF3C947FF8C5DFFF860FCD41D85" box="[759,829,1951,1975]" name="Anpin" pageId="37" pageNumber="38" stateProvince="Tainan">Anpin</location>
