<taxonomicName id="C4B94D63FFE5FFE0F715862607395740" box="[287,535,1874,1896]" class="Demospongiae" family="Thorectidae" genus="Scalarispongia" kingdom="Animalia" order="Dictyoceratida" pageId="5" pageNumber="163" phylum="Porifera" rank="species" species="tubulata" status="sp. nov.">
<emphasis id="31CDEAF2FFE5FFE0F715862607395740" box="[287,535,1874,1896]" italics="true" pageId="5" pageNumber="163">Scalarispongia tubulata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="2AFE5789FFE5FFE0F42A8626075D574F" box="[544,627,1874,1895]" pageId="5" pageNumber="163" rank="species">
<emphasis id="31CDEAF2FFE5FFE0F42A8626075D574F" bold="true" box="[544,627,1874,1895]" pageId="5" pageNumber="163">sp. nov.</emphasis>
