<subSubSection id="2C91DA8AFFABFFD7FF72FB26FA0BFB33" box="[199,1426,1167,1194]" pageId="37" pageNumber="38" type="etymology">
<paragraph id="64348901FFABFFD7FF72FB26FA0BFB33" blockId="37.[151,1436,302,1464]" box="[199,1426,1167,1194]" pageId="37" pageNumber="38">
<emphasis id="56FF5513FFABFFD7FF72FB26FECAFB30" bold="true" box="[199,339,1167,1193]" pageId="37" pageNumber="38">Etymology.</emphasis>
