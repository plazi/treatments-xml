<materialsCitation id="3EF73314FFA6726CFF7451BEFACE80FF" ID-GBIF-Occurrence="1913870701" box="[151,1347,1461,1486]" location="Guangdong" pageId="23" pageNumber="24" specimenCount="1" stateProvince="Guangdong" typeStatus="holotype">
<emphasis id="BCEBE55BFFA6726CFF7451BEFEA280FF" bold="true" box="[151,303,1461,1486]" pageId="23" pageNumber="24">
<typeStatus id="512487EBFFA6726CFF7451BEFF5F80FF" box="[151,210,1461,1486]" pageId="23" pageNumber="24">Type</typeStatus>
locality
</emphasis>
: 
<collectingRegion id="4C5BF7ABFFA6726CFEDD51BEFE4980FF" box="[318,452,1461,1486]" country="China" name="Guangdong" pageId="23" pageNumber="24">Guangdong</collectingRegion>
