<taxonomicName id="4C63C2D3FFCE88230C491D39D53B7035" authority="An &amp; Yang, 2009" authorityName="An &amp; Yang" authorityYear="2009" box="[316,531,1625,1646]" class="Insecta" family="Chloropidae" genus="Platycephala" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="405" phylum="Arthropoda" rank="species" species="lateralis" status="sp. nov.">
<emphasis id="B9176542FFCE88230C491D39D53B7035" box="[316,531,1625,1646]" italics="true" pageId="7" pageNumber="405">Platycephala lateralis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A224D839FFCE88230F6C1D39D5427034" box="[537,618,1625,1647]" pageId="7" pageNumber="405" rank="species">sp. nov.</taxonomicNameLabel>
