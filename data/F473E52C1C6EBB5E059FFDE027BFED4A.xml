<subSubSection id="34C007B11C6EBB5E059FFDE0252FE819" box="[151,362,547,574]" pageId="40" pageNumber="41" type="nomenclature">
<paragraph id="7C65543A1C6EBB5E059FFDE0252FE819" blockId="40.[151,362,547,574]" box="[151,362,547,574]" pageId="40" pageNumber="41">
<heading id="272DE3561C6EBB5E059FFDE0252FE819" bold="true" box="[151,362,547,574]" fontSize="11" level="2" pageId="40" pageNumber="41" reason="4">
<emphasis id="4EAE88281C6EBB5E059FFDE0252FE819" bold="true" box="[151,362,547,574]" pageId="40" pageNumber="41">
<taxonomicName id="BBDA2FB91C6EBB5E059FFDE02554E81A" authority="Kim &amp; Sikorski &amp; O’Reilly &amp; Boxshall, 2013" authorityName="Kim &amp; Sikorski &amp; O’Reilly &amp; Boxshall" authorityYear="2013" box="[151,273,548,574]" class="Hexanauplia" family="Clausiidae" genus="Vivgottoia" kingdom="Animalia" order="Cyclopoida" pageId="40" pageNumber="41" phylum="Arthropoda" rank="genus" status="n. gen.">
