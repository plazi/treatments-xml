<taxonomicName id="301F20F025C00B69E5645A31FF636EEC" class="Malacostraca" family="Maeridae" genus="Parelasmopus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parelasmopus dancaui" order="Amphipoda" pageId="0" pageNumber="59" phylum="Arthropoda" rank="species" species="dancaui">Parelasmopus dancaui</taxonomicName>
