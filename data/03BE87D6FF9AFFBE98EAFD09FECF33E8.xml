<taxonomicName id="4C174D43FF9AFFBE98EAFD09F8DA3310" authority="(Tilesius, 1811)" baseAuthorityName="Tilesius" baseAuthorityYear="1811" box="[151,667,763,790]" class="Cephalaspidomorphi" family="Petromyzontidae" genus="Lethenteron" kingdom="Animalia" order="Petromyzontiformes" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="camtschaticum">
<emphasis id="B963EAD2FF9AFFBE98EAFD09FB9D3313" bold="true" box="[151,476,763,790]" italics="true" pageId="11" pageNumber="12">Lethenteron camtschaticum</emphasis>
