<location id="34064798FFC3FFE6FE1923C8FE4CFB4A" LSID="urn:lsid:plazi:treatment:B970A055FFC3FFE1FEA52362FCF4FAC8:34064798FFC3FFE6FE1923C8FE4CFB4A" box="[386,501,1182,1201]" country="Russia" name="Chita Region" pageId="3" pageNumber="92">Chita Region</location>
