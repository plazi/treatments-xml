<typeStatus id="344F882BFFEF9533A7FAFC5FB989BF28" box="[958,1013,1012,1038]" pageId="17" pageNumber="36">Type</typeStatus>
species: 
<taxonomicName id="2CF44D0AFFEF9533A4FDFBBCB8D8BF17" authority="Cameron, 1904" authorityName="Cameron" authorityYear="1904" box="[185,676,1047,1073]" class="Insecta" family="Hymenoptera (awaiting allocation)" genus="Xanthomicrodus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="36" phylum="Arthropoda" rank="species" species="iridipennis">
<emphasis id="D980EA9BFFEF9533A4FDFBBCBB9ABF17" box="[185,486,1047,1073]" italics="true" pageId="17" pageNumber="36">Xanthomicrodus iridipennis</emphasis>
Cameron, 1904
