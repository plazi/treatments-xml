<subSubSection id="C34D653EFFB5FFBAFF47319CFC0AE02E" box="[151,949,501,524]" pageId="39" pageNumber="40" type="materials_examined">
<paragraph id="8BE836B5FFB5FFBAFF47319CFC0AE02E" blockId="39.[151,949,469,524]" box="[151,949,501,524]" pageId="39" pageNumber="40">
<typeStatus id="54EC8817FFB5FFBAFF47319CFF73E02E" box="[151,204,501,524]" pageId="39" pageNumber="40">Type</typeStatus>
species: 
<taxonomicName id="4C574D36FFB5FFBAFEFD319CFD11E02E" authority="Wocke, 1862" authorityName="Wocke" authorityYear="1862" box="[301,686,501,524]" class="Insecta" family="Gelechiidae" genus="Gelechia" kingdom="Animalia" order="Lepidoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="perspersella">
