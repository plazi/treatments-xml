<taxonomicName id="07DF4D50FFD6FFA5FBD7FC88FAF06A07" authority="Kieffer. In" authorityName="Kieffer. In" authorityYear="1919" box="[1093,1397,855,881]" class="Insecta" family="Chironomidae" genus="Stictochironomus" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="51" phylum="Arthropoda" rank="genus">
