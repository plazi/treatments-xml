<taxonomicName id="4C7C4D27FFA21B412A0C4176FC7FF8D2" authority="Atyeo, 1966" authorityName="Atyeo" authorityYear="1966" box="[587,977,1822,1850]" class="Arachnida" family="Proctophyllodidae" genus="Nycteridocaulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="2" pageNumber="879" phylum="Arthropoda" rank="genus">
