<taxonomicName id="4C3B4D67FFE9FFB8FA5722CDCA5AD18B" authorityName="Ament &amp; Amorim" authorityYear="2013" class="Insecta" family="Phoridae" genus="Neopleurophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="brachytarsus">
