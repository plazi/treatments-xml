, 
<taxonomicName id="4C4179E7FFFE355E12B1FCFA62069122" box="[668,803,836,860]" class="Arachnida" family="Laelapidae" genus="Haemolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="289" phylum="Arthropoda" rank="species" species="domrowi">
