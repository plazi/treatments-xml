and 
<taxonomicName id="4C4179E7FFFE355E143BFCFA65959122" box="[1046,1200,836,860]" class="Arachnida" family="Laelapidae" genus="Haemolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="289" phylum="Arthropoda" rank="species" species="flagellatus">
