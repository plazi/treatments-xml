<treatmentCitationGroup id="AB58C02098240B21FF19F9DA4F931C79" box="[192,720,1570,1594]" pageId="31" pageNumber="30">
<taxonomicName id="4C489C8D98240B21FF19F9DA4C9D1C79" authority="(Say)" baseAuthorityName="Say" baseAuthorityYear="1823" box="[192,478,1570,1594]" class="Insecta" family="Heteroceridae" genus="Tropicus" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="30" phylum="Arthropoda" rank="species" species="pusillus">
