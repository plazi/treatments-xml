<taxonomicName id="6183B088FF89E016FD27640EFBC3FA82" box="[762,1034,1324,1349]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="illiterata">
