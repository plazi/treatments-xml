<taxonomicName id="6183B088FF89E016FDA36486FCC5FA7A" box="[638,780,1444,1469]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="illiterata">
