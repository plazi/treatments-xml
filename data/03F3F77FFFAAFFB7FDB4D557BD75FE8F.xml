<taxonomicName id="4C5A3DEAFFAAFFB6FCB6D62CBC8BF9D7" baseAuthorityName="Temminck" baseAuthorityYear="1840" box="[823,960,1629,1653]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="52" pageNumber="53" phylum="Chordata" rank="species" species="horsfieldii">
