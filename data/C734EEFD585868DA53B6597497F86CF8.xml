<taxonomicName id="7FE7CAD5E5A6A53C8F23C834B75FC2B3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:183208" authority="Wheeler, 1910" authorityName="Wheeler" authorityYear="1910" class="Insecta" family="Formicidae" genus="Nesomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Nesomyrmex denticulatus (Mayr)" order="Hymenoptera" pageNumber="39" phylum="Arthropoda" rank="species" species="denticulatus">N. denticulatus</taxonomicName>
