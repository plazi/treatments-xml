<taxonomicName id="D65BC513FD81C9EACBA6B53B4BC8CA14" LSID="825452" authority="Y. P. Tan &amp; R. G. Shivas" class="Dothideomycetes" family="Pleosporaceae" genus="Curvularia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Curvularia coatesiae" order="Pleosporales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="coatesiae">Curvularia coatesiae Y.P. Tan &amp; R.G. Shivas</taxonomicName>
<taxonomicNameLabel id="D16A4DE1EEDCEA6942A127348FE7EB91" pageId="0" pageNumber="1">sp. nov.</taxonomicNameLabel>
