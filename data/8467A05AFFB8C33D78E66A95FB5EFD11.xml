and NC00­ 
<number id="BA06036BFFB8C33E7D706B80FAAFFE6F" box="[1281,1294,386,412]" pageId="9" pageNumber="54" value="8.0">8</number>
, and 
<number id="BA06036BFFB8C33E7D3D6B80FAFAFE6F" box="[1356,1371,386,412]" pageId="9" pageNumber="54" value="1.0">1</number>
from NC00­ 
