<taxonomicName id="4C314D48E267FF94AD53ACB1CECDFFEA" box="[556,694,997,1020]" class="Malacostraca" family="Hyalidae" genus="Insula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="squamata" status="sp. nov.">
<emphasis id="B945EAD9E267FF94AD53ACB1CECDFFEA" box="[556,694,997,1020]" italics="true" pageId="13" pageNumber="14">I. squamata</emphasis>
</taxonomicName>
<emphasis id="B945EAD9E267FF94ADBDACB0CF5AFFEB" bold="true" box="[706,801,996,1021]" pageId="13" pageNumber="14">
<taxonomicNameLabel id="A27657A2E267FF94ADBDACB0CF5AFFEB" box="[706,801,996,1021]" pageId="13" pageNumber="14" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
