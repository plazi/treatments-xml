<number id="3DF924ECE267FF94ACEBAB5CCF81F837" box="[916,1018,1032,1057]" pageId="13" pageNumber="14" value="163165.0">163, 165</number>
</figureCitation>
; 
<number id="3DF924ECE267FF94AB79AB5DC817F837" box="[1030,1132,1032,1057]" pageId="13" pageNumber="14" value="223226.0">223, 226</number>
