<taxonomicName id="0CDC4D57FFF46531FE23EE70D656122F" authority="Pfr. N." authorityName="Pfr. N." box="[438,636,1582,1609]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="48" pageNumber="49" phylum="Mollusca" rank="species" species="lambda">H. lambda Pfr. N.</taxonomicName>
