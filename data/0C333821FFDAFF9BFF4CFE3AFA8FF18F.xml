<taxonomicName id="439AF2B4FFDAFF9AFEC2F926FD2BF6C0" authority="Lucanas and Lit" authorityName="Lucañas &amp; Lit" authorityYear="2018" box="[281,688,1700,1722]" class="Insecta" family="Oligotomidae" genus="Aposthonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Embioptera" pageId="4" pageNumber="177" phylum="Arthropoda" rank="species" species="merdelynae" status="n. sp.">
<emphasis id="B6EE5525FFDAFF9AFEC2F926FD9CF6C0" box="[281,519,1700,1722]" italics="true" pageId="4" pageNumber="177">Aposthonia merdelynae</emphasis>
Lucañas and Lit
</taxonomicName>
<emphasis id="B6EE5525FFDAFF9AFD6DF926FD6BF6C3" bold="true" box="[694,752,1700,1721]" pageId="4" pageNumber="177">
<taxonomicNameLabel id="ADDDE85EFFDAFF9AFD6DF926FD6BF6C3" box="[694,752,1700,1721]" pageId="4" pageNumber="177" rank="species">n. sp.</taxonomicNameLabel>
