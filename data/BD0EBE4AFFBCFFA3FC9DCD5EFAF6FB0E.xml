<taxonomicName id="F2A774DFFFBCFFA3FC23CADCFBDAFB72" box="[1022,1113,1164,1185]" class="Aves" family="Turdidae" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="298" phylum="Chordata" rank="family">Turdidae</taxonomicName>
