160, 
<emphasis id="B60402BB5173E83E4BC2CB9D2409F452" italics="true" pageId="0" pageNumber="177">ex</emphasis>
10; notogastral setae range 150-190; 
<emphasis id="E92EC77F37EF7EF0E693894ACE2DC788" italics="true" pageId="0" pageNumber="177">c</emphasis>
- 
<emphasis id="1416A3AEBB2A6C6D50CE0A55DD9C35B5" italics="true" pageId="0" pageNumber="177">c</emphasis>
170, 
<emphasis id="D8BA011EBFDE03340174F415DBF00AE9" italics="true" pageId="0" pageNumber="177">la</emphasis>
- 
<emphasis id="1D8966428ED0089C8918190D6159C824" italics="true" pageId="0" pageNumber="177">la</emphasis>
220, 
<emphasis id="FDDE4A347C1B7E46E3A2694334540E9B" italics="true" pageId="0" pageNumber="177">lm</emphasis>
- 
<emphasis id="72157FCCFB55BB7FD4DB1EE2328FD630" italics="true" pageId="0" pageNumber="177">lm</emphasis>
180, 
<emphasis id="B04871909E5D14FC1EFDE4AD4C04EA3D" italics="true" pageId="0" pageNumber="177">lp</emphasis>
- 
<emphasis id="A4DF09ADA4E1F4A1649528EC4567BEED" italics="true" pageId="0" pageNumber="177">lp</emphasis>
230, 
<emphasis id="C890AA40B2F60E2F9164ACD8AB64F0B4" italics="true" pageId="0" pageNumber="177">h</emphasis>
2- 
