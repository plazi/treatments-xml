<taxonomicName id="4C6C4D5FFF9FB9132B414918FC98F9E3" authorityName="Horstmann" authorityYear="1981" box="[628,786,1550,1574]" class="Insecta" family="Ichneumonidae" genus="Barycnemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="165" phylum="Arthropoda" rank="species" species="punctifrons">
