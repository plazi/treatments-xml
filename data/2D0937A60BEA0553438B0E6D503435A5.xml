<paragraph id="EBF74D1FCD81719981B1C94E90247C41" pageId="3" pageNumber="38">
<taxonomicName id="F4AEF61C374C1E110EB369CD1A8C9F88" class="Arachnida" family="Tetrablemmidae" genus="Singaporemma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Singaporemma singularis" order="Araneae" pageId="3" pageNumber="38" phylum="Arthropoda" rank="species" species="singularis">Singaporemma singularis</taxonomicName>
