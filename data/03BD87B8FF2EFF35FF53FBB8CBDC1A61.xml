<taxonomicName id="4C144D2DFF2EFF35FC6AFAE0CA021A4D" box="[942,1090,1340,1364]" class="Insecta" family="Carabidae" genus="Amara" kingdom="Animalia" order="Coleoptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="communis">
