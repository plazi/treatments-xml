<number id="3DFC24E0FFA7AE10FDCB113BA5B20579" box="[607,665,515,539]" pageId="2" pageNumber="346" value="1941.0">1941</number>
)
</bibRefCitation>
as 
<taxonomicName id="4C344D44FFA7AE10FD53113CA4AC0579" authority="Pettey, 1918" authorityName="Pettey" authorityYear="1918" box="[711,903,514,539]" class="Insecta" family="Sciaridae" genus="Neosciara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="346" phylum="Arthropoda" rank="species" species="lignea">
