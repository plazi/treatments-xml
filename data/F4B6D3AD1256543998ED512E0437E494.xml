<taxonomicName id="4CB4CB71FB394263C48ADEF4B8C5BBB4" baseAuthorityName="Burnup" baseAuthorityYear="1914" class="Gastropoda" family="Streptaxidae" genus="Gulella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gulella mooiensis" order="Stylommatophora" pageId="0" pageNumber="107" phylum="Mollusca" rank="species" species="mooiensis">
