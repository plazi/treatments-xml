</taxonomicName>
<number id="3DCD8B57FFC26359FE2A9904833AFEE3" box="[441,542,260,284]" pageId="10" pageNumber="543" value="210.5" valueMax="224.0" valueMin="197.0">197–224</number>
, 
<taxonomicName id="4C05E2F3FFC26359FDBE99058391FEE2" box="[557,693,260,285]" class="Arachnida" family="Oppiidae" genus="Brachioppiella" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="543" phylum="Arthropoda" rank="species" species="biseriata">
<emphasis id="B9714562FFC26359FDBE99058391FEE2" box="[557,693,260,285]" italics="true" pageId="10" pageNumber="543">B. biseriata</emphasis>
</taxonomicName>
<number id="3DCD8B57FFC26359FD5399048201FEE2" box="[704,805,260,285]" pageId="10" pageNumber="543" value="432.5" valueMax="440.0" valueMin="425.0">425–440</number>
, 
<taxonomicName id="4C05E2F3FFC26359FCA799058296FEE2" box="[820,946,260,285]" class="Arachnida" family="Oppiidae" genus="Brachioppiella" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="543" phylum="Arthropoda" rank="species" species="higginsi">
