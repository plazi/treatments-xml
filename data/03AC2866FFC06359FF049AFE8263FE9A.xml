, 
<taxonomicName id="4C05E2F3FFC26359FE7A994D8346FE9A" box="[489,610,332,357]" class="Arachnida" family="Oppiidae" genus="Brachioppiella" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="543" phylum="Arthropoda" rank="species" species="higginsi">
