) than 
<emphasis id="B9714562FFC0635BFBB59DB58564FA32" box="[1062,1088,1461,1485]" italics="true" pageId="8" pageNumber="541">
<number id="3DCD8B57FFC0635BFBB59DB58516FA32" box="[1062,1074,1461,1485]" pageId="8" pageNumber="541" value="1.0">1</number>
c
</emphasis>
, 
<emphasis id="B9714562FFC0635BFBC29DB5854FFA32" box="[1105,1131,1461,1485]" italics="true" pageId="8" pageNumber="541">
<number id="3DCD8B57FFC0635BFBC29DB5857BFA32" box="[1105,1119,1461,1485]" pageId="8" pageNumber="541" value="3.0">3</number>
c
</emphasis>
, 
