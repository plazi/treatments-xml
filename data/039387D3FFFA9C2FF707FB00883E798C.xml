<emphasis id="B94EEAD7FFFA9C29F4B6FA8F88E37D11" bold="true" box="[697,929,1329,1355]" pageId="2" pageNumber="43">
<collectingCountry id="F32D7655FFFA9C29F4B6FA8F885D7D11" box="[697,799,1329,1355]" name="China" pageId="2" pageNumber="43">CHINA</collectingCountry>
