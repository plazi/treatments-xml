<typeStatus id="7563883FDD74FF8C4BD5FC15FE97FC25" box="[151,258,989,1015]" pageId="26" pageNumber="51">Paratype</typeStatus>
: possibly 
<collectionCode id="CCC9AE58DD74FF8C4A3BFC15FE44FC25" LSID="urn:lsid:biocol.org:col:34992" box="[377,465,989,1015]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="26" pageNumber="51">RMNH</collectionCode>
