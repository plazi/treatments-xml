[[ 
<taxonomicName id="35DA19B597B1AC92722473716DB1F576" LSID-ZBK="urn:lsid:zoobank.org:act:BED2BB84-3FBC-4BDC-9032-0D537AD3F726" class="Actinopterygii" family="Erethistidae" genus="Pseudolaguvia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pseudolaguvia Misra 1976:253" order="Siluriformes" pageNumber="59" phylum="Chordata" rank="genus">Pseudolaguvia</taxonomicName>
