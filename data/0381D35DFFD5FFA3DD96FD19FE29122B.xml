</taxonomicName>
(
<taxonomicName id="4C2819C8FFD5FFA3DFE4FB4AFBC310E8" box="[966,1084,1239,1263]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="90" pageNumber="1976" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
