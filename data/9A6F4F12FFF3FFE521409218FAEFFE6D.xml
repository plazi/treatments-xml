<taxonomicName id="D5C68587FFF3FFE721DC95E7FD84F926" box="[404,617,1763,1786]" class="Diplopoda" family="Dalodesmidae" genus="Bromodesmus" kingdom="Animalia" order="Polydesmida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="rufus" status="n. sp.">
