<taxonomicName id="D5C68587FFF1FFE522429293FDABFE6C" box="[522,582,407,432]" class="Diplopoda" family="Dalodesmidae" genus="Bromodesmus" kingdom="Animalia" order="Polydesmida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="rufus">
