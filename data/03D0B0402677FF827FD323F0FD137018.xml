<taxonomicName id="4C797AD52675FF827BEF25C9FEBF715C" authority="Linnaeus" authorityName="Linnaeus" class="Aves" family="Phalacrocoracidae" genus="Phalacrocorax" kingdom="Animalia" order="Suliformes" pageId="3" pageNumber="66" phylum="Chordata" rank="species" species="carbo">
