<taxonomicName id="4C797AD52675FF827E982235FD04715C" box="[476,683,1043,1067]" class="Aves" family="Phalacrocoracidae" kingdom="Animalia" order="Suliformes" pageId="3" pageNumber="66" phylum="Chordata" rank="family">Phalacrocoracidae</taxonomicName>
