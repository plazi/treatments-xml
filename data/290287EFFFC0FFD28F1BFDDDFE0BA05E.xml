<taxonomicName id="66AB4D7AFFC1FFD18C74FDB7FF39A111" box="[95,202,536,559]" class="Reptilia" family="Polychrotidae" genus="Lophyrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="37" pageNumber="38" phylum="Chordata" rank="genus">
