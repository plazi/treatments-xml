<taxonomicName id="4C5F4D5FFFCA7F20FD88DA25FCF4F290" authorityName="Henry &amp; Menard" authorityYear="2020" baseAuthorityName="Henry &amp; Menard" baseAuthorityYear="2020" box="[579,891,1904,1927]" class="Insecta" family="Miridae" genus="Laterospinocoris" kingdom="Animalia" order="Hemiptera" pageId="47" pageNumber="248" phylum="Arthropoda" rank="species" species="cyaneipennis">
