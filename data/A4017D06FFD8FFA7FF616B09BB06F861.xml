<quantity id="EB5061F5FFDAFFA7FE2E689FBA27FB55" box="[472,638,1232,1259]" metricMagnitude="-4" metricUnit="m" metricValue="3.65" metricValueMax="3.8" metricValueMin="3.5" pageId="14" pageNumber="15" unit="mm" value="0.365" valueMax="0.38" valueMin="0.35">0.35–0.38 mm</quantity>
