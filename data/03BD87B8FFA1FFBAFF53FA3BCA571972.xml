<taxonomicName id="4C144D2DFFA1FFBAFCB1F9F3CDAD195F" box="[885,1005,1583,1606]" class="Insecta" family="Carabidae" genus="Procrustes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
