<taxonomicName id="75B54D4C971DFFDEFF1F2D67B8EFA41A" authorityName="Carl" authorityYear="1932" box="[151,395,558,582]" class="Diplopoda" family="Paradoxosomatidae" genus="Polydrepanum" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="493" phylum="Arthropoda" rank="species" species="tamulum">
