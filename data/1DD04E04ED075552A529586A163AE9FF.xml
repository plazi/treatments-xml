<taxonomicName id="3FD8F7F69C3440F503D5F1071C5421E7" class="Gastropoda" family="Helicidae" genus="Cantareus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cantareus subapertus" order="Stylommatophora" pageId="11" pageNumber="12" phylum="Mollusca" rank="species" species="subapertus">
