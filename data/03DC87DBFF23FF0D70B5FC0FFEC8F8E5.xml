<taxonomicName id="4C754D4EFF23FF10730CF9FFFC1CFC18" box="[814,957,1638,1662]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="175" pageNumber="176" phylum="Nematoda" rank="genus">
