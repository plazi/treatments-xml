<taxonomicName id="B62D08F76C8E8E7945B6212B6279A9C2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:224595" class="Insecta" family="Formicidae" genus="Azteca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Azteca alfaroi r. lucidula Forel" lsidName-HNS="Azteca alfaroi r. lucidula Forel" order="Hymenoptera" pageNumber="113" phylum="Arthropoda" rank="species" species="lucidula">lucidula</taxonomicName>
