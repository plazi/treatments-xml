<date id="373E106B7069731A3597200BFB7D0E5C" box="[1159,1269,1792,1818]" pageId="10" pageNumber="39" value="2001-01-11">
<number id="F548248C7069731A3597200BFB2B0E5C" box="[1159,1187,1792,1818]" pageId="10" pageNumber="39" value="11.0">11</number>
