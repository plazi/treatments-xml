<taxonomicName id="22EEA58E4DC3E5E39AB55ECC926DA994" authorityName="Sosef &amp; Dauby" authorityYear="2012" baseAuthorityName="Sosef &amp; Dauby" baseAuthorityYear="2012" class="Dicotyledoneae" family="Clusiaceae" genus="Garcinia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Guttiferales" pageId="2" pageNumber="43" phylum="Angiospermae" rank="species" species="echirensis" status="comb. nov.">Garcinia echirensis</taxonomicName>
