<collectingCountry id="F37FA1A8A85F682CF99077E1F91AF949" box="[1615,1692,1669,1690]" name="Panama" pageId="2" pageNumber="427">Panama</collectingCountry>
</vernacularName>
/ 
<emphasis id="B91C3D2AA85F682CF96D77E1F88BF949" box="[1714,1805,1669,1690]" pageId="2" pageNumber="427">German:</emphasis>
<vernacularName id="056B9116A85F682CF8C877E1F84BF949" box="[1815,1997,1669,1690]" pageId="2" pageNumber="427">Panama-Nachtaffe</vernacularName>
