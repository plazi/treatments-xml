<taxonomicName id="1ED44D4FCF47AE1EFEC7F8E99A6CF8F5" box="[357,626,1816,1839]" class="Insecta" family="Cicadellidae" genus="Exitianus" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="apophysisiosus" status="sp. nov.">
<emphasis id="EBA0EADECF47AE1EFEC7F8E99A6CF8F5" box="[357,626,1816,1839]" italics="true" pageId="20" pageNumber="21">
Exitianus 
<taxonomicName id="1ED44D4FCF47AE1EFE73F8E99A6CF8F5" box="[465,626,1816,1839]" pageId="20" pageNumber="21" rank="species" species="apophysisiosus" status="sp. nov.">apophysisiosus</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="EBA0EADECF47AE1EFDDBF8E69AD1F8F5" bold="true" box="[633,719,1815,1839]" pageId="20" pageNumber="21">
<taxonomicNameLabel id="F09357A5CF47AE1EFDDBF8E69AD1F8F5" box="[633,719,1815,1839]" pageId="20" pageNumber="21" rank="species">sp. nov.</taxonomicNameLabel>
