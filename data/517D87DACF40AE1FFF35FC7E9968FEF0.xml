<caption id="8DAB6644CF47AE1EFF35F8E69C55F8AE" httpUri="https://zenodo.org/record/182743/files/figure.png" pageId="20" pageNumber="21" targetBox="[160,1430,637,1784]" targetPageId="20">
<paragraph id="D96B36CCCF47AE1EFF35F8E69C55F8AE" blockId="20.[151,1436,1815,1908]" pageId="20" pageNumber="21">
