<taxonomicName id="1CA940912F01AF6AFB00FF634713FF3A" box="[1214,1313,224,248]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="3" pageNumber="390" phylum="Cnidaria" rank="species" species="albida">
<emphasis id="E9DDE7002F01AF6AFB00FF634713FF3A" box="[1214,1313,224,248]" italics="true" pageId="3" pageNumber="390">S. albida</emphasis>
</taxonomicName>
.
