<taxonomicName id="6E483E4DDCB8162129E2F4DEDAF2F076" class="Liliopsida" family="Hydrocharitaceae" genus="Thalassia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Thalassia testudinum" order="Alismatales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="testudinum">Thalassia testudinum</taxonomicName>
