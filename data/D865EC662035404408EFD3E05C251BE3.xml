[round, blue-bordered label], 
<typeStatus id="8F77E3D2203640440D1CD152589A1A7A" box="[1380,1436,549,573]" pageId="10" pageNumber="311">Type</typeStatus>
[round, red-bordered label], V. de Chiriqui, 
<quantity id="9734F095203640440AF1D13F5FFF1A27" box="[649,761,583,608]" metricMagnitude="2" metricUnit="m" metricValue="4.575048" metricValueMax="9.144" metricValueMin="0.006096" pageId="10" pageNumber="311" unit="ft" value="1501.0" valueMax="3000.0" valueMin="2.0">
