<taxonomicName id="97CC26F3203640440B7ED130587E1A27" authority="Gorham, Champion, B. C. A. Col." authorityName="Gorham, Champion, B. C. A. Col." box="[774,1400,583,608]" class="Insecta" family="Cleridae" genus="Hydnocera" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="311" phylum="Arthropoda" rank="species" species="lateralis">
