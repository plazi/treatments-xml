<taxonomicName id="BBA34D3D7C47FF8C5E7DFD74FB5A8DD6" box="[892,1173,655,680]" class="Insecta" family="Psyllidae" genus="Euphalerus" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="39" phylum="Arthropoda" rank="species" species="fossiconis">
