<taxonomicName id="47C50F1E1171AB267F215A05ED47C549" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33833" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole littoralis Cole" lsidName-HNS="Pheidole littoralis Cole" order="Hymenoptera" pageNumber="589" phylum="Arthropoda" rank="species" species="littoralis">littoralis</taxonomicName>
