<taxonomicName id="57B1F4F7D8097CBE9878B915A90DF8B9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33670" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole clementensis Gregg" lsidName-HNS="Pheidole clementensis Gregg" order="Hymenoptera" pageNumber="589" phylum="Arthropoda" rank="species" species="clementensis">clementensis</taxonomicName>
