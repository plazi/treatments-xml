, 
<taxonomicName id="DC7548AF249DD7128E9AED012F1F614E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181973" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hoplitica Wilson" lsidName-HNS="Pheidole hoplitica Wilson" order="Hymenoptera" pageNumber="589" phylum="Arthropoda" rank="species" species="hoplitica">hoplitica</taxonomicName>
, 
<taxonomicName id="CFF53CACB6ADC9656E976BE04A6AE60A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33833" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole littoralis Cole" lsidName-HNS="Pheidole littoralis Cole" order="Hymenoptera" pageNumber="589" phylum="Arthropoda" rank="species" species="littoralis">littoralis</taxonomicName>
