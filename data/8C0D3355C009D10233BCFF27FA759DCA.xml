<taxonomicName id="C3A4F9C0C009D13C320EF904FE009D83" authority="Zanol &amp; Hutchings &amp; Fauchald, 2020" authorityName="Zanol &amp; Hutchings &amp; Fauchald" authorityYear="2020" box="[293,502,1722,1746]" class="Polychaeta" family="Eunicidae" genus="Leodice" kingdom="Animalia" order="Eunicida" pageId="31" pageNumber="32" phylum="Annelida" rank="species" species="jimedwardsi" status="n. sp.">
