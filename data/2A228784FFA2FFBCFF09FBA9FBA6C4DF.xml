<collectingCountry id="DA9C7602FFA0FFBCFEADFB9FFE03C777" box="[333,391,1513,1536]" name="Haiti" pageId="14" pageNumber="15">Haiti</collectingCountry>
and 
<collectingCountry id="DA9C7602FFA0FFBCFE31FB9FFF4CC76A" name="Dominican Republic" pageId="14" pageNumber="15">Dominican Republic</collectingCountry>
