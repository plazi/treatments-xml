<taxonomicName id="4C571393FFF53877FF0EC4E1FE08F900" baseAuthorityName="Lundbeck" baseAuthorityYear="1898" box="[151,460,1765,1791]" class="Insecta" family="Chironomidae" genus="Eukiefferiella" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="claripennis">
<emphasis id="B923B402FFF53877FF0EC4E1FE08F900" bold="true" box="[151,460,1765,1791]" italics="true" pageId="23" pageNumber="24">Eukiefferiella claripennis</emphasis>
