<taxonomicName id="4C37D68BFFA7FF92FB92B0C6FB173394" authorityName="M. Adanson" authorityYear="1763" box="[1127,1208,402,428]" class="Phaeophyceae" family="Dictyotaceae" genus="Padina" kingdom="Chromista" order="Dictyotales" pageId="6" pageNumber="107" phylum="Ochrophyta" rank="genus">
