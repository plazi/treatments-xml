<taxonomicName id="4C37D68BFFAFFF9AFF33B24BFD073101" authority=", T. Pennsylvanica" authorityName="T. Pennsylvanica" box="[198,680,799,825]" class="Demospongiae" family="Spongillidae" genus="Trochospongilla" kingdom="Animalia" order="Spongillida" pageId="14" pageNumber="115" phylum="Porifera" rank="species" species="horrida">
