(Lea, 1845) from 
<collectingCountry id="F320ED98FFAFFF9AFC58B5D3FBAB3699" box="[941,1028,1159,1185]" name="Canada" pageId="14" pageNumber="115">Canada</collectingCountry>
, 
<collectingCountry id="F320ED98FFAFFF9AFBE7B5D3FBC53699" box="[1042,1130,1159,1185]" name="Mexico" pageId="14" pageNumber="115">Mexico</collectingCountry>
