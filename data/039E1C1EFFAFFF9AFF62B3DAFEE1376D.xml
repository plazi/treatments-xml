<taxonomicName id="4C37D68BFFAFFF9AFB61B3AFFA3A312D" baseAuthorityName="Mills" baseAuthorityYear="1888" box="[1172,1429,763,789]" class="Demospongiae" family="Spongillidae" genus="Dosilia" kingdom="Animalia" order="Spongillida" pageId="14" pageNumber="115" phylum="Porifera" rank="species" species="radiospiculata">
