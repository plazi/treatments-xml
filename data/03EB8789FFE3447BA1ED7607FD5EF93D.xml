<taxonomicName id="4C424D1CFFE3447CA0AC722AFCD5F93A" authority="Zone, White Chalk, Lagerdorf, Schleswig-Holstein" authorityName="Zone, White Chalk, Lagerdorf, Schleswig-Holstein" box="[160,839,1645,1668]" class="Cephalopoda" family="Belemnitida (awaiting allocation)" genus="Goniotheutis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Belemnitida" pageId="11" pageNumber="1667" phylum="Mollusca" rank="species" species="quadrata">
