<taxonomicName id="4C424D1CFFE3447CA10B7217FE55F9D9" baseAuthorityName="Voigt" baseAuthorityYear="1949" box="[263,455,1616,1639]" class="Gymnolaemata" family="Onychocellidae" genus="Aechmellina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="11" pageNumber="1667" phylum="Bryozoa" rank="species" species="falcifera">
