<subSubSection id="C3146553CD4CFF98569AD2DEFE1ABE8E" box="[151,488,949,975]" pageId="4" pageNumber="28" type="nomenclature">
<paragraph id="8BB136D8CD4CFF98569AD2DEFE1ABE8E" blockId="4.[151,488,949,975]" box="[151,488,949,975]" pageId="4" pageNumber="28">
<heading id="D0F981B4CD4CFF98569AD2DEFE1ABE8E" bold="true" box="[151,488,949,975]" fontSize="11" level="1" pageId="4" pageNumber="28" reason="1">
<emphasis id="B97AEACACD4CFF98569AD2DEFE1ABE8E" bold="true" box="[151,488,949,975]" pageId="4" pageNumber="28">
Genus 
<taxonomicName id="4C0E4D5BCD4CFF9856E3D2DEFE62BE8E" box="[238,400,949,975]" class="Malacostraca" family="Kalliapseudidae" genus="Postispinatus" kingdom="Animalia" order="Tanaidacea" pageId="4" pageNumber="28" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="B97AEACACD4CFF9856E3D2DEFE62BE8E" bold="true" box="[238,400,949,975]" italics="true" pageId="4" pageNumber="28">Postispinatus</emphasis>
</taxonomicName>
