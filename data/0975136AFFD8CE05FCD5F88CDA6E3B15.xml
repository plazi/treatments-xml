<taxonomicName id="46DCD9FFFFDBCE0BFF56F81ADA783BCA" baseAuthorityName="Doleschall" baseAuthorityYear="1871" box="[169,299,1995,2021]" class="Arachnida" family="Actinopodidae" genus="Actinopus" kingdom="Animalia" order="Araneae" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="nattereri">
