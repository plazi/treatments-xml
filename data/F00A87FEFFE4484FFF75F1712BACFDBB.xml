<quantity id="BF5B9B0DFFE4484EFCBDF1CD29B3FA47" box="[890,949,1411,1431]" metricMagnitude="4" metricUnit="m" metricValue="2.1" pageId="4" pageNumber="769" unit="km" value="21.0">21 km</quantity>
