<quantity id="BF5B9B0DFFE4484EFF75F1D42AEEFA7E" box="[178,232,1434,1454]" metricMagnitude="2" metricUnit="m" metricValue="1.5" pageId="4" pageNumber="769" unit="m" value="150.0">150m</quantity>
