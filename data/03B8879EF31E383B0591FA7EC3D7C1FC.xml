<taxonomicName id="4C114D0BF31E383B0468FA7EC0DEC2BD" authority="Blumenbach, 1779" authorityName="Blumenbach" authorityYear="1779" box="[293,687,1491,1517]" class="Mammalia" kingdom="Animalia" order="Chiroptera" pageId="14" pageNumber="215" phylum="Chordata" rank="order">CHIROPTERA Blumenbach, 1779</taxonomicName>
