<taxonomicName id="C3C0D7654CD8022B3BD04B3077D9E201" authority="Sasaji, 1967" authorityName="Sasaji" authorityYear="1967" class="Insecta" family="Coccinellidae" genus="Shirozuella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Shirozuella nibagou" order="Coleoptera" pageId="7" pageNumber="94" phylum="Arthropoda" rank="species" species="nibagou">Shirozuella nibagou</taxonomicName>
