<collectingCountry id="F34A7652FF821A7AFABDFF38B899FF7D" box="[1326,1432,152,176]" name="Germany" pageId="5" pageNumber="356">Germany</collectingCountry>
, 
<collectingCountry id="F34A7652FF821A7AFF04FF1DBDFBFF18" box="[151,250,189,213]" name="Hungary" pageId="5" pageNumber="356">Hungary</collectingCountry>
, Italian mainland, 
<collectingCountry id="F34A7652FF821A7AFE4FFF1CBF2CFF18" box="[476,557,188,213]" name="Poland" pageId="5" pageNumber="356">Poland</collectingCountry>
, 
<collectingCountry id="F34A7652FF821A7AFDADFF1CBFA2FF18" box="[574,675,188,213]" name="Slovakia" pageId="5" pageNumber="356">Slovakia</collectingCountry>
, 
<collectingCountry id="F34A7652FF821A7AFD27FF1CBE18FF18" box="[692,793,188,213]" name="Slovenia" pageId="5" pageNumber="356">Slovenia</collectingCountry>
, 
<collectingCountry id="F34A7652FF821A7AFCB9FF1CBE89FF18" box="[810,904,188,213]" name="Ukraine" pageId="5" pageNumber="356">Ukraine</collectingCountry>
