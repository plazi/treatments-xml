<date id="6C1D102AFFE4FFC7FBA5FA9B3EA0AD13" box="[1029,1165,1381,1407]" pageId="27" pageNumber="28" value="1938-12-21">
<number id="AE6B24CDFFE4FFC7FBA5FA9B3E0EAD13" box="[1029,1059,1381,1407]" pageId="27" pageNumber="28" value="21.0">21</number>
.xii. 
