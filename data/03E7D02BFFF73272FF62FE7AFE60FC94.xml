(
<taxonomicName id="4C4E1ABEFFF73270FDDCF912FD05F8F2" box="[553,701,1786,1812]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName id="4C4E1ABEFFF73270FD3DF912FCDAF8F2" box="[712,866,1786,1812]" class="Insecta" family="Geometridae" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="family">Geometridae</taxonomicName>
