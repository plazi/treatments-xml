<paragraph id="D75FB6A11C4ADE8DB74CF3D738DBB54C" pageId="0" pageNumber="117">Type species.</paragraph>
<paragraph id="3073A1364F7D3F0EF92F148827961582" pageId="0" pageNumber="117">
<taxonomicName id="C8B25BB4958210C6C196B80FFB3774D0" class="Insecta" family="Blaberidae" genus="Pseudophoraspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudophoraspis fruhstorferi" order="Blattodea" pageId="0" pageNumber="117" phylum="Arthropoda" rank="species" species="fruhstorferi">Pseudophoraspis fruhstorferi</taxonomicName>
