<taxonomicName id="D3C14D78C54EFFE3FF56F8DBFE04F8A4" box="[216,409,1865,1885]" class="Magnoliopsida" family="Balsaminaceae" genus="Impatiens" kingdom="Plantae" order="Ericales" pageId="12" pageNumber="1285" phylum="Tracheophyta" rank="species" species="lutzmannii">
<emphasis id="26B5EAE9C54EFFE3FF56F8DBFE04F8A4" box="[216,409,1865,1885]" italics="true" pageId="12" pageNumber="1285">Impatiens lutzmannii</emphasis>
</taxonomicName>
. 
