<taxonomicName id="E08A3CA70E259A7641904F55905663A3" authorityName="J. Gao, Engel, Shih, &amp; T. Gao" authorityYear="2021" family="Anaxyelidae" genus="Orthosyntexis" kingdom="Animalia" lsidName="Orthosyntexis elegans" order="Hymenoptera" pageId="0" pageNumber="151" rank="species" species="elegans">
