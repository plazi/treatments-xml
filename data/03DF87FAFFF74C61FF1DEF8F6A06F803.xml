<quantity id="4C8E9B09FFF74C61FDAFED4E6DB5F8B2" box="[549,645,1796,1822]" metricMagnitude="-4" metricUnit="m" metricValue="8.299999999999999" pageId="11" pageNumber="12" unit="mm" value="0.83">0.83mm</quantity>
