<quantity id="4CEB9B19FF96830BFDE3F8A61CFDF8E6" box="[615,702,1880,1907]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="3" pageNumber="76" unit="mm" value="1.1">1.1 mm</quantity>
