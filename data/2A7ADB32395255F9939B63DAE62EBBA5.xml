<taxonomicName id="67613D47647112058AD689EBC859D7C9" LSID="2A7ADB32-3952-55F9-939B-63DAE62EBBA5" authority="Guimaraes &amp; Ferreira-Jr, 2019" authorityName="Guimaraes &amp; Ferreira-Jr" authorityYear="2019" family="Noteridae" genus="N." higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="N. teramnus" order="Coleoptera" pageId="0" pageNumber="177" rank="species" species="teramnus">
