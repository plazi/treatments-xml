<number id="3DA924E74976FFFDFB7D3D9DDB85229A" box="[1215,1230,386,412]" pageId="8" pageNumber="27" value="5.0">5</number>
and 
<number id="3DA924E74976FFFDFAC73D9DDA5C229A" box="[1285,1303,386,412]" pageId="8" pageNumber="27" value="6.0">6</number>
