<subSubSection id="BDB2B81D3499AD93088CE12BBFB65C9F" type="reference_group">
<paragraph id="A600A1B91B6AE41AD8D4B5017F1C479A" pageNumber="34">
<taxonomicName id="312D0A757D9178776AA1392D54DD08F1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28523" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex daguerrei Santschi" order="Hymenoptera" pageNumber="34" phylum="Arthropoda" rank="species" species="daguerrei">Cyphomyrmex daguerrei</taxonomicName>
