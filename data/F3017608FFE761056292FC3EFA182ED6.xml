<taxonomicName id="BCA8BC9DFFE7610563DBFC3EFDB328AC" box="[478,560,976,1002]" class="Insecta" family="Tephritidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="104" phylum="Arthropoda" rank="tribe" tribe="Dacini">Dacini</taxonomicName>
