<taxonomicName id="008C4AA6FF8E8668FF5804AAACC5FC78" authorityName="Jocque" authorityYear="1991" box="[128,230,976,997]" class="Arachnida" family="Zodariidae" genus="Platnickia" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
