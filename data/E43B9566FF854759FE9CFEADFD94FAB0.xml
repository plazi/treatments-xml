(
<figureCitation id="F4A938F5FF854759FB9BFEADFBB7FE9C" box="[1039,1138,283,309]" captionStart="FIGURES 15 ­ 17" captionStartId="10.[264,383,1836,1860]" captionTargetBox="[386,1202,336,1821]" captionTargetId="figure@10.[379,1208,329,1825]" captionTargetPageId="10" captionText="FIGURES 15 ­ 17. Male terminalia, left lateral view: 15, Thecomyia papaveroi Marinoni and Steyskal; 16, T. signorelli Marinoni and Steyskal; 17, T. tricuneata Marinoni and Steyskal." httpUri="https://zenodo.org/record/156861/files/figure.png" pageId="26" pageNumber="27">Figs. 17</figureCitation>
, 
<figureCitation id="F4A938F5FF854759FBEBFEADFB5BFE9C" box="[1151,1182,283,309]" captionStart="FIGURES 24 ­ 29" captionStartId="12.[264,382,1658,1682]" captionTargetBox="[275,1317,352,1566]" captionTargetId="figure@12.[267,1319,329,1605]" captionTargetPageId="12" captionText="FIGURES 24 ­ 29. Hypandrium, frontal view and right lateral view: 24, Thecomyia chrysacra Marinoni and Steyskal; 25, T. mathisi Marinoni; 26, T. naponica Marinoni and Steyskal; 27, T. papaveroi Marinoni and Steyskal; 28, T. signorelli Marinoni and Steyskal; 29, T. tricuneata Marinoni and Steyskal." httpUri="https://zenodo.org/record/156863/files/figure.png" pageId="26" pageNumber="27">29</figureCitation>
, 40, 41, 
<number id="DA5A3657FF854759FA97FEADFAE7FE9C" box="[1283,1314,283,309]" pageId="26" pageNumber="27" value="42.0">42</number>
