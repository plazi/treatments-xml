<number id="E72BBD00322D251C306F6F1FFA39F891" box="[1377,1431,1848,1874]" pageId="17" pageNumber="18" value="0.13">0.13</number>
, ALE 
<number id="E72BBD00322D251C35D76F47FEA1F8B9" box="[217,271,1888,1914]" pageId="17" pageNumber="18" value="0.18">0.18</number>
