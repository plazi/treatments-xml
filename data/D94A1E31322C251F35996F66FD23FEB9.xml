+ 
<number id="E72BBD00322D251C347E6F97FE3AF809" box="[368,404,1968,1994]" pageId="17" pageNumber="18" value="3.2">3.2</number>
+ 
<number id="E72BBD00322D251C34BD6F97FE76F809" box="[435,472,1968,1994]" pageId="17" pageNumber="18" value="2.2">2.2</number>
+ 
<number id="E72BBD00322D251C34F96F97FDB1F809" box="[503,543,1968,1994]" pageId="17" pageNumber="18" value="1.5">1.5</number>
); III: 9.0 (
<number id="E72BBD00322D251C379B6F97FD15F809" box="[661,699,1968,1994]" pageId="17" pageNumber="18" value="2.5">2.5</number>
+ 
<number id="E72BBD00322D251C37D46F97FD50F809" box="[730,766,1968,1994]" pageId="17" pageNumber="18" value="2.9">2.9</number>
+ 
<number id="E72BBD00322D251C36136F97FCECF809" box="[797,834,1968,1994]" pageId="17" pageNumber="18" value="2.3">2.3</number>
+ 
<number id="E72BBD00322D251C366F6F97FC27F809" box="[865,905,1968,1994]" pageId="17" pageNumber="18" value="1.3">1.3</number>
); IV: 
<number id="E72BBD00322D251C36C56F97FBA2F809" box="[971,1036,1968,1994]" pageId="17" pageNumber="18" value="11.95">11.95</number>
(
<number id="E72BBD00322D251C31156F97FBEFF809" box="[1051,1089,1968,1994]" pageId="17" pageNumber="18" value="3.2">3.2</number>
+ 
<number id="E72BBD00322D251C316E6F97FB3DF809" box="[1120,1171,1968,1994]" pageId="17" pageNumber="18" value="3.85">3.85</number>
+ 
<number id="E72BBD00322D251C31BC6F97FB78F809" box="[1202,1238,1968,1994]" pageId="17" pageNumber="18" value="3.3">3.3</number>
+ 
<number id="E72BBD00322D251C31FB6F97FAB0F809" box="[1269,1310,1968,1994]" pageId="17" pageNumber="18" value="1.6">1.6</number>
