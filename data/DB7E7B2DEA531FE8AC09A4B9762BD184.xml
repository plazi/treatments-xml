<taxonomicName id="CB9D5D7F5F22CEB149A9339DD509D39A" authority="Seguenza, 1864" authorityName="Seguenza" authorityYear="1864" class="Anthozoa" family="Caryophylliidae" genus="Stephanocyathus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stephanocyathus" order="Scleractinia" pageId="0" pageNumber="1" phylum="Cnidaria" rank="genus">Stephanocyathus Seguenza, 1864</taxonomicName>
