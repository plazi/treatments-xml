<taxonomicName id="42DA06B9662BD471FB3B9B60FA12C742" baseAuthorityName="Szilady" baseAuthorityYear="1938" box="[1225,1433,1315,1339]" class="Insecta" family="Syrphidae" genus="Cheilosia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="107" phylum="Arthropoda" rank="species" species="cumanica">Cheilosia cumanica</taxonomicName>
