<taxonomicName id="4C734D730F56FFB9FF19BD5A3584F82B" baseAuthorityName="Fabricius" baseAuthorityYear="1805" box="[151,424,1917,1944]" class="Insecta" family="Calliphoridae" genus="Mesembrinella" kingdom="Animalia" order="Diptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="bicolor">
<emphasis id="B907EAE20F56FFB9FF19BD5A3584F82B" bold="true" box="[151,424,1917,1944]" italics="true" pageId="108" pageNumber="109">Mesembrinella bicolor</emphasis>
