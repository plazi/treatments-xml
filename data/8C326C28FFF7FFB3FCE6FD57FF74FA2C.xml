<taxonomicName id="C39BA6BDFFF7FFB5FBCAFBEDFC09FB7F" authority="(Miers, 1882)" baseAuthorityName="Miers" baseAuthorityYear="1882" class="Malacostraca" family="Callianassidae" genus="Neocallichirus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="27" pageNumber="74" phylum="Arthropoda" rank="species" species="mauritianus">
<emphasis id="36EF012CFFF7FFB5FBCAFBEDFC9EFB7F" italics="true" pageId="27" pageNumber="74">Neocallichirus mauritianus</emphasis>
(Miers, 1882)
</taxonomicName>
: 
<emphasis id="36EF012CFFF7FFB5FC45FB04FB95FB7F" bold="true" box="[926,1032,1153,1176]" pageId="27" pageNumber="74">
<collectingCountry id="7C8C9DAEFFF7FFB5FC45FB04FB99FB7F" box="[926,1028,1153,1176]" name="Mauritius" pageId="27" pageNumber="74">Mauritius</collectingCountry>
