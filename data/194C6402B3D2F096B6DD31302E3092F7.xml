<taxonomicName id="0EC437D2587EC379D3DC93A36BC7BB75" authorityName="Cockerell" authorityYear="1920" class="Insecta" family="Scarabaeidae" genus="Pelidnotites" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pelidnotites atavus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="atavus">Pelidnotites atavus</taxonomicName>
