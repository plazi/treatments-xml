<specimenCount id="B3AAFD3D5461FFF7FA2EBD01FE91FA82" pageId="36" pageNumber="35" type="generic">7 specimens</specimenCount>
of 
<taxonomicName id="62AC4D375461FFF7FEDEBD61FE76FA82" authorityName="New" authorityYear="1985" box="[353,473,1346,1370]" class="Insecta" family="Myrmeleontidae" genus="Xantholeon" kingdom="Animalia" order="Neuroptera" pageId="36" pageNumber="35" phylum="Arthropoda" rank="species" species="xadnus">
