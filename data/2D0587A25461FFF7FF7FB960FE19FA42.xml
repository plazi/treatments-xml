<taxonomicName id="62AC4D375461FFF7FD69BD61FCDFFA82" authorityName="New" authorityYear="1985" box="[726,880,1347,1370]" class="Insecta" family="Myrmeleontidae" genus="Xantholeon" kingdom="Animalia" order="Neuroptera" pageId="36" pageNumber="35" phylum="Arthropoda" rank="species" species="montanus">
