<paragraph id="D2EBFB9D5406196EF745A4E2C939279A" pageId="16" pageNumber="101">
<taxonomicName id="55AA6BD965F4808B725C45157DA7053A" authority="Dahlbom, 1835" authorityName="*" authorityYear="1835" baseAuthorityName="Stephens" baseAuthorityYear="1835" class="Insecta" family="Tenthredinidae" genus="Strongylogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Strongylogaster xanthocera" order="Hymenoptera" pageId="16" pageNumber="101" phylum="Arthropoda" rank="species" species="xanthocera">
<emphasis id="8D7E87B2596CFF4850849060B3C8C9B5" italics="true" pageId="16" pageNumber="101">Strongylogaster xanthocera</emphasis>
</taxonomicName>
