<taxonomicName id="4C304D5BEF1A2633542A40079B7AF880" box="[301,536,1840,1863]" class="Insecta" family="Myrmeleontidae" genus="Ganguilus" kingdom="Animalia" order="Neuroptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="flavipennis">
