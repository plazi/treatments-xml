<normalizedToken id="3285C6301B590A3434670C03536400A4" originalValue="Stengelblätter" pageId="null" pageNumber="57">Stengelblaetter</normalizedToken>
(
<normalizedToken id="5B365C4587DD9B482636BB7F15C0F306" originalValue="Hochblätter" pageId="null" pageNumber="57">Hochblaetter</normalizedToken>
