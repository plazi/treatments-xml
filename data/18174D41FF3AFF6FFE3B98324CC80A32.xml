<collectingRegion id="527A32B5FF45FF6FFD649BA64E5F0B9B" box="[674,788,1170,1196]" country="United States of America" name="Alabama" pageId="192" pageNumber="193">Alabama</collectingRegion>
</emphasis>
</heading>
</paragraph>
<paragraph id="9001FC57FF45FF6FFF7B9BF04CC80A32" blockId="192.[189,1399,1220,1285]" pageId="192" pageNumber="193">
<taxonomicName id="57BE87D4FF45FF6FFF7B9BF04CF90BE9" authorityName="- Nolf" authorityYear="2013" box="[189,434,1220,1246]" class="Actinopterygii" family="Congridae" genus="Gnathophis" kingdom="Animalia" order="Anguilliformes" pageId="192" pageNumber="193" phylum="Chordata" rank="species" species="meridies">
<emphasis id="A2CA2045FF45FF6FFF7B9BF04CF90BE9" box="[189,434,1220,1246]" italics="true" pageId="192" pageNumber="193">Gnathophis meridies</emphasis>
</taxonomicName>
