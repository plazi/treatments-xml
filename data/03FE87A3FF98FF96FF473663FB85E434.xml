<taxonomicName id="4C574D36FF99FF96FD26340FFCD7E6A2" box="[758,872,1126,1152]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
