<taxonomicName id="2CB3921DEA3103474273037C7A3B1C2B" class="Arachnida" family="Anapidae" genus="Anapisona" genus.bestMatchDistance="1863" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="hamigera">A. hamigera</taxonomicName>
