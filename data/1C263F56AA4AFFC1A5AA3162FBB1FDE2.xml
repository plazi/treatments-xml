<taxonomicName id="538FF5C3AA4BFFC1A47D368AFE34FDE3" authorityName="Ma &amp; Li" authorityYear="2012" box="[320,489,498,522]" class="Insecta" family="Staphylinidae" genus="Lesteva" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="brevimacula">
<emphasis id="A6FB5252AA4BFFC1A47D368AFE34FDE3" box="[320,489,498,522]" italics="true" pageId="3" pageNumber="4">L. brevimacula</emphasis>
</taxonomicName>
