; 
<collectorName id="794D535EFFBBFFD7FC66EFDDFBE6168A" box="[935,1057,668,695]" pageId="13" pageNumber="14">M. Stoltze</collectorName>
leg.; 
<specimenCode id="841E9EF3FFBBFFD7FBA5EFDDFAE6168A" box="[1124,1313,668,695]" collectionCode="NHMD" pageId="13" pageNumber="14">NHMD 621641</specimenCode>
</materialsCitation>
