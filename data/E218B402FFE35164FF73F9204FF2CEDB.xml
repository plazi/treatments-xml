<taxonomicName id="ADB17E97FFE35164FD3CF8FF4F51CE8A" box="[728,855,1905,1930]" class="Phaeophyceae" family="Sargassaceae" genus="Sargassum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fucales" pageId="16" pageNumber="17" phylum="Heterokontophyta" rank="genus">
