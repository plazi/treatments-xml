<paragraph id="8ABA7DD7E564265A610EB8BF08EF3C71" pageId="2" pageNumber="105">
<taxonomicName id="820CA04821472F8FF8A014EA8CA4F157" class="Insecta" family="Pompilidae" genus="Nipponodipogon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nipponodipogon" order="Hymenoptera" pageId="2" pageNumber="105" phylum="Arthropoda" rank="genus">Nipponodipogon</taxonomicName>
