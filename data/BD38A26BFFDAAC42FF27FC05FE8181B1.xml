<taxonomicName id="F29168FEFFDAAC4DFF27FBB8FCC48413" authority="Stal, 1860" authorityName="Stal" authorityYear="1860" box="[151,394,1044,1066]" class="Insecta" family="Cydnidae" genus="Magoa" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="424" phylum="Arthropoda" rank="species" species="pusio">
