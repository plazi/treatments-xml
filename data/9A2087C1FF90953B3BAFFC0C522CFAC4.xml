<taxonomicName id="D5894D54FF90953B3823FB0955D3FB1D" authorityName="Karsch" authorityYear="1879" box="[795,901,1232,1258]" class="Arachnida" family="Dipluridae" genus="Linothele" kingdom="Animalia" order="Araneae" pageId="68" pageNumber="69" phylum="Arthropoda" rank="genus">
