<quantity id="D5719B32FF90953B3FB5FB2D5377FAF8" box="[1165,1313,1268,1295]" metricMagnitude="3" metricUnit="m" metricValue="2.2715" metricValueMax="2.318" metricValueMin="2.225" pageId="68" pageNumber="69" unit="m" value="2271.5" valueMax="2318.0" valueMin="2225.0">2225–2318m</quantity>
