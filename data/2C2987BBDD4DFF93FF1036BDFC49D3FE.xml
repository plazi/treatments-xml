<collectingCountry id="DC97763DDD73FF93FD47305AFCEDD3A6" box="[704,806,1648,1673]" name="Thailand" pageId="33" pageNumber="484">Thailand</collectingCountry>
, southern 
<collectingCountry id="DC97763DDD73FF93FC1E305AFC30D3A6" box="[921,1019,1648,1673]" name="Vietnam" pageId="33" pageNumber="484">Vietnam</collectingCountry>
