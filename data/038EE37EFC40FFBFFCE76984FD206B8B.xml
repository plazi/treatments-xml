<location id="8EF804B3FC43FFBFFDC66F0BFDE46B6B" LSID="urn:lsid:plazi:treatment:038EE37EFC40FFBFFCE76984FD206B8B:8EF804B3FC43FFBFFDC66F0BFDE46B6B" box="[542,584,1833,1857]" county="The" municipality="Gongga" name="The" pageId="6" pageNumber="225" stateProvince="Shan">The</location>
