<paragraph id="3B851C8C8A9FA042160BE4092963D92A" pageId="7" pageNumber="26">
<taxonomicName id="AFFA1A8068D6939E7A1364F8C952BF18" class="Insecta" family="Eumenidae" genus="Calligaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calligaster himalayensis" order="Hymenoptera" pageId="7" pageNumber="26" phylum="Arthropoda" rank="species" species="himalayensis">Calligaster himalayensis</taxonomicName>
