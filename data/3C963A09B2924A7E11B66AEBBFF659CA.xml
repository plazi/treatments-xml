<taxonomicName id="E969A2D8E37A23BB16AF73F29AF56460" class="Insecta" family="Eumenidae" genus="Zethus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zethus himalayensis" order="Hymenoptera" pageId="7" pageNumber="26" phylum="Arthropoda" rank="species" species="himalayensis">
<pageBreakToken id="D21E8F4AAFF0DF5271D5A7E458183EFD" pageId="7" pageNumber="26" start="start">Zethus</pageBreakToken>
himalayensis
</taxonomicName>
Cameron, 1904: 13, ♀, 
