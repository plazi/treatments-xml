<quantity id="4CF79B33FFBAFF93C0F6FAB4FD47FA6D" box="[655,721,1389,1409]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="85" pageNumber="240" unit="m" value="1200.0">1200m</quantity>
