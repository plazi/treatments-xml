<taxonomicName id="DFC44D5E2422FF83358DFA5BFB8ABD09" baseAuthorityName="L. Koch" baseAuthorityYear="1882" box="[935,1073,1533,1557]" class="Arachnida" family="Gnaphosidae" genus="Zelotes" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="532" phylum="Arthropoda" rank="species" species="flagellans">
