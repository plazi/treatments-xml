<taxonomicName id="DFC44D5E2422FF8335B9FA3BFB94BEA9" box="[915,1071,1437,1461]" class="Arachnida" family="Gnaphosidae" genus="Zelotes" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="532" phylum="Arthropoda" rank="species" species="pediculatus">
