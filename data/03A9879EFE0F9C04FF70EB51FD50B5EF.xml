<taxonomicName id="4C004D0BFE019C06FEBDEBAAFE7EB0DF" box="[346,504,1761,1784]" class="Arachnida" family="Cosmetidae" genus="Roquettea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="8" pageNumber="43" phylum="Arthropoda" rank="species" species="peba" status="sp. nov.">
