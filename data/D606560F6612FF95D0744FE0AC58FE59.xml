<quantity id="99574AFC6612FF97D0A54943AA4BFAE6" box="[473,581,1359,1385]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="8" pageNumber="61" unit="mm" value="7.5" valueMax="10.0" valueMin="5.0">5–10 µm</quantity>
