<geoCoordinate id="3B9B81DE6612FF97D33249E3AAAAF986" box="[590,676,1519,1545]" degrees="1.24" direction="west" orientation="longitude" pageId="8" pageNumber="61" precision="555" value="-1.24">W 1.24</geoCoordinate>
