<quantity id="99574AFC6612FF97D074497BA99FFA1E" box="[264,401,1399,1425]" metricMagnitude="-1" metricUnit="m" metricValue="1.035" metricValueMax="1.1" metricValueMin="0.97" pageId="8" pageNumber="61" unit="mm" value="103.5" valueMax="110.0" valueMin="97.0">97–110 µm</quantity>
