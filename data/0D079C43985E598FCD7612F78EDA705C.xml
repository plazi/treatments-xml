<subSubSection id="1D291CFC6DE3D3533A8B36F9ABF21D2E" pageId="633" pageNumber="634" type="reference_group">
<paragraph id="9598E704564F864B9134665755E60854" pageId="633" pageNumber="634">
<taxonomicName id="E5FE0503CEA7856EA0D5E8E5BEDB4E93" authorityName="Haldeman" authorityYear="1843" class="Hexapoda" family="Carabidae" genus="Badister" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="633" pageNumber="634" phylum="Arthropoda" rank="species" species="notatus">Badister notatus</taxonomicName>
