<taxonomicName id="4C004D2DFFEAEF3F628EFD73FD57FD29" authority="Sharp, 1903" authorityName="Sharp" authorityYear="1903" box="[420,701,694,716]" class="Insecta" family="Carabidae" genus="Mecyclothorax" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="388" phylum="Arthropoda" rank="genus">
