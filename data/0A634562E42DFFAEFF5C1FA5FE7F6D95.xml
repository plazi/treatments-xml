<collectionCode id="E4DB6CB1E42DFFA8FED71E17FE896B2C" box="[284,332,1873,1899]" httpUri="http://grbio.org/cool/kjjc-ziy5" name="Paleontological Institute, Russian Academy of Sciences" pageId="2" pageNumber="57">PIN</collectionCode>
<number id="3402E653E42DFFA8FE9F1E17FE556B2C" box="[340,400,1873,1899]" pageId="2" pageNumber="57" value="2010.0">2010</number>
/ 
<number id="3402E653E42DFFA8FE6A1E17FE7A6B2C" box="[417,447,1873,1899]" pageId="2" pageNumber="57" value="16.0">16</number>
