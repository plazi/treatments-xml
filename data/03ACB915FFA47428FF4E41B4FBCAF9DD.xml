<taxonomicName id="4C057380FFA37428FE114317FDB9F9DC" authorityName="Miller &amp; Griswold &amp; Yin" authorityYear="2009" box="[467,584,1610,1636]" class="Arachnida" family="Symphytognathidae" genus="Crassignatha" kingdom="Animalia" order="Araneae" pageId="60" pageNumber="69" phylum="Arthropoda" rank="species" species="quanqu">
