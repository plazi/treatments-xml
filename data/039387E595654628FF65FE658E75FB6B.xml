</taxonomicName>
, 
<taxonomicName id="4C3A4D7095654656FCE7FD6289C1FD41" authorityName="Mello-Leitao" authorityYear="1931" box="[789,919,709,735]" class="Arachnida" family="Gonyleptidae" genus="Eusarcus" kingdom="Animalia" order="Opiliones" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="pusillus">
