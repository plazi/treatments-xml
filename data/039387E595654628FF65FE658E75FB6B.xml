</taxonomicName>
, 
<taxonomicName id="4C3A4D7095654656FC5EFD628ED5FD41" box="[940,1155,709,735]" class="Arachnida" family="Gonyleptidae" genus="Eusarcus" kingdom="Animalia" order="Opiliones" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="nigrimaculatus">
