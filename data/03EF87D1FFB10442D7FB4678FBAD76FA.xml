<taxonomicName id="4C464D44FFBF0442D73847DFFDD276FA" authorityName="Albury &amp; Franz &amp; Rimoli &amp; Lehman &amp; Rosenberger" authorityYear="2018" box="[272,545,1233,1259]" class="Reptilia" family="Testudinidae" genus="Chelonoidis" kingdom="Animalia" order="Testudines" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="dominicensis">
