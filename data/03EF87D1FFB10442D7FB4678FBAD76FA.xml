<collectorName id="26B35311FFBF0442D64441C9FF4D70F0" box="[108,190,711,737]" name="Nancy A. Albury" pageId="8" pageNumber="9">N.A.A.</collectorName>
and 
<collectorName id="26B35311FFBF0442D6DE41C9FED170F0" box="[246,290,711,737]" name="Richard Franz" pageId="8" pageNumber="9">R.F.</collectorName>
