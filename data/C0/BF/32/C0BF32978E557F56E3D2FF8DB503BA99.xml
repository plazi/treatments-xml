<subSubSection type="nomenclature">
<paragraph pageNumber="307">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:245078" class="Insecta" family="Formicidae" genus="Stenamma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Stenamma westwoodi var. striatula" order="Hymenoptera" pageNumber="307" phylum="Arthropoda" rank="variety" species="westwoodi" variety="striatula">var. striatula</taxonomicName>
</paragraph>
