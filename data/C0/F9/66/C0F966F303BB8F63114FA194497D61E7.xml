<taxonomicName class="Polypodiopsida" family="Aspleniaceae" genus="Asplenium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Asplenium antiquum" order="Polypodiales" pageId="5" pageNumber="126" phylum="Tracheophyta" rank="species" species="antiquum">Asplenium antiquum</taxonomicName>
