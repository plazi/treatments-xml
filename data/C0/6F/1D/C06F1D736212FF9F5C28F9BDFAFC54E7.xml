<materialsCitation ID-GBIF-Occurrence="2598720876" collectingDate="2017-11-03" country="Iran" latitude="31.216389" location="Kerman Province" longLatPrecision="20" longitude="56.814445" pageId="0" pageNumber="83" specimenCount="1" stateProvince="Kerman Province">
