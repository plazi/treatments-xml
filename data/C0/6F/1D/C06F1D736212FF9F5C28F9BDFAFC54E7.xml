<taxonomicName box="[160,361,1771,1794]" class="Magnoliopsida" family="Amaranthaceae" kingdom="Plantae" order="Caryophyllales" pageId="0" pageNumber="83" phylum="Tracheophyta" rank="family">Amaranthaceae</taxonomicName>
) in 
<collectingCountry box="[418,473,1771,1794]" name="Iran" pageId="0" pageNumber="83">Iran</collectingCountry>
, Ravar, 
<collectingRegion box="[584,815,1771,1794]" country="Iran" name="Kerman" pageId="0" pageNumber="83">Kerman Province</collectingRegion>
, 
<geoCoordinate box="[830,1014,1771,1794]" degrees="31" direction="north" minutes="12" orientation="latitude" pageId="0" pageNumber="83" precision="15" seconds="59" value="31.216389">31° 12' 59&quot; N</geoCoordinate>
, 
<geoCoordinate box="[1033,1213,1771,1794]" degrees="56" direction="east" minutes="48" orientation="longitude" pageId="0" pageNumber="83" precision="15" seconds="52" value="56.814445">56° 48' 52&quot; E</geoCoordinate>
, 
<quantity box="[1231,1331,1771,1794]" metricMagnitude="3" metricUnit="m" metricValue="1.197" pageId="0" pageNumber="83" unit="m" value="1197.0">1197 m</quantity>
