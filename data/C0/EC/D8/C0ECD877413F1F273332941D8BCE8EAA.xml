<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:6AFF212F-BC74-4CC4-9833-62832A9228DA" class="Actinopterygii" family="Bagridae" genus="Tachysurus" kingdom="Animalia" lsidName-ZBK="Tachysurus Lacépède 1803:150" order="Siluriformes" pageNumber="34" phylum="Chordata" rank="genus">Tachysurus</taxonomicName>
