<taxonomicName class="Actinopterygii" family="Ariidae" genus="Potamosilurus" kingdom="Animalia" order="Siluriformes" pageNumber="34" phylum="Chordata" rank="genus">Potamosilurus</taxonomicName>
(with exception of 
<taxonomicName class="Actinopterygii" family="Ariidae" genus="Potamosilurus" kingdom="Animalia" order="Siluriformes" pageNumber="34" phylum="Chordata" rank="species" species="latirostris">P. latirostris</taxonomicName>
) and 
<taxonomicName class="Insecta" family="Cerambycidae" genus="Sciades" kingdom="Animalia" order="Coleoptera" pageNumber="34" phylum="Arthropoda" rank="genus">Sciades</taxonomicName>
(with exception of 
<taxonomicName class="Insecta" family="Cerambycidae" genus="Sciades" kingdom="Animalia" order="Coleoptera" pageNumber="34" phylum="Arthropoda" rank="species" species="couma">S. couma</taxonomicName>
