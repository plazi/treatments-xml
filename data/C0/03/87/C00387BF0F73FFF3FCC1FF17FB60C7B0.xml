<quantity box="[724,859,692,716]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" metricValueMax="1.2" metricValueMin="0.9" pageId="7" pageNumber="66" unit="mm" value="1.05" valueMax="1.2" valueMin="0.9">0.9–1.2 mm</quantity>
