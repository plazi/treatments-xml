<quantity box="[305,440,727,751]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="7" pageNumber="66" unit="mm" value="0.45" valueMax="0.5" valueMin="0.4">0.4–0.5 mm</quantity>
