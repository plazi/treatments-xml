<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.0999999999999996" metricValueMax="3.3" metricValueMin="2.9" pageId="7" pageNumber="66" unit="mm" value="3.0999999999999996" valueMax="3.3" valueMin="2.9">2.9–3.3 mm</quantity>
