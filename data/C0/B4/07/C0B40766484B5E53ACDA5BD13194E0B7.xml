<taxonomicName class="Insecta" family="Eulophidae" genus="Aprostocetus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Aprostocetus" order="Hymenoptera" pageId="0" pageNumber="59177" phylum="Arthropoda" rank="genus">
