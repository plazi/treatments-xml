<taxonomicName authorityName="Haliday" authorityYear="1844" class="Insecta" family="Eulophidae" genus="Tetrastichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetrastichus" order="Hymenoptera" pageId="0" pageNumber="59177" phylum="Arthropoda" rank="genus">
