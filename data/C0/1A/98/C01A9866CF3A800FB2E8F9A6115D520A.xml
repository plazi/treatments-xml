<number box="[469,507,2007,2031]" pageId="9" pageNumber="10" value="7.9">7.9</number>
, Ƥ 
<number box="[539,627,2007,2031]" pageId="9" pageNumber="10" value="8.65" valueMax="8.9" valueMin="8.4">8.4–8.9</number>
; ovipositor: 
<number box="[766,853,2007,2031]" pageId="9" pageNumber="10" value="5.85" valueMax="5.9" valueMin="5.8">5.8–5.9</number>
.
