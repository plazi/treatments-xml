<taxonomicName authorityName="Freytag" authorityYear="2012" box="[415,515,1794,1818]" class="Insecta" family="Cicadellidae" genus="Neokrisna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="breva">
