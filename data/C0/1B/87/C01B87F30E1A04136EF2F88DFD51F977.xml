<taxonomicName authority="Liu &amp; Liu &amp; Liu, 2010" authorityName="Liu &amp; Liu &amp; Liu" authorityYear="2010" box="[206,370,2032,2053]" class="Malacostraca" family="Axianassidae" genus="Axianassa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="195" phylum="Arthropoda" rank="species" species="sinica" status="new species">
