<taxonomicName authority="Liu &amp; Liu &amp; Liu, 2010" authorityName="Liu &amp; Liu &amp; Liu" authorityYear="2010" box="[182,346,2090,2111]" class="Malacostraca" family="Axianassidae" genus="Axianassa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="194" phylum="Arthropoda" rank="species" species="sinica" status="new species">
