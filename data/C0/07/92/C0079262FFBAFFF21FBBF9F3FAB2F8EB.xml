<materialsCitation ID-GBIF-Occurrence="1671715562" collectionCode="CBE" collectorName="Ragusa" location="Sicilia" pageId="92" pageNumber="93" specimenCount="1" specimenCount-female="1" stateProvince="Sicilia">
