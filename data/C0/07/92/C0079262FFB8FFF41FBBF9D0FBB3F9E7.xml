<date box="[1113,1234,188,212]" pageId="90" pageNumber="91" value="2011-01-28">28 I 2011</date>
<geoCoordinate box="[1248,1406,188,212]" direction="north" orientation="latitude" pageId="90" pageNumber="91" precision="1" value="39.30165">N 39°18.099'</geoCoordinate>
