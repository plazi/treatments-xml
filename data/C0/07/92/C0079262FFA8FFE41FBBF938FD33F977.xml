<paragraph blockId="74.[151,1437,151,1688]" pageId="74" pageNumber="75">
<emphasis bold="true" box="[199,451,1268,1293]" pageId="74" pageNumber="75">
<typeStatus box="[199,308,1268,1293]" pageId="74" pageNumber="75">Paratype</typeStatus>
variability.
</emphasis>
