<emphasis box="[506,753,947,969]" italics="true" pageId="35" pageNumber="381">Rhionaeschna variegata</emphasis>
</taxonomicName>
; b 
<taxonomicName box="[785,927,947,969]" class="Insecta" family="Petaluridae" genus="Phenes" kingdom="Animalia" order="Odonata" pageId="35" pageNumber="381" phylum="Arthropoda" rank="species" species="raptor">
<emphasis box="[785,927,947,969]" italics="true" pageId="35" pageNumber="381">Phenes raptor</emphasis>
