soil, C/N 
<number box="[641,694,1367,1393]" pageId="15" pageNumber="16" value="11.5">11.5</number>
, proportion of gravels (&gt; 
<quantity box="[1012,1086,1367,1393]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="15" pageNumber="16" unit="mm" value="2.0">2 mm</quantity>
) 
<number box="[1104,1136,1367,1393]" pageId="15" pageNumber="16" value="36.0">36</number>
%, clay content 
<number box="[264,296,1410,1436]" pageId="15" pageNumber="16" value="50.0">50</number>
%, pH (water) 
<number box="[467,506,1410,1436]" pageId="15" pageNumber="16" value="5.7">5.7</number>
, cations 
<number box="[609,645,1410,1436]" pageId="15" pageNumber="16" value="6.9">6.9</number>
cmol kg­ 
<number box="[755,763,1408,1434]" pageId="15" pageNumber="16" value="1.0">1</number>
soil, CEC 
<number box="[893,945,1410,1436]" pageId="15" pageNumber="16" value="18.9">18.9</number>
cmol kg­ 
