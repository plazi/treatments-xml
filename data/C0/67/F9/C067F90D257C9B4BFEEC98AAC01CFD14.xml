/ 
<number box="[1099,1131,1164,1190]" pageId="16" pageNumber="17" value="22.0">22</number>
; paired slightly median to seminal grooves, presetal at 
<number box="[721,752,1204,1230]" pageId="16" pageNumber="17" value="18.0">18</number>
, postsetal at 17, 18, 
<number box="[991,1023,1204,1230]" pageId="16" pageNumber="17" value="19.0">19</number>
, setal at 
<number box="[1125,1157,1204,1230]" pageId="16" pageNumber="17" value="18.0">18</number>
, intersegmental at 
<number box="[335,364,1244,1270]" pageId="16" pageNumber="17" value="17.0">17</number>
/ 
<number box="[374,403,1244,1270]" pageId="16" pageNumber="17" value="18.0">18</number>
/ 
