, postsetal at 16, 18, intersegmental at 
<number box="[476,505,1284,1310]" pageId="16" pageNumber="17" value="14.0">14</number>
/ 
<number box="[515,543,1284,1310]" pageId="16" pageNumber="17" value="15.0">15</number>
/ 
<number box="[552,581,1284,1310]" pageId="16" pageNumber="17" value="16.0">16</number>
/17, 20/ 
<number box="[674,702,1284,1310]" pageId="16" pageNumber="17" value="21.0">21</number>
/ 
<number box="[711,741,1284,1310]" pageId="16" pageNumber="17" value="22.0">22</number>
; single at midventral line, presetal at 17, 18, 
<number box="[1283,1315,1284,1310]" pageId="16" pageNumber="17" value="19.0">19</number>
, postsetal at 16, 17, 19, 20, intersegmental at 
<number box="[807,837,1324,1350]" pageId="16" pageNumber="17" value="16.0">16</number>
/17, 19/ 
