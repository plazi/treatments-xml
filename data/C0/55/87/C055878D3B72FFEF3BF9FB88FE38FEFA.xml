<taxonomicName authority="Westwood, 1844" authorityName="Westwood" authorityYear="1844" box="[531,773,683,702]" class="Insecta" family="Dinidoridae" genus="Eumenotes" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="113" phylum="Arthropoda" rank="genus">
<emphasis box="[531,625,683,702]" italics="true" pageId="4" pageNumber="113">Eumenotes</emphasis>
Westwood, 1844
</taxonomicName>
and 
<taxonomicName authority="Kment &amp; Kocorek, 2014" authorityName="Kment &amp; Kocorek" authorityYear="2014" box="[814,921,683,702]" class="Insecta" family="Dinidoridae" genus="Afromenotes" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="113" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis box="[814,921,683,702]" italics="true" pageId="4" pageNumber="113">Afromenotes</emphasis>
</taxonomicName>
