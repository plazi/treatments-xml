<taxonomicName authorityName="Saito" authorityYear="1969" box="[767,1092,1122,1148]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="177" pageNumber="178" phylum="Arthropoda" rank="species" species="megaspinosa">
<emphasis box="[767,1092,1122,1148]" italics="true" pageId="177" pageNumber="178">Haemaphysalis megaspinosa</emphasis>
</taxonomicName>
from 
<taxonomicName authorityName="Warburton" authorityYear="1908" box="[1159,1436,1122,1148]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="177" pageNumber="178" phylum="Arthropoda" rank="species" species="japonica">
