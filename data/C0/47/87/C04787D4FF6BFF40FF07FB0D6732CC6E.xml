<taxonomicName authority="Neumann, 1899" authorityName="Roberts" authorityYear="1953" box="[547,781,1339,1365]" class="Arachnida" family="Ixodidae" genus="Aponomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="130" pageNumber="131" phylum="Arthropoda" rank="species" species="tropicum">
<emphasis box="[547,781,1339,1365]" italics="true" pageId="130" pageNumber="131">Aponomma tropicum</emphasis>
