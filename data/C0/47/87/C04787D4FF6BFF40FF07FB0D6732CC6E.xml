and 
<bibRefCitation author="Santos Dias, J. A. T." box="[617,834,1483,1509]" pageId="130" pageNumber="131" pagination="1 - 204" refId="ref243614" refString="Santos Dias, J. A. T. (1993) Contribuicao para o estudo da sistematica e taxonomia das especies do genero Aponomma Neumann, 1899 (Acarina-Ixodoidea). Estudos, Ensaios e Documentos, 157, 1 - 204." type="journal article" year="1993">Santos Dias (1993)</bibRefCitation>
, both under the name 
<taxonomicName authority="Neumann, 1899" authorityName="Neumann" authorityYear="1899" box="[1082,1317,1483,1509]" class="Arachnida" family="Ixodidae" genus="Aponomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="130" pageNumber="131" phylum="Arthropoda" rank="species" species="concolor">
<emphasis box="[1082,1317,1483,1509]" italics="true" pageId="130" pageNumber="131">Aponomma concolor</emphasis>
</taxonomicName>
