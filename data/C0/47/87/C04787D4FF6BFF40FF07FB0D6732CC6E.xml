<taxonomicName authority="Neumann, 1899" authorityName="Neumann" authorityYear="1899" box="[550,785,1375,1401]" class="Arachnida" family="Ixodidae" genus="Aponomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="130" pageNumber="131" phylum="Arthropoda" rank="species" species="concolor">
