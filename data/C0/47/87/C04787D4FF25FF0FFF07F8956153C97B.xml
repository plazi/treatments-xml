<taxonomicName authorityName="Sharif" authorityYear="1928" box="[780,1003,510,536]" class="Arachnida" family="Ixodidae" genus="Hyalomma" kingdom="Animalia" order="Ixodida" pageId="205" pageNumber="206" phylum="Arthropoda" rank="species" species="hussaini">
