<taxonomicName baseAuthorityName="Say" baseAuthorityYear="1821" box="[940,1204,1483,1509]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="variabilis">
