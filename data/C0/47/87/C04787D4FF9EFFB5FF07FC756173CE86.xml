<taxonomicName authority="Lahille, 1905" authorityName="Lahille" authorityYear="1905" box="[562,857,1087,1113]" class="Arachnida" family="Ixodidae" genus="Aponomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="119" pageNumber="120" phylum="Arthropoda" rank="species" species="soembawensis">
<emphasis box="[562,857,1087,1113]" italics="true" pageId="119" pageNumber="120">Aponomma soembawensis</emphasis>
</taxonomicName>
