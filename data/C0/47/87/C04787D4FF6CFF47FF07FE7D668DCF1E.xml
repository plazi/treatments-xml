<taxonomicName authority="Marx" authorityName="Marx" box="[571,912,619,645]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="variegatus">
