<taxonomicName authorityName="Bowdich" authorityYear="1821" box="[212,328,619,645]" class="Mammalia" kingdom="Animalia" order="Carnivora" pageId="107" pageNumber="108" phylum="Chordata" rank="order">Carnivora</taxonomicName>
