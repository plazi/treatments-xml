<taxonomicName authorityName="Morel and Vassiliades" authorityYear="1963" box="[749,1011,834,860]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="223" pageNumber="224" phylum="Arthropoda" rank="species" species="guilhoni">
<emphasis box="[749,1011,834,860]" italics="true" pageId="223" pageNumber="224">Rhipicephalus guilhoni</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Morel, Mouchet and Rodhain" authorityYear="1976" box="[1068,1336,834,860]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="223" pageNumber="224" phylum="Arthropoda" rank="species" species="camicasi">
<emphasis box="[1068,1336,834,860]" italics="true" pageId="223" pageNumber="224">Rhipicephalus camicasi</emphasis>
</taxonomicName>
