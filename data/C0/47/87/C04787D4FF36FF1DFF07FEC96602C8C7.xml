<taxonomicName authorityName="Morel and Vassiliades" authorityYear="1963" box="[309,569,870,896]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="223" pageNumber="224" phylum="Arthropoda" rank="species" species="guilhoni">
