<taxonomicName authorityName="Cantor" authorityYear="1847" box="[536,742,1735,1761]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="geoemydae">
