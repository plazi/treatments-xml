<taxonomicName authorityName="Schulze" authorityYear="1935" box="[897,1150,1699,1725]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="233" pageNumber="234" phylum="Arthropoda" rank="species" species="pumilio">
