<taxonomicName authorityName="Olenev" authorityYear="1929" box="[766,1023,1735,1761]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="233" pageNumber="234" phylum="Arthropoda" rank="species" species="schulzei">
