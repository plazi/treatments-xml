<taxonomicName authorityName="Yakimov and Kohl-Yakimova" authorityYear="1911" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="233" pageNumber="234" phylum="Arthropoda" rank="species" species="rossicus">
