<paragraph blockId="206.[151,1437,726,1366]" box="[151,530,726,753]" pageId="206" pageNumber="207">
<emphasis bold="true" box="[151,187,726,752]" pageId="206" pageNumber="207">15.</emphasis>
<taxonomicName authority="Sharif, 1928" authorityName="Sharif" authorityYear="1928" box="[199,526,726,753]" class="Arachnida" family="Ixodidae" genus="Hyalomma" kingdom="Animalia" order="Ixodida" pageId="206" pageNumber="207" phylum="Arthropoda" rank="species" species="isaaci">
<emphasis bold="true" box="[199,389,726,753]" italics="true" pageId="206" pageNumber="207">Hyalomma isaaci</emphasis>
<bibRefCitation author="Sharif, M." box="[394,526,727,753]" pageId="206" pageNumber="207" pagination="217 - 344" refId="ref246264" refString="Sharif, M. (1928) A revision of the Indian Ixodidae with special reference to the collection in the Indian Museum. Records of the Indian Museum, 30, 217 - 344." type="journal article" year="1928">Sharif, 1928</bibRefCitation>
