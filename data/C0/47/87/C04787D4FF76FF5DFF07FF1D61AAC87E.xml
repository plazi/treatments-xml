, under the name 
<taxonomicName authority=", Teng and Jiang (1991)" authorityName=", Teng and Jiang" authorityYear="1991" box="[527,1122,547,573]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="159" pageNumber="160" phylum="Arthropoda" rank="species" species="xinjiangensis">
<emphasis box="[527,855,547,573]" italics="true" pageId="159" pageNumber="160">Haemaphysalis xinjiangensis</emphasis>
, 
<bibRefCitation author="Teng, K. F. &amp; Jiang, Z." box="[868,1122,547,573]" pageId="159" pageNumber="160" refId="ref250405" refString="Teng, K. F. &amp; Jiang, Z. J (1991) Economic insect fauna of China. Fascicule 39, Acari: Ixodidae. Science Press, Beijing, 355 pp. [in Chinese]" type="book" year="1991">Teng and Jiang (1991)</bibRefCitation>
</taxonomicName>
, 
<bibRefCitation author="Yu, X. &amp; Ye, R. &amp; Dong, Z." box="[1132,1308,547,573]" pageId="159" pageNumber="160" refId="ref256456" refString="Yu, X., Ye, R. &amp; Dong, Z. (1997) The tick fauna of Xinjiang. Xinjiang Scientific, Technological and Medical Publishing House, Urumqi, 169 pp. [in Chinese]" type="book" year="1997">
Yu 
<emphasis box="[1170,1226,547,573]" italics="true" pageId="159" pageNumber="160">et al.</emphasis>
(1997)
</bibRefCitation>
</paragraph>
