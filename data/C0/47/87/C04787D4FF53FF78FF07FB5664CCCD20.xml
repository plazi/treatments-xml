<taxonomicName authorityName="in Matthysse and Colbo" authorityYear="1987" box="[695,980,1577,1603]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="186" pageNumber="187" phylum="Arthropoda" rank="species" species="spinulosa">
