<emphasis bold="true" box="[151,216,1577,1603]" pageId="186" pageNumber="187">Note:</emphasis>
<taxonomicName authorityName="Tomlinson and Apanaskevich" authorityYear="2019" box="[222,497,1577,1603]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="186" pageNumber="187" phylum="Arthropoda" rank="species" species="princeps">
