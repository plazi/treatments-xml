<taxonomicName authorityName="Koch" authorityYear="1844" box="[875,1174,1590,1616]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="219" pageNumber="220" phylum="Arthropoda" rank="species" species="decoloratus">
