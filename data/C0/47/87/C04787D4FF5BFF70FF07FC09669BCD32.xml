<taxonomicName authorityName="Nuttall" authorityYear="1912" box="[547,878,1591,1617]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="178" pageNumber="179" phylum="Arthropoda" rank="species" species="montgomeryi">
