<taxonomicName authority="Linnæus, 1758" authorityName="Fabricius" authorityYear="1794" box="[573,778,655,681]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="127" pageNumber="128" phylum="Arthropoda" rank="species" species="variegatus">
