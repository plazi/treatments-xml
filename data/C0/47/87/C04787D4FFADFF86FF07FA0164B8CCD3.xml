<taxonomicName authorityName="Birula" authorityYear="1895" box="[1073,1304,1878,1904]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="trianguliceps">
