<taxonomicName authorityName="Bowdich" authorityYear="1821" box="[648,764,186,212]" class="Mammalia" kingdom="Animalia" order="Carnivora" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Carnivora</taxonomicName>
: 
<taxonomicName box="[774,905,186,212]" class="Mammalia" family="Mephitidae" kingdom="Animalia" order="Carnivora" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Mephitidae</taxonomicName>
, 
<taxonomicName box="[915,1039,186,212]" class="Mammalia" family="Mustelidae" kingdom="Animalia" order="Carnivora" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Mustelidae</taxonomicName>
and 
<taxonomicName authorityName="Gray" authorityYear="1821" box="[1093,1214,186,212]" class="Mammalia" family="Viverridae" kingdom="Animalia" order="Carnivora" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Viverridae</taxonomicName>
, and 
<taxonomicName box="[1271,1433,186,212]" class="Aves" kingdom="Animalia" order="Passeriformes" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
: 
<taxonomicName box="[151,310,222,248]" class="Aves" family="Muscicapidae" kingdom="Animalia" order="Passeriformes" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Muscicapidae</taxonomicName>
; nymphs alone have been recovered from 
<taxonomicName authorityName="Gregory" authorityYear="1910" box="[785,948,222,248]" class="Mammalia" kingdom="Animalia" order="Soricomorpha" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Soricomorpha</taxonomicName>
: 
<taxonomicName box="[959,1068,222,248]" class="Mammalia" family="Soricidae" kingdom="Animalia" order="Soricomorpha" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Soricidae</taxonomicName>
, and 
<taxonomicName box="[1127,1233,222,248]" class="Mammalia" kingdom="Animalia" order="Rodentia" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Rodentia</taxonomicName>
: 
<taxonomicName box="[1243,1352,222,248]" class="Mammalia" family="Sciuridae" kingdom="Animalia" order="Rodentia" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Sciuridae</taxonomicName>
. 
<taxonomicName class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="spinicoxalis">
<emphasis italics="true" pageId="67" pageNumber="68">Ixodes spinicoxalis</emphasis>
</taxonomicName>
is a very rare parasite of humans.
</paragraph>
