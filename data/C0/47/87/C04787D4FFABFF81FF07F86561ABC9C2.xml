<subSubSection pageId="67" pageNumber="68" type="description">
<paragraph blockId="67.[151,1437,150,645]" pageId="67" pageNumber="68">
<taxonomicName box="[151,278,150,176]" class="Mammalia" kingdom="Animalia" order="Scandentia" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Scandentia</taxonomicName>
: 
<taxonomicName authorityName="Gray" authorityYear="1825" box="[288,403,150,176]" class="Mammalia" family="Tupaiidae" kingdom="Animalia" order="Scandentia" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Tupaiidae</taxonomicName>
; adults and immature stages have been found on 
<taxonomicName box="[952,1058,150,176]" class="Mammalia" kingdom="Animalia" order="Rodentia" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Rodentia</taxonomicName>
: 
<taxonomicName box="[1068,1166,150,176]" class="Mammalia" family="Muridae" kingdom="Animalia" order="Rodentia" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Muridae</taxonomicName>
, and 
<taxonomicName box="[1224,1386,150,176]" class="Aves" kingdom="Animalia" order="Passeriformes" pageId="67" pageNumber="68" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
: 
<taxonomicName authorityName="Swainson" authorityYear="1831" class="Aves" family="Pittidae" kingdom="Animalia" order="Passeriformes" pageId="67" pageNumber="68" phylum="Chordata" rank="family">Pittidae</taxonomicName>
