<taxonomicName authorityName="Warburton" authorityYear="1908" box="[726,1050,727,753]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="154" pageNumber="155" phylum="Arthropoda" rank="species" species="campanulata">
