<taxonomicName authority="Lahille, 1905" authorityName="Lahille" authorityYear="1905" baseAuthorityName="Kaufman" baseAuthorityYear="1972" box="[613,741,1555,1581]" class="Arachnida" family="Ixodidae" genus="Aponomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="88" pageNumber="89" phylum="Arthropoda" rank="genus">
<emphasis box="[613,741,1555,1581]" italics="true" pageId="88" pageNumber="89">Aponomma</emphasis>
</taxonomicName>
