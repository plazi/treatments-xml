<paragraph blockId="189.[151,1437,1050,1617]" pageId="189" pageNumber="190">
<emphasis bold="true" box="[151,216,1555,1581]" pageId="189" pageNumber="190">Note:</emphasis>
<taxonomicName authorityName="Santos Dias" authorityYear="1956" box="[222,477,1555,1581]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="189" pageNumber="190" phylum="Arthropoda" rank="species" species="rugosa">
<emphasis box="[222,477,1555,1581]" italics="true" pageId="189" pageNumber="190">Haemaphysalis rugosa</emphasis>
