<taxonomicName authorityName="Mendez Arocha and Ortiz" authorityYear="1958" box="[722,889,331,357]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="lasallei">
