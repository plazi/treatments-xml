<taxonomicName authorityName="Warburton" authorityYear="1910" box="[588,729,1087,1113]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="gigas">
