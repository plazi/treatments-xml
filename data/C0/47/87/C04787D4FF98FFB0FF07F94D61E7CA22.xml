<taxonomicName authorityName=", Robinson" authorityYear="1926" box="[1212,1436,294,320]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="pictum">
