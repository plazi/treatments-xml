<taxonomicName authority="Sharif, 1928" authorityName="Sharif" authorityYear="1928" box="[199,664,1878,1905]" class="Arachnida" family="Ixodidae" genus="Hyalomma" kingdom="Animalia" order="Ixodida" pageId="202" pageNumber="203" phylum="Arthropoda" rank="species" species="brevipunctatum">
