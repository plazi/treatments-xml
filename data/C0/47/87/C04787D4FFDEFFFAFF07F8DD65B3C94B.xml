<taxonomicName authorityName=", Nemenz" authorityYear="1968" box="[514,648,220,246]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="kerri">
<emphasis box="[514,648,220,246]" italics="true" pageId="56" pageNumber="57">Ixodes kerri</emphasis>
</taxonomicName>
