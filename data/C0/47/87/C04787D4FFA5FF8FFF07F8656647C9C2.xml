<taxonomicName authorityName="Nuttall" authorityYear="1916" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="victoriensis">
<emphasis italics="true" pageId="77" pageNumber="78">Ixodes victoriensis</emphasis>
</taxonomicName>
