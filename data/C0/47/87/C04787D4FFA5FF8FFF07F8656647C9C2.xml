<taxonomicName authorityName="Owen" authorityYear="1866" box="[151,313,151,177]" class="Mammalia" kingdom="Animalia" order="Diprotodontia" pageId="77" pageNumber="78" phylum="Chordata" rank="order">Diprotodontia</taxonomicName>
: 
<taxonomicName authorityName="Gray" authorityYear="1821" box="[322,443,151,177]" class="Mammalia" family="Potoroidae" kingdom="Animalia" order="Diprotodontia" pageId="77" pageNumber="78" phylum="Chordata" rank="family">Potoroidae</taxonomicName>
