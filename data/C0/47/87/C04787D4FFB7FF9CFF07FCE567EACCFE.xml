<taxonomicName box="[1327,1433,943,969]" class="Mammalia" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="order">Rodentia</taxonomicName>
: 
<taxonomicName box="[151,313,979,1005]" class="Mammalia" family="Dasyproctidae" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Dasyproctidae</taxonomicName>
and 
<taxonomicName authority=", Crocodilia" authorityName="Crocodilia" box="[374,677,979,1005]" class="Mammalia" family="Erethizontidae" kingdom="Animalia" order="Rodentia" pageId="94" pageNumber="95" phylum="Chordata" rank="family">Erethizontidae, Crocodilia</taxonomicName>
