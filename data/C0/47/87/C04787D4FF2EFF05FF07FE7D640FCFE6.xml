<paragraph blockId="199.[151,1437,474,1185]" pageId="199" pageNumber="200">
<emphasis bold="true" box="[151,216,1087,1113]" pageId="199" pageNumber="200">Note:</emphasis>
although 
<taxonomicName authorityName="Nuttall and Warburton" authorityYear="1908" box="[336,646,1087,1113]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="wellingtoni">
