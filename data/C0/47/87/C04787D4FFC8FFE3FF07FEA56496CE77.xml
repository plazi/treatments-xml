<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[549,707,654,680]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="ricinus">
<emphasis box="[549,707,654,680]" italics="true" pageId="33" pageNumber="34">Ixodes ricinus</emphasis>
</taxonomicName>
