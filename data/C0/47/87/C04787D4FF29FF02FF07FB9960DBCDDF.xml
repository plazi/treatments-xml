<paragraph blockId="192.[151,1437,1086,1689]" pageId="192" pageNumber="193">
<emphasis bold="true" box="[151,216,1626,1652]" pageId="192" pageNumber="193">Note:</emphasis>
see 
<taxonomicName authorityName="Kohls" authorityYear="1950" box="[263,507,1626,1652]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="192" pageNumber="193" phylum="Arthropoda" rank="species" species="minuta">
