<taxonomicName baseAuthorityName="Latreille" baseAuthorityYear="1806" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="216" pageNumber="217" phylum="Arthropoda" rank="species" species="sanguineus">
