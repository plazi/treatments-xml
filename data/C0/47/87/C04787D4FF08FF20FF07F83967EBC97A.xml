<taxonomicName authorityName="Pomerantzev" authorityYear="1946" box="[644,889,511,537]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="226" pageNumber="227" phylum="Arthropoda" rank="species" species="leporis">
