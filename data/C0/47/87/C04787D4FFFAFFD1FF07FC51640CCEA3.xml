<emphasis box="[151,335,1482,1508]" italics="true" pageId="19" pageNumber="20">
<taxonomicName authorityName="Cooley and Kohls" authorityYear="1943" box="[151,331,1482,1508]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="conepati">Ixodes conepati</taxonomicName>
