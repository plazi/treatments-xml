<collectingRegion box="[1194,1234,438,464]" country="South Sudan" name="Lakes" pageId="103" pageNumber="104">See</collectingRegion>
also 
<taxonomicName authorityName="Keirans, Bull and Duffield" authorityYear="1996" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="103" pageNumber="104" phylum="Arthropoda" rank="species" species="vikirri">
<emphasis italics="true" pageId="103" pageNumber="104">Amblyomma vikirri</emphasis>
</taxonomicName>
