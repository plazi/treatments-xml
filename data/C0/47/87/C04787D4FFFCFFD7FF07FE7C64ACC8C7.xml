<emphasis box="[250,439,835,861]" italics="true" pageId="21" pageNumber="22">
<taxonomicName box="[250,435,835,861]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="auritulus">Ixodes auritulus</taxonomicName>
