<paragraph blockId="21.[151,1436,474,897]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[151,216,799,825]" pageId="21" pageNumber="22">Note:</emphasis>
<taxonomicName authorityName="Arthur" authorityYear="1960" box="[224,396,799,825]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="cornuae">
