<taxonomicName authorityName="Gerstacker" authorityYear="1873" box="[590,862,619,645]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="233" pageNumber="234" phylum="Arthropoda" rank="species" species="pulchellus">
