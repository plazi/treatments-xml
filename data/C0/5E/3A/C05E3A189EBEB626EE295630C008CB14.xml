<taxonomicName authorityName="Parmasto" authorityYear="1968" baseAuthorityName="Pat." class="Basidiomycetes" family="Fabaceae" genus="Subulicystidium" kingdom="Fungi" lsidName="Subulicystidium longisporum" order="Trechisporales" pageId="0" pageNumber="41" phylum="Basidiomycota" rank="species" species="longisporum">
<emphasis italics="true" pageId="0" pageNumber="41">Subulicystidium longisporum</emphasis>
