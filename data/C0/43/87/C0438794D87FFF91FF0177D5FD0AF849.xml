and 
<taxonomicName authorityName="Broun" authorityYear="1881" box="[1253,1429,1992,2018]" class="Insecta" family="Staphylinidae" genus="Bryaxis" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="paganus" subGenus="Byraxis">
