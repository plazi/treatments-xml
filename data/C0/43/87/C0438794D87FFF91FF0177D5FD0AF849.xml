<taxonomicName box="[791,972,1992,2018]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="sylvicola" subGenus="Byraxis">
