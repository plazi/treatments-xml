) 
<emphasis box="[872,972,1992,2018]" italics="true" pageId="61" pageNumber="62">sylvicola</emphasis>
</taxonomicName>
, 
<taxonomicName box="[986,1198,1992,2018]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="rhyssarthra" subGenus="Byraxis">
<emphasis box="[986,1003,1992,2017]" italics="true" pageId="61" pageNumber="62">E</emphasis>
