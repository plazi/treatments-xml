<subSubSection pageId="59" pageNumber="60" type="etymology">
<paragraph blockId="59.[151,1437,1671,1986]" pageId="59" pageNumber="60">
<emphasis bold="true" box="[199,338,1708,1734]" pageId="59" pageNumber="60">Etymology:</emphasis>
