<typeStatus box="[391,489,1791,1817]" pageId="74" pageNumber="75">holotype</typeStatus>
of 
<emphasis box="[523,540,1792,1817]" italics="true" pageId="74" pageNumber="75">E</emphasis>
. 
<taxonomicName baseAuthorityName="Broun" baseAuthorityYear="1881" box="[552,682,1791,1817]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="ignotus" subGenus="Byraxis">
