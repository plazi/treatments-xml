<taxonomicName authorityName="King. A" authorityYear="1866" box="[729,819,1864,1889]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="genus">
