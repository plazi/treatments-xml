<subSubSection pageId="45" pageNumber="46" type="etymology">
<paragraph blockId="45.[151,1437,152,755]" pageId="45" pageNumber="46">
<emphasis bold="true" box="[199,338,621,647]" pageId="45" pageNumber="46">Etymology:</emphasis>
