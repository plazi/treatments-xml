<quantity box="[603,721,295,319]" metricMagnitude="-2" metricUnit="m" metricValue="3.81" metricValueMax="4.064" metricValueMin="3.5559999999999996" pageId="4" pageNumber="591" unit="in" value="1.5" valueMax="1.6" valueMin="1.4">1.4–1.6 in</quantity>
