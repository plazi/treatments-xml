<quantity box="[232,350,295,319]" metricMagnitude="-1" metricUnit="m" metricValue="1.4986" metricValueMax="1.6001999999999998" metricValueMin="1.397" pageId="4" pageNumber="591" unit="in" value="5.9" valueMax="6.3" valueMin="5.5">5.5–6.3 in</quantity>
