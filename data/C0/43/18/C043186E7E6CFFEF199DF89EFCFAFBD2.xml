<quantity box="[1322,1436,332,356]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" metricValueMax="8.382" metricValueMin="6.858" pageId="4" pageNumber="591" unit="in" value="3.0" valueMax="3.3" valueMin="2.7">2.7–3.3 in</quantity>
