<quantity box="[1305,1436,295,319]" metricMagnitude="-2" metricUnit="m" metricValue="3.3655" metricValueMax="3.81" metricValueMin="2.921" pageId="4" pageNumber="591" unit="in" value="1.325" valueMax="1.5" valueMin="1.15">1.15–1.5 in</quantity>
