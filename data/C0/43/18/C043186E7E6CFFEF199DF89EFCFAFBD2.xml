<quantity box="[774,888,367,391]" metricMagnitude="-1" metricUnit="m" metricValue="1.0032999999999999" metricValueMax="1.1683999999999999" metricValueMin="0.8382" pageId="4" pageNumber="591" unit="in" value="3.9499999999999997" valueMax="4.6" valueMin="3.3">3.3–4.6 in</quantity>
