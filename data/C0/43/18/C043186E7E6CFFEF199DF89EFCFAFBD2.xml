<quantity box="[618,758,260,284]" metricMagnitude="-1" metricUnit="m" metricValue="1.1557" metricValueMax="1.2827" metricValueMin="1.0287" pageId="4" pageNumber="591" unit="in" value="4.55" valueMax="5.05" valueMin="4.05">4.05–5.05 in</quantity>
