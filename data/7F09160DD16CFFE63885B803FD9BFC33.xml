<emphasis id="C5D47B09D16DFFE63885BE93FF1AFF42" box="[151,177,230,255]" italics="true" pageId="7" pageNumber="60">ps</emphasis>
<number id="4168B53CD16DFFE638A3BE80FF12FEBE" box="[177,185,245,259]" pageId="7" pageNumber="60" value="2.0">2</number>
