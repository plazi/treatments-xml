<number id="4168B53CD16CFFE738EEB9F7FEB7F821" box="[252,284,1922,1948]" pageId="6" pageNumber="59" value="12.0">12</number>
</date>
, 
<emphasis id="C5D47B09D16CFFE7393AB9F7FEEBF826" box="[296,320,1922,1947]" italics="true" pageId="6" pageNumber="59">sc</emphasis>
