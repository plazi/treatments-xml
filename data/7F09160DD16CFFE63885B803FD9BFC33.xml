<number id="4168B53CD16DFFE63910BFE6FEA1FE1C" box="[258,266,403,417]" pageId="7" pageNumber="60" value="2.0">2</number>
<number id="4168B53CD16DFFE63903BFF6FE9AFE20" box="[273,305,387,413]" pageId="7" pageNumber="60" value="12.0">12</number>
