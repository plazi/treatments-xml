<number id="4168B53CD16DFFE63D45BF1EFAF4FEC4" box="[1367,1375,363,377]" pageId="7" pageNumber="60" value="2.0">2</number>
<number id="4168B53CD16DFFE63D74BF2EFA28FEC8" box="[1382,1411,347,373]" pageId="7" pageNumber="60" value="18.0">18</number>
&gt; φ 
<number id="4168B53CD16DFFE638BABFE6FF1BFE1C" box="[168,176,403,417]" pageId="7" pageNumber="60" value="1.0">1</number>
<number id="4168B53CD16DFFE638A5BFF6FF78FE20" box="[183,211,387,413]" pageId="7" pageNumber="60" value="11.0">11</number>
