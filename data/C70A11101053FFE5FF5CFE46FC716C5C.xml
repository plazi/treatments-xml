<number id="F96BB2211053FFE6FBEFFDFEFBF56F4F" box="[1060,1125,533,559]" pageId="1" pageNumber="44" value="2009.0">2009</number>
. 
<date id="3B1D86C61053FFE6FBA2FDFEFB266F4F" box="[1129,1206,533,559]" pageId="1" pageNumber="44" value="1918-07">
