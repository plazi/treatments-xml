Strobl (= 
<taxonomicName id="4C6B7AB8FFF4FFF661FCF96AFA02F948" authority="Becker" authorityName="Becker" authorityYear="1907" box="[1258,1430,1703,1725]" class="Insecta" family="Empididae" genus="Empis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="95" phylum="Arthropoda" rank="species" species="suberis">
