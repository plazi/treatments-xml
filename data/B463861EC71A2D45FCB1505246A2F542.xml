<quantity id="FB329AEDC71A2D45FB4F51DE474CF68F" box="[1236,1316,1452,1476]" metricMagnitude="5" metricUnit="m" metricValue="1.64" pageId="17" pageNumber="746" unit="km" value="164.0">164 km</quantity>
