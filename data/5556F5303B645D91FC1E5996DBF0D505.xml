<taxonomicName id="76974FC6757FAB1777D7BAEAC2F42973" authority="Olmi, 1984" authorityName="Olmi" authorityYear="1984" class="Insecta" family="Dryinidae" genus="Esagonatopus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Esagonatopus niger" order="Hymenoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="species" species="niger">Esagonatopus niger</taxonomicName>
