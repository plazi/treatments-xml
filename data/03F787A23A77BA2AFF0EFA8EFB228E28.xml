<typeStatus id="54E588163A77BA28FF0EF97DFF0A8D52" box="[151,204,1590,1613]" pageId="1" pageNumber="42">Type</typeStatus>
species. 
<taxonomicName id="4C5E4D373A77BA28FEB5F97DFD8C8D52" box="[300,586,1590,1613]" class="Actinopterygii" family="Cichlidae" genus="Enigmatochromis" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="42" phylum="Chordata" rank="species" species="lucanusi" status="new species">
<emphasis id="B92AEAA63A77BA28FEB5F97DFD8C8D52" box="[300,586,1590,1613]" italics="true" pageId="1" pageNumber="42">Enigmatochromis lucanusi</emphasis>
</taxonomicName>
, 
