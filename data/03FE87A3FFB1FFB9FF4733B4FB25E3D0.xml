<typeStatus id="54EC8817FFB1FFBEFF47342AFF73E678" box="[151,204,1091,1114]" pageId="35" pageNumber="36">Type</typeStatus>
species: 
<taxonomicName id="4C574D36FFB1FFBEFEFD342AFD20E678" authority="Haworth, 1828" authorityName="Haworth" authorityYear="1828" box="[301,671,1091,1114]" class="Insecta" family="Gelechiidae" genus="Recurvaria" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="diffinis">
