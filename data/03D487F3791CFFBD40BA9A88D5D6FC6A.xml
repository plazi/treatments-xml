<taxonomicName id="4C7D4D66791CFFBD40BA9B6FD549FCF3" box="[491,619,816,840]" class="Mammalia" family="Galagidae" kingdom="Animalia" order="Primates" pageId="20" pageNumber="284" phylum="Chordata" rank="family">Galagidae</taxonomicName>
