<taxonomicName id="13984D1FFFD3FFE369B3F8155232A3BC" authority="Cranston et Edward" authorityName="Cranston et Edward" authorityYear="1999" box="[905,1307,1961,1987]" class="Insecta" family="Chironomidae" genus="Botryocladius" kingdom="Animalia" order="Diptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="genus">
