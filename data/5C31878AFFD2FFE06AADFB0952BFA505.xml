<taxonomicName id="13984D1FFFD0FFE06AC2FF2456D8A4CE" authorityName="Goetghebuer" authorityYear="1932" box="[248,497,152,177]" class="Insecta" family="Chironomidae" genus="Gymnometriocnemus" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">
