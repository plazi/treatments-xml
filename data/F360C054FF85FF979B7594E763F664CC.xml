<taxonomicName id="BCC90AC1FF85FF94994092AC62236F0F" authority="Assing, 2013" authorityName="Assing" authorityYear="2013" box="[266,529,2029,2050]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="38" phylum="Arthropoda" rank="species" species="fortepunctatum" status="sp. n.">
