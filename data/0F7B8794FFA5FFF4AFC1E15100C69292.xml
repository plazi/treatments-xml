<taxonomicName id="40D24D01FFA5FFF4AA66E34F00C6926D" authorityName="Calvert" authorityYear="1903" box="[1328,1436,1795,1817]" class="Insecta" family="Gomphidae" genus="Epigomphus" kingdom="Animalia" order="Odonata" pageId="19" pageNumber="92" phylum="Arthropoda" rank="species" species="quadracies">
