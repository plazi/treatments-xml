, 
<collectingRegion id="EC20F8326668A977FF4FF5A4FE5FFA22" box="[242,403,1436,1460]" country="Afghanistan" name="Kabul" pageId="1" pageNumber="226">Kabul Province</collectingRegion>
, 
<quantity id="E91C9B356668A977FE20F5A4FE1FFA22" box="[413,467,1436,1460]" metricMagnitude="3" metricUnit="m" metricValue="5.0" pageId="1" pageNumber="226" unit="km" value="5.0">5 km</quantity>
