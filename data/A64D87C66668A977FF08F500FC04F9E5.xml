</materialsCitation>
<collectingRegion id="EC20F8326668A977FDB3F5A4FD83FA22" box="[526,591,1436,1460]" country="Afghanistan" name="Kabul" pageId="1" pageNumber="226">Kabul</collectingRegion>
, Kargha locality, 
<quantity id="E91C9B356668A977FD42F5A4FC9EFA25" box="[767,850,1436,1460]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="1" pageNumber="226" unit="m" value="2000.0">2000 m</quantity>
, 15- 
<date id="5A5A10106668A977FCC1F5A4FC37FA22" box="[892,1019,1436,1460]" pageId="1" pageNumber="226" value="2010-05-16">16.05.2010</date>
