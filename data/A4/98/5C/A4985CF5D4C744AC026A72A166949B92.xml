<taxonomicName class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Strumigenys dyschima" order="Hymenoptera" pageId="33" pageNumber="50" phylum="Arthropoda" rank="species" species="dyschima">Strumigenys dyschima</taxonomicName>
