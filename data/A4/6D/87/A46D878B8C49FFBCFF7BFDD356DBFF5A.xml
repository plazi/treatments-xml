</taxonomicName>
, 
<taxonomicName box="[1057,1169,189,214]" class="Insecta" family="Cicadellidae" genus="Rhabdotalebra" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="plummeri">
<emphasis box="[1057,1169,189,214]" italics="true" pageId="5" pageNumber="58">plummeri</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1183,1281,189,214]" class="Insecta" family="Cicadellidae" genus="Rhabdotalebra" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="brunnea">
<emphasis box="[1183,1281,189,214]" italics="true" pageId="5" pageNumber="58">brunnea</emphasis>
</taxonomicName>
and 
<emphasis box="[1338,1424,189,214]" italics="true" pageId="5" pageNumber="58">
<taxonomicName box="[1338,1420,189,214]" class="Insecta" family="Cicadellidae" genus="Rhabdotalebra" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="ornata">ornata</taxonomicName>
