<paragraph pageId="8" pageNumber="45">Distribution.</paragraph>
<paragraph pageId="8" pageNumber="45">
<taxonomicName class="Hexapoda" family="Vespidae" genus="Vespula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="8" pageNumber="45" phylum="Arthropoda" rank="species" species="consobrina">
