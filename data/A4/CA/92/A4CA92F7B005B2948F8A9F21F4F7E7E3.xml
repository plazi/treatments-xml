<taxonomicName class="Insecta" family="Crambidae" genus="Asturodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Asturodes fimbriauralis" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="fimbriauralis">
