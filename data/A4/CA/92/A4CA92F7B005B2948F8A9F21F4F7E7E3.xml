) feeding on 
<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Guettarda" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Guettarda macrosperma" order="Gentianales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="macrosperma">
<emphasis italics="true" pageId="0" pageNumber="1">Guettarda macrosperma</emphasis>
</taxonomicName>
(
<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Guettarda" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Gentianales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
