<subSubSection pageId="0" pageNumber="1" type="etymology">
<paragraph pageId="0" pageNumber="1">Etymology.</paragraph>
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Hormius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hormius juanmatai" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="juanmatai">
<emphasis italics="true" pageId="0" pageNumber="1">Hormius juanmatai</emphasis>
</taxonomicName>
