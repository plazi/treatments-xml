<paragraph pageId="0" pageNumber="1">
Guanacaste, Sector Mundo Nuevo, Mamones, 
<geoCoordinate degrees="10.77074" direction="north" orientation="latitude" precision="1" value="10.77074">10.77074</geoCoordinate>
