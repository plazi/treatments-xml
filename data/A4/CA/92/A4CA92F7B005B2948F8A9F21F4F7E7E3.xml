<taxonomicName authorityName="Amsel" authorityYear="1956" class="Insecta" family="Crambidae" genus="Palpusia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Palpusia" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
