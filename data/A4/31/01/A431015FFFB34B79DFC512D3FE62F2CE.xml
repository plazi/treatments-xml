<tr box="[159,1429,845,866]" gridrow="4" pageId="12" pageNumber="13" rowspan-0="1">
<td box="[323,591,845,866]" gridcol="1" gridrow="4" pageId="12" pageNumber="13">
Microbasic 
<emphasis box="[437,449,845,866]" italics="true" pageId="12" pageNumber="13">p</emphasis>
–mastigophore
</td>
<td box="[647,1178,845,866]" gridcol="2" gridrow="4" pageId="12" pageNumber="13">None seen</td>
<td box="[1227,1429,845,866]" gridcol="3" gridrow="4" pageId="12" pageNumber="13">None seen</td>
