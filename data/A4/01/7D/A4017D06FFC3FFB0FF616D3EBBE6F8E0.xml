<quantity box="[543,632,1629,1655]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="23" pageNumber="24" unit="mm" value="0.7">0.7 mm</quantity>
. LID 
<quantity box="[697,800,1629,1655]" metricMagnitude="-4" metricUnit="m" metricValue="6.6" pageId="23" pageNumber="24" unit="mm" value="0.66">0.66 mm</quantity>
