<paragraph blockId="56.[151,633,507,562]" box="[151,633,507,532]" pageId="56" pageNumber="57">
<typeStatus box="[151,201,508,532]" pageId="56" pageNumber="57">Type</typeStatus>
genus: 
<taxonomicName authority="Balfour-Browne, 1945" authorityName="Balfour-Browne" authorityYear="1945" box="[280,633,507,531]" class="Insecta" family="Brentidae" genus="Lepanomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">
<emphasis box="[280,397,508,531]" italics="true" pageId="56" pageNumber="57">Lepanomus</emphasis>
<bibRefCitation author="Balfour-Browne, J." box="[403,633,507,531]" pageId="56" pageNumber="57" pagination="55 - 60" refId="ref43609" refString="Balfour-Browne, J. (1945) A contribution to the study of the fauna of Ukerewe Island, Victoria Nyanza (Col.: Curculionidae; Apioninae). Journal of the East Africa Natural History Society, 18 (1 - 2), 55 - 60." type="journal article" year="1945">Balfour-Browne, 1945</bibRefCitation>
