<paragraph blockId="23.[160,1155,142,306]" box="[160,386,142,167]" pageId="23" pageNumber="1535">
<heading box="[160,386,142,167]" fontSize="11" level="3" pageId="23" pageNumber="1535" reason="5">
<emphasis bold="true" box="[160,386,142,167]" inLineHeading="true" italics="true" pageId="23" pageNumber="1535" reason="3">Comparative notes</emphasis>
</heading>
</paragraph>
<paragraph blockId="23.[160,1155,142,306]" pageId="23" pageNumber="1535">
Externally, 
<taxonomicName authorityName="Leal &amp; Creao-Duarte" authorityYear="2016" box="[284,440,178,202]" class="Insecta" family="Cicadellidae" genus="Scopogonalia" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="1535" phylum="Arthropoda" rank="species" species="agkistroides">
<emphasis box="[284,440,178,202]" italics="true" pageId="23" pageNumber="1535">S. agkistroides</emphasis>
</taxonomicName>
resembles 
<emphasis box="[572,964,178,202]" italics="true" pageId="23" pageNumber="1535">
<taxonomicName authorityName="Cavichioli" authorityYear="1986" box="[572,699,178,202]" class="Insecta" family="Cicadellidae" genus="Scopogonalia" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="1535" phylum="Arthropoda" rank="species" species="altmanni">S. altmanni</taxonomicName>
, 
<taxonomicName authorityName="Young" authorityYear="1977" box="[712,836,178,202]" class="Insecta" family="Cicadellidae" genus="Scopogonalia" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="1535" phylum="Arthropoda" rank="species" species="echinura">S. echinura</taxonomicName>
, 
<taxonomicName baseAuthorityName="Leal" baseAuthorityYear="2014" box="[849,964,178,202]" class="Insecta" family="Cicadellidae" genus="Scopogonalia" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="1535" phylum="Arthropoda" rank="species" species="oglobini">S. oglobini</taxonomicName>
</emphasis>
and 
<taxonomicName authority="Leal &amp; Creão-Duarte &amp; Mejdalani, 2016" authorityName="Leal &amp; Creão-Duarte &amp; Mejdalani" authorityYear="2016" box="[1026,1155,178,202]" class="Insecta" family="Cicadellidae" genus="Scopogonalia" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="1535" phylum="Arthropoda" rank="species" species="dolixoura" status="sp. nov.">
<emphasis box="[1026,1155,178,202]" italics="true" pageId="23" pageNumber="1535">S. dolixoura</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[160,249,212,236]" pageId="23" pageNumber="1535" rank="species">sp. nov.</taxonomicNameLabel>
