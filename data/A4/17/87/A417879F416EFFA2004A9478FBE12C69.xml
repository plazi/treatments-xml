<collectingCountry box="[439,494,1837,1863]" name="Italy" pageId="3" pageNumber="56">Italy</collectingCountry>
, 
<collectingCountry box="[507,655,1837,1863]" name="North Korea" pageId="3" pageNumber="56">North Korea</collectingCountry>
