<collectingRegion box="[517,590,846,867]" country="Italy" name="Veneto" pageId="4" pageNumber="957">Veneto</collectingRegion>
, 
<collectingRegion box="[603,692,846,867]" country="Italy" name="Toscana" pageId="4" pageNumber="957">Tuscany</collectingRegion>
, 
<collectingRegion box="[702,777,845,867]" country="Italy" name="Lazio" pageId="4" pageNumber="957">Latium</collectingRegion>
, 
<collectingRegion box="[787,877,845,867]" country="Italy" name="Abruzzo" pageId="4" pageNumber="957">Abruzzo</collectingRegion>
, and 
<collectingRegion box="[930,1018,845,867]" country="Italy" name="Sardegna" pageId="4" pageNumber="957">Sardinia</collectingRegion>
