<quantity id="4CF39B4DFFA6A506CC39C21EFC0CA44B" box="[788,917,590,616]" metricMagnitude="-2" metricUnit="m" metricValue="5.2" metricValueMax="6.8" metricValueMin="3.6" pageId="31" pageNumber="32" unit="mm" value="52.0" valueMax="68.0" valueMin="36.0">36–68 mm</quantity>
; SW 
<quantity id="4CF39B4DFFA6A506CCF8C21EFBCFA44B" box="[981,1110,590,616]" metricMagnitude="-2" metricUnit="m" metricValue="2.45" metricValueMax="3.2" metricValueMin="1.7" pageId="31" pageNumber="32" unit="mm" value="24.5" valueMax="32.0" valueMin="17.0">17–32 mm</quantity>
