<taxonomicName id="87EF361F7DF96B6D68D9E36AB7A5A1BD" LSID="http://zoobank.org/E7A1215B-D41F-4721-89E9-226176994F0C" class="Clitellata" family="Enchytraeidae" genus="Claparedrilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Claparedrilus" order="Enchytraeida" pageId="35" pageNumber="50" phylum="Annelida" rank="genus">Claparedrilus</taxonomicName>
