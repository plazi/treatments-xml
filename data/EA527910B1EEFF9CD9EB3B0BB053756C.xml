<paragraph id="464B9EB62218109C7EA0C4CFAD5A637F" pageId="35" pageNumber="50">
<taxonomicName id="ECE3DDC42E72285591B1570276459FEA" class="Clitellata" family="Enchytraeidae" genus="Claparedrilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Claparedrilus semifuscus" order="Enchytraeida" pageId="35" pageNumber="50" phylum="Annelida" rank="species" species="semifuscus">Claparedrilus semifuscus</taxonomicName>
