<collectingCountry id="F340A9EEFFA33D0DDC95F984FBAFF9F3" box="[1003,1100,1543,1570]" name="Sweden" pageId="86" pageNumber="87">Sweden</collectingCountry>
), uppermost Red 
<taxonomicName id="4C5792FDFFA33D0DDA60F98BFEDAF995" authority="Limestone" authorityName="Limestone" class="Cephalopoda" family="Lituitidae" genus="Lituites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthocerida" pageId="86" pageNumber="87" phylum="Mollusca" rank="genus">
