<paragraph id="8BA636A09E497466FA61288DD28FDDBF" blockId="31.[264,689,1249,1275]" box="[264,689,1249,1275]" pageId="31" pageNumber="32">
<heading id="D0EE81CC9E497466FA61288DD28FDDBF" bold="true" box="[264,689,1249,1275]" fontSize="11" level="1" pageId="31" pageNumber="32" reason="1">
<taxonomicName id="4C194D239E497466FA61288DD28FDDBF" authority="Stal, 1860" authorityName="Stal" authorityYear="1860" box="[264,689,1249,1275]" class="Insecta" family="Reduviidae" genus="Microlestria" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="fuscicollis">
<emphasis id="B96DEAB29E497466FA61288DD28FDDBF" bold="true" box="[264,689,1249,1275]" pageId="31" pageNumber="32">
<emphasis id="B96DEAB29E497466FA61288DD223DDBF" bold="true" box="[264,541,1249,1275]" italics="true" pageId="31" pageNumber="32">Microlestria fuscicollis</emphasis>
(
<bibRefCitation id="EF884B519E497466F944288DD297DDBF" author="Stal" box="[557,681,1249,1275]" pageId="31" pageNumber="46" refString="Stal, C. (1860) Bidrag till Rio Janeiro-Traktens Hemipter-Fauna. Kongliga Svenska Vetenskaps- Akademiens Handlingar, 2, 1 - 84." type="journal article" year="1860">
Stål, 
<number id="3DD124879E497466F904288DD297DDBF" box="[621,681,1249,1275]" pageId="31" pageNumber="32" value="1860.0">1860</number>
</bibRefCitation>
