</taxonomicName>
and 
<taxonomicName id="9AF160E0E01B4C616A35F8665D7BF85E" authority="Thomson, 1872" authorityName="Thomson" authorityYear="1872" class="Crinoidea" family="Bathycrinidae" genus="Bathycrinus" kingdom="Animalia" order="Bourgueticrinida" pageId="1" pageNumber="2" phylum="Echinodermata" rank="species" species="gracilis">
<emphasis id="6F85C771E01B4C616A35F86659A7F802" box="[1319,1435,1924,1948]" italics="true" pageId="1" pageNumber="2">B. gracilis</emphasis>
<bibRefCitation id="39606692E01B4C616F85F84B5D7BF85E" author="Thomson" box="[151,327,1960,1984]" pageId="1" pageNumber="2" refString="Thomson, C. W. (1872) On the crinoids of the &quot; Porcupine &quot; deep-sea dredging expedition. Proceedings of the Royal Society of Edinburg, 7, 764 - 773." type="journal article" year="1872">
