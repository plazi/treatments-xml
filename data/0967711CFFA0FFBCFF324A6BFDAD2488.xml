<taxonomicName id="46CEBB89FFA0FFBCFCAA4852FC6B2450" box="[783,964,854,879]" class="Magnoliopsida" family="Campanulaceae" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="417" phylum="Tracheophyta" rank="family">Campanulaceae</taxonomicName>
