<heading id="5E09B16BFFF04261FF55FF61D96BFF30" bold="true" box="[151,568,151,177]" fontSize="11" level="1" pageId="51" pageNumber="52" reason="1">
<taxonomicName id="C2FE7D84FFF04261FF55FF61D96BFF30" authority="Boulenger 1900" authorityName="Boulenger" authorityYear="1900" box="[151,568,151,177]" class="Reptilia" family="Gymnophthalmidae" genus="Neusticurus" kingdom="Animalia" order="Squamata" pageId="51" pageNumber="52" phylum="Chordata" rank="species" species="rudis">
