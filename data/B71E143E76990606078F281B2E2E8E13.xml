<taxonomicName id="08921C55106835ED97BA30C51809CF3B" class="Magnoliopsida" family="Asteraceae" genus="Gorteria" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="16" pageNumber="111" phylum="Tracheophyta" rank="genus">
