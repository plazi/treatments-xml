<heading id="D0FD6929CA6A5A65F0ECF8E4FDA66E73" bold="true" box="[151,586,1837,1863]" fontSize="11" level="1" pageId="11" pageNumber="12" reason="1">
<emphasis id="B97E0257CA6A5A65F0ECF8E4FDA66E73" bold="true" box="[151,586,1837,1863]" pageId="11" pageNumber="12">
Genus 
<taxonomicName id="4C0AA5C6CA6A5A65F095F8E4FDA66E73" authority="Kolliker, 1865" authorityName="Kolliker" authorityYear="1865" box="[238,586,1837,1863]" class="Anthozoa" family="Plexauridae" genus="Echinogorgia" kingdom="Animalia" order="Alcyonacea" pageId="11" pageNumber="12" phylum="Cnidaria" rank="genus">
