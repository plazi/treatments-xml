<paragraph id="AB827D82195263E008531B7FCF695273" pageId="0" pageNumber="89">
<taxonomicName id="59C4CD53287447655BA3A3495910959C" class="Malacostraca" family="Gecarcinucidae" genus="Terrathelphusa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Terrathelphusa secula" order="Decapoda" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="secula">Terrathelphusa secula</taxonomicName>
