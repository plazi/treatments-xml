<taxonomicName id="D906C9059D34F07A812D6FE81A57A494" class="Insecta" genus="H." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="H. angulicollis" order="Psocoptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="angulicollis">
