<collectingCountry id="F32B25AFFFBEFFB1FD77F885FD6AF8BA" box="[643,755,1878,1904]" name="Sri Lanka" pageId="15" pageNumber="373">Sri Lanka</collectingCountry>
, 
<collectingCountry id="F32B25AFFFBEFFB1FD0AF885FCD5F8BA" box="[766,844,1878,1904]" name="Russia" pageId="15" pageNumber="373">Russia</collectingCountry>
