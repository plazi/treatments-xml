Material examined: 
<specimenCount id="9D60FD6DFFD5FFCD2C315286768C49E2" box="[376,479,655,679]" pageId="55" pageNumber="56" type="male">90 males</specimenCount>
, 
<specimenCount id="9D60FD6DFFD5FFCD2CA25286753749E2" box="[491,612,655,679]" pageId="55" pageNumber="56" type="female">89 females</specimenCount>
