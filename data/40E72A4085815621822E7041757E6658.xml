<geoCoordinate id="22EFE07C8FF8B00AFB1A4A492A30F0D4" degrees="35" direction="north" minutes="38" orientation="latitude" precision="15" seconds="45" value="35.645836">35°38'45&quot;N</geoCoordinate>
