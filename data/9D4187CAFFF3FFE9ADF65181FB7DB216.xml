<taxonomicName id="D2E84D5FFFF0FFE9AC0A53BAFCE2B3F2" authority="(Bigot)" authorityName="Bigot" baseAuthorityName="Bigot" baseAuthorityYear="1888" box="[363,815,1234,1259]" class="Insecta" family="Simuliidae" genus="Paraustrosimulium" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="anthracinum">
<emphasis id="279CEACEFFF0FFE9AC0A53BAFD15B3F2" box="[363,728,1234,1259]" italics="true" pageId="2" pageNumber="453">Paraustrosimulium anthracinum</emphasis>
(Bigot)
</taxonomicName>
(South America), 
<taxonomicName id="D2E84D5FFFF0FFE9AE9F53BAFECCB216" authority="(Davies &amp; Gyorkos)" authorityName=", Davies &amp; Gyorkos" authorityYear="1988" baseAuthorityName="Davies &amp; Gyorkos" class="Insecta" family="Simuliidae" genus="Paraustrosimulium" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="colboi">
<emphasis id="279CEACEFFF0FFE9AE9F53BAFAEFB3F2" box="[1022,1314,1234,1259]" italics="true" pageId="2" pageNumber="453">Paraustrosimulium colboi</emphasis>
(Davies &amp; Györkös)
</taxonomicName>
(
<collectingRegion id="D72CF83EFFF0FFE9AC6C539FFEA2B216" box="[269,367,1271,1295]" country="Australia" name="Victoria" pageId="2" pageNumber="453">Victoria</collectingRegion>
, 
<collectingCountry id="6DFF764CFFF0FFE9AC18539FFE25B216" box="[377,488,1271,1295]" name="Australia" pageId="2" pageNumber="453">Australia</collectingCountry>
), 
<taxonomicName id="D2E84D5FFFF0FFE9AC975390FCB2B216" authority="Craig, Moulton, Currie" authorityName="Craig &amp; Moulton &amp; Currie" authorityYear="2017" box="[502,895,1270,1295]" class="Insecta" family="Simuliidae" genus="Paraustrosimulium" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="obcidens" status="n. sp.">
<emphasis id="279CEACEFFF0FFE9AC975390FDBBB216" box="[502,630,1270,1295]" italics="true" pageId="2" pageNumber="453">P. obcidens</emphasis>
Craig, Moulton, Currie
</taxonomicName>
<taxonomicNameLabel id="3CAF57B5FFF0FFE9AEE7539EFC08B216" box="[902,965,1270,1295]" pageId="2" pageNumber="453" rank="species">
<emphasis id="279CEACEFFF0FFE9AEE7539EFC08B216" bold="true" box="[902,965,1270,1295]" pageId="2" pageNumber="453">n. sp.</emphasis>
</taxonomicNameLabel>
(
<collectingRegion id="D72CF83EFFF0FFE9AEB0539FFB6AB216" box="[977,1191,1271,1295]" country="Australia" name="Western Australia" pageId="2" pageNumber="453">
Western 
<collectingCountry id="6DFF764CFFF0FFE9A956539FFB6AB216" box="[1079,1191,1271,1295]" name="Australia" pageId="2" pageNumber="453">Australia</collectingCountry>
</collectingRegion>
).
</paragraph>
</subSubSection>
