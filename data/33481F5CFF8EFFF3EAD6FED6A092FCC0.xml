<materialsCitation id="0B89A417FF8EFFF3EA8AFE16A384FCA0" collectingCountry="Mexico" collectionCode="OSF" collectorName="Images &amp; Box Z" country="Mexico" location="Mexique" pageId="18" pageNumber="321" specimenCount="3" specimenCount-female="3" typeStatus="syntype">
One 
<specimenCount id="ADE765C3FF8EFFF3EB54FE14A34CFE20" box="[266,288,417,443]" pageId="18" pageNumber="321" type="female">♀</specimenCount>
<typeStatus id="645A10E8FF8EFFF3EB75FE11A312FE27" box="[299,382,420,444]" pageId="18" pageNumber="321">syntype</typeStatus>
with labels: “Opomala 
<taxonomicName id="7CE1D5C9FF8EFFF3E8D4FE16A152FE20" authority="Sauss." authorityName="Saussure" authorityYear="1861" box="[650,830,419,443]" class="Insecta" family="Acrididae" genus="Proctolabus" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="321" phylum="Arthropoda" rank="species" species="mexicana">mexicana Sauss.</taxonomicName>
<specimenCount id="ADE765C3FF8EFFF3E917FE14A133FE20" box="[841,863,417,443]" pageId="18" pageNumber="321" type="female">♀</specimenCount>
” [handwritten on blue paper]; “ 
<taxonomicName id="7CE1D5C9FF8EFFF3EB43FE71A3EAFE47" authorityName="Stal" authorityYear="1873" box="[285,390,452,476]" class="Insecta" family="Acrididae" genus="Leptysma" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="321" phylum="Arthropoda" rank="genus">Leptysma</taxonomicName>
