<caption id="81B553A9AC34FF8E9FA99DD2FE083AC8" ID-DOI="http://doi.org/10.5281/zenodo.5306069" ID-Zenodo-Dep="5306069" httpUri="https://zenodo.org/record/5306069/files/figure.png" pageId="3" pageNumber="716" startId="3.[151,256,1975,1999]" targetBox="[170,1419,181,1961]" targetPageId="3">
<paragraph id="D5750321AC34FF8E9FA99DD2FE083AC8" blockId="3.[151,1436,1975,2033]" pageId="3" pageNumber="716">
