<paragraph id="8B9436F4FFA3D83BFE87FA04FD8EC00F" blockId="7.[264,639,1520,1581]" box="[264,639,1520,1547]" pageId="7" pageNumber="8">
<heading id="D0DC8198FFA3D83BFE87FA04FD8EC00F" bold="true" box="[264,639,1520,1547]" fontSize="11" level="1" pageId="7" pageNumber="8" reason="1">
<taxonomicName id="4C2B4D77FFA3D83BFE87FA04FD8EC00F" authority="Chandler" authorityName="Chandler" box="[264,639,1520,1547]" class="Insecta" family="Sisyridae" genus="Climacia" kingdom="Animalia" order="Neuroptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="californica">
<emphasis id="B95FEAE6FFA3D83BFE87FA04FD8EC00F" bold="true" box="[264,639,1520,1547]" pageId="7" pageNumber="8">
<emphasis id="B95FEAE6FFA3D83BFE87FA04FDF1C00E" bold="true" box="[264,512,1521,1547]" italics="true" pageId="7" pageNumber="8">Climacia californica</emphasis>
Chandler
</emphasis>
</taxonomicName>
