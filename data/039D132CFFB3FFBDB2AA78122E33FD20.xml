<taxonomicName id="4C34D9B9FFB3FFBDB2AA78122C5AFE0E" authorityName="Volynkin &amp; Černý" authorityYear="2017" box="[151,374,367,393]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="449" phylum="Arthropoda" rank="species" species="fansipana">
<emphasis id="B9407E28FFB3FFBDB2AA78122C5AFE0E" bold="true" box="[151,374,367,393]" italics="true" pageId="8" pageNumber="449">Barsine fansipana</emphasis>
