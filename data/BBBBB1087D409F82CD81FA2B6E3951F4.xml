<taxonomicName id="71A620366CC8C9514011BCE7DAF2EF6D" class="Insecta" family="Cicadellidae" genus="Parazyginella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parazyginella lingtianensis" order="Hemiptera" pageId="1" pageNumber="18" phylum="Arthropoda" rank="species" species="lingtianensis">Parazyginella lingtianensis</taxonomicName>
