<number id="3DB1643BFFEED330C89162D0C8E59D6D" box="[151,210,1260,1285]" pageId="87" pageNumber="88" value="1887.0">1887</number>
b
</bibRefCitation>
: 
<number id="3DB1643BFFEED330C8EB62D0C8CC9D6C" box="[237,251,1260,1284]" pageId="87" pageNumber="88" value="8.0">8</number>
