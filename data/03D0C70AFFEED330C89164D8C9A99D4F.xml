<taxonomicName id="4C790D9FFFEED330C93C62D1CBFF9D6C" authority="G. Canestrini &amp; Fanzago, 1877" authorityName="G. Canestrini &amp; Fanzago" authorityYear="1877" box="[314,968,1260,1285]" class="Arachnida" family="Parasitidae" genus="Gamasus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="bicornis" subGenus="Seius">
<emphasis id="B90DAA0EFFEED330C93C62D1C9959D6C" box="[314,418,1261,1284]" italics="true" pageId="87" pageNumber="88">Gamasus</emphasis>
(
<emphasis id="B90DAA0EFFEED330C9B262D1C9D89D6C" box="[436,495,1261,1284]" italics="true" pageId="87" pageNumber="88">Seius</emphasis>
) 
<emphasis id="B90DAA0EFFEED330CA0462D0CA699D6D" box="[514,606,1260,1285]" italics="true" pageId="87" pageNumber="88">bicornis</emphasis>
<bibRefCitation id="EFE80BEDFFEED330CA6062D0CBFF9D6C" author="Canestrini" box="[614,968,1260,1285]" pageId="87" pageNumber="88" refString="Canestrini, G. &amp; Fanzago, F. (1877) Nuovi Acari italiani. Atti della Societa Veneto - Trentina di Scienze Naturali, Padova, 5, 99 - 111." type="journal article" year="1877">
G. Canestrini &amp; Fanzago, 
<number id="3DB1643BFFEED330CB9662D0CBFF9D6C" box="[912,968,1260,1284]" pageId="87" pageNumber="88" value="1877.0">1877</number>
</bibRefCitation>
</taxonomicName>
