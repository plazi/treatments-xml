<taxonomicName id="4C790D9FFFEED330C8F3632CC9AF9D40" authority="C.H.Heyden, 1826" authorityName="C.H.Heyden" authorityYear="1826" box="[245,408,1295,1320]" class="Arachnida" family="Ascidae" genus="Asca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="87" pageNumber="88" phylum="Arthropoda" rank="species" species="aphidioides">
<emphasis id="B90DAA0EFFEED330C8F3632CC9319D4F" box="[245,262,1296,1319]" italics="true" pageId="87" pageNumber="88">A</emphasis>
. 
<emphasis id="B90DAA0EFFEED330C9136333C9AF9D40" box="[277,408,1295,1320]" italics="true" pageId="87" pageNumber="88">aphidioides</emphasis>
