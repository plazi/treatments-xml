, 
<collectingCountry id="7AE066F0FFBDFF8AFAA7F9FDFF75F925" name="Equatorial Guinea" pageId="7" pageNumber="242">Equatorial Guinea</collectingCountry>
including Island Bioco, 
<collectingCountry id="7AE066F0FFBDFF8AFE70F9D1FDFCF925" box="[511,609,1658,1683]" name="Ethiopia" pageId="7" pageNumber="242">Ethiopia</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBDFF8AFDE1F9D1FD26F925" box="[622,699,1658,1683]" name="Gabon" pageId="7" pageNumber="242">Gabon</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBDFF8AFD48F9D1FC8EF925" box="[711,787,1658,1683]" name="Ghana" pageId="7" pageNumber="242">Ghana</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBDFF8AFC90F9D0FC3AF925" box="[799,935,1659,1683]" name="Ivory Coast" pageId="7" pageNumber="242">Ivory Coast</collectingCountry>
