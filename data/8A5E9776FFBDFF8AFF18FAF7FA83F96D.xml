<collectingCountry id="7AE066F0FFBDFF8AFC3DF9D0FC63F925" box="[946,1022,1659,1683]" name="Kenya" pageId="7" pageNumber="242">Kenya</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBDFF8AFB85F9D1FBFCF925" box="[1034,1121,1658,1683]" name="Nigeria" pageId="7" pageNumber="242">Nigeria</collectingCountry>
, 
<collectingCountry id="7AE066F0FFBDFF8AFBE2F9D1FA9CF925" box="[1133,1281,1658,1683]" name="South Africa" pageId="7" pageNumber="242">South Africa</collectingCountry>
