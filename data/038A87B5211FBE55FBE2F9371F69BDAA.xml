HABITAT. — 
<taxonomicName id="4C234D20211FBE55F8A2FD6D1F9DBD91" baseAuthorityName="Brisout de Barneville" baseAuthorityYear="1854" box="[944,1152,683,707]" class="Insecta" family="Pamphagidae" genus="Euryparyphes" kingdom="Animalia" order="Orthoptera" pageId="101" pageNumber="706" phylum="Arthropoda" rank="species" species="sitifensis">
<emphasis id="B957EAB1211FBE55F8A2FD6D1F9DBD91" box="[944,1152,683,707]" italics="true" pageId="101" pageNumber="706">Euryparyphes sitifensis</emphasis>
