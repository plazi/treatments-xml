<number id="524124D7FF98FFEA560953AA9551F965" box="[578,611,1710,1736]" pageId="6" pageNumber="7" value="49.0">49</number>
</treatmentCitation>
</taxonomicName>
. Type-species: 
<taxonomicName id="23894D73FF98FFEA575D53AA9375F965" authority="Say, 1823" authorityName="Say" authorityYear="1823" box="[790,1095,1710,1736]" class="Insecta" family="Syrphidae" genus="Scaeva" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="obliqua">
