<treatmentCitationGroup id="C49911DEFF98FFEA54DC53AA93EBF965" box="[151,1241,1710,1736]" pageId="6" pageNumber="7">
<taxonomicName id="23894D73FF98FFEA54DC53AA9551F965" authority="Osten Sacken 1875: 49" authorityName="Osten Sacken" authorityPageNumber="49" authorityYear="1875" box="[151,611,1710,1736]" class="Insecta" family="Syrphidae" genus="Allograpta" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis id="D6FDEAE2FF98FFEA54DC53AA9678F96A" box="[151,330,1710,1735]" italics="true" pageId="6" pageNumber="7">ALLOGRAPTA</emphasis>
<treatmentCitation id="652810E1FF98FFEA551A53AA9551F965" author="Osten" box="[337,611,1710,1736]" page="49" pageId="6" pageNumber="7" year="1875">
<bibRefCitation id="80184B01FF98FFEA551A53AA9505F965" author="Osten" box="[337,567,1710,1736]" pageId="6" pageNumber="19" refString="Osten Sacken, C. R. (1875) A list of the North American Syrphidae. Bulletin. Buffalo of the Society of Natural Sciences, 3, 38 - 71. [1875.11. ??]" type="journal article" year="1875">
Osten Sacken 
<number id="524124D7FF98FFEA55B353AA9505F965" box="[504,567,1710,1736]" pageId="6" pageNumber="7" value="1875.0">1875</number>
</bibRefCitation>
