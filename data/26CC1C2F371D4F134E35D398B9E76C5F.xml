<taxonomicName id="2AF11D412A74328C388D2EDD19CEC7A4" authority="Bowerbank, 1845" authorityName="Bowerbank" authorityYear="1845" class="Demospongiae" family="Aplysinidae" genus="Verongia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Verongia rosea" order="Verongida" pageId="8" pageNumber="9" phylum="Porifera" rank="species" species="rosea">Verongia rosea</taxonomicName>
