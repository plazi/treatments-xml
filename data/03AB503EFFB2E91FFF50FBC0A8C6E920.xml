<taxonomicName id="4C029AABFFB4E91EFEDEF81FAD9DECA0" authority="He &amp; Yan &amp; Yang, 2021" authorityName="He &amp; Yan &amp; Yang" authorityYear="2021" box="[281,433,1922,1945]" class="Insecta" family="Cicadellidae" genus="Ujna" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="448" phylum="Arthropoda" rank="species" species="cavipenis" status="sp. nov.">
<emphasis id="B9763D3AFFB4E91EFEDEF81FAD9DECA0" box="[281,433,1922,1945]" italics="true" pageId="4" pageNumber="448">Ujna cavipenis</emphasis>
</taxonomicName>
