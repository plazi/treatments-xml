<paragraph id="8BF7E70E982A0B2FFF19FE5B4E7D1BF9" blockId="17.[192,830,386,442]" box="[192,830,418,442]" pageId="17" pageNumber="16">
<treatmentCitationGroup id="AB58C020982A0B2FFF19FE5B4E7D1BF9" box="[192,830,418,442]" pageId="17" pageNumber="16">
<taxonomicName id="4C489C8D982A0B2FFF19FE5B4F191BF9" authority="(LeConte)" baseAuthorityName="LeConte" baseAuthorityYear="1863" box="[192,602,418,442]" class="Insecta" family="Heteroceridae" genus="Neoheterocerus" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="16" phylum="Arthropoda" rank="species" species="gnatho">
