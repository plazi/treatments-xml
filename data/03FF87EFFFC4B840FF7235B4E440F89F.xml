<taxonomicName id="4C564D7AFFC4B840FDE13153E2FEF8C7" box="[516,661,1808,1832]" class="Insecta" family="Scarabaeidae" genus="Ochicanthon" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
