(
<taxonomicName id="4C54319EFFDF7F05FE73FCCCFD8B8FCB" box="[451,569,894,918]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="4" pageNumber="51" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
