<taxonomicName id="4C54319EFFDF7F05FD75FCE8FC8E8F2F" box="[709,828,858,882]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="4" pageNumber="51" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
