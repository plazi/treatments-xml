<taxonomicName id="4C54319EFFDF7F05FC18FCCDFAA18FCB" authority="Robert Brown" authorityName="Robert Brown" box="[936,1299,894,918]" class="Magnoliopsida" family="Pentaphylacaceae" genus="Eurya" kingdom="Plantae" order="Ericales" pageId="4" pageNumber="51" phylum="Tracheophyta" rank="species" species="chinensis">
