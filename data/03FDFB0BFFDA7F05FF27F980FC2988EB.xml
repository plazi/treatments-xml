<taxonomicName id="4C54319EFFDF7F05FF00FCE8FE708F2F" box="[176,450,858,882]" class="Magnoliopsida" family="Lauraceae" genus="Actinodaphne" kingdom="Plantae" order="Laurales" pageId="4" pageNumber="51" phylum="Tracheophyta" rank="species" species="acuminat">
