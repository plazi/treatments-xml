<taxonomicName id="4C314D53FFD49E33FE81410BFDF50C21" box="[295,520,1839,1861]" class="Polychaeta" family="Terebellidae" genus="Lanicola" kingdom="Animalia" order="Terebellida" pageId="62" pageNumber="546" phylum="Annelida" rank="species" species="rectangulata" status="n. sp.">
