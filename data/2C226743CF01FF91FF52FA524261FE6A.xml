<location id="A154808ECF02FF91FCBDFEED4265FEF2" LSID="urn:lsid:plazi:treatment:2C226743CF01FF91FF52FA524261FE6A:A154808ECF02FF91FCBDFEED4265FEF2" box="[856,1058,351,376]" country="Brazil" municipality="This" name="Serra da Canastra" pageId="6" pageNumber="3005" stateProvince="Sao Paulo">Serra da Canastra</location>
