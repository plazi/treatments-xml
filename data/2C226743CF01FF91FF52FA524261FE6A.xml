<quantity id="63737BB0CF02FF91FC92FE8E458CFEDF" box="[887,971,316,341]" metricMagnitude="5" metricUnit="m" metricValue="1.25" pageId="6" pageNumber="3005" unit="km" value="125.0">125 km</quantity>
