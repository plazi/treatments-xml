(mean 
<quantity id="4CA29B61203ABE33138EF3276A27F80C" box="[507,610,1901,1928]" metricMagnitude="-4" metricUnit="m" metricValue="3.2" pageId="3" pageNumber="436" unit="mm" value="0.32">0.32 mm</quantity>
); EL 
<quantity id="4CA29B61203ABE3310EEF3276B78F80C" box="[667,829,1901,1928]" metricMagnitude="-4" metricUnit="m" metricValue="7.3" metricValueMax="7.8" metricValueMin="6.8" pageId="3" pageNumber="436" unit="mm" value="0.73" valueMax="0.78" valueMin="0.68">0.68–0.78 mm</quantity>
(mean 
