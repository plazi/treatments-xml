<taxonomicName id="1BCB4D4231303F69CCB7FF5EFEABFF02" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="neuquenensis">
