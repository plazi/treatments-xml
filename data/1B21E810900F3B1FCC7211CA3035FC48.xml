, 1834 [syn. of 
<taxonomicName id="1FA6071BEEB33CCFAFAEF834ED190798" class="Insecta" family="Curculionidae" genus="Hadromerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hadromerus" order="Coleoptera" pageId="266" pageNumber="267" phylum="Arthropoda" rank="genus">Hadromerus</taxonomicName>
<normalizedToken id="0CC918B17E2ADAF9EF374D2764412CD8" originalValue="Schönherr">Schoenherr</normalizedToken>
