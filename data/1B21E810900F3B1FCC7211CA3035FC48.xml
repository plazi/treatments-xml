<taxonomicName id="E1F1734E8771177EB7E81EDC1D383761" class="Insecta" family="Curculionidae" genus="Hadromerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hadromerus" order="Coleoptera" pageId="266" pageNumber="267" phylum="Arthropoda" rank="genus">Hadromerus</taxonomicName>
<normalizedToken id="389D0CB5C7DB48420DBA3847B3454043" originalValue="Schönherr">Schoenherr</normalizedToken>
, 1823 [
<taxonomicName id="0928A09548A159898B983205EC4A69AC" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Coleoptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Coleoptera" order="Malvales" pageId="266" pageNumber="267" phylum="Tracheophyta" rank="genus">Coleoptera</taxonomicName>
