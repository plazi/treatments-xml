<taxonomicName id="F11C50E390CBCB807B59D866F0A3BA78" class="Insecta" family="Curculionidae" genus="Hadromerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hadromerus" order="Coleoptera" pageId="266" pageNumber="267" phylum="Arthropoda" rank="genus">Hadromerus</taxonomicName>
<normalizedToken id="1A20EEC77F0484D7F9B65398230A07C6" originalValue="Schönherr">Schoenherr</normalizedToken>
