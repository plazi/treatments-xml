<taxonomicName id="4C184D0D6904FFB09B75FA1CFDB64F84" authorityName="Leavengood" authorityYear="2020" box="[367,618,1488,1514]" class="Insecta" family="Cleridae" genus="Phyllobaenus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="turnbowi">
<emphasis id="B96CEA9C6904FFB09B75FA1CFDB64F84" box="[367,618,1488,1514]" italics="true" pageId="6" pageNumber="5">Phyllobaenus turnbowi</emphasis>
