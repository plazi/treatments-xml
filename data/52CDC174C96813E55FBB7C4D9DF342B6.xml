<taxonomicName id="50C9C424127B3E85414A902E4556DDA7" LSID="http://species-id.net/wiki/Mymarilla" authority="Westwood" class="Insecta" family="Mymaridae" genus="Mymarilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mymarilla" order="Hymenoptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="genus">Mymarilla Westwood</taxonomicName>
