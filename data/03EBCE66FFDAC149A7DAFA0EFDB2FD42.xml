<taxonomicName id="4C4204F3FFD9C146A67DF879FD63F872" authority="(Walker, 1849)" authorityName="Li &amp; Yeates" authorityYear="2020" baseAuthorityName="Walker" baseAuthorityYear="1849" box="[304,714,1923,1948]" class="Insecta" family="Bombyliidae" genus="Dissodesma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="38" pageNumber="239" phylum="Arthropoda" rank="species" species="tetratricha" status="comb. nov.">
