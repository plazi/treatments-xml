<taxonomicName id="4C61EEEBD02DFFA3FF62FBD5FF752F80" box="[151,229,1144,1167]" class="Mollusca (awaiting allocation)" family="Mollusca (awaiting allocation)" genus="Nereus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca (awaiting allocation)" pageId="47" pageNumber="48" phylum="Mollusca" rank="genus">
