<taxonomicName id="95C0A9FBFFECFFECFCDEFA7F8CAF8587" authority="Tragardh, 1938" authorityName="Tragardh" authorityYear="1938" box="[892,1269,1457,1482]" class="Arachnida" family="Cercomegistidae" kingdom="Animalia" order="Mesostigmata" pageId="3" pageNumber="5" phylum="Arthropoda" rank="family">Cercomegistidae Trägårdh, 1938</taxonomicName>
