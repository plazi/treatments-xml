(
<collectingRegion id="4A02F827FFE0EC1DB4D6F80EFDFED4D4" box="[469,577,1929,1954]" country="Vietnam" name="Lai Chau" pageId="5" pageNumber="156">Lai Chau</collectingRegion>
, 
<collectingRegion id="4A02F827FFE0EC1DB74EF80DFD16D4D4" box="[589,681,1929,1954]" country="Vietnam" name="Lao Cai" pageId="5" pageNumber="156">Lao Cai</collectingRegion>
, 
<collectingRegion id="4A02F827FFE0EC1DB7B6F80DFCB0D4D4" box="[693,783,1929,1954]" country="Vietnam" name="Yen Bai" pageId="5" pageNumber="156">Yen Bai</collectingRegion>
and 
