<quantity id="4CEF9B25FF89FFAD9AC9F88EF97A3792" box="[692,827,1916,1943]" metricMagnitude="2" metricUnit="m" metricValue="5.4" metricValueMax="10.629999999999999" metricValueMin="0.16999999999999998" pageId="24" pageNumber="25" unit="m" value="540.0" valueMax="1063.0" valueMin="17.0">17–1,063 m</quantity>
(
<quantity id="4CEF9B25FF89FFAD9B36F88EF9903792" box="[843,977,1916,1943]" metricMagnitude="2" metricUnit="m" metricValue="5.399532" metricValueMax="10.628376" metricValueMin="0.170688" pageId="24" pageNumber="25" unit="ft" value="1771.5" valueMax="3487.0" valueMin="56.0">56–3,487 ft</quantity>
) (min.: Weigman 2016; max.: 
<bibRefCitation id="EF864B31FF89FFAD9D53F88EFB4337B9" author="Dolganov" firstAuthor="Dolganov" pageId="24" pageNumber="25" pagination="415 - 425" refId="ref274934" refString="Dolganov, V. N. (1985) New skate species of the family Rajidae from the northwestern Pacific. Voprosy Ikhtiologii, 25, 415 - 425 [In Russian.]" type="journal article" year="1985">Dolganov 1985</bibRefCitation>
). Also as 
<taxonomicName id="4C174D43FF89FFAD9909F853F94E37BE" authority="Dolganov, 1983" authorityName="Dolganov" authorityYear="1983" box="[372,783,1953,1979]" class="Elasmobranchii" family="Arhynchobatidae" genus="Rhinoraja" kingdom="Animalia" order="Rajiformes" pageId="24" pageNumber="25" phylum="Chordata" rank="species" species="taranetzi">
