<collectingCountry id="F331764DFFF6FF96FEE6F902FD8FF95B" box="[359,514,1728,1753]" name="New Zealand" pageId="5" pageNumber="536">New Zealand</collectingCountry>
: ND, AK, CL, NN; 
<collectingCountry id="F331764DFFF6FF96FD63F903FCC0F95B" box="[738,845,1728,1753]" name="Australia" pageId="5" pageNumber="536">Australia</collectingCountry>
