<emphasis id="6A89EADAFFB78C4CFF283FFDFEF196A4" bold="true" box="[151,349,1188,1212]" pageId="6" pageNumber="66">FIGURES 18–21.</emphasis>
<taxonomicName id="9FFD4D4BFFB78C4CFEDB3FFCFDE996A4" authority="Wang &amp; Liu, 2010" authorityName="Wang &amp; Liu" authorityYear="2010" box="[356,581,1189,1212]" class="Insecta" family="Osmylidae" genus="Osmylus" kingdom="Animalia" order="Neuroptera" pageId="6" pageNumber="66" phylum="Arthropoda" rank="species" species="bipapillatus" status="sp. nov.">
<emphasis id="6A89EADAFFB78C4CFEDB3FFCFDE996A4" box="[356,581,1189,1212]" italics="true" pageId="6" pageNumber="66">Osmylus bipapillatus</emphasis>
</taxonomicName>
<emphasis id="6A89EADAFFB78C4CFDF23FFDFD0E96A4" bold="true" box="[589,674,1188,1212]" pageId="6" pageNumber="66">
<taxonomicNameLabel id="71BA57A1FFB78C4CFDF23FFDFD0E96A4" box="[589,674,1188,1212]" pageId="6" pageNumber="66" rank="species">sp. nov.</taxonomicNameLabel>
