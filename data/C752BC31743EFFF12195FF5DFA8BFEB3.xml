<taxonomicName id="88FB76A4743EFFF1232BFF2BFD01FF01" box="[553,685,225,250]" class="Insecta" family="Oecophoridae" genus="Thyrocopa" kingdom="Animalia" order="Lepidoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">
