<taxonomicName id="4C6F6E98FFF8382CF311B8BDA9F3FF71" authorityName="Saussure" authorityYear="1877" box="[615,727,188,212]" class="Insecta" family="Gryllidae" genus="Scapsipedus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="503" phylum="Arthropoda" rank="species" species="micado">
