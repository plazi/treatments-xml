<taxonomicName id="17BD4D24FFE2A722D5E038DFFE3A0CD8" authority="Davis 1939" authorityName="Davis" authorityYear="1939" box="[151,465,200,226]" class="Insecta" family="Anisembiidae" genus="Saussurembia" kingdom="Animalia" order="Embioptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
<emphasis id="E2C9EAB5FFE2A722D5E038DFFEA90CD8" bold="true" box="[151,322,200,226]" italics="true" pageId="21" pageNumber="22">Saussurembia</emphasis>
Davis 
<number id="66752480FFE2A722D4E138DFFE3A0CD8" box="[406,465,200,226]" pageId="21" pageNumber="22" value="1939.0">1939</number>
</taxonomicName>
(= 
<taxonomicName id="17BD4D24FFE2A722D48438DFFCFA0CD8" authority="Davis 1939" authorityName="Davis" authorityYear="1939" box="[499,785,200,226]" class="Insecta" family="Tetrigidae" genus="Saussurella" kingdom="Animalia" order="Orthoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="genus">
<emphasis id="E2C9EAB5FFE2A722D48438DFFD6A0CD8" bold="true" box="[499,641,200,226]" italics="true" pageId="21" pageNumber="22">Saussurella</emphasis>
Davis 
<number id="66752480FFE2A722D7A238DFFCFA0CD8" box="[725,785,200,226]" pageId="21" pageNumber="22" value="1939.0">1939</number>
</taxonomicName>
, homonym), 
<typeStatus id="0F068805FFE2A722D6C338DFFC020CD8" box="[948,1001,200,226]" pageId="21" pageNumber="22">type</typeStatus>
species.
</emphasis>
</heading>
</paragraph>
