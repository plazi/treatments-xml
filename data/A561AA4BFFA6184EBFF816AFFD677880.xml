<number id="9B00097AFFA6184EBFF8102FFF367880" box="[151,210,1383,1408]" pageId="16" pageNumber="217" value="1995.0">1995</number>
; Coscarón 
<number id="9B00097AFFA6184EBE211020FE6D7880" box="[334,393,1384,1408]" pageId="16" pageNumber="217" value="1997.0">1997</number>
