<number id="9B00097AFFA6184EBD3111B4FD7C7815" box="[606,664,1276,1301]" pageId="16" pageNumber="217" value="1995.0">1995</number>
) reported 
<taxonomicName id="EAC860DEFFA6184EBC7F11B5FB437814" authority="Stal, 1870" authorityName="Stal" authorityYear="1870" box="[784,1191,1276,1301]" class="Insecta" family="Reduviidae" genus="Peirates" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="217" phylum="Arthropoda" rank="species" species="atromaculatus">
