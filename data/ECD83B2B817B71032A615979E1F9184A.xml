<subSubSection id="C1EC95139316583EFA95BCFC0AC9463D" pageId="47" pageNumber="48" type="reference_group">
<paragraph id="6BC7DFE73D4E07EC6BD51AE0358E83D4" pageId="47" pageNumber="48">
<taxonomicName id="830B0A0B5A33D17A11DA77C1CC7E0453" class="Insecta" family="Tenthredinidae" genus="Nematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nematus nigriceps" order="Hymenoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="nigriceps">
