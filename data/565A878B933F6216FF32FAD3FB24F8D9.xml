<taxonomicName id="19F34D1E933F6216FF60F8B9FE6EF8D9" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" box="[197,487,1844,1869]" class="Insecta" family="Scutelleridae" genus="Eurygaster" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
