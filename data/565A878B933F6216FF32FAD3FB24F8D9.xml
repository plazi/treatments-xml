<taxonomicName id="19F34D1E933F6216FB5FF89DFAFFF8BC" authorityName="Agassiz" authorityYear="1846" box="[1274,1398,1809,1832]" class="Insecta" family="Scutelleridae" genus="Eurygaster" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
