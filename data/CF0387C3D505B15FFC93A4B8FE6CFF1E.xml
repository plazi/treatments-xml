<taxonomicName id="80AA4D56D500B15EFB3BA660FA26F89B" authorityName="Diesing" authorityYear="1863" box="[1243,1431,1890,1914]" class="Enopla" family="Ototyphlonemertidae" genus="Ototyphlonemertes" kingdom="Animalia" order="Monostilifera" pageId="14" pageNumber="27" phylum="Nemertea" rank="genus">
<emphasis id="75DEEAC7D500B15EFB3BA660FA26F89B" box="[1243,1431,1890,1914]" italics="true" pageId="14" pageNumber="27">Ototyphlonemertes</emphasis>
</taxonomicName>
(
<taxonomicName id="80AA4D56D500B15EFCDDA681FC23F87A" authorityName="Correa" authorityYear="1950" box="[829,914,1923,1947]" class="Enopla" family="Ototyphlonemertidae" genus="Ototyphlonemertes" kingdom="Animalia" order="Monostilifera" pageId="14" pageNumber="27" phylum="Nemertea" rank="species" species="parmula">
<emphasis id="75DEEAC7D500B15EFCDDA681FC23F87A" box="[829,914,1923,1947]" italics="true" pageId="14" pageNumber="27">parmula</emphasis>
</taxonomicName>
) 
<taxonomicName id="80AA4D56D500B15EFC44A681FC49F87A" authority="Kajihara &amp; Tamura &amp; Tomioka, 2018" authorityName="Kajihara &amp; Tamura &amp; Tomioka" authorityYear="2018" box="[932,1016,1923,1947]" class="Enopla" family="Ototyphlonemertidae" genus="Ototyphlonemertes" kingdom="Animalia" order="Monostilifera" pageId="14" pageNumber="27" phylum="Nemertea" rank="species" species="nakaoae" status="sp. nov.">
<emphasis id="75DEEAC7D500B15EFC44A681FC49F87A" box="[932,1016,1923,1947]" italics="true" pageId="14" pageNumber="27">nakaoae</emphasis>
</taxonomicName>
<taxonomicNameLabel id="6EED57BCD500B15EFBE1A681FBE1F87B" box="[1025,1104,1923,1946]" pageId="14" pageNumber="27" rank="species">sp. nov.</taxonomicNameLabel>
was sister to 
<emphasis id="75DEEAC7D500B15EFB02A681FB48F87B" box="[1250,1273,1923,1946]" italics="true" pageId="14" pageNumber="27">O.</emphasis>
(
<taxonomicName id="80AA4D56D500B15EFAEAA681FAEEF87A" authorityName="Correa" authorityYear="1950" box="[1290,1375,1923,1947]" class="Enopla" family="Ototyphlonemertidae" genus="Ototyphlonemertes" kingdom="Animalia" order="Monostilifera" pageId="14" pageNumber="27" phylum="Nemertea" rank="species" species="parmula">
<emphasis id="75DEEAC7D500B15EFAEAA681FAEEF87A" box="[1290,1375,1923,1947]" italics="true" pageId="14" pageNumber="27">parmula</emphasis>
</taxonomicName>
) 
<taxonomicName id="80AA4D56D500B15EFA91A681FC35F85A" authority="Kajihara &amp; Tamura &amp; Tomioka, 2018" authorityName="Kajihara &amp; Tamura &amp; Tomioka" authorityYear="2018" class="Enopla" family="Ototyphlonemertidae" genus="Ototyphlonemertes" kingdom="Animalia" order="Monostilifera" pageId="14" pageNumber="27" phylum="Nemertea" rank="species" species="tsukagoshii" status="sp. nov.">
<emphasis id="75DEEAC7D500B15EFA91A681FC35F85A" italics="true" pageId="14" pageNumber="27">tsukagoshii</emphasis>
</taxonomicName>
