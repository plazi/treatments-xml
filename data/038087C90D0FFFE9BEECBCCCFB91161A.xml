<number id="3DE124F80D0EFFE9BAC2BCBBFB6615BE" box="[1209,1262,224,248]" pageId="3" pageNumber="450" value="0.77">0.77</number>
); II 
<number id="3DE124F80D0EFFE9BB5BBC84FAD915BE" box="[1312,1361,223,248]" pageId="3" pageNumber="450" value="5.52">5.52</number>
(
<number id="3DE124F80D0EFFE9BB18BCBBFA1F15BE" box="[1379,1431,224,248]" pageId="3" pageNumber="450" value="1.48">1.48</number>
, 
<number id="3DE124F80D0EFFE9BEECBD5FFF42145B" box="[151,202,260,285]" pageId="3" pageNumber="450" value="1.95">1.95</number>
, 
<number id="3DE124F80D0EFFE9BEAEBD5FFE81145A" box="[213,265,260,284]" pageId="3" pageNumber="450" value="1.29">1.29</number>
