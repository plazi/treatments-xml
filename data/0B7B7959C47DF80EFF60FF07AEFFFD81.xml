from
</taxonomicName>
<collectingMunicipality id="63095235C47DF80EFC8FFE93AEDEFE85" box="[888,976,259,285]" pageId="23" pageNumber="567">Bichate</collectingMunicipality>
, 
<collectingCounty id="6A0CB0C3C47DF80EFC2DFE93A990FE85" box="[986,1182,259,285]" pageId="23" pageNumber="567">Arfak Mountains</collectingCounty>
