<taxonomicName id="34B37486AB1CFFC3FE39F8B0FD96C110" authorityName="Bate and Westwood" authorityYear="1868" box="[407,571,1800,1823]" class="Malacostraca" family="Vibiliidae" genus="Vibilia" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="borealis">
