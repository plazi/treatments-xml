<quantity id="4C92E4E7FFE0FF8BFECCF951FB86F91A" box="[991,1090,1720,1742]" metricMagnitude="-1" metricUnit="m" metricValue="3.45" metricValueMax="3.6" metricValueMin="3.3" pageId="0" pageNumber="221" unit="cm" value="34.5" valueMax="36.0" valueMin="33.0">33–36 cm</quantity>
