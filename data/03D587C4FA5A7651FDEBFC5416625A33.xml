<accessionNumber id="942FAB31FA5A7651FFCEFB54174F5A11" box="[653,771,1257,1281]" httpUri="http://www.ncbi.nlm.nih.gov/protein/REP01541" pageId="19" pageNumber="864">REP01541</accessionNumber>
, 
<accessionNumber id="942FAB31FA5A7651FDEBFAB615535A33" box="[168,287,1291,1315]" httpUri="http://www.ncbi.nlm.nih.gov/protein/REP01605" pageId="19" pageNumber="864">REP01605</accessionNumber>
