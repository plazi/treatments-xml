<quantity id="4CF9A38AFF8FFFAFDA4BF7FCCB7AFE0C" box="[1026,1124,460,479]" metricMagnitude="2" metricUnit="m" metricValue="4.305" metricValueMax="6.0" metricValueMin="2.61" pageId="16" pageNumber="79" unit="m" value="430.5" valueMax="600.0" valueMin="261.0">261–600 m</quantity>
