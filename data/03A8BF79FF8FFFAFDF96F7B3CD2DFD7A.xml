<geoCoordinate id="EE3568A8FF8FFFAFDD2DF7FCCCB3FE0C" box="[868,941,460,479]" degrees="14" direction="north" minutes="52" orientation="latitude" pageId="16" pageNumber="79" precision="925" value="14.866667">14°52ʹN</geoCoordinate>
