<taxonomicName id="70CA4D3CFF85E22184A2FF6FDE6365FD" authority="Verrill, 1874" authorityName="Verrill" authorityYear="1874" box="[493,877,188,213]" class="Hydrozoa" family="Halopterididae" genus="Halopteris" kingdom="Animalia" order="Leptothecata" pageId="104" pageNumber="105" phylum="Cnidaria" rank="species" species="tenella">
