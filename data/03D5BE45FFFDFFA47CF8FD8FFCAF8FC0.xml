<taxonomicName id="4C7C74D0FFFDFFA47FE3FC3EFBA18D6D" box="[908,1115,1015,1041]" class="Magnoliopsida" family="Asteraceae" genus="Acroptylon" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="209" phylum="Tracheophyta" rank="species" species="repens">
