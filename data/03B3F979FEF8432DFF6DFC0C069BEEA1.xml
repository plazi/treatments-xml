<taxonomicName id="4C1A33ECFEF8432DFE3BFAF70790EE3E" box="[449,645,1327,1352]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="267" pageNumber="268" phylum="Arthropoda" rank="species" species="robusta">
