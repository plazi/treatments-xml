and 
<taxonomicName id="34E64D289526FFDAFF1F5B9DFED578CF" authorityName="Roding" authorityYear="1798" box="[151,257,1427,1452]" class="Bivalvia" family="Pteriidae" genus="Pinctada" kingdom="Animalia" order="Pterioida" pageId="48" pageNumber="49" phylum="Mollusca" rank="genus">
