<quantity id="341E9B4E9526FFDAFD5F5B14FCF37857" box="[727,807,1306,1332]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="48" pageNumber="49" unit="mm" value="13.0">13 mm</quantity>
(n=10, r:6–13, µ=9 ±2.2), OL= 
<quantity id="341E9B4E9526FFDAFB285B14FB377857" box="[1184,1251,1306,1332]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="48" pageNumber="49" unit="mm" value="6.0">6 mm</quantity>
