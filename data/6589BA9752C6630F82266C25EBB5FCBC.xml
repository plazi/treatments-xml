, 
<taxonomicName id="631B9C28CE3B07D20610F7F41650A4B5" authority="lattrei" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="86" pageNumber="87" phylum="Mollusca" rank="subSpecies" species="lattrei" subGenus="Drymaeus" subSpecies="lattrei">
