<collectionCode id="ED14AE31FFB0FFC6FF62FAD8399CFAD0" box="[157,251,1375,1396]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="11" pageNumber="11">AMNH</collectionCode>
