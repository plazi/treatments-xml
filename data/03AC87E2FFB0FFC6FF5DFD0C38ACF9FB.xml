<collectionCode id="ED14AE31FFB0FFC6FE27FAA23B51FA9E" box="[472,566,1317,1338]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="11" pageNumber="11">AMNH</collectionCode>
