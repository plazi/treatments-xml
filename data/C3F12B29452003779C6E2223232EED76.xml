<paragraph id="3276A178CD40B266450B6E299A302DFA" pageId="24" pageNumber="85">
<taxonomicName id="92A1D1C4662328118656A1F4F93F29FE" class="Insecta" family="Braconidae" genus="Stantonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stantonia qui" order="Hymenoptera" pageId="24" pageNumber="85" phylum="Arthropoda" rank="species" species="qui">Stantonia qui</taxonomicName>
