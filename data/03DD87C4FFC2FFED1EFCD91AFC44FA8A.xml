<taxonomicName id="4C744D51FFC2FFED1EFCD91AFC09FBA2" authority="Hoogstraal, Trapido and Kohls, 1965" authorityName="Hoogstraal, Trapido and Kohls" authorityYear="1965" box="[151,931,1123,1149]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="nadchatrami">
<emphasis id="B900EAC0FFC2FFED1EFCD91AFC09FBA2" bold="true" box="[151,931,1123,1149]" pageId="26" pageNumber="27">
<emphasis id="B900EAC0FFC2FFED1EFCD91AFE49FBA3" bold="true" box="[151,483,1123,1148]" italics="true" pageId="26" pageNumber="27">Haemaphysalis nadchatrami</emphasis>
