, 
<taxonomicName id="B1AA4D67FF98FFED6F7BF8A77740F846" authority="Karg, 1982" authorityName="Karg" authorityYear="1982" class="Arachnida" family="Laelapidae" genus="Hypoaspis" kingdom="Animalia" order="Mesostigmata" pageId="11" pageNumber="476" phylum="Arthropoda" rank="species" species="cavitatis">
