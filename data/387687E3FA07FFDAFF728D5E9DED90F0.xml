<date id="C4611035FA06FFDDFC0B8DAF9B1796D6" box="[1006,1119,2002,2028]" pageId="3" pageNumber="47" value="1991-06-07">
<number id="061724D2FA06FFDDFC0B8DAF9CB596D6" box="[1006,1021,2002,2028]" pageId="3" pageNumber="47" value="7.0">7</number>
