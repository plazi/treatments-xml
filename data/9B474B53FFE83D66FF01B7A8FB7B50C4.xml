<taxonomicName id="D4EE81C6FFE63D67FE28B50CFD8E576F" authorityName="Stal" authorityYear="1872" box="[446,606,1896,1918]" class="Insecta" family="Heterogastridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="59" phylum="Arthropoda" rank="family">Heterogastridae</taxonomicName>
