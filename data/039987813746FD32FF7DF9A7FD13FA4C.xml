<materialsCitation id="3B583CCA3746FD33FF6DF9D4FF0DF97B" collectorName="de Geologia" location="Departamento de Geologia" pageId="0" pageNumber="1" specimenCount="1" stateProvince="Rio de Janeiro">
<superScript id="7C459BDF3746FD33FF6DF9D4FF76F95E" attach="right" box="[138,146,1655,1667]" fontSize="5" pageId="0" pageNumber="1">2</superScript>
