<emphasis id="D0DD39F6763D58772C41E0A71FA4A96A" italics="true" pageId="0" pageNumber="1">Dendrophthora apiculata</emphasis>
</taxonomicName>
, 
<taxonomicName id="5AC8F3DA1A1E608F8925708B00D983DA" genus="D." lsidName="D. coronata" pageId="0" pageNumber="1" rank="species" species="coronata">
<emphasis id="19AA2A167A4420539A19F0FE079F3625" italics="true" pageId="0" pageNumber="1">D. coronata</emphasis>
</taxonomicName>
, and 
<taxonomicName id="97DC9F3ADC9E78FC17109B03CBB23470" genus="D." lsidName="D. lindeniana" pageId="0" pageNumber="1" rank="species" species="lindeniana">
<emphasis id="A001AB4D606572B68A3D96B71C648D8D" italics="true" pageId="0" pageNumber="1">D. lindeniana</emphasis>
