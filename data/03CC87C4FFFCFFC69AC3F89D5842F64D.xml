<number id="3DAD24F5FFF9FFC7992CFD825EEEF64D" box="[888,925,592,618]" pageId="6" pageNumber="67" value="3.5">3.5</number>
long, 
<number id="3DAD24F5FFF9FFC799B8FD825962F64D" box="[1004,1041,592,618]" pageId="6" pageNumber="67" value="2.2">2.2</number>
