<paragraph id="B54C4461570E01C86E0A85242F07EC8A" pageId="null" pageNumber="306">
Artengruppe des 
<taxonomicName id="C110A44ADD2E10A16933A9A3343D2058" authority="L." class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="306" phylum="Tracheophyta" rank="species" species="aparine">
Galium 
<normalizedToken id="37E353FB68079374D39854CB879B4684" originalValue="Aparíne" pageId="null" pageNumber="306">Aparine</normalizedToken>
<authorityName id="8D6221AEF41D16AEFCFF6057BD984C4B" pageId="null" pageNumber="306">L.</authorityName>
