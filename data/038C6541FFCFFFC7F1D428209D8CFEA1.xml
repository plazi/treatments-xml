<figureCitation id="131EC8D2FFCFFFC6F55028BE9C4EF905" box="[1043,1111,1748,1771]" captionStart="FIGURES 1–4" captionStartId="3.[151,269,1974,1998]" captionTargetBox="[226,1361,406,1929]" captionTargetId="figure-107@3.[209,1378,389,1949]" captionTargetPageId="3" captionText="FIGURES 1–4. 1. Head of D. chilensis; 2 Metascutum D. moundi, 3. Antenna of D. moundi, 4.. Sternites VI–VII of D. chilensis." pageId="2" pageNumber="19">Figs 1</figureCitation>
, 
<figureCitation id="131EC8D2FFCFFFC6F52528BE9CB2F905" box="[1126,1195,1748,1771]" captionStart="FIGURE 15–20" captionStartId="7.[151,255,1892,1916]" captionTargetBox="[254,1340,217,1852]" captionTargetId="figure-37@7.[221,1366,194,1868]" captionTargetPageId="7" captionText="FIGURE 15–20. D. moundi (female) 15 Head, 16 Pronotum; D. chilensis (male) 17 Head, 18 Pronotum; D. edentulus 19 Head, 20 Pronotum." pageId="2" pageNumber="19">17, 19</figureCitation>
), pronotal discal setae more developed (
<figureCitation id="131EC8D2FFCFFFC6F0C3289E99E0F8E5" box="[384,505,1780,1803]" captionStart="FIGURE 15–20" captionStartId="7.[151,255,1892,1916]" captionTargetBox="[254,1340,217,1852]" captionTargetId="figure-37@7.[221,1366,194,1868]" captionTargetPageId="7" captionText="FIGURE 15–20. D. moundi (female) 15 Head, 16 Pronotum; D. chilensis (male) 17 Head, 18 Pronotum; D. edentulus 19 Head, 20 Pronotum." pageId="2" pageNumber="19">Figs 18, 20</figureCitation>
) ...................................................................................................................................... 2
</keyLead>
</paragraph>
</keyStep>
<keyStep id="30D1CCF2FFCFFFC6F1D4297E9D82F8A5" pageId="2" pageNumber="19">
<paragraph id="8B9AD457FFCFFFC6F1D4297E9D85F8C5" blockId="2.[151,1436,1684,2027]" box="[151,1436,1812,1835]" pageId="2" pageNumber="19">
