<taxonomicName id="4C064D5C530EFFD6FD0CF96F02F9F8A9" authority="Karny, 1926" authorityName="Karny" authorityYear="1926" box="[699,1011,1773,1799]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="17" pageNumber="468" phylum="Arthropoda" rank="species" species="rammei" subGenus="Tachycines">
<emphasis id="B972EACD530EFFD6FD0CF96F03C2F8A9" box="[699,712,1775,1798]" italics="true" pageId="17" pageNumber="468">T</emphasis>
. (
<emphasis id="B972EACD530EFFD6FD56F96F03E4F8A9" box="[737,750,1775,1798]" italics="true" pageId="17" pageNumber="468">T</emphasis>
