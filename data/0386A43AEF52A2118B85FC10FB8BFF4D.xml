<taxonomicName id="4C2F6EAFEF51A2108A65F842FE5EF829" authorityName="Needham &amp; Murphy ." authorityYear="1924" box="[375,470,1926,1948]" class="Insecta" family="Polymitarcyidae" genus="Campsurus" kingdom="Animalia" order="Ephemeroptera" pageId="38" pageNumber="339" phylum="Arthropoda" rank="species" species="segnis">
