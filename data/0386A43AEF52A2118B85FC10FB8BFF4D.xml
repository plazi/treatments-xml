<taxonomicName id="4C2F6EAFEF51A2108F54F842FA8CF829" authority="Molineri &amp; Salles, 2017" authorityName="Molineri &amp; Salles" authorityYear="2017" box="[1094,1284,1926,1948]" class="Insecta" family="Polymitarcyidae" genus="Campsurus" kingdom="Animalia" order="Ephemeroptera" pageId="38" pageNumber="339" phylum="Arthropoda" rank="species" species="cotaxe" status="sp. nov.">
<emphasis id="B95BC93EEF51A2108F54F842FA8CF829" box="[1094,1284,1926,1948]" italics="true" pageId="38" pageNumber="339">
Campsurus 
<taxonomicName id="4C2F6EAFEF51A2108FD0F843FA8CF829" box="[1218,1284,1927,1948]" pageId="38" pageNumber="339" rank="species" species="cotaxe" status="sp. nov.">cotaxe</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="B95BC93EEF51A2108E1EF842FAD7F82E" bold="true" box="[1292,1375,1926,1947]" pageId="38" pageNumber="339">
