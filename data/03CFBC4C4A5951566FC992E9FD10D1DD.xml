<number id="3DAE1F7D4A5951566DBE958AFD27D38C" box="[736,764,1252,1276]" pageId="10" pageNumber="308" value="13.0">13</number>
September 
<number id="3DAE1F7D4A5951566CDB958AFC1BD38C" box="[901,960,1252,1276]" pageId="10" pageNumber="308" value="2010.0">2010</number>
</date>
, slurp gun, bottle 3, 
<specimenCount id="9D60C6D34A5951566BF2958AFAD4D38D" box="[1196,1295,1252,1277]" pageId="10" pageNumber="308" type="female">1 female</specimenCount>
(cl 
<quantity id="4C9EA0BF4A5951566A63958AFA43D38C" box="[1341,1432,1252,1276]" metricMagnitude="-3" metricUnit="m" metricValue="9.2" pageId="10" pageNumber="308" unit="mm" value="9.2">9.2 mm</quantity>
; infested by bopyrid), MNHN-IU- 
<number id="3DAE1F7D4A5951566D4C9469FD52D250" box="[530,649,1287,1312]" pageId="10" pageNumber="308" value="3541.5" valueMax="5072.0" valueMin="2011.0">2011-5072</number>
