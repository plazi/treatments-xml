<subSubSection id="0E8B7CFD6F700666FF60F915285FC35A" box="[199,1314,1770,1795]" pageId="16" pageNumber="317" type="distribution">
<paragraph id="462E2F766F700666FF60F915285FC35A" blockId="16.[151,1437,258,1867]" box="[199,1314,1770,1795]" pageId="16" pageNumber="317">
<emphasis id="74E5F3646F700666FF60F9152C1DC35A" bold="true" box="[199,352,1770,1795]" pageId="16" pageNumber="317">Distribution.</emphasis>
<collectingRegion id="8455E1946F700666FEC0F9152CC1C35A" box="[359,444,1770,1795]" country="Mexico" name="Mexico" pageId="16" pageNumber="317">
<collectingCountry id="3E866FE66F700666FEC0F9152CC1C35A" box="[359,444,1770,1795]" name="Mexico" pageId="16" pageNumber="317">Mexico</collectingCountry>
</collectingRegion>
to 
<collectingRegion id="8455E1946F700666FE47F9152F24C35A" box="[480,601,1770,1795]" country="Guatemala" name="Guatemala" pageId="16" pageNumber="317">
<collectingCountry id="3E866FE66F700666FE47F9152F24C35A" box="[480,601,1770,1795]" name="Guatemala" pageId="16" pageNumber="317">Guatemala</collectingCountry>
