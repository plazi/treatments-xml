<taxonomicName id="1E37CAF35942B5788B9A96E2CF31DBC7" class="Stenolaemata" family="Lichenoporidae" genus="Lichenopora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lichenopora hispida" order="Cyclostomatida" pageId="0" pageNumber="10672" phylum="Bryozoa" rank="species" species="hispida">Lichenopora hispida</taxonomicName>
