<taxonomicName id="4C7566CC1A69477C4844FD5250C713D9" authorityName="" authorityYear="1758" box="[1318,1433,680,702]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="511" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
