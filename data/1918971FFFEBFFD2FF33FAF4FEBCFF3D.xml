<taxonomicName id="56B15D8AFFE9FFD2FD46FA58FC80FC8D" box="[738,891,1466,1491]" class="Insecta" family="Termitidae" genus="Rhynchotermes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isoptera" pageId="7" pageNumber="508" phylum="Arthropoda" rank="species" species="bulbinasus">
