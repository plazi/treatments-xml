<taxonomicName id="DADF140A521963FAC384A75D011EF8CD" authority="" authorityName="" class="Malacostraca" family="Mysidae" genus="S." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="S. divergens" order="Mysida" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="species" species="divergens">
