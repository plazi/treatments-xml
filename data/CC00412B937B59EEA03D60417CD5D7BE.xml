<taxonomicName id="9BA412B2AC9475B30D1C5E9A024F61CF" authorityName="Walker" authorityYear="1860" class="Insecta" family="Geometridae" genus="Rhuma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rhuma" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="genus">
