<bibRefCitation id="9CF23474B13BC91F8B994F2C474535C8" author="Pitkin, L" journalOrPublisher="Zoological Journal of the Linnean Society" pageId="0" pageNumber="52190" pagination="343 - 412" refId="B5536999" refString="Pitkin, L, Han, H, James, S, 2007. Moths of the tribe Pseudoterpnini (Geometridae: Geometrinae): a review of the genera. Zoological Journal of the Linnean Society 150 (2): 343 - 412" title="Moths of the tribe Pseudoterpnini (Geometridae: Geometrinae): a review of the genera" volume="150" year="2007">Pitkin et al. (2007)</bibRefCitation>
designated 
<taxonomicName id="EFFC64FCB46F5D57E086C46F04920481" authorityName="Warren" authorityYear="1898" class="Insecta" family="Geometridae" genus="Sterictopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sterictopsis" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="genus">
<emphasis id="54B52855C7D7C82DD4DFB6C4149900BD" italics="true" pageId="0" pageNumber="52190">Sterictopsis</emphasis>
</taxonomicName>
and 
<taxonomicName id="75BAD618AF541F2B4A9E404DAC4F45C3" authorityName="Turner" authorityYear="1936" class="Insecta" family="Geometridae" genus="Oxyphanes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oxyphanes" order="Lepidoptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="genus">
<emphasis id="E6070D1BA88D061B66CE0D6DE8E60C83" italics="true" pageId="0" pageNumber="52190">Oxyphanes</emphasis>
</taxonomicName>
