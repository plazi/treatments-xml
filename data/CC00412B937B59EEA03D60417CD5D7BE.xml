<taxonomicName id="A5BD2C0A8B35EB9C037E1E0E78B795CA" authority="" authorityName="" class="Insecta" family="Chironomidae" genus="O." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="O. thiobapta" order="Diptera" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="species" species="thiobapta">
