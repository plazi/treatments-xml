<taxonomicName id="0AB286CC55E7330E81584176497DE5D6" authority="" authorityName="" class="Malacostraca" family="Mysidae" genus="S." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="S. argyraspis" order="Mysida" pageId="0" pageNumber="52190" phylum="Arthropoda" rank="species" species="argyraspis">
<emphasis id="2BAA00E2971B95532CF2F139CE95D2E2" italics="true" pageId="0" pageNumber="52190">S. argyraspis</emphasis>
</taxonomicName>
