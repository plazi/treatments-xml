species 
<taxonomicName id="4C344D31AC052014FD1D7C91D87F8B86" authority="(Deeleman-Reinhold, 1971)" authorityName="Deeleman-Reinhold" authorityYear="1971" baseAuthorityName="Deeleman-Reinhold" baseAuthorityYear="1971" box="[697,1142,720,742]" class="Arachnida" family="Leptonetidae" genus="Sulcia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="585" phylum="Arthropoda" rank="species" species="kanellisi">
