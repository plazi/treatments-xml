</taxonomicName>
, 
<taxonomicName id="72DC4D77D166FFE5FE54FF321F8000C6" baseAuthorityName="Brown" baseAuthorityYear="1995" box="[430,751,198,219]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="1237" phylum="Chordata" rank="species" species="kitangladensis">
<emphasis id="87A8EAE6D166FFE5FE54FF321F8000C6" box="[430,751,198,219]" italics="true" pageId="20" pageNumber="1237">Parvoscincus kitangladensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="72DC4D77D166FFE5FF6AFF111CB000E4" baseAuthorityName="Brown &amp; Alcala" baseAuthorityYear="1980" box="[144,479,228,250]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="1237" phylum="Chordata" rank="species" species="laterimaculatus">
