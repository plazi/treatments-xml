</taxonomicName>
, 
<taxonomicName id="72DC4D77D166FFE5FF6AFF321CEE00C6" baseAuthorityName="Brown &amp; Alcala" baseAuthorityYear="1980" box="[144,385,198,219]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="1237" phylum="Chordata" rank="species" species="lawtoni">
