<taxonomicName id="72DC4D77D161FFE2FBEEFDA018070274" baseAuthorityName="Dumeril &amp; Bibron" baseAuthorityYear="1839" box="[1044,1384,596,617]" class="Reptilia" family="Scincidae" genus="Sphenomorphus" kingdom="Animalia" order="Squamata" pageId="19" pageNumber="1236" phylum="Chordata" rank="species" species="melanopogon">
<emphasis id="87A8EAE6D161FFE2FBEEFDA018070274" box="[1044,1384,596,617]" italics="true" pageId="19" pageNumber="1236">Sphenomorphus melanopogon</emphasis>
</taxonomicName>
, and 
<taxonomicName id="72DC4D77D161FFE2FCC0FD86195D029A" baseAuthorityName="Lidth de Juede" baseAuthorityYear="1905" box="[826,1074,626,647]" class="Reptilia" family="Scincidae" genus="Tytthoscincus" kingdom="Animalia" order="Squamata" pageId="19" pageNumber="1236" phylum="Chordata" rank="species" species="hallieri">
<emphasis id="87A8EAE6D161FFE2FCC0FD86195D029A" box="[826,1074,626,647]" italics="true" pageId="19" pageNumber="1236">Tytthoscincus hallieri</emphasis>
