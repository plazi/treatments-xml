</taxonomicName>
, 
<taxonomicName id="72DC4D77D161FFE2FB73F8AD18F50773" baseAuthorityName="Boulenger" baseAuthorityYear="1894" box="[1161,1434,1881,1902]" class="Reptilia" family="Scincidae" genus="Parvoscincus" kingdom="Animalia" order="Squamata" pageId="19" pageNumber="1236" phylum="Chordata" rank="species" species="luzonensis">
