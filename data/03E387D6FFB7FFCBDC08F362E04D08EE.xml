, 
<taxonomicName id="4C4A4D43FFB7FFCBD846F462E04908EE" authority="(d'Orbigny, 1835)" baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Bulimulidae" genus="Kuschelenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="27" pageNumber="84" phylum="Mollusca" rank="species" species="tupacii" subGenus="Kuschelenia">
<emphasis id="B93EEAD2FFB7FFCBD846F462E6F008EE" italics="true" pageId="27" pageNumber="84">Kuschelenia</emphasis>
(
<emphasis id="B93EEAD2FFB7FFCBDE86F442E16408EE" box="[912,1044,621,644]" italics="true" pageId="27" pageNumber="84">Kuschelenia</emphasis>
) 
<emphasis id="B93EEAD2FFB7FFCBD932F442E11E08EE" box="[1060,1134,621,644]" italics="true" pageId="27" pageNumber="84">tupacii</emphasis>
