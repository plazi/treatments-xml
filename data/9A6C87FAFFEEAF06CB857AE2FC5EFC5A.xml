<quantity id="D53D9B09FFECAF07C9187C53FDD3FE6E" box="[533,614,375,399]" metricMagnitude="-3" metricUnit="m" metricValue="5.2" pageId="6" pageNumber="152" unit="mm" value="5.2">5.2 mm</quantity>
