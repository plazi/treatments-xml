<emphasis id="B90BEAD4FF87987B19E8CCF1FDA4E7C2" bold="true" box="[151,534,1703,1730]" pageId="33" pageNumber="334">
Genus 
<taxonomicName id="4C7F4D45FF87987B1991CCFEFDA4E7C2" authority="Newstead" authorityName="Newstead" box="[238,534,1703,1730]" class="Insecta" family="Margarodidae" genus="Aspidoproctus" kingdom="Animalia" order="Hemiptera" pageId="33" pageNumber="334" phylum="Arthropoda" rank="genus">
