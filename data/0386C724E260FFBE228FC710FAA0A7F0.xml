<taxonomicName id="4C2F0DB1E260FFBE228FC7BBFE10A0E7" authority="Hardy, 1932" authorityName="Hardy" authorityYear="1932" box="[151,500,970,992]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="159" phylum="Arthropoda" rank="species" species="spinifera">
