<subSubSection id="A4CF2DD466A6E871396D9F0CE26DCE24" pageId="16" pageNumber="17" type="reference_group">
<paragraph id="B439F3D88B3A21B498B35A683A2AD8FF" pageId="16" pageNumber="17">
<taxonomicName id="1E82C49CF338EF5590F8BF9D7FC5AD94" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus intactus" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="intactus">
