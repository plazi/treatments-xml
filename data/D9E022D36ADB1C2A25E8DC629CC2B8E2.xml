<taxonomicName id="BE63BF84A16EDCCFBFF78CDDA217C3DF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:36830" class="Insecta" family="Formicidae" genus="Technomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Technomyrmex madecassus Forel" lsidName-HNS="Technomyrmex madecassus Forel" order="Hymenoptera" pageNumber="199" phylum="Arthropoda" rank="species" species="madecassus">Technomyrmex madecassus</taxonomicName>
