<quantity id="4C9E9B51FF91FF98FCFF3321FC07FCFC" box="[884,938,844,865]" metricMagnitude="2" metricUnit="m" metricValue="8.2" pageId="9" pageNumber="788" unit="m" value="820.0">820m</quantity>
