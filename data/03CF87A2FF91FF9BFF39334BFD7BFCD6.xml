<geoCoordinate id="EE525073FF91FF98FC3D3321FB94FCFC" box="[950,1081,844,865]" degrees="0.90427148" direction="south" orientation="latitude" pageId="9" pageNumber="788" precision="1" value="-0.9042715">0.90427148 S</geoCoordinate>
