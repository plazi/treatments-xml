<geoCoordinate id="EE525073FF91FF98FF543309FEDAFCE4" box="[223,375,868,889]" degrees="132.71981431" direction="east" orientation="longitude" pageId="9" pageNumber="788" precision="1" value="132.71982">132.71981431 E</geoCoordinate>
