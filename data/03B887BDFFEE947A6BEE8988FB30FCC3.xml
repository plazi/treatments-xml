<collectionCode id="ED00AE6EFFEE947A69A68B2CFCB6FCB5" LSID="urn:lsid:biocol.org:col:34988" box="[735,829,770,796]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="15" pageNumber="16">MNHN</collectionCode>
