</taxonomicName>
(
<taxonomicName id="BA94A089A32465445972FB8E68265D1D" box="[895,1064,1035,1062]" class="Magnoliopsida" family="Anacardiaceae" kingdom="Plantae" order="Sapindales" pageId="21" pageNumber="516" phylum="Tracheophyta" rank="family">Anacardiaceae</taxonomicName>
