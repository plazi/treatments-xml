<emphasis id="DAAD367FFF8FAB597E33FAE7FB828771" box="[868,1104,1289,1312]" italics="true" pageId="18" pageNumber="19">
<taxonomicName id="2FD991EEFF8FAB597E33FAE7FB9E8771" box="[868,1100,1289,1312]" class="Malacostraca" family="Eusiridae" genus="Regalia" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="gracilimana">
