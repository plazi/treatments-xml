<quantity id="D15FACB9FF8D5A66FBB8FD22FBBFFD15" box="[1049,1143,673,696]" metricMagnitude="-3" metricUnit="m" metricValue="5.4" pageId="2" pageNumber="31" unit="mm" value="5.4">5.40 mm</quantity>
