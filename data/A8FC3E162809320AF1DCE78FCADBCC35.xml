<paragraph id="EAF5F861E11467E44B1DFE619178DA84" pageId="10" pageNumber="69">
Genus 
<taxonomicName id="A0E494D65C1118605EEB70F57686B001" LSID="http://species-id.net/wiki/Paracanace" authority="Mathis and Wirth" class="Insecta" family="Canacidae" genus="Paracanace" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracanace" order="Diptera" pageId="10" pageNumber="69" phylum="Arthropoda" rank="genus">Paracanace Mathis and Wirth</taxonomicName>
