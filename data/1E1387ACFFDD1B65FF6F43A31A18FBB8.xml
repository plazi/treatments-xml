<taxonomicName id="51BA4D39FFDC1B65FDF547E41B8BFC5C" authority="Attems, 1898" authorityName="Attems" authorityYear="1898" box="[525,1187,986,1011]" class="Diplopoda" family="Paradoxosomatidae" genus="Chondromorpha" kingdom="Animalia" order="Polydesmida" pageId="23" pageNumber="474" phylum="Arthropoda" rank="subSpecies" species="xanthotricha" subSpecies="xanthotricha">
