<taxonomicName id="51BA4D39FFDC1B65FB1A47E41D77FBB8" authority="Verhoeff, 1936" authorityName="Verhoeff" authorityYear="1936" class="Diplopoda" family="Paradoxosomatidae" genus="Chondromorpha" kingdom="Animalia" order="Polydesmida" pageId="23" pageNumber="474" phylum="Arthropoda" rank="subSpecies" species="xanthotricha" subSpecies="hamuligera">
<emphasis id="A4CEEAA8FFDC1B65FB1A47E41E86FBB8" italics="true" pageId="23" pageNumber="474">Chondromorpha xanthotricha hamuligera</emphasis>
Verhoeff, 
<number id="2072249DFFDC1B65FDDC47C01D77FBB8" box="[548,607,1022,1047]" pageId="23" pageNumber="474" value="1936.0">1936</number>
