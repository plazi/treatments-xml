<quantity id="60406CAFFF945F06ED1FFF70E6C6FF56" box="[1062,1157,222,245]" metricMagnitude="-3" metricUnit="m" metricValue="3.15" pageId="5" pageNumber="536" unit="mm" value="3.15">3.15 mm</quantity>
