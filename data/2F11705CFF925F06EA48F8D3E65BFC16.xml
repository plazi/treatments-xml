<quantity id="60406CAFFF945F06EC57FF70E12BFEB6" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="5" pageNumber="536" unit="mm" value="2.3">2.30 mm</quantity>
, WHe 
