<taxonomicName id="4C3CB3C61E0C8F542F34F851DAA9F84F" box="[293,430,1928,1950]" class="Reptilia" family="Colubridae" genus="Dendrelaphis" higherTaxonomySource="CoL" kingdom="Animalia" order="Squamata" pageId="18" pageNumber="19" phylum="Chordata" rank="genus">Dendrelaphis</taxonomicName>
