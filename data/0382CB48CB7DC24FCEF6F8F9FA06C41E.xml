<quantity id="4CD3D7BBCB7EC24FCAA6FBE3FA8BC506" box="[1223,1283,1032,1058]" metricMagnitude="3" metricUnit="m" metricValue="8.0" pageId="98" pageNumber="99" unit="km" value="8.0">8 km</quantity>
SE 
<location id="8EF42C85CB7EC24FCB55FBE3FF4CC56E" LSID="urn:lsid:plazi:treatment:0382CB48CB7DC24FCEF6F8F9FA06C41E:8EF42C85CB7EC24FCB55FBE3FF4CC56E" name="Boca del Rio" pageId="98" pageNumber="99">Boca del Rio</location>
