<subSubSection id="C33129D5CB7DC24FCEF6F8B6FA06C41E" lastPageId="98" lastPageNumber="99" pageId="97" pageNumber="98" type="materials_examined">
<paragraph id="8B947A5ECB7DC24CCEF6F8B6FC46C650" blockId="97.[151,974,1885,1908]" box="[151,974,1885,1908]" pageId="97" pageNumber="98">
<materialsCitation id="3B437003CB7DC24CCEF6F8B6FC46C650" box="[151,974,1885,1908]" collectionCode="CISC" collectorName="P. D. Hurd" country="Mexico" location="Mexico" pageId="97" pageNumber="98" specimenCount="1" typeStatus="holotype">
