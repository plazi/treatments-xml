<collectingCountry id="F5CF90A7FF99B312FEB5FDADFE3FFDD7" box="[369,486,571,593]" name="Malawi" pageId="11" pageNumber="68">Nyasaland</collectingCountry>
<date id="F966F6F7FF99B312FE36FDADFD8AFDD7" box="[498,595,571,593]" pageId="11" pageNumber="68" value="1913-03-05">5.III.1913</date>
