<taxonomicName id="4C6C3DB1F212FFD1FB50FA45EB81FA2D" box="[1265,1436,1478,1501]" class="Insecta" family="Tachinidae" genus="Trischidocera" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="282" phylum="Arthropoda" rank="species" species="yunnanensis">
