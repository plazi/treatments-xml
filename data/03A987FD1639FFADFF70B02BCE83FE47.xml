</emphasis>
, 
<taxonomicName id="4C004D681639FFAEFB87B087CA40F84C" authority="Lawrence, 2019" authorityName="Lawrence" authorityYear="2019" box="[1120,1280,1933,1958]" class="Insecta" family="Ciidae" genus="Cis" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="452" phylum="Arthropoda" rank="species" species="macilentus" status="sp. nov.">
