<taxonomicNameLabel id="A2475782163AFFADFE88B666CE88FE62" box="[367,456,367,392]" pageId="2" pageNumber="453" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C004D68163AFFADFE32B679CD3CFE62" authority="Lawrence, 2019" authorityName="Lawrence" authorityYear="2019" box="[469,636,367,392]" class="Insecta" family="Ciidae" genus="Cis" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="453" phylum="Arthropoda" rank="species" species="occidentalis" status="sp. nov">
