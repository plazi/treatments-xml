<taxonomicName id="71C04D63FF9B3E028B7DFD8FFCD8FDD9" authority="Attems, 1905" authorityName="Attems" authorityYear="1905" box="[280,810,548,575]" class="Chilopoda" family="Lithobiidae" genus="Lithobius" kingdom="Animalia" order="Lithobiomorpha" pageId="9" pageNumber="141" phylum="Arthropoda" rank="species" species="argaeensis" subGenus="Ezembius">
