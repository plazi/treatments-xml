<geoCoordinate id="C215A34B58CD5F0FDCE00539C6376751" direction="north" orientation="latitude" precision="1" value="29.54669">N29.54669</geoCoordinate>
, 
<geoCoordinate id="D12C213B691872D867392290098C76F7" direction="east" orientation="longitude" precision="1" value="103.33552">E103.33552</geoCoordinate>
