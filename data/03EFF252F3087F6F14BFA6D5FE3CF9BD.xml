<taxonomicName id="4C4638C7F3087F6F1629A7B5FD1BFA61" box="[513,744,1477,1502]" class="Insecta" family="Carabidae" genus="Systolocranius" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="subSpecies" species="tenebrosus" subSpecies="fatalis">
