</taxonomicName>
, 
<taxonomicName id="4C4638C7F3087F6F16D1A7B5FBADFA61" authorityName="Boheman" authorityYear="1848" box="[761,1118,1477,1502]" class="Insecta" family="Carabidae" genus="Chlaenius" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="species" species="bipustulatus" subGenus="Pachydinodes">
