<taxonomicName id="065EE1851B1940B3817D69571736F550" authority="L." class="Magnoliopsida" family="Rubiaceae" genus="Asperula" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="283" phylum="Tracheophyta" rank="species" species="cynanchica">
