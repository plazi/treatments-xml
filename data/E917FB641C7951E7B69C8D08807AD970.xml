(= 
<taxonomicName id="52574A6D89BBF8D781F827E0581153A3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:245237" class="Insecta" family="Formicidae" genus="Myrmicocrypta" kingdom="Animalia" lsidName-HNS="Myrmicocrypta uncinata" order="Hymenoptera" pageNumber="295" phylum="Arthropoda" rank="species" species="uncinata">uncinata Mayr</taxonomicName>
