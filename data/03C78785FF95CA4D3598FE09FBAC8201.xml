<collectionCode id="ED7FAE56FF95CA4D3791FD4CFD4E83E1" LSID="urn:lsid:biocol.org:col:34871" box="[670,755,648,674]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="8" pageNumber="9">USNM</collectionCode>
