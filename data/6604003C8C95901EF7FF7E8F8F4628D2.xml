<taxonomicName id="54DFEEBC2C759A790C5000A84CCA5FB1" class="Actinopterygii" family="Macrouridae" genus="Coryphaenoides" kingdom="Animalia" order="Gadiformes" pageId="9" pageNumber="26" phylum="Chordata" rank="species" species="serrulatus">Coryphaenoides serrulatus</taxonomicName>
