<subSubSection id="DEA27E77208C280E5BB4A534FD8A465E" type="distribution">
<paragraph id="E133FB8396B760F78AABB594A94CCFF1" pageNumber="88">
<taxonomicName id="3D3FEB5F3516FDDFF579556363126323" class="Pucciniomycetes" family="Pucciniaceae" genus="Puccinia" higherTaxonomySource="CoL" kingdom="Fungi" order="Pucciniales" pageNumber="88" phylum="Basidiomycota" rank="species" species="arechavaletae">P. arechavaletae</taxonomicName>
