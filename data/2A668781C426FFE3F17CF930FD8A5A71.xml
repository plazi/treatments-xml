<taxonomicName id="65CF4D14C438FFE3F78DFE89FACF5904" baseAuthorityName="Miers" baseAuthorityYear="1877" box="[1183,1352,349,375]" class="Malacostraca" family="Armadillidiidae" genus="Eluma" kingdom="Animalia" order="Isopoda" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="caelata">
