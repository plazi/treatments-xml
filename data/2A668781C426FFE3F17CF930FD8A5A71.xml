<taxonomicName id="65CF4D14C438FFE3F087FEEEFB4E5927" authority="Cruz, 1991" authorityName="Cruz" authorityYear="1991" box="[917,1225,313,340]" class="Malacostraca" family="Armadillidiidae" genus="Eluma" kingdom="Animalia" order="Isopoda" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="tuberculata">
