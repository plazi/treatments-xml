</paragraph>
<paragraph id="8BF83684C43B3810D2B3F9FD83522D95" blockId="32.[189,1399,1619,2027]" pageId="32" pageNumber="33">
<taxonomicName id="4C474D07C43B3810D2B3F9FD86982C13" baseAuthorityName="Olliff" baseAuthorityYear="1893" box="[189,408,1620,1646]" class="Insecta" family="Braconidae" genus="Cotesia" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="nonagriae">
