<taxonomicName id="F76C62F3E3D4DF0AA8D08F2FC0757EE6" baseAuthorityName="Burnup" baseAuthorityYear="1914" class="Gastropoda" family="Streptaxidae" genus="Gulella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gulella darglensis subsp. illovoensis" order="Stylommatophora" pageId="0" pageNumber="107" phylum="Mollusca" rank="subSpecies" species="darglensis" subSpecies="illovoensis">
