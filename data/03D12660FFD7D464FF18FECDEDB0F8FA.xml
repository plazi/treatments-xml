<quantity id="4C803A93FFD7D464FD2AFD01ED6AFC98" box="[677,769,750,776]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="1" pageNumber="709" unit="mm" value="2.6">2.6 mm</quantity>
, AM 
<accessionNumber id="942B0A95FFD7D464FCCBFD01EDC9FC98" box="[836,930,750,776]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/P75499" pageId="1" pageNumber="709">P75499</accessionNumber>
