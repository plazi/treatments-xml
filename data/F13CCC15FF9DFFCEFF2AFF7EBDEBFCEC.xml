<collectingCountry id="01823D93FF9DFFCEFEDAFD14BED4FCEC" box="[359,417,754,780]" name="India" pageId="6" pageNumber="583">India</collectingCountry>
(
<collectingRegion id="BB51B3E1FF9DFFCEFE0DFD14BD34FCED" box="[432,577,754,781]" country="India" name="Uttarakhand" pageId="6" pageNumber="583">Uttarakhand</collectingRegion>
), 
<collectingCountry id="01823D93FF9DFFCEFDEEFD14BDEFFCEC" box="[595,666,754,780]" name="Nepal" pageId="6" pageNumber="583">Nepal</collectingCountry>
