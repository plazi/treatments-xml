<quantity id="4CB363D6FFBDFF8CD8E2F81F74D948E8" box="[1162,1227,1972,1995]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="67" pageNumber="68" unit="mm" value="5.0">5 mm</quantity>
