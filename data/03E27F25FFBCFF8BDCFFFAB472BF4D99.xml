<taxonomicName id="4C4BB5B0FFBDFF8CDD45F83A71C1488B" authority="porpita" authorityName="porpita (Linnaeus" authorityYear="1758" box="[301,467,1937,1960]" class="Hydrozoa" family="Porpitidae" genus="Porpita" kingdom="Animalia" order="Anthoathecata" pageId="67" pageNumber="68" phylum="Cnidaria" rank="species" species="porpita">
