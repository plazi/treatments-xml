<paragraph id="EA6B36F7FFEEF70AFF7EFCA4FC9C7E51" blockId="16.[151,844,831,898]" box="[151,844,831,857]" pageId="16" pageNumber="17">
<heading id="B123819BFFEEF70AFF7EFCA4FC9C7E51" bold="true" box="[151,844,831,857]" fontSize="11" level="1" pageId="16" pageNumber="17" reason="1">
<emphasis id="D8A0EAE5FFEEF70AFF7EFCA4FC9C7E51" bold="true" box="[151,844,831,857]" pageId="16" pageNumber="17">
<taxonomicName id="2DD44D74FFEEF70AFF7EFCA4FD7F7E51" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,687,831,857]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="agnetae" status="new species">
<emphasis id="D8A0EAE5FFEEF70AFF7EFCA4FE1B7E51" bold="true" box="[151,459,831,857]" italics="true" pageId="16" pageNumber="17">Cheumatopsyche agnetae</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFFEEF70AFD55FCA4FC9C7E51" box="[700,844,831,857]" pageId="16" pageNumber="17" rank="species">new species</taxonomicNameLabel>
</emphasis>
