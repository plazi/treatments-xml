<subSubSection id="C31C6518A3732766FF54241A18AFF8AC" box="[199,700,1838,1864]" pageId="10" pageNumber="11" type="etymology">
<paragraph id="8BB93693A3732766FF54241A18AFF8AC" blockId="10.[151,1437,1801,2008]" box="[199,700,1838,1864]" pageId="10" pageNumber="11">
<emphasis id="B972EA81A3732766FF54241A1B5DF8AC" bold="true" box="[199,334,1838,1864]" pageId="10" pageNumber="11">Etymology.</emphasis>
