<paragraph id="9C36369D13685412FF33FA5E75C9E488" blockId="25.[151,943,1466,1523]" box="[151,943,1500,1523]" pageId="25" pageNumber="26">
<treatmentCitationGroup id="BC9911B313685412FF33FA5E75C9E488" box="[151,943,1500,1523]" pageId="25" pageNumber="26">
<taxonomicName id="5B894D1E13685412FF33FA5E7722E489" box="[151,324,1500,1523]" class="Insecta" family="Cicadellidae" genus="Onukia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="connexia">
