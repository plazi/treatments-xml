<subSubSection id="C31858E1FFC6FFC9FF463CA2FAF0FD5F" box="[199,1377,727,752]" pageId="13" pageNumber="331" type="etymology">
<paragraph id="8BBD0B6AFFC6FFC9FF463CA2FAF0FD5F" blockId="13.[151,1437,151,752]" box="[199,1377,727,752]" pageId="13" pageNumber="331">
<emphasis id="B976D778FFC6FFC9FF463CA2FEDFFD40" bold="true" box="[199,334,727,752]" pageId="13" pageNumber="331">Etymology.</emphasis>
