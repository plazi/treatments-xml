<taxonomicName id="4C094D0C6442FF9AFE4EA1D2FCB9FC35" authority="Bruggen, 1909" authorityName="Bruggen" authorityYear="1909" box="[395,487,919,942]" pageId="9" pageNumber="64" rank="species" species="sibiricus">sibiricus</taxonomicName>
