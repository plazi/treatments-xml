<taxonomicName id="4C1B4D37422EFFE7FDAAFAE8FC92FAA5" authorityName="White" authorityYear="1917" box="[571,820,1306,1327]" class="Insecta" family="Phoridae" genus="Sciadocera" kingdom="Animalia" order="Diptera" pageId="122" pageNumber="626" phylum="Arthropoda" rank="species" species="rufomaculata">
