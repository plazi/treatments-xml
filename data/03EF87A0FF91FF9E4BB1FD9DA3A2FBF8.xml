<emphasis id="B932EAA4FF90FF9E4AF8FD9DA34FFD98" box="[212,322,559,583]" italics="true" pageId="9" pageNumber="1887">
<typeStatus id="54FD8814FF90FF9E4AF8FD9DA332FD98" box="[212,319,559,583]" pageId="9" pageNumber="1887">Holotype</typeStatus>
.
</emphasis>
3, 
<collectingCountry id="F3517626FF90FF9E4B5DFD9DA3B4FD98" box="[369,441,559,583]" name="China" pageId="9" pageNumber="1887">China</collectingCountry>
, Kwangtung, Kan-Lin San, M. Lien Ping Distr., 
<quantity id="4CBE9B53FF90FF9E4E21FD9DA682FD98" box="[1037,1167,559,583]" metricMagnitude="2" metricUnit="m" metricValue="8.0" metricValueMax="9.0" metricValueMin="7.0" pageId="9" pageNumber="1887" unit="m" value="800.0" valueMax="900.0" valueMin="700.0">700–900 m</quantity>
, 
<date id="FFF81076FF90FF9E4AF8FDFDA362FDB8" box="[212,367,591,615]" pageId="9" pageNumber="1887" value="1940-04-20">20 April 1940</date>
