<subSubSection id="368565433D3BFFC7FF1FD61F01B8C1C5" pageId="25" pageNumber="326" type="etymology">
<paragraph id="7E2036C83D3BFFC7FF1FD61F01B8C1C5" blockId="25.[151,1437,546,2011]" pageId="25" pageNumber="326">
<emphasis id="4CEBEADA3D3BFFC7FF1FD61F001FC17D" bold="true" box="[199,334,870,895]" pageId="25" pageNumber="326">Etymology.</emphasis>
