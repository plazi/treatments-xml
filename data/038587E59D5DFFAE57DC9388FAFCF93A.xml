<collectingCountry id="F33B76639D5CFFAE562C90DDFE6BF98E" box="[359,445,1520,1546]" name="Mexico" pageId="71" pageNumber="72">Mexico</collectingCountry>
(
<collectingRegion id="49E8F8119D5CFFAE568790DDFDFCF98E" box="[460,554,1520,1546]" country="Mexico" name="Hidalgo" pageId="71" pageNumber="72">Hidalgo</collectingRegion>
, 
<collectingRegion id="49E8F8119D5CFFAE557D90DCFD5BF98F" box="[566,653,1521,1547]" country="Mexico" name="Oaxaca" pageId="71" pageNumber="72">Oaxaca</collectingRegion>
