. 
<taxonomicName id="4C474D1C3B70A15F6B5EF8B2AC31F897" authorityName="Cooke" authorityYear="1964" box="[293,562,1807,1831]" class="Arachnida" family="Prodidomidae" genus="Plutonodomus" kingdom="Animalia" order="Araneae" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="kungwensis">
