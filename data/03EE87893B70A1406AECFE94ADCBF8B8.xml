<taxonomicName id="4C474D1C3B6FA14068B2FB37AD58FB12" authorityName="Cooke" authorityYear="1964" box="[713,859,1162,1186]" class="Arachnida" family="Prodidomidae" genus="Plutonodomus" kingdom="Animalia" order="Araneae" pageId="48" pageNumber="49" phylum="Arthropoda" rank="genus">
