<taxonomicName id="813354061E18666FF5DEF687102A39FA" authorityName="Zheng &amp; Chen" authorityYear="2021" class="Insecta" family="Dictyopharidae" genus="Saigona" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Saigona baiseensis" order="Hemiptera" pageId="0" pageNumber="185" phylum="Arthropoda" rank="species" species="baiseensis">
<emphasis id="AC31B7C020E9DDE39EFDAB934F4E3270" italics="true" pageId="0" pageNumber="185">Saigona baiseensis</emphasis>
