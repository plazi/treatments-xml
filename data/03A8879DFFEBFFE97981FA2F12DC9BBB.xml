<subSubSection id="C31B6500FFEBFFE8793EF9CC172D997A" box="[813,1092,1594,1618]" pageId="26" pageNumber="730" type="materials_examined">
<paragraph id="8BBE368BFFEBFFE8793EF9CC172D997A" blockId="26.[813,1092,1594,1618]" box="[813,1092,1594,1618]" pageId="26" pageNumber="730">
