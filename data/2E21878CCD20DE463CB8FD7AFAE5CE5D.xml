<taxonomicName id="61884D19CD20DE463DC5F905FDEAC9EE" baseAuthorityName="E. A. Smith" baseAuthorityYear="1885" box="[429,520,1672,1694]" class="Bivalvia" family="Propeamussiidae" genus="Propeamussium" kingdom="Animalia" order="Pectinoida" pageId="16" pageNumber="417" phylum="Mollusca" rank="species" species="jeffreysii">
