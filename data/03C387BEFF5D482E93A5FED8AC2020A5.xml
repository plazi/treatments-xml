species: 
<taxonomicName id="4C6A4D2BFF5D48289214FEF4A9CF2122" box="[294,402,361,383]" class="Insecta" family="Cicadellidae" genus="Afrovarta" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="297" phylum="Arthropoda" rank="species" species="angelae" status="sp.nov.">
