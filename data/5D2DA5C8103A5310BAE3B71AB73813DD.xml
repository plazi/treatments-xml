<taxonomicName id="20F92792CBF542F2BAEA47CC718FD070" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="61" pageNumber="62" phylum="Mollusca" rank="species" species="fourmiersi">
