</taxonomicName>
; 67, 
<taxonomicName id="6ED14D329911802D48B35B42FCAF6106" authorityName="Stal" authorityYear="1872" box="[707,813,1280,1299]" class="Insecta" family="Pentatomidae" genus="Brachystethus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="441" phylum="Arthropoda" rank="species" species="signoreti">
