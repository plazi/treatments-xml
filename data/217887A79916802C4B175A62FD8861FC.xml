<taxonomicName id="6ED14D329911802D4B0D5B42FD126106" authorityName="Dallas" authorityYear="1851" box="[381,656,1280,1299]" class="Insecta" family="Pentatomidae" genus="Brachystethus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="441" phylum="Arthropoda" rank="species" species="rubromaculatus">
