<taxonomicName id="54DF68A6D740FFAEA770A40366B09F7F" box="[288,411,1538,1563]" class="Trilobita" family="Proetidae" genus="Monodechenella" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Proetida" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="grandis">
