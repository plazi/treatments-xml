<materialsCitation id="AEC53CC80E84FF26FC10AACBFAAAFDF5" collectionCode="SMNH" pageId="191" pageNumber="332" specimenCode="SMNH 771" specimenCount="2" specimenCount-female="1" specimenCount-male="1" typeStatus="holotype">
