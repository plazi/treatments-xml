<specimenCount id="08ABFD1C0E84FF26FC6CAACCFC0BFE01" box="[997,1014,476,492]" pageId="191" pageNumber="332" type="male">♂</specimenCount>
<specimenCode id="4E0B9EEE0E84FF26FC72AACBFBABFE06" box="[1019,1110,475,491]" collectionCode="SMNH" country="Sweden" httpUri="http://grbio.org/cool/8z6v-f8is" name="Department of Paleozoology, Swedish Museum of Natural History" pageId="191" pageNumber="332">SMNH 771</specimenCode>
, labelled as 
<typeStatus id="C11688370E84FF26FB31AACBFAFDFE06" box="[1208,1280,475,491]" pageId="191" pageNumber="332">paratype</typeStatus>
<specimenCount id="08ABFD1C0E84FF26FA8CAACCFAEEFE01" box="[1285,1299,476,492]" pageId="191" pageNumber="332" type="female">♀</specimenCount>
of 
<taxonomicName id="D9AD4D160E84FF26FAA2AACBFA78FE06" baseAuthorityName="Lundblad" baseAuthorityYear="1925" box="[1323,1413,475,491]" class="Arachnida" family="Hygrobatidae" genus="Megapus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="191" pageNumber="332" phylum="Arthropoda" rank="species" species="lacustris">
