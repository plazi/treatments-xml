<subSubSection id="C31696B0FFCCAE13FF26AE24CE0EF85B" box="[199,894,1905,1930]" pageId="25" pageNumber="226" type="diagnosis">
<paragraph id="8BB3C53BFFCCAE13FF26AE24CE0EF85B" blockId="25.[151,1436,1869,2033]" box="[199,894,1905,1930]" pageId="25" pageNumber="226">
