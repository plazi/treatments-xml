<taxonomicName id="D8B84D36B045FFBDF26DFEAFFD0CFEF4" box="[596,714,268,294]" class="Insecta" family="Baetidae" genus="Baetis" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="24" phylum="Arthropoda" rank="species" species="undetermined">
