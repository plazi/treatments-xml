<specimenCount id="9D09FD74AB5CFFF9FF20FEC0FF0DFE84" box="[196,224,303,327]" pageId="2" pageNumber="733" type="female">1♀</specimenCount>
, 
<date id="FFB1103DAB5CFFF9FF0FFEC0FED4FE87" box="[235,313,303,324]" pageId="2" pageNumber="733" value="1997-10-07">7.x.1997</date>
