? and 
<taxonomicName id="77981F88755B9175102E36890D5DD382" class="Insecta" family="Elateridae" genus="Pleonomus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pleonomus canaliculatus" order="Coleoptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="species" species="canaliculatus">
