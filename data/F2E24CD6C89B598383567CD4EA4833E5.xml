<taxonomicName id="F69B1741F0C946753D372E38233576DC" class="Insecta" family="Strophariaceae" genus="Melanotus" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Melanotus caudex" order="Agaricales" pageId="0" pageNumber="79" phylum="Arthropoda" rank="species" species="caudex">
