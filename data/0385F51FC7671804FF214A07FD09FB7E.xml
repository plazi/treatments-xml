<collectingCountry id="F33B0499C7641804FED14BA2FE32FC36" box="[359,430,994,1019]" name="China" pageId="30" pageNumber="527">China</collectingCountry>
: 
<collectingRegion id="49E88AEBC7641804FE0F4BA2FDA3FC36" box="[441,575,994,1019]" country="China" name="Guangdong" pageId="30" pageNumber="527">Guangdong</collectingRegion>
