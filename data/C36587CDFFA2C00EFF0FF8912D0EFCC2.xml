<taxonomicName id="8CCC4D58FFA0C00FFDAFF93D2BC4F930" box="[567,708,1714,1738]" class="Arachnida" family="Corinnidae" genus="Otacilia" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="219" phylum="Arthropoda" rank="species" species="longituba">
