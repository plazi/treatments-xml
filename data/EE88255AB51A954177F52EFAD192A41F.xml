[[ 
<taxonomicName id="D0D2B4265B5A118FAD6709FB11806C47" LSID-ZBK="urn:lsid:zoobank.org:act:0A1502AF-3B84-4BA1-95C1-C24D72CCC5EE" class="Actinopterygii" family="Sisoridae" genus="Pseudecheneis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pseudecheneis Blyth 1860:154" order="Siluriformes" pageNumber="1" phylum="Chordata" rank="genus">Pseudecheneis Blyth</taxonomicName>
