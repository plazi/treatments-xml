<taxonomicName id="D29FC0FFF569562280AA7DB0362AFB12" authority="Westwood, 1832" authorityName="Westwood" authorityYear="1832" box="[978,1112,1158,1182]" class="Insecta" family="Bethylidae" genus="Epyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="372" phylum="Arthropoda" rank="species" species="herschae" status="sp. nov.">
