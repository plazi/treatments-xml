<taxonomicName id="4C0C4D28BB2B5E6A63F1667F9CC06026" authority="Vargas, 2021" authorityName="Vargas" authorityYear="2021" box="[189,448,1056,1082]" class="Insecta" family="Heliodinidae" genus="Helioandesia" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="123" phylum="Arthropoda" rank="species" species="tarregai" status="gen. et sp. nov.">
