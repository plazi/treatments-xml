<number id="CA1F24B0FFF5FFC2F9B2F998FF5CF98B" box="[151,197,1544,1568]" pageId="1" pageNumber="59" value="108.0">108</number>
° 
<number id="CA1F24B0FFF5FFC2F9EBF998FF72F98B" box="[206,235,1544,1568]" pageId="1" pageNumber="59" value="31.0">31</number>
’E
</geoCoordinate>
, elevation 
<quantity id="BB2F9B72FFF5FFC2F8A4F998FE53F98B" box="[385,458,1544,1568]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="1" pageNumber="59" unit="m" value="800.0">800 m</quantity>
a.s.l.); 
<collectionCode id="1AC6AE52FFF5FFC2FB04F999FDF5F98B" box="[545,620,1545,1568]" pageId="1" pageNumber="59">PNKB</collectionCode>
