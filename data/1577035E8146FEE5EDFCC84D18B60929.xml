<quantity id="5A261FAD8146FEE5ED80C915181D0928" box="[235,320,1852,1877]" metricMagnitude="1" metricUnit="m" metricValue="2.55" metricValueMax="5.0" metricValueMin="0.1" pageId="322" pageNumber="323" unit="m" value="25.5" valueMax="50.0" valueMin="1.0">1–50 m</quantity>
