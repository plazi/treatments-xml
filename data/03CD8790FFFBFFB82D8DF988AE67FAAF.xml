<taxonomicName id="4C644D05FFF8FFB8289DFAA6ADE1FAAF" class="Insecta" family="Geometridae" genus="Visiana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="507" phylum="Arthropoda" rank="species" species="incertata">
