<taxonomicName id="4C6F4D77E771FF92FE9AED6AFE16FDC5" authority="Wang, 2021" authorityName="Wang" authorityYear="2021" box="[260,405,574,597]" class="Insecta" family="Panorpidae" genus="Neopanorpa" kingdom="Animalia" order="Mecoptera" pageId="8" pageNumber="211" phylum="Arthropoda" rank="species" species="liuxingyuei" status="sp. nov.">
