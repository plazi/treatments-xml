<treatmentCitation id="370410AFFFE0FFB7FF3CF97DAB454D83" author="Porter CC" box="[142,541,1744,1766]" page="57" pageId="28" pageNumber="325" year="1984">
<taxonomicName id="71A54D3DFFE0FFB7FF3CF97DAB454D83" authority="Porter, 1984: 57" authorityName="Porter" authorityPageNumber="57" authorityYear="1984" box="[142,541,1744,1766]" class="Insecta" family="Ichneumonidae" genus="Thyreodon" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="325" phylum="Arthropoda" rank="species" species="apricus">
<emphasis id="84D1EAACFFE0FFB7FF3CF97DA8394D83" box="[142,353,1745,1766]" italics="true" pageId="28" pageNumber="325">Thyreodon apricus</emphasis>
<bibRefCitation id="D2344B4FFFE0FFB7FEDBF97DAB454D83" author="Porter CC" box="[361,541,1744,1766]" pageId="28" pageNumber="325" pagination="40 - 71" refId="ref45465" refString="Porter CC. 1984. Laticinctus group Thyreodon in the northern Neotropics (Hymenoptera: Ichneumonidae). Wasmann Journal of Biology 42: 40 - 71." type="journal article" year="1984">Porter, 1984: 57</bibRefCitation>
