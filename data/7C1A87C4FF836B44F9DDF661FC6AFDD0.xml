<taxonomicName id="33B34D51FF836B41F9DDF661F8C9F5EF" authority="Bradbury &amp; Williams, 1996" authorityName="Bradbury &amp; Williams" authorityYear="1996" box="[1592,1810,2541,2580]" class="Malacostraca" family="Bogidiellidae" genus="Bogidomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="35" pageNumber="68" phylum="Arthropoda" rank="genus" status="n. gen.">
