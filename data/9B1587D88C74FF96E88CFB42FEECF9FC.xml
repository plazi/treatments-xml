<collectionCode id="75ADAE0B8C74FF96EA66F91DFD52F9D8" box="[637,711,1595,1619]" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="24" pageNumber="25">MZSP</collectionCode>
