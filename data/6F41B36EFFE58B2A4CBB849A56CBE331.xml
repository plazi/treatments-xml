<taxonomicName id="20E879FBFFE58B2A4CE6855856E3E142" authority="(REUSS)" baseAuthorityName="Reuss" baseAuthorityYear="1874" box="[202,497,275,295]" class="Gymnolaemata" family="Buffonellodidae" genus="Hippadenella" kingdom="Animalia" order="Cheilostomatida" pageId="3" pageNumber="96" phylum="Bryozoa" rank="species" species="regularis">
