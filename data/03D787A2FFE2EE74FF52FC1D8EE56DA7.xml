<taxonomicName id="4C7E4D37FFE3EE74FBB6FE358E336DA7" authority="Li &amp; Cai" authorityName="Li &amp; Cai" authorityYear="2021" box="[1139,1354,415,437]" class="Insecta" family="Nosodendridae" genus="Archaenosodendron" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="226" phylum="Arthropoda" rank="species" species="angulare" status="sp. nov.">
