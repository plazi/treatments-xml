<taxonomicName id="87D74D69FFCCAD42FEF670820E19FA01" box="[340,515,1462,1484]" class="Insecta" family="Encyrtidae" genus="Anagyrus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="260" phylum="Arthropoda" rank="species" species="brevifuniculus" status="sp. nov.">
<emphasis id="72A3EAF8FFCCAD42FEF670820E19FA01" box="[340,515,1462,1484]" italics="true" pageId="3" pageNumber="260">A. brevifuniculus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="69905783FFCCAD42FDAF70830E7BFA06" box="[525,609,1462,1483]" pageId="3" pageNumber="260" rank="species">
<emphasis id="72A3EAF8FFCCAD42FDAF70830E7BFA06" bold="true" box="[525,609,1462,1483]" pageId="3" pageNumber="260">sp. nov.</emphasis>
