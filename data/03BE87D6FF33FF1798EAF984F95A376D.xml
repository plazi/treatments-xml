<quantity id="4CEF9B25FF33FF179CEDF985FE9F3697" box="[1168,1246,1655,1682]" metricMagnitude="-1" metricUnit="m" metricValue="4.6482" pageId="162" pageNumber="163" unit="in" value="18.3">18.3 in</quantity>
