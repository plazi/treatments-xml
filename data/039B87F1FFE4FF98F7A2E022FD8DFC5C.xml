<typeStatus id="54898845FFE4FF98F7C7E00CFE55FEA6" box="[413,455,314,333]" pageId="2" pageNumber="619">Type</typeStatus>
species 
<taxonomicName id="4C324D64FFE4FF98F44AE00CFC9DFEA6" authority="Herbst, 1784" authorityName="Herbst" authorityYear="1784" box="[528,783,314,333]" class="Insecta" family="Nitidulidae" genus="Nitidula" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="619" phylum="Arthropoda" rank="species" species="silacea">
