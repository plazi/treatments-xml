<taxonomicName id="67EC4D5E826AFF8C6E19F8C69F1EF8ED" authorityName="Rioual &amp; Gao &amp; Peng &amp; Chu" authorityYear="2013" box="[604,836,1885,1906]" class="Bacillariophyceae" family="Stauroneidaceae" genus="Stauroneis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Naviculales" pageId="3" pageNumber="50" phylum="Bacillariophyta" rank="species" species="lacusvulcani">
