<taxonomicName id="855F60420D7652EAECDC1F3F4DC3B0F0" class="Collembola" family="Entomobryidae" genus="Homidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Homidia taibaiensis" order="Entomobryomorpha" pageId="6" pageNumber="73" phylum="Arthropoda" rank="species" species="taibaiensis">Homidia taibaiensis</taxonomicName>
