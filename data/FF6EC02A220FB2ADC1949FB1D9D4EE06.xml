<geoCoordinate id="C54DBEBAAA055A14440987D2728BF17D" direction="north" orientation="latitude" precision="9" value="34.0945">34°05.67'N</geoCoordinate>
, 
<geoCoordinate id="060EFC21D66EDE660C954BDEDE8E22F8" direction="east" orientation="longitude" precision="9" value="107.706665">107°42.40'E</geoCoordinate>
