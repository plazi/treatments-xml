<emphasis id="B911487CFFB2144DFE8CF05AFEDBE144" box="[287,371,1877,1899]" italics="true" pageId="7" pageNumber="158">Quedius</emphasis>
(
<emphasis id="B911487CFFB2144DFE16F05AFE4AE144" box="[389,482,1877,1899]" italics="true" pageId="7" pageNumber="158">Raphirus</emphasis>
) 
<emphasis id="B911487CFFB2144DFE67F059FDDAE144" box="[500,626,1878,1899]" italics="true" pageId="7" pageNumber="158">ningxiaensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A222F507FFB2144DFDEFF059FD78E144" box="[636,720,1878,1899]" pageId="7" pageNumber="158" rank="species">
<emphasis id="B911487CFFB2144DFDEFF059FD78E144" bold="true" box="[636,720,1878,1899]" pageId="7" pageNumber="158">sp. nov.</emphasis>
