<emphasis id="B911487CFFBD1442FF04F0AEFE82E199" bold="true" box="[151,298,1953,1975]" pageId="8" pageNumber="159">FIGURE 3-1.</emphasis>
<taxonomicName id="4C65EFEDFFBD1442FEA7F0AEFD2FE198" box="[308,647,1953,1975]" class="Insecta" family="Staphylinidae" genus="Quedius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="159" phylum="Arthropoda" rank="species" species="ningxiaensis" status="sp. nov." subGenus="Raphirus">
<emphasis id="B911487CFFBD1442FEA7F0AEFE20E198" box="[308,392,1953,1975]" italics="true" pageId="8" pageNumber="159">Quedius</emphasis>
(
<emphasis id="B911487CFFBD1442FE09F0AEFE5FE198" box="[410,503,1953,1975]" italics="true" pageId="8" pageNumber="159">Raphirus</emphasis>
) 
<emphasis id="B911487CFFBD1442FD9AF0ADFD2FE198" box="[521,647,1954,1975]" italics="true" pageId="8" pageNumber="159">ningxiaensis</emphasis>
</taxonomicName>
