<caption id="DF1AC4E6FFBD1442FF04F0AEFD31E1DB" httpUri="https://zenodo.org/record/245446/files/figure.png" pageId="8" pageNumber="159" targetBox="[181,1405,193,1931]" targetPageId="8">
