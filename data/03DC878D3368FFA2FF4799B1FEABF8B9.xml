<taxonomicName id="4C754D183368FFBCFE4D9B78FDA5F8B8" box="[413,523,1843,1867]" class="Insecta" family="Nymphalidae" genus="Cithaerias" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="555" phylum="Arthropoda" rank="species" species="aurora">
