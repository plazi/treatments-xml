<taxonomicName id="2D5B1C695A13F63C9BFF9897D3ED2D73" class="Magnoliopsida" family="Ebenaceae" genus="Diospyros" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Diospyros morrisiana" order="Ericales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="morrisiana">
