<taxonomicName id="4C35DCDBFFF7FFFBDC91F955FE587E45" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[163,450,1090,1113]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="118" pageNumber="119" phylum="Arthropoda" rank="species" species="distinctus" status="new species">
<emphasis id="B9417B4AFFF7FFFBDC91F955FE587E45" bold="true" box="[163,450,1090,1113]" italics="true" pageId="118" pageNumber="119">Paramblynotus distinctus</emphasis>
</taxonomicName>
