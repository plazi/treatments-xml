<taxonomicName id="D094D8E0FFCA6415DD29FE930E9A4F49" authorityName="Apostolescu" authorityYear="1959" box="[837,1010,265,291]" class="Ostracoda" family="BAIRDIIDAE" genus="Isobythocypris" kingdom="Animalia" order="Halocyprida" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
