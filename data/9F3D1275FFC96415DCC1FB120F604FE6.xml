<taxonomicName id="D094D8E0FFCA6415DABAFE930F454F2C" authority="Herrig, 1979" authorityName="Herrig" authorityYear="1979" class="Ostracoda" family="Bairdiidae" genus="Bairdiacypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="38" pageNumber="39" phylum="Arthropoda" rank="subSpecies" species="triassica" subSpecies="postera">
