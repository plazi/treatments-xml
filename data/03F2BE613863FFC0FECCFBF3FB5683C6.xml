<taxonomicName id="4C5B74F43863FFC0FED4FB10FE7581CE" box="[272,420,1159,1185]" class="Arachnida" family="Eriophyidae" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="42" phylum="Arthropoda" rank="family">Eriophyidae</taxonomicName>
