<taxonomicName id="2DCD9BDB7D408A1FE92BFD96FD68FD96" authority="Thunberg, 1789" authorityName="Thunberg" authorityYear="1789" box="[264,676,535,560]" class="Insecta" family="Baissogryllidae" genus="Locusta" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="53" phylum="Arthropoda" rank="species" species="aethiopica">
