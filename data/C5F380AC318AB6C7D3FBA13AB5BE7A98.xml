<taxonomicName id="E7468035957BC3B3A13F9F94EE36D40A" class="Dicotyledoneae" family="Araliaceae" genus="Polyscias" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Umbelliflorae" pageId="19" pageNumber="52" phylum="Angiospermae" rank="genus">
