<quantity id="EC4305FCEC1FFF8954463D86BFAFFF7B" box="[1102,1267,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="1.1749999999999998" metricValueMax="1.2" metricValueMin="1.15" pageId="10" pageNumber="515" unit="mm" value="1.1749999999999998" valueMax="1.2" valueMin="1.15">1.15–1.20 mm</quantity>
, Edist 
<quantity id="EC4305FCEC1FFF8955373D89BEC4FF7B" box="[1343,1432,152,176]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="10" pageNumber="515" unit="mm" value="0.8">0.8 mm</quantity>
, Ediam 
<quantity id="EC4305FCEC1FFF8950EF3DADBA11FF00" box="[231,333,188,212]" metricMagnitude="-4" metricUnit="m" metricValue="4.699999999999999" pageId="10" pageNumber="515" unit="mm" value="0.47">0.47 mm</quantity>
, LE 
<quantity id="EC4305FCEC1FFF89518A3DADB957FF00" box="[386,523,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="8.7" metricValueMax="8.9" metricValueMin="8.5" pageId="10" pageNumber="515" unit="mm" value="8.7" valueMax="8.9" valueMin="8.5">8.5–8.9 mm</quantity>
