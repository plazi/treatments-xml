<taxonomicName id="4C647DF8B52DFFDAFAB4FC8BFA1C7D51" box="[1389,1455,810,834]" class="Arachnida" family="Species" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="53" phylum="Arthropoda" rank="family">species</taxonomicName>
