<paragraph id="006E064EBA599F074CEE2380FEC0FBD3" blockId="47.[189,1398,909,1040]" pageId="47" pageNumber="48">
<materialsCitation id="B0B90C13BA599F074CEE2380FEC0FBD3" ID-GBIF-Occurrence="2625219314" collectionCode="CA" country="United States of America" location="It" municipality="Harvard University Paleobotanical Collection" pageId="47" pageNumber="48" specimenCode="HU-62706" specimenCount="1" typeStatus="holotype">
