<taxonomicName id="8C814D5EFFAEBA5DFF3DFF60FD07EA90" authority="(Danford &amp; Alston, 1877)" authorityName="Danford &amp; Alston" authorityYear="1877" baseAuthorityName="Danford &amp; Alston" baseAuthorityYear="1877" box="[151,746,151,178]" class="Mammalia" family="Muridae" genus="Apodemus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="62" pageNumber="63" phylum="Chordata" rank="species" species="mystacinus">
