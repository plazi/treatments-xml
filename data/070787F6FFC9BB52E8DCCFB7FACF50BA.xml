<quantity id="48569B05FFC9BB53EB94CB8BFD3850E3" box="[664,757,1692,1719]" metricMagnitude="-3" metricUnit="m" metricValue="8.5" pageId="52" pageNumber="53" unit="mm" value="8.5">8.5 mm</quantity>
