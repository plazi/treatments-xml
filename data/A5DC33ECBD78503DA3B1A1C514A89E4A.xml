<paragraph id="697BD23D4E0E637C736D223097D6C2E6" pageId="1" pageNumber="2">
<taxonomicName id="6F99DF0C5A7D9B07024D709277F4817A" authority="Kang &amp; Long, 2020" authorityName="Kang" authorityYear="2020" class="Insecta" family="Braconidae" genus="Orientocardiochiles" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Orientocardiochiles joeburrowi" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="joeburrowi">
