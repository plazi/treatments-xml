<taxonomicName id="4EF80FABFFBCFFB99DDCF84747A48898" box="[151,394,2012,2034]" class="Insecta" family="Cleridae" genus="Cymatodera" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="534" phylum="Arthropoda" rank="species" species="brailovskyi">
<emphasis id="BB8CA83AFFBCFFB99DDCF84747A48898" box="[151,394,2012,2034]" italics="true" pageId="15" pageNumber="534">
<taxonomicName id="4EF80FABFFBCFFB99DDCF847473A8898" box="[151,276,2012,2034]" class="Insecta" family="Cleridae" genus="Cymatodera" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="534" phylum="Arthropoda" rank="genus">Cymatodera</taxonomicName>
