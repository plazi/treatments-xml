<taxonomicName id="4C194D239E767459FA612D70D2D2D871" authority="Pelaez, 1942" authorityName="Pelaez" authorityYear="1942" box="[264,748,283,310]" class="Insecta" family="Reduviidae" genus="Opisthacidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="mexicanus">
<emphasis id="B96DEAB29E767459FA612D70D2D2D871" bold="true" box="[264,748,283,310]" pageId="32" pageNumber="33">
<emphasis id="B96DEAB29E767459FA612D70D209D872" bold="true" box="[264,567,284,310]" italics="true" pageId="32" pageNumber="33">Opisthacidius mexicanus</emphasis>
(
<bibRefCitation id="EF884B519E767459F92E2D77D2DAD871" author="Pelaez" box="[583,740,283,309]" pageId="32" pageNumber="46" refString="Pelaez, D. (1942) Un nuevo Spiniger mexicano del subgenero Opisthacidius Berg (Hemiptera, Reduviidae). Ciencia, 3, 60 - 62." type="journal article" year="1942">
Peláez, 
<number id="3DD124879E767459F9C12D77D2DAD871" box="[680,740,283,309]" pageId="32" pageNumber="33" value="1942.0">1942</number>
</bibRefCitation>
)
</emphasis>
