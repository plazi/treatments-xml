<quantity id="4CCA8F95F530FFF7FDCDB1B2E8D4F949" box="[590,666,1674,1697]" metricMagnitude="-2" metricUnit="m" metricValue="5.969" pageId="4" pageNumber="3" unit="in" value="2.35">2.35 in</quantity>
