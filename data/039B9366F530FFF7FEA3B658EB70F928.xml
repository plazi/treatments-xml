<quantity id="4CCA8F95F530FFF7FE8DB3E5EB20FB1C" box="[270,366,1245,1268]" metricMagnitude="-1" metricUnit="m" metricValue="4.826" metricValueMax="5.08" metricValueMin="4.572" pageId="4" pageNumber="3" unit="in" value="19.0" valueMax="20.0" valueMin="18.0">18–20 in</quantity>
