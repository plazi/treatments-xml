<quantity id="4C9B4DBF6B37FFA7F6646038FB9FFBA7" box="[1026,1139,1123,1147]" metricMagnitude="2" metricUnit="m" metricValue="1.165" metricValueMax="1.21" metricValueMin="1.1199999999999999" pageId="13" pageNumber="772" unit="m" value="116.5" valueMax="121.0" valueMin="112.0">112-121 m</quantity>
