<quantity id="4CD59B23FF9BFFEE65DE86F1DB11FD87" box="[1206,1366,1993,2019]" metricMagnitude="-3" metricUnit="m" metricValue="2.76" metricValueMax="2.8" metricValueMin="2.72" pageId="1" pageNumber="373" unit="mm" value="2.76" valueMax="2.8" valueMin="2.72">2.72–2.80mm</quantity>
