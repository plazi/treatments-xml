<taxonomicNameLabel id="A24857ADFFD09663FAB4F80FFC348BFC" box="[902,986,2002,2028]" pageId="15" pageNumber="118" rank="species">sp. nov.</taxonomicNameLabel>
(
<quantity id="4CF79B21FFD09663FADBF80CFBD98BFC" box="[1001,1079,2001,2028]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="15" pageNumber="118" unit="cm" value="0.7">0,7 cm</quantity>
chez 
<taxonomicName id="4C0F4D47FFD09663FD4BF80FFB338BFC" box="[1145,1245,2002,2028]" class="Magnoliopsida" family="Rutaceae" genus="Vepris" kingdom="Plantae" order="Sapindales" pageId="15" pageNumber="118" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B97BEAD6FFD09663FD4BF80FFB588BFB" box="[1145,1206,2002,2027]" italics="true" pageId="15" pageNumber="118">Vepris</emphasis>
