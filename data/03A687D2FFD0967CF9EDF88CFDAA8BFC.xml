<quantity id="4CF79B21FFCF967CF8F1F90FFE178AFC" box="[451,505,1746,1772]" metricMagnitude="-2" metricUnit="m" metricValue="6.0" pageId="16" pageNumber="119" unit="cm" value="6.0">6 cm</quantity>
