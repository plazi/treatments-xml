<taxonomicName id="4C0F4D47FFCF967CF84BF96EFD448ADC" authority="Onana" authorityName="Onana" authorityYear="2015" box="[377,682,1714,1740]" class="Magnoliopsida" family="Rutaceae" genus="Vepris" kingdom="Plantae" order="Sapindales" pageId="16" pageNumber="119" phylum="Tracheophyta" rank="species" species="montisbambutensis" status="sp. nov.">
