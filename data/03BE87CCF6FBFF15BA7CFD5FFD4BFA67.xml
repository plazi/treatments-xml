<typeStatus id="54AC8878F6FBFF15BB7AFCE3FF1CFC94" box="[113,169,791,815]" pageId="145" pageNumber="486">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C174D59F6FBFF15BA07FCECFC84FC94" authority="Grouvelle, 1915: 107 - 108" authorityName="Grouvelle" authorityPageNumber="107 - 108" authorityYear="1915" box="[268,817,791,815]" class="Insecta" family="Nitidulidae" genus="Microporum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="145" pageNumber="486" phylum="Arthropoda" rank="species" species="congolense">
