<taxonomicName id="4C5C4D6D46ADC7CEDAC6FABFF7E7C322" authority=": HEINRICH 1974" authorityName=": HEINRICH" authorityYear="1974" box="[178,439,1347,1367]" class="Insecta" family="Curculionidae" genus="Hypomecus" kingdom="Animalia" order="Coleoptera" pageId="185" pageNumber="1502" phylum="Arthropoda" rank="genus">
