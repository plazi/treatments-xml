<geoCoordinate id="EE37C7D95650FFE2C32AFB88FD6CFBAB" box="[613,709,1053,1077]" direction="north" orientation="latitude" pageId="11" pageNumber="112" precision="555" value="25.87">
N 
<number id="3DCBB3395650FFE2C335FB88FD15FBAB" box="[634,700,1053,1077]" pageId="11" pageNumber="112" value="25.87">25.87</number>
