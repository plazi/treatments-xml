</geoCoordinate>
, 
<geoCoordinate id="EE37C7D95650FFE2C39FFB88FC92FBAB" box="[720,827,1053,1077]" direction="east" orientation="longitude" pageId="11" pageNumber="112" precision="555" value="110.42">
E 
