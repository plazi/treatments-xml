<quantity id="4CFB0CFB5650FFE2C2F4FBD4FBA7FBC7" box="[955,1038,1089,1113]" metricMagnitude="3" metricUnit="m" metricValue="1.9" pageId="11" pageNumber="112" unit="m" value="1900.0">1900m</quantity>
