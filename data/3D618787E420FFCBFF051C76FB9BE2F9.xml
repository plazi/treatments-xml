<taxonomicName id="72C84D12E420FFCBFF051C76FED7E126" authority="Dworakowska &amp; Viraktamath, 1975" authorityName="Dworakowska &amp; Viraktamath" authorityYear="1975" box="[151,370,1041,1067]" class="Insecta" family="Cicadellidae" genus="Takama" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="66" phylum="Arthropoda" rank="species" species="jianfenga">
