<taxonomicName id="4C514D69FFBCFF827ED8FBE3FCA6FBC8" box="[749,843,1037,1061]" class="Liliopsida" family="Araceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Alismatales" pageId="1" pageNumber="1878" phylum="Liliopsida" rank="family">Araceae</taxonomicName>
