(
<bibRefCitation id="EFDA4B37405F2939101FFBE3F601FBBD" author="Sanborn, A. F." box="[885,1061,1101,1128]" pageId="19" pageNumber="472" pagination="1 - 80" refId="ref22342" refString="Sanborn, A. F. (2020 d) The cicadas (Hemiptera: Cicadidae) of Ecuador including the descriptions of five new species, a new subtribe, four new synonymies, and fifteen new records. Zootaxa, 4880 (1), 1 - 80. https: // doi. org / 10.11646 / zootaxa. 4880.1.1" type="journal article" year="2020">Sanborn 2020d</bibRefCitation>
), and 
<collectingCountry id="F35C7656405F2939170DFBE3F6E2FBB2" box="[1127,1222,1101,1127]" name="Trinidad and Tobago" pageId="19" pageNumber="472">Trinidad</collectingCountry>
