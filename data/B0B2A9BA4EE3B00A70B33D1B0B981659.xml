<taxonomicName id="6B7321062380B0C680B96F02A87C05B0" class="Magnoliopsida" family="Myrtaceae" genus="Metrosideros" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Metrosideros" order="Myrtales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="genus">Metrosideros</taxonomicName>
