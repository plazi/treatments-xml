<taxonomicName id="0DC33C3B342BC2391A76C0FF8E73271E" class="Polypodiopsida" family="Cibotiaceae" genus="Cibotium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cibotium" order="Cyatheales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="genus">Cibotium</taxonomicName>
