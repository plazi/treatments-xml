<taxonomicName id="D5779B8CF2A8E06164D4ACC7977E1BF4" authority="Fleming, 1821" authorityName="Fleming" authorityYear="1821" genus="Trichiina" lsidName="Trichiina" pageId="136" pageNumber="137" rank="tribe" tribe="Trichiina">Trichiina Fleming, 1821</taxonomicName>
