<geoCoordinate id="26A15046C3088717FE561CDEFE4BF983" box="[423,488,1532,1556]" degrees="0.113" direction="north" orientation="latitude" pageId="50" pageNumber="3593" precision="55" value="0.113">0.113</geoCoordinate>
