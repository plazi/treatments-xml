<geoCoordinate id="26A15046C3088717FCF11F3EFCE7F9A3" box="[768,836,1564,1588]" degrees="0.132" direction="north" orientation="latitude" pageId="50" pageNumber="3593" precision="55" value="0.132">0.132</geoCoordinate>
