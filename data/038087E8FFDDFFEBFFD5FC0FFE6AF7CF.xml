<taxonomicName id="4C294D7DFFDDFFEBFB56FB27FA2AFB4B" baseAuthorityName="L. Koch" baseAuthorityYear="1869" box="[1249,1412,1225,1245]" class="Arachnida" family="Linyphiidae" genus="Caracladus" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="92" phylum="Arthropoda" rank="species" species="avicula">
