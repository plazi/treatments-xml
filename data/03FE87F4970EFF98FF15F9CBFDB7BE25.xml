<taxonomicName id="4C574D619700FF9CFD29FA49FCB6BB37" authority="Schubert, 2019" authorityName="Schubert" authorityYear="2019" box="[683,882,1522,1543]" class="Arachnida" family="Salticidae" genus="Maratus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="90" phylum="Arthropoda" rank="species" species="combustus" status="sp. nov.">
