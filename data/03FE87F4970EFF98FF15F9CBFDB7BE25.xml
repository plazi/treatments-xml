<taxonomicName id="4C574D619702FF9EFC19F9A5FBA7BB03" authority="Schubert, 2019" authorityName="Schubert" authorityYear="2019" box="[923,1123,1566,1587]" class="Arachnida" family="Salticidae" genus="Maratus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="11" pageNumber="92" phylum="Arthropoda" rank="species" species="combustus" status="sp. nov.">
