<caption id="DF7B948684023F6ADED3E6C65CFAF935" box="[151,1436,1682,1704]" httpUri="https://zenodo.org/record/262187/files/figure.png" pageId="4" targetBox="[281,1300,157,1650]" targetPageId="4">
<paragraph id="8BBBC40E84023F6ADED3E6C65CFAF935" blockId="4.[151,1436,1682,1704]" box="[151,1436,1682,1704]" pageId="4">
<emphasis id="B970181C84023F6ADED3E6C65874F93A" bold="true" box="[151,274,1682,1704]" pageId="4">FIGURE 3.</emphasis>
<taxonomicName id="4C04BF8D84023F6ADF5CE6C658E9F935" box="[280,399,1682,1704]" class="Maxillopoda" family="Caligidae" genus="Caligus" kingdom="Animalia" order="Siphonostomatoida" pageId="4" phylum="Arthropoda" rank="species" species="lini" status="n. sp.">
<emphasis id="B970181C84023F6ADF5CE6C658E9F935" box="[280,399,1682,1704]" italics="true" pageId="4">Caligus lini</emphasis>
</taxonomicName>
<emphasis id="B970181C84023F6ADFD1E6C658B2F93A" bold="true" box="[405,468,1682,1703]" pageId="4">
<taxonomicNameLabel id="A243A56784023F6ADFD1E6C658A9F93A" box="[405,463,1682,1703]" pageId="4" rank="species">n. sp.</taxonomicNameLabel>
,
</emphasis>
