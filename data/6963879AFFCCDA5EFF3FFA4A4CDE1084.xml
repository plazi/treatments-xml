<treatmentCitationGroup id="C1DA11A2FFCCDA5FFF3FFAF64C4F15E8" box="[151,516,1940,1962]" pageId="2">
<taxonomicName id="26CA4D0FFFCCDA5FFF3FFAF64C4F15E8" authority="Meigen, 1830: 17" authorityName="Meigen" authorityPageNumber="17" authorityYear="1830" box="[151,516,1940,1962]" class="Insecta" family="Sciomyzidae" genus="Sciomyza" kingdom="Animalia" order="Diptera" pageId="2" phylum="Arthropoda" rank="species" species="limbata">
