<geoCoordinate id="EE6CD39FFFA89D5EFD6FC930FCB9FBBE" box="[734,837,1052,1076]" direction="east" orientation="longitude" pageId="3" pageNumber="368" precision="925" value="29.816668">
<number id="3D90A77FFFA89D5EFD6FC930FD00FBBE" box="[734,764,1052,1076]" pageId="3" pageNumber="368" value="29.0">29</number>
