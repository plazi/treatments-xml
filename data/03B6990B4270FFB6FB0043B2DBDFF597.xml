<quantity id="4CE785F84270FFB6F8BA4A58D9E9F7BB" box="[1840,1984,2061,2095]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="18" pageNumber="296" unit="mm" value="1.1">1.10 mm</quantity>
