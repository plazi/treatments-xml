<date id="FFA10EDD4270FFB6FB0E4A14DACFF7F7" box="[1156,1254,2113,2147]" pageId="18" pageNumber="296" value="1945-01-04">
I 
<quantity id="4CE785F84270FFB6FB144A14DB1EF7F7" box="[1182,1335,2113,2147]" metricMagnitude="-3" metricUnit="m" metricValue="4.45" pageId="18" pageNumber="296" unit="mm" value="4.45">4.45 mm</quantity>
