<quantity id="4CEF9B25FFB5FF919A5FFEF1F830311B" box="[546,625,259,286]" metricMagnitude="-1" metricUnit="m" metricValue="7.0104" pageId="36" pageNumber="37" unit="in" value="27.6">27.6 in</quantity>
