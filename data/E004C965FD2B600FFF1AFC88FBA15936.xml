<collectingCountry id="10BA38E3FD2A600FFEEAFC60FE055936" box="[359,458,1015,1039]" name="Thailand" pageId="29" pageNumber="30">Thailand</collectingCountry>
(
<collectingRegion id="AA69B691FD2A600FFE54FC60FDAD5936" box="[473,610,1015,1039]" country="Thailand" name="Chiang Mai" pageId="29" pageNumber="30">Chiang Mai</collectingRegion>
, 
<collectingRegion id="AA69B691FD2A600FFDE3FC60FD345936" box="[622,763,1015,1039]" country="Thailand" name="Chanthaburi" pageId="29" pageNumber="30">Chanthaburi</collectingRegion>
, 
<collectingRegion id="AA69B691FD2A600FFC8BFC60FC6C5936" box="[774,931,1015,1039]" country="Thailand" name="Kanchanaburi" pageId="29" pageNumber="30">Kanchanaburi</collectingRegion>
and 
<collectingRegion id="AA69B691FD2A600FFC57FC60FBAD5936" box="[986,1122,1015,1039]" country="Thailand" name="Surat Thani" pageId="29" pageNumber="30">Surat Thani</collectingRegion>
).
</paragraph>
