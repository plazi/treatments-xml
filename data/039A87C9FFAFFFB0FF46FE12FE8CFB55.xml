<emphasis id="B947EACDFFAFFFB0FF46FE12FE29FE07" bold="true" box="[246,392,427,451]" pageId="47" pageNumber="48">
<taxonomicName id="4C334D5CFFAFFFB0FF46FE12FE20FE07" authority="BLAGODEROV &amp; GRIMALDI, 2004" authorityName="BLAGODEROV &amp; GRIMALDI" authorityYear="2004" box="[246,385,427,451]" class="Insecta" family="Mycetophilidae" genus="Disparoleia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B947EACDFFAFFFB0FF46FE12FE20FE07" bold="true" box="[246,385,427,451]" italics="true" pageId="47" pageNumber="48">Disparoleia</emphasis>
</taxonomicName>
,
</emphasis>
