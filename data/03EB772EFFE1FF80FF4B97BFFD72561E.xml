<heading id="D0B57154FFE1FF8EFF4B97BFFDC950FD" bold="true" box="[151,549,1482,1508]" fontSize="11" level="1" pageId="23" pageNumber="24" reason="1">
<emphasis id="B9361A2AFFE1FF8EFF4B97BFFDC950FD" bold="true" box="[151,549,1482,1508]" pageId="23" pageNumber="24">
<taxonomicName id="4C42BDBBFFE1FF8EFF4B97BFFE2C50FD" box="[151,448,1482,1508]" class="Insecta" family="Miridae" genus="Rhinocylapus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="pallescens" status="sp. nov.">
<emphasis id="B9361A2AFFE1FF8EFF4B97BFFE2C50FD" bold="true" box="[151,448,1482,1508]" italics="true" pageId="23" pageNumber="24">Rhinocylapus pallescens</emphasis>
