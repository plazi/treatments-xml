<taxonomicName id="4C25A984ED7C002B602DFEB0555BC596" authorityName="Choi" authorityYear="1986" box="[321,561,1750,1776]" class="Arachnida" family="Galumnidae" genus="Pergalumna" kingdom="Animalia" order="Sarcoptiformes" pageId="8" pageNumber="415" phylum="Arthropoda" rank="species" species="jongkyui">
