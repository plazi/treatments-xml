<heading id="D0E181C7B543FFA9FF2EA154C35ED2FC" bold="true" box="[192,821,1129,1154]" fontSize="10" level="2" pageId="5" pageNumber="4" reason="3">
<taxonomicName id="4C164D28B543FFA9FF2EA154C35ED2FC" authority="Allen, 1995" authorityName="Allen" authorityYear="1995" box="[192,821,1129,1154]" class="Insecta" family="Machilidae" genus="Pedetontus" kingdom="Animalia" order="Archaeognatha" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="gershneri" subGenus="Verhoeffilis">
<emphasis id="B962EAB9B543FFA9FF2EA154C35ED2FC" bold="true" box="[192,821,1129,1154]" pageId="5" pageNumber="4">
<emphasis id="B962EAB9B543FFA9FF2EA154C13CD2FF" bold="true" box="[192,343,1129,1153]" italics="true" pageId="5" pageNumber="4">Pedetontus</emphasis>
(
<emphasis id="B962EAB9B543FFA9FE87A154C26ED2FF" bold="true" box="[361,517,1129,1153]" italics="true" pageId="5" pageNumber="4">Verhoeffilis</emphasis>
) 
<emphasis id="B962EAB9B543FFA9FDF9A154C2F1D2FF" bold="true" box="[535,666,1129,1153]" italics="true" pageId="5" pageNumber="4">gershneri</emphasis>
<bibRefCitation id="EF874B5AB543FFA9FD4FA157C35ED2FC" author="Allen" box="[673,821,1130,1154]" firstAuthor="Allen" pageId="5" pageNumber="4" pagination="195 - 198" refId="ref6280" refString="Allen, R. T. 1995. Pedetontus gershneri, a new species of Machilidae from the interior highlands of North America (Insecta: Microcoryphia). Entomological News 106: 195 - 198." type="journal article" year="1995">Allen, 1995</bibRefCitation>
</emphasis>
</taxonomicName>
