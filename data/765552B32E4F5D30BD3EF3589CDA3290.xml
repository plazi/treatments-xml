<taxonomicName id="8EF6BE1FDDB625B2FA943A47AD71A58E" authorityName="Wilkinson" authorityYear="1929" class="Insecta" family="Braconidae" genus="Hecabalodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hecabalodes anthaxiae" order="Hymenoptera" pageId="0" pageNumber="41" phylum="Arthropoda" rank="species" species="anthaxiae">Hecabalodes anthaxiae</taxonomicName>
