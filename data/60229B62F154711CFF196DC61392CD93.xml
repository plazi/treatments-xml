<heading id="B37C9D18F154711CFF196DC613F9CF9B" bold="true" box="[151,692,399,426]" fontSize="11" level="1" pageId="8" pageNumber="409" reason="1">
<emphasis id="DAFFF666F154711CFF196DC613F9CF9B" bold="true" box="[151,692,399,426]" pageId="8" pageNumber="409">
<taxonomicName id="2F8B51F7F154711CFF196DC610D4CF9B" authority="Massa" authorityName="Massa &amp; Annoyer &amp; Perez &amp; Danflous &amp; Duvot" authorityYear="2020" box="[151,409,399,426]" class="Magnoliopsida" family="Sterculiaceae" genus="Paraeulioptera" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="8" pageNumber="409" phylum="Tracheophyta" rank="genus" status="new genus">
<emphasis id="DAFFF666F154711CFF196DC61009CF9B" bold="true" box="[151,324,399,426]" italics="true" pageId="8" pageNumber="409">Paraeulioptera</emphasis>
Massa
</taxonomicName>
, 
<taxonomicNameLabel id="C1CC4B1DF154711CFE2B6DD9136ECF9B" box="[421,547,400,426]" pageId="8" pageNumber="409" rank="genus">new genus</taxonomicNameLabel>
