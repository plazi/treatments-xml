<taxonomicName id="FD4D6569877FEDAAAFE387BF354B386B" class="Sordariomycetes" family="Ophiostomataceae" genus="Phialographium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Phialographium erubescens" order="Ophiostomatales" pageId="0" pageNumber="93" phylum="Ascomycota" rank="species" species="erubescens">
<emphasis id="F2B095A75237B763DC33DB61E8F6AABE" italics="true" pageId="0" pageNumber="93">Phialographium erubescens</emphasis>
</taxonomicName>
