<taxonomicName id="8FEE4D41FF4AFF61FB60F8386446CC9B" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="163" pageNumber="164" phylum="Arthropoda" rank="species" species="neumanni">
