<paragraph id="78E9B12301021933B2653C92E9094E72" pageId="45" pageNumber="98">
ZIMBABWE: W. Matetsi [
<geoCoordinate id="28A8592B80B59A5DA07D64DBB361BE25" degrees="18.15" direction="south" orientation="latitude" precision="555" value="-18.15">18.15S</geoCoordinate>
, 
<geoCoordinate id="31E2CD30C1BC64474BDE7E3722F7A5E4" degrees="25.55" direction="east" orientation="longitude" precision="555" value="25.55">25.55E</geoCoordinate>
], iv.1934 (R.H.R. Stevenson), 2 ♀♀ [SAMC ex NMBZ]; Lupane [
<geoCoordinate id="47990CA2627C02B06BF0B9CA90EB0A1A" degrees="18.55" direction="south" orientation="latitude" precision="555" value="-18.55">18.55S</geoCoordinate>
, 
<geoCoordinate id="447670120784E3EC808E5621295D437D" degrees="27.45" direction="east" orientation="longitude" precision="555" value="27.45">27.45E</geoCoordinate>
], xii.1938 (National Museum S. Rhodesia), 1 ♀ [AMGS], 1 ♂ [SAMC ex NMBZ]; Gwaai [
<geoCoordinate id="55E6BE34BA146F1D17CE87C7CBD1C838" degrees="19.17" direction="south" orientation="latitude" precision="555" value="-19.17">19.17S</geoCoordinate>
, 
<geoCoordinate id="A80C5690C697FB628910EF0E7D57F90F" degrees="27.43" direction="east" orientation="longitude" precision="555" value="27.43">27.43E</geoCoordinate>
], 16.i.1927 (G. Arnold), 1 ♂ (holotype), 1 ♀ (allotype), 1 ♀ [SAMC ex NMBZ]; Insuza River [
<geoCoordinate id="821250AD0A148B81EED7D4376632BBCC" degrees="19.25" direction="south" orientation="latitude" precision="555" value="-19.25">19.25S</geoCoordinate>
, 
<geoCoordinate id="5C3D4CC28EB2E198FAC05EA9EA24C75C" degrees="18.05" direction="east" orientation="longitude" precision="555" value="18.05">18.05E</geoCoordinate>
