<geoCoordinate id="F30A236AF6E7E1DD45CF17304633B966" degrees="20.28" direction="south" orientation="latitude" precision="555" value="-20.28">20.28S</geoCoordinate>
, 
<geoCoordinate id="D7A2A15F677790E18FFAF3BB0CF3BCF7" degrees="14.51" direction="east" orientation="longitude" precision="555" value="14.51">14.51E</geoCoordinate>
