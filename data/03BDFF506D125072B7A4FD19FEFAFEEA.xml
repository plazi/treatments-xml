<paragraph id="8BAB4E466D125075B7A4FC82FC75FC8A" blockId="11.[151,923,870,893]" box="[151,923,870,893]" pageId="11" pageNumber="232">
<taxonomicName id="4C1435C56D125075B7A4FC82FDE7FC8A" authority="Linnaeus, 1771: 530" authorityName="Linnaeus" authorityPageNumber="530" authorityYear="1771" box="[151,521,870,893]" class="Insecta" family="Curculionidae" genus="Silpha" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="232" phylum="Arthropoda" rank="species" species="carolina">
