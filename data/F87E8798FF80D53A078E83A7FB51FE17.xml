<quantity id="B72F9B6BFF80D538054A80C6FD66F911" box="[603,759,1724,1747]" metricMagnitude="-3" metricUnit="m" metricValue="1.305" metricValueMax="1.37" metricValueMin="1.24" pageId="1" pageNumber="138" unit="mm" value="1.305" valueMax="1.37" valueMin="1.24">1.24–1.37 mm</quantity>
(mean = 1.3, SD = 0.04, n = 11; 
