<quantity id="B72F9B6BFF80D538057B80A6FD51F931" box="[618,704,1756,1779]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="1" pageNumber="138" unit="mm" value="1.3">1.3 mm</quantity>
