) to 
<quantity id="4CE99B213E7BFFCBFEE3E23C6CF09C6D" box="[347,404,1598,1622]" metricMagnitude="1" metricUnit="m" metricValue="2.7" pageId="4" pageNumber="179" unit="m" value="27.0">27 m</quantity>
