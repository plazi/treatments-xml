<subSubSection id="B58E6525051CFD5EFF22FA16FBA1FA5E" box="[151,1135,1506,1531]" pageId="26" pageNumber="506" type="materials_examined">
<paragraph id="FD2B36AE051CFD5EFF22FA16FBA1FA5E" blockId="26.[151,1437,1506,2035]" box="[151,1135,1506,1531]" pageId="26" pageNumber="506">
<emphasis id="CFE0EABC051CFD5EFF22FA16FEE5FA5E" bold="true" box="[151,299,1506,1531]" pageId="26" pageNumber="506">
<typeStatus id="222F880C051CFD5EFF22FA16FF1CFA5E" box="[151,210,1506,1531]" pageId="26" pageNumber="506">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="3A944D2D051CFD5EFE8CFA10FD3EFA5E" authority="Haswell, 1881" authorityName="Haswell" authorityYear="1881" box="[313,752,1506,1531]" class="Malacostraca" family="Hymenosomatidae" genus="Hymenosoma" kingdom="Animalia" order="Decapoda" pageId="26" pageNumber="506" phylum="Arthropoda" rank="species" species="rostratum">
<emphasis id="CFE0EABC051CFD5EFE8CFA10FD8BFA5E" box="[313,581,1508,1531]" italics="true" pageId="26" pageNumber="506">Hymenosoma rostratum</emphasis>
Haswell, 
<number id="4B5C2489051CFD5EFD03FA17FD3EFA5E" box="[694,752,1507,1531]" pageId="26" pageNumber="506" value="1881.0">1881</number>
</taxonomicName>
