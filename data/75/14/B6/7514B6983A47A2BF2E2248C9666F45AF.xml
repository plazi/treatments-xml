<paragraph pageId="304" pageNumber="305">
<taxonomicName authorityName="Van Dyke" authorityYear="1943" class="Hexapoda" family="Carabidae" genus="Dyschirius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="304" pageNumber="305" phylum="Arthropoda" rank="species" species="arizonicus">Dyschirius arizonicus</taxonomicName>
