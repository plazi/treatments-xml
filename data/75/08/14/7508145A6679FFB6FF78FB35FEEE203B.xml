) 
<collectingCountry box="[457,563,703,727]" name="Australia" pageId="8" pageNumber="59">Australia</collectingCountry>
, 
<collectionCode LSID="urn:lsid:biocol.org:col:15627" box="[574,637,703,727]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15627" name="Royal Botanic Gardens, National Herbarium of New South Wales" pageId="8" pageNumber="59">NSW</collectionCode>
