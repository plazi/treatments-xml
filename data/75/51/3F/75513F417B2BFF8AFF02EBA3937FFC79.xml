<collectionCode box="[870,931,1575,1601]" pageId="90" pageNumber="91">LGC</collectionCode>
, 
<collectionCode box="[943,1033,1575,1601]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="90" pageNumber="91" type="Museum">USNM</collectionCode>
