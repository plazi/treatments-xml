</taxonomicName>
, 
<taxonomicName authorityName="in Beringia" baseAuthorityName="Gibson" baseAuthorityYear="1920" box="[793,938,378,404]" class="Insecta" family="Noctuidae" genus="Lasionycta" kingdom="Animalia" order="Lepidoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="subfumosa">
