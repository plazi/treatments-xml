, 
<collectionCode box="[235,324,765,791]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="36" pageNumber="37" type="Museum">USNM</collectionCode>
, personal collection of 
<location LSID="urn:lsid:plazi:treatment:75513F417B55FFF3FF02E88E9359FB19:F827D88C7B55FFF2FDC7EAD493DCFCEF" box="[585,753,765,791]" country="United States of America" municipality="Cape Th" name="Glenn Morrell" pageId="36" pageNumber="37" stateProvince="Alaska">Glenn Morrell</location>
