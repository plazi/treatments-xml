<subSubSection pageId="5" pageNumber="373" type="etymology">
<paragraph blockId="5.[828,1463,1336,2100]" pageId="5" pageNumber="373">
<emphasis box="[858,972,1437,1461]" italics="true" pageId="5" pageNumber="373">Etymology</emphasis>
