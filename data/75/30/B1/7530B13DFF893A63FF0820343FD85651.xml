<typeStatus box="[199,258,1089,1115]" pageId="4" pageNumber="465">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Chim &amp; Tong, 2020" authorityName="Chim &amp; Tong" authorityYear="2020" box="[361,635,1089,1115]" class="Malacostraca" family="Typhlotanaidae" genus="Unispinosus" kingdom="Animalia" order="Tanaidacea" pageId="4" pageNumber="465" phylum="Arthropoda" rank="species" species="eopacificus" status="n. sp.">
<emphasis box="[361,635,1089,1115]" italics="true" pageId="4" pageNumber="465">Unispinosus eopacificus</emphasis>
</taxonomicName>
