); 
<taxonomicName authority="Stimpson, 1858" authorityName="Stimpson" authorityYear="1858" class="Malacostraca" family="Hymenosomatidae" genus="Rhynchoplax" higherTaxonomySource="CoL" kingdom="Animalia" order="Decapoda" pageId="29" pageNumber="509" phylum="Arthropoda" rank="species" species="setirostris" status="n. comb.">
<emphasis box="[1290,1304,1016,1039]" italics="true" pageId="29" pageNumber="509">S</emphasis>
. 
<emphasis box="[1325,1436,1017,1040]" italics="true" pageId="29" pageNumber="509">setirostris</emphasis>
(Stimpson, 
<number box="[279,336,1052,1077]" pageId="29" pageNumber="509" value="1858.0">1858</number>
)
