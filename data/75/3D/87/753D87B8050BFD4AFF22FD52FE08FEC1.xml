<taxonomicName authority="Lucas, 1980" authorityName="Lucas" authorityYear="1980" box="[416,780,1463,1488]" class="Malacostraca" family="Hymenosomatidae" genus="Micas" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="493" phylum="Arthropoda" rank="species" species="afecundus">
<emphasis box="[416,609,1463,1488]" italics="true" pageId="13" pageNumber="493">Micas afecundus</emphasis>
(Lucas, 
<number box="[715,772,1464,1488]" pageId="13" pageNumber="493" value="1980.0">1980</number>
)
</taxonomicName>
(ex 
<taxonomicName box="[838,983,1463,1488]" class="Malacostraca" family="Hymenosomatidae" genus="Halicarcinus" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="493" phylum="Arthropoda" rank="genus">
<emphasis box="[838,983,1463,1488]" italics="true" pageId="13" pageNumber="493">Halicarcinus</emphasis>
</taxonomicName>
); 
<taxonomicName authority="Ng &amp; Richer" authorityName="Ng &amp; Richer" box="[1012,1305,1463,1489]" class="Malacostraca" family="Hymenosomatidae" genus="Micas" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="493" phylum="Arthropoda" rank="species" species="falcipes">
<emphasis box="[1012,1140,1463,1488]" italics="true" pageId="13" pageNumber="493">M. falcipes</emphasis>
Ng &amp; Richer
</taxonomicName>
de Forges, 
<number box="[151,211,1500,1525]" pageId="13" pageNumber="493" value="1996.0">1996</number>
; 
<taxonomicName authority="De Man, 1888" authorityName="De Man" authorityYear="1888" box="[221,512,1500,1525]" class="Malacostraca" family="Hymenosomatidae" genus="Micas" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="493" phylum="Arthropoda" rank="species" species="filholi">
<emphasis box="[221,326,1500,1525]" italics="true" pageId="13" pageNumber="493">M. filholi</emphasis>
(De Man, 
<number box="[447,504,1500,1524]" pageId="13" pageNumber="493" value="1888.0">1888</number>
)
</taxonomicName>
