<collectingCountry box="[803,888,1444,1469]" name="Taiwan" pageId="25" pageNumber="505">Taiwan</collectingCountry>
, Peninsular 
<collectingCountry box="[1039,1145,1444,1469]" name="Malaysia" pageId="25" pageNumber="505">Malaysia</collectingCountry>
, 
<collectingCountry box="[1163,1280,1444,1469]" name="Singapore" pageId="25" pageNumber="505">Singapore</collectingCountry>
; intertidal to shallow subtidal (
<quantity box="[349,425,1481,1505]" metricMagnitude="0" metricUnit="m" metricValue="2.5" metricValueMax="3.0" metricValueMin="2.0" pageId="25" pageNumber="505" unit="m" value="2.5" valueMax="3.0" valueMin="2.0">2–3 m</quantity>
).
</paragraph>
</subSubSection>
