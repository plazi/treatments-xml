<emphasis italics="true" pageId="0" pageNumber="23">Vaccinium bangliangense</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Ericaceae" genus="V." kingdom="Plantae" lsidName="V. sciaphilum" order="Ericales" pageId="0" pageNumber="23" phylum="Tracheophyta" rank="species" species="sciaphilum">
