<quantity id="4CEF9B25FFABFF8F9B2DF935F9EE36E4" box="[848,943,1735,1761]" metricMagnitude="3" metricUnit="m" metricValue="2.161" pageId="58" pageNumber="59" unit="m" value="2161.0">2,161 m</quantity>
(
<quantity id="4CEF9B25FFABFF8F9BBDF935FE5F36E7" box="[960,1054,1735,1762]" metricMagnitude="3" metricUnit="m" metricValue="2.1604224" pageId="58" pageNumber="59" unit="ft" value="7088.0">7,088 ft</quantity>
