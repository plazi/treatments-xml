<geoCoordinate id="EE235007FFABFF8F9B0DF950F99536B8" box="[880,980,1698,1725]" degrees="50" direction="north" minutes="48" orientation="latitude" pageId="58" pageNumber="59" precision="925" value="50.8">50°48’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FFABFF8F9B9DF951FE1936BB" box="[992,1112,1699,1726]" degrees="129" direction="west" minutes="06" orientation="longitude" pageId="58" pageNumber="59" precision="925" value="-129.1">129°06’W</geoCoordinate>
), British Columbia (
<bibRefCitation id="EF864B31FFABFF8F9D41F951FB0536E7" author="Milkova" etAl="et al." firstAuthor="Milkova" pageId="58" pageNumber="59" pagination="113 - 123" refId="ref301891" refString="Milkova, V., Hanke, G., Gillespie, G., Fong, K., Boutillier, J. &amp; Bedard, J. (2016) Range records for ten species of Stomiiform, Aulopiform, and Myctophiform fishes in British Columbia, Canada. Northwestern Naturalist, 97, 113 - 123. https: // doi. org / 10.1898 / nwn 15 - 11.1" type="journal article" year="2016">
Milkova 
<emphasis id="B963EAD2FFABFF8F98BBF935FB4036E4" box="[198,257,1735,1761]" italics="true" pageId="58" pageNumber="59">et al.</emphasis>
2016
</bibRefCitation>
) to 
<collectingCountry id="F3007650FFABFF8F9908F935FBF336E4" box="[373,434,1735,1761]" name="Chile" pageId="58" pageNumber="59">Chile</collectingCountry>
(
<bibRefCitation id="EF864B31FFABFF8F99BEF935F82A36E4" author="Pequeno" box="[451,619,1735,1761]" firstAuthor="Pequeno" pageId="58" pageNumber="59" pagination="1 - 132" refId="ref311468" refString="Pequeno R., G. (1989) Peces de Chile lista sistematica revisada y comentada. Revista de Biologia Marina y Oceanografia, 24, 1 - 132." type="journal article" year="1989">Pequeño 1989</bibRefCitation>
