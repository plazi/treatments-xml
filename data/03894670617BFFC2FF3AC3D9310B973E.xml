<taxonomicName id="4C208CE5617BFFC2FE8BC41F37309640" authority="John, 1929: 34" authorityName="John" authorityPageNumber="34" authorityYear="1929" box="[294,774,1086,1108]" class="Insecta" family="Thripidae" genus="Neohydatothrips" kingdom="Animalia" order="Thysanoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="latereostriatus">
