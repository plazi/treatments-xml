, 
<taxonomicName id="379BADDA96A5DD2001580178E8352957" baseAuthorityName="Broderip &amp; Sowerby" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="110" pageNumber="111" phylum="Mollusca" rank="species" species="nux">
