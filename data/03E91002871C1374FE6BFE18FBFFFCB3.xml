<collectionCode id="ED5139D1871C1376FCCEFD59FC51FD7D" box="[869,938,662,686]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="24" pageNumber="2886" type="Museum">FMNH</collectionCode>
