<collectingRegion id="49846FF6871C1376FC5CFD59FB7AFD7D" box="[1015,1153,662,686]" country="United States of America" name="Washington" pageId="24" pageNumber="2886">Washington</collectingRegion>
, 
<collectingCountry id="F357E184871C1376FF0BFD76FF29FD02" box="[160,210,697,721]" name="United States of America" pageId="24" pageNumber="2886">USA</collectingCountry>
