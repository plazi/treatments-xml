<taxonomicName id="4C45BCCAFFF0FFC2FB3CFC8E14A4C835" authority="Ragge, 1954" authorityName="Ragge" authorityYear="1954" box="[1221,1436,833,855]" class="Insecta" family="Baissogryllidae" genus="Anaptygus" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="123" phylum="Arthropoda" rank="species" species="rectus">
