<taxonomicName id="61164CEC6BDB747DE9AA8EF78C622A8E" class="Magnoliopsida" family="Fabaceae" genus="Gleditsia" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="488" phylum="Tracheophyta" rank="species" species="radiata">
<italic id="97D15C5EF7703B625A9DE5B7333F8DF6" pageId="null" pageNumber="488">G. radiata</italic>
