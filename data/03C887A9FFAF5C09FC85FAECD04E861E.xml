<typeStatus id="54DA881DFFAF5C0AFB96FAB6D50D804A" box="[1030,1080,1346,1367]" pageId="1" pageNumber="364">Type</typeStatus>
species: 
<treatmentCitation id="0AC010AEFFAF5C0AFD57FAA9D558806F" author="Moser" box="[711,1133,1373,1394]" page="170" pageId="1" pageNumber="364" year="1918">
