<taxonomicName id="4C384D52FF21984ACC0B4C76FEAEFB87" authorityName="Rasool &amp; Abdel-Dayem &amp; Alattal &amp; Aldhafer" authorityYear="2021" box="[280,382,1060,1085]" class="Insecta" family="Thripidae" genus="Eremiothrips" kingdom="Animalia" order="Thysanoptera" pageId="13" pageNumber="1612" phylum="Arthropoda" rank="species" species="unicolour">
