<location id="6034604B59139832D29988C776C19A43" LSID="urn:lsid:plazi:treatment:ED42878659139832D18D8A4471119A0B:6034604B59139832D29988C776C19A43" box="[899,995,999,1025]" country="Libya" county="Barka" municipality="Bruno Klaptocz" name="Austrian" pageId="70" pageNumber="71" stateProvince="Tarabulus">Austrian</location>
