<taxonomicName id="09F5B3F811D997307BA053E2A443D617" class="Insecta" family="Anthribidae" genus="Xylinada" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xylinada" order="Coleoptera" pageId="243" pageNumber="244" phylum="Arthropoda" rank="genus">Xylinada</taxonomicName>
