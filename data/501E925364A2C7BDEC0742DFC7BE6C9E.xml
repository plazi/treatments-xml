<normalizedToken id="584ACE899630F119D68DCED9B93DFA30" originalValue="grobkörnig" pageNumber="217">grobkoernig</normalizedToken>
; 3 ag; 
<normalizedToken id="9B0811FB6D276E41826A54F761DFD990" originalValue="Körperlänge" pageNumber="217">Koerperlaenge</normalizedToken>
