<taxonomicName id="4C0BAAEB8760FF8BB026FA1FFC7AFA38" box="[809,918,1479,1502]" class="Chilopoda" family="Linotaeniidae" genus="Strigamia" kingdom="Animalia" order="Geophilomorpha" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
