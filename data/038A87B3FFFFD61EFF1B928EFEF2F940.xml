<collectingCountry id="F3347635FFFFD61EFD01977EFD4DF99C" box="[653,712,1605,1629]" name="United States of America" pageId="5" pageNumber="62">USA</collectingCountry>
, 
<collectingCountry id="F3347635FFFFD61EFD5B977EFCCAF99C" box="[727,847,1605,1629]" name="Zimbabwe" pageId="5" pageNumber="62">Zimbabwe</collectingCountry>
(
