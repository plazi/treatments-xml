<collectingCountry id="F3347635FFFFD61EFCA5971AFCF0F9F8" box="[809,885,1569,1593]" name="Ghana" pageId="5" pageNumber="62">Ghana</collectingCountry>
, Hawaii, 
<collectingCountry id="F3347635FFFFD61EFC64971AFBDDF9F8" box="[1000,1112,1569,1593]" name="Honduras" pageId="5" pageNumber="62">Honduras</collectingCountry>
, 
<collectingCountry id="F3347635FFFFD61EFBEB9719FB36F9FB" box="[1127,1203,1570,1594]" name="Kenya" pageId="5" pageNumber="62">Kenya</collectingCountry>
, 
