<quantity id="4C9462FFA779ED21B199FCE3FDB0A65E" box="[451,518,828,853]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="5" pageNumber="6" unit="mm" value="5.0">5 mm</quantity>
