<taxonomicName id="9FEE2E9CB02B131CFA95049DFABBBFB5" box="[1280,1345,1940,1965]" class="Gastropoda" family="Milacidae" genus="Milax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="7" pageNumber="278" phylum="Mollusca" rank="genus">
