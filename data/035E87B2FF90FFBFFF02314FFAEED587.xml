<taxonomicName id="4CF74D27FF90FFBEFF79309FFE6FD745" authority="NHMUK In" authorityName="NHMUK In" box="[236,468,1517,1543]" class="Insecta" family="Diptera (awaiting allocation)" genus="Protomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="lapidaria">
