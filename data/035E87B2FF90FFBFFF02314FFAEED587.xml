<taxonomicName id="4CF74D27FF90FFBEFD4F3360FAA9D76E" authority="Stadtmuseum Siegburg Rott" authorityName="Stadtmuseum Siegburg Rott" box="[730,1298,1553,1580]" class="Insecta" family="Bibionidae" genus="Bibio" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="brachypteroides">
