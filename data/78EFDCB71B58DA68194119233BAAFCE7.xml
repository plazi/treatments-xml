<taxonomicName id="95CC28428C5357CAAEDDA5EC9930B9BE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:141742" authority="Mayr" authorityName="Mayr" class="Hexapoda" family="Formicidae" genus="Bothroponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Bothroponera lamottei Bernard" order="Hymenoptera" pageNumber="188" phylum="Arthropoda" rank="species" species="lamottei">Bothroponera lamottei</taxonomicName>
