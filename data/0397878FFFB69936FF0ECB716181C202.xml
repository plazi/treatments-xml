) and 
<taxonomicName id="4C3E4D1AFFB09936FE8FCA5B6183C23A" box="[278,400,1072,1098]" class="Insecta" family="Brentidae" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="503" phylum="Arthropoda" rank="subFamily" subFamily="Apioninae">Apioninae</taxonomicName>
