<typeStatus id="22174EFBFF907371FF29FF55FF6343B4" box="[151,210,151,176]" pageId="19" pageNumber="568">Type</typeStatus>
material. 
<typeStatus id="22174EFBFF907371FEF2FF55FE0C43B4" box="[332,445,151,176]" pageId="19" pageNumber="568">Holotype</typeStatus>
