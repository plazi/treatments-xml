<materialsCitation id="3B48DE30FFC42E6FFE8B8BACF6991E26" box="[296,680,496,522]" collectingDate="2336-04" collectionCode="R" collectorName="Sultana &amp; Reg. No." pageId="5" pageNumber="2894" specimenCount="1">
