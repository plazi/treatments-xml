<number id="B366A95FFFA1FF87222E8F70D32EFBD2" pageId="13" pageNumber="14" value="43.45">43, 45</number>
, 
<number id="B366A95FFFA1FF87261C8F18D386FBD2" box="[308,384,1119,1145]" pageId="13" pageNumber="14" value="47.43">47, 43</number>
