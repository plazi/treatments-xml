; maximum width 
<number id="B366A95FFFA1FF8726F888A0D3F9FBAA" box="[464,511,999,1025]" pageId="13" pageNumber="14" value="145.0">145</number>
; postocular setae 
<number id="B366A95FFFA1FF8725E588A0D0E8FBAA" box="[717,750,999,1025]" pageId="13" pageNumber="14" value="30.0">30</number>
. Pronotum, length 
<number id="B366A95FFFA1FF8724E388A0D1FCFBAA" box="[971,1018,999,1025]" pageId="13" pageNumber="14" value="105.0">105</number>
; median width 
<number id="B366A95FFFA1FF87238588A0D6DBFBAA" box="[1197,1245,999,1025]" pageId="13" pageNumber="14" value="200.0">200</number>
; major setae am 
<number id="B366A95FFFA1FF87265E8F48D390FB82" box="[374,406,1039,1065]" pageId="13" pageNumber="14" value="35.0">35</number>
, aa?, ml 
<number id="B366A95FFFA1FF87252E8F48D020FB82" box="[518,550,1039,1065]" pageId="13" pageNumber="14" value="25.0">25</number>
, epim 
<number id="B366A95FFFA1FF87255D8F48D093FB82" box="[629,661,1039,1065]" pageId="13" pageNumber="14" value="50.0">50</number>
, pa 
<number id="B366A95FFFA1FF8725ED8F48D0E0FB82" box="[709,742,1039,1065]" pageId="13" pageNumber="14" value="40.0">40</number>
