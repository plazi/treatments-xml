<geoCoordinate id="609ADDBFFFA1FF8726F88F70D01FFBFA" box="[464,537,1079,1105]" direction="south" orientation="latitude" pageId="13" pageNumber="14" precision="925" value="-2.25">
S 
<number id="B366A95FFFA1FF8726CB8F70D3E9FBFA" box="[483,495,1079,1105]" pageId="13" pageNumber="14" value="1.0">1</number>
<number id="B366A95FFFA1FF8726D08F70D01FFBFA" box="[504,537,1079,1105]" pageId="13" pageNumber="14" value="75.0">75</number>
</geoCoordinate>
; S 
<number id="B366A95FFFA1FF87251F8F70D040FBFA" box="[567,582,1079,1105]" pageId="13" pageNumber="14" value="2.0">2</number>
<number id="B366A95FFFA1FF8725788F70D079FBFA" box="[592,639,1079,1105]" pageId="13" pageNumber="14" value="100.0">100</number>
