<quantity id="5F329B1F8061FFC8FD49FF742E7758C8" box="[762,823,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="5" pageNumber="130" unit="mm" value="4.0">4mm</quantity>
