<quantity id="5F329B1F8061FFC8FEB5FF742C1E58CF" box="[262,350,188,212]" metricMagnitude="0" metricUnit="m" metricValue="6.5" metricValueMax="10.0" metricValueMin="3.0" pageId="5" pageNumber="130" unit="m" value="6.5" valueMax="10.0" valueMin="3.0">3–10 m</quantity>
