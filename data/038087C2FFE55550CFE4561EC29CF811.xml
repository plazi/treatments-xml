<emphasis id="B95DEAC6FFE55550CFE45711C1C9FA21" bold="true" box="[151,397,1397,1423]" pageId="65" pageNumber="66">Material examined:</emphasis>
<specimenCount id="9D2FFD5DFFE55550CEE75711C245FA21" box="[404,513,1397,1423]" pageId="65" pageNumber="66" type="larva">10 larvae</specimenCount>
deriving from 
<specimenCount id="9D2FFD5DFFE55550CDC25711C371FA21" box="[689,821,1397,1423]" pageId="65" pageNumber="66" type="female">10 females</specimenCount>
