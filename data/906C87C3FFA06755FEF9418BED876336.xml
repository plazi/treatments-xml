<taxonomicName id="DFC54D56FFA16756FF094003EFCC6462" authorityName="Smith" authorityYear="2008" box="[249,479,1304,1326]" class="Insecta" family="Aulacidae" genus="Pristaulacus" kingdom="Animalia" order="Hymenoptera" pageId="65" pageNumber="332" phylum="Arthropoda" rank="species" species="omninoniger">
