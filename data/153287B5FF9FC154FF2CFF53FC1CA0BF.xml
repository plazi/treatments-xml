<tableCitation id="D0190318FF9CC15AFDB3FEC0FD85A154" box="[520,604,260,284]" captionStart="TABLE 1" captionStartId="7.[151,235,928,949]" captionText="TABLE 1. Characters differentiating A. boltoni and A. helenae." pageId="6" pageNumber="7">
Table 
<number id="2B532484FF9CC15AFDF6FEC0FD85A154" box="[589,604,260,284]" pageId="6" pageNumber="7" value="1.0">1</number>
