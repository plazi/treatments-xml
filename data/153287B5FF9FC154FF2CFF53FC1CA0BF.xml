<taxonomicName id="5A9B4D20FF9CC15AFBDDFF24FB0CA0B0" box="[1126,1237,224,248]" class="Insecta" family="Formicidae" genus="Anomalomyrma" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="boltoni">
<emphasis id="AFEFEAB1FF9CC15AFBDDFF24FB0CA0B0" box="[1126,1237,224,248]" italics="true" pageId="6" pageNumber="7">A. boltoni</emphasis>
</taxonomicName>
