<collectionCode LSID="urn:lsid:biocol.org:col:34994" box="[564,646,1815,1838]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34994" name="Lund University" pageId="21" pageNumber="72">MZLU</collectionCode>
, 
<collectionCode box="[664,752,1815,1839]" httpUri="http://grbio.org/cool/2knt-7f1r" name="Natural History Museum, University of Oslo" pageId="21" pageNumber="72">NHMO</collectionCode>
