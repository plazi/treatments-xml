[
<collectionCode LSID="urn:lsid:biocol.org:col:34795" box="[459,546,1815,1838]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="21" pageNumber="72">FMNH</collectionCode>
