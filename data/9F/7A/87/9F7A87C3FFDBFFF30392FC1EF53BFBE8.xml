<collectingCountry box="[620,715,1089,1115]" name="Portugal" pageId="12" pageNumber="213">Portugal</collectingCountry>
historically (
<bibRefCitation author="Baldock, D. &amp; Wood, T. J. &amp; Cross, I. &amp; Smit, J." box="[862,1089,1089,1115]" pageId="12" pageNumber="213" pagination="1 - 164" refId="ref20369" refString="Baldock, D., Wood, T. J., Cross, I. &amp; Smit, J. (2018) The Bees of Portugal. Entomofauna, Supplement 22, 1 - 164." type="journal article" year="2018">
