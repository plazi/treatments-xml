<emphasis box="[575,832,368,394]" italics="true" pageId="13" pageNumber="214">Scabiosa atropurpurea</emphasis>
</taxonomicName>
and 
<taxonomicName box="[886,1131,368,394]" class="Magnoliopsida" family="Dipsacaceae" genus="Cephalaria" kingdom="Plantae" order="Dipsacales" pageId="13" pageNumber="214" phylum="Tracheophyta" rank="species" species="leucantha">
