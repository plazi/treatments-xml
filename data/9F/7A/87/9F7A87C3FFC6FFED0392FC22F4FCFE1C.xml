<paragraph blockId="17.[151,1437,944,2015]" box="[151,669,944,971]" pageId="17" pageNumber="218">
<heading bold="true" box="[151,669,944,971]" fontSize="11" level="1" pageId="17" pageNumber="218" reason="1">
<taxonomicName authority="(Muller, 1776)" baseAuthorityName="Muller" baseAuthorityYear="1776" box="[151,669,944,971]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="218" phylum="Arthropoda" rank="species" species="nitida" subGenus="Melandrena">
<emphasis bold="true" box="[151,669,944,971]" pageId="17" pageNumber="218">
<emphasis bold="true" box="[151,252,944,971]" italics="true" pageId="17" pageNumber="218">Andrena</emphasis>
(
<emphasis bold="true" box="[269,411,944,971]" italics="true" pageId="17" pageNumber="218">Melandrena</emphasis>
) 
<emphasis bold="true" box="[427,494,944,971]" italics="true" pageId="17" pageNumber="218">nitida</emphasis>
(Müller, 1776)
</emphasis>
