[
<taxonomicName authority="" authorityName="" class="Magnoliopsida" family="Cucurbitaceae" genus="Physedra" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Cucurbitales" pageId="130" pageNumber="131" phylum="Tracheophyta" rank="family">Cucurbitaceae</taxonomicName>
