, ag 
<number box="[711,725,1838,1862]" pageId="4" pageNumber="61" value="2.0">2</number>
, ag 
<number box="[770,784,1838,1862]" pageId="4" pageNumber="61" value="3.0">3</number>
</emphasis>
and 
