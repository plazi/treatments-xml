<taxonomicName authority="Kollmann, 1963" authorityName="Kollmann" authorityYear="1963" box="[865,1190,1857,1884]" class="Globothalamea" family="Trochamminidae" genus="Trochammina" kingdom="Chromista" order="Lituolida" pageId="23" pageNumber="24" phylum="Foraminifera" rank="species" species="alpina">
