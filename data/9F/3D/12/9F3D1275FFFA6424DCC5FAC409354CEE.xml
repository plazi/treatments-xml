<paragraph blockId="22.[189,303,1904,1930]" box="[189,303,1904,1930]" pageId="22" pageNumber="23">
<heading bold="true" box="[189,303,1904,1930]" fontSize="11" level="3" pageId="22" pageNumber="23" reason="3">
