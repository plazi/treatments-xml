<tr pageId="null" pageNumber="57">
<td align="left" pageId="null" pageNumber="57" valign="top">
2*. 
<normalizedToken originalValue="Grundständige" pageId="null" pageNumber="57">Grundstaendige</normalizedToken>
<normalizedToken originalValue="Blätter" pageId="null" pageNumber="57">Blaetter</normalizedToken>
nicht 
<normalizedToken originalValue="überwinternd" pageId="null" pageNumber="57">ueberwinternd</normalizedToken>
, zur 
<normalizedToken originalValue="Blütezeit" pageId="null" pageNumber="57">Bluetezeit</normalizedToken>
noch 
<normalizedToken originalValue="unvollständig" pageId="null" pageNumber="57">unvollstaendig</normalizedToken>
entwickelt; 
<normalizedToken originalValue="Perigonblätter" pageId="null" pageNumber="57">Perigonblaetter</normalizedToken>
