<paragraph pageId="4" pageNumber="23">
<taxonomicName class="Lecanoromycetes" family="Curculionidae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xyleborus atratus" order="Coleoptera" pageId="4" pageNumber="23" phylum="Ascomycota" rank="species" species="atratus">Xyleborus atratus</taxonomicName>
