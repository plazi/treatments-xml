(
<bibRefCitation author="Gunther" box="[262,433,693,719]" pageId="16" pageNumber="45" refString="Gunther, K. (1933) Uber eine kleine Sammlung von Phasmoiden und Forficuliden aus Melanesien. Verhandlungen der Naturforschenden Gesellschaft, Basel, 44 (2): 151 - 161." type="journal article" year="1933">
Günther, 
<number box="[373,433,693,719]" pageId="16" pageNumber="17" value="1933.0">1933</number>
</bibRefCitation>
