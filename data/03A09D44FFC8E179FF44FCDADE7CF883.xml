<subSubSection id="C3137FD9FFC8E17DFF44FC4ADCD3FC62" box="[151,960,929,955]" pageId="2" pageNumber="34" type="materials_examined">
<paragraph id="8BB62C52FFC8E17DFF44FC4ADCD3FC62" blockId="2.[151,1437,929,2038]" box="[151,960,929,955]" pageId="2" pageNumber="34">
<emphasis id="B97DF040FFC8E17DFF44FC4ADE28FC62" bold="true" box="[151,315,929,955]" pageId="2" pageNumber="34">
<typeStatus id="54B292F0FFC8E17DFF44FC4ADFC7FC62" box="[151,212,929,955]" pageId="2" pageNumber="34">Type</typeStatus>
