<taxonomicName id="4C104D06FFCE5C77FDEAF8A8FDFF3D4B" box="[530,637,1838,1863]" class="Insecta" family="Blattidae" genus="Blatta" kingdom="Animalia" order="Blattodea" pageId="17" pageNumber="88" phylum="Arthropoda" rank="species" species="orientalis">
