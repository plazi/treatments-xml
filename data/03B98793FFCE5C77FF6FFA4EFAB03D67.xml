<collectionCode id="ED01AE40FFCE5C77FD58F8A9FD773D4A" box="[672,757,1839,1862]" httpUri="http://grbio.org/cool/pvva-bewi" name="Martin Luther Universitaet" pageId="17" pageNumber="88">MLUH</collectionCode>
