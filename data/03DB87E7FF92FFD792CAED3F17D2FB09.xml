<taxonomicName id="4C724D72FF92FFD792F0ED3F1312FCA0" authority="(Martius. ex Bentham 1837: 77) Urban (1928: 457)" authorityName="Urban" authorityPageNumber="457" authorityYear="1928" baseAuthorityName="Martius. ex Bentham" baseAuthorityPageNumber="77" baseAuthorityYear="1837" box="[178,1071,842,869]" class="Magnoliopsida" family="Fabaceae" genus="Macroptilium" kingdom="Plantae" order="Fabales" pageId="20" pageNumber="265" phylum="Tracheophyta" rank="species" species="erythroloma">
