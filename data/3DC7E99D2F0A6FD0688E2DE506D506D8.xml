<taxonomicName id="D37F338F5D1795BED7FD681F4CBBC9E2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182061" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole obrima Wilson" lsidName-HNS="Pheidole obrima Wilson" order="Hymenoptera" pageNumber="700" phylum="Arthropoda" rank="species" species="obrima">obrima</taxonomicName>
