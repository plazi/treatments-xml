, 
<taxonomicName id="E8C1312C466192FAF0422ADA2672B4FD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34070" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole tristis (Smith)" lsidName-HNS="Pheidole tristis (Smith)" order="Hymenoptera" pageNumber="700" phylum="Arthropoda" rank="species" species="tristis">tristis</taxonomicName>
