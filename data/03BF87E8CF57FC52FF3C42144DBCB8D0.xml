<emphasis id="B962EAECCF57FC52FF6C441C4C67B868" bold="true" box="[199,315,1123,1149]" pageId="11" pageNumber="96">Remarks.</emphasis>
<taxonomicName id="4C164D7DCF57FC52FEEB441C4F49B868" authorityName="Jaschhof &amp; Hippa" authorityYear="2005" box="[320,533,1123,1149]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="96" phylum="Arthropoda" rank="species" species="acutistylus">
