<heading id="F27BDF651B2E29757ED9C35FF72DFC09" bold="true" box="[151,822,870,896]" fontSize="11" level="1" pageId="14" pageNumber="15" reason="1">
<emphasis id="9BF8B41B1B2E29757ED9C35FF72DFC09" bold="true" box="[151,822,870,896]" pageId="14" pageNumber="15">
Family 
<taxonomicName id="6E8C138A1B2E29757EB8C35FF60FFC09" authority="Gray, 1867" authorityName="Gray" authorityYear="1867" box="[246,532,870,896]" class="Demospongiae" family="Geodiidae" kingdom="Animalia" order="Astrophorida" pageId="14" pageNumber="15" phylum="Porifera" rank="family">
Geodiidae 
<bibRefCitation id="CD1D15F81B2E29757F32C35FF60FFC09" author="Gray" box="[380,532,870,896]" pageId="14" pageNumber="15" refString="Gray, J. E. (1867 a) Notes on the Arrangement of Sponges, with the Descriptions of some New Genera. Proceedings of the Zoological Society of London, 1867 (2), 492 - 558." type="journal article" year="1867" yearSuffix="a">
Gray, 
<number id="1F447A2E1B2E29757F85C35FF61DFC09" box="[459,518,870,896]" pageId="14" pageNumber="15" value="1867.0">1867</number>
a
</bibRefCitation>
</taxonomicName>
(Subfamily 
<taxonomicName id="6E8C138A1B2E29757CE3C35FF735FC09" box="[685,814,870,896]" class="Demospongiae" family="Geodiidae" kingdom="Animalia" order="Astrophorida" pageId="14" pageNumber="15" phylum="Porifera" rank="subFamily" subFamily="Geodiinae">Geodiinae</taxonomicName>
