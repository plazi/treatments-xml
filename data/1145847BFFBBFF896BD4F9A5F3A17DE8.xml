</taxonomicName>
: 
<taxonomicName id="5EEC4EEEFFBBFF8A6F53F8D1F2B178C2" authority="Ludwig &amp; Heding, 1935" authorityName="Ludwig &amp; Heding" authorityYear="1935" box="[1040,1432,1942,1966]" class="Holothuroidea" family="Cucumariidae" genus="Cucumella" kingdom="Animalia" order="Dendrochirotida" pageId="1" pageNumber="41" phylum="Echinodermata" rank="species" species="triplex">
<emphasis id="AB98E97FFFBBFF8A6F53F8D1F30A78C2" box="[1040,1059,1943,1966]" italics="true" pageId="1" pageNumber="41">C</emphasis>
. 
<emphasis id="AB98E97FFFBBFF8A6F72F8D0F35378C2" box="[1073,1146,1942,1966]" italics="true" pageId="1" pageNumber="41">triplex</emphasis>
<bibRefCitation id="FD7D489CFFBBFF8A6FC2F8D0F2B178C2" author="Ludwig" box="[1153,1432,1942,1966]" pageId="1" pageNumber="41" refString="Ludwig, H. L. &amp; Heding, S. G. (1935) Die Holothurien I. Fußlose und dendrochirote Formen. Wissenschaftliche Ergebnisse der Deutschen Tiefsee - Expedition auf dem Damfer ' Valdivia' 1898 - 1899, 24 (2), 123 - 241, 2 plates." type="book chapter" year="1935">
Ludwig &amp; Heding, 
<number id="2F24274AFFBBFF8A6E1EF8D0F2B178C2" box="[1373,1432,1942,1966]" pageId="1" pageNumber="41" value="1935.0">1935</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="5EEC4EEEFFBBFF8A6BD4F8FDF54778BE" authority="Heding &amp; Panning, 1954" authorityName="Heding &amp; Panning" authorityYear="1954" box="[151,622,1978,2003]" class="Holothuroidea" family="Cucumariidae" genus="Cucumella" kingdom="Animalia" order="Dendrochirotida" pageId="1" pageNumber="41" phylum="Echinodermata" rank="species" species="problematica">
<emphasis id="AB98E97FFFBBFF8A6BD4F8FDF66578BF" box="[151,332,1979,2003]" italics="true" pageId="1" pageNumber="41">C. problematica</emphasis>
<bibRefCitation id="FD7D489CFFBBFF8A6A17F8FCF54778BE" author="Heding" box="[340,622,1978,2002]" pageId="1" pageNumber="41" refString="Heding, S. G. &amp; Panning, A. (1954) Phyllophoridae. Eine Bearbeitung der polytentaculaten dendrochiroten Holothurien des zoologischen Museums in Kopenhagen. Spolia Zoologica Musei Hauniensis, XIII 13, 7 - 209." type="book chapter" year="1954">
Heding &amp; Panning, 
<number id="2F24274AFFBBFF8A6975F8FCF54778BE" box="[566,622,1978,2002]" pageId="1" pageNumber="41" value="1954.0">1954</number>
</bibRefCitation>
</taxonomicName>
