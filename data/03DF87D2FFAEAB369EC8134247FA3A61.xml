<specimenCount id="9D70FD4DFFAEAB379D4A1786453A3F9E" box="[789,871,1955,1981]" pageId="66" pageNumber="67" type="generic">
<quantity id="4C8E9B21FFAEAB379D4A1786453E3F9E" box="[789,867,1955,1981]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="66" pageNumber="67" unit="m" value="300.0">
