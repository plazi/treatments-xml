<taxonomicName id="4C764D47FFAFAB369FB910CD44383922" authorityName="Borkent" authorityYear="2008" box="[486,613,232,257]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="globosa">
