<collectingRegion id="49B052A4AA7C6730D7D6FC1793F0916A" box="[999,1201,1003,1028]" country="Australia" name="Western Australia" pageId="15" pageNumber="62">
<collectingRegion id="49B052A4AA7C6730D7D6FC179303916D" box="[999,1090,1004,1027]" country="Papua New Guinea" name="Western" pageId="15" pageNumber="62">Western</collectingRegion>
<collectingCountry id="F363DCD6AA7C6730D07BFC1093F0916A" box="[1098,1201,1003,1028]" name="Australia" pageId="15" pageNumber="62">Australia</collectingCountry>
</collectingRegion>
