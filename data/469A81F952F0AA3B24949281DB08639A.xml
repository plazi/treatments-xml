<taxonomicName id="77A6648FF6FB77B42AD10B6ECCD2F813" class="Insecta" family="Curculionidae" genus="Platygaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platygaster semiclavata" order="Coleoptera" pageId="2" pageNumber="101" phylum="Arthropoda" rank="species" species="semiclavata">Platygaster semiclavata</taxonomicName>
