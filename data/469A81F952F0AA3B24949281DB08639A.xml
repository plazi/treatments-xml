<taxonomicName id="EBC42429E92C9F955D265F267D105CDD" class="Insecta" family="Curculionidae" genus="Platygaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platygaster semiclavata" order="Coleoptera" pageId="2" pageNumber="101" phylum="Arthropoda" rank="species" species="semiclavata">Platygaster semiclavata</taxonomicName>
