<taxonomicName id="C94A6E7536CE897C67A0C101170CA45F" class="Insecta" family="Curculionidae" genus="Platygaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platygaster marikovskii" order="Coleoptera" pageId="2" pageNumber="101" phylum="Arthropoda" rank="species" species="marikovskii">Platygaster marikovskii</taxonomicName>
