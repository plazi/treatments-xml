<collectingCountry id="F338767835531C4DB911F868B097F82B" box="[369,444,1952,1978]" name="Belize" pageId="4" pageNumber="21">Belize</collectingCountry>
(
<typeStatus id="5494884A35531C4DB9AEF868B329F82B" box="[462,514,1952,1978]" pageId="4" pageNumber="21">type</typeStatus>
locality); 
<collectingCountry id="F338767835531C4DBAE2F868B3EFF82B" box="[642,708,1952,1978]" name="Cuba" pageId="4" pageNumber="21">Cuba</collectingCountry>
; Yucatan Peninsula of 
<collectingCountry id="F338767835531C4DBBBCF868B512F82B" box="[988,1081,1952,1978]" name="Mexico" pageId="4" pageNumber="21">Mexico</collectingCountry>
; 
<collectingCountry id="F338767835531C4DBC29F868B544F82B" box="[1097,1135,1952,1978]" name="United States of America" pageId="4" pageNumber="21">US</collectingCountry>
<collectingCountry id="F338767835531C4DBC19F868B40AF82B" box="[1145,1313,1952,1978]" name="Virgin Islands" pageId="4" pageNumber="21">Virgin Islands</collectingCountry>
(
<bibRefCitation id="EFBE4B1935531C4DBD54F868B1F8F873" author="Barnard" pageId="4" pageNumber="29" refString="Barnard, K. H. (1925) A revision of the family Anthuridae (Crustacea: Isopoda) with remarks on certain morphological peculiarities. Journal of the Linnean Society (Zoology) 36, 109 - 160." type="journal article" year="1925">
Barnard, 
<number id="3DE724CF35531C4DB8F7F800B1F8F873" box="[151,211,1992,2018]" pageId="4" pageNumber="21" value="1925.0">1925</number>
