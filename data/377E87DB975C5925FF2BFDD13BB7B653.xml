<taxonomicName id="78D74D4E975C5925FEAEFD293BDBB676" authority="Rognes, 2011" authorityName="Rognes" authorityYear="2011" box="[274,402,747,771]" class="Insecta" family="Calliphoridae" genus="Mafikengia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus" status="gen. nov.">
