<heading id="D0D78195FF9BFFEEFF61C4DB3DFB3BFE" bold="true" box="[151,481,974,1000]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<taxonomicName id="4C204D7AFF9BFFEEFF61C4DB3DFB3BFE" authority="Stevens, 1829" authorityName="Stevens" authorityYear="1829" box="[151,481,974,1000]" class="Insecta" family="Tortricidae" genus="Phtheochroa" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis id="B954EAEBFF9BFFEEFF61C4DB3DFB3BFE" bold="true" box="[151,481,974,1000]" pageId="1" pageNumber="2">
<emphasis id="B954EAEBFF9BFFEEFF61C4DB3D293BFE" bold="true" box="[151,307,974,1000]" italics="true" pageId="1" pageNumber="2">Phtheochroa</emphasis>
Stevens, 
<number id="3DE824DEFF9BFFEEFE50C4DB3DFB3BFE" box="[422,481,974,1000]" pageId="1" pageNumber="2" value="1829.0">1829</number>
</emphasis>
