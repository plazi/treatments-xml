<taxonomicName id="E10B387C5B1D721BE75242DE31872CD7" authorityName="Weber" authorityYear="1801" class="Hexapoda" family="Carabidae" genus="Calosoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="171" pageNumber="172" phylum="Arthropoda" rank="genus">
