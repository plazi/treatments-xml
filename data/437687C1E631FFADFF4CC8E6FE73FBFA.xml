<paragraph id="CB6036D7E631FFAAFF4CCB69FC6DF95C" blockId="43.[151,1436,1581,1668]" box="[151,921,1645,1668]" pageId="43" pageNumber="44">
<typeStatus id="14648875E631FFAAFF4CCB69FF38F95C" box="[151,204,1645,1668]" pageId="43" pageNumber="44">Type</typeStatus>
species: 
<taxonomicName id="0CDF4D54E631FFAAFEF6CB69FD66F95C" authority="Fain, 1956" authorityName="Fain" authorityYear="1956" box="[301,658,1645,1668]" class="Arachnida" family="Gastronyssidae" genus="Rodhainyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="yunkeri">
