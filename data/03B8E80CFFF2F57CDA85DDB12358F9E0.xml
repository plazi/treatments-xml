<paragraph id="8BAE591AFFF2F573DA85DD9C2325FF3D" blockId="7.[151,542,151,211]" box="[151,542,186,211]" pageId="7" pageNumber="508">
(
<figureCitation id="132A459FFFF2F573DA8DDD9C21D7FF3D" box="[159,236,186,211]" captionStart="FIGURE 1" captionStartId="3.[151,250,1890,1912]" captionTargetBox="[151,1436,932,1868]" captionTargetId="figure@3.[151,1436,932,1869]" captionTargetPageId="3" captionText="FIGURE 1. Heads, mesosomata and metasomata of Apophua, dorsal view, female. — A, A. aquilonia; B, A. bipunctoria; C, A. elegans sp. nov. (holotype); D, A. evanescens; E, A. honmai; F, A. kikuchii; G, A. maetai; H, A. stena; I, A. sugaharai; J, A. tobensis; K, L, A. yamato sp. nov. (holotype)." httpUri="https://zenodo.org/record/227831/files/figure.png" pageId="7" pageNumber="508">
Figs. 
<number id="3DD94B3DFFF2F573DACCDD9D21D7FF3D" box="[222,236,187,211]" pageId="7" pageNumber="508" value="1.0">1</number>
</figureCitation>
C, 
<number id="3DD94B3DFFF2F573DB06DD9D2019FF3D" box="[276,290,187,211]" pageId="7" pageNumber="508" value="3.0">3</number>
A, 
<number id="3DD94B3DFFF2F573DB59DD9D2062FF3D" box="[331,345,187,211]" pageId="7" pageNumber="508" value="4.0">4</number>
D, 
<number id="3DD94B3DFFF2F573DB91DD9C20AAFF3D" box="[387,401,186,211]" pageId="7" pageNumber="508" value="5.0">5</number>
C, 
<number id="3DD94B3DFFF2F573DBAADD9C20FDFF3D" box="[440,454,186,211]" pageId="7" pageNumber="508" value="6.0">6</number>
C, 
<number id="3DD94B3DFFF2F573DBFCDD9D20C7FF3C" box="[494,508,187,210]" pageId="7" pageNumber="508" value="7.0">7</number>
C)
