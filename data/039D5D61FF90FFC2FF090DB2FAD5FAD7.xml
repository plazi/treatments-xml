, AW 
<quantity id="4CCC4192FF92FFC2FB8B0EC2FB70FC1D" box="[1045,1211,943,968]" metricMagnitude="-3" metricUnit="m" metricValue="1.4649999999999999" metricValueMax="1.52" metricValueMin="1.41" pageId="3" pageNumber="438" unit="mm" value="1.4649999999999999" valueMax="1.52" valueMin="1.41">1.41–1.52 mm</quantity>
