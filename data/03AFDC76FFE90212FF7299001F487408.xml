<taxonomicName id="4C0616E3FFEA0212FCAF9FAA195575ED" authorityName="Yu, Yang and Gong" authorityYear="1986" box="[842,889,223,248]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="hsui">
