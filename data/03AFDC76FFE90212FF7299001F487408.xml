<taxonomicName id="4C0616E3FFEA0212FDDF9FAA184575ED" authorityName="STEKOLNIKOV" authorityYear="2013" box="[570,617,223,248]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="ushi">
