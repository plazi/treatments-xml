<taxonomicName id="43BED3B3745DFFC4CC8FF921FEEC93FD" box="[151,302,1721,1746]" class="Actinopterygii" family="Bryconidae" genus="Anacanthorus" kingdom="Animalia" order="Characiformes" pageId="1" pageNumber="138" phylum="Chordata" rank="species" species="kruidenieri">
<emphasis id="B6CA7422745DFFC4CC8FF921FF6A93FE" box="[151,168,1722,1745]" italics="true" pageId="1" pageNumber="138">A</emphasis>
. 
<emphasis id="B6CA7422745DFFC4CCB7F922FEEC93FD" box="[175,302,1721,1746]" italics="true" pageId="1" pageNumber="138">kruidenieri</emphasis>
</taxonomicName>
, and 
<taxonomicName id="43BED3B3745DFFC4CD71F921FDF893FD" box="[361,570,1721,1746]" class="Actinopterygii" family="Bryconidae" genus="Anacanthorus" kingdom="Animalia" order="Characiformes" pageId="1" pageNumber="138" phylum="Chordata" rank="species" species="parakruidenieri">
<emphasis id="B6CA7422745DFFC4CD71F921FDF893FD" box="[361,570,1721,1746]" italics="true" pageId="1" pageNumber="138">A. parakruidenieri</emphasis>
