<taxonomicName id="D1836DB1FFF5FFF6FEE8F92EC63EF953" authority="Westwood" authorityName="Westwood" box="[351,614,1748,1774]" class="Insecta" family="Formicidae" genus="Stenamma" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
