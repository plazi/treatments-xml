<number id="3DFA784DFF8BFF95FCBB9C62FCD0F8EA" box="[801,855,1888,1914]" pageId="7" pageNumber="30" value="0.18">0.18</number>
, 
<number id="3DFA784DFF8BFF95FCF99C62FC1EF8EA" box="[867,921,1888,1914]" pageId="7" pageNumber="30" value="0.18">0.18</number>
