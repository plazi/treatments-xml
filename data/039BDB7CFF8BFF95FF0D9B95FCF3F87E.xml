, 
<number id="3DFA784DFF8BFF95FB479C62FA94F8EA" box="[1245,1299,1888,1914]" pageId="7" pageNumber="30" value="0.12">0.12</number>
, 
<number id="3DFA784DFF8BFF95FA859C62FAD2F8EA" box="[1311,1365,1888,1914]" pageId="7" pageNumber="30" value="0.13">0.13</number>
