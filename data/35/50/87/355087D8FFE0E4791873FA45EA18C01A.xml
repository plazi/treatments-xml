<collectingCountry box="[189,300,864,890]" name="Malaysia" pageId="26" pageNumber="27">Malaysia</collectingCountry>
, Borneo; 
<collectingCountry box="[413,530,864,890]" name="Indonesia" pageId="26" pageNumber="27">Indonesia</collectingCountry>
, 
<collectingRegion box="[542,682,864,890]" country="Indonesia" name="Kalimantan Utara" pageId="26" pageNumber="27">Kalimantan</collectingRegion>
, 
<collectingRegion box="[693,748,864,890]" country="Indonesia" name="Jawa Timur" pageId="26" pageNumber="27">Java</collectingRegion>
, 
<collectingRegion box="[760,862,864,890]" country="Indonesia" name="Sumatera Utara" pageId="26" pageNumber="27">Sumatra</collectingRegion>
