<paragraph blockId="22.[188,1399,735,834]" pageId="22" pageNumber="23">
<taxonomicName box="[189,429,735,761]" class="Magnoliopsida" family="Rubiaceae" genus="Chassalia" kingdom="Plantae" order="Gentianales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="curviflora">
<emphasis box="[189,429,735,761]" italics="true" pageId="22" pageNumber="23">Chassalia curviflora</emphasis>
</taxonomicName>
