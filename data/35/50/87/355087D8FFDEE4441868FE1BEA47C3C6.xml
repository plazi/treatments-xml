<location LSID="urn:lsid:plazi:treatment:355087D8FFDEE4441868FE1BEA47C3C6:B8266015FFDEE4441999FB1DEB24C0E0" box="[964,1039,1205,1231]" country="Malaysia" name="Ridley" pageId="39" pageNumber="40" stateProvince="Borneo">Ridley</location>
s.n.
</emphasis>
; 
<collectionCode box="[1103,1132,1205,1231]" country="United Kingdom" lsid="urn:lsid:biocol.org:col:15867" name="Royal Botanic Gardens" pageId="39" pageNumber="40" type="Herbarium">K</collectionCode>
