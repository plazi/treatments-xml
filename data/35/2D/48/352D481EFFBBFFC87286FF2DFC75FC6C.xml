<taxonomicName authority="Hampson, 1907" authorityName="Hampson" authorityYear="1907" box="[862,1259,788,812]" class="Insecta" family="Crambidae" genus="Eudonia" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="microdontalis">
