(
<emphasis box="[405,555,1736,1762]" italics="true" pageId="2" pageNumber="298">Dendrolasius</emphasis>
) 
<emphasis box="[571,674,1737,1762]" italics="true" pageId="2" pageNumber="298">capitatus</emphasis>
