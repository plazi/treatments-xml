<subSubSection lastPageId="3" lastPageNumber="299" pageId="2" pageNumber="298" type="biology_ecology">
<paragraph blockId="2.[151,1437,1736,2015]" box="[199,1011,1736,1763]" pageId="2" pageNumber="298">
<emphasis bold="true" box="[199,308,1736,1762]" pageId="2" pageNumber="298">Host ant.</emphasis>
<taxonomicName authority="(Kuznetsov-Ugamsky, 1928)" baseAuthorityName="Kuznetsov-Ugamsky" baseAuthorityYear="1928" box="[315,1005,1736,1763]" class="Insecta" family="Formicidae" genus="Lasius" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="298" phylum="Arthropoda" rank="species" species="capitatus" subGenus="Dendrolasius">
