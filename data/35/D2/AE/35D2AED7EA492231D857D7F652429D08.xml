<taxonomicName authority="Walker, 1833" authorityName="Walker" authorityYear="1833" lsidName="" pageId="0" pageNumber="8013" rank="subFamily" subFamily="Miscogastrinae">Miscogastrinae Walker, 1833</taxonomicName>
