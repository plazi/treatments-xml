<location LSID="urn:lsid:plazi:treatment:352C264A8E74FFF1D9570A63DBB2BD4D:B85AC1878E77FFF1D8930E84D9D3BC15" box="[212,228,509,533]" country="Vanuatu" county="Arc" municipality="Norfolk Ridge" name="If" pageId="46" pageNumber="527">If</location>
