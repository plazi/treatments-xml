<quantity box="[433,552,543,567]" metricMagnitude="2" metricUnit="m" metricValue="4.79" metricValueMax="5.08" metricValueMin="4.5" pageId="46" pageNumber="527" unit="m" value="479.0" valueMax="508.0" valueMin="450.0">
