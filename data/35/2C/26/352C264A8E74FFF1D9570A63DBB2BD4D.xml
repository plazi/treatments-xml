<location LSID="urn:lsid:plazi:treatment:352C264A8E74FFF1D9570A63DBB2BD4D:B85AC1878E77FFF1DADC0EA3DA0CBFF2" box="[667,827,474,498]" country="Vanuatu" county="Arc" municipality="Norfolk Ridge" name="Austral Islands" pageId="46" pageNumber="527">Austral Islands</location>
, with empty shells in 
<quantity box="[1061,1178,474,498]" metricMagnitude="2" metricUnit="m" metricValue="5.35" metricValueMax="8.0" metricValueMin="2.7" pageId="46" pageNumber="527" unit="m" value="535.0" valueMax="800.0" valueMin="270.0">
<elevation box="[1061,1178,474,498]" metricMagnitude="2" metricUnit="m" metricValue="5.35" metricValueMax="8.0" metricValueMin="2.7" pageId="46" pageNumber="527" unit="m" value="535.0" valueMax="800.0" valueMin="270.0">270-800 m</elevation>
