<quantity box="[1226,1332,1202,1226]" metricMagnitude="2" metricUnit="m" metricValue="1.45" metricValueMax="2.45" metricValueMin="0.45" pageId="38" pageNumber="519" unit="m" value="145.0" valueMax="245.0" valueMin="45.0">
