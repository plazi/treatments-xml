<paragraph blockId="14.[827,1443,506,620]" pageId="14">
<emphasis box="[827,949,507,528]" italics="true" pageId="14">Karyotype:</emphasis>
2 
<emphasis box="[975,990,507,528]" italics="true" pageId="14">n</emphasis>
= 16 + Xy (
<specimenCount box="[1116,1135,507,528]" pageId="14" type="male">♂</specimenCount>
). 
<taxonomicName authorityName="d Orchymont" authorityYear="1913" box="[1153,1285,506,527]" class="Insecta" family="Hydrophilidae" genus="Horelophus" kingdom="Animalia" order="Coleoptera" pageId="14" phylum="Arthropoda" rank="genus">
<emphasis box="[1153,1285,506,527]" italics="true" pageId="14">Horelophus</emphasis>
</taxonomicName>
