<taxonomicName box="[462,697,1536,1559]" class="Magnoliopsida" family="Rhamnaceae" genus="Ziziphus" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="72" phylum="Tracheophyta" rank="species" species="spina-christi">
<emphasis box="[462,697,1536,1559]" italics="true" pageId="5" pageNumber="72">Ziziphus spina-christi</emphasis>
</taxonomicName>
(
<taxonomicName box="[713,856,1536,1559]" class="Magnoliopsida" family="Rhamnaceae" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="72" phylum="Tracheophyta" rank="family">Rhamnaceae</taxonomicName>
), 
<taxonomicName box="[876,1040,1536,1561]" class="Magnoliopsida" family="Rubiaceae" genus="Coffea" kingdom="Plantae" order="Gentianales" pageId="5" pageNumber="72" phylum="Tracheophyta" rank="species" species="arabica">
<emphasis box="[876,1040,1536,1561]" italics="true" pageId="5" pageNumber="72">Coffea arabica</emphasis>
</taxonomicName>
(
<taxonomicName class="Magnoliopsida" family="Rubiaceae" kingdom="Plantae" order="Gentianales" pageId="5" pageNumber="72" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
) and 
<taxonomicName box="[293,367,1568,1591]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="72" phylum="Tracheophyta" rank="genus">
<emphasis box="[293,367,1568,1591]" italics="true" pageId="5" pageNumber="72">Acacia</emphasis>
</taxonomicName>
spp. (
<taxonomicName box="[432,573,1567,1590]" class="Magnoliopsida" family="Mimosaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="72" phylum="Tracheophyta" rank="family">Mimosaceae</taxonomicName>
