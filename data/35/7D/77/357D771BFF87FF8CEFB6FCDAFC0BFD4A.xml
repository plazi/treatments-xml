<subSubSection pageId="44" pageNumber="45" type="materials_examined">
<paragraph blockId="44.[128,654,846,1759]" box="[156,535,1025,1049]" pageId="44" pageNumber="45">
<typeStatus box="[156,284,1025,1049]" pageId="44" pageNumber="45">HOLOTYPE</typeStatus>
