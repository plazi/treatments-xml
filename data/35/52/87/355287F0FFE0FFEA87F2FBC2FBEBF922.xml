<taxonomicName authority="Reitter, 1912" authorityName="Reitter" authorityYear="1912" class="Insecta" family="Curculionidae" genus="Misenatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="27" phylum="Arthropoda" rank="genus">
