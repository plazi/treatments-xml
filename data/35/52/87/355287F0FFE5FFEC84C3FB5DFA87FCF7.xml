<paragraph blockId="9.[905,1362,1260,1284]" box="[905,1362,1260,1284]" pageId="9" pageNumber="24">
<heading bold="true" box="[905,1362,1260,1284]" centered="true" fontSize="10" level="4" pageId="9" pageNumber="24" reason="0">
<emphasis bold="true" box="[905,1362,1260,1284]" pageId="9" pageNumber="24">
Genus 
<taxonomicName authority="Germar, 1822" authorityName="Germar" authorityYear="1822" box="[985,1298,1260,1284]" class="Insecta" family="Curculionidae" genus="Otiorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="24" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[985,1136,1260,1284]" italics="true" pageId="9" pageNumber="24">Otiorhynchus</emphasis>
Germar, 1822
</taxonomicName>
<emphasis bold="true" box="[1305,1362,1261,1284]" italics="true" pageId="9" pageNumber="24">s. str.</emphasis>
</emphasis>
</heading>
</paragraph>
