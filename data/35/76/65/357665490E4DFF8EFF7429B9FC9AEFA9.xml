setae (
<number box="[1141,1153,1728,1752]" pageId="12" pageNumber="13" value="1.0">1</number>
biserrate); 
<emphasis box="[1286,1391,1728,1752]" italics="true" pageId="12" pageNumber="13">propodus</emphasis>
<number box="[1401,1436,1728,1752]" pageId="12" pageNumber="13" value="0.8">0.8</number>
