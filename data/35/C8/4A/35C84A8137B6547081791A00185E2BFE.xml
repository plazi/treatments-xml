(Crawford, 1937) and 
<taxonomicName authorityName="Conlan" authorityYear="1990" class="Malacostraca" family="Ischyroceridae" genus="Jassa" kingdom="Animalia" lsidName="Jassa slatteryi" order="Amphipoda" pageId="0" pageNumber="76050" phylum="Arthropoda" rank="species" species="slatteryi">
