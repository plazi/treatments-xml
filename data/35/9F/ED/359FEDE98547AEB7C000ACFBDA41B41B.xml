Stanley (
<taxonomicName genus="Verbenaceae" lsidName="Verbenaceae" pageId="120" pageNumber="121" rank="genus">Verbenaceae</taxonomicName>
) (
<bibRefCitation author="Staines, CL" journalOrPublisher="Proceedings of the Entomological Society of Washington" pageId="293" pageNumber="294" pagination="672 - 673" title="A new species of Cephaloleia (Coleoptera: Chrysomelidae: Hispinae) from Costa Rica." volume="100" year="1998">Staines 1998</bibRefCitation>
