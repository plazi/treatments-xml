<taxonomicName id="4C414D38FFF57A24FD18FDE9F9FBFD3A" authority="Huber, 2000" authorityName="Huber" authorityYear="2000" box="[659,928,614,641]" class="Arachnida" family="Pholcidae" genus="Chibchea" kingdom="Animalia" order="Araneae" pageId="37" pageNumber="38" phylum="Arthropoda" rank="genus">
