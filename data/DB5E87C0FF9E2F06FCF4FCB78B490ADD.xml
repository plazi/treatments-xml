<emphasis id="6183EAC4FF9E2F05FE34FAD48B540F86" box="[599,633,1399,1423]" italics="true" pageId="1" pageNumber="302">
<emphasis id="6183EAC4FF9E2F05FE34FAD48B470F87" bold="true" box="[599,618,1399,1422]" italics="true" pageId="1" pageNumber="302">ω</emphasis>
<number id="E53F24F1FF9E2F05FE08FAD48B540F86" box="[619,633,1399,1423]" pageId="1" pageNumber="302" value="1.0">1</number>
</emphasis>
, tarsi II: 
<number id="E53F24F1FF9E2F05FE82FAD48BD00F86" box="[737,765,1399,1423]" pageId="1" pageNumber="302" value="10.0">10</number>
