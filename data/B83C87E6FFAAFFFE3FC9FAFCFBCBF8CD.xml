(Olivier, 
<number id="865D24D7FFAAFFFE3A05F909FA03F905" box="[1371,1428,1754,1778]" pageId="16" pageNumber="297" value="1790.0">1790</number>
)
</taxonomicName>
(
<taxonomicName id="F7954D73FFAAFFFE3FC0F92EFE88F8E1" box="[158,287,1789,1814]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="297" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
