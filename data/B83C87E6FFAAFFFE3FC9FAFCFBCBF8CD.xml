<collectionCode id="5684AE35FFAAFFFE3C6EF92DFC10F8E1" box="[816,903,1790,1814]" httpUri="http://grbio.org/cool/pyng-6456" name="Museo Argentino de Ciencias Naturales Bernardino Rivadavia" pageId="16" pageNumber="297">MACN</collectionCode>
