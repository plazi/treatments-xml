<subSubSection id="8AEA6528315D1E33FB7CCE4FE0338472" box="[797,1103,1639,1660]" pageId="9" type="nomenclature">
<paragraph id="C24F36A3315D1E33FB7CCE4FE0338472" blockId="9.[797,1103,1639,1660]" box="[797,1103,1639,1660]" pageId="9">
<heading id="990781CF315D1E33FB7CCE4FE0338472" box="[797,1103,1639,1660]" centered="true" fontSize="10" level="2" pageId="9" reason="2">
<emphasis id="F084EAB1315D1E33FB7CCE4FE0338472" bold="true" box="[797,1103,1639,1660]" pageId="9">
<taxonomicName id="05F04D20315D1E33FB7CCE4FE7AB8472" authority="Ratcliffe" authorityName="Ratcliffe" authorityYear="2014" box="[797,983,1639,1660]" class="Insecta" family="Dynastidae" genus="Adelaeus" kingdom="Animalia" order="Coleoptera" pageId="9" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="F084EAB1315D1E33FB7CCE4FE70B8472" bold="true" box="[797,887,1639,1660]" italics="true" pageId="9">Adelaeus</emphasis>
Ratcliffe
</taxonomicName>
, 
<taxonomicNameLabel id="EBB757CA315D1E33FB85CE4FE0338472" box="[996,1103,1639,1660]" pageId="9" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
