<collectingCountry id="F34584A5A86FFFFBFD94FC1B45F5A0CF" box="[538,707,967,993]" name="French Guiana" pageId="8" pageNumber="9">French Guiana</collectingCountry>
, 
<collectingCountry id="F34584A5A86FFFFBFD40FC144432A0CC" box="[718,772,968,994]" name="Peru" pageId="8" pageNumber="9">Peru</collectingCountry>
, 
<collectingCountry id="F34584A5A86FFFFBFC81FC1B44B0A0CF" box="[783,902,967,993]" name="Venezuela" pageId="8" pageNumber="9">Venezuela</collectingCountry>
