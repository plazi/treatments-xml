<taxonomicName id="E96A72CCD56CA7F9A275AC2843E588D1" authority="limosa" class="Arachnida" family="Scolopacidae" genus="Limosa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Limosa limosa" order="Astigmata" pageId="0" pageNumber="81" phylum="Arthropoda" rank="species" species="limosa">
<emphasis id="815D441FA905C108756A9C72885FEDA0" italics="true" pageId="0" pageNumber="81">Limosa limosa</emphasis>
</taxonomicName>
(
<taxonomicName id="ACBB7366310BCEE59AA292C0A98BB09D" authorityName="Brisson" authorityYear="1760" class="Aves" genus="Limosa" kingdom="Animalia" lsidName="" order="Charadriiformes" pageId="0" pageNumber="81" phylum="Chordata" rank="order">Charadriiformes</taxonomicName>
, 
