<paragraph id="413936D0FFEFFFF6FF20E7CFFC8AFF7F" blockId="2.[151,875,226,250]" box="[151,875,226,250]" pageId="2" pageNumber="3">
<emphasis id="73F2EAC2FFEFFFF6FF20E7CFFED0FF7C" bold="true" box="[151,305,226,250]" pageId="2" pageNumber="3">Composition:</emphasis>
<taxonomicName id="86864D53FFEFFFF6FE80E7CFFDA1FF7F" authority="Perez, 1900" authorityName="Perez" authorityYear="1900" box="[311,576,226,249]" class="Malacostraca" family="Crinoniscidae" genus="Crinoniscus" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
<emphasis id="73F2EAC2FFEFFFF6FE80E7CFFE59FF7F" box="[311,440,226,249]" italics="true" pageId="2" pageNumber="3">Crinoniscus</emphasis>
Perez, 
<number id="F74E24F7FFEFFFF6FDBFE7CFFDA1FF7F" box="[520,576,226,249]" pageId="2" pageNumber="3" value="1900.0">1900</number>
</taxonomicName>
; 
<taxonomicName id="86864D53FFEFFFF6FDFCE7CFFC86FF7F" authority="Darwin, 1854" authorityName="Darwin" authorityYear="1854" box="[587,871,226,249]" class="Malacostraca" family="Crinoniscidae" genus="Proteolepas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
