and 
<taxonomicName id="B0E22EC1FFC1FFFD9E45B2EAFB18E4AC" box="[1107,1203,904,924]" class="Dinophyceae" family="Gonyaulacaceae" genus="Gonyaulax" higherTaxonomySource="GBIF" kingdom="Chromista" order="Gonyaulacales" pageId="5" pageNumber="956" phylum="Myzozoa" rank="genus">
<emphasis id="45968950FFC1FFFD9E45B2EAFB18E4AC" box="[1107,1203,904,924]" italics="true" pageId="5" pageNumber="956">Gonyaulax</emphasis>
</taxonomicName>
. 
<emphasis id="45968950FFC1FFFD9EAAB2EAFB43E4AC" bold="true" box="[1212,1256,904,924]" pageId="5" pageNumber="956">A, B.</emphasis>
<taxonomicName id="B0E22EC1FFC1FFFD9EFAB2EAFA20E4AC" authorityName="Meunier" authorityYear="1919" box="[1260,1419,904,924]" class="Dinophyceae" family="Gonyaulacaceae" genus="Amylax" higherTaxonomySource="GBIF" kingdom="Chromista" order="Gonyaulacales" pageId="5" pageNumber="956" phylum="Myzozoa" rank="species" species="diacantha">
