<emphasis id="45968950FFC1FFFD9F4BB2C7FA20E489" bold="true" box="[1373,1419,933,953]" pageId="5" pageNumber="956">C, D.</emphasis>
<taxonomicName id="B0E22EC1FFC1FFFD993BB2A0FC76E4E6" authorityName="Diesing" authorityYear="1866" baseAuthorityName="Claparede &amp; Lachmann" box="[813,989,962,982]" class="Dinophyceae" family="Gonyaulacaceae" genus="Gonyaulax" higherTaxonomySource="GBIF" kingdom="Chromista" order="Gonyaulacales" pageId="5" pageNumber="956" phylum="Myzozoa" rank="species" species="spinifera">
