(
<taxonomicName id="4C3CDAC2FFF3CA53FEE913A5FE21FC1A" box="[323,437,943,968]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
