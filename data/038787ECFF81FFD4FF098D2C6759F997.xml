Taylor
</taxonomicName>
on 
<taxonomicName id="4C2E4D79FF81FFD4FBE08CB46706FA3A" authority="(Cav.) S. T. Blake" authorityName="S. T. Blake" authorityYear="1958" baseAuthorityName="Cav." class="Magnoliopsida" family="Myrtaceae" genus="Melaleuca" higherTaxonomySource="CoL" kingdom="Fungi" order="Myrtales" pageId="8" pageNumber="409" phylum="Tracheophyta" rank="species" species="quinquenervia">
