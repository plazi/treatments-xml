<taxonomicName id="4C2E4D79FF81FFD4FB458F186336F9F2" baseAuthorityName="Meyrick" box="[1243,1378,1550,1576]" class="Insecta" family="Gelechiidae" genus="Ardozyga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="species" species="stratifera">
