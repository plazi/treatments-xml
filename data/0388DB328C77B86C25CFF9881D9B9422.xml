<collectionCode id="ED30F2E18C78B86D24F4FA561FBF9384" box="[428,490,1434,1457]" httpUri="http://grbio.org/cool/2vmj-fp0v" name="Finnish Museum of Natural History" pageId="24" pageNumber="395">MZH</collectionCode>
<number id="3DE978038C78B86D24AAFA551C699387" box="[498,572,1433,1458]" pageId="24" pageNumber="395" value="20507.0">20507</number>
