<subSubSection id="AC954A89FF9D1476D95B7C2B6F00F692" box="[113,526,883,907]" pageId="25" pageNumber="368" type="materials_examined">
<paragraph id="E4301902FF9D1476D95B7C2B6F00F692" blockId="25.[113,526,883,907]" box="[113,526,883,907]" pageId="25" pageNumber="368">
<typeStatus id="3B34A7A0FF9D1476D95B7C2B6DACF691" box="[113,162,883,906]" pageId="25" pageNumber="368">TYPE</typeStatus>
SPECIES. — 
<taxonomicName id="238F6281FF9D1476D8367C2B6CDCF693" authority="Mclean, 2012" authorityName="Mclean" authorityYear="2012" box="[284,466,883,907]" class="Gastropoda" family="Colloniidae" genus="Depressipoma" kingdom="Animalia" order="Trochida" pageId="25" pageNumber="368" phylum="Mollusca" rank="species" species="laddi" status="n. sp.">
<emphasis id="D6FBC510FF9D1476D8367C2B6CDCF693" box="[284,466,883,907]" italics="true" pageId="25" pageNumber="368">Depressipoma laddi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CDC8786BFF9D1476D8F27C2B6F00F692" box="[472,526,883,906]" pageId="25" pageNumber="368" rank="species">n. sp.</taxonomicNameLabel>
