<taxonomicName id="4C2158ECFF8EFFFB742B2D91FACDF859" box="[1146,1322,1965,1990]" class="Arachnida" family="Trombidiidae" genus="Azaritrombium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acarina" pageId="2" pageNumber="52" phylum="Arthropoda" rank="genus">
