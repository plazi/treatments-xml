<paragraph id="616C36EAFFE2FF89FF35537713003736" blockId="5.[151,793,1595,1656]" box="[151,523,1630,1656]" pageId="5" pageNumber="32">
<figureCitation id="F9E82A6FFFE2FF89FF35537711E33736" box="[151,232,1630,1656]" captionStart="FIGURES 1 – 6" captionStartId="2.[151,269,1817,1841]" captionTargetBox="[186,1403,219,1816]" captionTargetId="figure@2.[151,1436,193,1887]" captionTargetPageId="2" captionText="FIGURES 1 – 6. Labia. 1. Canthochilum brodzinskyorum; 2. Canthochilum magnum; 3. Canthochilum matthewsi; 4. Canthochilum mimicum; 5. Canthochilum nebulonemi; 6. Canthochilum platycnemis." httpUri="https://zenodo.org/record/181297/files/figure.png" pageId="5" pageNumber="32">
Figs. 
<number id="D71B24CDFFE2FF89FF7A537711E33736" box="[216,232,1630,1656]" pageId="5" pageNumber="32" value="3.9">3</number>
</figureCitation>
, 9, 
<number id="D71B24CDFFE2FF89FEB0537710553736" box="[274,350,1630,1656]" pageId="5" pageNumber="32" value="15.21">15, 21</number>
, 
<number id="D71B24CDFFE2FF89FEC8537710BD3736" box="[362,438,1630,1656]" pageId="5" pageNumber="32" value="27.36">27, 36</number>
, 
<number id="D71B24CDFFE2FF89FE60537713003736" box="[450,523,1630,1656]" pageId="5" pageNumber="32" value="45.46">45, 46</number>
