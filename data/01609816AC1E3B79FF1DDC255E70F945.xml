<materialsCitation id="39A1235DAC1E3B79FE78DD585D5AFAEF" collectingDate="1957-03-05" collectingDateMax="1957-03-30" collectingDateMin="1957-03-05" collectionCode="V" collectorName="Ziela, U." county="Thio" location="Yalanzou" municipality="Nion" pageId="82" pageNumber="475" specimenCount="1" stateProvince="Nimba">
