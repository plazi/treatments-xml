<subSubSection id="7D8B40F6FFDBAC4CFF77F8FAFDAD87AA" pageId="22" pageNumber="423" type="etymology">
<paragraph id="352E137DFFDBAC4CFF77F8FAFDAD87AA" blockId="22.[151,1436,402,1939]" pageId="22" pageNumber="423">
<emphasis id="07E5CF6FFFDBAC4CFF77F8FAFC008756" bold="true" box="[199,334,1878,1903]" pageId="22" pageNumber="423">Etymology.</emphasis>
