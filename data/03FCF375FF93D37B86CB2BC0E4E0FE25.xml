<paragraph id="8BEA4263FF93D37887AA2A64E4ECFEE1" blockId="37.[140,1095,225,600]" box="[163,696,289,313]" pageId="37" pageNumber="1457">
<quantity id="4CADEF86FF93D37887AA2A64E6ACFEE1" box="[163,248,289,313]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="37" pageNumber="1457" unit="mm" value="2.5">2.5 mm</quantity>
, one 
<quantity id="4CADEF86FF93D37886472A64E7CBFEE1" box="[334,415,289,313]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="37" pageNumber="1457" unit="mm" value="2.2">2.2 mm</quantity>
