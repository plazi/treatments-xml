<taxonomicName id="6FCBCDE45F5F7CF43BD77AB3A043D63F" authorityName="Wunderlich" authorityYear="1995" class="Arachnida" family="Lycosidae" genus="Hogna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hogna nonannulata" order="Araneae" pageId="0" pageNumber="84" phylum="Arthropoda" rank="species" species="nonannulata">
