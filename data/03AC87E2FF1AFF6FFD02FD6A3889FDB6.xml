(
<collectorName id="26F05322FF1AFF6CFC92FB743D70FAAC" box="[877,1047,1267,1288]" pageId="161" pageNumber="161">W. Longmore</collectorName>
, personal commun.). 
<collectorName id="26F05322FF1AFF6CFCDAFA973A35FA82" box="[805,850,1296,1318]" pageId="161" pageNumber="161">The</collectorName>
<collectionCode id="ED14AE31FF1AFF6CFC92FA973AADFA81" box="[877,970,1296,1317]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="161" pageNumber="161">AMNH</collectionCode>
specimen, 
<emphasis id="B971EAE6FF1AFF6CFB8EFA973A68FAE6" bold="true" pageId="161" pageNumber="161">
<specimenCode id="DBA39E8FFF1AFF6CFB8EFA973A68FAE6" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="161" pageNumber="161">AMNH 694538</specimenCode>
