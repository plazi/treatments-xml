</materialsCitation>
; 
<materialsCitation id="3B6D3CA9FF1AFF6CFCABFB3F3A0BFA3F" collectingDate="1911-09-08" collectionCode="AMNH" collectorName="W. Longmore &amp; The" country="Australia" county="Kow Plains" latitude="-35.14" location="It" longLatPrecision="717" longitude="141.22" municipality="One" pageId="161" pageNumber="161" specimenCode="AMNH 694538" specimenCount="3" stateProvince="Victoria" typeStatus="paralectotype">
