<taxonomicName id="4C244D13FFB8FF95FE1AEA9EFD148A77" authorityName="Stimpson" authorityYear="1856" box="[508,725,757,781]" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="71" pageNumber="72" phylum="Tracheophyta" rank="species" species="stylifera">
