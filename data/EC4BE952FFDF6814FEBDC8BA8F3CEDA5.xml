<taxonomicName id="A3E223C7FFDD6814FD57CB528E23EDF6" authorityName="Blahnik Table Of Contents" authorityYear="2005" box="[738,875,364,389]" class="Insecta" family="Philopotamidae" genus="Alterosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="guapimirim">
