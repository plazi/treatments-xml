<emphasis id="56968456FFDC6815FEBDCC668CDFEA03" bold="true" box="[264,407,1624,1648]" pageId="30" pageNumber="31">FIGURE 14.</emphasis>
<taxonomicName id="A3E223C7FFDC6815FE11CC678FC9EA03" authority="Blahnik Table Of Contents, 2005" authorityName="Blahnik Table Of Contents" authorityYear="2005" box="[420,641,1625,1648]" class="Insecta" family="Philopotamidae" genus="Alterosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="guapimirim" status="new species">
<emphasis id="56968456FFDC6815FE11CC678FC9EA03" box="[420,641,1625,1648]" italics="true" pageId="30" pageNumber="31">Alterosa guapimirim</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="4DA5392DFFDC6815FD3BCC678E59EA03" box="[654,785,1625,1648]" pageId="30" pageNumber="31" rank="species">new species</taxonomicNameLabel>
