<taxonomicName id="F1A74D36897A2652814C13A5EE1B4CA2" authority="UNGER." authorityName="UNGER." box="[1088,1308,689,712]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="2" pageNumber="294" phylum="Tracheophyta" rank="species" species="hepios">
