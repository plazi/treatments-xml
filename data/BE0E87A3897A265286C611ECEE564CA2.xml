<bibRefCitation id="52364B44897A265286911385EF364CC2" author="Menzel, P." box="[925,1073,657,680]" pageId="2" pageNumber="294" pagination="85 - 110" refId="ref13729" refString="Menzel, P. (1901): Die Gymnospermen der nordbohmischen Braunkohlenformation. - Abhandlungen der Naturwissenschaftlichen Gesellschaft ISIS zu Dresden, 1900 (2): 49 - 69, 85 - 110." type="journal article" year="1901">Menzel 1901</bibRefCitation>
; as 
<taxonomicName id="F1A74D36897A2652816C1385EE3A4CCC" authority="UNGER" authorityName="UNGER" box="[1120,1341,657,680]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="2" pageNumber="294" phylum="Tracheophyta" rank="species" species="hepios">
<emphasis id="04D3EAA7897A2652816C1385EFEA4CC2" box="[1120,1261,657,680]" italics="true" pageId="2" pageNumber="294">Pinus hepios</emphasis>
UNGER
</taxonomicName>
; 
<bibRefCitation id="52364B44897A265280421385E8924CA2" author="Brabenec, B." pageId="2" pageNumber="294" pagination="1 - 144" refId="ref11754" refString="Brabenec, B. (1909): Souborna kvetena ceskeho utvaru tretihorniho [Comprehensive Flora of the Czech Tertiary] [part I]. - Archiv pro prirodovedecke prozkoumani Cech, 14 (3): 1 - 144. (in Czech)" type="journal article" year="1909">Brabenec 1909: 48</bibRefCitation>
