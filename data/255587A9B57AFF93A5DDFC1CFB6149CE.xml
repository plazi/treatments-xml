<emphasis id="9F88EAADB57BFF93A721FBEFFD5649CE" box="[619,668,188,212]" italics="true" pageId="5" pageNumber="39">et al</emphasis>
. 
<number id="1B342498B57BFF93A7E0FBEEFD2949CE" box="[682,739,188,212]" pageId="5" pageNumber="39" value="2005.0">2005</number>
</bibRefCitation>
) and from 
<collectingCountry id="D5EB762FB57BFF93A615FBEEFC2649CE" box="[863,1004,188,212]" name="North Korea" pageId="5" pageNumber="39">North Korea</collectingCountry>
