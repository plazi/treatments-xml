<date id="4F5F07BBCE451246385BFED4FB5FFEEB" box="[1047,1201,284,310]" pageId="25" pageNumber="26" value="2003-07-15">
<collectingDate id="5F1BFE53CE451246385BFED4FB5FFEEB" box="[1047,1201,284,310]" pageId="25" pageNumber="26" value="2003-07-15">15.VII. 2003</collectingDate>
