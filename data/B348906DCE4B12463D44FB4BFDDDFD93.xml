<materialsCitation id="8B892B26CE4512463D81FE8CFBB1FE83" box="[461,1119,324,350]" collectingDate="2003-07-15" collectionCode="CAU" collectorName="M. Wang" location="BEIJING" pageId="25" pageNumber="26" specimenCount="8" specimenCount-female="5" specimenCount-male="3" stateProvince="Beijing" typeStatus="paratype">
