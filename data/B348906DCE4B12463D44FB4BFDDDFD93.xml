<specimenCount id="2DE7EAF2CE4512463E0AFE8CFD4EFE83" box="[582,672,324,350]" pageId="25" pageNumber="26" type="male">3 males</specimenCount>
and 
<specimenCount id="2DE7EAF2CE4512463E95FE8CFCA0FE83" box="[729,846,324,350]" pageId="25" pageNumber="26" type="female">5 females</specimenCount>
