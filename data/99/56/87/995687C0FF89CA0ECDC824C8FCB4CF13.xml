<taxonomicName authorityName="Jager &amp; Dankittipakul" authorityYear="2010" box="[1028,1095,724,751]" class="Arachnida" family="Clubionidae" genus="Clubiona" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="207" phylum="Arthropoda" rank="species" species="kai">
