<collectingCountry box="[618,743,1755,1780]" name="Costa Rica" pageId="9" pageNumber="460">Costa Rica</collectingCountry>
, 
<collectingCountry box="[756,848,1756,1779]" name="Panama" pageId="9" pageNumber="460">Panama</collectingCountry>
, 
<collectingCountry box="[859,972,1755,1780]" name="Colombia" pageId="9" pageNumber="460">Colombia</collectingCountry>
, 
<collectingCountry box="[985,1103,1755,1780]" name="Venezuela" pageId="9" pageNumber="460">Venezuela</collectingCountry>
, and 
<collectingCountry box="[1163,1256,1755,1780]" name="Ecuador" pageId="9" pageNumber="460">Ecuador</collectingCountry>
(
<bibRefCitation author="Endrodi" box="[1272,1431,1755,1780]" pageId="9" pageNumber="460" refString="Endrodi, S. (1969) Monographie der Dynastinae. 4. Tribus: Pentodontini (Coleoptera, Lamellicornia). Entomologische Abhandlungen Museum fur Tierkunde, Dresden, 37, 1 - 145." type="journal article" year="1969">
