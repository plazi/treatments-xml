, IAvH, 
<collectionCode box="[1274,1325,1818,1842]" httpUri="http://grbio.org/cool/xsgz-nxkh" name="Instituto de Ciencias Naturales, Museo de Historia Natural" pageId="32" pageNumber="483">ICN</collectionCode>
, 
<collectionCode box="[1337,1432,1818,1842]" httpUri="http://grbio.org/cool/ct7t-ipa9" name="Museo Entomologico Francisco Luis Gallego" pageId="32" pageNumber="483">MEFLG</collectionCode>
, 
