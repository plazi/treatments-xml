<geoCoordinate direction="north" orientation="latitude" precision="9" value="2.6553333">02°39.32'N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="9" value="113.0405">113°02.43'E</geoCoordinate>
