<taxonomicName authorityName="Siliwal, Molur &amp; Raven" authorityYear="2015" box="[653,798,731,757]" class="Arachnida" family="Nemesiidae" genus="Damarchilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="523" phylum="Arthropoda" rank="genus">
