(
<taxonomicName box="[343,463,1478,1500]" class="Insecta" family="Formicidae" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="984" phylum="Arthropoda" rank="family">Formicidae</taxonomicName>
