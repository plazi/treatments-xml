<paragraph pageId="0" pageNumber="93">
<taxonomicName baseAuthorityName="Schrank" baseAuthorityYear="1781" class="Insecta" family="Limoniidae" genus="Metalimnobia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metalimnobia (M.) bifasciata" order="Diptera" pageId="0" pageNumber="93" phylum="Arthropoda" rank="species" species="bifasciata" subGenus="M.">Metalimnobia (M.) bifasciata</taxonomicName>
