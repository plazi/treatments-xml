<paragraph blockId="75.[189,334,939,965]" box="[189,334,939,965]" pageId="75" pageNumber="76">
<heading bold="true" box="[189,334,939,965]" fontSize="11" level="3" pageId="75" pageNumber="76" reason="3">
<emphasis bold="true" box="[189,334,939,965]" pageId="75" pageNumber="76">Description</emphasis>
