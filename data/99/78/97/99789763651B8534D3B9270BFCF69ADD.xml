<subSubSection pageId="78" pageNumber="79" type="diagnosis">
<paragraph blockId="78.[189,310,683,709]" box="[189,310,683,709]" pageId="78" pageNumber="79">
<heading bold="true" box="[189,310,683,709]" fontSize="11" level="3" pageId="78" pageNumber="79" reason="3">
