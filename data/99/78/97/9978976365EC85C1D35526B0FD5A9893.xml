<subSubSection pageId="185" pageNumber="186" type="diagnosis">
<paragraph blockId="185.[189,310,1158,1184]" box="[189,310,1158,1184]" pageId="185" pageNumber="186">
<heading bold="true" box="[189,310,1158,1184]" fontSize="11" level="3" pageId="185" pageNumber="186" reason="3">
