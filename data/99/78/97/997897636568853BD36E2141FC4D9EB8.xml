<subSubSection pageId="61" pageNumber="62" type="diagnosis">
<paragraph blockId="61.[189,310,1265,1291]" box="[189,310,1265,1291]" pageId="61" pageNumber="62">
<heading bold="true" box="[189,310,1265,1291]" fontSize="11" level="3" pageId="61" pageNumber="62" reason="3">
