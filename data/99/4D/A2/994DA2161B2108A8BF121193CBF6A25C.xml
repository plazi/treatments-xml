<taxonomicName LSID="urn:lsid:zoobank.org:act:BB4BC6AF-1C4F-4E85-9139-656A4A32D34B" authorityName="Linnaeus" authorityYear="1758" class="Gastropoda" family="Muricidae" genus="Murex" kingdom="Animalia" order="Neogastropoda" phylum="Mollusca" rank="species" species="gyrinus" status="spec. nov." zbkClass="Vermes" zbkKingdom="Animalia" zbkOrder="Testacea">Murex gyrinus</taxonomicName>
