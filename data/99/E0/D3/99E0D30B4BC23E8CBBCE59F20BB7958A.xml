<taxonomicName class="Arachnida" family="Suctobelbidae" genus="Suctobelba" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="138" phylum="Arthropoda" rank="species" species="reticulata">reticulata</taxonomicName>
