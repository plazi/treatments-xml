<paragraph blockId="1.[151,459,902,928]" box="[151,459,902,928]" pageId="1" pageNumber="2">
<heading bold="true" box="[151,459,902,928]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<emphasis bold="true" box="[151,459,902,928]" pageId="1" pageNumber="2">
Genus 
<taxonomicName authority="Thor, 1898" authorityName="Thor" authorityYear="1898" box="[237,459,902,928]" class="Arachnida" family="Axonopsidae" genus="Ljania" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[237,317,902,928]" italics="true" pageId="1" pageNumber="2">Ljania</emphasis>
Thor, 
<number box="[400,459,902,928]" pageId="1" pageNumber="2" value="1898.0">1898</number>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
