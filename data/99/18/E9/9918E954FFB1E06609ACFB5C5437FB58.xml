<subSubSection pageId="5" pageNumber="294" type="etymology">
<paragraph blockId="5.[813,1451,1400,1455]" pageId="5" pageNumber="294">
<emphasis bold="true" box="[813,957,1400,1424]" italics="true" pageId="5" pageNumber="294">Etymology. –</emphasis>
