</taxonomicName>
and 
<taxonomicName baseAuthorityName="Michaelsen" baseAuthorityYear="1896" class="Clitellata" family="Megascolecidae" genus="Pleionogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="309" phylum="Annelida" rank="species" species="ternatae">
