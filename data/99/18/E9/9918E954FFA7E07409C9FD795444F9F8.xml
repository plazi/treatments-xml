<taxonomicName authorityName="Stephenson" authorityYear="1933" box="[554,683,1345,1368]" class="Clitellata" family="Megascolecidae" genus="Pleionogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="309" phylum="Annelida" rank="species" species="sivickisi">
