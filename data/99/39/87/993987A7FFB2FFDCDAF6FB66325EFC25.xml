<paragraph blockId="7.[151,927,1178,1204]" box="[151,927,1178,1204]" pageId="7" pageNumber="36">
<heading bold="true" box="[151,927,1178,1204]" fontSize="11" level="1" pageId="7" pageNumber="36" reason="1">
<emphasis bold="true" box="[151,927,1178,1204]" pageId="7" pageNumber="36">
