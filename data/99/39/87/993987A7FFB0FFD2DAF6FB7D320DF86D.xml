<specimenCount box="[1332,1436,1722,1748]" pageId="5" pageNumber="34" type="female">1 female</specimenCount>
imagos from 
<location LSID="urn:lsid:plazi:treatment:993987A7FFB0FFD2DAF6FB7D320DF86D:144F606AFFB0FFD2DB52F91D36D0F900" box="[307,464,1761,1787]" country="Brazil" name="Serra do Japi" pageId="5" pageNumber="34">Serra do Japi</location>
