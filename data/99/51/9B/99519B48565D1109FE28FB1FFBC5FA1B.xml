and the 
<taxonomicName authorityName="Emery" authorityYear="1887" box="[833,1011,1931,1957]" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="tetracantha">
