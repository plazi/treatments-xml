) 
<taxonomicName authority="Latzel, 1884" authorityName="Latzel" authorityYear="1884" box="[980,1246,367,392]" class="Diplopoda" family="Julidae" genus="Typhloiulus" kingdom="Animalia" order="Julida" pageId="25" pageNumber="359" phylum="Arthropoda" rank="species" species="psilonotus">
