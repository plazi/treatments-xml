<typeStatus id="54D9F8AFFFACFF9EC8FAFE76772E474B" box="[990,1085,506,526]" pageId="12" pageNumber="1149">Holotypus</typeStatus>
(): &quot; 
<taxonomicName id="4C623D8EFFACFF9ECA05FD9E729E4763" box="[289,397,529,550]" pageId="12" pageNumber="1149" rank="variety" variety="divaricat">
