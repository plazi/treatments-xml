<collectingCountry id="F30D764DFFEDCF56FB95FD4A67CCFD4C" box="[1048,1268,741,766]" name="Papua New Guinea" pageId="18" pageNumber="319">
<collectingRegion id="49DEF83FFFEDCF56FB95FD4A6765FD4F" box="[1048,1117,742,765]" country="Indonesia" name="Papua Barat" pageId="18" pageNumber="319">Papua</collectingRegion>
