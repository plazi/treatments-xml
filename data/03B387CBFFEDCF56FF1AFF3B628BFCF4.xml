<collectingCountry id="F30D764DFFEDCF56FB72FD496655FD4C" box="[1279,1389,741,766]" name="Sri Lanka" pageId="18" pageNumber="319">
<collectingRegion id="49DEF83FFFEDCF56FB72FD496618FD4C" box="[1279,1312,741,766]" country="United Kingdom" name="Surrey" pageId="18" pageNumber="319">Sri</collectingRegion>
