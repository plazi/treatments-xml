, 
<collectingCountry id="F30D764DFFEDCF56FD53FD496073FD4C" box="[734,843,741,766]" name="Indonesia" pageId="18" pageNumber="319">Indonesia</collectingCountry>
(
<collectingRegion id="49DEF83FFFEDCF56FCD7FD4A60A8FD4C" box="[858,912,742,766]" country="Indonesia" name="Jawa Timur" pageId="18" pageNumber="319">Java</collectingRegion>
, 
<collectingRegion id="49DEF83FFFEDCF56FC16FD49673CFD4C" box="[923,1028,741,766]" country="Indonesia" name="Sulawesi Utara" pageId="18" pageNumber="319">Sulawesi</collectingRegion>
