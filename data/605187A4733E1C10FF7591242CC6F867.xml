<taxonomicName id="2FF84D31733F1C10FD17918A2D63F907" baseAuthorityName="Gray" baseAuthorityYear="1830" box="[757,859,1696,1720]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="gotyla">
<emphasis id="DA8CEAA0733F1C10FD17918A2D63F907" box="[757,859,1696,1720]" italics="true" pageId="11" pageNumber="60">G. gotyla</emphasis>
