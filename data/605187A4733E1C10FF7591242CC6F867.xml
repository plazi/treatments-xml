<taxonomicName id="2FF84D31733F1C10FBD5918A2AE8F908" box="[1079,1232,1696,1719]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="qiaojiensis">
<emphasis id="DA8CEAA0733F1C10FBD5918A2AE8F908" box="[1079,1232,1696,1719]" italics="true" pageId="11" pageNumber="60">G. qiaojiensis</emphasis>
