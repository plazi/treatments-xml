<taxonomicName id="2FF84D31733F1C10FC89918A2DCEF907" authorityName="Vishwanath" authorityYear="1993" box="[875,1014,1696,1720]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="litanensis">
