<taxonomicName id="2FF84D31733F1C10FEA291EE2FFDF963" authorityName="Vishwanath &amp; Kosygin" authorityYear="2000" box="[320,453,1732,1756]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="elongata">
