<taxonomicName id="2FF84D31733F1C10FCF391562D9CF92B" authorityName="Sun &amp; Li &amp; Zhou &amp; Li" authorityYear="2018" box="[785,932,1660,1684]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="surgifrons">
<emphasis id="DA8CEAA0733F1C10FCF391562D9CF92B" box="[785,932,1660,1684]" italics="true" pageId="11" pageNumber="60">G. surgifrons</emphasis>
