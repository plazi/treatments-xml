<quantity id="2F009B57733F1C10FBF391EE2A40F963" box="[1041,1144,1731,1756]" metricMagnitude="-1" metricUnit="m" metricValue="8.382" metricValueMax="8.636" metricValueMin="8.128" pageId="11" pageNumber="60" unit="in" value="33.0" valueMax="34.0" valueMin="32.0">32–34 in</quantity>
<taxonomicName id="2FF84D31733F1C10FB6091EE2B38F963" authorityName=": Chu &amp; Cui" authorityYear="1987" box="[1154,1280,1732,1756]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="gravelyi">
