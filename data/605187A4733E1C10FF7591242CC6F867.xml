<collectingRegion id="2A3CF850733F1C10FEA490952F81F867" box="[326,441,1983,2008]" country="Myanmar" name="Ayeyawady" pageId="11" pageNumber="60">Irrawaddy</collectingRegion>
River), west 
<collectingRegion id="2A3CF850733F1C10FDB290EB2C90F867" box="[592,680,1985,2008]" country="China" name="Yunnan" pageId="11" pageNumber="60">Yunnan</collectingRegion>
, 
<collectingCountry id="90EF7622733F1C10FD5190952CC2F867" box="[691,762,1983,2008]" name="China" pageId="11" pageNumber="60">China</collectingCountry>
.
