<quantity id="2F009B57733F1C10FCEA91CD2D57F8BF" box="[776,879,1767,1792]" metricMagnitude="-1" metricUnit="m" metricValue="8.763" metricValueMax="8.89" metricValueMin="8.636" pageId="11" pageNumber="60" unit="in" value="34.5" valueMax="35.0" valueMin="34.0">34–35 in</quantity>
