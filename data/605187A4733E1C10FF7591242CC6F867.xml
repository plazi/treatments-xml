<taxonomicName id="2FF84D31733F1C10FE8C90B62C6BF80B" authorityName="Zhang" authorityYear="2006" box="[366,595,1948,1972]" class="Actinopterygii" family="Cyprinidae" genus="Garra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="11" pageNumber="60" phylum="Chordata" rank="species" species="rotundinasus">
