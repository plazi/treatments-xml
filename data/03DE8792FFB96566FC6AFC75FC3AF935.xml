<taxonomicName id="4C774D07FFB96566FC05FC75FB16FC59" authority="Hooker &amp; Russell, 2012" authorityName="Hooker &amp; Russell" authorityYear="2012" box="[1012,1174,965,986]" class="Mammalia" family="Louisinidae" genus="Prolouisina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macroscelidea" pageId="43" pageNumber="899" phylum="Chordata" rank="genus" status="GEN. NOV.">PROLOUISINA</taxonomicName>
