<materialsCitation id="B0993CE9FFD98A3FF495FBEDF496DDCA" ID-GBIF-Occurrence="2597533940" country="China" county="Szechwan Province" location="Wanhsien" pageId="22" pageNumber="301" specimenCount="1" typeStatus="type">
