Taxonomy. 
<taxonomicName id="C7F14D37FFD98A3FF4A4FBB7F375DD5E" authorityName="G. M. Allen" authorityYear="1923" box="[2920,3312,1046,1085]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="22" pageNumber="301" phylum="Chordata" rank="species" species="episcopus">
