<tr id="7B9C743EFA35D5B8FF202CBDFAED243F" box="[159,1372,244,266]" gridrow="1" pageId="4" pageNumber="486">
<th id="384D1D42FA35D5B8FF202CBDFE0B243F" box="[159,442,244,266]" gridcol="0" gridrow="1" pageId="4" pageNumber="486">Length of pronotum</th>
<td id="384D1D42FA35D5B8FE5D2CBDFCDC243F" box="[482,877,244,266]" gridcol="1" gridrow="1" pageId="4" pageNumber="486">shorter, not reaching epiproct</td>
<td id="384D1D42FA35D5B8FC752CBDFAED243F" box="[970,1372,244,266]" gridcol="2" gridrow="1" pageId="4" pageNumber="486">longer, reaching end of epiproct</td>
</tr>
<tr id="7B9C743EFA35D5B8FF202D54FAED2406" box="[159,1372,285,307]" gridrow="2" pageId="4" pageNumber="486">
<th id="384D1D42FA35D5B8FF202D54FE0B2406" box="[159,442,285,307]" gridcol="0" gridrow="2" pageId="4" pageNumber="486">Vertex</th>
<td id="384D1D42FA35D5B8FE5D2D54FCDC2406" box="[482,877,285,307]" gridcol="1" gridrow="2" pageId="4" pageNumber="486">width 1.5 times diameter of eye</td>
<td id="384D1D42FA35D5B8FC752D54FAED2406" box="[970,1372,285,307]" gridcol="2" gridrow="2" pageId="4" pageNumber="486">width 2.0 times diameter of eye</td>
</tr>
