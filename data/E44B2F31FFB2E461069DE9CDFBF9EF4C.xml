(
<bibRefCitation id="0873E3D6FFB2E46104AFEAADFCB4EF4C" box="[677,813,1015,1039]" pageId="5" pageNumber="6" refId="ref18803" refString="Bates, H. W. (1881) Biologia Centrali-Americana, Insecta, Coleoptera. Vol. 5. Published for the editors by R. H. Porter, London, 72 pp. [pp. 153 - 224]" type="book chapter">Bates, 1881</bibRefCitation>
)
</taxonomicName>
, 
<emphasis id="5E964235FFB2E461054BEAACFC5BEF4C" bold="true" box="[833,962,1014,1039]" pageId="5" pageNumber="6">
<taxonomicNameLabel id="45A5FF4EFFB2E461054BEAACFC5BEF4C" box="[833,962,1014,1039]" pageId="5" pageNumber="6">comb. nov.</taxonomicNameLabel>
