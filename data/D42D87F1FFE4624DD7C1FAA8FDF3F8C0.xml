<taxonomicName id="9B844D64FFE4624DD4C9F955FBC1F978" box="[927,1098,1697,1720]" class="Insecta" family="Tettigoniidae" genus="Platycleis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="nigrosignata">
