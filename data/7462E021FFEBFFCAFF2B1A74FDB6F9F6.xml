<taxonomicName id="3BCB2AB4FFEBFFCAFDE71C04FC49FAB5" authority="(Meyrick, 1912)" baseAuthorityName="Meyrick" baseAuthorityYear="1912" box="[524,984,1361,1386]" class="Insecta" family="Tortricidae" genus="Polythora" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="viridescens">
