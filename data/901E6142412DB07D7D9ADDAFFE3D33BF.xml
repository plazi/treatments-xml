</taxonomicName>
<taxonomicNameLabel id="31F0B13D4132B07C7974D8EBFB4734E4" box="[1145,1213,189,215]" pageId="16" pageNumber="41" rank="species">sp. n.</taxonomicNameLabel>
; with a distinctive rounded area on the harp as in 
<taxonomicName id="DFB7ABD74132B07C7CF7D8B4FD5834C8" box="[506,674,226,251]" class="Insecta" family="Haglotettigoniidae" genus="Lebinthus" kingdom="Animalia" order="Orthoptera" pageId="16" pageNumber="41" phylum="Arthropoda" rank="species" species="villemantae" status="n. sp.">
<emphasis id="2AC30C464132B07C7CF7D8B4FD5834C8" box="[506,674,226,251]" italics="true" pageId="16" pageNumber="41">L. villemantae</emphasis>
</taxonomicName>
<taxonomicNameLabel id="31F0B13D4132B07C7FA5D8B4FD1334CF" box="[680,745,226,252]" pageId="16" pageNumber="41" rank="species">
<emphasis id="2AC30C464132B07C7FA5D8B4FD1334CF" bold="true" box="[680,745,226,252]" pageId="16" pageNumber="41">n. sp.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="DFB7ABD74132B07C7E2CD8B4FBBB34CF" authority="Otte, 2007" authorityName="Otte" authorityYear="2007" box="[801,1089,226,252]" class="Insecta" family="Haglotettigoniidae" genus="Lebinthus" kingdom="Animalia" order="Orthoptera" pageId="16" pageNumber="41" phylum="Arthropoda" rank="species" species="makilingus">
<emphasis id="2AC30C464132B07C7E2CD8B4FC4534C8" box="[801,959,226,251]" italics="true" pageId="16" pageNumber="41">L. makilingus</emphasis>
<bibRefCitation id="7C26ADA54132B07C7ECBD8B4FBBB34CF" author="Otte" box="[966,1089,226,252]" pageId="16" pageNumber="47" refString="Otte, D. (2007) New species of Cardiodactylus from the western Pacific region (Gryllidae: Eneopterinae). Proceedings of the Academy of Natural Sciences of Philadelphia, 156, 341 - 400." type="journal article" year="2007">
Otte, 
<number id="AE7FC2734132B07C790BD8B4FBBB34CF" box="[1030,1089,226,252]" pageId="16" pageNumber="41" value="2007.0">2007</number>
</bibRefCitation>
