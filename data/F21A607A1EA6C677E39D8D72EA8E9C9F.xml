<taxonomicName id="28176D35EBF63435B53044AD9C548C7E" class="Insecta" family="Lycidae" genus="Calochromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calochromus perfacetus" order="Coleoptera" pageId="3" pageNumber="118" phylum="Arthropoda" rank="species" species="perfacetus">Calochromus perfacetus</taxonomicName>
