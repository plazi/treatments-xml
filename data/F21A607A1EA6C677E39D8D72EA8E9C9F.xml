<taxonomicName id="93C09F62F86AA085E319FA1691D6B13C" class="Magnoliopsida" family="Rosaceae" genus="Spiraea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Spiraea alba" order="Rosales" pageId="3" pageNumber="118" phylum="Tracheophyta" rank="species" species="alba">Spiraea alba</taxonomicName>
