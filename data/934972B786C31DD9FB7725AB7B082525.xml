<subSubSection id="B4406C4F271445C7DB256D492A998CD6" type="nomenclature">
<paragraph id="7A7A5EB7A579021D8CC4D79B7E694D8F" pageNumber="66">
<taxonomicName id="1800DBA6D13BC40122CC483324DADD09" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:229335" authority="Fernandez, 2003" authorityName="Fernandez" authorityYear="2003" class="Insecta" family="Formicidae" genus="Tyrannomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Tyrannomyrmex dux Borowiec" order="Hymenoptera" pageNumber="66" phylum="Arthropoda" rank="species" species="dux">Tyrannomyrmex dux</taxonomicName>
