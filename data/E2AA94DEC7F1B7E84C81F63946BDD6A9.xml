<paragraph id="5FF73171F720B75F0364BCAB5C0696A3" pageId="null" pageNumber="295">
<pageBreakToken id="0B87232B818CD437E97490026DDFCD7A" pageId="null" pageNumber="295" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="6999CE6956B8E55950A914370D1DF1F0" authority="L." class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="295" phylum="Tracheophyta" rank="species" species="pusillum">
Galium 
<normalizedToken id="D56614303FBCBD661A307D09DD9842AD" originalValue="pusíllum" pageId="null" pageNumber="295">pusillum</normalizedToken>
<authorityName id="98D11DEA6D389A16D1971320E10EB7A6" pageId="null" pageNumber="295">L.</authorityName>
