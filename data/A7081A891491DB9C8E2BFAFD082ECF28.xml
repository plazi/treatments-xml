<taxonomicName id="A70F7237F51481788B714E057EB4D818" authorityName="Gistel" authorityYear="1857" class="Insecta" family="Scarabaeidae" genus="Rutela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rutela caesarea" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="caesarea">
