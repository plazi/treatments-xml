<taxonomicName id="4C638795FFE2BD1AFC833B32FB995333" authorityName=", Baly" authorityYear="1876" box="[871,1049,801,827]" class="Insecta" family="Chrysomelidae" genus="Chaetocnema" kingdom="Animalia" order="Coleoptera" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="simplicifrons">
<emphasis id="B9172004FFE2BD1AFC833B32FB995333" box="[871,1049,801,827]" italics="true" pageId="114" pageNumber="115">C. simplicifrons</emphasis>
</taxonomicName>
(length 
<quantity id="4C9B51F3FFE2BD1AFB923B33FA8F5333" box="[1142,1295,800,827]" metricMagnitude="-3" metricUnit="m" metricValue="1.725" metricValueMax="1.8" metricValueMin="1.65" pageId="114" pageNumber="115" unit="mm" value="1.725" valueMax="1.8" valueMin="1.65">1.65–1.8 mm</quantity>
).
</paragraph>
