<taxonomicName id="4C30BB9BAA78FFD9FBAAF96E6397F95D" box="[1141,1428,1755,1780]" class="Insecta" family="Curculionidae" genus="Crypturgus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="49" phylum="Arthropoda" rank="species" species="subcribrosus">
