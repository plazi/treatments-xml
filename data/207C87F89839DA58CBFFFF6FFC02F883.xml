<collectingCountry id="D0C2767E9839DA58CA0FF8CBFE30F882" box="[359,416,1843,1866]" name="Laos" pageId="7" pageNumber="448">Laos</collectingCountry>
, 
<collectingCountry id="D0C2767E9839DA58CAC4F8CAFD9EF883" box="[428,526,1842,1867]" name="Vietnam" pageId="7" pageNumber="448">Vietnam</collectingCountry>
