<quantity id="4CF79B33FFBEFF97C02AFB6EFD1FFB27" box="[595,649,1207,1227]" metricMagnitude="2" metricUnit="m" metricValue="5.4" pageId="81" pageNumber="236" unit="m" value="540.0">540m</quantity>
