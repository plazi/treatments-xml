<paragraph id="8BB036D6FFBEFF97C2EEFB45FE10FB0A" blockId="81.[151,1060,1180,1254]" pageId="81" pageNumber="236">
<collectingRegion id="49CBF834FFBEFF97C2EEFB45FEB7FB5C" box="[151,289,1180,1200]" country="Austria" name="Oberoesterreich" pageId="81" pageNumber="236">Oberösterreich</collectingRegion>
