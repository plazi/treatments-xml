<paragraph id="8B99035EFF8EFF8989B9C2E223BA9B7B" blockId="63.[151,1008,1265,1291]" box="[151,1008,1265,1291]" pageId="63" pageNumber="64">
<heading id="D0D1B432FF8EFF8989B9C2E223BA9B7B" bold="true" box="[151,1008,1265,1291]" fontSize="11" level="1" pageId="63" pageNumber="64" reason="1">
<taxonomicName id="4C2678DDFF8EFF8989B9C2E223BA9B7B" authority="(Rudolphi, 1819) Baudin-Laurencin, 1971" authorityName="(Rudolphi, 1819) Baudin-Laurencin" authorityYear="1971" box="[151,1008,1265,1291]" class="Secernentea" family="Camallanidae" genus="Oncophora" kingdom="Animalia" order="Camallanida" pageId="63" pageNumber="64" phylum="Nematoda" rank="species" species="melanocephala">
