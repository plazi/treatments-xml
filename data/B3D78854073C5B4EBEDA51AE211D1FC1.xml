<paragraph id="EF75B0F690452F1A265CFA029C1D57F9" pageId="0" pageNumber="65">
<taxonomicName id="5DA4959F75E7EC479B88EC5F3FC5C45E" authorityName="S. T. Huang, J. W. Xia, X. G. Zhang &amp; Z. Li" authorityYear="2021" class="Sordariomycetes" family="Diaporthaceae" genus="Diaporthe" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Diaporthe melastomatis" order="Diaporthales" pageId="0" pageNumber="65" phylum="Ascomycota" rank="species" species="melastomatis">
