<collectorName id="61335367322D8A72FD5886FEBE03F8F4" box="[696,794,1785,1811]" pageId="7" pageNumber="359">Vietnam</collectorName>
, 
<collectorName id="61335367322D8A72FCC486FDBE6FF8F3" box="[804,886,1786,1812]" pageId="7" pageNumber="359">Annam</collectorName>
(
<collectionCode id="AAD7AE74322D8A72FC6586FEBEF8F8F4" box="[901,993,1785,1811]" country="Switzerland" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="7" pageNumber="359" type="Museum">MHNG</collectionCode>
).
</materialsCitation>
</paragraph>
