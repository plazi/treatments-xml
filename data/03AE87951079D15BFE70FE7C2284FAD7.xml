<quantity id="4CFF9B661079D15DFEF1FA2B22B0FA69" box="[325,408,1420,1444]" metricMagnitude="-3" metricUnit="m" metricValue="8.1" pageId="3" pageNumber="798" unit="mm" value="8.1">8.1 mm</quantity>
