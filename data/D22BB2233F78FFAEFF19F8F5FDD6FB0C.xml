<paragraph id="5A3D03353F79FFAEFF48FD7DFA89FD2F" blockId="7.[151,1437,151,1260]" box="[198,1337,693,719]" pageId="7" pageNumber="8">
<emphasis id="68F6DF273F79FFAEFF48FD7DFEA6FD2F" bold="true" box="[198,278,693,719]" pageId="7" pageNumber="8">Hosts:</emphasis>
<taxonomicName id="9D8278B63F79FFAEFE93FD7DFE11FD2F" box="[285,417,693,719]" class="Actinopterygii" family="Sciaenidae" kingdom="Animalia" order="Perciformes" pageId="7" pageNumber="8" phylum="Chordata" rank="family">Sciaenidae</taxonomicName>
: 
<taxonomicName id="9D8278B63F79FFAEFE22FD7DFCF3FD2F" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[428,835,693,719]" class="Actinopterygii" family="Sciaenidae" genus="Umbrina" kingdom="Animalia" order="Perciformes" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="cirrosa">
<emphasis id="68F6DF273F79FFAEFE22FD7DFDC0FD2E" box="[428,624,693,718]" italics="true" pageId="7" pageNumber="8">Umbrina cirrosa</emphasis>
(Linnaeus, 
<number id="EC4A11123F79FFAEFD75FD7DFC89FD2F" box="[763,825,693,719]" pageId="7" pageNumber="8" value="1758.0">1758</number>
)
</taxonomicName>
, 
<taxonomicName id="9D8278B63F79FFAEFCC0FD7DFA85FD2F" authority="Valenciennes, 1843" authorityName="Valenciennes" authorityYear="1843" box="[846,1333,693,719]" class="Actinopterygii" family="Sciaenidae" genus="Umbrina" kingdom="Animalia" order="Perciformes" pageId="7" pageNumber="8" phylum="Chordata" rank="species" species="canariensis">
<emphasis id="68F6DF273F79FFAEFCC0FD7DFBF7FD2E" box="[846,1095,693,718]" italics="true" pageId="7" pageNumber="8">Umbrina canariensis</emphasis>
Valenciennes, 
<number id="EC4A11123F79FFAEFB79FD7DFA85FD2F" box="[1271,1333,693,719]" pageId="7" pageNumber="8" value="1843.0">1843</number>
</taxonomicName>
