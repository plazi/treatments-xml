<paragraph id="D95D36A30128370F76E5FEF7EE40054B" blockId="24.[151,1437,255,311]" pageId="24" pageNumber="25">
<treatmentCitationGroup id="F9F2118D0128370F76E5FEF7EE40054B" pageId="24" pageNumber="25">
<treatmentCitation id="584310B20128370F76E5FEF7EDE8056A" author="Enock, F." box="[151,446,255,280]" page="453" pageId="24" pageNumber="25" year="1909">
<taxonomicName id="1EE24D200128370F76E5FEF7EDE8056A" authority="Enock, 1909: 453" authorityName="Enock" authorityPageNumber="453" authorityYear="1909" box="[151,446,255,280]" class="Insecta" family="Mymaridae" genus="Cleruchus" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">
<emphasis id="EB96EAB10128370F76E5FEF7ECA90565" box="[151,255,256,280]" italics="true" pageId="24" pageNumber="25">Cleruchus</emphasis>
<bibRefCitation id="BD734B520128370F7774FEF7EDE8056A" author="Enock, F." box="[262,446,255,280]" pageId="24" pageNumber="25" pagination="449 - 459" refId="ref48423" refString="Enock, F. (1909) New genera of British Mymaridae (Haliday). Transactions of the Entomological Society of London, 1909, 449 - 459, pls. XII - XV. https: // doi. org / 10.1111 / j. 1365 - 2311.1909. tb 02179. x" type="journal article" year="1909">Enock, 1909: 453</bibRefCitation>
</taxonomicName>
</treatmentCitation>
. 
<typeStatus id="065988010128370F77B5FEF7EDAF0565" box="[455,505,256,280]" pageId="24" pageNumber="25">Type</typeStatus>
