, 
<accessionNumber id="943810ADFFE7FFDB198D85DEFC21FF78" box="[908,988,217,241]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/B13450" pageId="11" pageNumber="652">B13450</accessionNumber>
, 
<accessionNumber id="943810ADFFE7FFDB19EB85DEFBC6FF78" box="[1002,1083,217,241]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/B13452" pageId="11" pageNumber="652">B13452</accessionNumber>
, 
<accessionNumber id="943810ADFFE7FFDB1E4885DEFB64FF78" box="[1097,1177,217,241]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/B13463" pageId="11" pageNumber="652">B13463</accessionNumber>
