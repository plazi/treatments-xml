<accessionNumber id="943810ADFFE7FFDB1ECB842FFAE5FEC9" box="[1226,1304,296,320]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/B13446" pageId="11" pageNumber="652">B13446</accessionNumber>
)
</materialsCitation>
; 
