<paragraph id="B16D98FD8720DF31B2780D2A605FE554" pageId="3" pageNumber="4">
<taxonomicName id="5C8C1EAFA829A926B9C17BD1937A820E" class="Anthozoa" family="Plexauridae" genus="Bebryce" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bebryce boninensis" order="Alcyonacea" pageId="3" pageNumber="4" phylum="Cnidaria" rank="species" species="boninensis">
