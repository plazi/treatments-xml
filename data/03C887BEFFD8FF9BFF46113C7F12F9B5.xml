<location id="8EBE6073FFD8FF94FF4611BA7C3DF91C" LSID="urn:lsid:plazi:treatment:03C887BEFFD8FF9BFF46113C7F12F9B5:8EBE6073FFD8FF94FF4611BA7C3DF91C" box="[192,257,1699,1723]" country="United Kingdom" county="European" municipality="North American" name="Note" pageId="7" pageNumber="6">Note</location>
