: male, 
<collectionCode id="ED70AE6DFFD8FF94FC84119D7E62F93C" box="[770,862,1668,1691]" pageId="7" pageNumber="6">BMNH</collectionCode>
, 
<location id="8EBE6073FFD8FF94FCEE119A7EF9F93C" LSID="urn:lsid:plazi:treatment:03C887BEFFD8FF9BFF46113C7F12F9B5:8EBE6073FFD8FF94FCEE119A7EF9F93C" box="[872,965,1667,1691]" name="London" pageId="7" pageNumber="6">London</location>
[examined]; 
<materialsCitation id="3B093CF5FFD8FF94FBE2119A7F31F95C" country="United Kingdom" county="European" location="Note" municipality="North American" pageId="7" pageNumber="6" specimenCount="1">
