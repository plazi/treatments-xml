<typeStatus id="FA5F9FE0CD206A22F2DD45A9FE88FD20" box="[189,301,643,669]" pageId="68" pageNumber="69">paratypes</typeStatus>
(
<accessionNumber id="3AB7BCA1CD206A22F35F45A9FE70FD23" box="[319,469,643,670]" httpUri="http://www.ncbi.nlm.nih.gov/protein/PMU24748" pageId="68" pageNumber="69">
<collectionCode id="43F5B987CD206A22F35F45A9FE24FD20" box="[319,385,643,669]" country="Sweden" httpUri="http://grbio.org/cool/32ji-dbbk" name="Paleontological Museum of Uppsala" pageId="68" pageNumber="69">PMU</collectionCode>
