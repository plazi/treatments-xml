<typeStatus id="FA5F9FE0CD206A22F19B454AFBC7FDC7" box="[1019,1122,608,634]" pageId="68" pageNumber="69">holotype</typeStatus>
(
<accessionNumber id="3AB7BCA1CD206A22F612454AFAA2FDC7" box="[1138,1287,608,634]" httpUri="http://www.ncbi.nlm.nih.gov/protein/PMU24747" pageId="68" pageNumber="69">
