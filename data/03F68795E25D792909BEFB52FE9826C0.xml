<taxonomicName id="4C5F4D00E25C792A0A69FE91FBCB2628" authority="Dall, 1916" authorityName="Dall" authorityYear="1916" box="[832,1122,260,284]" class="Bivalvia" family="Myidae" genus="Sphenia" kingdom="Animalia" order="Myoida" pageId="17" pageNumber="56" phylum="Mollusca" rank="species" species="pholadidea">
<emphasis id="B92BEA91E25C792A0A69FE91FC752628" box="[832,988,260,284]" italics="true" pageId="17" pageNumber="56">S. pholadidea</emphasis>
<bibRefCitation id="EFCE4B72E25C792A0ACFFE90FBCB2628" author="Dall" box="[998,1122,260,284]" pageId="17" pageNumber="56" refString="Dall, W. H. (1916) Diagnoses of new species of marine bivalve mollusks from the northwest coast of America in the collection of the United States National Museum. Proceedings of the United States National Museum, 52 (2183), 393 - 417." type="journal article" year="1916">
Dall, 
<number id="3D9724A4E25C792A0D0EFE90FBCB2628" box="[1063,1122,260,284]" pageId="17" pageNumber="56" value="1916.0">1916</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="4C5F4D00E25C792A0D46FE90FF66260B" authority="Oldroyd, 1918" authorityName="Oldroyd" authorityYear="1918" class="Bivalvia" family="Cuspidariidae" genus="Cuspidaria" kingdom="Animalia" order="Anomalodesmata" pageId="17" pageNumber="56" phylum="Mollusca" rank="species" species="nana">
<emphasis id="B92BEA91E25C792A0D46FE90FA872628" box="[1135,1326,260,284]" italics="true" pageId="17" pageNumber="56">Cuspidaria nana</emphasis>
<bibRefCitation id="EFCE4B72E25C792A0C1EFE90FF66260B" author="Oldroyd" pageId="17" pageNumber="56" refString="Oldroyd, I. S. (1918) A new species of Cuspidaria from Monterey. The Nautilus, 32 (1), 28." type="journal article" year="1918">
Oldroyd, 
<number id="3D9724A4E25C792A09BEFEB3FF66260B" box="[151,207,295,319]" pageId="17" pageNumber="56" value="1918.0">1918</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="4C5F4D00E25C792A082CFEBCFD22260B" authority="Dall, 1919 (Coan 1999)" authorityName="Dall, 1919 (Coan" authorityYear="1999" box="[261,651,295,320]" class="Bivalvia" family="Myidae" genus="Sphenia" kingdom="Animalia" order="Myoida" pageId="17" pageNumber="56" phylum="Mollusca" rank="species" species="globula">
<emphasis id="B92BEA91E25C792A082CFEBCFEDF2674" box="[261,374,296,320]" italics="true" pageId="17" pageNumber="56">S. globula</emphasis>
<bibRefCitation id="EFCE4B72E25C792A0857FEB3FE5D260B" author="Dall" box="[382,500,295,319]" pageId="17" pageNumber="56" refString="Dall, W. H. (1919) Descriptions of new species of Mollusca from the north Pacific Ocean in the collection of the United States National Museum. Proceedings of the United States National Museum, 56 (2295), 297 - 371." type="journal article" year="1919">
Dall, 
<number id="3D9724A4E25C792A0895FEB3FE5D260B" box="[444,500,295,319]" pageId="17" pageNumber="56" value="1919.0">1919</number>
</bibRefCitation>
(
<bibRefCitation id="EFCE4B72E25C792A0B2AFEB3FD2B260B" author="Coan" box="[515,642,295,319]" pageId="17" pageNumber="56" refString="Coan, E. V. (1999) The eastern Pacific species of Sphenia (Bivalvia: Myidae). The Nautilus, 113 (4), 103 - 120." type="journal article" year="1999">
Coan 
<number id="3D9724A4E25C792A0B6FFEB3FD2B260B" box="[582,642,295,319]" pageId="17" pageNumber="56" value="1999.0">1999</number>
</bibRefCitation>
)
</taxonomicName>
.
</paragraph>
</subSubSection>
<caption id="DF20660BE25C792A09BEF8B2FD1D20E2" httpUri="https://zenodo.org/record/281742/files/figure.png" pageId="17" pageNumber="56" targetBox="[151,1435,373,1808]" targetPageId="17">
