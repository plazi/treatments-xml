<caption id="EAD712C09E129372A3F8F95AFB7CDCB9" box="[151,1214,1726,1749]" pageId="75" pageNumber="76" startId="75.[151,250,1726,1749]" targetBox="[203,1397,213,1683]" targetPageId="75">
<paragraph id="BE1742489E129372A3F8F95AFB7CDCB9" blockId="75.[151,1214,1726,1749]" box="[151,1214,1726,1749]" pageId="75" pageNumber="76">
