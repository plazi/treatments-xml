<taxonomicName id="79A839CB9E129372A25EF95BFDCADCB9" authorityName="Perkins" authorityYear="2012" box="[305,520,1726,1748]" class="Insecta" family="Epimetopidae" genus="Epimetopus" kingdom="Animalia" order="Coleoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="deceptus">
