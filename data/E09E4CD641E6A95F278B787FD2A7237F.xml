<paragraph id="F011E9CC3BF2EE4BD883BAA72A00A5D1" pageId="null" pageNumber="353">
<pageBreakToken id="64FF41ED53AFCE5D012A728415BA42AB" pageId="null" pageNumber="353">Artengruppe</pageBreakToken>
der 
<taxonomicName id="AE7E3E58874195F785464CEE8A5657E9" authority="L." class="Magnoliopsida" family="Caprifoliaceae" genus="Scabiosa" kingdom="Plantae" order="Dipsacales" pageId="null" pageNumber="353" phylum="Tracheophyta" rank="species" species="columbaria">
Scabiosa 
