<taxonomicName id="4C324D33B44FFFE4AA73D189FC84A48F" box="[776,880,153,176]" class="Insecta" family="Cicadellidae" genus="Japanagallia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="javana">
