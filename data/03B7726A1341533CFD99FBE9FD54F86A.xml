<paragraph id="8BA1C37C1341533CFF4CFB26FD94FB75" blockId="9.[189,530,1244,1270]" box="[189,530,1244,1270]" pageId="9" pageNumber="10">
<treatmentCitationGroup id="AB0EE4521341533CFF4CFB26FD94FB75" box="[189,530,1244,1270]" pageId="9" pageNumber="10">
<taxonomicName id="4C1EB8FF1341533CFF4CFB26FEAFFB75" authorityName="Dejean" authorityYear="1831" box="[189,297,1244,1270]" class="Insecta" family="Carabidae" genus="Dyscolus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
