<taxonomicName id="C2C04D3C7077FFB6BB1B9EB7FA98FB76" box="[1110,1342,1185,1210]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="5" pageNumber="156" phylum="Tracheophyta" rank="species" species="melongena">
