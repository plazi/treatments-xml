<taxonomicName id="C2C04D3C7077FFB6BDF19EFCFCE3FACE" authorityName="Walker, 1849:" authorityYear="1098" box="[700,837,1257,1282]" class="Insecta" family="Ephydridae" genus="Notiphila" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="smaragdi">
