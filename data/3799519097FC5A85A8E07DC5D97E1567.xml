<paragraph id="31C6691C6F82073CC29BDA2883C4C982" pageId="0" pageNumber="129">
<taxonomicName id="B2349A0486E4979F6001496AC5A831A0" LSID="37995190-97FC-5A85-A8E0-7DC5D97E1567" class="Insecta" family="Tortricidae" genus="Crocidosema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crocidosema nitsugai" order="Lepidoptera" pageId="0" pageNumber="129" phylum="Arthropoda" rank="species" species="nitsugai">Crocidosema nitsugai</taxonomicName>
