<typeStatus id="567297A2AC573B30FE9CD9685DCAFEE5" box="[333,393,301,324]" pageId="27" pageNumber="420">TYPE</typeStatus>
. — 
<location id="8C167FDBAC573B30FE65D9685ED3FEE5" LSID="urn:lsid:plazi:treatment:01609816AC563B30FF1DDE6358ABF939:8C167FDBAC573B30FE65D9685ED3FEE5" box="[436,656,301,324]" country="Guinea-Bissau" name="Plateau de Zouguepo" pageId="27" pageNumber="420">
