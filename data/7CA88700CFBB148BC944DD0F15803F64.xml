<taxonomicName id="0EA3CCFF4CD49F952E41FE86C9AC483E" authority="L." class="Magnoliopsida" family="Solanaceae" genus="Physalis" kingdom="Plantae" order="Solanales" pageId="null" pageNumber="166" phylum="Tracheophyta" rank="species" species="alkekengi">
Physalis 
<normalizedToken id="8D63065E0B49A4484CDE975AD7C9A85D" originalValue="Alkekéngi" pageId="null" pageNumber="166">Alkekengi</normalizedToken>
<authorityName id="42B46F0A5ADACC9A379DA3F6A27C49AB" pageId="null" pageNumber="166">L.</authorityName>
