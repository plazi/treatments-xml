<collectionCode id="F8CB878FFFBCF51CFC36FAE8FB81404C" box="[987,1067,1319,1342]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="11" pageNumber="412" type="Museum">RMNH</collectionCode>
