<taxonomicName id="F3C64D19FF96223BED33FDF4B26AFDC4" box="[775,895,564,589]" class="Insecta" family="Sesiidae" genus="Carmenta" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="leptosoma">
<emphasis id="06B2EA88FF96223BED33FDF4B26AFDC4" box="[775,895,564,589]" italics="true" pageId="11" pageNumber="12">leptosoma</emphasis>
