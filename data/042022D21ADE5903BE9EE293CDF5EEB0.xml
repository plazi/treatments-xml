<taxonomicName id="5A149E1D1870273A52A651279ADDDBA9" authority="Germar, 1834" authorityName="Germar" authorityYear="1834" class="Insecta" family="Dermestidae" genus="Thorictus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="T munganasti" order="Coleoptera" pageId="0" pageNumber="155" phylum="Arthropoda" rank="species" species="munganasti">
