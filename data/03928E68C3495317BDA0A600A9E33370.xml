. 
<taxonomicName id="4C3B44FDC3495318BE3CA65EAA5B359A" authority="(C. B. Rob.) W. H. Brown ex Merrill (1915: 231)" authorityName="Merrill" authorityPageNumber="231" authorityYear="1915" baseAuthorityName="C. B. Rob." class="Magnoliopsida" family="Annonaceae" genus="Griffithianthus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Magnoliales" pageId="7" pageNumber="34" phylum="Tracheophyta" rank="species" species="merrillii">
