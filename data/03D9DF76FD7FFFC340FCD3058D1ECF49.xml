<taxonomicName id="4C7015E3FD7EFFC040FCD0838ED6C9F7" authority="Fleut., C. M. F." authorityName="Fleut., C. M. F." box="[151,668,2008,2034]" class="Insecta" family="Elateridae" genus="Nycterilampus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="56" phylum="Arthropoda" rank="species" species="velutinus">
