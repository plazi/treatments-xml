<taxonomicName id="4C224D2889687825FBE3301C4610D605" box="[189,362,542,568]" class="Arachnida" family="Pholcidae" genus="Pehrforsskalia" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="12" phylum="Arthropoda" rank="genus">
