(
<bibRefCitation id="EFFDA7BD660EB476C4684F4938C92527" author="Urban, C." pageId="91" pageNumber="92" pagination="65 - 79" refId="ref412580" refString="Urban, C. (1929) Beitrage zur Naturgeschichte einiger Russelkafer II. Entomologische Blatter, 25, 65 - 79." type="journal article" year="1929">Urban 1929</bibRefCitation>
