<taxonomicNameLabel id="629357CA6651FF95FF66FCA2FEE8FA41" box="[240,273,836,860]" pageId="9" pageNumber="380" sensu="lato">s.l.</taxonomicNameLabel>
<taxonomicName id="8CD44D206651FF95FE8EFCA2FE96FA41" authority="Jewett, 1960" authorityName="Jewett" authorityYear="1960" box="[280,367,836,860]" class="Insecta" family="Capniidae" genus="Capnia" kingdom="Animalia" order="Plecoptera" pageId="9" pageNumber="380" phylum="Arthropoda" rank="species" species="gibbera">gibbera</taxonomicName>
</emphasis>
