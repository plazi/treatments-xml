<geoCoordinate id="22C727D03B385C2CEAF0FDCCFDBAC42D" box="[418,512,546,570]" degrees="2" direction="south" minutes="39.70" orientation="latitude" pageId="34" pageNumber="833" precision="9" value="-2.6616666">2°39.70’S</geoCoordinate>
