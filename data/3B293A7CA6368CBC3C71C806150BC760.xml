<taxonomicName id="483C718BDF97D7C6DF86C2A3D32710C4" authority="Vaucher" authorityName="Vaucher" class="Magnoliopsida" family="Orobanchaceae" genus="Orobanche" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="260" phylum="Tracheophyta" rank="species" species="artemisiae">
