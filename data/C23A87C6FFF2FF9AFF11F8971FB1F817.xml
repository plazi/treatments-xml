<number id="FC5B24F7FFF1FF9AFF75F8F41D52F882" box="[243,340,1892,1917]" pageId="66" pageNumber="67" value="164.0" valueMax="175.0" valueMin="153.0">153–175</number>
<date id="3E2D1010FFF1FF9AFEDCF8F51D9EF882" box="[346,408,1892,1917]" pageId="66" pageNumber="67">
x 
<number id="FC5B24F7FFF1FF9AFEE9F8F41D9EF882" box="[367,408,1892,1917]" pageId="66" pageNumber="67" value="6.5" valueMax="8.0" valueMin="5.0">5–8</number>
