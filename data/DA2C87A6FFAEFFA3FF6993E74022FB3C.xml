<number id="E44D2497FFAEFFA3FCCE97284140FB8C" box="[816,887,1112,1136]" pageId="9" pageNumber="10" value="32.5" valueMax="34.0" valueMin="31.0">31–34</number>
% 
<collectionCode id="3494AE75FFAEFFA3FC6A97284182FB8C" box="[916,949,1112,1136]" pageId="9" pageNumber="10">SL</collectionCode>
in 
<taxonomicName id="95854D33FFAEFFA3FC249728467FFB8C" box="[986,1096,1112,1136]" class="Actinopterygii" family="Apogonidae" genus="Apogon" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="mosavi">
<emphasis id="60F1EAA2FFAEFFA3FC249728467FFB8C" box="[986,1096,1112,1136]" italics="true" pageId="9" pageNumber="10">A. mosavi</emphasis>
</taxonomicName>
). Additional larval specimens of 
<taxonomicName id="95854D33FFAEFFA3FF4B97F0433BFB64" box="[181,268,1152,1176]" class="Actinopterygii" family="Apogonidae" genus="Apogon" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="10" phylum="Chordata" rank="genus">
<emphasis id="60F1EAA2FFAEFFA3FF4B97F0433BFB64" box="[181,268,1152,1176]" italics="true" pageId="9" pageNumber="10">Apogon</emphasis>
