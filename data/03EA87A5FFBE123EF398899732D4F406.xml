. 
<typeStatus id="54F88811FFBE123EF15F884F31BCF678" box="[592,642,1297,1318]" pageId="23" pageNumber="24">Type</typeStatus>
genus 
<taxonomicName id="4C434D30FFBE123EF1DF884E3700F678" authority="Miranda-Ribeiro, 1923" authorityName="Miranda-Ribeiro" authorityYear="1923" box="[720,1086,1296,1318]" genus="Lophyohyla" kingdom="Animalia" pageId="23" pageNumber="24" rank="genus">
