<emphasis id="39A7C046FFAC6A555AA1FEBDFC68FED1" box="[914,993,262,287]" italics="true" pageId="8" pageNumber="439">
<taxonomicName id="CCD367D7FFAC6A555AA1FEBDFC55FED1" authorityName="Sorensen" authorityYear="1932" box="[914,988,262,287]" class="Arachnida" family="Phalangodidae" genus="Isaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="8" pageNumber="439" phylum="Arthropoda" rank="genus">Isaeus</taxonomicName>
,
</emphasis>
