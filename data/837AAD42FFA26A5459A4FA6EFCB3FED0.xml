<taxonomicName id="CCD367D7FFAC6A555B7DFF5AFD5EFF35" authorityName="Banks" authorityYear="1900" box="[590,727,225,251]" class="Arachnida" family="Phalangodidae" genus="Hoplobunus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="8" pageNumber="439" phylum="Arthropoda" rank="genus">
