<taxonomicNameLabel id="22947D3DFFAC6A555B68FEBEFD4DFED1" box="[603,708,261,287]" pageId="8" pageNumber="439" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="CCD367D7FFAC6A555BE0FEBEFCD5FED1" authorityName="Banks" authorityYear="1900" box="[723,860,261,287]" class="Arachnida" family="Phalangodidae" genus="Hoplobunus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="8" pageNumber="439" phylum="Arthropoda" rank="genus">
<emphasis id="39A7C046FFAC6A555BE0FEBEFCD5FED1" box="[723,860,261,287]" italics="true" pageId="8" pageNumber="439">Hoplobunus</emphasis>
