</paragraph>
<paragraph id="8838422BFFF9FF91FF7C9EFE6376FA9D" blockId="6.[151,1437,546,1399]" pageId="6" pageNumber="446">
<materialsCitation id="38EF4876FFF9FF91FF7C9EFE60A4FB55" ID-GBIF-Occurrence="1440784464" box="[199,730,1230,1255]" country="Mexico" location="MEXICO" pageId="6" pageNumber="446" specimenCount="1" typeStatus="holotype">
