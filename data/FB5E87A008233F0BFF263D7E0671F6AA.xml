<location id="7628606D08233F0BFC093C6704CCF64F" LSID="urn:lsid:plazi:treatment:FB5E87A008233F0BFF263D7E0671F6AA:7628606D08233F0BFC093C6704CCF64F" box="[952,1022,1887,1912]" country="Philippines" county="The" municipality="The" name="Drake" pageId="13" pageNumber="287" stateProvince="Java and Sri">Drake</location>
1927), 
<collectingRegion id="B133F85408233F0BFBE43C5803ECF64F" box="[1109,1246,1887,1912]" isEnumeration="true" pageId="13" pageNumber="287">Java and Sri</collectingRegion>
