<paragraph id="725B8C5A9449C2907B684B182316AFBA" pageId="null" pageNumber="443">
<pageBreakToken id="437C16D9CC92C87323F2E5FCA996FF7A" pageId="null" pageNumber="443" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="DF55EC10E0317E1731FCD8061DF4A453" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="443" phylum="Tracheophyta" rank="genus">Centaurea</taxonomicName>
