<taxonomicName id="19A4B093FF9C360DDFE87A832DACA0AD" class="Insecta" family="Delphacidae" genus="Vizcaya" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="615" phylum="Arthropoda" rank="species" species="undetermined">
