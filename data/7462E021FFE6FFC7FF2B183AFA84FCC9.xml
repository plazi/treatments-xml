<taxonomicName id="3BCB2AB4FFE6FFC7FDD91B8BFD04FD2A" authorityName="Ruiz &amp; Pavon" authorityYear="1794" box="[562,661,734,757]" class="Magnoliopsida" family="Melastomataceae" genus="Miconia" kingdom="Plantae" order="Myrtales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="genus">
