, T = 
<number id="3DFEAC6CFF8BFFB77FC8C466FC832E70" box="[697,797,1086,1110]" pageId="17" pageNumber="18" value="115.0" valueMax="123.0" valueMin="107.0">107–123</number>
, HF = 
<number id="3DFEAC6CFF8BFFB77E1BC466FC2D2E70" box="[874,947,1086,1110]" pageId="17" pageNumber="18" value="24.0" valueMax="25.0" valueMin="23.0">23–25</number>
, E = 
<number id="3DFEAC6CFF8BFFB77E9CC466FBA82E70" box="[1005,1078,1086,1110]" pageId="17" pageNumber="18" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
