<number id="3D8424B6FFBE331E550C010FA006FA84" box="[768,783,1348,1374]" pageId="9" pageNumber="42" value="2.0">2</number>
ɗɗ (
<collectionCode id="ED5DAE54FFBE331E5549010FA08EFA84" LSID="urn:lsid:biocol.org:col:33074" box="[837,903,1348,1374]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33074" name="China Agricultural University" pageId="9" pageNumber="42">CAU</collectionCode>
, 
<collectionCode id="ED5DAE54FFBE331E559F010FA0F9FA84" box="[915,1008,1348,1374]" pageId="9" pageNumber="42">BMNH</collectionCode>
), same data as 
<typeStatus id="54F78833FFBE331E5293010FA600FA84" box="[1183,1289,1348,1374]" pageId="9" pageNumber="42">holotype</typeStatus>
