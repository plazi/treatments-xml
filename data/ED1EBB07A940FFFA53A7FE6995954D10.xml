<collectingCountry id="1DA04A81A940FFFA50C4FB9994334CC8" box="[1003,1063,1130,1156]" name="India" pageId="15" pageNumber="280">India</collectingCountry>
, 
<collectingCountry id="1DA04A81A940FFFA571DFB9994A04CC8" box="[1074,1204,1130,1156]" name="Philippines" pageId="15" pageNumber="280">Philippines</collectingCountry>
