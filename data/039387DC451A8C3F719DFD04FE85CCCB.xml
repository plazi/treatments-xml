Mermithids from 
<taxonomicName id="4C3A4D49451A8C387307F8BDFD28CAC6" box="[525,695,1902,1925]" class="Insecta" family="Nemouridae" genus="Protonemura" kingdom="Animalia" order="Plecoptera" pageId="11" pageNumber="69" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="B94EEAD8451A8C387307F8BDFD0CCAC6" box="[525,659,1902,1925]" italics="true" pageId="11" pageNumber="69">Protonemura</emphasis>
sp.
</taxonomicName>
(
<taxonomicName id="4C3A4D49451A8C3873CFF8BDFC83CAC5" authorityName="Theischinger" authorityYear="1976" box="[709,796,1902,1926]" class="Insecta" family="Nemouridae" genus="Protonemura" kingdom="Animalia" order="Plecoptera" pageId="11" pageNumber="69" phylum="Arthropoda" rank="species" species="aculeata">
