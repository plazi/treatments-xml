<collectingCountry id="F31C378ADA19FF87FE30FD12FE38BA5B" box="[394,479,761,786]" name="Mexico" pageId="2" pageNumber="181">Mexico</collectingCountry>
(
<collectingRegion id="49CFB9F8DA19FF87FE55FD12FDA6BA5B" box="[495,577,761,786]" country="Mexico" name="Jalisco" pageId="2" pageNumber="181">Jalisco</collectingRegion>
