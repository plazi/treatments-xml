<taxonomicName id="90180DAE153A644A2A42A3BE6EEB701B" class="Diplopoda" family="Paradoxosomatidae" genus="Antheromorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Antheromorpha pardalis" order="Polydesmida" pageId="5" pageNumber="26" phylum="Arthropoda" rank="species" species="pardalis">Antheromorpha pardalis</taxonomicName>
