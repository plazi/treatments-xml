<taxonomicName id="83BE976C4327CE0E766180ECAB367232" class="Insecta" family="Chalcididae" genus="Brachymeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachymeria coxodentata" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="coxodentata">Brachymeria coxodentata</taxonomicName>
