<emphasis id="18E67E339E6053CE2A95DB0B829F837F" italics="true" pageId="14" pageNumber="137">Muhlenbergia ligularis</emphasis>
</taxonomicName>
, 
<taxonomicName id="1D3A7C7C9D3174E6729A62E58BF82F68" class="Magnoliopsida" family="Poaceae" genus="M." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="M. peruviana" order="Poales" pageId="14" pageNumber="137" phylum="Angiospermae" rank="species" species="peruviana">
<emphasis id="F3184C69BB021A25723E53C435A0F3F1" italics="true" pageId="14" pageNumber="137">M. peruviana</emphasis>
