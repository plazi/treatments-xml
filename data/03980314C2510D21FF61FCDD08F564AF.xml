<geoCoordinate id="EE05D4C5C2510D2DFE27FAF20E146374" box="[465,623,1294,1320]" degrees="22" direction="south" minutes="41" orientation="latitude" pageId="52" pageNumber="53" precision="1" seconds="37.7" value="-22.693804">22º41’37.7’’S</geoCoordinate>
