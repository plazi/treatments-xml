<paragraph id="3C47AF12FFEFFFE425BEBA09C83C5202" blockId="61.[128,654,1023,1759]" pageId="61" pageNumber="61">
Female (
<typeStatus id="E34311B0FFEFFFE42427BA09C83453B0" box="[260,355,1142,1164]" pageId="61" pageNumber="61">allotype</typeStatus>
