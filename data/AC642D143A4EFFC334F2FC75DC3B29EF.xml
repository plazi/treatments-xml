<number id="92058E253A4EFFCC35B6FAB0DCB42E76" box="[467,495,1340,1364]" pageId="7" pageNumber="300" value="29.0">29</number>
and 
<quantity id="E33531E73A4EFFCC3649FAB0DF2F2E76" box="[556,628,1340,1364]" metricMagnitude="-1" metricUnit="m" metricValue="4.4" pageId="7" pageNumber="300" unit="cm" value="44.0">44 cm</quantity>
