<quantity id="E33531E73A4EFFCC3696FAECDE3E2E55" box="[755,869,1376,1400]" metricMagnitude="-1" metricUnit="m" metricValue="3.75" metricValueMax="4.4" metricValueMin="3.1" pageId="7" pageNumber="300" unit="cm" value="37.5" valueMax="44.0" valueMin="31.0">31–44 cm</quantity>
(N= 
<number id="92058E253A4EFFCC37C4FAECDEE62E5A" box="[929,957,1376,1400]" pageId="7" pageNumber="300" value="13.0">13</number>
