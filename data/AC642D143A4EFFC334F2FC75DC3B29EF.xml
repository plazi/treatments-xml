<collectingCountry id="5CDADC923A4EFFCC302DFA09D9AD2EBF" box="[1096,1270,1412,1437]" name="Western Sahara" pageId="7" pageNumber="300">western Sahara</collectingCountry>
. Vulnerability: LC (
<collectionCode id="42DC04C73A4EFFCC34ACFA24DC482EE2" box="[201,275,1448,1472]" pageId="7" pageNumber="300">IUCN</collectionCode>
), 
<collectionCode id="42DC04C73A4EFFCC3542FA24DC332E9D" box="[295,360,1448,1471]" httpUri="http://grbio.org/cool/b30k-qgwg" name="Musee de Haute Volta" pageId="7" pageNumber="300">MHV</collectionCode>
