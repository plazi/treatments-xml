<collectingCountry id="5CDADC923A4EFFCC3698FA09DE172EBE" box="[765,844,1413,1436]" name="France" pageId="7" pageNumber="300">France</collectingCountry>
, 
<collectingCountry id="5CDADC923A4EFFCC3732FA08DEC12EBF" box="[855,922,1412,1437]" name="Spain" pageId="7" pageNumber="300">Spain</collectingCountry>
, 
<collectingCountry id="5CDADC923A4EFFCC37C0FA09D9562EBE" box="[933,1037,1413,1436]" name="Morocco" pageId="7" pageNumber="300">Morocco</collectingCountry>
