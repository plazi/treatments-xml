</paragraph>
<paragraph id="24729C023A4EFFCC34A2FAB0DCAB2EE2" blockId="7.[151,1437,1303,1472]" pageId="7" pageNumber="300">
<specimenCount id="32CB578B3A4EFFCC34A2FAB0DC392E77" box="[199,354,1340,1365]" pageId="7" pageNumber="300" type="generic">
