<quantity id="E33531E73A4EFFCC3023FAB0D84B2E76" box="[1094,1296,1340,1365]" metricMagnitude="3" metricUnit="m" metricValue="1.5665" metricValueMax="1.683" metricValueMin="1.45" pageId="7" pageNumber="300" unit="m" value="1566.5" valueMax="1683.0" valueMin="1450.0">1,450 to 1,683 m</quantity>
, with males between 
<quantity id="E33531E73A4EFFCC349BFAECDC2A2E55" box="[254,369,1376,1400]" metricMagnitude="-1" metricUnit="m" metricValue="3.5" metricValueMax="4.1" metricValueMin="2.9" pageId="7" pageNumber="300" unit="cm" value="35.0" valueMax="41.0" valueMin="29.0">29–41 cm</quantity>
TL (N= 
<number id="92058E253A4EFFCC35B2FAD3DCBF2E5A" box="[471,484,1375,1400]" pageId="7" pageNumber="300" value="5.0">5</number>
