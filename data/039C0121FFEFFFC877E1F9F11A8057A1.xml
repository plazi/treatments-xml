(
<bibRefCitation id="EFA4CDC6FFEFFFC87220FD061DA25753" author="Ziegler" etAl="et al." firstAuthor="Ziegler" pageId="21" pageNumber="275" pagination="73 - 80" refId="ref20566" refString="ZIEGLER, S., G. NIKOLAUS, and R. HUTTERER. 2002. High mammalian diversity in the newly established Parc National du Haute Niger, Republic of Guinea. Oryx, 36: 73 - 80." type="journal article" year="2002">
