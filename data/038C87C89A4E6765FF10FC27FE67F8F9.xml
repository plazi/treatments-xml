<specimenCount id="9D23FD579A4E6765FC3DFA2FFC24FA29" box="[954,990,1413,1439]" pageId="63" pageNumber="64" type="male">6♂</specimenCount>
<typeStatus id="549E887C9A4E6765FC62FA2FFBAFFA29" box="[997,1109,1413,1439]" pageId="63" pageNumber="64">paratypes</typeStatus>
: 3.07–3.56), ThL = 
<quantity id="4CDD9B3B9A4E6765FABFFA2FFA67FA29" box="[1336,1437,1413,1439]" metricMagnitude="-3" metricUnit="m" metricValue="1.67" pageId="63" pageNumber="64" unit="mm" value="1.67">1.67 mm</quantity>
(1.47–1.67), WL = 
<quantity id="4CDD9B3B9A4E6765FEE9FA03FE2BFA75" box="[366,465,1449,1475]" metricMagnitude="-3" metricUnit="m" metricValue="2.71" pageId="63" pageNumber="64" unit="mm" value="2.71">2.71 mm</quantity>
