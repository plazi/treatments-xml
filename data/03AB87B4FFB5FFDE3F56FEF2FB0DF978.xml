<taxonomicName id="4C024D21FFB7FFDE3F91F9AAFDC0F9A3" box="[463,606,1604,1629]" class="Insecta" family="Colletidae" genus="Chilicola" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="colliguay">
