<taxonomicName id="27DB528BA62AEC1665109DF1B29D2FD0" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="14" pageNumber="15" phylum="Angiospermae" rank="species" species="shawii">
