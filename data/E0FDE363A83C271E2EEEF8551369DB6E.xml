<taxonomicName id="CD7CD584F122D498D2265F5EDD2242CF" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia ehrenbergiana" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="ehrenbergiana">
