<subSubSection id="C15B7AD14FE1D890769629C1B43C01F6" pageId="14" pageNumber="15" type="collecting month and method.">
<paragraph id="675EEF41A9465B245906494334999CA9" pageId="14" pageNumber="15">Collecting month and method.</paragraph>
<paragraph id="D20594D5C38D505D3017153D15F46EE5" pageId="14" pageNumber="15">
