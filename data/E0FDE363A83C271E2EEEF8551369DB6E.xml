<taxonomicName id="12835608B7EF2A1B61B15E8DEB51CCCF" class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="nummularia">
