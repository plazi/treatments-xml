<collectingCountry id="F31EE9A2FFCFFFF8FDE707C7FD2A8CC4" box="[606,679,655,680]" name="Brazil" pageId="20" pageNumber="421">Brazil</collectingCountry>
, 
<collectingCountry id="F31EE9A2FFCFFFF8FD0B07C7FC878CC4" box="[690,778,655,680]" name="Mexico" pageId="20" pageNumber="421">Mexico</collectingCountry>
