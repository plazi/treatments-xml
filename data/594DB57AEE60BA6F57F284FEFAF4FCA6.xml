<collectingRegion id="1320CA8EEE7FBA6F5490873DFB9AFE85" box="[930,1050,290,314]" country="United States of America" name="Arkansas" pageId="16" pageNumber="15">Arkansas</collectingRegion>
, 
<collectingRegion id="1320CA8EEE7FBA6F5315873DFB0AFE85" box="[1063,1162,290,314]" country="United States of America" name="Indiana" pageId="16" pageNumber="15">Indiana</collectingRegion>
