<collectingRegion id="1320CA8EEE7FBA6F53AA873DFA92FE85" box="[1176,1298,290,314]" country="United States of America" name="Kentucky" pageId="16" pageNumber="15">Kentucky</collectingRegion>
, 
<collectingRegion id="1320CA8EEE7FBA6F522C873DFA1BFE85" box="[1310,1435,290,314]" country="United States of America" name="Louisiana" pageId="16" pageNumber="15">Louisiana</collectingRegion>
