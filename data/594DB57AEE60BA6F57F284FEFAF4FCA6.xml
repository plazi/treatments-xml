<collectingRegion id="1320CA8EEE7FBA6F57F2875DFE01FEE5" box="[192,385,322,346]" country="United States of America" name="North Carolina" pageId="16" pageNumber="15">North Carolina</collectingRegion>
, 
<collectingRegion id="1320CA8EEE7FBA6F56BC875DFD91FEE5" box="[398,529,322,346]" country="United States of America" name="Oklahoma" pageId="16" pageNumber="15">Oklahoma</collectingRegion>
