<collectingCountry id="F3087AB88B70FFC59178A082FA3BFE24" box="[1319,1432,476,501]" name="Colombia" pageId="22" pageNumber="23">Colombia</collectingCountry>
, and parts of 
<collectingCountry id="F3087AB88B70FFC5957DA0A1FE22FDC9" box="[290,385,511,536]" name="Ecuador" pageId="22" pageNumber="23">Ecuador</collectingCountry>
, 
<collectingCountry id="F3087AB88B70FFC595D3A35EFE1CFDC6" box="[396,447,512,535]" name="Peru" pageId="22" pageNumber="23">Peru</collectingCountry>
and 
<collectingCountry id="F3087AB88B70FFC595A9A0A1FDEFFDC9" box="[502,588,511,536]" name="Bolivia" pageId="22" pageNumber="23">Bolivia</collectingCountry>
.
