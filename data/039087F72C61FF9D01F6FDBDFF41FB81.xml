<quantity id="4CC19B042C61FF9D036FFD24FD99FD3B" box="[526,574,682,702]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="26" pageNumber="147" unit="m" value="30.0">30 m</quantity>
