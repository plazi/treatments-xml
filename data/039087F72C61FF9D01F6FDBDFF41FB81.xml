<quantity id="4CC19B042C61FF9D0289FDD8FB84FDEF" box="[1000,1059,598,618]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="147" unit="m" value="100.0">100 m</quantity>
