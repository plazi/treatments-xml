<taxonomicName id="6AB857A8B03CFB79FF50433AFE02E360" authority="Nguyen &amp; Sierwald &amp; Marek, 2019" authorityName="Nguyen &amp; Sierwald &amp; Marek" authorityYear="2019" box="[207,442,1321,1343]" class="Diplopoda" family="Glomeridellidae" genus="Tonkinomeris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Glomerida" pageId="5" pageNumber="265" phylum="Arthropoda" rank="species" species="napoensis" status="new species">
