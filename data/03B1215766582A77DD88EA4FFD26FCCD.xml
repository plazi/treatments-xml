<taxonomicNameLabel id="A25FF128665A2A68D94BEA31FB0DF9AA" box="[1108,1191,1539,1560]" pageId="15" pageNumber="50" rank="species">
<emphasis id="B96C4C53665A2A68D94BEA31FBC6F9AA" bold="true" box="[1108,1132,1539,1560]" pageId="15" pageNumber="50">sp</emphasis>
