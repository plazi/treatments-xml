<number id="E2472495E59CFF046C7C0AE919235DC7" box="[1063,1076,1967,1991]" pageId="160" pageNumber="161" value="1.0">1</number>
), F 
<number id="E2472495E59CFF046C070AE9197E5DC7" box="[1116,1129,1967,1991]" pageId="160" pageNumber="161" value="5.0">5</number>
(
<number id="E2472495E59CFF046C220AE9199E5DC7" box="[1145,1161,1967,1991]" pageId="160" pageNumber="161" value="2.0">2</number>
), F 
<number id="E2472495E59CFF046CF50AE919AC5DC7" box="[1198,1211,1967,1991]" pageId="160" pageNumber="161" value="6.0">6</number>
(
<number id="E2472495E59CFF046C970AE919CF5DC7" box="[1228,1240,1967,1991]" pageId="160" pageNumber="161" value="1.0">1</number>
), F 
<number id="E2472495E59CFF046D5B0AF6181A5DC7" box="[1280,1293,1968,1991]" pageId="160" pageNumber="161" value="7.0">7</number>
(
<number id="E2472495E59CFF046D450AE9183D5DC7" box="[1310,1322,1967,1991]" pageId="160" pageNumber="161" value="1.0">1</number>
or 
<number id="E2472495E59CFF046D140AE918485DC7" box="[1359,1375,1967,1991]" pageId="160" pageNumber="161" value="2.0">2</number>
), and F 
<number id="E2472495E59CFF0468F30A941DA25DEA" box="[168,181,2002,2026]" pageId="160" pageNumber="161" value="8.0">8</number>
(
<number id="E2472495E59CFF04689E0A941DC15DEA" box="[197,214,2002,2026]" pageId="160" pageNumber="161" value="2.0">2</number>
); clava with 
<number id="E2472495E59CFF04693D0A941C635DEA" box="[358,372,2002,2026]" pageId="160" pageNumber="161" value="8.0">8</number>
mps, about 
<number id="E2472495E59CFF0469A70A941F375DEA" box="[508,544,2002,2026]" pageId="160" pageNumber="161" value="3.8">3.8</number>
× as long as wide, slightly longer than combined length of F 
<number id="E2472495E59CFF046C990A9419C75DEA" box="[1218,1232,2002,2026]" pageId="160" pageNumber="161" value="6.0">6</number>
–F 
<number id="E2472495E59CFF046CAA0A9419EA5DEA" box="[1265,1277,2002,2026]" pageId="160" pageNumber="161" value="8.0">8</number>
.
</paragraph>
