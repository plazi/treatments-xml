, 
<collectingCounty id="62AACFAFD624FFE1FA9EFF2AFE83FF7F" pageId="27" pageNumber="532">University of Copenhagen</collectingCounty>
(
<collectionCode id="ED652FE6D624FFE1FEAAFF76FE26FF7F" box="[306,390,224,248]" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="27" pageNumber="532">ZMUC</collectionCode>
