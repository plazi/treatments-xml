<location id="8EABE1F8D624FFE1FE5EFF2AFDB1FF52" LSID="urn:lsid:plazi:treatment:03DD0635D625FFE1FF0FFABBFD07FEC7:8EABE1F8D624FFE1FE5EFF2AFDB1FF52" box="[454,529,188,213]" county="University of Copenhagen" municipality="Zoological Museum" name="Whilst" pageId="27" pageNumber="532">Whilst</location>
the 
<typeStatus id="54CF0981D624FFE1FDDCFF2BFDDFFF52" box="[580,639,189,213]" pageId="27" pageNumber="532">types</typeStatus>
of 
<taxonomicName id="4C74CCA0D624FFE1FD30FF2BFCF5FF53" authorityName="Grube" authorityYear="1857" box="[680,853,188,212]" class="Polychaeta" family="Onuphidae" genus="Diopatra" kingdom="Animalia" order="Eunicida" pageId="27" pageNumber="532" phylum="Annelida" rank="species" species="rhizophorae">
