<taxonomicName id="4C74CCA0D624FFE1FA9CFF77FA3CFF7F" authorityName="Grube" authorityYear="1857" box="[1284,1436,224,248]" class="Polychaeta" family="Onuphidae" genus="Diopatra" kingdom="Animalia" order="Eunicida" pageId="27" pageNumber="532" phylum="Annelida" rank="species" species="brevicirris">
<emphasis id="B9006B31D624FFE1FA9CFF77FA3CFF7F" box="[1284,1436,224,248]" italics="true" pageId="27" pageNumber="532">D. brevicirris</emphasis>
