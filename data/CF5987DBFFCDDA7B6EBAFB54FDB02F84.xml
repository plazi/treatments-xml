<taxonomicName id="80F04D4EFFCADA7B6DD3F991FA632E4A" authority="Martins and Galileo, 1990" authorityName="Martins and Galileo" authorityYear="1990" box="[937,1436,1583,1607]" class="Insecta" family="Cerambycidae" genus="Hesychotypa" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="ableptema">
<emphasis id="7584EADFFFCADA7B6DD3F991FBB22E4A" box="[937,1101,1583,1607]" italics="true" pageId="4" pageNumber="3">H. ableptema</emphasis>
Martins and Galileo, 1990
</taxonomicName>
, 
<taxonomicName id="80F04D4EFFCADA7B6EBAF9EEFE032E6A" authority="(Bates, 1865)" baseAuthorityName="Bates" baseAuthorityYear="1865" box="[192,508,1615,1639]" class="Insecta" family="Cerambycidae" genus="Hesychotypa" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="jaspidea">
<emphasis id="7584EADFFFCADA7B6EBAF9EEFEB02E6A" box="[192,335,1615,1639]" italics="true" pageId="4" pageNumber="3">H. jaspidea</emphasis>
(Bates, 1865)
</taxonomicName>
and 
