<taxonomicName id="4C544D2FFFB12E766551F9A7D14ACC00" authority="Corbera, 2006" authorityName="Corbera" authorityYear="2006" box="[313,773,1566,1591]" class="Malacostraca" family="Lampropidae" genus="Misceolamprops" kingdom="Animalia" order="Cumacea" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="dolorsae">
