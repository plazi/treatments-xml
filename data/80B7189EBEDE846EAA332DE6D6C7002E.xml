<paragraph id="18E6B2CA7D2B6CFD8BC5AF1938948290" pageId="99" pageNumber="100">
<taxonomicName id="96FAF3E80A52335B233B8867501B6B27" class="Gastropoda" family="Trochomorphidae" genus="Geotrochus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Geotrochus" order="Stylommatophora" pageId="99" pageNumber="100" phylum="Mollusca" rank="genus">Geotrochus</taxonomicName>
