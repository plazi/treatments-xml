<taxonomicName id="4C7BA1B7FF880F455AE5F90EFD7EF881" authority="Kethley, 1970" authorityName="Kethley" authorityYear="1970" box="[415,688,1787,1810]" class="Arachnida" family="Syringophilidae" genus="Niglarobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="ereuneti">
