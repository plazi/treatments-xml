<taxonomicName id="10F5AB85FF8DB911FB7BFB23FF17FB11" authority="Boulenger 1885" authorityName="Boulenger" authorityYear="1885" class="Reptilia" family="Gekkonidae" genus="Geckoella" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="344" phylum="Chordata" rank="species" species="speciosus">
<emphasis id="E5810C14FF8DB911FB7BFB23FAE1FB33" box="[1188,1315,1197,1219]" italics="true" pageId="5" pageNumber="344">C. speciosus</emphasis>
