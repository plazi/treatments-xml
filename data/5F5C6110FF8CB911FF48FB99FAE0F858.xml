<taxonomicName id="10F5AB85FF8DB911FE6AFBE1FD0FFB75" box="[437,717,1135,1157]" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="344" phylum="Chordata" rank="species" species="collegalensis">
