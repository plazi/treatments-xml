<subSubSection id="27B46539FFBEE155FF0DE365CB268A05" box="[199,1113,548,572]" pageId="51" pageNumber="52" type="etymology">
<paragraph id="6F1136B2FFBEE155FF0DE365CB268A05" blockId="51.[151,1437,151,679]" box="[199,1113,548,572]" pageId="51" pageNumber="52">
<emphasis id="5DDAEAA0FFBEE155FF0DE365CE328A05" bold="true" box="[199,333,548,572]" inLineHeading="true" pageId="51" pageNumber="52" reason="1">Etymology.</emphasis>
