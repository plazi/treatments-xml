<taxonomicName id="16658F1687B0E2ABD6FE7FA18924C452" class="Arachnida" family="Selenopidae" genus="Karaops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Karaops manaayn" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="manaayn">Karaops manaayn</taxonomicName>
