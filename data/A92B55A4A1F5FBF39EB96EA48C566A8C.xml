<taxonomicName id="74791DB7038122580E6B60AB8F67AD7C" class="Arachnida" family="Selenopidae" genus="Anyphops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anyphops barnardi" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="barnardi">Anyphops barnardi</taxonomicName>
