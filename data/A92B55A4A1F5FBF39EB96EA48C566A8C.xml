<taxonomicName id="B0D0A2520B4C07A39103DFED68B9DFFE" class="Arachnida" family="Selenopidae" genus="Karaops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Karaops raveni" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="raveni">Karaops raveni</taxonomicName>
