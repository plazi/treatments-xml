<taxonomicName id="AAE63E98B0C42AF7BDF4F66A0904A70D" class="Arachnida" family="Selenopidae" genus="Karaops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Karaops badgeradda" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="badgeradda">Karaops badgeradda</taxonomicName>
