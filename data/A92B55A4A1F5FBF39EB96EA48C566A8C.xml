<taxonomicName id="36A823D46FEE1FD7A75E3E70126A63D3" class="Arachnida" family="Selenopidae" genus="Karaops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Karaops dawara" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="dawara">Karaops dawara</taxonomicName>
