<taxonomicName id="D1ED3EF1E863F5D39FDF89243A782537" class="Arachnida" family="Selenopidae" genus="Karaops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Karaops toolbrunup" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="toolbrunup">Karaops toolbrunup</taxonomicName>
