<emphasis id="B925EA8D8476FFF8FF5C2C47AE1E9FA4" bold="true" box="[199,347,1902,1928]" pageId="31" pageNumber="482">Dimensions:</emphasis>
<collectingRegion id="4995F87D8476FFF8FEF92C47AED99FA4" box="[354,412,1902,1928]" country="Maldives" name="Maale" pageId="31" pageNumber="482">Male</collectingRegion>
specimen 3960M, LVL 
<quantity id="4CA99B7A8476FFF8FD372C47AC659FA5" box="[684,800,1902,1929]" metricMagnitude="-3" metricUnit="m" metricValue="1.008" pageId="31" pageNumber="482" unit="mm" value="1.008">1.008 mm</quantity>
