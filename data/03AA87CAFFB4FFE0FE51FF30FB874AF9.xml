<subSubSection id="C3196557FFB4FFE0FFAEFD72FB874AF9" pageId="95" pageNumber="94" type="description">
<paragraph id="8BBC36DCFFB4FFE0FFAEFD72FE424B1A" blockId="95.[113,666,276,774]" box="[113,417,753,774]" pageId="95" pageNumber="94">
<emphasis id="B977EACEFFB4FFE0FFAEFD72FEC04B19" bold="true" box="[113,291,753,773]" pageId="95" pageNumber="94">Species group:</emphasis>
