<taxonomicName id="9182416CA1B7778FBB12CBBA666FF3A5" class="Insecta" family="Braconidae" genus="Doryctobracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Doryctobracon" order="Hymenoptera" pageId="8" pageNumber="19" phylum="Arthropoda" rank="genus">Doryctobracon</taxonomicName>
