<paragraph id="041ECC050704FFCFFF16F88BFB76FDCB" blockId="4.[151,1437,1460,2009]" lastBlockId="6.[151,1437,152,514]" lastPageId="6" lastPageNumber="54" pageId="4" pageNumber="52">
<emphasis id="36D510170704FFCDFF16F88BFEA8F88A" bold="true" box="[197,324,1833,1859]" pageId="4" pageNumber="52">Diagnosis.</emphasis>
