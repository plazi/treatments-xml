<taxonomicName id="C3A1B7860704FFCDFBBBF888FB17F88A" box="[1128,1275,1834,1859]" class="Insecta" family="Cicadidae" genus="Fidicinoides" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="52" phylum="Arthropoda" rank="genus">
