<taxonomicName id="C3A1B7860704FFCDFD1FF8EDFB6CF8A0" authority="Santos &amp; Martinelli" authorityName="Santos &amp; Martinelli" box="[716,1152,1871,1897]" class="Insecta" family="Cicadidae" genus="Fidicinoides" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="52" phylum="Arthropoda" rank="species" species="rosabasalae">
