<paragraph id="0B7DBD25814F9CAF1FB4CAA245FDB3FF" pageId="null" pageNumber="38">
<taxonomicName id="1890E4CE0EBFD62D7829A107CFC5ED0E" authority="L." class="Magnoliopsida" family="Ranunculaceae" genus="Helleborus" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="38" phylum="Tracheophyta" rank="species" species="niger">
