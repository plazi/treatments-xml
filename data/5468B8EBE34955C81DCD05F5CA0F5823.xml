<taxonomicName id="9601CC2FB8FE51D2E6A1F38FD561A0A5" authority="in Systema Naturae" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="species" species="ornithopodioides">
<emphasis id="28F164C0A81E1624131333804CC274AE" italics="true" pageNumber="898">T. ornithopodioides</emphasis>
