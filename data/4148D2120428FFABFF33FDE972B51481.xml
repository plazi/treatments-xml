cruise, sta 40/367: 42°55”N, 30°20”W, 
<quantity id="0E19CEE10428FFABFB47FBFD7382147C" box="[1251,1424,1072,1099]" metricMagnitude="0" metricUnit="m" metricValue="2.961" metricValueMax="2.968" metricValueMin="2.954" pageId="120" pageNumber="121" unit="m" value="2.961" valueMax="2.968" valueMin="2.954">
