Mayr, 
<taxonomicName id="62658CDEB84936415BD8D89E969CF04B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34471" class="Insecta" family="Formicidae" genus="Polyrhachis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polyrhachis ceramensis Mayr" lsidName-HNS="Polyrhachis ceramensis Mayr" order="Hymenoptera" pageNumber="67" phylum="Arthropoda" rank="species" species="ceramensis">ceramensis</taxonomicName>
Mayr et 
<taxonomicName id="DCBA1F16CBFBA8188C98F52C8EC3D073" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143011" class="Insecta" family="Formicidae" genus="Polyrhachis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polyrhachis orsyllus r. halmaheirae Forel" lsidName-HNS="Polyrhachis orsyllus r. halmaheirae Forel" order="Hymenoptera" pageNumber="67" phylum="Arthropoda" rank="species" species="halmaheirae">Halmaheirae</taxonomicName>
