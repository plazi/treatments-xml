. 
<taxonomicName id="4C744D5DFF87730B1968FB29FCC393E6" authority="Le Souef, 1924" authorityName="Le Souef" authorityYear="1924" box="[346,790,1218,1242]" class="Mammalia" family="Macropodidae" genus="Petrogale" kingdom="Animalia" order="Diprotodontia" pageId="62" pageNumber="339" phylum="Chordata" rank="species" species="purpureicollis">
