<geoCoordinate id="EE405019FF87730B1AF6FEEBFCCC9624" box="[708,793,256,280]" degrees="32" direction="south" minutes="48" orientation="latitude" pageId="62" pageNumber="339" precision="925" value="-32.8">32°48'S</geoCoordinate>
