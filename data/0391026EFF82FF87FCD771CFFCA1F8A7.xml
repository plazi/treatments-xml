<taxonomicName id="4C38C8FBFF82FF80FB197235FC87FCE7" baseAuthorityName="Pace" baseAuthorityYear="1998" class="Insecta" family="Staphylinidae" genus="Anaulacaspis" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="244" phylum="Arthropoda" rank="species" species="beijingensis">
<emphasis id="B94C6F6AFF82FF80FB197235FC87FCE7" italics="true" pageId="43" pageNumber="244">A. beijingensis</emphasis>
</taxonomicName>
