<subSubSection id="C34E6527FFFD2E3A64AFFD4CD311C94E" pageId="17" pageNumber="18" type="diagnosis">
<paragraph id="8BEB36ACFFFD2E3A64AFFD4CD741C961" blockId="17.[151,1437,685,1322]" pageId="17" pageNumber="18">
<emphasis id="B920EABEFFFD2E3A64AFFD4CD375C939" bold="true" box="[199,314,757,782]" pageId="17" pageNumber="18">Diagnosis</emphasis>
