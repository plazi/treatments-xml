4.2.4. 
<taxonomicName id="33A9501FB54A7C65111E59CA59693305" LSID="http://zoobank.org/F148AFA0-7C11-44F0-8A8A-4FECEB5F22A7" authority="Grebennikov, 2021" authorityName="Grebennikov" authorityYear="2021" class="Insecta" family="Curculionidae" genus="Typoderus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Typoderus clytius" order="Coleoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="species" species="clytius" status="sp. nov.">Typoderus clytius</taxonomicName>
