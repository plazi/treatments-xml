<taxonomicName id="4C58A0B51E05FFB8FC59A69CFB1BD8AA" authority="STAUDINGER" authorityName="Staudinger" authorityYear="1892" box="[963,1270,677,701]" class="Insecta" family="Geometridae" genus="Gnopharmia" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="283" phylum="Arthropoda" rank="genus">
