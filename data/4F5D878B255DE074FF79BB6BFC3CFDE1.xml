<taxonomicName id="00F44D1E255DE074FD60BBFBFCDBFE51" box="[654,822,441,467]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
