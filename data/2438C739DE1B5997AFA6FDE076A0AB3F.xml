<taxonomicName id="386CA7640B027E3A94431BB16EC8968D" authorityName="Dejean" authorityYear="1831" class="Hexapoda" family="Carabidae" genus="Cymindis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="801" pageNumber="802" phylum="Arthropoda" rank="species" species="pustulata">
