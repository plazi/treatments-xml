<taxonomicName id="4C4D4D23FFB2FFD1D5CB9117FE0EFDD9" box="[151,391,565,591]" class="Reptilia" family="Gekkonidae" genus="Phelsuma" kingdom="Animalia" order="Squamata" pageId="23" pageNumber="24" phylum="Chordata" rank="species" species="standingi">
<emphasis id="B939EAB2FFB2FFD1D5CB9117FE0EFDD9" bold="true" box="[151,391,565,591]" italics="true" pageId="23" pageNumber="24">Phelsuma standingi</emphasis>
