<taxonomicName id="4C654D20FFA15661FD9721AC6F1F8497" authority="Tanasevitch, 2020" authorityName="Tanasevitch" authorityYear="2020" box="[571,842,1428,1449]" class="Arachnida" family="Linyphiidae" genus="Pseudomicrocentria" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="476" phylum="Arthropoda" rank="species" species="uncata" status="new species">
