. 
<emphasis id="DCE9EAD2FFA8FFD2FB53F86DFB0DCBF1" box="[1158,1252,1936,1959]" italics="true" pageId="3" pageNumber="60">xakriaba</emphasis>
</taxonomicName>
, 
<taxonomicName id="299D4D43FFA8FFD2FB3AF86DFAB5CBF1" authorityName="Regan" authorityYear="1909" box="[1263,1372,1936,1959]" class="Actinopterygii" family="Loricariidae" genus="Otocinclus" kingdom="Animalia" order="Siluriformes" pageId="3" pageNumber="60" phylum="Chordata" rank="species" species="arnoldi">
