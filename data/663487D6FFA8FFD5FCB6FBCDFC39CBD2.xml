<emphasis id="DCE9EAD2FFA8FFD2FADBF86DFAB5CBF1" box="[1294,1372,1936,1959]" italics="true" pageId="3" pageNumber="60">arnoldi</emphasis>
</taxonomicName>
, and 
<taxonomicName id="299D4D43FFA8FFD5FA47F86DFF24CB12" authorityName="Cope" authorityYear="1894" class="Actinopterygii" family="Loricariidae" genus="Otocinclus" kingdom="Animalia" lastPageId="4" lastPageNumber="61" order="Siluriformes" pageId="3" pageNumber="60" phylum="Chordata" rank="species" species="flexilis">
<emphasis id="DCE9EAD2FFA8FFD2FA47F86DFA4CCBF1" box="[1426,1445,1936,1959]" italics="true" pageId="3" pageNumber="60">O</emphasis>
