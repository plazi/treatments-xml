<paragraph id="8BA76A5A8B144E204E88FA953FBEFA7D" blockId="57.[189,1398,1385,1447]" pageId="57" pageNumber="58">
<taxonomicName id="4C1811D98B144E204E88FA953DB0FA58" authorityName="Nurinsiyah &amp; Neiber &amp; Hausdorf" authorityYear="2019" baseAuthorityName="Gude" baseAuthorityYear="1905" box="[189,445,1385,1411]" class="Gastropoda" family="Camaenidae" genus="Landouria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="57" pageNumber="58" phylum="Mollusca" rank="species" species="leucochila">
