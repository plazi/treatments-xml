<taxonomicName id="4C1811D98B144E204CC9F8BF3F75F886" authorityName="Godwin-Austen" authorityYear="1918" box="[764,888,1859,1885]" class="Gastropoda" family="Camaenidae" genus="Landouria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="57" pageNumber="58" phylum="Mollusca" rank="genus">
