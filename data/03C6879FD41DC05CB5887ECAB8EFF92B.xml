<taxonomicName id="4C6F4D0AD41DC05CB41E7ECDBABEFA79" authority="Zeledon and Ponce" authorityName="Zeledon and Ponce" box="[257,645,1443,1470]" class="Insecta" family="Reduviidae" genus="Triatoma" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="ryckmani">
<emphasis id="B91BEA9BD41DC05CB41E7ECDB9B6FA7A" bold="true" box="[257,397,1444,1470]" italics="true" pageId="19" pageNumber="20">T. ryckmani</emphasis>
