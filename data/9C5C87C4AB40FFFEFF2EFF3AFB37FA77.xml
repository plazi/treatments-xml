<caption id="408A665AAB40FFFFFF2EF894FC68F808" ID-DOI="http://doi.org/10.5281/zenodo.3944041" ID-Zenodo-Dep="3944041" httpUri="https://zenodo.org/record/3944041/files/figure.png" pageId="14" pageNumber="15" startId="14.[151,250,1847,1872]" targetBox="[151,1436,370,1821]" targetPageId="14">
<paragraph id="144A36D2AB40FFFFFF2EF894FC68F808" blockId="14.[151,1437,1847,1944]" pageId="14" pageNumber="15">
