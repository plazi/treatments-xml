species:
</emphasis>
<taxonomicName id="4C7E4D310D08FFDEFE84696CFCB2F8E1" authority="Redtenbacher, 1906" authorityName="Redtenbacher" authorityYear="1906" box="[364,848,1855,1881]" class="Insecta" family="Pseudophasmatidae" genus="Pachyphloea" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="547" phylum="Arthropoda" rank="species" species="aberrans">
