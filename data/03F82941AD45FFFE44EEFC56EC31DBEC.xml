<subSubSection id="C34BCBDCAD45FFFE44EEFB90E813DB79" pageId="8" pageNumber="9" type="diagnosis">
<paragraph id="8BEE9857AD45FFFE44EEFB90E813DB79" blockId="8.[151,1436,1079,1248]" pageId="8" pageNumber="9">
<emphasis id="B9254445AD45FFFE44EEFB90E857DB5C" bold="true" box="[151,266,1079,1104]" pageId="8" pageNumber="9">Diagnosis</emphasis>
