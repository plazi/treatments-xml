<taxonomicName id="5EF118B6FFA2FFD0FCFCFA2AFC75664D" box="[813,950,1516,1538]" class="Insecta" family="Staphylinidae" genus="Phloeocharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="brachyptera">
