<taxonomicName id="5EF118B6FFA2FFD0FDE8FA2AFD7E664D" authorityName="Assing" authorityYear="2015" box="[569,701,1516,1538]" class="Insecta" family="Staphylinidae" genus="Phloeocharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="gredosensis">
<emphasis id="AB85BF27FFA2FFD0FDE8FA2AFD7E664D" box="[569,701,1516,1538]" italics="true" pageId="3" pageNumber="60">P. gredosensis</emphasis>
</taxonomicName>
