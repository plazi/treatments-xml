<quantity id="5E09CED0FFA2FFD0FC8EF9F7FC646609" box="[863,935,1584,1605]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="3" pageNumber="60" unit="mm" value="0.1">0.1 mm</quantity>
