<quantity id="4CDC9B75FFB7FF9AFC50EA0EFBCE8B07" box="[950,1039,613,637]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" pageId="72" pageNumber="73" unit="mm" value="5.5">5.5 mm</quantity>
