<taxonomicName id="16D34D1BFFAEFF82FE10F5FF85AFF1FE" box="[481,672,727,753]" class="Insecta" family="Cicadellidae" genus="Hecaloidella" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="399" phylum="Arthropoda" rank="species" species="brunnifacialis">
