<taxonomicName id="CE2CAC03079B49E86793423DFC1CC4CD" authorityName="Costa &amp; Katz" authorityYear="2022" class="Pisces" family="Trichomycteridae" genus="Trichomycterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trichomycterus largoperculatus" order="Siluriformes" pageId="0" pageNumber="13" phylum="Chordata" rank="species" species="largoperculatus">
<emphasis id="22BB1CB77BD311FC82A5005E11663C0C" italics="true" pageId="0" pageNumber="13">Trichomycterus largoperculatus</emphasis>
</taxonomicName>
