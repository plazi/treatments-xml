<taxonomicName id="4C3715E5FFA0A17EFF66DB871261FF11" authority="Rowe et al., 1986: 251" authorityName="Rowe et al." authorityPageNumber="251" authorityYear="1986" box="[151,606,182,205]" class="Crinoidea" family="Comasteridae" genus="Oxycomanthus" kingdom="Animalia" order="Comatulida" pageId="30" pageNumber="181" phylum="Echinodermata" rank="species" species="exilis">
