<taxonomicName id="F809FD86F6D49836D41DC0D260F5C1CC" authorityName="Braun-Blanquet &amp; Maire, Bull. Soc. Hist. Nat. Afrique N. 13: 18. 1922. (Braun-Blanquet and Maire" authorityYear="1922" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus glaouorum" order="Solanales" pageId="63" pageNumber="64" phylum="Tracheophyta" rank="species" species="glaouorum">
