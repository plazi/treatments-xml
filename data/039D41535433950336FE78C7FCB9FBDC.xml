<taxonomicName id="4C348BC654329503375778B8FDABFF1A" box="[318,624,152,178]" class="Chilopoda" family="Mecistocephalidae" genus="Proterotaiwanella" kingdom="Animalia" order="Geophilomorpha" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="undetermined">
