<quantity id="4CCC5DA054329503333E78B8FA4DFF1A" box="[1367,1430,152,178]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="65" pageNumber="66" unit="cm" value="2.0">2 cm</quantity>
