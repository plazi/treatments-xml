, HW = 
<quantity box="[343,431,1071,1092]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="7" unit="mm" value="0.15">0.15 mm</quantity>
; PL = 
<quantity box="[501,590,1071,1092]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="7" unit="mm" value="0.13">0.13 mm</quantity>
, PW = 
<quantity box="[173,261,1097,1118]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="7" unit="mm" value="0.2">0.20 mm</quantity>
; EL = 
<quantity box="[332,421,1097,1118]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" pageId="7" unit="mm" value="0.25">0.25 mm</quantity>
, EW = 
<quantity box="[500,588,1097,1118]" metricMagnitude="-4" metricUnit="m" metricValue="3.1" pageId="7" unit="mm" value="0.31">0.31 mm</quantity>
; A1–6, respectively, = 0.04, 0.08, 0.06, 0.07, 0.09, 
<quantity box="[570,656,1124,1145]" metricMagnitude="-5" metricUnit="m" metricValue="3.0" pageId="7" unit="mm" value="0.03">0.03 mm</quantity>
; An1–11, respectively, = 0.02, 0.02, 0.01, 0.01, 0.01, 0.01, 0.01, 0.01, 0.02, 0.02, 
<quantity box="[425,508,1177,1198]" metricMagnitude="-5" metricUnit="m" metricValue="6.0" pageId="7" unit="mm" value="0.06">0.06 mm</quantity>
