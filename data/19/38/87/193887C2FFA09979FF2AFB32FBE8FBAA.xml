; PL = 
<quantity box="[1035,1124,354,375]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="5" unit="mm" value="0.15">0.15 mm</quantity>
, PW = 
<quantity box="[706,795,380,401]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="5" unit="mm" value="0.2">0.20 mm</quantity>
; EL = 
<quantity box="[866,955,380,401]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="5" unit="mm" value="0.3">0.30 mm</quantity>
, EW = 
<quantity box="[1034,1122,380,401]" metricMagnitude="-4" metricUnit="m" metricValue="2.8" pageId="5" unit="mm" value="0.28">0.28 mm</quantity>
; A1–6, respectively, = 0.06, 0.09, 0.09, 0.07, 0.06, 
<quantity box="[1104,1189,407,428]" metricMagnitude="-5" metricUnit="m" metricValue="3.0" pageId="5" unit="mm" value="0.03">0.03 mm</quantity>
; An1–11, respectively, = 0.04, 0.03, 0.01, 0.01, 0.01, 0.01, 0.01, 0.01, 0.02, 0.02, 
<quantity box="[1022,1108,460,481]" metricMagnitude="-5" metricUnit="m" metricValue="6.0" pageId="5" unit="mm" value="0.06">0.06 mm</quantity>
; MP1–4, respectively, = 0.01, 0.03, 0.02, 
<quantity box="[1031,1122,486,507]" metricMagnitude="-5" metricUnit="m" metricValue="2.0" pageId="5" unit="mm" value="0.02">0.02 mm</quantity>
; GL = 
<quantity box="[706,793,513,534]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" pageId="5" unit="mm" value="0.41">0.41 mm</quantity>
, GW = 
<quantity box="[873,960,513,534]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" pageId="5" unit="mm" value="0.25">0.25 mm</quantity>
