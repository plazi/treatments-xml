<heading bold="true" box="[151,528,151,178]" fontSize="11" level="1" pageId="33" pageNumber="34" reason="1">
<emphasis bold="true" box="[151,528,151,178]" pageId="33" pageNumber="34">
<taxonomicName authority="Mah, 2018" authorityName="Mah" authorityYear="2018" box="[151,455,151,177]" class="Asteroidea" family="Goniasteridae" genus="Eknomiaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Valvatida" pageId="33" pageNumber="34" phylum="Echinodermata" rank="species" species="horologium" status="n. sp.">
<emphasis bold="true" box="[151,455,151,177]" italics="true" pageId="33" pageNumber="34">Eknomiaster horologium</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[462,528,152,178]" pageId="33" pageNumber="34" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
