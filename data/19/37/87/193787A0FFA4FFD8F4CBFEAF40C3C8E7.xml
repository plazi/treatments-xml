<paragraph blockId="70.[151,444,367,427]" box="[151,444,367,394]" pageId="70" pageNumber="71">
<heading bold="true" box="[151,444,367,394]" fontSize="11" level="1" pageId="70" pageNumber="71" reason="1">
<emphasis bold="true" box="[151,444,367,394]" pageId="70" pageNumber="71">
<taxonomicName authority="Mah, 2018" authorityName="Mah" authorityYear="2018" box="[151,370,367,393]" class="Asteroidea" family="Goniasteridae" genus="Pergamaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Valvatida" pageId="70" pageNumber="71" phylum="Echinodermata" rank="species" species="ultra" status="n. sp.">
<emphasis bold="true" box="[151,370,367,393]" italics="true" pageId="70" pageNumber="71">Pergamaster ultra</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[377,444,368,394]" pageId="70" pageNumber="71" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
