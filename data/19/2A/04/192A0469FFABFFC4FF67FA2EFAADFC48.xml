<number box="[749,821,367,392]" pageId="6" value="89.95">
89, 
<number box="[790,821,367,392]" pageId="6" value="95.143">95</number>
</number>
, 143; 
<number box="[889,948,367,392]" pageId="6" value="1891.0">1891</number>
b: p. 
<number box="[1002,1046,367,392]" pageId="6" value="350.0">350</number>
) [= 
<taxonomicName authority="Gistel, 1848" authorityName="Gistel" authorityYear="1848" box="[1092,1393,367,392]" class="Holothuroidea" family="Psolidae" genus="Ceto" kingdom="Animalia" order="Dendrochirotida" pageId="6" phylum="Echinodermata" rank="species" species="cuvieria">
