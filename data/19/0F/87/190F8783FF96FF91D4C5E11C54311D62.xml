<emphasis box="[386,628,1707,1729]" italics="true" pageId="75" pageNumber="76">Holopothrips porrosati</emphasis>
</taxonomicName>
and 
<taxonomicName box="[693,816,1708,1729]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="signatus">
<emphasis box="[693,816,1708,1729]" italics="true" pageId="75" pageNumber="76">H. signatus</emphasis>
</taxonomicName>
. (221) 
<taxonomicName box="[905,1148,1707,1729]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="porrosati">
<emphasis box="[905,1148,1707,1729]" italics="true" pageId="75" pageNumber="76">Holopothrips porrosati</emphasis>
</taxonomicName>
paratype, body; 222–225. 
<taxonomicName box="[151,378,1737,1759]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="signatus">
<emphasis box="[151,378,1737,1759]" italics="true" pageId="75" pageNumber="76">Holopothrips signatus</emphasis>
</taxonomicName>
