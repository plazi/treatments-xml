<taxonomicName authority="(Nyl.) Hafellner" authorityName="Hafellner" authorityYear="1997" baseAuthorityName="Nyl." box="[151,629,151,177]" class="Lecanoromycetes" family="Brigantiaeaceae" genus="Brigantiaea" higherTaxonomySource="CoL" kingdom="Fungi" order="Teloschistales" pageId="3" pageNumber="38" phylum="Ascomycota" rank="species" species="phaeomma">
