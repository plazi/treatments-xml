<subSubSection box="[151,1061,1519,1543]" pageId="7" pageNumber="42" type="etymology">
<paragraph blockId="7.[151,1436,1423,1543]" box="[151,1061,1519,1543]" pageId="7" pageNumber="42">
<emphasis bold="true" box="[151,274,1519,1543]" pageId="7" pageNumber="42">Etymology</emphasis>
