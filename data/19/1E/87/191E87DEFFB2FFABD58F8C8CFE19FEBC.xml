<quantity box="[181,302,259,285]" metricMagnitude="1" metricUnit="m" metricValue="5.6" metricValueMax="10.0" metricValueMin="1.2" pageId="14" pageNumber="15" unit="m" value="56.0" valueMax="100.0" valueMin="12.0">
