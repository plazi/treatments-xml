SE of San Isidro del General, under bridge on road San Isidro-Dominical, 
<collectingRegion box="[151,249,1663,1689]" country="Costa Rica" name="San Jose" pageId="8" pageNumber="9">San José</collectingRegion>
(
<geoCoordinate box="[264,374,1662,1689]" degrees="09.35264" direction="north" orientation="latitude" pageId="8" pageNumber="9" precision="1" value="9.35264">09.35264</geoCoordinate>
, 
<geoCoordinate box="[385,504,1663,1689]" degrees="83.72790" direction="west" orientation="longitude" pageId="8" pageNumber="9" precision="1" value="-83.7279">-83.72790</geoCoordinate>
), at 
<specimenCount box="[551,634,1663,1689]" pageId="8" pageNumber="9" type="generic">
<quantity box="[551,630,1663,1689]" metricMagnitude="2" metricUnit="m" metricValue="6.1899999999999995" pageId="8" pageNumber="9" unit="m" value="619.0">619 m</quantity>
.
</specimenCount>
a.s.l., on 
<date box="[732,911,1662,1689]" pageId="8" pageNumber="9" value="2013-03-25">March 25, 2013</date>
