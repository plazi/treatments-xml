<quantity box="[180,287,510,537]" metricMagnitude="2" metricUnit="m" metricValue="2.7" metricValueMax="5.4" metricValueMin="0.0" pageId="15" pageNumber="16" unit="m" value="270.0" valueMax="540.0" valueMin="0.0">
