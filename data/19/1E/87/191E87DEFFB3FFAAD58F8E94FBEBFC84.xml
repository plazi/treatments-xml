<specimenCount box="[1158,1226,799,825]" pageId="15" pageNumber="16" type="generic">
<quantity box="[1158,1221,799,825]" metricMagnitude="1" metricUnit="m" metricValue="2.6" pageId="15" pageNumber="16" unit="m" value="26.0">26 m</quantity>
