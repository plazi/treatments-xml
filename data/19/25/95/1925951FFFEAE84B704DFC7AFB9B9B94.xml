<paragraph blockId="3.[138,509,1119,1143]" box="[138,509,1119,1143]" pageId="3" pageNumber="309">
St. 
<quantity box="[179,329,1119,1143]" metricMagnitude="6" metricUnit="m" metricValue="6.08917" pageId="3" pageNumber="309" unit="m" value="6089170.0">6089, 170 m</quantity>
, 
