<paragraph blockId="5.[142,781,1206,1841]" pageId="5" pageNumber="5">
<location LSID="urn:lsid:plazi:treatment:195169142273FF88FF1344BB6D3CFB30:94278ED92273FF80FF1346796915F9BC" box="[142,321,1586,1607]" country="Poland" municipality="Ozimek" name="Krasiejow clay pit" pageId="5" pageNumber="5" stateProvince="Upper Silesia">Krasiejów clay pit</location>
, near 
<collectingMunicipality box="[379,459,1586,1607]" pageId="5" pageNumber="5">Ozimek</collectingMunicipality>
, 
<collectingRegion box="[468,605,1586,1607]" pageId="5" pageNumber="5">Upper Silesia</collectingRegion>
