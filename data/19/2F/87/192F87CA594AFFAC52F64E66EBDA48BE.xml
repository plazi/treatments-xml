<subSubSection pageId="3" pageNumber="161" type="nomenclature">
<paragraph blockId="3.[151,734,1394,1420]" box="[151,734,1394,1420]" pageId="3" pageNumber="161">
<heading bold="true" box="[151,734,1394,1420]" fontSize="11" level="1" pageId="3" pageNumber="161" reason="1">
<emphasis bold="true" box="[151,734,1394,1420]" pageId="3" pageNumber="161">
Genus 
<taxonomicName authority="Storozhenko" authorityName="Storozhenko" box="[238,612,1394,1420]" class="Insecta" family="Tetrigidae" genus="Tripetaloceroides" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="161" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,446,1394,1420]" italics="true" pageId="3" pageNumber="161">Tripetaloceroides</emphasis>
Storozhenko
</taxonomicName>
, gen. nov.
</emphasis>
</heading>
</paragraph>
<paragraph blockId="3.[151,881,1465,1487]" box="[151,881,1465,1487]" pageId="3" pageNumber="161">
<typeStatus box="[151,201,1466,1487]" pageId="3" pageNumber="161">Type</typeStatus>
species: 
<taxonomicName authority="Gunther, 1938" authorityName="Gunther" authorityYear="1938" box="[294,705,1465,1487]" class="Insecta" family="Tetrigidae" genus="Tripetalocera" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="161" phylum="Arthropoda" rank="species" species="tonkinensis">
<emphasis box="[294,552,1465,1487]" italics="true" pageId="3" pageNumber="161">Tripetalocera tonkinensis</emphasis>
