<collectingCountry box="[465,744,1620,1644]" name="Bosnia and Herzegovina" pageId="12" pageNumber="413">Bosnia and Herzegovina</collectingCountry>
, 
<collectingCountry box="[755,840,1620,1644]" name="Croatia" pageId="12" pageNumber="413">Croatia</collectingCountry>
, 
<collectingCountry box="[851,903,1620,1644]" name="Italy" pageId="12" pageNumber="413">Italy</collectingCountry>
, 
<collectingCountry box="[915,1055,1621,1645]" name="Serbia" pageId="12" pageNumber="413">Montenegro</collectingCountry>
, 
