22. 
<taxonomicName authority="(Marschall, 1836)" baseAuthorityName="Marschall" baseAuthorityYear="1836" box="[195,696,1021,1047]" class="Insecta" family="Baissogryllidae" genus="Tropidopola" kingdom="Animalia" order="Orthoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="cylindrica">
