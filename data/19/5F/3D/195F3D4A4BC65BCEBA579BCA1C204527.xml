</taxonomicName>
Grunow, 
<taxonomicName genus="C." higherTaxonomySource="GBIF,CoL" kingdom="Chromista" lsidName="C. fasciculatus" pageId="0" pageNumber="31" phylum="Ochrophyta" rank="species" species="fasciculatus">
