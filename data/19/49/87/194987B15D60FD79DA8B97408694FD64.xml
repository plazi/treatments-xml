<quantity metricMagnitude="-2" metricUnit="m" metricValue="5.715" metricValueMax="5.842" metricValueMin="5.588" pageId="29" pageNumber="184" unit="in" value="2.25" valueMax="2.3" valueMin="2.2">2.2–2.3 in</quantity>
