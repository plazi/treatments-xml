<quantity box="[458,555,271,293]" metricMagnitude="-2" metricUnit="m" metricValue="7.111999999999999" metricValueMax="9.906" metricValueMin="4.318" pageId="29" pageNumber="184" unit="in" value="2.8" valueMax="3.9" valueMin="1.7">1.7–3.9 in</quantity>
