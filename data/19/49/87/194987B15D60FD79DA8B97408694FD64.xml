<quantity box="[478,587,241,263]" metricMagnitude="-1" metricUnit="m" metricValue="2.5781" metricValueMax="3.175" metricValueMin="1.9812" pageId="29" pageNumber="184" unit="in" value="10.15" valueMax="12.5" valueMin="7.8">7.8–12.5 in</quantity>
