<taxonomicName baseAuthorityName="Regan" baseAuthorityYear="1908" box="[1302,1436,329,351]" class="Actinopterygii" family="Bothidae" genus="Engyprosopon" kingdom="Animalia" order="Pleuronectiformes" pageId="29" pageNumber="184" phylum="Chordata" rank="species" species="maldivense">
