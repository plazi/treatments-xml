<quantity box="[985,1084,359,381]" metricMagnitude="-2" metricUnit="m" metricValue="7.111999999999999" metricValueMax="7.366" metricValueMin="6.858" pageId="29" pageNumber="184" unit="in" value="2.8" valueMax="2.9" valueMin="2.7">2.7–2.9 in</quantity>
