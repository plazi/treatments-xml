<normalizedToken originalValue="Südeuropa" pageId="13" pageNumber="431">Suedeuropa</normalizedToken>
; westl. Sudeten in Polen, 
<normalizedToken originalValue="östliches" pageId="13" pageNumber="431">oestliches</normalizedToken>
<normalizedToken originalValue="Österreich" pageId="13" pageNumber="431">Oesterreich</normalizedToken>
