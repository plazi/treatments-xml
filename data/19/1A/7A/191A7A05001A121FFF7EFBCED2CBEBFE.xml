<date box="[588,674,1476,1501]" pageId="87" pageNumber="88" value="90-11-16">161190</date>
, 1 ♀, 
<collectionCode box="[741,799,1476,1500]" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="87" pageNumber="88">CNC</collectionCode>
<taxonomicName box="[805,889,1476,1501]" kingdom="Plantae" order="Diptera" pageId="87" pageNumber="88" rank="order">Diptera</taxonomicName>
