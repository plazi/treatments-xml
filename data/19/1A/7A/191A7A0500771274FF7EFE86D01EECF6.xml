<subSubSection pageId="59" pageNumber="60" type="description">
<paragraph blockId="59.[151,1437,530,2018]" box="[151,402,530,555]" pageId="59" pageNumber="60">
<emphasis bold="true" box="[151,219,530,555]" pageId="59" pageNumber="60">
<collectingCountry box="[151,214,530,555]" name="Malta" pageId="59" pageNumber="60">Male</collectingCountry>
.
</emphasis>
<collectingCountry box="[225,283,530,555]" name="Malta" pageId="59" pageNumber="60">Male</collectingCountry>
