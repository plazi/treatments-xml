<collectingCountry box="[1291,1387,1384,1410]" name="Namibia" pageId="24" pageNumber="25">Namibia</collectingCountry>
and 
<collectingRegion box="[151,209,1421,1447]" country="South Africa" name="Western Cape" pageId="24" pageNumber="25">Cape</collectingRegion>
