<collectingCountry box="[146,265,922,946]" name="Indonesia" pageId="17" pageNumber="1179">Indonesia</collectingCountry>
, 
<collectingCountry box="[286,351,922,946]" name="Palau" pageId="17" pageNumber="1179">Palau</collectingCountry>
