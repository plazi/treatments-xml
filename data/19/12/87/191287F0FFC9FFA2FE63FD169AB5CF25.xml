<geoCoordinate box="[138,248,954,978]" degrees="17.855" direction="south" orientation="latitude" pageId="17" pageNumber="1179" precision="55" value="-17.855">
17.855 
<emphasis bold="true" box="[219,229,954,978]" italics="true" pageId="17" pageNumber="1179">°</emphasis>
S
</geoCoordinate>
, 
<geoCoordinate box="[262,389,954,978]" degrees="146.585" direction="east" orientation="longitude" pageId="17" pageNumber="1179" precision="55" value="146.585">
146.585 
<emphasis bold="true" box="[358,368,954,978]" italics="true" pageId="17" pageNumber="1179">°</emphasis>
E
</geoCoordinate>
, 
<quantity box="[403,485,954,978]" metricMagnitude="1" metricUnit="m" metricValue="4.34" pageId="17" pageNumber="1179" unit="m" value="43.4">43.4 m</quantity>
