<quantity box="[658,758,310,335]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="3" pageNumber="196" unit="mm" value="3.5" valueMax="4.0" valueMin="3.0">3–4 mm</quantity>
