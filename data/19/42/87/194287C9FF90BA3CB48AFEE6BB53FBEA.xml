</paragraph>
</subSubSection>
<subSubSection box="[1421,1795,716,745]" pageId="44" pageNumber="663" type="breeding">
<paragraph blockId="44.[1419,2632,278,1065]" box="[1421,1795,716,745]" pageId="44" pageNumber="663">
