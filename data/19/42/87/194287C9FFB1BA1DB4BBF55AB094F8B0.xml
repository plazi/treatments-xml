<subSubSection pageId="13" pageNumber="632" type="discussion">
<paragraph blockId="13.[778,1374,705,1128]" pageId="13" pageNumber="632">
<taxonomicName baseAuthorityName="Gundlach" baseAuthorityYear="1861" box="[778,954,819,852]" class="Mammalia" family="Molossidae" genus="Eumops" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="632" phylum="Chordata" rank="species" species="ferox">Eumops ferox</taxonomicName>
