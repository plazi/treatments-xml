<collectingCountry box="[2130,2215,2218,2251]" name="Kenya" pageId="46" pageNumber="665">Kenya</collectingCountry>
and N 
<collectingCountry box="[2321,2468,2218,2251]" name="Zimbabwe" pageId="46" pageNumber="665">Zimbabwe</collectingCountry>
