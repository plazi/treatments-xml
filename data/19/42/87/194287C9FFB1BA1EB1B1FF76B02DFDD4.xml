<subSubSection pageId="13" pageNumber="632" type="discussion">
<paragraph blockId="13.[2052,2647,2240,2669]" pageId="13" pageNumber="632">
<taxonomicName baseAuthorityName="Miller" baseAuthorityYear="1900" box="[2054,2247,2359,2392]" class="Mammalia" family="Molossidae" genus="Eumops" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="632" phylum="Chordata" rank="species" species="nanus">Eumops nanus</taxonomicName>
