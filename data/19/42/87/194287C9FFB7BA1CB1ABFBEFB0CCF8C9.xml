<subSubSection lastPageId="12" lastPageNumber="631" pageId="11" pageNumber="630" type="reference_group">
<paragraph blockId="11.[1462,2667,3389,3471]" pageId="11" pageNumber="630">
<emphasis bold="true" box="[1463,1619,3389,3422]" pageId="11" pageNumber="630">Taxonomy.</emphasis>
Fumops 
<taxonomicName authority="Thomas, 1914" authorityName="Thomas" authorityYear="1914" box="[1749,2072,3389,3422]" class="Mammalia" family="Molossidae" genus="Eumops" kingdom="Animalia" order="Chiroptera" pageId="11" pageNumber="630" phylum="Chordata" rank="species" species="dabbenei">dabbenei Thomas, 1914</taxonomicName>
, 
<materialsCitation pageId="11" pageNumber="630">
“ 
<collectingRegion box="[2107,2196,3389,3422]" country="Argentina" name="Chaco" pageId="11" pageNumber="630">Chaco</collectingRegion>
