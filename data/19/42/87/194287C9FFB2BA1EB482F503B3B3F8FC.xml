, S 
<collectingCountry box="[1180,1262,1021,1050]" name="Brazil" pageId="14" pageNumber="633">Brazil</collectingCountry>
, 
<collectingCountry name="Paraguay" pageId="14" pageNumber="633">Paraguay</collectingCountry>
, 
