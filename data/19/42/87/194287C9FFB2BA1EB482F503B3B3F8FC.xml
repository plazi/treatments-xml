<caption ID-DOI="http://doi.org/10.5281/zenodo.6418339" ID-Zenodo-Dep="6418339" httpUri="https://zenodo.org/record/6418339/files/figure.png" inLine="true" pageId="14" pageNumber="633" targetBox="[137,729,752,1167]" targetPageId="14">
