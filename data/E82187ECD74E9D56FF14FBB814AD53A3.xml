<collectionCode id="0699AE3FD74E9D56FF13FA9514A6537B" box="[175,226,1296,1320]" pageId="10" pageNumber="801">SVL</collectionCode>
<quantity id="A7709B1FD74E9D56FF56FA95157C5374" box="[234,312,1296,1320]" metricMagnitude="-2" metricUnit="m" metricValue="5.2" pageId="10" pageNumber="801" unit="mm" value="52.0">52 mm</quantity>
