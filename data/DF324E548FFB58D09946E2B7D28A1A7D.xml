, 
<taxonomicName id="E17AF14A8C7CA4362C3D80ABD0198530" baseAuthorityName="Fulton" baseAuthorityYear="1908" class="Gastropoda" family="Amphibulimidae" genus="Dryptus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="16" pageNumber="17" phylum="Mollusca" rank="species" species="jubeus">
