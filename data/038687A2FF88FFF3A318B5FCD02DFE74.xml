and 
<taxonomicName id="4C2F4D37FF88FFF4A46DB136D72FF880" authority="Timb." authorityName="Timb." box="[1113,1352,1840,1867]" class="Insecta" family="Scelionidae" genus="Scelio" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="3193" phylum="Arthropoda" rank="species" species="hieroglyphi">
