<taxonomicName id="4C43AAED4530FC1DFF1E724FFDB8F9E3" authority="(Newman)" baseAuthorityName="Newman" baseAuthorityYear="1839" box="[227,542,1574,1599]" class="Insecta" family="Perlidae" genus="Perlinella" kingdom="Animalia" order="Plecoptera" pageId="9" pageNumber="178" phylum="Arthropoda" rank="species" species="drymo">
