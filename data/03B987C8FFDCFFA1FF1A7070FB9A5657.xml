<number id="3DD824F9FFC2FFA1FC33746CFC245336" box="[958,989,260,284]" pageId="48" pageNumber="49" value="33.0">33</number>
; third valvula length, 
<number id="3DD824F9FFC2FFA1FB6B746CFAE25337" box="[1254,1307,260,285]" pageId="48" pageNumber="49" value="5.75">5.75</number>
; ovipositor stylets length, 
<number id="3DD824F9FFC2FFA1FEB57440FEAE536A" box="[312,343,296,320]" pageId="48" pageNumber="49" value="31.0">31</number>
