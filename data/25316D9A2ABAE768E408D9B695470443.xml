<taxonomicName id="FF06FCE2E235D16D0D5679878D322585" authority="Marshall in André, 1896" authorityName="Marshall in André" authorityYear="1896" class="Insecta" family="Braconidae" genus="DYSCRITUS" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="DYSCRITUS" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">DYSCRITUS</taxonomicName>
