(
<number id="3D818C7EB745FF90FDC2C0ACFB32F93C" box="[512,558,1697,1721]" pageId="3" pageNumber="393" value="100.0">100</number>
), 
<number id="3D818C7EB745FF90FD84C0ADFB85F93C" box="[582,665,1696,1721]" pageId="3" pageNumber="393" value="92.5" valueMax="105.0" valueMin="80.0">80–105</number>
