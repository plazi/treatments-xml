<taxonomicName id="90C6AAC4FFB3FF9B75CEFD38C398FD40" box="[958,1261,690,712]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="83" pageNumber="783" phylum="Arthropoda" rank="species" species="futunae" subGenus="Stegomyia">
<emphasis id="65B20D55FFB3FF9B75CEFD38C48AFD40" box="[958,1023,690,711]" italics="true" pageId="83" pageNumber="783">Aedes</emphasis>
