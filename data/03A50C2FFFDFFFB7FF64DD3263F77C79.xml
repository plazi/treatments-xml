<paragraph id="8BB3BD39FFDFFFB7FF64DD1764EF7BE9" blockId="28.[151,1436,833,928]" pageId="28" pageNumber="380">
(
<number id="3DC4AF1EFFDFFFB7FF51DD1761C77B35" box="[162,174,868,892]" pageId="28" pageNumber="380" value="1.0">1</number>
