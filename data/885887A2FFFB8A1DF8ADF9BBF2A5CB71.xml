populations from NE 
<collectingCountry id="78E67624FFFB8A1DF6CDF5BBF6E2D322" box="[2305,2407,2586,2625]" name="India" pageId="52" pageNumber="331">India</collectingCountry>
(
<collectingRegion id="C235F856FFFB8A1DF648F5BBF574D322" box="[2436,2801,2586,2625]" country="India" name="Arunachal Pradesh" pageId="52" pageNumber="331">Arunachal Pradesh</collectingRegion>
and 
<collectingRegion id="C235F856FFFB8A1DF49AF5BBF387D322" box="[2902,3074,2586,2625]" country="India" name="Mizoram" pageId="52" pageNumber="331">Mizoram</collectingRegion>
