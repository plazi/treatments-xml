<number id="5C232485B95CFFD74C6F058FFAB42AAA" box="[1298,1311,1975,1999]" pageId="12" pageNumber="329" value="4.0">4</number>
<number id="5C232485B95CFFD74C5A058FFAFA2AAA" box="[1319,1361,1975,1999]" pageId="12" pageNumber="329" value="120.0">120</number>
(
<number id="5C232485B95CFFD74C1C058FFA252AB5" box="[1377,1422,1975,2000]" pageId="12" pageNumber="329" value="125.0">125</number>
), 
<number id="5C232485B95CFFD749EA05E3FF6A2A96" box="[151,193,2011,2035]" pageId="12" pageNumber="329" value="170.0">170</number>
