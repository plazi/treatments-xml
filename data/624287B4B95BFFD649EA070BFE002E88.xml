<number id="5C232485B95CFFD74BF405E3FD3E2A91" box="[649,661,2011,2036]" pageId="12" pageNumber="329" value="5.0">5</number>
<number id="5C232485B95CFFD74BE005E3FD6C2A96" box="[669,711,2011,2035]" pageId="12" pageNumber="329" value="133.0">133</number>
