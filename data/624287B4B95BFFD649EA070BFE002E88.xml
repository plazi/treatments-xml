<number id="5C232485B95CFFD74A0B05E3FC292A91" box="[886,898,2011,2036]" pageId="12" pageNumber="329" value="6.0">6</number>
<number id="5C232485B95CFFD74AF705E3FC1F2A91" box="[906,948,2011,2036]" pageId="12" pageNumber="329" value="165.0">165</number>
