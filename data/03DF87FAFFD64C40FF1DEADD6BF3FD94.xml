<taxonomicName id="4C764D6FFFD64C40FF5AEAB56D12FEBB" authority="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord, 2022" authorityName="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord" authorityYear="2022" box="[208,546,255,279]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="42" pageNumber="43" phylum="Arthropoda" rank="subSpecies" species="heliopolisensis" status="n. ssp." subSpecies="hespera">
