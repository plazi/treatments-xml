<quantity id="5A261FAD8070FFD3EDFCCDFD19A70D90" box="[151,250,980,1005]" metricMagnitude="1" metricUnit="m" metricValue="8.15" metricValueMax="16.0" metricValueMin="0.3" pageId="116" pageNumber="117" unit="m" value="81.5" valueMax="160.0" valueMin="3.0">3–160 m</quantity>
