<taxonomicName id="007F229495A976A96A6E99F5E127FF0C" LSID-ZBK="urn:lsid:zoobank.org:act:BED2BB84-3FBC-4BDC-9032-0D537AD3F726" class="Actinopterygii" family="Erethistidae" genus="Pseudolaguvia" kingdom="Animalia" lsidName-ZBK="Pseudolaguvia Misra 1976:253" order="Siluriformes" pageNumber="20" phylum="Chordata" rank="genus">Pseudolaguvia Misra, 1976</taxonomicName>
