<collectionCode id="E39F0B849EA0E3DAFB73F877DFE264F6" box="[1193,1246,1934,1958]" country="USA" httpUri="http://grbio.org/cool/2agb-3d98" name="Hymenoptera Institute Collection, University of Kentucky" pageId="197" pageNumber="198" type="University or college">HIC</collectionCode>
)
</materialsCitation>
