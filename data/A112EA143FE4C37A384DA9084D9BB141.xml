<taxonomicName id="60CCFA2C504EF219EF9D6A827B32B029" class="Actinopterygii" family="Sciaenidae" genus="Micropogonias" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Micropogonias furnieri" order="Perciformes" pageId="33" pageNumber="34" phylum="Chordata" rank="species" species="furnieri">Micropogonias furnieri</taxonomicName>
