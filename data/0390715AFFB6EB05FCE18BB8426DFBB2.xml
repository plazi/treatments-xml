; 
<taxonomicName id="4C39BBCFFFB6EB04FC2B8E93441DFB68" authority="(Decher, Hutterer &amp; Monadjem, 2015)" baseAuthorityName="Decher, Hutterer &amp; Monadjem" baseAuthorityYear="2015" class="Mammalia" family="Vespertilionidae" genus="Pseudoromicia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="1132" phylum="Chordata" rank="species" species="isabella">
<emphasis id="B94D1C5EFFB6EB04FC2B8E9344B1FB48" box="[909,1190,1225,1246]" italics="true" pageId="18" pageNumber="1132">Pseudoromicia isabella</emphasis>
(
<bibRefCitation id="EFA8BDBDFFB6EB04FB118E934415FB68" author="Decher J &amp; Hoffmann A &amp; Schaer J &amp; Norris RW &amp; Kadjo B &amp; Astrin J &amp; Monadjem A &amp; Hutterer R" pageId="18" pageNumber="1132" pagination="255 - 282" refId="ref21165" refString="Decher J, Hoffmann A, Schaer J, Norris RW, Kadjo B, Astrin J, Monadjem A, Hutterer R. 2015. Bat diversity in the Simandou Mountain Range of Guinea, with the description of a new white-winged vespertilionid. Acta Chiropterologica 17: 255 - 282." type="journal article" year="2015">Decher, Hutterer &amp; Monadjem, 2015</bibRefCitation>
)
</taxonomicName>
; 
<taxonomicName id="4C39BBCFFFB6EB04FBB18EB24397FA8B" authority="(Thomas, 1889)" baseAuthorityName="Thomas" baseAuthorityYear="1889" class="Mammalia" family="Vespertilionidae" genus="Pseudoromicia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="1132" phylum="Chordata" rank="species" species="rendalli">
<emphasis id="B94D1C5EFFB6EB04FBB18EB2453AFB6B" box="[1047,1325,1256,1277]" italics="true" pageId="18" pageNumber="1132">Pseudoromicia rendalli</emphasis>
(Thomas, 1889)
</taxonomicName>
; 
<taxonomicName id="4C39BBCFFFB6EB04FC368F5C436AFAAD" authority="(Monadjem et al., 2013)" baseAuthorityName="Monadjem" baseAuthorityYear="2013" class="Mammalia" family="Vespertilionidae" genus="Pseudoromicia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="1132" phylum="Chordata" rank="species" species="roseveari">
<emphasis id="B94D1C5EFFB6EB04FC368F5C44A9FA8A" box="[912,1214,1286,1308]" italics="true" pageId="18" pageNumber="1132">Pseudoromicia roseveari</emphasis>
(
<bibRefCitation id="EFA8BDBDFFB6EB04FB778F5C4362FAAD" author="Monadjem A &amp; Richards L &amp; Taylor PJ &amp; Stoffberg S" pageId="18" pageNumber="1132" pagination="191 - 207" refId="ref22783" refString="Monadjem A, Richards L, Taylor PJ, Stoffberg S. 2013. High diversity of pipistrelloid bats (Vespertilionidae: Hypsugo, Neoromicia, and Pipistrellus) in a West African rainforest with the description of a new species. Zoological Journal of the Linnean Society 167: 191 - 207." type="journal article" year="2013">
Monadjem 
<emphasis id="B94D1C5EFFB6EB04FAFB8F5D4583FA8D" box="[1373,1428,1286,1308]" italics="true" pageId="18" pageNumber="1132">et al</emphasis>
., 2013
</bibRefCitation>
)
