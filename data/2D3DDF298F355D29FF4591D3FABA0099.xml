<taxonomicName id="629415BC8F375D29FF0892ACFE3703CC" authority="(Moron)" baseAuthorityName="Moron" box="[218,482,384,410]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="43" phylum="Arthropoda" rank="species" species="chorotega">
