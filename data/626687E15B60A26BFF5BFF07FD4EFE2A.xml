<taxonomicName id="2DCF4D745B61A26CFEA8F910FDC0F977" box="[356,593,1664,1687]" class="Arachnida" family="Pholcidae" genus="Mesabolivar" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="58" phylum="Arthropoda" rank="species" species="rudilapsi" status="n. sp.">
<emphasis id="D8BBEAE55B61A26CFEA8F910FDC0F977" box="[356,593,1664,1687]" italics="true" pageId="3" pageNumber="58">Mesabolivar rudilapsi</emphasis>
