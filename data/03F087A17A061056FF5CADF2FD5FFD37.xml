<taxonomicName id="4C594D347A061054FED5AA40FE43F8C5" authority="Yu &amp; Yang &amp; Liu, 2018" authorityName="Yu &amp; Yang &amp; Liu" authorityYear="2018" box="[286,443,1834,1855]" class="Collembola" family="Tomoceridae" genus="Tomocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="5" pageNumber="230" phylum="Arthropoda" rank="species" species="nan" status="sp. nov.">
