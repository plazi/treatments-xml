<quantity id="DC269B6CFFDDD57FFE9453402E7AFDC3" box="[321,376,533,554]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="12" pageNumber="523" unit="mm" value="4.0">4 mm</quantity>
