<taxonomicName id="66DC4D1C0765FFFE1215067DFDE0ABAA" authority="Quenstedt, 1857" authorityName="Quenstedt" authorityYear="1857" box="[299,632,1143,1167]" class="Malacostraca" family="Erymidae" genus="Pustulina" higherTaxonomySource="GBIF" kingdom="Fungi" order="Decapoda" pageId="29" pageNumber="52" phylum="Arthropoda" rank="species" species="suevica">
