<taxonomicName id="4C7E3589FF16FF30FE41F9253301FD93" authorityName="Kolbe" authorityYear="1910" box="[457,637,587,611]" class="Insecta" family="Dynastidae" genus="Hemiphileurus" kingdom="Animalia" order="Coleoptera" pageId="221" pageNumber="220" phylum="Arthropoda" rank="genus">
