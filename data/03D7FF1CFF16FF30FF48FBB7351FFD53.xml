<taxonomicName id="4C7E3589FF16FF30FD03F92532C1FD93" authority="Fabricius, 1801" authorityName="Fabricius" authorityYear="1801" box="[651,957,587,611]" class="Insecta" family="Dynastidae" genus="Phileurus" kingdom="Animalia" order="Coleoptera" pageId="221" pageNumber="220" phylum="Arthropoda" rank="species" species="depressus">
<emphasis id="B90A9218FF16FF30FD03F9253383FD93" box="[651,767,587,611]" italics="true" pageId="221" pageNumber="220">depressus</emphasis>
<bibRefCitation id="EFEF33FBFF16FF30FC8CF92532C1FD93" author="Fabricius, J. C." bookContentInfo="506 p." box="[772,957,587,611]" journalOrPublisher="Tomus I. Impensis Bibliopolii Academici Novi; Kiel, Germany" pageId="221" pageNumber="220" refId="ref201994" refString="Fabricius, J. C. 1801 a. Systema eleutheratorum secundum ordines, genera, species adjectis synonymis, locis, observationibus, descriptionibus. Tomus I. Impensis Bibliopolii Academici Novi; Kiel, Germany. 506 p." title="Systema eleutheratorum secundum ordines, genera, species adjectis synonymis, locis, observationibus, descriptionibus" type="book" year="1801">Fabricius, 1801</bibRefCitation>
</taxonomicName>
(
<bibRefCitation id="EFEF33FBFF16FF30FC43F9253541FD93" author="Fabricius, J. C." bookContentInfo="506 p." box="[971,1085,587,611]" journalOrPublisher="Tomus I. Impensis Bibliopolii Academici Novi; Kiel, Germany" pageId="221" pageNumber="220" refId="ref201994" refString="Fabricius, J. C. 1801 a. Systema eleutheratorum secundum ordines, genera, species adjectis synonymis, locis, observationibus, descriptionibus. Tomus I. Impensis Bibliopolii Academici Novi; Kiel, Germany. 506 p." title="Systema eleutheratorum secundum ordines, genera, species adjectis synonymis, locis, observationibus, descriptionibus" type="book" year="1801">1801a: 18</bibRefCitation>
) is redescribed by 
<bibRefCitation id="EFEF33FBFF16FF30FA9EF9253038FD73" author="Burmeister, H. C. C." bookContentInfo="584 p." journalOrPublisher="Besondere Entomologie, Fortsetzung. Coleoptera Lamellicornia Xylophila et Pectinicornia. Theod. Chr. Fr. Enslin; Berlin" pageId="221" pageNumber="220" refId="ref195707" refString="Burmeister, H. C. C. 1847. Handbuch der Entomologie. Funfter Band. Besondere Entomologie, Fortsetzung. Coleoptera Lamellicornia Xylophila et Pectinicornia. Theod. Chr. Fr. Enslin; Berlin. 584 p." title="Handbuch der Entomologie. Funfter Band" type="book" year="1847">Burmeister (1847: 156)</bibRefCitation>
