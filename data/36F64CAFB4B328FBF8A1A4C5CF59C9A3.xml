<taxonomicName id="B227FB61528318085F3F7A0703F7E564" class="Insecta" family="Staphylinidae" genus="Apocellus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apocellus sphaericollis" order="Coleoptera" pageId="10" pageNumber="249" phylum="Arthropoda" rank="species" species="sphaericollis">Apocellus sphaericollis</taxonomicName>
