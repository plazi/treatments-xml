<emphasis id="6EADB626FFCFAA5DFED3A33D857BF8B0" bold="true" box="[284,342,1871,1892]" pageId="4" pageNumber="525">5–10.</emphasis>
<taxonomicName id="9BD911B7FFCFAA5DFE90A3228609F8B1" authorityName="Chatzaki" authorityYear="2018" baseAuthorityName="Zhang, Song &amp; Zhu" baseAuthorityYear="2001" box="[351,548,1871,1893]" class="Arachnida" family="Gnaphosidae" genus="Marjanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="525" phylum="Arthropoda" rank="species" species="platnicki" status="comb. nov.">
<emphasis id="6EADB626FFCFAA5DFE90A3228609F8B1" box="[351,548,1871,1893]" italics="true" pageId="4" pageNumber="525">Marjanus platnicki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="759E0B5DFFCFAA5DFDE3A33D868EF8B0" box="[556,675,1871,1892]" pageId="4" pageNumber="525">
