<paragraph id="262ED978304CFF93FF4AFB04FD650C5E" blockId="21.[189,1399,1145,1348]" pageId="21" pageNumber="22">
<typeStatus id="F92A67DA304CFF93FF4AFB04FED50DAB" box="[189,298,1180,1206]" pageId="21" pageNumber="22">Holotype</typeStatus>
(type locality: 
<collectingCountry id="5E8699E8304CFF93FE17FB04FD920DAB" box="[480,621,1180,1206]" name="Cape Verde" pageId="21" pageNumber="22">Cabo Verde</collectingCountry>
