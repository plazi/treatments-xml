<subSubSection id="C6DE656DFFAE9033FF5D66CDFD03FF51" pageId="19" pageNumber="20" type="diagnosis">
<paragraph id="8E7B36E6FFAE9033FF5D66CDFD03FF51" blockId="19.[151,1437,151,717]" pageId="19" pageNumber="20">
<emphasis id="BCB0EAF4FFAE9033FF5D66CDFEBBFF34" bold="true" box="[151,266,151,176]" pageId="19" pageNumber="20">Diagnosis</emphasis>
