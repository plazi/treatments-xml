<taxonomicName id="D5E34D68A0413F2DFD200514FCD0B01C" authority="Assing" authorityName="Assing" box="[648,870,1331,1353]" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="497" phylum="Arthropoda" rank="species" species="bozdaghense">
<emphasis id="2097EAF9A0413F2DFD200514FCAAB01C" box="[648,796,1332,1353]" italics="true" pageId="4" pageNumber="497">S. bozdaghense</emphasis>
Assing
</taxonomicName>
(star); 
<taxonomicName id="D5E34D68A0413F2DFC030514FB3AB01C" authority="Assing" authorityName="Assing" box="[939,1164,1331,1353]" class="Insecta" family="Staphylinidae" genus="Sunius" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="497" phylum="Arthropoda" rank="species" species="fortespinosus">
<emphasis id="2097EAF9A0413F2DFC030514FBF5B01C" box="[939,1091,1332,1353]" italics="true" pageId="4" pageNumber="497">S. fortespinosus</emphasis>
Assing
</taxonomicName>
(down pointing triangle); 
