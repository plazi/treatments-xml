<taxonomicName id="4C2190B5FFC771236628FC38FB3EFC21" box="[1042,1198,899,924]" class="Insecta" family="Chironomidae" genus="Cryptotendipes" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="tuberosus">
<emphasis id="B9553724FFC771236628FC38FB3EFC21" box="[1042,1198,899,924]" italics="true" pageId="7" pageNumber="8">C. tuberosus</emphasis>
</taxonomicName>
with the following exceptions: Wing length 
<quantity id="4CD946D3FFC771236381FC13FDB8FC7F" box="[443,552,936,962]" metricMagnitude="-3" metricUnit="m" metricValue="1.32" pageId="7" pageNumber="8" unit="mm" value="1.32">1.32 mm</quantity>
