<subSubSection id="C34A6558FF8FF77C0132152F4FD6C9AD" lastPageId="32" lastPageNumber="249" pageId="30" pageNumber="247" type="etymology">
<paragraph id="8BEF36D3FF8FF7420132152F4EFDCBEB" blockId="30.[839,1467,1386,1439]" pageId="30" pageNumber="247">
<emphasis id="B924EAC1FF8FF7420132152F4ED7CBF6" bold="true" box="[839,962,1386,1410]" pageId="30" pageNumber="247">Etymology</emphasis>
