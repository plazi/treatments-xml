<taxonomicName id="43CC4D26A74991667FA6F91CFD70F8F7" authority="Negrobov, Kumazawa &amp; Tago" authorityName="Negrobov, Kumazawa &amp; Tago" authorityYear="2016" class="Insecta" family="Dolichopodidae" genus="Chrysotus" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="saigusai" status="sp. nov.">
<emphasis id="B6B8EAB7A74991667FA6F91CFEE7F8F6" italics="true" pageId="7" pageNumber="8">Chrysotus saigusаi</emphasis>
Negrobov, Kumazawa &amp; Tago
</taxonomicName>
<taxonomicNameLabel id="AD8B57CCA749916679D6F8C0FD11F8F7" box="[655,745,1793,1819]" pageId="7" pageNumber="8" rank="species">sp. nov.</taxonomicNameLabel>
runs to 
<taxonomicName id="43CC4D26A7499166781FF8C1FA8FF8F6" authority="Negrobov &amp; Maslova, 2000" authorityName="Negrobov &amp; Maslova" authorityYear="2000" box="[838,1399,1792,1818]" class="Insecta" family="Dolichopodidae" genus="Chrysotus" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="defensus">
