<collectingCountry id="F3A15F858B48FFCFFBBDF9EDFB0B6CCE" box="[1059,1157,1644,1670]" name="Pakistan" pageId="60" pageNumber="61">Pakistan</collectingCountry>
, 
<collectingCountry id="F3A15F858B48FFCFFB0EF9EDFB766CCE" box="[1168,1272,1644,1670]" name="Palestine" pageId="60" pageNumber="61">Palestine</collectingCountry>
, 
<collectingCountry id="F3A15F858B48FFCFFA9DF9EDFAEC6CCE" box="[1283,1378,1644,1670]" name="Somalia" pageId="60" pageNumber="61">Somalia</collectingCountry>
, 
<collectingCountry id="F3A15F858B48FFCFFAF2F9ECFF4F6CE2" name="Turkey" pageId="60" pageNumber="61">Turkey</collectingCountry>
, 
