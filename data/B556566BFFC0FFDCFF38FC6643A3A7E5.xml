<taxonomicName id="FAFF9CFEFFC1FFDCFDA2FF34438FA7FA" authorityName="Puthz" authorityYear="2017" box="[569,713,204,225]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="1364" phylum="Arthropoda" rank="species" species="detestabilis">
