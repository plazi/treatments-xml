<emphasis id="B909EAFA6B285516FED8712BFDD3818F" box="[285,514,1896,1922]" italics="true" pageId="67" pageNumber="68">
<taxonomicName id="4C7D4D6B6B285516FED8712BFE56818C" authorityName="H. Milne Edwards" authorityYear="1837" box="[285,391,1896,1922]" class="Malacostraca" family="Atyidae" genus="Caridina" kingdom="Animalia" order="Decapoda" pageId="67" pageNumber="68" phylum="Arthropoda" rank="genus">Caridina</taxonomicName>
