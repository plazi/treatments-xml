<taxonomicName id="4C6066FC1E6FFFAB66A0F824B055EBFB" authority="Richer de Forges &amp; Ng, 2012" authorityName="Richer de Forges &amp; Ng" authorityYear="2012" box="[680,1138,1929,1956]" class="Malacostraca" family="Epialtidae" genus="Griffinia" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="takedai">
