<collectingCountry id="F3775DEF1E6FFFAB67F0F8CBB05EEB27" box="[1016,1145,1893,1919]" name="Philippines" pageId="1" pageNumber="2">Philippines</collectingCountry>
, 
<taxonomicName id="4C6066FC1E6FFFAB608CF8C8B4FCEBFC" authority="(Rathbun, 1916)" authorityName="Rathbun" authorityYear="1916" baseAuthorityName="Rathbun" baseAuthorityYear="1916" class="Malacostraca" family="Epialtidae" genus="Griffinia" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="gilloloensis">
