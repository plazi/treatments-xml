<collectingRegion id="360A839B2D08FF93FC96F8B1ECEF76D5" box="[809,951,461,487]" country="Thailand" name="Chiang Mai" pageId="59" pageNumber="60">Chiang Mai</collectingRegion>
, 
<location id="F1111BA22D08FF93FC7AF8B1EBAE76D5" LSID="urn:lsid:plazi:treatment:7C67FC6F2D08FF93FF28F826EC197134:F1111BA22D08FF93FC7AF8B1EBAE76D5" box="[965,1270,461,487]" country="Thailand" name="Doi Suthep Pui Nat. Park" pageId="59" pageNumber="60" stateProvince="Chiang Mai">Doi Suthep Pui Nat. Park</location>
, 
<quantity id="3336E09C2D08FF93FABAF8B1EA3A76D5" box="[1285,1378,461,487]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="59" pageNumber="60" unit="m" value="1200.0">
<elevation id="7FE3AA4A2D08FF93FABAF8B1EA3A76D5" box="[1285,1378,461,487]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="59" pageNumber="60" unit="m" value="1200.0">1200 m</elevation>
