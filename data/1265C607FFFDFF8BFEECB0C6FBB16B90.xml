<number id="2C046536FFFDFF8BFDF3B7DBFDB36AD0" box="[535,564,1154,1180]" pageId="8" pageNumber="63" value="60.0">60</number>
<number id="2C046536FFFDFF8BFDA6B7DBFDD66AD0" box="[578,593,1154,1180]" pageId="8" pageNumber="63" value="5.0">5</number>
