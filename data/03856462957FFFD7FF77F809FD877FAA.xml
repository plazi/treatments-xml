<taxonomicName id="4C2CAEF79578FFD7FBE0FEC7FB5A7CA7" authorityName="Hutton" authorityYear="1879" box="[1024,1152,367,392]" class="Gastropoda" family="Athoracophoridae" genus="Konophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="28" pageNumber="229" phylum="Mollusca" rank="genus">
