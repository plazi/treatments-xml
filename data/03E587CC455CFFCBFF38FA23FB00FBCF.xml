<quantity id="4CB49B3F455CFFCAFDFFF9ACFD32F9DE" box="[592,651,1568,1594]" metricMagnitude="1" metricUnit="m" metricValue="5.0927" pageId="4" pageNumber="52" unit="in" value="2005.0">2005</quantity>
</date>
