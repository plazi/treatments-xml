<date id="FFF2101A455CFFCAFE16F9ACFD32F9DE" box="[441,651,1568,1594]" pageId="4" pageNumber="52" value="2005-02-17">
<number id="3D8424FD455CFFCAFE16F9ACFE6FF9DE" box="[441,470,1568,1594]" pageId="4" pageNumber="52" value="17.0">17</number>
