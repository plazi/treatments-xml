<quantity id="4CB49B3F455CFFCAFE65F9E1FDAFF963" box="[458,534,1645,1671]" metricMagnitude="-1" metricUnit="m" metricValue="4.0" pageId="4" pageNumber="52" unit="cm" value="40.0">40 cm</quantity>
