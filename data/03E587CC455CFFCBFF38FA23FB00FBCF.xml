<number id="3D8424FD455CFFCAFC0CF9E1FC7DF963" box="[931,964,1645,1671]" pageId="4" pageNumber="52" value="20.0">20</number>
%. Further data: 
<collectingCountry id="F35B764A455CFFCAFB29F9E1FB22F963" box="[1158,1179,1645,1671]" name="Iceland" pageId="4" pageNumber="52">O</collectingCountry>
<number id="3D8424FD455CFFCAFB33F9F1FB1DF96F" box="[1180,1188,1661,1675]" pageId="4" pageNumber="52" value="2.0">2</number>
