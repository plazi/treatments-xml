<number id="3D8424FD455CFFCAFE1BFA75FE54F9F7" box="[436,493,1529,1555]" pageId="4" pageNumber="52" value="1592.0">1592</number>
/ 
<number id="3D8424FD455CFFCAFE59FA75FD88F9F7" box="[502,561,1529,1555]" pageId="4" pageNumber="52" value="2007.0">2007</number>
(
<specimenCount id="9D4AFD53455CFFCAFDEAFA75FD74F9F7" box="[581,717,1529,1555]" pageId="4" pageNumber="52" type="generic">
<number id="3D8424FD455CFFCAFDEAFA75FDEBF9F7" box="[581,594,1529,1555]" pageId="4" pageNumber="52" value="1.0">1</number>
