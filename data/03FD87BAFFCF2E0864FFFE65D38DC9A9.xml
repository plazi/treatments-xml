<taxonomicName id="4C544D2FFFCF2E086713FCDBD1AAC94E" authorityName="Corbera" authorityYear="2008" box="[891,997,866,889]" class="Malacostraca" family="Lampropidae" genus="Bathylamprops" kingdom="Animalia" order="Cumacea" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="pagesi">
