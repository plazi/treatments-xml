<taxonomicName id="4C544D2FFFCF2E086077FCDBD6D5C94D" authorityName="Zimmer" authorityYear="1908" box="[1055,1178,865,890]" class="Malacostraca" family="Lampropidae" genus="Bathylamprops" kingdom="Animalia" order="Cumacea" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="calmani">
