<tr id="355984384C520079FED67A43FAB5FD99" box="[264,1323,524,550]" gridrow="2" pageId="9" pageNumber="10" rowspan-1="1" rowspan-10="1" rowspan-11="1" rowspan-12="1" rowspan-13="1" rowspan-14="1" rowspan-2="1" rowspan-3="1" rowspan-4="1" rowspan-5="1" rowspan-6="1" rowspan-7="1" rowspan-8="1" rowspan-9="1">
<th id="7688ED444C520079FED67A43FD97FD99" box="[264,521,524,550]" gridcol="0" gridrow="2" pageId="9" pageNumber="10">hind tibial combs.</th>
</tr>
