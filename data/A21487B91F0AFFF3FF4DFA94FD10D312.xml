<subSubSection id="62A765241F0CFFF3FF1DFD00FEF1D359" pageId="13" pageNumber="508" type="description">
<paragraph id="2A0236AF1F0CFFF3FF1DFD00FEF1D359" blockId="13.[151,1437,151,845]" pageId="13" pageNumber="508">
<emphasis id="18C9EABD1F0CFFF3FF1DFD00FE82D2BB" bold="true" box="[199,309,715,740]" pageId="13" pageNumber="508">Variation</emphasis>
. Body length 
<quantity id="ED459B4A1F0CFFF3FE07FD00FD31D2BC" box="[477,646,715,739]" metricMagnitude="-3" metricUnit="m" metricValue="3.83" metricValueMax="4.33" metricValueMin="3.33" pageId="13" pageNumber="508" unit="mm" value="3.83" valueMax="4.33" valueMin="3.33">3.33–4.33 mm</quantity>
; forewing 
