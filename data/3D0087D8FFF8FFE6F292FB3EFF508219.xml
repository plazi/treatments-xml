<taxonomicName id="72A94D4DFFFBFFE6F353FC74FE398789" box="[329,393,983,1004]" class="Insecta" family="Pyralidae" genus="Etiella" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="151" phylum="Arthropoda" rank="genus">
