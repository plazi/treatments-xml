</subSubSection>
<subSubSection id="C30307BDE8155860FA63A607EC64F3BD" pageId="5" pageNumber="142" type="reference_group">
<emphasis id="B96D8824E8155860FA63A607ED00F394" box="[1464,1619,3110,3139]" pageId="5" pageNumber="142">Taxonomy.</emphasis>
<taxonomicName id="4C192FB5E8155860F985A607EC86F394" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[1630,2005,3110,3139]" class="Mammalia" family="Equidae" genus="Equus" kingdom="Animalia" order="Perissodactyla" pageId="5" pageNumber="142" phylum="Chordata" rank="species" species="zebra">Equus zebra Linnaeus, 1758</taxonomicName>
, 
<collectingCountry id="F30E14A6E8155860F83EA607E3C6F394" box="[2021,2197,3110,3139]" name="South Africa" pageId="5" pageNumber="142">South Africa</collectingCountry>
, south-western 
<collectingRegion id="49DD9AD4E8155860F6B4A607E16FF394" box="[2415,2620,3110,3139]" country="South Africa" name="Western Cape" pageId="5" pageNumber="142">Cape Province</collectingRegion>
