<taxonomicName id="9EAB4D24181FC07FFD4684013AEC055E" authority="Kirk" authorityName="Kirk" box="[671,912,1303,1329]" class="Liliopsida" family="Asteliaceae" genus="Astelia" kingdom="Plantae" order="Asparagales" pageId="14" pageNumber="315" phylum="Tracheophyta" rank="species" species="trinervia">
