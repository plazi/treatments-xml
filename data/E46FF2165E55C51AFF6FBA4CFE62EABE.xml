<date id="187865C05E55C51AFF0BB98EFE47EBDE" box="[243,401,1693,1717]" pageId="87" pageNumber="88" value="1968-07-09" valueMax="1968-07-12" valueMin="1968-07-09">
<collectingDate id="083C9C285E55C51AFF0BB98EFE47EBDE" box="[243,401,1693,1717]" pageId="87" pageNumber="88" value="1968-07-09" valueMax="1968-07-12" valueMin="1968-07-09">9–12.vii.1968</collectingDate>
</date>
, 
<specimenCount id="7AC088895E55C51AFE64B98EFE23EBDE" box="[412,501,1693,1717]" pageId="87" pageNumber="88" type="male">2 males</specimenCount>
, Mena. (
<collectionCode id="0AD7DBC55E55C51AFDAEB98EFD7CEBDE" box="[598,682,1693,1717]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="87" pageNumber="88" type="Museum">BPBM</collectionCode>
