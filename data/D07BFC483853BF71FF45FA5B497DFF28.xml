<taxonomicName id="9FD236DD3856BF70FD80FAEA4B5CFA54" authority="Pilipеnko, 2017" authorityName="Pilipеnko" authorityYear="2017" box="[594,797,1390,1412]" class="Insecta" family="Limoniidae" genus="Gonempeda" kingdom="Animalia" order="Diptera" pageId="4" phylum="Arthropoda" rank="species" species="mariya" status="n. sp.">
