; width across eyes 
<number id="C67424C1FFAE7664FAFBFD7DFAF96FCD" box="[1324,1368,726,750]" pageId="11" pageNumber="12" value="110.0">110</number>
; ocellar setae 
<date id="04021026FFAE7664FF2EFD50FE9E6E31" box="[249,319,762,786]" pageId="11" pageNumber="12">
III 
<number id="C67424C1FFAE7664FECCFD51FE9B6E31" box="[283,314,762,786]" pageId="11" pageNumber="12" value="30.0">30</number>
.
</date>
Pronotum, length 
<number id="C67424C1FFAE7664FDDAFD51FD986E31" box="[525,569,762,786]" pageId="11" pageNumber="12" value="115.0">115</number>
; pa inner setae 
<number id="C67424C1FFAE7664FD34FD51FCA36E31" box="[739,770,762,786]" pageId="11" pageNumber="12" value="55.0">55</number>
; pa outer setae 
<number id="C67424C1FFAE7664FC7AFD51FC6D6E31" box="[941,972,762,786]" pageId="11" pageNumber="12" value="60.0">60</number>
