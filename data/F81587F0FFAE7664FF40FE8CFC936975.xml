<number id="C67424C1FFAE7664FC98FCB5FCFB6E15" box="[847,858,798,822]" pageId="11" pageNumber="12" value="1.0">1</number>
<number id="C67424C1FFAE7664FCB4FCB5FC316E15" box="[867,912,798,822]" pageId="11" pageNumber="12" value="100.0">100</number>
; S 
<number id="C67424C1FFAE7664FC7BFCB5FC1A6E15" box="[940,955,798,822]" pageId="11" pageNumber="12" value="2.0">2</number>
<number id="C67424C1FFAE7664FC15FCB5FC4F6E15" box="[962,1006,798,822]" pageId="11" pageNumber="12" value="110.0">110</number>
. Tergite X setae 
<geoCoordinate id="15885021FFAE7664FB79FCB5FB536E15" box="[1198,1266,798,822]" direction="south" orientation="latitude" pageId="11" pageNumber="12" precision="925" value="-2.4166665">
S 
<number id="C67424C1FFAE7664FB17FCB5FB6A6E15" box="[1216,1227,798,822]" pageId="11" pageNumber="12" value="1.0">1</number>
<number id="C67424C1FFAE7664FB04FCB5FB536E15" box="[1235,1266,798,822]" pageId="11" pageNumber="12" value="85.0">85</number>
</geoCoordinate>
. Antennal segments III–VIII length 
<number id="C67424C1FFAE7664FE58FCE9FE796E79" box="[399,472,834,858]" pageId="11" pageNumber="12" value="30.25">30, 25</number>
, 
<number id="C67424C1FFAE7664FE34FCE9FD8A6E79" box="[483,555,834,858]" pageId="11" pageNumber="12" value="30.45">30, 45</number>
, 
<number id="C67424C1FFAE7664FDE0FCE9FDD36E79" box="[567,626,834,858]" pageId="11" pageNumber="12" value="8.1">8, 10</number>
