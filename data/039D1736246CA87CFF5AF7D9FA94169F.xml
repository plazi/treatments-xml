<collectingCountry id="F323E6B0246FA87CFB99F7D6FB7814CF" box="[1108,1267,152,178]" name="New Zealand" pageId="18" pageNumber="61">New Zealand</collectingCountry>
, 
<quantity id="4CCC0BC5246FA87CFACDF7D6FA1D14CF" box="[1280,1430,152,178]" metricMagnitude="3" metricUnit="m" metricValue="1.02" metricValueMax="1.097" metricValueMin="0.943" pageId="18" pageNumber="61" unit="m" value="1020.0" valueMax="1097.0" valueMin="943.0">943–1097 m</quantity>
