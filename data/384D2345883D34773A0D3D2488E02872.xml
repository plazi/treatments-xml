<geoCoordinate id="E028FE541DB29144395D468592C3892F" direction="north" orientation="latitude" precision="55" value="5.341">5.341°N</geoCoordinate>
, 
<geoCoordinate id="DBDA8F761473D00AEC8A9B10913CD147" direction="west" orientation="longitude" precision="55" value="-67.755">67.755°W</geoCoordinate>
, 06 Jan 2006, Short, Andrew E (4, SEMC); Bolivar, Gran Sabana, between Kavanayen and Rt 10, 
<geoCoordinate id="D9892DDAEDA9D0134F159C44DD5C8E98" direction="north" orientation="latitude" precision="55" value="5.741">5.741°N</geoCoordinate>
