<quantity id="D5719B32FF90953B3F29F9855209F980" box="[1041,1119,1628,1655]" metricMagnitude="3" metricUnit="m" metricValue="1.285" pageId="68" pageNumber="69" unit="m" value="1285.0">1285m</quantity>
