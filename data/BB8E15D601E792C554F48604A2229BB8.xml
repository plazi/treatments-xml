<taxonomicName id="C80EE1C0F8721D2E3A0B419F5388C5B8" class="Magnoliopsida" family="Caprifoliaceae" genus="Succisa" kingdom="Plantae" order="Dipsacales" pageId="null" pageNumber="342" phylum="Tracheophyta" rank="species" species="inflexa">
<italic id="3E34995DCC7B69ECE9C947AF3AD94334" pageId="null" pageNumber="342">S. inflexa</italic>
