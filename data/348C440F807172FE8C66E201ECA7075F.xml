<taxonomicName id="A15B46A6DE62348EF8EC2E222B1E44AE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230253" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" lsidName="Uploaded - Aenictus pharao" lsidName-HNS="Uploaded - Aenictus pharao" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="pharao">Aenictus pharao</taxonomicName>
