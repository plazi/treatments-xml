<taxonomicName id="4C744D51FFC5FFEA1EFCDDEEFD58FF6F" authority="Warburton and Nuttall, 1909" authorityName="Warburton and Nuttall" authorityYear="1909" box="[151,754,151,176]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="vidua">
