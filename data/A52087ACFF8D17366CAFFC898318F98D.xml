<taxonomicName id="EA894D39FF8F17366ED2FA408443FA1D" authority="Gunther" authorityName="Gunther" box="[746,1039,1416,1441]" class="Reptilia" family="Colubridae" genus="Lycodon" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="119" phylum="Chordata" rank="species" species="laoensis">
<emphasis id="1FFDEAA8FF8F17366ED2FA4083E0FA1D" box="[746,940,1416,1441]" italics="true" pageId="3" pageNumber="119">Lycodon laoensis</emphasis>
Günther
</taxonomicName>
(
<taxonomicName id="EA894D39FF8F17366826FA4084EDFA1D" box="[1054,1185,1416,1441]" class="Reptilia" family="Colubridae" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="119" phylum="Chordata" rank="family">Colubridae</taxonomicName>
