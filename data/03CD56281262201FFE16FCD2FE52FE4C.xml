<quantity id="4C9C4ADB1262201EFE92FA69FE9DFA20" box="[295,380,1508,1532]" metricMagnitude="-3" metricUnit="m" metricValue="9.2" pageId="2" pageNumber="73" unit="mm" value="9.2">9.2 mm</quantity>
