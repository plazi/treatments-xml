<collectorName id="26957AEC614EFFE4FBDAFD0A14B2FD91" box="[1122,1211,531,557]" pageId="4" pageNumber="137">Kolkata</collectorName>
(3791/18): 
<specimenCount id="9D66D4B3614EFFE4FA84FD0A1595FD91" box="[1340,1436,531,557]" pageId="4" pageNumber="137" type="female">1 female</specimenCount>
