<taxonomicName id="4C174D43FF8DFFA998EAF919F86E3700" authority="(Walbaum, 1792)" baseAuthorityName="Walbaum" baseAuthorityYear="1792" box="[151,559,1771,1798]" class="Elasmobranchii" family="Myliobatidae" genus="Mobula" kingdom="Animalia" order="Myliobatiformes" pageId="28" pageNumber="29" phylum="Chordata" rank="species" species="birostris">
<emphasis id="B963EAD2FF8DFFA998EAF919FB1A3703" bold="true" box="[151,347,1771,1798]" italics="true" pageId="28" pageNumber="29">Mobula birostris</emphasis>
