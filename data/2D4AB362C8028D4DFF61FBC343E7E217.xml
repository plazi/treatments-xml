<taxonomicName id="62E379F7C8028D4DFDA5FA6B423DE0D7" authority="Raffray" authorityName="Raffray" authorityYear="1900" box="[595,838,1478,1503]" class="Insecta" family="Staphylinidae" genus="Eupinoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="258" phylum="Arthropoda" rank="species" species="leana">
