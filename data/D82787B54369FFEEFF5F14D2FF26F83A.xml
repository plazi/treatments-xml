<emphasis id="62FAEAB14369FFECFCEC11D8FC9DF915" box="[804,833,1728,1753]" italics="true" pageId="1" pageNumber="63">
<number id="E64624844369FFECFCEC11D8FCEFF915" box="[804,819,1728,1753]" pageId="1" pageNumber="63" value="3.0">3</number>
