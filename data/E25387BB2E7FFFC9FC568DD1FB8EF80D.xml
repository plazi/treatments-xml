<taxonomicName id="ADFA4D2E2E7FFFC9FB198C6BFA39F867" authority="Hartlaub, 1882" authorityName="Hartlaub" authorityYear="1882" box="[1162,1451,1896,1920]" class="Aves" family="Laniidae" genus="Lanius" kingdom="Animalia" order="Passeriformes" pageId="49" pageNumber="74" phylum="Chordata" rank="species" species="gubernator">
