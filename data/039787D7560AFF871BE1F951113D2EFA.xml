and northern 
<collectingCountry id="F32976515609FF871963FCBE10232E97" box="[533,628,871,896]" name="Vietnam" pageId="18" pageNumber="469">Vietnam</collectingCountry>
(
<bibRefCitation id="EFAF4B305609FF8719F2FCBE117F2E97" author="Portevin" box="[644,808,871,896]" pageId="18" pageNumber="469" refString="Portevin, G. (1926) Les Grands Necrophages du Globe. Silphini - Necrodini - Necrophorini. Encyclopedie Entomologique (Serie A), Vol. 6. Lechevalier, Paris, 269 pp." type="book" year="1926">
