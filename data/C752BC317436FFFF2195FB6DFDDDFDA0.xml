<taxonomicName id="88FB76A47436FFF92205F999FC21F997" box="[775,909,1619,1644]" class="Insecta" family="Oecophoridae" genus="Thyrocopa" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="indecora">
<emphasis id="7D8FD1357436FFF92205F999FC21F997" box="[775,909,1619,1644]" italics="true" pageId="34" pageNumber="35">T. indecora</emphasis>
</taxonomicName>
, and 
<taxonomicName id="88FB76A47436FFF922CDF999FBE8F997" box="[975,1092,1619,1644]" class="Insecta" family="Oecophoridae" genus="Thyrocopa" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="apatela">
<emphasis id="7D8FD1357436FFF922CDF999FBE8F997" box="[975,1092,1619,1644]" italics="true" pageId="34" pageNumber="35">T. apatela</emphasis>
</taxonomicName>
