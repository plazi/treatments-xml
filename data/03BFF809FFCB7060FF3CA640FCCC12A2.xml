</emphasis>
<typeStatus id="54ADF7BDFFCB7061FEE7A695FE4A11B3" box="[332,439,1249,1274]" pageId="28" pageNumber="29">Holotype</typeStatus>
, ovigerous female, 
<quantity id="4CEEE4FAFFCB7061FD3AA696FD1411B0" box="[657,745,1250,1274]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="28" pageNumber="29" unit="mm" value="2.2">2.2 mm</quantity>
, 
<collectionCode id="ED07D1DAFFCB7061FD5EA696FCBB11B3" LSID="urn:lsid:biocol.org:col:34871" box="[757,838,1250,1274]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="28" pageNumber="29">USNM</collectionCode>
<number id="3DDE5B38FFCB7061FCE6A695FC5911B3" box="[845,932,1249,1274]" pageId="28" pageNumber="29" value="107585.0">107585</number>
