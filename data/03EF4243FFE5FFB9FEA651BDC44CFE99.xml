<taxonomicName id="4C4688D6FFE5FFB9FE3C51E7C7D7FE9D" authority="Bage, 1938" authorityName="Bage" authorityYear="1938" class="Malacostraca" family="Scyllaridae" genus="Scyllarus" kingdom="Animalia" order="Decapoda" pageId="29" pageNumber="528" phylum="Arthropoda" rank="species" species="mawsoni">
