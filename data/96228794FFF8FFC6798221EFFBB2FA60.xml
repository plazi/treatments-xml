<collectingCountry id="669C7612FFF8FFC67A2C2597FC0EF9C0" box="[825,927,1425,1447]" name="Colombia" pageId="15" pageNumber="1160">Colombia</collectingCountry>
, 
<collectingCountry id="669C7612FFF8FFC67AA42597FB8FF9C0" box="[945,1054,1425,1447]" name="Venezuela" pageId="15" pageNumber="1160">Venezuela</collectingCountry>
, 
<collectingCountry id="669C7612FFF8FFC6798225A8FF75F9A3" box="[151,228,1454,1476]" name="Bolivia" pageId="15" pageNumber="1160">Bolivia</collectingCountry>
