<subSubSection id="5B079BAF19AB765642E735BB655BE509" pageId="1" pageNumber="230" type="type species">
<paragraph id="4D3F8EA1BC1AB523B6057984B06D3E37" pageId="1" pageNumber="230">Type species.</paragraph>
<paragraph id="E6AA0B6989DC907886FC0863C6F1C2E4" pageId="1" pageNumber="230">
<taxonomicName id="5BF8B7E83D477FF2688FCF646024356F" class="Insecta" family="Carabidae" genus="Cretorabus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cretorabus capitatus" order="Coleoptera" pageId="1" pageNumber="230" phylum="Arthropoda" rank="species" species="capitatus">Cretorabus capitatus</taxonomicName>
