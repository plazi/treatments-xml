<taxonomicName id="03A54D3ACE79667EFF42FF29FAF6FF55" authority="Shereef &amp; Afifi, 1980: 125" authorityName="Shereef &amp; Afifi" authorityPageNumber="125" authorityYear="1980" box="[151,679,222,245]" class="Arachnida" family="Laelapidae" genus="Hypoaspis" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="548" phylum="Arthropoda" rank="species" species="zachvatkinae">
<emphasis id="F6D1EAABCE79667EFF42FF29F9DDFF55" box="[151,396,222,245]" italics="true" pageId="13" pageNumber="548">Hypoaspis zachvatkinae</emphasis>
<treatmentCitation id="450410A8CE79667EFE46FF17FAF6FF55" author="Shereef" box="[403,679,222,244]" page="125" pageId="13" pageNumber="548" year="1980">
<bibRefCitation id="A0344B48CE79667EFE46FF17FA25FF55" author="Shereef" box="[403,628,222,244]" pageId="13" pageNumber="551" refString="Shereef, G. M., Afifi, A. M. (1980) Five new species of mesostigmatid mites, inhabiting organic manures (Acari: Laelapidae). Proceedings of the First Conference of the Plant Protection Research Institute, 3, 121 - 134." type="journal article" year="1980">
