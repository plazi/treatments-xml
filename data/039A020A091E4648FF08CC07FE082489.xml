<subSubSection id="C329E097091C4648FF5AC9AEFBD624D3" box="[197,1073,151,176]" pageId="22" pageNumber="23" type="etymology">
<paragraph id="8B8CB31C091C4648FF5AC9AEFBD624D3" blockId="22.[151,1435,151,245]" box="[197,1073,151,176]" pageId="22" pageNumber="23">
<emphasis id="B9476F0E091C4648FF5AC9AEFEAC24CC" bold="true" box="[197,331,152,176]" inLineHeading="true" pageId="22" pageNumber="23" reason="1">Etymology.</emphasis>
