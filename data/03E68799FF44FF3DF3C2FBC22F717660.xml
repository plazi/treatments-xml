resembles 
<taxonomicName id="4C4F4D0CFF4BFF3DF1E2FDFD2DD17628" authority="(Melichar)" authorityName="Fennah" authorityYear="1969" baseAuthorityName="Melichar" baseAuthorityYear="1903" box="[695,985,511,536]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="iridipennis">
