<taxonomicName id="4C4F4D0CFF4BFF3DF21DFDFD2C317627" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Muir" baseAuthorityYear="1913" box="[328,569,512,536]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="laratica">
