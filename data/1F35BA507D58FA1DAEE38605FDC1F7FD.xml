, 
<collectingRegion id="5558C5A47D56FA1DAC93811BFEF3F07D" box="[173,331,300,321]" country="Australia" name="South Australia" pageId="24" pageNumber="25">South Australia</collectingRegion>
, 
<collectingRegion id="5558C5A47D56FA1DAD66811BFE11F07D" box="[344,425,300,321]" country="Australia" name="Victoria" pageId="24" pageNumber="25">Victoria</collectingRegion>
, and 
<collectingRegion id="5558C5A47D56FA1DADDC811BFD2BF07D" box="[482,659,300,321]" country="Australia" name="Western Australia" pageId="24" pageNumber="25">Western Australia</collectingRegion>
(
<figureCitation id="0FA717C37D56FA1DAC8A8170FEAAF060" box="[180,274,327,348]" captionStart="Figs" captionStartId="93.[175,215,1480,1499]" captionTargetBox="[252,1071,238,1451]" captionTargetId="figure-1@93.[151,1173,183,1504]" captionTargetPageId="93" captionText="Figs. 173–174. Distribution of Deretaphrus species. 173) D. alveolatus, D. antennatus, D. parviceps, and D. rodmani;" figureDoi="http://doi.org/10.5281/zenodo.4907532" httpUri="https://zenodo.org/record/4907532/files/figure.png" pageId="24" pageNumber="25">Fig. 174</figureCitation>
). 
<taxonomicName id="509C70C57D56FA1DAD168170FDABF060" box="[296,531,327,348]" class="Insecta" family="Bothrideridae" genus="Deretaphrus" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="carinatus">
