<taxonomicName id="4C4D4D333E2E421A3165FEB0FD554BBC" box="[631,720,370,395]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="66" phylum="Chordata" rank="species" species="tempeli">tempeli</taxonomicName>
, 
<taxonomicName id="4C4D4D333E2E421A31CEFEB0FCBF4BBC" box="[732,826,370,395]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="66" phylum="Chordata" rank="species" species="werneri">werneri</taxonomicName>
, 
<taxonomicName id="4C4D4D333E2E421A3057FEB0FC644BBC" box="[837,993,370,395]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="66" phylum="Chordata" rank="species" species="wiedersheimi">wiedersheimi</taxonomicName>
</emphasis>
