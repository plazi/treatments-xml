, 
<taxonomicName id="4C4D4D333E2E421A326EFEB0FE6B4BBC" box="[380,494,370,395]" class="Reptilia" family="Chamaeleonidae" genus="Trioceros" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="66" phylum="Chordata" rank="species" species="schubotzi">schubotzi</taxonomicName>
, 
<taxonomicName id="4C4D4D333E2E421A32EBFEB0FDEE4BBC" box="[505,619,370,395]" class="Reptilia" family="Chamaeleonidae" genus="Chamaeleo" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="66" phylum="Chordata" rank="species" species="sternfeldi">sternfeldi</taxonomicName>
