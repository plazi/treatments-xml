<taxonomicName id="9DEB4D3EFFB38E407C4F76026E51FCE0" authority="Oppenheim" authorityName="Oppenheim" authorityYear="1901" box="[295,622,794,817]" class="Anthozoa" family="Actinacididae" genus="Actinacis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="107" pageNumber="108" phylum="Cnidaria" rank="species" species="cognata">
