<location id="8EAD2FC7BB63FFE3C2F20C05FA82FA2C" LSID="urn:lsid:plazi:treatment:03DBC80ABB63FFE0C6EA0A0DFEB1FE6A:8EAD2FC7BB63FFE3C2F20C05FA82FA2C" box="[1167,1312,1399,1425]" country="Paraguay" municipality="Chaco" name="Departament" pageId="13" pageNumber="60" stateProvince="Presidente Hayes">Departament</location>
