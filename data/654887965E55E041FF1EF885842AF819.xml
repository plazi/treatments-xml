<taxonomicName id="2AE14D035E55E041FE27F8DE875FF842" authorityName="Speiser" authorityYear="1923" box="[430,595,1905,1929]" class="Insecta" family="Muscidae" genus="Syllimnophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="385" phylum="Arthropoda" rank="genus">
