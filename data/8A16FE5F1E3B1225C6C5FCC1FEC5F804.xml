<taxonomicName id="C5BF34CA1E261225C46DFF0BFD73FF33" box="[575,720,192,217]" class="Rhabditophora" family="Rhynchodemidae" genus="Microplana" kingdom="Animalia" order="Seriata" pageId="18" pageNumber="19" phylum="Platyhelminthes" rank="species" species="terrestris">
