Records of 
<taxonomicName id="4C6F63AEFFE5FFE6FE71FCD9845F3B1E" authorityName="Saito" authorityYear="1977" box="[410,624,849,871]" class="Arachnida" family="Linyphiidae" genus="Porrhomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="ohkawai">
