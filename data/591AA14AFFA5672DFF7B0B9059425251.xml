<subSubSection id="99A943D7FFA5672CFF4E097B59A354D6" pageId="18" pageNumber="93" type="etymology">
<paragraph id="D10C105CFFA5672CFF4E097B59A354D6" blockId="18.[140,1108,377,756]" pageId="18" pageNumber="93">
<emphasis id="E3C7CC4EFFA5672CFF4E097B5910543B" bold="true" box="[185,320,589,615]" pageId="18" pageNumber="93">
<emphasis id="E3C7CC4EFFA5672CFF4E097B596B543B" box="[185,315,589,615]" inLineHeading="true" pageId="18" pageNumber="93" reason="1">Etymology</emphasis>
.
</emphasis>
