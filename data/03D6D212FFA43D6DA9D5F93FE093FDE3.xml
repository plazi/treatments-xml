<taxonomicName id="4C7F1887FFA43D6EAC61F8BBE13EF8A3" authority="de Saussure, 1859" authorityName="de Saussure" authorityYear="1859" class="Diplopoda" family="Polydesmidae" genus="Polydesmus" kingdom="Animalia" order="Polydesmida" pageId="13" pageNumber="520" phylum="Arthropoda" rank="species" species="vermiformis">
