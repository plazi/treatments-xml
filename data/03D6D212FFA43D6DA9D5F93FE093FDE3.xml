<taxonomicName id="4C7F1887FFA73D6DAA23FF18E757FF25" authority="Chamberlin, 1957" authorityName="Chamberlin" authorityYear="1957" box="[865,1145,182,204]" class="Diplopoda" family="Dalodesmidae" genus="Chilorus" higherTaxonomySource="CoL" kingdom="Animalia" order="Polydesmida" pageId="14" pageNumber="521" phylum="Arthropoda" rank="genus">
