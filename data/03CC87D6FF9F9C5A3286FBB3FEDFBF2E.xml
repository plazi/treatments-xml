<taxonomicName id="4C654D43FF9F9C5A3307FBE7FE2DBC09" box="[278,441,1114,1140]" class="Jungermanniopsida" family="Lejeuneaceae" kingdom="Plantae" order="Porellales" pageId="14" pageNumber="15" phylum="Bryophyta" rank="family">Lejeuneaceae</taxonomicName>
