<quantity id="4CA488ACFF8B1E61FB5AFEFEFA84C742" box="[1203,1288,259,280]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="32" pageNumber="406" unit="mm" value="0.4">0.4 mm</quantity>
