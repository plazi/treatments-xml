<quantity id="4CA488ACFF8B1E61FC10FEDCFBC2C76D" box="[1017,1102,289,311]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="32" pageNumber="406" unit="mm" value="0.5">0.5 mm</quantity>
