<taxonomicName id="0BE6D291FF9EA537FDF9FF28C2C6FF21" authority="Waterston" authorityName="Waterston" box="[630,919,229,252]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="1" pageNumber="62" phylum="Arthropoda" rank="species" species="victrix">
