<emphasis id="D8C6EAEDFFCBFFB17625F94BB9DAF8AE" box="[639,920,1786,1810]" italics="true" pageId="3" pageNumber="533">Megarthrus bimaculatus</emphasis>
</taxonomicName>
, 
<taxonomicName id="2DB24D7CFFCBFFB177F1F94ABE51F8AE" box="[939,1043,1786,1810]" class="Insecta" family="Staphylinidae" genus="Megarthrus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="533" phylum="Arthropoda" rank="species" species="lanka" status="sp. n.">
<emphasis id="D8C6EAEDFFCBFFB177F1F94ABE51F8AE" box="[939,1043,1786,1810]" italics="true" pageId="3" pageNumber="533">M. lanka</emphasis>
</taxonomicName>
<emphasis id="D8C6EAEDFFCBFFB17044F94BBE20F8AF" bold="true" box="[1054,1122,1786,1811]" pageId="3" pageNumber="533">
