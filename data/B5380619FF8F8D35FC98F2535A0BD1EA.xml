<collectionCode id="5B802FCAFF888D35FE2DF6B6599DD4EE" box="[444,488,465,486]" collectionName="EAP" pageId="12" pageNumber="755">EAP</collectionCode>
, 
<collectionCode id="5B802FCAFF888D35FE63F6B65A5DD4EE" box="[498,552,465,486]" collectionName="PMA" pageId="12" pageNumber="755">PMA</collectionCode>
, 
<collectionCode id="5B802FCAFF888D35FDA2F6B65A10D4EF" box="[563,613,465,487]" collectionName="PTIS" pageId="12" pageNumber="755">PTIS</collectionCode>
