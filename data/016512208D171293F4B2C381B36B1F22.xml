<taxonomicName id="835CC286BC8241B9C9076F441D979677" authority="(Sep 1762)" authorityName="Linnaeus" authorityYear="1762" baseAuthorityName="Sep" baseAuthorityYear="1762" class="Liliopsida" family="Poaceae" genus="Nardus" kingdom="Plantae" order="Poales" phylum="Tracheophyta" rank="species" species="aristata">
