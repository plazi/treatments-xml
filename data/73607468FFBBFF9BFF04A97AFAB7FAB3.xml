</taxonomicName>
(= 
<taxonomicName id="3CC9BEFDFFBBFF9BFF6AACAAFEE8FB4B" box="[249,359,1215,1241]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="28" pageNumber="29" phylum="Tracheophyta" rank="species" species="durata">
<emphasis id="C9BD196CFFBBFF9BFF6AACAAFEE8FB4B" box="[249,359,1215,1241]" italics="true" pageId="28" pageNumber="29">Q. durata</emphasis>
</taxonomicName>
), 
<taxonomicName id="3CC9BEFDFFBBFF9BFEECACAAFDC8FB4B" authority="Nutt." authorityName="Nutt." box="[383,583,1215,1241]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="28" pageNumber="29" phylum="Tracheophyta" rank="species" species="gambelii">
<emphasis id="C9BD196CFFBBFF9BFEECACAAFD8BFB4B" box="[383,516,1215,1241]" italics="true" pageId="28" pageNumber="29">Q. gambelii</emphasis>
Nutt.
</taxonomicName>
, 
