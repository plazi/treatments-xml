<taxonomicName id="3CC9BEFDFFBBFF9BFF2AAC46FDD0FBFF" authority="(Gillette, 1893)" baseAuthorityName="Gillette" baseAuthorityYear="1893" box="[185,607,1107,1133]" class="Insecta" family="Cynipidae" genus="Disholcaspis" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="rubens">
<emphasis id="C9BD196CFFBBFF9BFF2AAC46FE2EFBFF" box="[185,417,1107,1133]" italics="true" pageId="28" pageNumber="29">Disholcaspis rubens</emphasis>
(Gillette, 1893)
</taxonomicName>
, 
<taxonomicName id="3CC9BEFDFFBBFF9BFDFDAC46FB17FBFF" authority="(Beutenmuller, 1909)" baseAuthorityName="Beutenmuller" baseAuthorityYear="1909" box="[622,1176,1107,1133]" class="Insecta" family="Cynipidae" genus="Disholcaspis" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="eldoradensis">
<emphasis id="C9BD196CFFBBFF9BFDFDAC46FC16FBFF" box="[622,921,1107,1133]" italics="true" pageId="28" pageNumber="29">Disholcaspis eldoradensis</emphasis>
(Beutenmüller, 1909)
</taxonomicName>
and 
<taxonomicName id="3CC9BEFDFFBBFF9BFB45AC46FECEFB03" authority="(Bassett, 1890)" baseAuthorityName="Bassett" baseAuthorityYear="1890" class="Insecta" family="Cynipidae" genus="Biorhiza" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="eburnea">
<emphasis id="C9BD196CFFBBFF9BFB45AC46FA13FBFF" box="[1238,1436,1107,1133]" italics="true" pageId="28" pageNumber="29">Biorhiza eburnea</emphasis>
(Bassett, 1890)
</taxonomicName>
(= 
<taxonomicName id="3CC9BEFDFFBBFF9BFEFAAC62FD6AFB03" authority="Gillette, 1894" authorityName="Gillette" authorityYear="1894" box="[361,741,1143,1169]" class="Insecta" family="Cynipidae" genus="Dryophanta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="glabra">
<emphasis id="C9BD196CFFBBFF9BFEFAAC62FDCDFB03" box="[361,578,1143,1169]" italics="true" pageId="28" pageNumber="29">Dryophanta glabra</emphasis>
