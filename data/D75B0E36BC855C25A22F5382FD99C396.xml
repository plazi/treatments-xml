<taxonomicName id="892CF320E81885AC34032D9F5A069BB3" authorityName="Zhang &amp; Li" authorityYear="2021" class="Arachnida" family="Pimoidae" genus="Pimoa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pimoa danba" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="danba">
