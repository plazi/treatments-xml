<taxonomicName id="75F184ED61FC889743316A87D50B8940" authority="" authorityName="" class="Insecta" family="Carabidae" genus="Moriomorphina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Moriomorphina ambiguus" order="Coleoptera" pageId="0" pageNumber="177" phylum="Arthropoda" rank="species" species="ambiguus">ambiguus</taxonomicName>
