<paragraph id="935AAB19FF971C19FF7ADA9CFA5AF8D7" blockId="1.[151,1437,621,1927]" pageId="1" pageNumber="2">
<keyLead id="2814082CFF971C19FF7ADA9CFA5AF8D7" pageId="1" pageNumber="2">
<number id="252DB93EFF971C19FF7ADA9CFF69F8EF" box="[151,169,1781,1807]" pageId="1" pageNumber="2" value="6.0">6</number>
