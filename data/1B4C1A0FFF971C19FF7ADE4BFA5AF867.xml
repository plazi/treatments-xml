</keyStep>
<keyStep id="2811B3BCFF971C19FF7ADA6CFA5AF907" pageId="1" pageNumber="2">
<paragraph id="935AAB19FF971C19FF7ADA6CFA55F98F" blockId="1.[151,1437,621,1927]" pageId="1" pageNumber="2">
<keyLead id="2814082CFF971C19FF7ADA6CFA55F98F" pageId="1" pageNumber="2">
<number id="252DB93EFF971C19FF7ADA6CFF68F9FF" box="[151,168,1541,1567]" pageId="1" pageNumber="2" value="5.0">5</number>
