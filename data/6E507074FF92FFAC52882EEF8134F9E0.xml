<number id="5031D345FF92FFAC568D2A9287CFFA70" box="[1170,1212,1482,1506]" pageId="13" pageNumber="42" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
inner and 
<number id="5031D345FF92FFAC572D2A928628FA70" box="[1330,1371,1482,1506]" pageId="13" pageNumber="42" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
