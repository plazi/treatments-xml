<taxonomicName id="21F9BAE1FF92FFAC53F92AB68198F994" box="[486,747,1518,1542]" class="Gymnolaemata" family="Candidae" genus="Scrupocellaria" kingdom="Animalia" order="Cheilostomatida" pageId="13" pageNumber="42" phylum="Bryozoa" rank="species" species="sinuosa">
<emphasis id="D48D1D70FF92FFAC53F92AB68198F994" box="[486,747,1518,1542]" italics="true" pageId="13" pageNumber="42">Scrupocellaria sinuosa</emphasis>
</taxonomicName>
is further distinguished from 
<taxonomicName id="21F9BAE1FF92FFAC56202AB787C2F994" box="[1087,1201,1518,1542]" class="Gymnolaemata" family="Candidae" genus="Scrupocellaria" kingdom="Animalia" order="Cheilostomatida" pageId="13" pageNumber="42" phylum="Bryozoa" rank="species" species="hamata" status="n. sp.">
<emphasis id="D48D1D70FF92FFAC56202AB7873EF994" box="[1087,1101,1519,1542]" italics="true" pageId="13" pageNumber="42">S</emphasis>
. 
<emphasis id="D48D1D70FF92FFAC56422AB687C2F994" box="[1117,1201,1518,1542]" italics="true" pageId="13" pageNumber="42">hamata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="CFBEA00BFF92FFAC56A62AB7878AF995" box="[1209,1273,1519,1543]" pageId="13" pageNumber="42" rank="species">
<emphasis id="D48D1D70FF92FFAC56A62AB7878AF995" bold="true" box="[1209,1273,1519,1543]" pageId="13" pageNumber="42">n. sp.</emphasis>
