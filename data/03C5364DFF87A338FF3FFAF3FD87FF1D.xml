<taxonomicName id="4C6CFCD8FF85A33EFD0AF838FC80F82E" authorityName="Moore" authorityYear="1872" box="[674,815,1970,1994]" class="Insecta" family="Arctiidae" genus="Agrisius" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="570" phylum="Arthropoda" rank="species" species="fuliginosus">
