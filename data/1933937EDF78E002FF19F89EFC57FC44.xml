<quantity id="56628F8DDF79E002FF19FC9DFEEFFC98" box="[151,313,836,860]" metricMagnitude="-3" metricUnit="m" metricValue="1.53" metricValueMax="1.54" metricValueMin="1.52" pageId="5" pageNumber="406" unit="mm" value="1.53" valueMax="1.54" valueMin="1.52">1.52–1.54 mm</quantity>
