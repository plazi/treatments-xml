<paragraph id="8BCA6937FFF9FFF6FF3CB76AFC24AE3C" blockId="1.[151,1436,868,1035]" box="[151,1019,868,893]" pageId="1" pageNumber="511">
<emphasis id="B901B525FFF9FFF6FF3CB76AFE03AE3D" bold="true" box="[151,476,868,893]" pageId="1" pageNumber="511">Southwest Pacific: 1198702</emphasis>
, 14/1270, [
<geoCoordinate id="EE410FF0FFF9FFF6FDF0B76AFD66AE3D" box="[603,697,868,893]" direction="south" orientation="latitude" pageId="1" pageNumber="511" precision="925" value="-57.566666">57°34’S</geoCoordinate>
<geoCoordinate id="EE410FF0FFF9FFF6FD6BB76AFCBDAE3D" box="[704,866,868,893]" direction="west" orientation="longitude" pageId="1" pageNumber="511" precision="15" value="-125.10833">125°06’30”W</geoCoordinate>
], 
<quantity id="4C8DC4D2FFF9FFF6FCDEB76AFC28AE3C" box="[885,1015,868,893]" metricMagnitude="2" metricUnit="m" metricValue="8.92" metricValueMax="9.33" metricValueMin="8.51" pageId="1" pageNumber="511" unit="m" value="892.0" valueMax="933.0" valueMin="851.0">851–933 m</quantity>
