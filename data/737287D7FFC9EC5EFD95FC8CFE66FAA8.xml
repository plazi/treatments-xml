(
<bibRefCitation id="9F4A4B30FFC9EC5EFEC3FC06FDF8FC04" author="Wygodzinsky" box="[325,557,958,984]" firstAuthor="Wygodzinsky" pageId="112" pageNumber="113" pagination="367 - 404" refId="ref76681" refString="Wygodzinsky P. 1944. Contribuicao ao conhecimento da familia Campodeidae (Entotrophi, Insecta) do Mexico. Annales de la Escuela nacional de Ciencias biologicas 3: 367 - 404." type="journal article" year="1944">Wygodzinsky 1944</bibRefCitation>
