<collectingRegion id="5D51F8529C44FFEDFA8DF83DFA2BBAC6" box="[1300,1432,1938,1964]" country="Turkey" name="Diyarbakir" pageId="70" pageNumber="71">Diyarbakir</collectingRegion>
, 
<collectingCountry id="E78276209C44FFEDFF0EF815FF5EBABE" box="[151,237,1978,2004]" name="Turkey" pageId="70" pageNumber="71">Turkey</collectingCountry>
