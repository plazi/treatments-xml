<taxonomicName id="83ED4D10FF83FFA3F2F53172FAB4F7EC" authorityName="G.O.Sars" authorityYear="1887" box="[1256,1383,2095,2116]" class="Branchiopoda" family="Cyclestheriidae" genus="Cyclestheria" kingdom="Animalia" order="Diplostraca" pageId="1" pageNumber="216" phylum="Arthropoda" rank="genus">Cyclestheria</taxonomicName>
