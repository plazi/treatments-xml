<number id="3D8924D1A51EFFE4FF36F8D1DC1D3E4C" box="[242,256,1810,1832]" pageId="8" pageNumber="505" value="5.0">5</number>
.
</emphasis>
Figs. 
<number id="3D8924D1A51EFFE4FE8EF8D1DC913E4C" box="[330,396,1810,1832]" pageId="8" pageNumber="505" value="32.5" valueMax="36.0" valueMin="29.0">29–36</number>
, 
<taxonomicName id="4C414D75A51EFFE4FE5EF8D1DF203E4C" box="[410,573,1810,1832]" class="Insecta" family="Reduviidae" genus="Pothea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="505" phylum="Arthropoda" rank="species" species="furtadoi">
