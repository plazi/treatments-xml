<taxonomicName id="4C2B94B3FFB9FFCCD0F9A38007BA5DB9" authority="Brum, 1973" authorityName="Brum" authorityYear="1973" box="[416,905,1708,1731]" class="Malacostraca" family="Pagurapseudidae" genus="Parapagurapseudopsis" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="carinatus">
