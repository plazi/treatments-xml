<subSubSection id="A6F16558BF282A433AE9FDD1FB0EFD19" box="[199,1171,620,645]" pageId="5" pageNumber="438" type="etymology">
<paragraph id="EE5436D3BF282A433AE9FDD1FB0EFD19" blockId="5.[151,1437,151,824]" box="[199,1171,620,645]" pageId="5" pageNumber="438">
<emphasis id="DC9FEAC1BF282A433AE9FDD1FED3FD19" bold="true" box="[199,334,620,645]" pageId="5" pageNumber="438">Etymology.</emphasis>
