<taxonomicName id="7ED4AE9DDC77FFF1ABC7FB88FDBBFBAB" box="[384,539,1044,1065]" class="Insecta" family="Cleridae" genus="Ichnea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="aequinoctialis">
