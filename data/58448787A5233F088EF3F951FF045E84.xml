<taxonomicName id="17ED4D12A5243F088E96FF28FE645EA9" authorityName="Ruppell" box="[242,384,188,213]" class="Aves" family="Picidae" genus="Dendropicos" kingdom="Animalia" order="Piciformes" pageId="60" pageNumber="61" phylum="Chordata" rank="species" species="dinemelli">
