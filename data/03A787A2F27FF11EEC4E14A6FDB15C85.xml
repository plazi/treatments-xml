<collectingCountry id="F3197624F27FF119EBD41283FB4D5876" box="[1166,1228,1263,1285]" name="India" pageId="75" pageNumber="75">India</collectingCountry>
(before the separation of 
<collectingCountry id="F3197624F27FF119ECBF1360FBCF5851" box="[997,1102,1292,1314]" name="Pakistan" pageId="75" pageNumber="75">Pakistan</collectingCountry>
), probably south to the 
<location id="8ED1606FF27FF119EC391346FBA95833" LSID="urn:lsid:plazi:treatment:03A787A2F27FF11EEC4E14A6FDB15C85:8ED1606FF27FF119EC391346FBA95833" box="[867,1064,1322,1344]" country="India" municipality="Hartert" name="Vindhya Range." pageId="75" pageNumber="75" stateProvince="Punjab">Vindhya Range.</location>
<typeStatus id="54B58816F27FF119EB6F1347FB295833" box="[1077,1192,1323,1344]" pageId="75" pageNumber="75">Paratypes</typeStatus>
in 
<collectionCode id="ED1FAE71F27FF119EDE71324FC9B582E" box="[701,794,1352,1373]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="75" pageNumber="75">AMNH</collectionCode>
