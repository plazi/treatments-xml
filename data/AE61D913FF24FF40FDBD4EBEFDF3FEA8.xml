<taxonomicName id="E1C81386FF27FF40FC0F4A68FB8FFE8C" baseAuthorityName="Verhoeff" baseAuthorityYear="1910" box="[930,1026,314,339]" class="Diplopoda" family="Julidae" genus="Ommatoiulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Julida" pageId="134" pageNumber="135" phylum="Arthropoda" rank="species" species="armatus">
