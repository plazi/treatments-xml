<date id="FFD51025FFAE3356FF1AFC0AFECEFC73" box="[186,262,957,982]" pageId="11" pageNumber="404" value="1935-07">
VII. 
<number id="3DA324C2FFAE3356FF49FC0AFECEFC73" box="[233,262,957,982]" pageId="11" pageNumber="404" value="35.0">35</number>
</date>
