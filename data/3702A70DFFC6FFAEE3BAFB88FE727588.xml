<typeStatus id="6010A8B9FFC6FFAFE3BAFB15FF7972EE" box="[151,212,1226,1252]" pageId="4" pageNumber="48">Type</typeStatus>
specimens.
</emphasis>
<typeStatus id="6010A8B9FFC6FFAFE25CFB15FE7372EE" box="[369,478,1226,1252]" pageId="4" pageNumber="48">Holotype</typeStatus>
(ɗ, 
<collectionCode id="D9BA8EDEFFC6FFAFE135FB15FDDF72EE" box="[536,626,1226,1252]" httpUri="http://grbio.org/cool/hj12-2xm7" name="National Museum Prague" pageId="4" pageNumber="48">NMPC</collectionCode>
): “ 
<collectingCountry id="C7BC568BFFC6FFAFE1BEFB15FCAF72EE" box="[659,770,1226,1252]" name="Tanzania" pageId="4" pageNumber="48">Tanzania</collectingCountry>
