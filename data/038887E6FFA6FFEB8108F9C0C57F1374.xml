<taxonomicName id="4C214D73FFA6FFEB81A3F9C2C44C1D83" authority="Charpentier, 1825" authorityName="Charpentier" authorityYear="1825" box="[208,821,1631,1684]" class="Insecta" family="Coenagrionidae" genus="Agrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="21" pageNumber="103" phylum="Arthropoda" rank="species" species="mercuriale">Agrion mercurìale Charp.</taxonomicName>
