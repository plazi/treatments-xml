<paragraph id="FCEF2EF56C3EC4867F059C5922ED61F5" pageId="22" pageNumber="23">
<taxonomicName id="4E09A2DEB49E1D4AFF1022BBB8E47AD7" authority="Maulik, 1931" authorityName="Maulik" authorityYear="1931" class="Insecta" family="Chrysomelidae" genus="Bikasha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bikasha tenuipunctata" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="tenuipunctata">Bikasha tenuipunctata</taxonomicName>
