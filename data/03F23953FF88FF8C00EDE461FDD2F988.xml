<taxonomicName id="4C5BF3C6FF89FF8C01D7E4DEFD55FBB9" authority=", IIE" authorityName="IIE" box="[395,661,1008,1028]" class="Insecta" family="Curculionidae" genus="Conorhynchus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="1042" phylum="Arthropoda" rank="species" species="mendicus">
