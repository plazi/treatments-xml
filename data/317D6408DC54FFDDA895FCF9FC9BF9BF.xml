<collectingCountry id="C1C3958EDC54FFD2A9FAFC58FC56FC5B" box="[957,1014,964,985]" name="Brazil" pageId="7" pageNumber="8">Brazil</collectingCountry>
, 
<collectorName id="1421B0C8DC54FFD2A9B9FC58FB98FC5B" box="[1022,1080,964,985]" pageId="7" pageNumber="8">v. Olf.</collectorName>
