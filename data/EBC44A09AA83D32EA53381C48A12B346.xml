<quantity id="81AB2F0B04BD7275C98EE7EFDC184809" metricMagnitude="-3" metricUnit="m" metricValue="1.0" unit="mm" value="1.0">1 mm</quantity>
(
<emphasis id="26EE178DC479546205C7357FBFD9D724" bold="true" pageId="0" pageNumber="9">B</emphasis>
