<subSubSection id="1A8459E929B05EF3DA47DAE5A37C4C08" pageId="0" pageNumber="9" type="phenology">
<paragraph id="6F698B79E887A0B29E97B765F6B3E3FC" pageId="0" pageNumber="9">Phenology.</paragraph>
<paragraph id="370AA7CB96684B7808811BE9ED2D4E6A" pageId="0" pageNumber="9">December to April.</paragraph>
</subSubSection>
