<emphasis id="05BCEA80FFF3FFDFFF33AA38444EF8D6" bold="true" box="[151,277,1846,1869]" pageId="31" pageNumber="32">FIGURE 11</emphasis>
. 
<taxonomicName id="F0C84D11FFF3FFDFFE86AA394491F8D6" authority="Salazar-Vallejo, 2012" authorityName="Salazar-Vallejo" authorityYear="2012" box="[290,458,1847,1869]" class="Polychaeta" family="Flabelligeridae" genus="Semiodera" kingdom="Animalia" order="Terebellida" pageId="31" pageNumber="32" phylum="Annelida" rank="species" species="nishii" status="n. sp.">
<emphasis id="05BCEA80FFF3FFDFFE86AA394491F8D6" box="[290,458,1847,1869]" italics="true" pageId="31" pageNumber="32">Semiodera nishii</emphasis>
</taxonomicName>
<emphasis id="05BCEA80FFF3FFDFFE74AA394753F8D7" bold="true" box="[464,520,1847,1868]" pageId="31" pageNumber="32">
<taxonomicNameLabel id="1E8F57FBFFF3FFDFFE74AA394753F8D7" box="[464,520,1847,1868]" pageId="31" pageNumber="32" rank="species">n. sp.</taxonomicNameLabel>
