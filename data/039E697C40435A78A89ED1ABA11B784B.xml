<taxonomicName id="4C37A3E940435A78AB92D59BA0817AC8" authority="Thomas 1894" authorityName="Thomas" authorityYear="1894" box="[923,1255,1192,1215]" class="Mammalia" family="Cricetidae" genus="Abrothrix" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="449" phylum="Chordata" rank="species" species="jelskii">
