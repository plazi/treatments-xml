<taxonomicName id="2DA14D11C36AC734E2AC726FFCB1F95D" box="[650,779,1662,1685]" class="Insecta" family="Staphylinidae" genus="Seeversiella" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="genus">Seeversiella</taxonomicName>
<taxonomicName id="2DA14D11C36AC734E333726FFC3BF95D" authority="Gusarov" authorityName="Gusarov" box="[789,897,1662,1685]" pageId="68" pageNumber="69" rank="species" species="lativentris" status="sp. n.">lativentris</taxonomicName>
</emphasis>
</taxonomicName>
Gusarov
</taxonomicName>
, 
<taxonomicNameLabel id="C3E657FBC36AC734E3D3726FFB8FF95E" box="[1013,1077,1662,1686]" pageId="68" pageNumber="69" rank="species">
<emphasis id="D8D5EA80C36AC734E3D3726FFB8FF95E" bold="true" box="[1013,1077,1662,1686]" pageId="68" pageNumber="69">sp. n.</emphasis>
