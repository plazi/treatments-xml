<taxonomicName id="FE06C5790BA55B63978B73979548FD2D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:37027" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Tetramorium guineense (Bernard)" order="Hymenoptera" pageNumber="382" phylum="Arthropoda" rank="species" species="guineense">Tetramorium guineense, Fabr</taxonomicName>
