South-eastern 
<collectingRegion id="9642F802F64CFFD1A401FEF6BF8EFE09" box="[520,655,367,391]" country="Australia" name="Queensland" pageId="65" pageNumber="66">Queensland</collectingRegion>
, 
<collectingCountry id="2C917670F64CFFD1A493FEF6BE05FE09" box="[666,772,367,391]" name="Australia" pageId="65" pageNumber="66">Australia</collectingCountry>
.
