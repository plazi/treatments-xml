<collectingCountry id="F6D6766F6549FF8FFC999937FCC786A6" box="[816,887,884,910]" name="Brazil" pageId="57" pageNumber="58">Brazil</collectingCountry>
and 
<collectingCountry id="F6D6766F6549FF8FFC199937FB9886A6" box="[944,1064,884,910]" name="Argentina" pageId="57" pageNumber="58">Argentina</collectingCountry>
