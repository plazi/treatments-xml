<subSubSection id="C333F097FFEE102AFF56FBDF2587AB96" pageId="28" pageNumber="229" type="materials_examined">
<paragraph id="8B96A31CFFEE102AFF56FBDF2694AB4E" blockId="28.[151,1436,1109,1494]" box="[151,756,1109,1134]" pageId="28" pageNumber="229">
<emphasis id="B95D7F0EFFEE102AFF56FBDF25E0AB4E" bold="true" box="[151,384,1109,1134]" pageId="28" pageNumber="229">Material examined.</emphasis>
CSM, 
<number id="3DE1B13BFFEE102AFE11FBDC25BEAB4E" box="[464,478,1110,1134]" pageId="28" pageNumber="229" value="1.0">1</number>
spc., H = 
<quantity id="4CD10EF9FFEE102AFD93FBDF26C3AB4D" box="[594,675,1109,1134]" metricMagnitude="-2" metricUnit="m" metricValue="6.0" pageId="28" pageNumber="229" unit="mm" value="60.0">60 mm</quantity>
