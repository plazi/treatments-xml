<taxonomicName id="541B6F06F9664BAE4BADB9480728079F" authorityName="Cuatrec" authorityYear="1980" class="Amphibia" family="Asteraceae" genus="Werneria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Werneria crassa subsp. orientalis" order="Asterales" pageId="0" pageNumber="1" phylum="Chordata" rank="subSpecies" species="crassa" subSpecies="orientalis">Werneria crassa subsp. orientalis</taxonomicName>
