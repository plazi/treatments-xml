<taxonomicName id="039D29403CA22F5C31282011E6154067" class="Arachnida" family="Tetracondylidae" genus="Papillocepheus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papillocepheus" order="Sarcoptiformes" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">Papillocepheus</taxonomicName>
