<taxonomicName id="5E95CCAA994D2B6617D54ECF417DCEEA" LSID="http://zoobank.org/CB231C4F-387C-46BE-9C54-0A71399761AD" authority="Ermilov, Anichkin &amp; Tolstikov" class="Arachnida" family="Tetracondylidae" genus="Papillocepheus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papillocepheus primus" order="Sarcoptiformes" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="primus">Papillocepheus primus Ermilov, Anichkin &amp; Tolstikov</taxonomicName>
<taxonomicNameLabel id="9242C51FB83264659E8234C71D260348" pageId="2" pageNumber="3">sp. n.</taxonomicNameLabel>
