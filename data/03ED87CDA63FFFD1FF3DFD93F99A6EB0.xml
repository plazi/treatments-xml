<number id="3D8C24FCA63FFFD1FCFBF97DF8BE6E2F" box="[849,964,1774,1798]" pageId="6" pageNumber="7" value="0.9950000000000001" valueMax="1.1" valueMin="0.89">0.89–1.10</number>
