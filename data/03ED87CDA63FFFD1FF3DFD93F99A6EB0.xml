<taxonomicName id="4C444D58A63FFFD1FCD0F8A4FE176E67" authority="Bidawid &amp; Fittkau, 1995" authorityName="Bidawid &amp; Fittkau" authorityYear="1995" box="[890,1389,1845,1870]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="yavalapiti" subGenus="Tripodura">
