<number id="3D8C24FCA63FFFD1FC65F97DFF7E6E2F" box="[975,1028,1774,1798]" pageId="6" pageNumber="7" value="0.98">0.98</number>
. HV 
<number id="3D8C24FCA63FFFD1FB97F97EFFD56E2F" box="[1085,1199,1773,1798]" pageId="6" pageNumber="7" value="1.935" valueMax="2.11" valueMin="1.76">1.76–2.11</number>
, 
<number id="3D8C24FCA63FFFD1FB10F97DFF956E2F" box="[1210,1263,1774,1798]" pageId="6" pageNumber="7" value="1.93">1.93</number>
