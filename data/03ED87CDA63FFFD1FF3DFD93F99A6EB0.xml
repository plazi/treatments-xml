. Wing length 
<number id="3D8C24FCA63FFFD1FC7CFB88FF336D1D" box="[982,1097,1051,1076]" pageId="6" pageNumber="7" value="1.055" valueMax="1.16" valueMin="0.95">0.95–1.16</number>
, 
<quantity id="4CBC9B3EA63FFFD1FBFCFB88FFC56D1A" box="[1110,1215,1051,1075]" metricMagnitude="-3" metricUnit="m" metricValue="1.07" pageId="6" pageNumber="7" unit="mm" value="1.07">1.07 mm</quantity>
