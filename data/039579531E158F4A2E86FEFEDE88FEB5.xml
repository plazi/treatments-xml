<quantity id="4CC465A01E128F4A2AB8FF4EDFFAFF7E" box="[1193,1277,151,175]" metricMagnitude="-2" metricUnit="m" metricValue="2.3" pageId="12" pageNumber="13" unit="mm" value="23.0">23 mm</quantity>
