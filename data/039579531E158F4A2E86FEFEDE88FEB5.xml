<taxonomicName id="4C3CB3C61E158F4D2F09F8D1D9A3F8CF" authority="Griffin, 1909" authorityName="Griffin" authorityYear="1909" box="[280,676,1800,1822]" class="Reptilia" family="Colubridae" genus="Dendrelaphis" kingdom="Animalia" order="Squamata" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="fuliginosus">
<emphasis id="B94814571E158F4D2F09F8D1D910F8CF" box="[280,535,1800,1822]" italics="true" pageId="11" pageNumber="12">Dendrelaphis fuliginosus</emphasis>
