<taxonomicName id="BC6CBC552AD013A19C3D5FD4C1BC7C44" LSID-ZBK="urn:lsid:zoobank.org:act:BD08FA7B-788B-4285-9548-0F120F97465D" class="Actinopterygii" family="Rivulidae" genus="Simpsonichthys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Simpsonichthys Carvalho 1959:2" order="Cyprinodontiformes" pageNumber="58" phylum="Chordata" rank="genus">Simpsonichthys de Carvalho</taxonomicName>
