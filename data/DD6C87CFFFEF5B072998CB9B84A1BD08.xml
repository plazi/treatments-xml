<taxonomicName id="92C54D5AFFED5B002817CC2287C2BD12" authority="Li &amp; Mo &amp; Wang, 2020" authorityName="Li &amp; Mo &amp; Wang" authorityYear="2020" box="[280,469,1824,1848]" class="Insecta" family="Perlidae" genus="Neoperla" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="576" phylum="Arthropoda" rank="species" species="delphina" status="sp. nov.">
