<subSubSection id="1DDF6552FFEA5B0729C8CD5782D0BC45" box="[199,1223,1620,1647]" pageId="4" pageNumber="577" type="etymology">
<paragraph id="557A36D9FFEA5B0729C8CD5782D0BC45" blockId="4.[151,1437,1116,1826]" box="[199,1223,1620,1647]" pageId="4" pageNumber="577">
<emphasis id="67B1EACBFFEA5B0729C8CD57875FBC45" bold="true" box="[199,328,1621,1647]" pageId="4" pageNumber="577">Etymology</emphasis>
