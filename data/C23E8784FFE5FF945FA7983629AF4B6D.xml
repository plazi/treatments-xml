<emphasis id="78E3EA80FFE4FF945FA79E5B2B174EF2" bold="true" box="[192,333,371,397]" pageId="53" pageNumber="52">Host plants.</emphasis>
<taxonomicName id="8D974D11FFE4FF945E339E5A28464EF2" authority="Desv." authorityName="Desv." box="[340,540,370,397]" class="Magnoliopsida" family="Fabaceae" genus="Desmodium" kingdom="Plantae" order="Fabales" pageId="53" pageNumber="52" phylum="Tracheophyta" rank="genus">
<emphasis id="78E3EA80FFE4FF945E339E5A2B834EF3" box="[340,473,370,396]" italics="true" pageId="53" pageNumber="52">Desmodium</emphasis>
Desv.
</taxonomicName>
spp. (
<taxonomicName id="8D974D11FFE4FF945D3B9E5A28924EF2" box="[604,712,370,397]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="53" pageNumber="52" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
