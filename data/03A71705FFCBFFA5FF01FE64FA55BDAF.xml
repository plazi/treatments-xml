<heading id="D0F9117FFFCBFFA5FF01FE64FE74BF8A" bold="true" box="[151,445,438,463]" fontSize="10" level="2" pageId="1" pageNumber="568" reason="2">
<taxonomicName id="4C0EDD90FFCBFFA5FF01FE64FEA1BF8A" authority="García Aldrete &amp; Manchola &amp; Obando, 2014" authorityName="García Aldrete &amp; Manchola &amp; Obando" authorityYear="2014" box="[151,360,438,463]" class="Insecta" family="Lachesillidae" genus="Acantholachesilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="1" pageNumber="568" phylum="Arthropoda" rank="genus" status="gen. n.">Acantholachesilla</taxonomicName>
