<taxonomicName id="D6CF4B8EFFDEFFAE9EE1FF5AFD89FF10" authorityName="Ito" authorityYear="2020" box="[472,528,224,250]" class="Insecta" family="Hydroptilidae" genus="Stactobiella" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="566" phylum="Arthropoda" rank="species" species="aichi">
