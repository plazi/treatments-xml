<taxonomicName id="5CBD7585FFACFFCEFBB2FEECF0499D3A" authority="Sterneck, 1928" authorityName="Sterneck" authorityYear="1928" box="[1026,1360,295,321]" class="Insecta" family="Geometridae" genus="Scardostrenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="genus">
