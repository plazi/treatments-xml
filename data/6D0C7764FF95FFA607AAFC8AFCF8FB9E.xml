<taxonomicName id="22A5BDF1FF95FFA60383FC1DFA9FFBD0" authorityName="Blackwall" authorityYear="1841" box="[1214,1330,1017,1044]" class="Arachnida" family="Salticidae" kingdom="Animalia" order="Araneae" pageId="38" pageNumber="39" phylum="Arthropoda" rank="family">Salticidae</taxonomicName>
