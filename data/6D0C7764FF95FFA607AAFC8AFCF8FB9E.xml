<taxonomicName id="22A5BDF1FF95FFA60330FC1EFB2BFBD7" authorityName="Peckham &amp; Peckham" authorityYear="1896" box="[1037,1158,1018,1043]" class="Arachnida" family="Salticidae" genus="Parnaenus" kingdom="Animalia" order="Araneae" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
