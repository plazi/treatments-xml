<paragraph id="8BAC1438F841FFDF70C0FADF78E9588C" blockId="15.[151,1437,867,1434]" pageId="15" pageNumber="16">
<taxonomicName id="4C136FBBF841FFDF70C0FADF7DD55863" authorityName="Goodwin" authorityYear="1958" box="[199,456,1371,1397]" class="Mammalia" family="Phyllostomidae" genus="Chiroderma" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="trinitatum">
