<taxonomicName id="4C471294BE11FFCEFEF9FD76FD858CAC" authority="Lu &amp; Li &amp; Chen, 2019" authorityName="Lu &amp; Li &amp; Chen" authorityYear="2019" box="[283,548,735,759]" class="Insecta" family="Cerambycidae" genus="Trachylophus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="583" phylum="Arthropoda" rank="species" species="tianmuensis" status="sp. nov.">
