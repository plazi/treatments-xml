<taxonomicName id="749F4D7EBF14D159FE60FF661E0E5FD0" authority="Murina" authorityName="Murina" box="[450,770,190,216]" class="Sipunculidea" family="Sipunculidae" genus="Sipunculus" kingdom="Animalia" order="Sipunculiformes" pageId="2" pageNumber="53" phylum="Sipuncula" rank="species" species="lomonossovi" subGenus="Sipunculus">
