<taxonomicName id="4C7C4D67FFCA4C01FAA23024FB9CFA47" authority="Thomas, 1915" authorityName="Thomas" authorityYear="1915" class="Mammalia" family="Emballonuridae" genus="Taphozous" kingdom="Animalia" order="Chiroptera" pageId="5" pageNumber="354" phylum="Chordata" rank="species" species="achates">
