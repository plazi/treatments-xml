7. 
<taxonomicName id="AE017650E75C46245CFDDFA63BD6DE72" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2414" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus Mayr" order="Hymenoptera" pageNumber="37" phylum="Arthropoda" rank="genus" status="n. sp.">CAMPONOTUS</taxonomicName>
ELLIOTI, 
