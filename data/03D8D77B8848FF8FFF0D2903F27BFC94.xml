<taxonomicName id="4C711DEE8848FF8CFEC129E9F7FCFA52" authority="Osgood, 1913" authorityName=", Osgood" authorityYear="1913" box="[260,494,1465,1489]" class="Mammalia" family="Cricetidae" genus="Oryzomys" kingdom="Animalia" order="Rodentia" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="polius">
