<taxonomicName id="4D207A49A14079FD52456DF8FC5F4788" authority="A. Kerner" authorityName="A. Kerner" class="Insecta" family="Eulophidae" genus="Thymus" kingdom="Animalia" order="Hymenoptera" pageId="null" pageNumber="149" phylum="Arthropoda" rank="species" species="polytrichus">
