(
<taxonomicName id="CFF1E888918DD8F300134FF767278480" class="Insecta" family="Eulophidae" genus="Thymus" kingdom="Animalia" order="Hymenoptera" pageId="null" pageNumber="149" phylum="Arthropoda" rank="subSpecies" species="praecox" subSpecies="polytrichus">
