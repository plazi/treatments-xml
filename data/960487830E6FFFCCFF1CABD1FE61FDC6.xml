<collectingCountry id="66BA76050E6FFFCDFE38AA4EFE05FE83" box="[433,504,350,366]" name="Hungary" pageId="84" pageNumber="225">Hungary</collectingCountry>
, 
<location id="1B72604E0E6FFFCDFE76AA4EFD8DFE83" LSID="urn:lsid:plazi:treatment:960487830E6FFFCCFF1CABD1FE61FDC6:1B72604E0E6FFFCDFE76AA4EFD8DFE83" box="[511,624,350,366]" country="Hungary" municipality="Fluss Garam" name="Garamveszele" pageId="84" pageNumber="225">Garamveszele</location>
