<taxonomicName id="238F6281FF931478DAA17C826ECDF6EA" authorityName="Mclean" authorityYear="2012" box="[907,963,986,1010]" class="Gastropoda" family="Colloniidae" genus="Liotipoma" kingdom="Animalia" order="Trochida" pageId="23" pageNumber="366" phylum="Mollusca" rank="species" species="clausa">
