<paragraph id="8BE371591935AE7905B8FD27173D8F40" blockId="9.[151,1437,727,1222]" pageId="9" pageNumber="443">
<emphasis id="B928AD4B1935AE7905B8FD2717768F0B" bold="true" box="[199,355,763,789]" pageId="9" pageNumber="443">Type locality.</emphasis>
<materialsCitation id="3B347B041935AE790417FD2717308F40" ID-GBIF-Occurrence="3071280301" collectorName="Unit E" country="Grenada" latitude="12.14573" location="Grenada" longLatPrecision="1" longitude="-61.613846" pageId="9" pageNumber="443" specimenCount="1">
