<taxonomicName id="4C77F7F0F44DC62D907DA30B49EC385C" authorityName="Grube" authorityYear="1863" box="[305,503,1118,1142]" class="Polychaeta" family="Polynoidae" genus="Polynoe" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="17" pageNumber="1904" phylum="Annelida" rank="species" species="longisetis">
