<quantity id="4CEF9B25FFF8FFDC9BF9FE45F99F31D4" box="[900,990,439,465]" metricMagnitude="-1" metricUnit="m" metricValue="4.83" pageId="105" pageNumber="106" unit="cm" value="48.3">48.3 cm</quantity>
