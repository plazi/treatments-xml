<quantity id="59729B48FF98D067FBF7FB9CFB4CFB8D" box="[1126,1214,1119,1145]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" pageId="1" pageNumber="26" unit="mm" value="15.0">15 mm</quantity>
