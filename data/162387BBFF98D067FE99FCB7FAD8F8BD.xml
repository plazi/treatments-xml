<quantity id="59729B48FF98D067FA9EFB9CFE90FB54" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="1" pageNumber="26" unit="mm" value="11.0">
11 
<emphasis id="ACFEEABFFF98D067FEA9FB44FE90FB54" box="[312,354,1159,1184]" italics="true" pageId="1" pageNumber="26">mm</emphasis>
