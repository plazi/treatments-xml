<quantity id="4CE1088CFFF8FFD277DB6B7E46EAFEEE" box="[384,472,353,380]" metricMagnitude="3" metricUnit="m" metricValue="1.921" pageId="22" pageNumber="23" unit="m" value="1921.0">1921 m</quantity>
