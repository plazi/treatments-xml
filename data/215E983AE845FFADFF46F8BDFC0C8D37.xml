<emphasis id="9B83F53EE848FFAFFF46FCECFE828F3D" bold="true" box="[151,281,846,870]" pageId="8" pageNumber="39">FIGURE 7.</emphasis>
<taxonomicName id="6EF752AFE848FFAFFEF1FCEDFE268F3D" authority="Stoddart &amp; Lowry, 2010" authorityName="Stoddart &amp; Lowry" authorityYear="2010" box="[288,445,847,870]" class="Malacostraca" family="Aristiidae" genus="Aristias" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="39" phylum="Arthropoda" rank="species" species="nowra" status="sp. nov.">
<emphasis id="9B83F53EE848FFAFFEF1FCEDFE268F3D" box="[288,445,847,870]" italics="true" pageId="8" pageNumber="39">Aristias nowra</emphasis>
</taxonomicName>
<emphasis id="9B83F53EE848FFAFFE15FCECFD818F3D" bold="true" box="[452,538,846,870]" pageId="8" pageNumber="39">
<taxonomicNameLabel id="80B04845E848FFAFFE15FCECFD818F3D" box="[452,538,846,870]" pageId="8" pageNumber="39" rank="species">sp. nov.</taxonomicNameLabel>
