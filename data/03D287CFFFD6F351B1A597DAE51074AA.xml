<taxonomicName id="4C7B4D5AFFD6F351B0D89663E49F7027" box="[493,673,289,314]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="146" phylum="Tracheophyta" rank="species" species="spinosa">
