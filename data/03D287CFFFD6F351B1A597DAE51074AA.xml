<taxonomicName id="4C7B4D5AFFD6F351B3BA9627E5117063" baseAuthorityName="Egger" baseAuthorityYear="1858" box="[655,815,356,383]" class="Insecta" family="Syrphidae" genus="Criorhina" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="146" phylum="Arthropoda" rank="species" species="pachymera">
