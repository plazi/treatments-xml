</taxonomicName>
et de 
<taxonomicName id="4C7B4D5AFFD6F351B2449627E23D7063" baseAuthorityName="Panzer" baseAuthorityYear="1804" box="[881,1027,356,383]" class="Insecta" family="Syrphidae" genus="Criorhina" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="146" phylum="Arthropoda" rank="species" species="ranunculi">
