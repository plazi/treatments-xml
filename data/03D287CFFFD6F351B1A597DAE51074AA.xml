(
<taxonomicName id="4C7B4D5AFFD6F351B02B97BCE4127004" authority=", Apoidea" authorityName="Apoidea" box="[286,556,254,281]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="146" phylum="Arthropoda" rank="order">Hymenoptera, Apoidea</taxonomicName>
