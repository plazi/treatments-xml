<taxonomicName id="4C1E4D4C01205F69FBE746516C84FA8B" authorityName="Álvarez-Padilla &amp; Ubick &amp; Griswold" authorityYear="2012" box="[1068,1181,1379,1405]" class="Arachnida" family="Oonopidae" genus="Tolegnaro" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
