<taxonomicName id="4C1E4D4C01235F6AFB9F42076CD1FEB8" authorityName="Álvarez-Padilla &amp; Ubick &amp; Griswold" authorityYear="2012" baseAuthorityName="Saaristo" baseAuthorityYear="2001" box="[1108,1224,309,334]" class="Arachnida" family="Oonopidae" genus="Noideattella" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="assumptia">
