<quantity id="4CE69B2A01235F6AFD7540B66B7AFC69" box="[702,867,900,927]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" metricValueMax="2.4" metricValueMin="2.1" pageId="10" pageNumber="11" unit="mm" value="2.25" valueMax="2.4" valueMin="2.1">2.1 to 2.4 mm</quantity>
