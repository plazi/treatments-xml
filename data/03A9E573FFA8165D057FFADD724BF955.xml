<taxonomicName id="4C002FE6FFA8165D057FFADD71DEFAA3" authority="Bocage, 1873" authorityName="Bocage" authorityYear="1873" box="[264,737,1342,1368]" class="Reptilia" family="Gekkonidae" genus="Lygodactylus" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="5" phylum="Chordata" rank="species" species="gutturalis">
