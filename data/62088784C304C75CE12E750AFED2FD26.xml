<taxonomicName id="2DA14D11C302C75CE23176BDFD4AFD0D" box="[535,752,684,709]" class="Insecta" family="Staphylinidae" genus="Seeversiella" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="badia">
