<subSubSection id="EB6CD95F53ADDC541EA92EEF48448807" pageId="1" pageNumber="2" type="reference_group">
<paragraph id="F8C3A36A514F965BF484A189747AE109" pageId="1" pageNumber="2">
<taxonomicName id="86A2780A6688B844D68F098DD0254E48" authority="" authorityName="" class="Insecta" family="Tenthredinidae" genus="Nipponorhynchus" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Nipponorhynchus bimaculatus" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="bimaculatus">Nipponorhynchus bimaculatus</taxonomicName>
