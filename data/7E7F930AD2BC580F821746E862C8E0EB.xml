</td>
<td id="7111278E9A59979C7A1D3ABFFB6075E3" align="left" pageId="null" pageNumber="317" valign="bottom">
<taxonomicName id="E22E49F21E9CBC6060D96907ABA61951" class="Liliopsida" family="Poaceae" genus="Koeleria" kingdom="Plantae" order="Poales" pageId="null" pageNumber="317" phylum="Tracheophyta" rank="species" species="brevifolia">
<italic id="7297E8D8CE7F6BFC1C2C1BBC0475E624" pageId="null" pageNumber="317">K. brevifolia</italic>
</taxonomicName>
