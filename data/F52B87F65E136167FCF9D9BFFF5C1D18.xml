<taxonomicName id="BA824D635E136164FC87DD50FBBE1E18" authorityName="FABRICIUS" authorityYear="1804" box="[846,1071,1809,1830]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="170" phylum="Arthropoda" rank="species" species="truncorum">
