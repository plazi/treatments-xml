<taxonomicName id="C39D4D294855044FF48754FAAF6BFBBF" box="[994,1124,1004,1030]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="42" phylum="Arthropoda" rank="species" species="eichleri">
<emphasis id="36E9EAB84855044FF48754FAA8F8FBBF" box="[994,1015,1004,1029]" italics="true" pageId="3" pageNumber="42">H</emphasis>
