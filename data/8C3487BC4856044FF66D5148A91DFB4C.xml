</taxonomicName>
, 
<taxonomicName id="C39D4D294855044FF31C54FAAFF6FBBF" authorityName="Janssens" authorityYear="1981" box="[1145,1273,1004,1030]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="42" phylum="Arthropoda" rank="species" species="persica">
