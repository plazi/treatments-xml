54. 
<taxonomicName id="4C4A9AB999258557E8D1FC4FD71FF3A5" authority="Verrall, 1875" authorityName="Verrall" authorityYear="1875" box="[193,590,944,971]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="strigipes">
