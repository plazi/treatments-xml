<taxonomicName id="4C5B62C05F1AFFBFFDB8FF62E82C58DD" authorityName="Furgol" authorityYear="2017" box="[514,627,223,249]" class="Entognatha" family="Entomobryidae" genus="Entomobrya" kingdom="Animalia" order="Collembola" pageId="9" pageNumber="128" phylum="Arthropoda" rank="species" species="emiliae">
