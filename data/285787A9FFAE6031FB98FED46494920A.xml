<paragraph id="A04136BFFFAE6031FBADFE656494920A" blockId="50.[136,1452,338,581]" pageId="50" pageNumber="51">
<typeStatus id="7F45881DFFAE6031FBADFE65672391B2" box="[189,301,483,509]" pageId="50" pageNumber="51">Lectotype</typeStatus>
