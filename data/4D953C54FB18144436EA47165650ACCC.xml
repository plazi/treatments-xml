<taxonomicName id="A15CF6F23AF7042E8EFA243DCD6E6D92" genus="Cyrtoquediina" lsidName="Cyrtoquediina fukiensis" pageId="49" pageNumber="50" rank="species" species="fukiensis">fukiensis</taxonomicName>
