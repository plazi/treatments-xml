<emphasis id="70E46F35F9424C130CDC30F4A29B1899" italics="true" pageId="12" pageNumber="479">
<taxonomicName id="7F76C5E07311FBB2169A4E83648318EA" class="Aves" family="Fringillidae" genus="Loxia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loxia fuscata" order="Passeriformes" pageId="12" pageNumber="479" phylum="Chordata" rank="species" species="fuscata">Loxia fuscata</taxonomicName>
, La 
<taxonomicName id="C5316546294E1EB09369194E543D0B24" authorityName="Reichenbach" authorityYear="1850" class="Aves" family="Estrildidae" genus="Padda" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Padda" order="Passeriformes" pageId="12" pageNumber="479" phylum="Chordata" rank="genus">Padda</taxonomicName>
