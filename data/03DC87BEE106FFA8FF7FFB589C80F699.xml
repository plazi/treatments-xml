<taxonomicName id="4C754D2BE104FFA8FB4EFB649A7AF608" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[1190,1291,1267,1293]" class="Insecta" family="Melolonthidae" genus="Amiserica" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="incisa">
