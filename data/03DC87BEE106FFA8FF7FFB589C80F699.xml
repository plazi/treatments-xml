<taxonomicName id="4C754D2BE104FFA8FD94FAC89B5EF67D" authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[636,1071,1375,1401]" class="Insecta" family="Melolonthidae" genus="Amiserica" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="funiushanica" status="new species">
