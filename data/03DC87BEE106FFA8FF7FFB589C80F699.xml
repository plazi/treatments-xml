<quantity id="4C8D9B4DE104FFA8FE4CFB589D5CF7ED" box="[420,557,1231,1257]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" metricValueMax="7.6" metricValueMin="6.4" pageId="27" pageNumber="28" unit="mm" value="7.0" valueMax="7.6" valueMin="6.4">6.4–7.6 mm</quantity>
, length of elytra: 5.0– 
<quantity id="4C8D9B4DE104FFA8FCCCFB599C0AF7ED" box="[804,891,1230,1257]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="27" pageNumber="28" unit="mm" value="5.9">5.9 mm</quantity>
, width: 
<quantity id="4C8D9B4DE104FFA8FC3DFB589B2EF7ED" box="[981,1119,1231,1257]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" metricValueMax="4.2" metricValueMin="3.2" pageId="27" pageNumber="28" unit="mm" value="3.7" valueMax="4.2" valueMin="3.2">3.2–4.2 mm</quantity>
