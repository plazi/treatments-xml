<taxonomicName id="4C754D2BE104FFA8FF7FFA809E28F635" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[151,345,1303,1329]" class="Insecta" family="Melolonthidae" genus="Amiserica" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="30" phylum="Arthropoda" rank="species" species="fengyangensis">
