<caption id="4C8BA787FFCCFFE8FF726F65FBA9F940" ID-DOI="http://doi.org/10.5281/zenodo.5100495" ID-Zenodo-Dep="5100495" httpUri="https://zenodo.org/record/5100495/files/figure.png" pageId="1" pageNumber="30" startId="1.[151,231,1651,1672]" subCaptionStartIDs="1.[561,602,1709,1731]" subCaptionStarts="Figs 3" targetBox="[356,1230,717,1625]" targetPageId="1">
