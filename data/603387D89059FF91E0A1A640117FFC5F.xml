<taxonomicName id="2F9A4D4D9059FF91E0A1A640170EFD31" authority="Chan &amp; Yu, 2000" authorityName="Chan &amp; Yu" authorityYear="2000" box="[151,519,618,644]" class="Malacostraca" family="Pandalidae" genus="Pandalus" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="589" phylum="Arthropoda" rank="species" species="taiwanica">
