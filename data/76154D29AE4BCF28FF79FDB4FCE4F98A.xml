<collectingMunicipality id="1E676645AE4BCF28FB5AFBD3FAB8FB9A" box="[1204,1295,1031,1057]" pageId="20" pageNumber="270">Western</collectingMunicipality>
edge of 
<location id="FB63AAE4AE4BCF28FA9AFBD3FEE3FBFF" LSID="urn:lsid:plazi:treatment:76154D29AE4BCF28FF79FDB4FCE4F98A:FB63AAE4AE4BCF28FA9AFBD3FEE3FBFF" county="Voskresensk Region" municipality="Western" name="Beloozerskiy Town" pageId="20" pageNumber="270" stateProvince="Moscow">Beloozerskiy Town</location>
