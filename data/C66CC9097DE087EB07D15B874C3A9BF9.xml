Tribe 
<taxonomicName id="6C7884645D8EDE4467248234320C9B24" authority="Schoenherr, 1823" authorityName="Schoenherr" authorityYear="1823" lsidName="" pageId="266" pageNumber="267" rank="tribe" tribe="Polydrusini">
