<taxonomicName id="4C2BC29DFFF2FFADFBFB5A9E5381F916" box="[1114,1237,1753,1776]" class="Insecta" family="Hydraenidae" genus="Hughleechia" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="24" phylum="Arthropoda" rank="species" species="giulianii">
<emphasis id="B95F650CFFF2FFADFBFB5A9E5381F916" box="[1114,1237,1753,1776]" italics="true" pageId="7" pageNumber="24">
H. 
<taxonomicName id="4C2BC29DFFF2FFADFBDA5A9E5381F916" box="[1147,1237,1753,1776]" pageId="7" pageNumber="24" rank="species" species="giulianii">giulianii</taxonomicName>
</emphasis>
</taxonomicName>
. —6. 
<taxonomicName id="4C2BC29DFFF2FFADFA855A9E52C1F916" box="[1316,1429,1753,1776]" class="Insecta" family="Hydraenidae" genus="Hughleechia" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="24" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis id="B95F650CFFF2FFADFA855A9E52C1F916" box="[1316,1429,1753,1776]" italics="true" pageId="7" pageNumber="24">
