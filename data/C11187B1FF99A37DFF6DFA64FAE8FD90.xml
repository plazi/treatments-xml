<number id="FF702480FF98A37DFEF4FE8BFEE7FEA0" box="[270,329,290,313]" pageId="3" pageNumber="68" value="2009.0">2009</number>
</date>
(
<collectionCode id="2FA9AE62FF98A37DFEACFE8BFE38FEA0" box="[342,406,290,313]" pageId="3" pageNumber="68">AQIS</collectionCode>
