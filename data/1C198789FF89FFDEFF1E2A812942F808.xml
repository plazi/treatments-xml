. 
<taxonomicName id="53B04D1CFF88FFDEFE922AD72E02F8B3" authority="Nurse" authorityName="Nurse" box="[283,573,1785,1807]" class="Insecta" family="Apidae" genus="Lisotrigona" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="408" phylum="Arthropoda" rank="species" species="cacciae">
