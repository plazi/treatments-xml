<taxonomicName id="5AAC59A2FFB5FF0F9853FDCCFD05FDB9" box="[538,752,522,547]" class="Actinopterygii" family="Macrouridae" genus="Nezumia" kingdom="Animalia" order="Gadiformes" pageId="12" pageNumber="37" phylum="Chordata" rank="species" species="atlantica">
