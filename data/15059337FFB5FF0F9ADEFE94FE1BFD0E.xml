<collectingCountry id="E5BB62B1FFB5FF0F99F8FDF6FBFBFDD0" box="[945,1038,560,586]" name="Mexico" pageId="12" pageNumber="37">Mexico</collectingCountry>
(
<geoCoordinate id="F89844E6FFB5FF0F9E68FDF6FB62FDD0" box="[1057,1175,560,586]" direction="north" orientation="latitude" pageId="12" pageNumber="37" precision="925" value="27.8">
<number id="2B643006FFB5FF0F9E68FDF6FBB6FDD0" box="[1057,1091,560,586]" pageId="12" pageNumber="37" value="27.0">27</number>
° 
<number id="2B643006FFB5FF0F9E04FDF6FB85FDD0" box="[1101,1136,560,586]" pageId="12" pageNumber="37" value="48.0">48</number>
’ N
</geoCoordinate>
, 
<geoCoordinate id="F89844E6FFB5FF0F9EECFDF6FAD7FDD0" box="[1189,1314,560,586]" direction="west" orientation="longitude" pageId="12" pageNumber="37" precision="925" value="-89.933334">
<number id="2B643006FFB5FF0F9EECFDF6FB33FDD0" box="[1189,1222,560,586]" pageId="12" pageNumber="37" value="89.0">89</number>
° 
<number id="2B643006FFB5FF0F9E98FDF6FB01FDD0" box="[1233,1268,560,586]" pageId="12" pageNumber="37" value="56.0">56</number>
