specimen:
</emphasis>
<specimenCode id="86479ED1FFA9FFEFFECCF892C69FF8CA" box="[372,485,1850,1872]" collectionCode="ROM" country="Canada" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34778" lsid="urn:lsid:biocol.org:col:34778" name="Royal Ontario Museum" pageId="1" pageNumber="911" type="Museum">ROM 796</specimenCode>
