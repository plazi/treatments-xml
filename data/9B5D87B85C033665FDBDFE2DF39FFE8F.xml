<collectingCountry id="6BE3763E5C033665FD7BFA22F317FEAB" box="[721,803,1464,1485]" name="Ethiopia" pageId="5" pageNumber="14">Ethiopia</collectingCountry>
, 
<collectingCountry id="6BE3763E5C033665FC81FA22F3B0FEAB" box="[811,900,1464,1485]" name="Tanzania" pageId="5" pageNumber="14">Tanzania</collectingCountry>
, 
<collectingCountry id="6BE3763E5C033665FC21FA22F3E3FEAB" box="[907,983,1464,1485]" name="Uganda" pageId="5" pageNumber="14">Uganda</collectingCountry>
, 
<collectingCountry id="6BE3763E5C033665FC75FA22F427FEAB" box="[991,1043,1464,1485]" name="Democratic Republic of the Congo" pageId="5" pageNumber="14">Zaire</collectingCountry>
