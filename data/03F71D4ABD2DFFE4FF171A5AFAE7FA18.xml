<quantity id="4CA601B9BD2DFFE4FC9A1A8DFCB6FBD7" box="[794,883,1010,1034]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="5" pageNumber="366" unit="mm" value="4.4">4.4 mm</quantity>
