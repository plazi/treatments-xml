<quantity id="4CA601B9BD2DFFE4FDAA1A8DFD71FBD7" box="[554,692,1010,1034]" metricMagnitude="-3" metricUnit="m" metricValue="4.05" metricValueMax="4.3" metricValueMin="3.8" pageId="5" pageNumber="366" unit="mm" value="4.05" valueMax="4.3" valueMin="3.8">3.8–4.3 mm</quantity>
