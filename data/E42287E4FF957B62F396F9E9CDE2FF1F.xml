<taxonomicName id="AB8B4D71FF957B63F603F892C943F8D3" box="[1282,1352,1822,1847]" class="Malacostraca" family="Aeglidae" genus="Aegla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="33" phylum="Arthropoda" rank="genus">
