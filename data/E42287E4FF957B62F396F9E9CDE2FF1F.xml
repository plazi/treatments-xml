<taxonomicName id="AB8B4D71FF957B63F180F802CF0CF843" box="[641,775,1934,1959]" class="Malacostraca" family="Aeglidae" genus="Aegla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="montana">
