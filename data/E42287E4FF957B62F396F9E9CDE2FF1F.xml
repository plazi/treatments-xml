<number id="DA4324D5FF957B63F1FAF8CFCF21F8B9" box="[763,810,1859,1885]" pageId="2" pageNumber="33" value="231.0">231</number>
)
</bibRefCitation>
described the male 
<typeStatus id="B3308850FF957B63F729F8CFC899F8B9" box="[1064,1170,1859,1885]" pageId="2" pageNumber="33">holotype</typeStatus>
specimen and 
<specimenCount id="7A8DFD7BFF957B63F64BF8CFC996F8B9" box="[1354,1437,1859,1885]" pageId="2" pageNumber="33" type="male">
<number id="DA4324D5FF957B63F64BF8CFC952F8B9" box="[1354,1369,1859,1885]" pageId="2" pageNumber="33" value="4.0">4</number>
male
</specimenCount>
<typeStatus id="B3308850FF957B63F396F8E4CD05F866" box="[151,270,1896,1922]" pageId="2" pageNumber="33">paratypes</typeStatus>
