<subSubSection id="61FB6ABC7296FAB2927D5492F3B8A0B7" pageId="132" pageNumber="133" type="names">
<paragraph id="60B91C13D219198E0B2657C94047C4C6" pageId="132" pageNumber="133">Names.</paragraph>
<paragraph id="4BF1D7F8170EDEB58099DA1CF33ACB13" pageId="132" pageNumber="133">
<emphasis id="49EFDEBF6841FD743BB247BF7BD7DA6F" bold="true" pageId="132" pageNumber="133">Myanmar</emphasis>
: 
<emphasis id="7B98CB6C7AE0D4BBE76C8BC4C5357FCA" italics="true" pageId="132" pageNumber="133">aukkyu</emphasis>
, 
<emphasis id="ECC1D80E51CEDECB9B67152D71BEB8A0" italics="true" pageId="132" pageNumber="133">aukkywe</emphasis>
, 
<emphasis id="59E53D7F2D34A09FB9A3CF7A8BA0F5FD" italics="true" pageId="132" pageNumber="133">hka-shatawi</emphasis>
, 
<emphasis id="32F2C165263BB1FF902E43F5A4522477" italics="true" pageId="132" pageNumber="133">kywe-tho</emphasis>
, 
<emphasis id="C70B546EB14BF83637F49C92BF831AE8" italics="true" pageId="132" pageNumber="133">po-gaungsa</emphasis>
, 
<emphasis id="518B1A16873F96E979E7EC24D0268ED7" italics="true" pageId="132" pageNumber="133">tayok-the</emphasis>
, 
<emphasis id="B4A4528844556B8A9C2AF3300067D2D8" italics="true" pageId="132" pageNumber="133">ye-padauk</emphasis>
, 
<emphasis id="0A5E665361DF2154D8FA93B977C63048" italics="true" pageId="132" pageNumber="133">yepadon</emphasis>
. 
<emphasis id="D8DBC3170DF00CE7E97C49AAD4A0A338" bold="true" pageId="132" pageNumber="133">English</emphasis>
