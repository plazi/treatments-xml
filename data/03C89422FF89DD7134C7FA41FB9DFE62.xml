<location id="8EBE73EFFF89DD703789F9A9FBB4F9F4" LSID="urn:lsid:plazi:treatment:03C89422FF89DD7134C7FA41FB9DFE62:8EBE73EFFF89DD703789F9A9FBB4F9F4" box="[985,1030,1610,1636]" country="Peru" county="Pacific" latitude="-11.333333" longLatPrecision="1295" longitude="-88.916664" municipality="The" name="The" pageId="88" pageNumber="89">The</location>
