<elevation id="0062D1DD297FFF8A7BF612019DE2FAF5" box="[445,509,1374,1400]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="56" pageNumber="57" unit="m" value="30.0">30 m</elevation>
</quantity>
, 
<date id="FFF1102E297FFF8A784A12019E61FAF5" box="[513,638,1374,1400]" pageId="56" pageNumber="57" value="1986-02-10">
<collectingDate id="EFB5E9C6297FFF8A784A12019E61FAF5" box="[513,638,1374,1400]" pageId="56" pageNumber="57" value="1986-02-10">10.II.1986</collectingDate>
