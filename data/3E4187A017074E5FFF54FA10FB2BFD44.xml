<taxonomicName id="71E84D3517054E5FFC53FD4FFB99FD44" authorityName="Jennings &amp; Parslow &amp; Austin" authorityYear="2018" box="[912,1033,656,680]" class="Insecta" family="Curculionidae" genus="Aulacus" kingdom="Animalia" order="Coleoptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="pallidus">
<emphasis id="849CEAA417054E5FFC53FD4FFB99FD44" box="[912,1033,656,680]" italics="true" pageId="86" pageNumber="87">A. pallidus</emphasis>
