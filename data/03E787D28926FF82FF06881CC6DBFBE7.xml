<materialsCitation id="3B263C998926FF82FC358913C606FC07" collectingDate="1877-07" collectionCode="ZMB" collectorName="Hildebrandt" country="Kenya" location="Taita" pageId="32" pageNumber="33" specimenCode="ZMB 5378 - 5379" specimenCount="1" stateProvince="Taita/Taveta" typeStatus="syntype">
