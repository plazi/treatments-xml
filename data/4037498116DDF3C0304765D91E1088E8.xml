Voet, 1806 = 
<taxonomicName id="35D25D44D92454A92692CEE6B2D98038" authority="Laicharting, 1781" authorityName="Laicharting" authorityYear="1781" class="Insecta" family="Chrysomelidae" genus="Clytra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Clytra monstrosa" order="Coleoptera" pageId="45" pageNumber="56" phylum="Arthropoda" rank="species" species="monstrosa">Clytra monstrosa</taxonomicName>
