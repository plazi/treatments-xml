<taxonomicName id="09F74D7CBD0AFFF824EFFD86FDE298EF" box="[402,528,393,417]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
