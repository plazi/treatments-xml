<taxonomicName id="09F74D7CBD0AFFF82758FD86FD3298EF" box="[549,704,393,417]" class="Magnoliopsida" family="Boraginaceae" kingdom="Plantae" order="Boraginales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="family">Boraginaceae</taxonomicName>
, 
<taxonomicName id="09F74D7CBD0AFFF827A9FD86FCCD98EF" box="[724,831,393,417]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
