<number id="3D9F24CAD238FFA7FD99F9FFFDE8F9E3" box="[521,563,1629,1654]" pageId="5" pageNumber="545" value="6.5" valueMax="7.0" valueMin="6.0">6‒7</number>
<emphasis id="B923EAFFD238FFA7FDAAF9FCFD93F9E0" box="[570,584,1630,1653]" italics="true" pageId="5" pageNumber="545">a</emphasis>
<number id="3D9F24CAD238FFA7FDD8F9CEFD8BF9EF" box="[584,592,1644,1658]" pageId="5" pageNumber="545" value="1.0">
<emphasis id="B923EAFFD238FFA7FDD8F9CEFD8BF9EF" box="[584,592,1644,1658]" italics="true" pageId="5" pageNumber="545">1</emphasis>
</number>
-glandular setae and 
<number id="3D9F24CAD238FFA7FCA8F9FFFCB9F9E3" box="[824,866,1629,1654]" pageId="5" pageNumber="545" value="6.5" valueMax="8.0" valueMin="5.0">5‒8</number>
<emphasis id="B923EAFFD238FFA7FCF9F9FCFCACF9E0" box="[873,887,1630,1653]" italics="true" pageId="5" pageNumber="545">a</emphasis>
<number id="3D9F24CAD238FFA7FCE7F9CEFCA4F9EF" box="[887,895,1644,1658]" pageId="5" pageNumber="545" value="2.0">
