<number id="3D9F24CAD238FFA7FD57FA4DFD0EF992" box="[711,725,1519,1543]" pageId="5" pageNumber="545" value="1.0">1</number>
or 
<number id="3D9F24CAD238FFA7FD6BFA4DFCD2F992" box="[763,777,1519,1543]" pageId="5" pageNumber="545" value="3.0">3</number>
