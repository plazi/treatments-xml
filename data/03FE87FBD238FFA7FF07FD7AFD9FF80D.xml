<taxonomicName id="4C574D6ED238FFA7FED6F89AFDBDF8C4" box="[326,614,1848,1873]" class="Entognatha" family="Campodeidae" genus="Tachycampa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplura" pageId="5" pageNumber="545" phylum="Arthropoda" rank="species" species="calosjuarezi" status="n. sp.">
