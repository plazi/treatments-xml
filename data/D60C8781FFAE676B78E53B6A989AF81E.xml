<taxonomicName id="99A54D14FFAE676B7B9E3C2A989AFAE9" authority="Garcia Aldrete" authorityName="Garcia Aldrete" box="[1004,1436,1344,1366]" class="Insecta" family="Lachesillidae" genus="Lachesilla" kingdom="Animalia" order="Psocodea" pageId="1" pageNumber="511" phylum="Arthropoda" rank="species" species="asymmetriproctus">
