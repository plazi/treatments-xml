<keyLead id="E55495A2FFAE676B78E53F46989DF9E1" pageId="1" pageNumber="511">
<number id="E86D24B0FFAE676B78E53F469DB6F9FE" box="[151,176,1580,1601]" pageId="1" pageNumber="511" value="11.0">11</number>
