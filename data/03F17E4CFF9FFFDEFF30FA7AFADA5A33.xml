<taxonomicName id="4C58B4D9FF9FFFDEFF49FA7DFDB75F4F" authority="Steiner, 1927" authorityName="Steiner" authorityYear="1927" box="[238,548,867,894]" class="Adenophorea" family="Epsilonematidae" genus="Epsilonema" kingdom="Animalia" order="Desmodorida" pageId="16" pageNumber="399" phylum="Nematoda" rank="genus">
