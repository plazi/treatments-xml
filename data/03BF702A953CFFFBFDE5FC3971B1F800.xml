<taxonomicName id="4C16BABF953CFFFBFDE5F9957261F858" baseAuthorityName="Burks 1979:" baseAuthorityYear="1093" box="[151,268,1553,1579]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="stellata">
<emphasis id="B9621D2E953CFFFBFDE5F9957261F858" box="[151,268,1553,1579]" italics="true" pageId="57" pageNumber="58">Q. stellata</emphasis>
</taxonomicName>
