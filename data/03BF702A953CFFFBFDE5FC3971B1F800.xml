<taxonomicName id="4C16BABF953CFFFBFF48F9B171A4F83C" baseAuthorityName="Burks 1979:" baseAuthorityYear="1093" box="[570,713,1589,1615]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="turbinella">Q. turbinella</taxonomicName>
