</taxonomicName>
section (
<taxonomicName id="4C16BABF953CFFFBF94FF99577A4F858" box="[1085,1225,1553,1579]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="arizonica">
<emphasis id="B9621D2E953CFFFBF94FF995773CF858" box="[1085,1105,1553,1579]" italics="true" pageId="57" pageNumber="58">Q</emphasis>
