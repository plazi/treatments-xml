<taxonomicName id="4C16BABF953CFFFBFEEEFA697777F874" box="[924,1050,1517,1543]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="velutina">
<emphasis id="B9621D2E953CFFFBFEEEFA697777F874" box="[924,1050,1517,1543]" italics="true" pageId="57" pageNumber="58">Q. velutina</emphasis>
</taxonomicName>
); 
<taxonomicName id="4C16BABF953CFFFBF941FA697636F874" baseAuthorityName="Osten-Sacken" baseAuthorityYear="1861" box="[1075,1371,1517,1543]" class="Insecta" family="Cynipidae" genus="Atrusca" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="quercuscentricola">
<emphasis id="B9621D2E953CFFFBF941FA697636F874" box="[1075,1371,1517,1543]" italics="true" pageId="57" pageNumber="58">Atrusca quercuscentricola</emphasis>
</taxonomicName>
