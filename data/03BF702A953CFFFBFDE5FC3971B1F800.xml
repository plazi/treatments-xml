<taxonomicName id="4C16BABF953CFFFBFCF7F9DD717CF800" baseAuthorityName="Burks 1979:" baseAuthorityYear="1092" box="[389,529,1625,1651]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="prinoides">
