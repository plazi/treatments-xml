<taxonomicName id="4C16BABF953CFFFBFEB7F9B277F2F83C" authorityName="Gillette" authorityYear="1896" baseAuthorityName="Fitch" baseAuthorityYear="1859" box="[965,1183,1589,1615]" class="Insecta" family="Cynipidae" genus="Disholcaspis" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="quercusglobulus">
