<taxonomicName id="EEF432404F24FDF27C5FB6271B381474" authorityName="Leach" authorityYear="1815" class="Insecta" family="Elateridae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="79" phylum="? arthropoda" rank="family">Elateridae</taxonomicName>
(e.g. 
<taxonomicName id="23A633F54AE5622AE2CF6209E81CC3C7" class="Insecta" family="Elateridae" genus="Agriotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agriotes" order="Coleoptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="genus">
<emphasis id="A943F0168FA9AE29FB0A86268EEBF1B9" italics="true" pageId="0" pageNumber="79">Agriotes</emphasis>
</taxonomicName>
