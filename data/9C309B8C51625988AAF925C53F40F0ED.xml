<taxonomicName id="6E29FC6B2AE597F63DAA12ACA45F94FD" genus="P." lsidName="P. stevensi" pageId="0" pageNumber="151" rank="species" species="stevensi">P. stevensi</taxonomicName>
sp. n., male; f, g) 
<taxonomicName id="249FB27951A4634E61564C150ACF7466" genus="P." lsidName="P. dalmasi" pageId="0" pageNumber="151" rank="species" species="dalmasi">P. dalmasi</taxonomicName>
