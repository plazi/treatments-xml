<emphasis id="B92F5457FFD5FFD003D7E61CFBFDFEFB" box="[907,1085,306,326]" italics="true" pageId="65" pageNumber="1078">
<taxonomicName id="4C5BF3C6FFD5FFD003D7E61CFC0FFEFB" box="[907,975,306,326]" class="Insecta" family="Curculionidae" genus="Liparus" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="1078" phylum="Arthropoda" rank="genus">Liparus</taxonomicName>
