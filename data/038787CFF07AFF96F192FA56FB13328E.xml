<taxonomicName id="4C2E4D5AF07DFF96F081FC72FDF03119" authority="Duan &amp; Dietrich, 2018" authorityName="Duan &amp; Dietrich" authorityYear="2018" box="[388,562,1019,1041]" class="Insecta" family="Cicadellidae" genus="Amplicephalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="559" phylum="Arthropoda" rank="species" species="anayensis" status="sp. n." subGenus="Nanctasus">
<emphasis id="B95AEACBF07DFF96F081FC72FE513119" box="[388,403,1019,1040]" italics="true" pageId="4" pageNumber="559">A</emphasis>
. 
<taxonomicName id="4C2E4D5AF07DFF96F0A5FC72FDF03119" box="[416,562,1019,1041]" pageId="4" pageNumber="559" rank="species" species="anayensis" status="sp. n." subGenus="N.">
(
<emphasis id="B95AEACBF07DFF96F0ADFC75FE7B3118" box="[424,441,1020,1041]" italics="true" pageId="4" pageNumber="559">N</emphasis>
