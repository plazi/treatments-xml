<taxonomicName id="4C6CA1CF6623B45BC40C48643D8F227A" authority="" authorityName="" box="[1328,1429,788,811]" class="Rosospsida" family="Brassicaceae" genus="Dentaria" higherTaxonomySource="GBIF" kingdom="Plantae" order="Brassicales" pageId="118" pageNumber="119" phylum="Magnoliophyta" rank="genus">
