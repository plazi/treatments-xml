<specimenCode id="DB8A9EF4420CFFDA9AF57F8DAB71F9BE" box="[384,543,1522,1544]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="48" pageNumber="49" type="Museum">USNM 87595</specimenCode>
)
</materialsCitation>
