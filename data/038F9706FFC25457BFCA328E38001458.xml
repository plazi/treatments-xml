<geoCoordinate id="EE1240D7FFC25457BDE6346B3A6D1772" box="[644,770,1284,1309]" degrees="09" direction="north" minutes="18" orientation="latitude" pageId="5" pageNumber="584" precision="15" seconds="52" value="9.314445">09°18′52″N</geoCoordinate>
, 
<geoCoordinate id="EE1240D7FFC25457BFCA3448385A1751" box="[168,309,1317,1343]" degrees="075" direction="west" minutes="23" orientation="longitude" pageId="5" pageNumber="584" precision="15" seconds="18" value="-75.38833">075°23′18″W</geoCoordinate>
