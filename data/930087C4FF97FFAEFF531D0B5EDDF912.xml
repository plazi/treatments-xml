<typeStatus id="C4128870FF97FFAEFDF21E095FDDF9B9" box="[566,674,1541,1567]" pageId="15" pageNumber="36">paratype</typeStatus>
<number id="AD6124F5FF97FFAEFD6F1E095F96F9B9" box="[683,745,1541,1567]" pageId="15" pageNumber="36" value="3551.0">3551</number>
/ 
<number id="AD6124F5FF97FFAEFD361E095E7EF9B9" box="[754,769,1541,1567]" pageId="15" pageNumber="36" value="4.0">4</number>
