<taxonomicName id="4C0D4D36FFBFFF962F59FA8AB306FA49" box="[599,827,1497,1520]" class="Polychaeta" family="Syllidae" genus="Haplosyllis" kingdom="Animalia" order="Phyllodocida" pageId="6" pageNumber="7" phylum="Annelida" rank="species" species="basticola">Haplosyllis basticola</taxonomicName>
