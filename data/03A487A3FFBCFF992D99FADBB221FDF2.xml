<taxonomicName id="4C0D4D36FFB1FF982F0DF96BB331F9F6" box="[515,780,1592,1615]" class="Polychaeta" family="Syllidae" genus="Haplosyllis" kingdom="Animalia" order="Phyllodocida" pageId="8" pageNumber="9" phylum="Annelida" rank="species" species="crassicirrata">
