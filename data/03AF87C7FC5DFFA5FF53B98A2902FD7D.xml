<geoCoordinate id="EE325016FC5DFFA5FCCBBA162E20FEE5" box="[801,873,359,384]" degrees="5" direction="north" minutes="1" orientation="latitude" pageId="14" pageNumber="15" precision="925" value="5.016667">
