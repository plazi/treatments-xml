<taxonomicName id="4C284D0DFF92B56DFD3D009AF84A3EFD" authority="Percy Csiro, 2003" authorityName="Percy Csiro" authorityYear="2003" box="[655,834,1706,1727]" class="Insecta" family="Psyllidae" genus="Arytinnis" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="419" phylum="Arthropoda" rank="species" species="romeria" status="sp. n.">
