, 
<taxonomicName id="F860F395A8C35461032EBD2D3A1B2816" baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Bulimulidae" genus="Kuschelenia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="40" pageNumber="41" phylum="Mollusca" rank="species" species="culminea" subGenus="Kuschelenia">
