<caption id="8BC9665CAB77FF99BACFFB52F49C5F3F" ID-DOI="http://doi.org/10.5281/zenodo.5302298" ID-Zenodo-Dep="5302298" httpUri="https://zenodo.org/record/5302298/files/figure.png" pageId="6" pageNumber="51" startId="6.[151,256,1231,1255]" targetBox="[466,1121,202,1200]" targetPageId="6">
<paragraph id="DF0936D4AB77FF99BACFFB52F49C5F3F" blockId="6.[151,1436,1231,1287]" pageId="6" pageNumber="51">
<emphasis id="EDC2EAC6AB77FF99BACFFB52F6515EDE" bold="true" box="[151,283,1231,1255]" pageId="6" pageNumber="51">FIGURE 4.</emphasis>
<taxonomicName id="18B64D57AB77FF99BB7BFB4DF51B5EDE" authority="Leduc &amp; Wharton, 2010" authorityName="Leduc &amp; Wharton" authorityYear="2010" box="[291,593,1232,1255]" class="Adenophorea" family="Desmodoridae" genus="Pseudochromadora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="6" pageNumber="51" phylum="Nematoda" rank="species" species="reathae" status="n. sp.">
<emphasis id="EDC2EAC6AB77FF99BB7BFB4DF51B5EDE" box="[291,593,1232,1255]" italics="true" pageId="6" pageNumber="51">Pseudochromadora reathae</emphasis>
</taxonomicName>
<emphasis id="EDC2EAC6AB77FF99B801FB52F5DD5EDE" bold="true" box="[601,663,1231,1255]" pageId="6" pageNumber="51">
<taxonomicNameLabel id="F6F157BDAB77FF99B801FB52F5DD5EDE" box="[601,663,1231,1255]" pageId="6" pageNumber="51" rank="species">n. sp.</taxonomicNameLabel>
