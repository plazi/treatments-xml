<taxonomicName id="AF939DCE365CAA72041A502F98B60DE2" class="Polychaeta" family="Opheliidae" genus="Ophelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ophelina curli" pageId="0" pageNumber="1" phylum="Annelida" rank="species" species="curli">
