(
<emphasis id="4685C125DC445D25157C4C9DCF23FA42" bold="true" box="[582,624,1440,1460]" pageId="8" pageNumber="1277">9-12</emphasis>
) and 
<taxonomicName id="B3F166B4DC445D2515934C9DCE9BFA45" authority="BERNHAUER" authorityName="BERNHAUER" authorityYear="1910" box="[681,968,1440,1460]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="1277" phylum="Arthropoda" rank="species" species="dimidiatipenne">
