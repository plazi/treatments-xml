3 
<taxonomicName id="B3F166B4DC4B5D2A15754A65CE76FC9A" authority="Assing, 2009" authorityName="Assing" authorityYear="2009" box="[591,805,856,876]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="1276" phylum="Arthropoda" rank="species" species="kastcheevi" status="sp.n.">
