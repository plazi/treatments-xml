<quantity id="3859ED89B817E52961C7F9B1619FF91C" box="[673,755,1577,1603]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="53" pageNumber="54" unit="mm" value="13.0">13 mm</quantity>
