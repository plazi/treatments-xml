Length 
<quantity id="4552BEFFFF85FFA1FE62F8F55349EC7A" box="[465,601,1863,1887]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="4.3" metricValueMin="3.7" pageId="22" pageNumber="23" unit="mm" value="4.0" valueMax="4.3" valueMin="3.7">3.7–4.3 mm</quantity>
