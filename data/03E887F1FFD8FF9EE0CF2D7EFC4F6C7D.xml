) e t 
<taxonomicName id="4C414D64FFD8FF9EE37F2E5BFB016D40" authority="(Tardieu) M. Kato." box="[827,1203,574,598]" class="Polypodiopsida" family="Athyriaceae" genus="Cornopteris" kingdom="Plantae" order="Polypodiales" pageId="6" pageNumber="363" phylum="Tracheophyta" rank="species" species="marojejyensis">
