<taxonomicName id="B6DDF077E11C8278F41394FB4214FD4F" class="Arachnida" family="Titanoecidae" genus="Pandava" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pandava kama" order="Araneae" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="kama">Pandava kama</taxonomicName>
