<taxonomicName id="8824D95917BF1F5B418B09CD21B5BF61" class="Arachnida" family="Titanoecidae" genus="Pandava" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pandava" order="Araneae" pageId="7" pageNumber="62" phylum="Arthropoda" rank="genus">Pandava</taxonomicName>
