<taxonomicName id="4C5B4D086C5AFFC3B56FFCEFEACFFCE9" authorityName="Perez" authorityYear="1879" box="[311,461,835,860]" class="Insecta" family="Apidae" genus="Eucera" kingdom="Animalia" order="Hymenoptera" pageId="124" pageNumber="125" phylum="Arthropoda" rank="species" species="nigrescens">
