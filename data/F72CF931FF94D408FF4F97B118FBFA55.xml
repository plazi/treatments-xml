<quantity id="B87DE5C2FF94D408FE0793A1184CFA55" box="[479,550,1447,1473]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="11" pageNumber="494" unit="m" value="1.5" valueMax="2.0" valueMin="1.0">1–2 m</quantity>
