<taxonomicName id="77DC4D0DFFF0FF93FEDF7097FDFFF0F9" authorityName="Por" authorityYear="1965" box="[303,546,2014,2037]" class="Hexanauplia" family="Argestidae" genus="Mesocletodes" kingdom="Animalia" order="Harpacticoida" pageId="36" pageNumber="250" phylum="Arthropoda" rank="species" species="robustus">
