<taxonomicName id="77DC4D0DFFF7FF94FEDF7098FDFFF0E4" authorityName="Por" authorityYear="1965" box="[303,546,2001,2024]" class="Hexanauplia" family="Argestidae" genus="Mesocletodes" kingdom="Animalia" order="Harpacticoida" pageId="35" pageNumber="249" phylum="Arthropoda" rank="species" species="robustus">
<emphasis id="82A8EA9CFFF7FF94FEDF7098FDFFF0E4" box="[303,546,2001,2024]" italics="true" pageId="35" pageNumber="249">Mesocletodes robustus</emphasis>
