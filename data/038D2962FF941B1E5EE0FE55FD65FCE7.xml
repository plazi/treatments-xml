material.
</emphasis>
<taxonomicName id="4C24E3F7FF941B1E5F51FD06FE03FD5D" authorityName="Beutenmuller" authorityYear="1918" box="[317,457,659,685]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="52" phylum="Arthropoda" rank="species" species="elsa">
