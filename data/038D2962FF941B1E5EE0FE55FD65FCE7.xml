<taxonomicName id="4C24E3F7FF941B1E5DAAFD06FC26FD5D" authorityName="Beutenmuller" authorityYear="1918" box="[966,1004,659,685]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="52" phylum="Arthropoda" rank="species" species="elsa">
