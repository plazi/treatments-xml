<taxonomicName id="912A06DC92FA64620CDCBE898C4C018C" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia mangium" order="Fabales" pageId="2" pageNumber="3" phylum="Tracheophyta" rank="species" species="mangium">Acacia mangium</taxonomicName>
