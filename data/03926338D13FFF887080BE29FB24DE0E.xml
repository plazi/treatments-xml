<bibRefCitation id="EFAAAFDFD13FFF8873A3B817FBD3DE6A" author="International Commission on Zoological Nomenclature &amp; ICZN" box="[948,1100,1315,1339]" pageId="1" pageNumber="454" pagination="1 - 306" refId="ref15069" refString="International Commission on Zoological Nomenclature (ICZN). (1999) International Code of Zoological Nomenclature. Fourth Edition. ITZN, London. i-xxx + 1 - 306." type="book chapter" year="1999">ICZN (1999)</bibRefCitation>
, and the 
<typeStatus id="54806C8CD13FFF8874ADB817FB75DE6A" box="[1210,1258,1315,1339]" pageId="1" pageNumber="454">type</typeStatus>
<taxonomicName id="4C3BA9ADD13FFF8874E3B810FAE7DE6A" authorityName="Westwood" authorityYear="1832" box="[1268,1400,1316,1339]" class="Insecta" family="Diapriidae" genus="Paramesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="genus">
<emphasis id="B94F0E3CD13FFF8874E3B810FAE7DE6A" box="[1268,1400,1316,1339]" italics="true" pageId="1" pageNumber="454">Paramesius</emphasis>
</taxonomicName>
(= 
<emphasis id="B94F0E3CD13FFF887080B872FF78DE0E" box="[151,231,1350,1375]" italics="true" pageId="1" pageNumber="454">Teleas)</emphasis>
<taxonomicName id="4C3BA9ADD13FFF8870F9B873FDADDE0E" authority="(Fonscolombe, 1832)" authorityName="Fonscolombe" authorityYear="1832" baseAuthorityName="Fonscolombe" baseAuthorityYear="1832" box="[238,562,1350,1375]" class="Insecta" family="Scelionidae" genus="Teleas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="species" species="rufipes">
<emphasis id="B94F0E3CD13FFF8870F9B873FEA3DE0E" box="[238,316,1351,1375]" italics="true" pageId="1" pageNumber="454">rufipes</emphasis>
(
<bibRefCitation id="EFAAAFDFD13FFF88715DB872FDB5DE0E" author="Fonscolombe &amp; Boyer de E" box="[330,554,1350,1375]" pageId="1" pageNumber="454" pagination="273 - 307" refId="ref14933" refString="Fonscolombe, Boyer de E. L. 1. H. (1832) Monographia chalciditum, Galloprovinciae circa aquas sextias degentium. Annales des Sciences Naturelles, 26 (1), 273 - 307. [Dating from Fergusson 1977] https: // doi. org / 10.5281 / zenodo. 24211" type="journal article" year="1832">Fonscolombe, 1832</bibRefCitation>
