<taxonomicName id="4C3BA9ADD13FFF8872E7B9CBFCEBDE47" authorityName="Westwood" authorityYear="1832" box="[752,884,1279,1302]" class="Insecta" family="Diapriidae" genus="Paramesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="genus">
<emphasis id="B94F0E3CD13FFF8872E7B9CBFCEBDE47" box="[752,884,1279,1302]" italics="true" pageId="1" pageNumber="454">Paramesius</emphasis>
</taxonomicName>
(= 
<emphasis id="B94F0E3CD13FFF88738CB9CAFC74DE46" box="[923,1003,1278,1303]" italics="true" pageId="1" pageNumber="454">Teleas)</emphasis>
<taxonomicName id="4C3BA9ADD13FFF8873E5B9CBFAA8DE46" authority="(Fonscolombe, 1832)" authorityName="Fonscolombe" authorityYear="1832" baseAuthorityName="Fonscolombe" baseAuthorityYear="1832" box="[1010,1335,1278,1303]" class="Insecta" family="Scelionidae" genus="Teleas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="species" species="rufipes">
<emphasis id="B94F0E3CD13FFF8873E5B9CBFBDFDE46" box="[1010,1088,1279,1303]" italics="true" pageId="1" pageNumber="454">rufipes</emphasis>
(
<bibRefCitation id="EFAAAFDFD13FFF88745AB9CAFAAFDE46" author="Fonscolombe &amp; Boyer de E" box="[1101,1328,1278,1303]" pageId="1" pageNumber="454" pagination="273 - 307" refId="ref14933" refString="Fonscolombe, Boyer de E. L. 1. H. (1832) Monographia chalciditum, Galloprovinciae circa aquas sextias degentium. Annales des Sciences Naturelles, 26 (1), 273 - 307. [Dating from Fergusson 1977] https: // doi. org / 10.5281 / zenodo. 24211" type="journal article" year="1832">Fonscolombe, 1832</bibRefCitation>
)
</taxonomicName>
