<taxonomicName id="4C3BA9ADD13FFF887080BEB7FE41D8C9" authority="Kieffer, 1913: 436" authorityName="Kieffer" authorityPageNumber="436" authorityYear="1913" box="[151,478,898,920]" class="Hexapoda" family="Diapriinae" genus="Aparamesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="454" phylum="Arthropoda" rank="genus">
