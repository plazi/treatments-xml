<taxonomicName id="7EE386B39C6E2A24EE705992FC955AE2" authority="(Latreille, 1807)" baseAuthorityName="Latreille" baseAuthorityYear="1807" box="[294,787,1964,1986]" class="Insecta" family="Myrmeleontidae" genus="Macronemurus" kingdom="Animalia" order="Neuroptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="appendiculatus">
