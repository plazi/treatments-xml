<taxonomicName id="8A128BC856968D2BBEE976F92B473F05" authorityName="Linnaeus" authorityYear="1758" class="Hexapoda" family="Carabidae" genus="Cicindela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="195" pageNumber="196" phylum="Arthropoda" rank="genus">
