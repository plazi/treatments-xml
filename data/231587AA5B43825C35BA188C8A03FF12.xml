<taxonomicName id="6CBC4D3F5B42825C37F11ED08F3DFF7A" authority="Kirby" authorityName="Kirby" authorityYear="1837" box="[732,1090,152,178]" class="Insecta" family="Apidae" genus="Anthophora" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="bomboides">
