<taxonomicName id="6CBC4D3F5B42825C31E01ED08E13FF7A" box="[1229,1388,152,178]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
