<taxonomicName id="F4803FAE4D5BEC0E653CF79AFAAC7CD2" authority="Rasnitsyn, 1969" authorityName="Rasnitsyn" authorityYear="1969" box="[1041,1399,1696,1722]" class="Insecta" family="Xyelidae" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="148" phylum="Arthropoda" rank="subFamily" subFamily="Madygellinae">
