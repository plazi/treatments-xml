54. 
<taxonomicName id="F9332B2549CDC6D0904ABCB87177E628" LSID="http://zoobank.org/6821B9F3-461A-48F2-A1E2-EAEE65AF0BAB" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus paraflorensis" order="Coleoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="paraflorensis">Trigonopterus paraflorensis Riedel</taxonomicName>
