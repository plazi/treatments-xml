<taxonomicName id="F1814D75FFA3FF862C72FDEA97CB8E24" authorityName="Young" authorityYear="1967" box="[805,934,519,543]" class="Actinopterygii" family="Serranidae" genus="Tetrancistsrum" kingdom="Animalia" order="Perciformes" pageId="24" pageNumber="1535" phylum="Chordata" rank="species" species="oraminii">
