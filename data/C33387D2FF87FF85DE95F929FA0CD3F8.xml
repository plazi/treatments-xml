(
<emphasis id="79EEEAD6FF89FF85DB0EFF19FAB2D2AE" bold="true" box="[1292,1314,183,204]" italics="true" pageId="8" pageNumber="418">C.</emphasis>
) 
<emphasis id="79EEEAD6FF89FF85DB32FF18FA0CD2AE" bold="true" box="[1328,1436,182,204]" italics="true" pageId="8" pageNumber="418">kurahashii</emphasis>
</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="4B2536C4FF89FF85DE95FF7BFA0CD365" blockId="8.[151,1437,153,410]" pageId="8" pageNumber="418">
<keyLead id="F06B95F1FF89FF85DE95FF7BFA0CD365" pageId="8" pageNumber="418">
