<taxonomicName id="BC4F7D2100FDC6675739D819E3855FA8" LSID="http://zoobank.org/D7A2BFB8-59A2-4BA4-A26C-630F9E5B7488" authority="Tian" class="Insecta" family="Carabidae" genus="Shiqianaphaenops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Shiqianaphaenops" order="Coleoptera" pageId="4" pageNumber="125" phylum="Arthropoda" rank="genus">Shiqianaphaenops Tian</taxonomicName>
<taxonomicNameLabel id="6826FBA3440A1EE4EB7F02C134B7B8E4" pageId="4" pageNumber="125">gen. n.</taxonomicNameLabel>
