<taxonomicName id="4C4A4D43FFA6FFDAD824F590E106099C" authority="(d'Orbigny, 1835)" baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" kingdom="Animalia" order="Stylommatophora" pageId="10" pageNumber="67" phylum="Mollusca" rank="species" species="crepundia">
