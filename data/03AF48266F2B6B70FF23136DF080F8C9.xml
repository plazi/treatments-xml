<quantity id="4CFE54D56F2B6B74FC8712B1F037F879" box="[819,875,2010,2034]" metricMagnitude="1" metricUnit="m" metricValue="5.090159999999999" pageId="1" pageNumber="352" unit="in" value="2004.0">2004</quantity>
