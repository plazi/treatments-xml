<location id="9057A9E6F149FFCCDF0E76832F6FFC6C" LSID="urn:lsid:plazi:treatment:1D214E2BF148FFCCDE8276AD2A09FBE0:9057A9E6F149FFCCDF0E76832F6FFC6C" box="[283,328,964,990]" county="Helene Perrin" municipality="Mt. Elgon" name="The" pageId="53" pageNumber="54">The</location>
