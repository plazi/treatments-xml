<quantity id="4CC5D18EFFC4BA0483EC4C1FFDCDB537" box="[497,573,1340,1363]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="20" pageNumber="203" unit="m" value="500.0">500 m</quantity>
