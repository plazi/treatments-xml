<geoCoordinate id="805D56246AAC99B49A211F7B5FA7E0C7" degrees="20" direction="north" minutes="45" orientation="latitude" precision="925" value="20.75">20°45'N</geoCoordinate>
<geoCoordinate id="A78543F4952CF4062B7FE1FBC37EFB73" degrees="102" direction="east" minutes="09" orientation="longitude" precision="925" value="102.15">102°09'E</geoCoordinate>
, 1100 m, 1-9.V.2002, leg. V. 
<normalizedToken id="764E7E7A2B9701314187D901202FCE26" originalValue="Kubáň">Kuban</normalizedToken>
