<taxonomicName id="32974D354234AF10D844F9C3FD07F9EE" authority="Oji, 1990: 423" authorityName="Oji" authorityPageNumber="423" authorityYear="1990" box="[286,700,1601,1623]" class="Crinoidea" family="Cainocrinidae" genus="Teliocrinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isocrinida" pageId="16" pageNumber="38" phylum="Echinodermata" rank="species" species="springeri">
