<number id="C73024AB0668296BFF69EBFDDBD0F94C" box="[186,193,1772,1786]" pageId="10" pageNumber="411" value="3.0">3</number>
and R 
<number id="C73024AB0668296BFEDFEBFDDA05F94C" box="[268,276,1772,1786]" pageId="10" pageNumber="411" value="4.0">4</number>
+ 
