<number id="C73024AB0668296BFBD8EBCDDF04F942" box="[1035,1045,1756,1780]" pageId="10" pageNumber="411" value="1.0">1</number>
; ultimate and penultimate sections of M 
<quantity id="B6009B690668296BFF1DEA02DBE2F8AD" box="[206,243,1795,1825]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="10" pageNumber="411" unit="in" value="1.0">1 in</quantity>
proportion of 
<number id="C73024AB0668296BFE46EA12DAB1F8AD" box="[405,416,1795,1819]" pageId="10" pageNumber="411" value="1.0">1</number>
