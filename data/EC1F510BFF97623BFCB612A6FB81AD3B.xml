<taxonomicName id="A3B69B9EFF94623BFC991021FC59AD7D" authorityName="Simon" authorityYear="1885" box="[850,925,1055,1076]" class="Arachnida" family="Lycosidae" genus="Hogna" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="89" phylum="Arthropoda" rank="genus">
