<taxonomicName id="258CA68AFB69FFB1FD9BE467FD34F8AD" box="[528,731,1792,1815]" class="Insecta" family="Noctuidae" genus="Heliothis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="maritima">
