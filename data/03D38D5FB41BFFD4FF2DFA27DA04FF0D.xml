<taxonomicName id="4C7A47CAB41BFFD5FEC9FA59D98BF9F3" box="[312,457,1522,1549]" class="Anthozoa" family="Merulinidae" genus="Mycedium" kingdom="Animalia" order="Scleractinia" pageId="12" phylum="Cnidaria" rank="species" species="undetermined">
