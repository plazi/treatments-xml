<taxonomicName id="9CC54D1F254FFF92FE04E90A423AF9E6" authorityName="Otto" authorityYear="1820" box="[403,636,1548,1572]" class="Magnoliopsida" family="Myrtaceae" genus="Melaleuca" kingdom="Plantae" order="Myrtales" pageId="69" pageNumber="69" phylum="Tracheophyta" rank="species" species="lanceolata">
<emphasis id="69B1EA8E254FFF92FE04E90A423AF9E6" box="[403,636,1548,1572]" italics="true" pageId="69" pageNumber="69">Melaleuca lanceolata</emphasis>
