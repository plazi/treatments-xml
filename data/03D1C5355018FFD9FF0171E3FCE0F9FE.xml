<taxonomicName id="4C780FA05018FFD9FF0171E3FE14FA91" authority="Hall, 1933" authorityName="Hall" authorityYear="1933" box="[151,484,1274,1301]" class="Insecta" family="Sarcophagidae" genus="Sarcofahrtiopsis" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="119" phylum="Arthropoda" rank="genus">
