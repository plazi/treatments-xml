<paragraph id="8BFD36D5860E6B28FEFD871BFCFFF45E" blockId="28.[245,1229,2826,3220]" pageId="28" pageNumber="91">
Type locality. 
<quantity id="4CBA9B30860E6B28FDE2871BFD33F40C" box="[570,681,2927,2967]" metricMagnitude="2" metricUnit="m" metricValue="1.5" pageId="28" pageNumber="91" unit="m" value="150.0">150 m</quantity>
