<quantity id="4CBA9B30860E6B28FF2D87EAFEC9F45E" box="[245,339,2974,3013]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="28" pageNumber="91" unit="km" value="4.0">4 km</quantity>
