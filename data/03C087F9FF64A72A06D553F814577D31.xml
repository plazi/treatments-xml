<location id="8EB66034FF65A72A04A5523610C97A55" LSID="urn:lsid:plazi:treatment:03C087F9FF64A72A06D553F814577D31:8EB66034FF65A72A04A5523610C97A55" box="[151,226,1097,1118]" country="United Kingdom" name="London" pageId="3" pageNumber="742">London</location>
, 
