<paragraph id="8BD9368A8B50FFE3FE9FFA3AFB19FA20" blockId="30.[264,1216,1369,1435]" box="[312,1216,1409,1435]" pageId="30" pageNumber="31">
<taxonomicName id="4C664D098B50FFE3FE9FFA3AFE2CFA21" authorityName="Sun" authorityYear="2003" box="[312,501,1409,1434]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="distincta">
