<collectingMunicipality id="9F54AC88FFA1FFFBFDFCFE3BFD614E31" box="[564,663,505,530]" pageId="54" pageNumber="55">Lüderitz</collectingMunicipality>
: 
<location id="7A506029FFA1FFFBFD64FE3BFD2A4E32" LSID="urn:lsid:plazi:treatment:F72687E4FFA1FFFBFF5FFF55FB1A4EE5:7A506029FFA1FFFBFD64FE3BFD2A4E32" box="[684,732,505,529]" county="Material" latitude="-26.6667" longLatPrecision="6" longitude="16.2667" municipality="Luderitz" name="Aus" pageId="54" pageNumber="55" stateProvince="Karas">Aus</location>
, 
<geoCoordinate id="1ABB5035FFA1FFFBFD3BFE3BFC9F4E31" box="[755,873,505,530]" direction="south" orientation="latitude" pageId="54" pageNumber="55" precision="5" value="-26.6667">26.6667°S</geoCoordinate>
<geoCoordinate id="1ABB5035FFA1FFFBFCB3FE3BFC054E31" box="[891,1011,505,530]" direction="east" orientation="longitude" pageId="54" pageNumber="55" precision="5" value="16.2667">16.2667°E</geoCoordinate>
