<taxonomicName id="389BF6BCFFD3B425FE369710FD2AF67B" baseAuthorityName="Bonaparte" baseAuthorityYear="1986" box="[451,730,2400,2436]" class="Reptilia" family="Piatnitzkysauridae" genus="Piatnitzkysaurus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="20" pageNumber="20" phylum="Chordata" rank="genus">Piatnitzkysaurus</taxonomicName>
