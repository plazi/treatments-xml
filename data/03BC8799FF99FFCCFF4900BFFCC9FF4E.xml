<taxonomicName id="4C154D0CFF98FFCBFDF0013EFD0CFBC5" box="[558,661,1124,1148]" class="Actinopterygii" family="Apogonidae" genus="Fowleria" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
<emphasis id="B961EA9DFF98FFCBFDF0013EFD0CFBC5" box="[558,661,1124,1148]" italics="true" pageId="3" pageNumber="4">Fowleria</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C154D0CFF98FFCBFD79013EFC86FBC5" box="[679,799,1124,1148]" class="Actinopterygii" family="Apogonidae" genus="Glossamia" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
<emphasis id="B961EA9DFF98FFCBFD79013EFC86FBC5" box="[679,799,1124,1148]" italics="true" pageId="3" pageNumber="4">Glossamia</emphasis>
