<taxonomicName id="4C154D0CFF98FFCBFEE9013EFE44FBC5" box="[311,477,1124,1148]" class="Actinopterygii" family="Apogonidae" genus="Apogonichthys" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
<emphasis id="B961EA9DFF98FFCBFEE9013EFE44FBC5" box="[311,477,1124,1148]" italics="true" pageId="3" pageNumber="4">Apogonichthys</emphasis>
</taxonomicName>
