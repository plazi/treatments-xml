<taxonomicName id="4C154D0CFF98FFCBFCEF013EFC36FBC5" box="[817,943,1124,1148]" class="Actinopterygii" family="Apogonidae" genus="Holapogon" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
