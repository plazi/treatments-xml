<taxonomicName id="4C154D0CFF98FFCBFE31013FFD85FBC5" box="[495,540,1125,1148]" class="Actinopterygii" family="Apogonidae" genus="Foa" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
<emphasis id="B961EA9DFF98FFCBFE31013FFD85FBC5" box="[495,540,1125,1148]" italics="true" pageId="3" pageNumber="4">Foa</emphasis>
</taxonomicName>
