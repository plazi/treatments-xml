<paragraph id="2F7A36D6873BFFE87ACA7E1C2357BEA8" blockId="3.[189,1398,758,1010]" pageId="3" pageNumber="4">
<taxonomicName id="E8C54D55873BFFE87ACA7E1C241FBE75" authority="Matsumura, 1916: 103" authorityName="Matsumura" authorityPageNumber="103" authorityYear="1916" box="[189,573,833,860]" class="Actinopterygii" family="Cottidae" genus="Daruma" kingdom="Animalia" order="Scorpaeniformes" pageId="3" pageNumber="4" phylum="Chordata" rank="genus">
<emphasis id="1DB1EAC4873BFFE87ACA7E1C273DBE72" box="[189,287,834,859]" italics="true" pageId="3" pageNumber="4">Daruma</emphasis>
<bibRefCitation id="4B544B27873BFFE87B5D7E1C27DFBE72" author="Matsumura S." box="[298,509,833,860]" journalOrPublisher="Transactions of the Sapporo Natural History Society" pageId="3" pageNumber="4" pagination="85 - 118" part="6" refString="Matsumura S. 1916. Synopsis der Issiden (Fulgoriden) Japans. Transactions of the Sapporo Natural History Society 6: 85 - 118." title="Synopsis der Issiden (Fulgoriden) Japans" type="journal article" year="1916">Matsumura, 1916</bibRefCitation>
