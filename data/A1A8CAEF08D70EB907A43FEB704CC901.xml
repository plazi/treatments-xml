<taxonomicName id="3190B0C6D647AE54C0B987533DFAABB3" LSID="http://zoobank.org/6D2D9E1C-04D0-405B-A00A-5F4E965C8654" authority="Hashimoto, 2006" authorityName="Hashimoto" authorityYear="2006" class="Insecta" family="Micropterigidae" genus="Issikiomartyria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Issikiomartyria trochos" order="Lepidoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="trochos">Issikiomartyria trochos</taxonomicName>
