<collectingCountry box="[638,697,448,474]" name="United States of America" pageId="8" pageNumber="85">USA</collectingCountry>
, 
<collectingCountry box="[710,799,448,475]" name="Mexico" pageId="8" pageNumber="85">Mexico</collectingCountry>
