<taxonomicName authorityName="Bregetova &amp; Koroleva" authorityYear="1964" box="[618,778,943,969]" class="Arachnida" family="Laelapidae" genus="Ololaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="9" pageNumber="473" phylum="Arthropoda" rank="species" species="ussuriensis">
