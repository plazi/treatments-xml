<emphasis box="[717,734,727,751]" italics="true" pageId="10" pageNumber="1211">si</emphasis>
35–40; 
<emphasis box="[826,848,727,751]" italics="true" pageId="10" pageNumber="1211">se</emphasis>
85–115; 
<emphasis box="[954,979,727,751]" italics="true" pageId="10" pageNumber="1211">c1</emphasis>
115–120; 
<emphasis box="[1097,1122,727,751]" italics="true" pageId="10" pageNumber="1211">c2</emphasis>
95– 100; 
<emphasis box="[265,291,759,783]" italics="true" pageId="10" pageNumber="1211">d1</emphasis>
15; 
<emphasis box="[337,364,759,783]" italics="true" pageId="10" pageNumber="1211">d2</emphasis>
35; 
<emphasis box="[410,422,759,783]" italics="true" pageId="10" pageNumber="1211">e</emphasis>
15; 
<emphasis box="[468,477,759,783]" italics="true" pageId="10" pageNumber="1211">f</emphasis>
15–20; 
<emphasis box="[563,576,759,783]" italics="true" pageId="10" pageNumber="1211">h</emphasis>
30–45; 
<emphasis box="[662,702,759,783]" italics="true" pageId="10" pageNumber="1211">ag1</emphasis>
