<paragraph blockId="21.[151,684,169,285]" pageId="21" pageNumber="26">
DIMENSIONS (
<collectionCode box="[290,311,240,256]" pageId="21" pageNumber="26">IN</collectionCode>
<collectionCode box="[318,353,240,256]" country="France" name="University of Montpellier" pageId="21" pageNumber="26">MM</collectionCode>
