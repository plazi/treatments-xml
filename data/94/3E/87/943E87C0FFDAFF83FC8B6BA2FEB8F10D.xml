<subSubSection pageId="60" pageNumber="65" type="materials_examined">
<paragraph blockId="60.[714,1248,650,1041]" pageId="60" pageNumber="65">
<materialsCitation collectionCode="SPECIES" pageId="60" pageNumber="65" specimenCount="1" typeStatus="holotype">
<typeStatus box="[714,763,692,715]" pageId="60" pageNumber="65">TYPE</typeStatus>
<collectionCode box="[769,840,697,714]" pageId="60" pageNumber="65">SPECIES</collectionCode>
. — 
<taxonomicName authority="Dumitrica &amp; Zügel, 2003" authorityName="Dumitrica &amp; Zügel" authorityYear="2003" box="[884,1108,693,717]" class="Polycystina" family="Ultranaporidae" genus="Poculinapora" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="60" pageNumber="65" phylum="Radiozoa" rank="species" species="poculigera" status="n. gen., n. sp.">
<emphasis box="[884,1108,693,717]" italics="true" pageId="60" pageNumber="65">Poculinapora poculigera</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1115,1248,692,716]" pageId="60" pageNumber="65" rank="species">n. gen., n. sp.</taxonomicNameLabel>
ETYMOLOGY. — From the Latin 
<emphasis box="[1058,1141,733,757]" italics="true" pageId="60" pageNumber="65">poculum</emphasis>
: cup
</materialsCitation>
; and 
<taxonomicName authorityName="Pessagno" authorityYear="1977" box="[714,785,758,781]" class="Polycystina" family="Ultranaporidae" genus="Napora" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="60" pageNumber="65" phylum="Radiozoa" rank="genus">
<emphasis box="[714,785,758,781]" italics="true" pageId="60" pageNumber="65">Napora</emphasis>
