<paragraph pageId="4" pageNumber="41">
<taxonomicName class="Clitellata" family="Glossiphoniidae" genus="Glossiphonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glossiphonia" order="Rhynchobdellida" pageId="4" pageNumber="41" phylum="Annelida" rank="genus">Glossiphonia</taxonomicName>
