10. 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34156" class="Insecta" family="Formicidae" genus="Plagiolepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Plagiolepis alluaudi Emery" lsidName-HNS="Plagiolepis alluaudi Emery" order="Hymenoptera" pageNumber="71" phylum="Arthropoda" rank="species" species="alluaudi">Plagiolepis alluaudi</taxonomicName>
