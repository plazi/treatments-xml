group species, dorsal habitus view. A 
<taxonomicName class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax cymindicus" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="cymindicus">Mecyclothorax cymindicus</taxonomicName>
(Ukulele Camp Pipeline, 1525 m) B 
<taxonomicName class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax cymindulus" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="cymindulus">Mecyclothorax cymindulus</taxonomicName>
(Kīpahulu, 1950 m) C 
<taxonomicName class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax robustus" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="robustus">Mecyclothorax robustus</taxonomicName>
(
<normalizedToken originalValue="Ke‘anae">Ke'anae</normalizedToken>
, 1325 m) D 
<taxonomicName class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax robustus" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="robustus">Mecyclothorax robustus</taxonomicName>
(Waikamoi, 1170 m). E 
<taxonomicName class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax haydeni" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="haydeni">Mecyclothorax haydeni</taxonomicName>
(
<normalizedToken originalValue="Helele‘ike‘oha">Helele'ike'oha</normalizedToken>
