<paragraph pageId="11" pageNumber="96">
<taxonomicName authority="Koch, 1990" authorityName="*" authorityYear="1990" baseAuthorityName="Konow" baseAuthorityYear="1892" class="Insecta" family="Tenthredinidae" genus="Neomessa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neomessa steusloffi" order="Hymenoptera" pageId="11" pageNumber="96" phylum="Arthropoda" rank="species" species="steusloffi">
<emphasis italics="true" pageId="11" pageNumber="96">Neomessa steusloffi</emphasis>
