<taxonomicName authority="Liebke" authorityName="Liebke" authorityYear="1938" class="Insecta" family="Carabidae" genus="Calocolliuris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="bruchi">
