<taxonomicName id="84A24D2B036AFFFDD6416DC6BBC3F825" box="[1132,1240,1932,1954]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="26" pageNumber="325" phylum="Tracheophyta" rank="genus">Epigynum</taxonomicName>
