<taxonomicName id="4C0E27CDF8756C206AAFF9BABA91A87E" box="[416,505,1557,1580]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="ivani">
