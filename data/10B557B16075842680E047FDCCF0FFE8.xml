<taxonomicName id="DB9B66FEFFAF39A690C4FFE1312B03E1" LSID="http://zoobank.org/293F8E56-1D3C-4858-9270-934463280DA0" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus dieciseis" order="Hymenoptera" pageId="341" pageNumber="342" phylum="Arthropoda" rank="species" species="dieciseis">
<pageBreakToken id="9B8B27B02A296E7C4D0CC3FA1CAAFF20" pageId="341" pageNumber="342" start="start">Heterospilus</pageBreakToken>
dieciseis Marsh
</taxonomicName>
<taxonomicNameLabel id="35C7DCF5560AD8F98E6D3CD1B858F8FD" pageId="341" pageNumber="342">sp. n.</taxonomicNameLabel>
Figure 231
</paragraph>
