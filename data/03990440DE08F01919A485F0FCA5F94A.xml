<number id="3DF8A771DE08F0191ACB810DFBE6FADA" box="[1016,1033,1322,1348]" pageId="28" pageNumber="29" value="2.0">2</number>
- 
<number id="3DF8A771DE08F0191D21810DFBCCFADA" box="[1042,1059,1322,1348]" pageId="28" pageNumber="29" value="2.0">2</number>
/ 
<number id="3DF8A771DE08F0191D1E810DFBBCFADA" box="[1069,1107,1322,1348]" pageId="28" pageNumber="29" value="1.2">1, 2</number>
