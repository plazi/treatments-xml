<taxonomicName id="13816ACCB6CAF8313A892836AAB920D5" LSID="http://zoobank.org/479C8BE5-6F5D-470E-9ECC-2F63175C6159" authority="Maddison &amp; Maddison" class="Arachnida" family="Salticidae" genus="Habronattus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Habronattus arcalorus" order="Araneae" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="arcalorus">Habronattus arcalorus Maddison &amp; Maddison</taxonomicName>
<taxonomicNameLabel id="BADD6F672F17AA67E8C07049834F2BDD" pageId="3" pageNumber="4">sp. n.</taxonomicNameLabel>
