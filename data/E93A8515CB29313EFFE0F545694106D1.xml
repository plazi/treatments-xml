<taxonomicName id="A6934F80CB29313EFFE0F43F68E906D0" box="[109,211,882,906]" class="Magnoliopsida" family="Poaceae" genus="Candollea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="23" pageNumber="132" phylum="Tracheophyta" rank="genus">
<emphasis id="53E7E811CB29313EFFE0F43F68E906D0" box="[109,211,882,906]" italics="true" pageId="23" pageNumber="132">Candollea</emphasis>
