<taxonomicName id="A66DF40E033EFC4912B5BCBF6DCD7D30" authority="Sternberg" class="Magnoliopsida" family="Saxifragaceae" genus="Saxifraga" kingdom="Plantae" order="Saxifragales" pageId="null" pageNumber="298" phylum="Tracheophyta" rank="species" species="arachnoidea">
