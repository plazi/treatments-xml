<quantity id="4CCABBAF1A79FFE6D1FB0A3DFC4FFCE3" box="[930,988,852,869]" metricMagnitude="-2" metricUnit="m" metricValue="6.0" pageId="39" pageNumber="350" unit="mm" value="60.0">60 mm</quantity>
