<taxonomicName id="74DF4D634206624CD4EAF8B3FE5CF8E4" authority="Lyman, 1878" authorityName="Lyman" authorityYear="1878" box="[189,478,1850,1876]" class="Ophiuroidea" family="Ophiernidae" genus="Ophiernus" kingdom="Animalia" order="Ophioleucida" pageId="36" pageNumber="37" phylum="Echinodermata" rank="genus">
