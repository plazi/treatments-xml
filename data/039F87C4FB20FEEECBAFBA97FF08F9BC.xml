<taxonomicName id="4C364D51FB23FEEECA1ABC6EFDF4FA21" box="[290,540,1397,1419]" class="Gastropoda" family="Drilliidae" genus="Syntomodrillia" kingdom="Animalia" order="Neogastropoda" pageId="326" pageNumber="327" phylum="Mollusca" rank="species" species="floridana" status="new species">
<emphasis id="B942EAC0FB23FEEECA1ABC6EFDF4FA21" box="[290,540,1397,1419]" italics="true" pageId="326" pageNumber="327">
<taxonomicName id="4C364D51FB23FEEECA1ABC6EFE50FA21" box="[290,440,1397,1419]" class="Gastropoda" family="Drilliidae" genus="Syntomodrillia" kingdom="Animalia" order="Neogastropoda" pageId="326" pageNumber="327" phylum="Mollusca" rank="genus">Syntomodrillia</taxonomicName>
<taxonomicName id="4C364D51FB23FEEECA86BC6EFDF4FA21" box="[446,540,1397,1419]" pageId="326" pageNumber="327" rank="species" species="floridana" status="new species">floridana</taxonomicName>
</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A27157BBFB23FEEEC912BC6DFD4FFA21" box="[554,679,1397,1419]" pageId="326" pageNumber="327" rank="species">new species</taxonomicNameLabel>
