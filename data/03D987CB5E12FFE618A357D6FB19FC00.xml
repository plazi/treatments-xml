<emphasis id="B904EACF5E12FFE618B856ABFC84FE60" box="[703,804,432,455]" italics="true" pageId="42" pageNumber="197">
<taxonomicName id="4C704D5E5E12FFE618B856ABFD5EFE60" authorityName="Berkenhout" authorityYear="1789" box="[703,766,432,455]" class="Actinopterygii" family="Congridae" genus="Morris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="42" pageNumber="197" phylum="Chordata" rank="genus">Morris</taxonomicName>
