<emphasis id="B90DEA8A8E19832FFE9EFBF3461DFBB4" box="[320,573,1048,1072]" italics="true" pageId="3" pageNumber="41">
<taxonomicName id="4C794D1B8E19832FFE9EFBF34589FBB4" box="[320,425,1048,1072]" class="Entognatha" family="Isotomidae" genus="Scutisotoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="3" pageNumber="41" phylum="Arthropoda" rank="species" species="indigirka">indigirka</taxonomicName>
