<taxonomicNameLabel id="A23E57F18E19832FFD9AFBF346BDFBB4" box="[580,669,1048,1072]" pageId="3" pageNumber="41" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C794D1B8E19832FFD75FBF34700FBB4" box="[683,800,1048,1072]" class="Entognatha" family="Isotomidae" genus="Scutisotoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="3" pageNumber="41" phylum="Arthropoda" rank="species" species="subarctica">
