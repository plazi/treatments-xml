<taxonomicName id="4C794D1B8E19832FFB9AFC6340F7FC24" box="[1092,1239,904,928]" class="Entognatha" family="Isotomidae" genus="Scutisotoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="3" pageNumber="41" phylum="Arthropoda" rank="species" species="christianseni">christianseni</taxonomicName>
, 
<taxonomicName id="4C794D1B8E19832FFB3AFC63416DFC24" box="[1252,1357,904,928]" class="Entognatha" family="Isotomidae" genus="Scutisotoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="3" pageNumber="41" phylum="Arthropoda" rank="species" species="fjellbergi">fjellbergi</taxonomicName>
