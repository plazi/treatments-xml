<taxonomicName id="4C654D27981E006ABF786AA83EB7FD6D" baseAuthorityName="Toda &amp; Peng" baseAuthorityYear="1990" box="[1102,1237,737,760]" class="Insecta" family="Drosophilidae" genus="Phortica" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="29" phylum="Arthropoda" rank="species" species="eugamma">
