<taxonomicName id="7E57BBDE59CE8D1BF00E4B14497DF865" class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta (Microdota) festinans" order="Coleoptera" pageId="11" pageNumber="114" phylum="Arthropoda" rank="species" species="festinans" subGenus="Microdota">Atheta (Microdota) festinans</taxonomicName>
