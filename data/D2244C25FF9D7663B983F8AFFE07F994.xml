<collectingCountry id="229ABDA3FF827663B873FA3FFE66FA5C" box="[359,426,1485,1511]" name="China" pageId="16" pageNumber="513">China</collectingCountry>
(
<collectingRegion id="984933D1FF827663B8ADFA3FFDD2FA5C" box="[441,542,1485,1511]" country="China" name="Guizhou" pageId="16" pageNumber="513">Guizhou</collectingRegion>
, 
<collectingRegion id="984933D1FF827663BB3CFA3FFD4FFA5C" box="[552,643,1485,1511]" country="China" name="Yunnan" pageId="16" pageNumber="513">Yunnan</collectingRegion>
