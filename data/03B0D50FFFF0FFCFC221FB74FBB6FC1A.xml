<taxonomicName id="4C191F9AFFF0FFCFC09CFC5FFBB8FC5F" authorityName="Birula" authorityYear="1913" box="[973,1087,935,958]" class="Arachnida" family="Scorpionidae" genus="Pandinops" kingdom="Animalia" order="Scorpiones" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis id="B96DB80BFFF0FFCFC09CFC5FFBB8FC5F" box="[973,1087,935,958]" italics="true" pageId="3" pageNumber="2">Pandinops</emphasis>
