<taxonomicName id="60C6B9FB1D36FF93FE9C9DD4FD8A9DA2" box="[268,568,1229,1254]" class="Insecta" family="Muscidae" genus="Bithoracochaeta" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="564" phylum="Arthropoda" rank="species" species="sociabilis">
