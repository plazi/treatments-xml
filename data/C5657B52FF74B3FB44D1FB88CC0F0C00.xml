<specimenCount id="5BCA01CDFF74B3FB405DF905CC5D0DCF" box="[1051,1115,1740,1764]" pageId="232" pageNumber="233" type="generic">
<quantity id="8A3467A1FF74B3FB405DF905CC510DCF" box="[1051,1111,1740,1764]" metricMagnitude="1" metricUnit="m" metricValue="8.0" pageId="232" pageNumber="233" unit="m" value="80.0">80 m</quantity>
