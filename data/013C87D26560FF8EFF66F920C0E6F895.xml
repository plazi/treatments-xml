<taxonomicName id="4E954D476563FF8EFE35FA8FC003FAE2" authority="Allgen" authorityName="Allgen" authorityYear="1929" box="[452,566,1350,1371]" pageId="34" pageNumber="35" rank="species" species="propinquus">propinquus</taxonomicName>
