<collectingDate id="EC3CE9EDFFF6EC0BB45CFE73FE6BD17A" box="[351,468,500,524]" pageId="19" pageNumber="170" value="2017-05-14">
<date id="FC781005FFF6EC0BB45CFE73FE6BD17A" box="[351,468,500,524]" pageId="19" pageNumber="170" value="2017-05-14">14.V.2017</date>
</collectingDate>
, 
<collectorName id="25335313FFF6EC0BB4E3FE72FDD8D17B" box="[480,615,500,525]" pageId="19" pageNumber="170">T. Kompier.</collectorName>
