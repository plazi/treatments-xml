<paragraph id="8B9B0968FFD6FFC178BAFE8FFA8F211D" blockId="17.[189,1399,313,868]" pageId="17" pageNumber="18">
<taxonomicName id="4C2472EBFFD6FFC178BAFE8FFEA5232D" authorityName="Smith" authorityYear="1841" box="[189,349,313,339]" class="Actinopterygii" family="Cyprinidae" genus="Cheilobarbus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="17" pageNumber="18" phylum="Chordata" rank="genus">
