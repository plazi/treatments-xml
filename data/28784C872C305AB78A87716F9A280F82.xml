<paragraph id="77292AD5B06B783FA6185C7017976550" pageId="0" pageNumber="177">TL = 1.50-1.63 (Holotype = 1.50 mm, mean = 1.56 mm, SD. = 0.045, males 1.50-1.63 mm, females 1.50-1.63 mm); TLPn = 1.38-1.48 (Holotype = 1.40 mm, mean = 1.42 mm, SD = 0.039, males = 1.40-1.45 mm, females = 1.38-1.48 mm); GW = 0.72-0.80 mm (Holotype = 0.72 mm, mean = 0.75 mm, SD. = 0.027, males = 0.72 mm-0.76 mm, females = 0.73-0.80 mm; HW = 0.41-0.45 mm (Holotype = 0.41 mm, mean = 0.43 mm, SD. = 0.013, males = 0.41-0.42 mm, females = 0.42-0.45 mm); EW = 0.18-0.22 mm (Holotype = 0.18 mm, mean = 0.20 mm, SD. = 0.013, males = 0.18-0.19 mm, females = 0.19-0.22 mm), TL/GW = 1.99-2.26 (Holotype = 2.08, mean = 2.07, SD. = 0.074, males = 2.06-2.26, females = 1.98-2.11); HW/EW = 2.04-2.33 (Holotype = 2.28, mean = 2.19, SD. = 0.088, males = 2.16-2.33, females = 2.04-2.26)</paragraph>
