<paragraph id="104936862F58FFC797BEFB8ADA5B3014" blockId="27.[151,1437,1044,2005]" pageId="27" pageNumber="328">
<emphasis id="2282EA942F58FFC797BEFB8AD9D8304C" bold="true" box="[151,404,1044,1069]" pageId="27" pageNumber="328">Specimens examined</emphasis>
. 
<typeStatus id="CF4D88242F58FFC7968FFB8BDA5A304C" box="[422,534,1045,1069]" pageId="27" pageNumber="328">Lectotype</typeStatus>
