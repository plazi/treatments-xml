</emphasis>
<taxonomicName id="4C348B86FF81691AB847FF19FD177F59" authority="Reuter, 1885" authorityName="Reuter" authorityYear="1885" box="[313,655,237,260]" class="Insecta" family="Miridae" genus="Lygus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="383" phylum="Arthropoda" rank="species" species="bengalicus">
