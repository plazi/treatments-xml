<taxonomicName id="4C324D648B03C61CFEAE6751BB21060B" box="[344,586,832,856]" class="Insecta" family="Piophilidae" genus="Arctopiophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="231" phylum="Arthropoda" rank="species" species="arctica">
