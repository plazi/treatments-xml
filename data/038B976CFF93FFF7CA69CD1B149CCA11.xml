<paragraph id="8B9D267AFF93FFF6C8F3CE321483C935" blockId="24.[189,476,1815,1841]" box="[189,476,1815,1841]" pageId="24" pageNumber="118">
<heading id="D0D59116FF93FFF6C8F3CE321483C935" bold="true" box="[189,476,1815,1841]" fontSize="11" level="3" pageId="24" pageNumber="118" reason="3">
