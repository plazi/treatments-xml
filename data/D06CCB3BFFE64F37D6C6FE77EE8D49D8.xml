Type species: 
<taxonomicName id="9FC501AEFFE64F37D777FDCFE86D4D51" authority="Fennah, 1978" authorityName="Fennah" authorityYear="1978" box="[294,570,574,597]" class="Insecta" family="Issidae" genus="Tetrica" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="360" phylum="Arthropoda" rank="species" species="philo">
<emphasis id="6AB1A63FFFE64F37D777FDCFEBF04D51" box="[294,423,574,597]" italics="true" pageId="5" pageNumber="360">Tetrica philo</emphasis>
<bibRefCitation id="3C5407DCFFE64F37D7FCFDB1E86D4D51" author="Fennah" box="[429,570,574,596]" pageId="5" pageNumber="360" refString="Fennah, R. G. (1978) Fulgoroidea (Homoptera) from Vietnam. Annales Zoologici, 34 (9), 207 - 279." type="journal article" year="1978">Fennah, 1978</bibRefCitation>
</taxonomicName>
