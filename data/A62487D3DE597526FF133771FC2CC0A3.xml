<number id="984524E2DE587523FDB53646FD97C167" box="[561,576,1891,1913]" pageId="1" pageNumber="96" value="5.0">5</number>
. Pro, meso and metasternum; 
<number id="984524E2DE587523FCF43646FCA8C167" box="[880,895,1891,1913]" pageId="1" pageNumber="96" value="6.0">6</number>
