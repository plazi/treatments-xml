<geoCoordinate id="80DF5003FFE58F5D9CFE23118C0001DF" box="[509,587,1656,1681]" direction="south" orientation="latitude" pageId="6" pageNumber="43" precision="925" value="-24.1">24°6’S</geoCoordinate>
