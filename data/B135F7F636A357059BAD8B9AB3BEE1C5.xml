<taxonomicName id="24AE7E2F5A17D015CB704059B2E9ABAC" baseAuthorityName="Peyron" baseAuthorityYear="1858" class="Insecta" family="Carabidae" genus="Sinechostictus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinechostictus (Sinechostictus) effluviorum" order="Coleoptera" pageId="0" pageNumber="563" phylum="Arthropoda" rank="species" species="effluviorum" subGenus="Sinechostictus">Sinechostictus (Sinechostictus) effluviorum</taxonomicName>
