<taxonomicName id="24D09B47AD1C9F4A52E7BE63102AD409" authority="Michael, 1898" authorityName="Michael" authorityYear="1898" class="Arachnida" family="Phthiracaridae" genus="Hoploderma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="italicum">Hoploderma italicum Oudemans</taxonomicName>
