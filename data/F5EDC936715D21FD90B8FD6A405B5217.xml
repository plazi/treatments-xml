(Montagu, 1813) | 
<taxonomicName id="7B7DC8284111F9B25E2995077ADED402" class="Polychaeta" family="Sabellidae" genus="Megalomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megalomma vesiculosum" order="Sabellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="vesiculosum">Megalomma vesiculosum</taxonomicName>
