<taxonomicName id="F7873649FBA052B1205E464FB567090F" class="Magnoliopsida" family="Lamiaceae" genus="Lamium" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="121" phylum="Tracheophyta" rank="species" species="orvala">
