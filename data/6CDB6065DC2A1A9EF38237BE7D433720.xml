<paragraph id="4B8FC827E9E5D950F4B9544D94115665" pageId="214" pageNumber="215">
<taxonomicName id="11850EF34DF83A52CA2F611712AD2ABA" class="Arachnida" family="Theraphosidae" genus="Aphonopelma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphonopelma (Delopelma) phasmus" order="Araneae" pageId="214" pageNumber="215" phylum="Arthropoda" rank="species" species="phasmus" subGenus="Delopelma">Aphonopelma (Delopelma) phasmus</taxonomicName>
