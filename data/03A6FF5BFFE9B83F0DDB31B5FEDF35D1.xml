<taxonomicName id="4C0F35CEFFE9B83E0DDB31B5FF1232F8" authority="Laurenti, 1768" box="[199,240,1431,1455]" class="Amphibia" family="Hylidae" genus="Hyla" kingdom="Animalia" order="Anura" pageId="0" pageNumber="597" phylum="Chordata" rank="genus">
<location id="8ED01896FFE9B83E0DDB31B5FF1232F8" LSID="urn:lsid:plazi:treatment:03A6FF5BFFE9B83F0DDB31B5FEDF35D1:8ED01896FFE9B83E0DDB31B5FF1232F8" box="[199,240,1431,1455]" country="Tunisia" county="ZooBank" municipality="Official Register of Zoological Nomenclature" name="The" pageId="0" pageNumber="597">The</location>
</taxonomicName>
