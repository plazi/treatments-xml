<paragraph id="8BAF57615C0E6D41FF07FBC293BAFBD9" blockId="20.[151,500,1052,1111]" box="[151,400,1086,1111]" pageId="20" pageNumber="21">
(Figs 
<number id="3DD845465C0E6D41FF47FBC292D6FBD9" box="[215,252,1086,1111]" pageId="20" pageNumber="21" value="3.5">3.5</number>
, 
<number id="3DD845465C0E6D41FE98FBC29311FBD9" box="[264,315,1086,1111]" pageId="20" pageNumber="21" value="16.8">16.8</number>
, 
<number id="3DD845465C0E6D41FED7FBC393A2FBD9" box="[327,392,1087,1111]" pageId="20" pageNumber="21" value="21.24">21.24</number>
