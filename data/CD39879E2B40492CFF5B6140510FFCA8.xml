<taxonomicName id="82904D0B2B40492CFF126311510BFCA8" authorityName="Smit and Gutierrez-Chacon" authorityYear="2008" box="[222,587,829,856]" class="Insecta" family="Syrphidae" genus="Paragus" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="binominalis" subGenus="Serratoparagus">
<emphasis id="77E4EA9A2B40492CFF12631153B3FCA8" box="[222,243,830,855]" italics="true" pageId="43" pageNumber="44">P.</emphasis>
(
<emphasis id="77E4EA9A2B40492CFECF631152F3FCA7" box="[259,435,830,856]" italics="true" pageId="43" pageNumber="44">Serratoparagus</emphasis>
) 
<emphasis id="77E4EA9A2B40492CFE0F6312510BFCA8" box="[451,587,829,855]" italics="true" pageId="43" pageNumber="44">binominalis</emphasis>
</taxonomicName>
