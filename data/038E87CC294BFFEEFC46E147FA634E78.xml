<quantity id="4CDF9B3F294CFFEEFE33E685FDA84DEF" box="[485,613,149,178]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="12" pageNumber="11" unit="mm" value="4.3">4.30 mm</quantity>
