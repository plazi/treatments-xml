<taxonomicName id="1EE496E280A7B9CE0DAC16469F07194C" authorityName="B. Xue &amp; Junhao Chen" authorityYear="2021" class="Magnoliopsida" family="Annonaceae" genus="Artabotrys" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Artabotrys pachypetalus" order="Magnoliales" pageId="0" pageNumber="71" phylum="Tracheophyta" rank="species" species="pachypetalus">
