<taxonomicName id="4C5645C2F339FFA0D03D669DFA3A1F61" authority="ROSINA et RUMMEL, 2012" authorityName="ROSINA et RUMMEL" authorityYear="2012" box="[830,1449,1532,1556]" class="Mammalia" family="Vespertilionidae" genus="Miostrellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="19" pageNumber="431" phylum="Chordata" rank="species" species="petersbuchensis">
