<taxonomicName id="4C5645C2F33EFFA7D3B5675DFEC91D26" authorityName="Rachl" authorityYear="1983" box="[182,346,1084,1107]" class="Mammalia" family="Vespertilionidae" genus="Miostrellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="20" pageNumber="432" phylum="Chordata" rank="species" species="risgoviensis">
