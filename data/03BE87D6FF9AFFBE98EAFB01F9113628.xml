<quantity id="4CEF9B25FF9AFFBE9CC7FA1DFF57360F" box="[1210,1302,1519,1546]" metricMagnitude="3" metricUnit="m" metricValue="3.1" pageId="11" pageNumber="12" unit="m" value="3100.0">3,100 m</quantity>
(
<quantity id="4CEF9B25FF9AFFBE9D58FA1DFFD1360F" box="[1317,1424,1519,1546]" metricMagnitude="3" metricUnit="m" metricValue="3.0992064" pageId="11" pageNumber="12" unit="ft" value="10168.0">10,168 ft</quantity>
