<quantity id="4CEF9B25FF9AFFBE99BBFA39F86335E3" box="[454,546,1483,1510]" metricMagnitude="3" metricUnit="m" metricValue="2.603" pageId="11" pageNumber="12" unit="m" value="2603.0">2,603 m</quantity>
