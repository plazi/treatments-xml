<quantity id="4CEF9B25FF9AFFBE9A9CFA55F93935C7" box="[737,888,1447,1474]" metricMagnitude="3" metricUnit="m" metricValue="1.475" metricValueMax="2.6" metricValueMin="0.35" pageId="11" pageNumber="12" unit="m" value="1475.0" valueMax="2600.0" valueMin="350.0">350–2,600 m</quantity>
