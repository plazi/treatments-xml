<taxonomicName id="4C174D43FF9AFFBE98BBFA02F867360C" authority="Karrer, 1972" authorityName="Karrer" authorityYear="1972" box="[198,550,1519,1546]" class="Holocephali" family="Rhinochimaeridae" genus="Harriota" kingdom="Animalia" order="Chimaeriformes" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="haeckeli">
