, cast 
<collectionCode id="ED18AE330E4FFFB4FD2AFC9AFCC7FC26" LSID="urn:lsid:biocol.org:col:34988" box="[706,797,884,909]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="6" pageNumber="107">MNHN</collectionCode>
