<specimenCount id="20FFFD50FFC9FFCAE89446DD2F6648C1" box="[227,264,1737,1761]" pageId="5" pageNumber="228" type="male">4 ♂</specimenCount>
, 
<specimenCount id="20FFFD50FFC9FFCAE96346DD2F5748C1" box="[276,313,1737,1761]" pageId="5" pageNumber="228" type="female">2 ♀</specimenCount>
.
</paragraph>
<paragraph id="364636D9FFC9FFCAE8E046FD2C124941" blockId="5.[150,756,1737,1985]" pageId="5" pageNumber="228">
<materialsCitation id="86913C84FFC9FFCAE8E046FD2CE34921" box="[151,653,1769,1793]" country="China" location="Sichuan Province" pageId="5" pageNumber="228" specimenCount="1" specimenCount-male="1" stateProvince="Sichuan" typeStatus="holotype">
<emphasis id="048DEACBFFC9FFCAE8E046FD2F644921" bold="true" box="[151,266,1769,1793]" pageId="5" pageNumber="228">
<typeStatus id="E942887BFFC9FFCAE8E046FD2F644921" box="[151,266,1769,1793]" pageId="5" pageNumber="228">Holotypus</typeStatus>
