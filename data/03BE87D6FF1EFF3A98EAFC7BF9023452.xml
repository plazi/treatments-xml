(
<quantity id="4CEF9B25FF1EFF3A9ACEFC5FF8B133CD" box="[691,752,941,968]" metricMagnitude="0" metricUnit="m" metricValue="1.524" pageId="143" pageNumber="144" unit="in" value="60.0">60 in</quantity>
