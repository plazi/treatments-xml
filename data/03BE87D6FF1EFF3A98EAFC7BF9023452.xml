<quantity id="4CEF9B25FF1EFF3A9CE0FC7BFF5A33A1" box="[1181,1307,905,932]" metricMagnitude="0" metricUnit="m" metricValue="1.05" metricValueMax="1.2" metricValueMin="0.9" pageId="143" pageNumber="144" unit="cm" value="105.0" valueMax="120.0" valueMin="90.0">90–120 cm</quantity>
(
<quantity id="4CEF9B25FF1EFF3A9D57FC7BFA9C33CD" metricMagnitude="0" metricUnit="m" metricValue="1.04902" metricValueMax="1.19888" metricValueMin="0.89916" pageId="143" pageNumber="144" unit="in" value="41.3" valueMax="47.2" valueMin="35.4">35.4–47.2 in</quantity>
