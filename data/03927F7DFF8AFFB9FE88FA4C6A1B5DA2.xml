<subSubSection id="C3219DE0FF8BFFB6FEB8F97B6F1C5ADE" box="[312,1107,1759,1786]" pageId="7" pageNumber="60" type="etymology">
<paragraph id="8B84CE6BFF8BFFB6FEB8F97B6F1C5ADE" blockId="7.[264,1323,1639,1906]" box="[312,1107,1759,1786]" pageId="7" pageNumber="60">
<emphasis id="B94F1279FF8BFFB6FEB8F97B6A8B5ADD" bold="true" box="[312,452,1759,1785]" pageId="7" pageNumber="60">Etymology.</emphasis>
