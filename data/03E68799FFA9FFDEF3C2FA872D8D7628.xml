and 
<taxonomicName id="4C4F4D0CFFA8FFDEF093FE442AFB75F0" authority="Zelazny" authorityName="Zelazny" authorityYear="1981" box="[966,1267,439,464]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="mindanaoensis">
<emphasis id="B93BEA9DFFA8FFDEF093FE442A8775EF" box="[966,1167,440,464]" italics="true" pageId="75" pageNumber="76">R. mindanaoensis</emphasis>
