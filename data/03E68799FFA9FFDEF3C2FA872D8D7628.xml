<taxonomicName id="4C4F4D0CFFA8FFDEF21FFE452C2175EF" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[330,553,440,464]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="kuncheriai">
