<taxonomicName id="717D82DF6032A20F909D97780FA1F508" class="Demospongiae" family="Stelligeridae" genus="Paratimea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paratimea oxeata" order="Axinellida" pageId="4" pageNumber="5" phylum="Porifera" rank="species" species="oxeata">Paratimea oxeata</taxonomicName>
