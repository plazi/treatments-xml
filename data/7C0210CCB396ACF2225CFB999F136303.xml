<taxonomicName id="5A1A3796FEA09A56A80E8B9253E7AA25" authorityName="Chevreux" authorityYear="1903" class="Malacostraca" family="Scopelocheiridae" genus="Paracallisoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracallisoma alberti" order="Amphipoda" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="alberti">
<emphasis id="DB8959A39B879953434970DC001B4406" italics="true" pageId="14" pageNumber="15">Paracallisoma alberti</emphasis>
