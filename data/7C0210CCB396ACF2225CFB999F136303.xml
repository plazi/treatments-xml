<taxonomicName id="444C872329D5227B2E202A1D1C15BA0F" authorityName="Chevreux" authorityYear="1903" class="Malacostraca" family="Scopelocheiridae" genus="Paracallisoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracallisoma alberti" order="Amphipoda" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="alberti">
