<quantity id="4CB29B7D2C4CFFF07C59B8B30AB85CB2" box="[338,431,1471,1497]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="4" pageNumber="55" unit="mm" value="3.2">3.2 mm</quantity>
