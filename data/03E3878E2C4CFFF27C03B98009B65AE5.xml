<quantity id="4CB29B7D2C4CFFF07975BB3B0FCC5F3A" box="[1150,1243,1591,1617]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="4" pageNumber="55" unit="mm" value="2.6">2.6 mm</quantity>
