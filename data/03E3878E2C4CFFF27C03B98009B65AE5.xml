<quantity id="4CB29B7D2C4CFFF07C91BB8B0AE05FCA" box="[410,503,1671,1697]" metricMagnitude="-3" metricUnit="m" metricValue="9.7" pageId="4" pageNumber="55" unit="mm" value="9.7">9.7 mm</quantity>
