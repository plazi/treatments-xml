<quantity id="4CB29B7D2C4CFFF07C97BB3B0AEB5F3A" box="[412,508,1591,1617]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" pageId="4" pageNumber="55" unit="mm" value="5.6">5.6 mm</quantity>
