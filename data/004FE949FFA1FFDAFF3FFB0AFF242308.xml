(field tag number 
<date id="FC587E9FFFA1FFD9FD04FADFFC9C2426" box="[684,776,1306,1332]" pageId="21" pageNumber="22" value="63-05-19">
<number id="3E2E4A78FFA1FFD9FD04FADFFC9C2426" box="[684,776,1306,1332]" pageId="21" pageNumber="22" value="190563.0">190563</number>
