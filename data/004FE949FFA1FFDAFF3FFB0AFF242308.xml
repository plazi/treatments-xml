<quantity id="4F1EF5BAFFA1FFD9FAE1FADFFA092426" box="[1353,1437,1306,1332]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="21" pageNumber="22" unit="km" value="2.5">2.5 km</quantity>
