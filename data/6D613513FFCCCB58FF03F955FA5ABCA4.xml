<taxonomicName id="22C8FF86FFCCCB5FFE3FF954FD8FBB80" authorityName="Latreille" authorityYear="1802" box="[427,585,1698,1724]" class="Insecta" family="Bostrichidae" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="504" phylum="Arthropoda" rank="family">Bostrichidae</taxonomicName>
