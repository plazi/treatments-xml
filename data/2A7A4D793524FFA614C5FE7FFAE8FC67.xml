<taxonomicName id="65D387EC3524FFA6154AFDCFFE45FD63" authorityName="de Jussieu" authorityYear="1789" box="[280,398,621,647]" class="Liliopsida" family="Juncaceae" kingdom="Plantae" order="Poales" pageId="9" pageNumber="10" phylum="Tracheophyta" rank="family">Juncaceae</taxonomicName>
