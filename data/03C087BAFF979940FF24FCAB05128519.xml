, 
<collectingCountry id="F37E763CFF979940FCC5FA0303D786B4" box="[886,975,1495,1520]" name="Malawi" pageId="18">Malawi</collectingCountry>
, 
<collectingCountry id="F37E763CFF979940FC6DFA03042B86B4" box="[990,1075,1495,1520]" name="Zambia" pageId="18">Zambia</collectingCountry>
(
<figureCitation id="13522A29FF979940FBF5FA03048586B4" box="[1094,1181,1495,1520]" captionStart="FIGURES 68–69" captionStartId="23.[151,264,1661,1684]" captionTargetBox="[151,1423,374,1634]" captionTargetId="figure@23.[151,1436,374,1640]" captionTargetPageId="23" captionText="FIGURES 68–69. Blepharidina s. str. species distribution. Vegetation types are from Sayre et al. (2013)." httpUri="https://zenodo.org/record/2618655/files/figure.png" pageId="18">Fig. 68</figureCitation>
