<taxonomicName id="6BF02AE814FE42035231A9BCC36C10FF" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1848" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="91" pageNumber="92" phylum="Mollusca" rank="species" species="granadensis" subGenus="Mesembrinus">
