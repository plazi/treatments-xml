<normalizedToken id="3D582F31C694F7F535F02B8D7CC449F7" originalValue="São">Sao</normalizedToken>
<normalizedToken id="AF505A46D25EA77C00A2EB64248BD008" originalValue="Tomé">Tome</normalizedToken>
