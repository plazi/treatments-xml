<taxonomicName id="4202241EE4E24BD20D40DD12CB9B92AB" class="Maxillopoda" family="Archaeobalanidae" genus="Conopea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Conopea calceola" order="Sessilia" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="calceola">Conopea calceola</taxonomicName>
