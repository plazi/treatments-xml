<taxonomicName id="240F7A4D9D6F349839BCA3DC92FFD45F" class="Anthozoa" family="Plexauridae" genus="Muriceopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Muriceopsis tuberculata" order="Alcyonacea" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="tuberculata">Muriceopsis tuberculata</taxonomicName>
