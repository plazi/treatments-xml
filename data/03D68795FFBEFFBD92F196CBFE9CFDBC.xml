<taxonomicName id="4C7F4D00FFBEFFBD935C96CBFE24FF17" authorityName="Newman" authorityYear="1834" box="[314,470,151,177]" class="Insecta" family="Psychodidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="426" phylum="Arthropoda" rank="subFamily" subFamily="Psychodinae">Psychodinae</taxonomicName>
