, S. Ingrisch; 
<number id="3DFCEB14F659FFF9FF12FF20FF14FF53" box="[151,166,152,178]" pageId="26" pageNumber="27" value="1.0">1</number>
<number id="3DFCEB14F659FFF9FF28FF2FFF71FF50" box="[173,195,151,177]" pageId="26" pageNumber="27" value="3.0">3</number>
, Trieste, Monrupino, 
<date id="FF8ADFF3F659FFF9FE40FF20FDE5FF53" box="[453,599,152,178]" pageId="26" pageNumber="27" value="1991-07-27">
<number id="3DFCEB14F659FFF9FE40FF20FE57FF53" box="[453,485,152,178]" pageId="26" pageNumber="27" value="27.0">27</number>
