<paragraph id="26222454A3E1725686FADFAA4E0917C4" pageId="1" pageNumber="130">
<taxonomicName id="3A9CBDBC498257B9926C882CFCA9BC6E" family="Cecidomyiidae" genus="Pulmilomyia" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Pulmilomyia artemisiae" order="Diptera" pageId="1" pageNumber="130" rank="species" species="artemisiae">Pulmilomyia artemisiae</taxonomicName>
