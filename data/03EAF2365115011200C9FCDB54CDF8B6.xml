<taxonomicName id="4C4338A35115011202F5FA8D524CFAC2" authority="Shafee, Alam &amp; Agarwal" authorityName="Shafee, Alam &amp; Agarwal" box="[683,1150,1337,1361]" class="Insecta" family="Encyrtidae" genus="Psyllaephagus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="34" phylum="Arthropoda" rank="species" species="aligarhensis">
<emphasis id="B9379F325115011202F5FA8D5560FAC2" box="[683,850,1337,1361]" italics="true" pageId="1" pageNumber="34">P. aligarhensis</emphasis>
Shafee, Alam &amp; Agarwal
</taxonomicName>
, 
<taxonomicName id="4C4338A35115011204D5FA8D5348FAC1" authority="Hayat" authorityName="Hayat" box="[1163,1402,1337,1362]" class="Insecta" family="Encyrtidae" genus="Psyllaephagus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="34" phylum="Arthropoda" rank="species" species="bengalensis">
<emphasis id="B9379F325115011204D5FA8D531EFAC2" box="[1163,1324,1337,1361]" italics="true" pageId="1" pageNumber="34">P. bengalensis</emphasis>
