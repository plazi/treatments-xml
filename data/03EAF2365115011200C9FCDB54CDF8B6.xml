<treatmentCitation id="0AE2653151150112016EFC055434FC5B" author="Ashmead" box="[304,518,946,968]" page="382" pageId="1" pageNumber="34" year="1900">
<bibRefCitation id="EFD23ED151150112016EFC0557E1FC5B" author="Ashmead" box="[304,467,946,968]" pageId="1" pageNumber="34" refString="Ashmead, W. H (1900) On the genera of chalcid-flies belonging to the sub-family Encyrtinae. Proceedings of the United States National Museum, 22, 323 - 412." type="journal article" year="1900">
Ashmead, 
<number id="3D8B51075115011201C3FC0457E1FC5B" box="[413,467,947,968]" pageId="1" pageNumber="34" value="1900.0">1900</number>
