<taxonomicName id="9B4D5849B2769D5AA41CF8D166DB1F45" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33656" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole centeotl Wheeler" lsidName-HNS="Pheidole centeotl Wheeler" order="Hymenoptera" pageNumber="608" phylum="Arthropoda" rank="species" species="centeotl">centeotl</taxonomicName>
, 
<taxonomicName id="26C82AD3A1DF97ADCEB2F190599080DD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33658" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole cerebrosior Wheeler" lsidName-HNS="Pheidole cerebrosior Wheeler" order="Hymenoptera" pageNumber="608" phylum="Arthropoda" rank="species" species="cerebrosior">cerebrosior</taxonomicName>
