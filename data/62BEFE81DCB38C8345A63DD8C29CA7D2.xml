, 
<taxonomicName id="6289631D529600C903488342CED9B34D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33762" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole gilvescens Creighton &amp; Gregg" lsidName-HNS="Pheidole gilvescens Creighton &amp; Gregg" order="Hymenoptera" pageNumber="608" phylum="Arthropoda" rank="species" species="gilvescens">gilvescens</taxonomicName>
, 
