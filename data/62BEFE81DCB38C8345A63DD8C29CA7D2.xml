<taxonomicName id="A0B062E2D2B16F8844F24DC7CDCC74E9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181974" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole macrops Wilson" lsidName-HNS="Pheidole macrops Wilson" order="Hymenoptera" pageNumber="608" phylum="Arthropoda" rank="species" species="macrops">macrops</taxonomicName>
, 
<taxonomicName id="4D368170F2A31A5A24F95AA3442DABB6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33856" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole marcidula Wheeler" lsidName-HNS="Pheidole marcidula Wheeler" order="Hymenoptera" pageNumber="608" phylum="Arthropoda" rank="species" species="marcidula">marcidula</taxonomicName>
, 
