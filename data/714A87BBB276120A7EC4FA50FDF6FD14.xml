<number id="4F2B248AB275120A7DB1FEF4FC28FE5B" box="[994,1008,379,403]" pageId="26" pageNumber="527" value="2.0">2</number>
and 
<number id="4F2B248AB275120A7A7AFEF4FBEFFE5B" box="[1065,1079,379,403]" pageId="26" pageNumber="527" value="3.0">3</number>
