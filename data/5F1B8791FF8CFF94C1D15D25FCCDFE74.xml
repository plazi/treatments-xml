<taxonomicName id="10B24D04FF8DFF94C2DB5F64FBD3FEBC" authorityName="Gutu" authorityYear="1989" box="[925,1098,295,321]" class="Malacostraca" family="Metapseudidae" genus="Julmarichardia" kingdom="Animalia" order="Tanaidacea" pageId="5" pageNumber="82" phylum="Arthropoda" rank="genus">
