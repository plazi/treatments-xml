<quantity id="4B339B311226FFB866992D376725301E" box="[709,800,592,614]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="10" pageNumber="57" unit="mm" value="0.45">0.45 mm</quantity>
