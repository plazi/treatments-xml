<paragraph id="1D3A3CB889F379B2EC0B5733C60FD952" pageId="25" pageNumber="574" type="mainText">
[1] 
<taxonomicName id="0633ED0683473C5B994F30785F358B9F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25261" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus gladiator (Mayr)" order="Hymenoptera" pageId="25" pageNumber="574" phylum="Arthropoda" rank="species" species="gladiator">Anochetus gladiator</taxonomicName>
</paragraph>
