<quantity id="4CF39B5EFFECFF8BFE942181FE6AFA25" box="[357,450,1415,1442]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="2" pageNumber="3" unit="mm" value="5.3">5.3 mm</quantity>
