<taxonomicName id="4C6F79EAFFF8C820FF7AFDF3FEFFB63D" authorityName="Allman" authorityYear="1859" box="[182,373,546,572]" class="Hydrozoa" family="Campanulariidae" genus="Laomedea" kingdom="Animalia" order="Leptothecata" pageId="19" pageNumber="170" phylum="Cnidaria" rank="species" species="loveni">
