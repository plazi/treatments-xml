<paragraph id="8B923A3D4F382C32F7B5319DF061B968" blockId="14.[151,1437,1268,2016]" pageId="14" pageNumber="79">
<emphasis id="B959E62F4F382C32F7B5319DF2E3B954" bold="true" box="[151,386,1268,1295]" pageId="14" pageNumber="79">Material examined.</emphasis>
<materialsCitation id="3B4530604F382C32F6A8319CF061B968" ID-GBIF-Occurrence="2365833270" collectionCode="AM" collectorName="Sydney, F. H" country="Australia" location="New South Wales" pageId="14" pageNumber="79" specimenCount="1" stateProvince="New South Wales" typeStatus="holotype">
