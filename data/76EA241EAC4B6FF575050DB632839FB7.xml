<taxonomicName id="F380271AD897B15BCD0E2D0E763D73D3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27178" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus sibreei Forel" order="Hymenoptera" pageNumber="53" phylum="Arthropoda" rank="species" species="sibreei" status="n. sp.">Camponotus sibreei</taxonomicName>
