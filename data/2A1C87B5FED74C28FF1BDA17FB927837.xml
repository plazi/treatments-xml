<caption id="F6CA662BFED54C2EFEF2D9C3FE407C1D" ID-DOI="http://doi.org/10.5281/zenodo.3761340" ID-Zenodo-Dep="3761340" httpUri="https://zenodo.org/record/3761340/files/figure.png" pageId="323" pageNumber="324" startId="323.[259,315,1712,1732]" targetBox="[260,1383,257,1632]" targetPageId="323">
<paragraph id="A20A36A3FED54C2EFEF2D9C3FE957DC2" blockId="323.[258,1387,1712,1819]" box="[259,346,1712,1732]" pageId="323" pageNumber="324">
