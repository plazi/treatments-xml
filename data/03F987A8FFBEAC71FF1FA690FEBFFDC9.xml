<quantity id="4CA89B5BFFBEAC70FD73A355F931F8AB" box="[763,856,1852,1878]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="6" pageNumber="25" unit="mm" value="1.9">1.9 mm</quantity>
