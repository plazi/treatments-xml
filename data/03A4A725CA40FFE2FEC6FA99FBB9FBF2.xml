<collectingRegion id="49C9D8D1CA46FFE2FD8EFD61FCC9FC9A" box="[576,892,764,790]" country="Indonesia" name="Kalimantan Barat" pageId="20" pageNumber="21">West Kalimantan Province</collectingRegion>
