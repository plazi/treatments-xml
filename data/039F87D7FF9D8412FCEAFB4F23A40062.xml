<taxonomicName id="4C364D42FF9F8413FE51F93320CE028A" box="[498,706,1745,1764]" class="Insecta" family="Cosmopterigidae" genus="Hyposmocoma" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="29" phylum="Arthropoda" rank="species" species="undetermined">
