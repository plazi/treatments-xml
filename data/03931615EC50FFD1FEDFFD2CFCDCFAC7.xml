<emphasis id="B94E7B11EC50FFD1FEDFFD2CFD32FD40" bold="true" box="[264,672,724,750]" pageId="32" pageNumber="33">
Genus 
<taxonomicName id="4C3ADC80EC50FFD1FE88FD2CFD32FD40" authority="Claus, 1879" authorityName="Claus" authorityYear="1879" box="[351,672,724,750]" class="Malacostraca" family="Lestrigonidae" genus="Phronimopsis" kingdom="Animalia" order="Amphipoda" pageId="32" pageNumber="33" phylum="Arthropoda" rank="genus">
