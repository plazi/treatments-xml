<taxonomicName id="FAC09D59D7F360744DE50BE831BD27CF" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rhododendron canadense" order="Ericales" pageId="7" pageNumber="328" phylum="Tracheophyta" rank="species" species="canadense">Rhododendron canadense</taxonomicName>
