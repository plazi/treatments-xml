<taxonomicName id="A4A6CCA71517ED9B1B8DB7C038D32719" authority="H.Srivastava, 1962" authorityName="H.Srivastava" authorityYear="1962" class="Sordariomycetes" family="Diaporthaceae" genus="Subramanella" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Subramanella arecae" order="Diaporthales" pageId="0" pageNumber="49" phylum="Ascomycota" rank="species" species="arecae">Subramanella arecae</taxonomicName>
