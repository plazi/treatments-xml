<paragraph id="8BC7CA65FFD86D77D99D6C83804E4DA2" blockId="3.[151,1437,153,1151]" box="[199,1144,405,431]" pageId="3" pageNumber="574">
Female 
<typeStatus id="54C374C7FFD86D77D87B6C8385B34DA2" box="[289,389,405,431]" pageId="3" pageNumber="574">paratype</typeStatus>
. Length, 
<quantity id="4C806780FFD86D77D8B66C8386674DA2" box="[492,593,405,431]" metricMagnitude="-2" metricUnit="m" metricValue="1.14" pageId="3" pageNumber="574" unit="mm" value="11.4">11.4 mm</quantity>
