<taxonomicName id="4C78B1E6FFD96D76D81B6A9386234B90" authority="Shear &amp; Crawford, 2019" authorityName="Shear &amp; Crawford" authorityYear="2019" box="[321,533,1925,1949]" class="Diplopoda" family="Conotylidae" genus="Taiyutyla" kingdom="Animalia" order="Chordeumatida" pageId="2" pageNumber="573" phylum="Arthropoda" rank="species" species="caseophila" status="new species">Taiyutyla caseophila</taxonomicName>
