<taxonomicName id="C79B8EE3FF8FFFB164F7FB2129F945CF" box="[484,641,1254,1278]" class="Actinopterygii" family="Poeciliidae" genus="Phalloptychus" kingdom="Animalia" order="Cyprinodontiformes" pageId="42" pageNumber="43" phylum="Chordata" rank="genus">
<emphasis id="32EF2972FF8FFFB164F7FB2129F945CF" bold="true" box="[484,641,1254,1278]" italics="true" pageId="42" pageNumber="43">Phalloptychus</emphasis>
