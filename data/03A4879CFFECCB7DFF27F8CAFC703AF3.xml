<taxonomicName id="4C0D4D09FFECCB7CFE96F88CFC983C5F" authority="Balakrishnan, 1985" authorityName="Balakrishnan" authorityYear="1985" box="[294,772,1894,1916]" class="Arachnida" family="Galumnidae" genus="Indogalumna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="microsulcata">
