<number id="BD4224B9FF9EFFE2FB95FC98FB83267B" box="[1029,1059,827,853]" pageId="16" pageNumber="17" value="30.0">30</number>
(
<collectionCode id="6D9BAE5BFF9EFFE2FBA4FC98FBD4267B" box="[1076,1140,827,853]" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="16" pageNumber="17">ZSM</collectionCode>
