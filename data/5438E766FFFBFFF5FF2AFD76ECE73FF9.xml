<emphasis id="EEE58A62FFF8FFF5FD90FD31EE9D3F98" box="[632,685,674,696]" italics="true" pageId="2" pageNumber="1913">dirus</emphasis>
</taxonomicName>
, 
<taxonomicName id="1B912DF3FFF8FFF5FD57FD31EF193F97" box="[703,809,674,695]" class="Mammalia" family="Canidae" genus="Aenocyon" kingdom="Animalia" order="Carnivora" pageId="2" pageNumber="1913" phylum="Chordata" rank="species" species="milleri">
