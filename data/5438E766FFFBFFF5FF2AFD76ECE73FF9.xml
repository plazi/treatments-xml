are 
<taxonomicName id="1B912DF3FFF8FFF5FDBCFD31EE9D3F98" box="[596,685,674,696]" class="Mammalia" family="Canidae" genus="Aenocyon" kingdom="Animalia" order="Carnivora" pageId="2" pageNumber="1913" phylum="Chordata" rank="species" species="dirus">
