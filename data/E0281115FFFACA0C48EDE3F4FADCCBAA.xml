<paragraph id="683EA003FFFACA0248EDE3F4FC86CDD9" blockId="6.[151,783,979,1039]" box="[151,783,979,1006]" pageId="6" pageNumber="407">
<heading id="3376176FFFFACA0248EDE3F4FC86CDD9" bold="true" box="[151,783,979,1006]" fontSize="11" level="1" pageId="6" pageNumber="407" reason="1">
<emphasis id="5AF57C11FFFACA0248EDE3F4FC86CDD9" bold="true" box="[151,783,979,1006]" pageId="6" pageNumber="407">
<taxonomicName id="AF81DB80FFFACA0248EDE3F4FDF1CDD9" authority="Balboa &amp; Ayala" authorityName="Ayala" authorityYear="2017" box="[151,632,979,1006]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="407" phylum="Arthropoda" rank="species" species="guatemalensis" status="new species">
<emphasis id="5AF57C11FFFACA0248EDE3F4FE22CDDA" bold="true" box="[151,427,979,1005]" italics="true" pageId="6" pageNumber="407">Colletes guatemalensis</emphasis>
Balboa &amp; Ayala
</taxonomicName>
<taxonomicNameLabel id="41C6C16AFFFACA024A05E3F3FC86CDD9" box="[639,783,980,1006]" pageId="6" pageNumber="407" rank="species">new species</taxonomicNameLabel>
</emphasis>
