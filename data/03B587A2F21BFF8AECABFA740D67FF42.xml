<taxonomicName id="4C1C4D37F219FF8BEF62F94F0802F927" box="[862,1133,1670,1694]" class="Insecta" family="Baetidae" genus="Indocloeon" higherTaxonomySource="CoL" kingdom="Animalia" order="Ephemeroptera" pageId="30" pageNumber="481" phylum="Arthropoda" rank="subGenus" subGenus="Hindocloeon">
<emphasis id="B968EAA6F219FF8BEF62F94F0FBFF927" box="[862,976,1670,1694]" italics="true" pageId="30" pageNumber="481">Indocloeon</emphasis>
(
<emphasis id="B968EAA6F219FF8BEFDEF94F080AF927" box="[994,1125,1670,1694]" italics="true" pageId="30" pageNumber="481">Hindocloeon</emphasis>
)
</taxonomicName>
<emphasis id="B968EAA6F219FF8BE84BF94F088AF927" box="[1143,1253,1670,1694]" italics="true" pageId="30" pageNumber="481">indonesiae</emphasis>
