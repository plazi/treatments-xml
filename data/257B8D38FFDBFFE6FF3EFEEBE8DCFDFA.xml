<paragraph id="AD6D3C2EFFDBFFE6FF3EFEA2E854FE4E" blockId="10.[151,1437,294,537]" pageId="10" pageNumber="11">
<emphasis id="9FA6E03CFFDBFFE6FF3EFEA2E8D6FE6B" bold="true" box="[151,461,366,392]" pageId="10" pageNumber="11">
Distribution in 
<collectingCountry id="D5C57CBEFFDBFFE6FEFBFEA2E8D2FE6B" box="[338,457,366,392]" name="Colombia" pageId="10" pageNumber="11">Colombia</collectingCountry>
