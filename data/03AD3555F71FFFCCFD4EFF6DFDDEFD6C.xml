<elevation id="00296370F71FFFCEFD4EFB47FC84FB46" box="[714,788,1152,1179]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="12" pageNumber="109" unit="m" value="800.0">800 m</elevation>
