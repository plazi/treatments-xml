<heading id="306781E0AC37FD1BFF3AFD3AB6F6FC99" bold="true" box="[151,473,764,790]" fontSize="11" level="1" pageId="7" pageNumber="252" reason="1">
<emphasis id="59E4EA9EAC37FD1BFF3AFD3AB6F6FC99" bold="true" box="[151,473,764,790]" pageId="7" pageNumber="252">
<taxonomicName id="AC904D0FAC37FD1BFF3AFD3AB65AFC99" box="[151,373,764,790]" class="Insecta" family="Cicadellidae" genus="Sobrala" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="252" phylum="Arthropoda" rank="species" species="lamellaris" status="sp. nov.">
<emphasis id="59E4EA9EAC37FD1BFF3AFD3AB65AFC99" bold="true" box="[151,373,764,790]" italics="true" pageId="7" pageNumber="252">Sobrala lamellaris</emphasis>
</taxonomicName>
<taxonomicNameLabel id="42D757E5AC37FD1BFED1FD3AB6F6FC99" box="[380,473,764,790]" pageId="7" pageNumber="252" rank="species">sp. nov.</taxonomicNameLabel>
