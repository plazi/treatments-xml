<subSubSection id="76EC657CFF9EFF9D0DB81952FC72CC02" pageId="7" pageNumber="368" type="diagnosis">
<paragraph id="3E4936F7FF9EFF9D0DB81952FC72CC02" blockId="7.[151,1437,151,871]" pageId="7" pageNumber="368">
<emphasis id="0C82EAE5FF9EFF9D0DB81952FEC8CC26" bold="true" box="[199,322,809,835]" pageId="7" pageNumber="368">Diagnosis.</emphasis>
