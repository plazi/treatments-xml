<number id="4D0124A0FFECFFCBBAE6FA1685EFFA7D" box="[437,490,1447,1471]" pageId="12" pageNumber="13" value="0.84">0.84</number>
; minimum width across hemelytron 
<number id="4D0124A0FFECFFCBB8D9FA1787BAFA7D" box="[906,959,1446,1471]" pageId="12" pageNumber="13" value="0.68">0.68</number>
; maximum width across hemelytron 
<number id="4D0124A0FFECFFCBBE37FA16819DFA7D" box="[1380,1432,1447,1471]" pageId="12" pageNumber="13" value="1.02">1.02</number>
; and length of metatibia 
<number id="4D0124A0FFECFFCBBAF2FA7A85D0FA21" box="[417,469,1483,1507]" pageId="12" pageNumber="13" value="1.72">1.72</number>
