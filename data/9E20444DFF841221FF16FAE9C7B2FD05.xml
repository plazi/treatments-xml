<taxonomicName id="D1898ED8FF84122EFEB6F9B4C5B2FA72" authority="(DU BUYSSON)" baseAuthorityName="DU BUYSSON" baseAuthorityYear="1887" box="[311,711,1555,1577]" class="Insecta" family="Chrysididae" genus="Pseudochrysis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="468" phylum="Arthropoda" rank="species" species="marqueti">
