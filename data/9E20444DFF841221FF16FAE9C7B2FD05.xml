<taxonomicName id="D1898ED8FF8B1221FC84FE8CC48EFD1A" baseAuthorityName="DU BUYSSON" baseAuthorityYear="1887" box="[773,1019,299,321]" class="Insecta" family="Chrysididae" genus="Pseudochrysis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="469" phylum="Arthropoda" rank="species" species="marqueti">
<emphasis id="24FD2949FF8B1221FC84FE8CC48EFD1A" box="[773,1019,299,321]" italics="true" pageId="8" pageNumber="469">Pseudochrysis marqueti</emphasis>
</taxonomicName>
for 
<collectingCountry id="6E9EB5CBFF8B1221FF16FEEEC7B6FD05" box="[151,195,329,350]" name="Iran" pageId="8" pageNumber="469">Iran</collectingCountry>
.
</paragraph>
