<collectingCountry id="8785BDF6FFF9C138FE46FBBDFE68FBF1" box="[391,489,1054,1075]" name="Botswana" pageId="3" pageNumber="13">Botswana</collectingCountry>
, 
<collectingCountry id="8785BDF6FFF9C138FE32FBBDFDF0FBF1" box="[499,625,1054,1075]" name="South Africa" pageId="3" pageNumber="13">South Africa</collectingCountry>
, 
<collectingCountry id="8785BDF6FFF9C138FDBBFBBDFD7AFBF1" box="[634,763,1054,1075]" name="Mozambique" pageId="3" pageNumber="13">Mozambique</collectingCountry>
