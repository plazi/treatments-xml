<taxonomicName id="A19548337BD53065F06D14526D9FD87F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230298" genus="Macromichoides" lsidName="Uploaded - Macromichoides aculeatus var. pulchellus" lsidName-HNS="Uploaded - Macromichoides aculeatus var. pulchellus" pageNumber="208" rank="variety" species="aculeatus" variety="pulchellus">Macromichoides aculeatus Mayr var. pulchellus</taxonomicName>
