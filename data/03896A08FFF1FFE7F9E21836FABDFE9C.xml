<taxonomicName id="4C20A09DFFF1FFE8FBE41A7FFC64F8B2" authority="Forel, 1913" authorityName="Forel" authorityYear="1913" box="[657,920,1818,1843]" class="Insecta" family="Formicidae" genus="Stigmatomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="84" phylum="Arthropoda" rank="species" species="minutum">
<emphasis id="B954070CFFF1FFE8FBE41A7FFCF3F8B3" box="[657,783,1819,1842]" italics="true" pageId="7" pageNumber="84">S. minutum</emphasis>
Forel, 
<number id="3DE8C939FFF1FFE8FA2B1A7FFC64F8B2" box="[862,920,1819,1843]" pageId="7" pageNumber="84" value="1913.0">1913</number>
