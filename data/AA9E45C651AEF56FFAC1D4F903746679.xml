<taxonomicName id="75B37107DFF51EF0FA038618F0A9C485" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia oerstediana" order="Myrtales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="oerstediana">Conostegia oerstediana</taxonomicName>
