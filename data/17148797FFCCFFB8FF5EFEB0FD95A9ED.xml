<number id="297524A6FFCCFFB8FF07FE94FF59ACC8" box="[206,216,367,391]" pageId="7" pageNumber="125" value="1.0">1</number>
–2
</figureCitation>
, 
<figureCitation id="07862A04FFCCFFB8FECFFE94FEBFACC8" box="[262,318,367,391]" captionStart="FIGURES 9 – 12" captionStartId="6.[151,264,1940,1963]" captionTargetBox="[225,1365,210,1907]" captionTargetId="figure@6.[205,1381,193,1919]" captionTargetPageId="6" captionText="FIGURES 9 – 12. Orthogonius rogueti n. sp. (9. Habitus, male; 10. Habitus, female; 11. Aedeagus, right lateral view; 12. Apical lamella, dorsal view)" httpUri="https://zenodo.org/record/260683/files/figure.png" pageId="7" pageNumber="125">
9– 
