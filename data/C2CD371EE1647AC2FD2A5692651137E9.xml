<taxonomicName id="DF5B12178F04B6E26BB1794D603FDD00" class="Arachnida" family="Corinnidae" genus="Cambalida" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cambalida fagei" order="Araneae" pageId="24" pageNumber="91" phylum="Arthropoda" rank="species" species="fagei">Cambalida fagei</taxonomicName>
