<quantity id="4C853FA3FFE1FFDEFD87FEABFD0CC1AC" box="[608,677,363,384]" metricMagnitude="3" metricUnit="m" metricValue="9.49" pageId="13" pageNumber="64" unit="m" value="9490.0">9490m</quantity>
