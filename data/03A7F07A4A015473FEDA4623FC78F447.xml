(
<figureCitation id="13355DE94A015470FED3464EFF75F26F" box="[158,245,840,864]" captionStart="FIGURE 46" captionStartId="32.[151,250,1276,1299]" captionTargetBox="[151,1436,301,1255]" captionTargetId="figure-88@32.[151,1436,301,1255]" captionTargetPageId="32" captionText="FIGURE 46. Distribution of Neobarombiella variopennis (Jacoby, 1897), N. suturalis (Laboissière, 1923), N. lineata sp. n. and N. spielbergi sp. n." pageId="89" pageNumber="90">Figs 46</figureCitation>
, 
<figureCitation id="13355DE94A015470FF4C464EFEE4F26F" box="[257,356,840,864]" captionStart-0="FIGURES 161–163" captionStart-1="FIGURE 164" captionStartId-0="90.[151,263,1050,1073]" captionStartId-1="90.[151,250,1942,1965]" captionTargetBox-0="[279,1308,248,1019]" captionTargetBox-1="[161,1347,1444,1887]" captionTargetId-0="figure-151@90.[273,1314,242,1029]" captionTargetId-1="figure-155@90.[151,1436,1432,1920]" captionTargetPageId-0="90" captionTargetPageId-1="90" captionText-0="FIGURES 161–163. Neobarombiella lineata sp. n. FIGURE 161. Dorsal view; FIGURE 162. Aedeagus, a—ventral, b—dorsal, c—lateral; FIGURE 163. Antennomeres 1–4 of two different males (a, b) and two different females (c, d)." captionText-1="FIGURE 164. Holotype of Neobarombiella lineata sp. n., a. with labels, b. detail." pageId="89" pageNumber="90">161–164</figureCitation>
)
</paragraph>
