<taxonomicName id="4C7B4D7AFF83E469AB7052B04582FF16" authority="(Willd.) G. L. Nesom, S." authorityName="(Willd.) G.L. Nesom, S." box="[770,1153,223,248]" class="Magnoliopsida" family="Asteraceae" genus="Solidago" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="species" species="pilosum">
