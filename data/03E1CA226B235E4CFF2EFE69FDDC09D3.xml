wide; 
<number id="3D8069136B235E4CFB7CFC77FB6A0FEA" box="[1221,1270,906,931]" pageId="15" pageNumber="466" value="2.65">2.65</number>
high. Clypeus 
<number id="3D8069136B235E4CFF2EFC52FF540F8E" box="[151,200,943,967]" pageId="15" pageNumber="466" value="0.43">0.43</number>
high. Abdomen 
<number id="3D8069136B235E4CFE3CFC53FE340F8E" box="[389,424,942,967]" pageId="15" pageNumber="466" value="5.3">5.3</number>
long; 
<number id="3D8069136B235E4CFE49FC53FD8F0F8E" box="[496,531,942,967]" pageId="15" pageNumber="466" value="3.5">3.5</number>
