<location id="8E966043FF99F463FC87FEFB07AEC56C" LSID="urn:lsid:plazi:treatment:03E0878EFF98F463FF39F8B10767C620:8E966043FF99F463FC87FEFB07AEC56C" box="[809,985,310,335]" country="Mexico" municipality="Neotropical region" name="Nearctic region" pageId="11" pageNumber="412" stateProvince="Oaxaca">Nearctic region</location>
