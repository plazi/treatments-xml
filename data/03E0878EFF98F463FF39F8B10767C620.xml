<collectingMunicipality id="6B92ACE2FF99F463FB84FE97008BC550" box="[1066,1276,346,371]" pageId="11" pageNumber="412">Neotropical region</collectingMunicipality>
from 
<collectingRegion id="498DF87AFF99F463FAEFFE9601EFC550" box="[1345,1432,347,371]" country="Mexico" name="Oaxaca" pageId="11" pageNumber="412">Oaxaca</collectingRegion>
, 
<collectingCountry id="F35E7608FF99F463FF39FEB30487C5B4" box="[151,240,382,407]" name="Mexico" pageId="11" pageNumber="412">Mexico</collectingCountry>
.
</materialsCitation>
</paragraph>
