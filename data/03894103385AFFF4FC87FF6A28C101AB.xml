<taxonomicName id="4C208B96385AFFF4FC3EF9292F37010B" authorityName="Benine, Mariguela, Oliveira" authorityYear="2009" box="[906,1133,1779,1803]" class="Actinopterygii" family="Characidae" genus="Moenkhausia" kingdom="Animalia" order="Characiformes" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="forestii">
