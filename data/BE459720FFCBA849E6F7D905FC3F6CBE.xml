<collectingMunicipality id="D637BC4CFFCBA848E6A8D81BFBF26AEA" box="[966,1045,894,916]" pageId="6" pageNumber="765">Cairns</collectingMunicipality>
, 
<collectingCountry id="4EFB66A6FFCBA848E15AD81BFB476AEA" box="[1076,1184,894,916]" name="Australia" pageId="6" pageNumber="765">Australia</collectingCountry>
(
<collectingRegion id="F428E8D4FFCBA848E1ADD81BFAA96AEA" box="[1219,1358,894,916]" country="Australia" name="Queensland" pageId="6" pageNumber="765">Queensland</collectingRegion>
), at 
<location id="333370EDFFCBA848E648D8F9FC046ACC" LSID="urn:lsid:plazi:treatment:BE459720FFCBA849E6F7D905FC3F6CBE:333370EDFFCBA848E648D8F9FC046ACC" box="[806,995,924,946]" country="Australia" municipality="Cairns" name="Yorkney's Knob" pageId="6" pageNumber="765" stateProvince="Queensland">Yorkney’s Knob</location>
; intertidal in fine to medium-fine sand (
<typeStatus id="E9579894FFCBA848E600D8DEFC476AAE" box="[878,928,955,976]" pageId="6" pageNumber="765">type</typeStatus>
locality) (
<date id="425200F6FFCBA848E178D8DEFB546AAF" box="[1046,1203,955,977]" pageId="6" pageNumber="765" value="1993-10">
<collectingDate id="5216F91EFFCBA848E178D8DEFB546AAF" box="[1046,1203,955,977]" pageId="6" pageNumber="765" value="1993-10">October 1993</collectingDate>
</date>
