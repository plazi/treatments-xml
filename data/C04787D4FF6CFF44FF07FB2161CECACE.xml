<taxonomicName id="8FEE4D41FF6CFF47FF4DF89464A1CC0A" authorityName="Stiles" authorityYear="1908" box="[221,482,1843,1869]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="133" pageNumber="134" phylum="Arthropoda" rank="species" species="andersoni">
