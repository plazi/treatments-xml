<geoCoordinate id="17D5502B249FBDC6FD6CD8CB1815A5A2" box="[716,810,1673,1699]" degrees="15" direction="south" minutes="09" orientation="latitude" pageId="129" pageNumber="130" precision="925" value="-15.15">15°09’S</geoCoordinate>
and 39°05’– 
<geoCoordinate id="17D5502B249FBDC6FC66D8CB1F0DA5A2" box="[966,1074,1673,1699]" degrees="39" direction="west" minutes="10" orientation="longitude" pageId="129" pageNumber="130" precision="925" value="-39.166668">39°10’W</geoCoordinate>
