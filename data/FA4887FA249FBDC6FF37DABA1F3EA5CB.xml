<collectingCountry id="0AF6767C249FBDC6FF16D8F21BC1A5CB" box="[182,254,1712,1738]" name="Brazil" pageId="129" pageNumber="130">Brazil</collectingCountry>
near Una, 
<collectingRegion id="B025F80E249FBDC6FEDFD8F21AFCA5CB" box="[383,451,1712,1738]" country="Brazil" name="Bahia" pageId="129" pageNumber="130">Bahia</collectingRegion>
