<emphasis id="B963EA93007CFF9B98B1F837FE9BC1FA" bold="true" box="[151,289,1982,2005]" pageId="47" pageNumber="48">FIGURE 23.</emphasis>
<taxonomicName id="4C174D02007CFF9B990EF836FE54C1FA" authority="Blahnik &amp; Holzenthal, 2011" authorityName="Blahnik &amp; Holzenthal" authorityYear="2011" box="[296,494,1983,2005]" class="Magnoliopsida" family="Apocynaceae" genus="Mortoniella" kingdom="Plantae" order="Gentianales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="alicula" status="new species">
<emphasis id="B963EA93007CFF9B990EF836FE54C1FA" box="[296,494,1983,2005]" italics="true" pageId="47" pageNumber="48">Mortoniella alicula</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A25057E8007CFF9B99DAF836FDC3C1FA" box="[508,633,1983,2005]" pageId="47" pageNumber="48" rank="species">new species</taxonomicNameLabel>
