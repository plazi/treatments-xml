<emphasis id="B901EADFFF66FF55734BF838FC3DFDDF" bold="true" box="[873,924,1953,1977]" pageId="234" pageNumber="235">A–C</emphasis>
). 
<emphasis id="B901EADFFF66FF557396F838FC66FDDF" bold="true" box="[948,967,1953,1977]" pageId="234" pageNumber="235">A</emphasis>
, dorsal view; 
<emphasis id="B901EADFFF66FF557440F838FBD5FDDF" bold="true" box="[1122,1140,1953,1977]" pageId="234" pageNumber="235">B</emphasis>
, hypostomal apex; 
<emphasis id="B901EADFFF66FF55756AF838FAFAFDDF" bold="true" box="[1352,1371,1953,1977]" pageId="234" pageNumber="235">C</emphasis>
, peritreme. Female (
<emphasis id="B901EADFFF66FF55711CF822FECEFDB5" bold="true" box="[318,367,1979,2003]" pageId="234" pageNumber="235">D–F</emphasis>
). 
<emphasis id="B901EADFFF66FF5571A6F822FE36FDB5" bold="true" box="[388,407,1979,2003]" pageId="234" pageNumber="235">D</emphasis>
, hypostomal apex; 
<emphasis id="B901EADFFF66FF557244F822FDD9FDB5" bold="true" box="[614,632,1979,2003]" pageId="234" pageNumber="235">E</emphasis>
, peritreme; 
<emphasis id="B901EADFFF66FF5572DBF822FCA6FDB5" bold="true" box="[761,775,1979,2003]" pageId="234" pageNumber="235">F</emphasis>
