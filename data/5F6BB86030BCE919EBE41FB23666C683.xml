<taxonomicName id="785D56E28ECF044946B5670772EC7718" authority="Savigny, 1822" authorityName="Savigny" authorityYear="1822" class="Clitellata" family="Glossiphoniidae" genus="Clepsine" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Clepsine hyalina" order="Rhynchobdellida" pageId="3" pageNumber="40" phylum="Annelida" rank="species" species="hyalina">Clepsine hyalina</taxonomicName>
