<taxonomicName id="4C3B4D0E7310AB4241E2FEF6FD6EFEA4" box="[672,762,294,319]" class="Insecta" family="Megachilidae" genus="Hoplitis" kingdom="Animalia" order="Hymenoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="genus">
