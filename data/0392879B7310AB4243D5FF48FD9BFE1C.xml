<taxonomicName id="4C3B4D0E7310AB4240C7FEF7FB35FEA4" authority="(Cresson)" authorityName="Cresson" baseAuthorityName="Cresson" baseAuthorityYear="1864" box="[901,1185,294,319]" class="Insecta" family="Megachilidae" genus="Hoplitis" kingdom="Animalia" order="Hymenoptera" pageId="101" pageNumber="102" phylum="Arthropoda" rank="species" species="pilosifrons">
