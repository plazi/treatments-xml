<collectingRegion id="E47585D71105FFEAFCE3F94F45A9F907" box="[779,998,1671,1696]" country="Mexico" name="Baja California Sur" pageId="16" pageNumber="167">
Baja 
<collectingRegion id="E47585D71105FFEAFCADF94F45A9F907" box="[837,998,1671,1696]" inferredBySuffix="true" pageId="16" pageNumber="167">
<collectingRegion id="E47585D71105FFEAFCADF94F45F8F938" box="[837,951,1671,1696]" country="United States of America" name="California" pageId="16" pageNumber="167">California</collectingRegion>
Sur
</collectingRegion>
</collectingRegion>
, 
<collectingRegion id="E47585D71105FFEAFC19F94F4207F938" box="[1009,1096,1671,1696]" country="Mexico" name="Sinaloa" pageId="16" pageNumber="167">Sinaloa</collectingRegion>
