<collectingRegion id="E47585D71105FFEAFBBAF94F42EEF938" box="[1106,1185,1671,1696]" country="Mexico" name="Jalisco" pageId="16" pageNumber="167">Jalisco</collectingRegion>
, 
<collectingRegion id="E47585D71105FFEAFB45F94F4363F938" box="[1197,1324,1671,1696]" country="Mexico" name="Michoacan de Ocampo" pageId="16" pageNumber="167">Michoacán</collectingRegion>
, 
<collectingRegion id="E47585D71105FFEAFADFF94F43D3F907" box="[1335,1436,1671,1695]" country="Mexico" name="Guerrero" pageId="16" pageNumber="167">Guerrero</collectingRegion>
and 
<collectingRegion id="E47585D71105FFEAFF2EF9634755F95B" box="[198,282,1707,1731]" country="Mexico" name="Oaxaca" pageId="16" pageNumber="167">Oaxaca</collectingRegion>
in 
<collectingRegion id="E47585D71105FFEAFED6F96347D9F95C" box="[318,406,1707,1732]" country="Mexico" name="Mexico" pageId="16" pageNumber="167">
<collectingCountry id="5EA60BA51105FFEAFED6F96347D9F95C" box="[318,406,1707,1732]" name="Mexico" pageId="16" pageNumber="167">Mexico</collectingCountry>
</collectingRegion>
, and in 
<collectingCountry id="5EA60BA51105FFEAFE06F9634423F95C" box="[494,620,1707,1732]" name="Costa Rica" pageId="16" pageNumber="167">Costa Rica</collectingCountry>
