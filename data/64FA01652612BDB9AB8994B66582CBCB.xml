<taxonomicName id="10A2E2A56DEB3030808720DE3A0AAB2D" authority="L." class="Magnoliopsida" family="Fabaceae" genus="Genista" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="491" phylum="Tracheophyta" rank="species" species="germanica">
Genista 
<normalizedToken id="CDE7D4F6906DFF988018F3185191C55A" originalValue="germánica" pageId="null" pageNumber="491">germanica</normalizedToken>
<authorityName id="694D670930062E0D63EB4DBCC2E08A4A" pageId="null" pageNumber="491">L.</authorityName>
</taxonomicName>
