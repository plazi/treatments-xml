<taxonomicName id="5EEC4D5CFF8AFFF6FDA8A759FD24FE94" box="[579,714,261,284]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="gigantus">
