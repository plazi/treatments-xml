<paragraph id="C77336C9A31AFF8CC19EF92CFCDDF947" blockId="5.[151,1437,1720,1961]" box="[199,787,1720,1745]" pageId="5" pageNumber="586">
<materialsCitation id="77A43C94A31AFF8CC19EF92CFCC1F947" ID-GBIF-Occurrence="3459912304" box="[199,783,1720,1745]" pageId="5" pageNumber="586" specimenCount="2" specimenCount-female="1" specimenCount-male="1" typeStatus="paratype">
