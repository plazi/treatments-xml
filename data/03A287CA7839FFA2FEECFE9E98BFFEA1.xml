<taxonomicName id="4C0B4D5F7839FFA3FE46F8829E0AF8E8" box="[418,574,1792,1815]" class="Insecta" family="Carabidae" genus="Agra" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="granodeoro">
<emphasis id="B97FEACE7839FFA3FE46F8829E0AF8E8" box="[418,574,1792,1815]" italics="true" pageId="44" pageNumber="45">
A. 
<taxonomicName id="4C0B4D5F7839FFA3FE25F8829E0AF8E8" box="[449,574,1792,1815]" pageId="44" pageNumber="45" rank="species" species="granodeoro">granodeoro</taxonomicName>
