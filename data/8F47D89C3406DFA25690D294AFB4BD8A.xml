<paragraph id="4574BF0CA31AE7988310FC65F5390870" pageId="0" pageNumber="9042">
<taxonomicName id="5F0A4E1159D9DB6DCA31E3383B96CDA2" authority="Gravenhorst, 1829" authorityName="Gravenhorst" authorityYear="1829" class="Insecta" family="Ichneumonidae" genus="Pezomachus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pezomachus annulicornis" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="annulicornis">Pezomachus annulicornis</taxonomicName>
