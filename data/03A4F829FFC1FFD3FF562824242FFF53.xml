<subSubSection id="C3171AB4FFCFFFD3FF042EE6242FFF53" pageId="9" pageNumber="48" type="etymology">
<paragraph id="8BB2493FFFCFFFD3FF042EE6242FFF53" blockId="9.[151,1436,151,212]" pageId="9" pageNumber="48">
<emphasis id="B979952DFFCFFFD3FF042EE62756FF37" bold="true" box="[197,332,152,176]" pageId="9" pageNumber="48">Etymology.</emphasis>
