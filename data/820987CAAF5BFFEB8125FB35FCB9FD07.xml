<taxonomicName id="CDA04D5FAF5BFFE88125FB35FEDAFB71" authority="Cope ED, 1876" authorityName="Cope ED" authorityYear="1876" baseAuthorityName="Cope" box="[55,268,1245,1265]" class="Amphibia" family="Scapherpetontidae" genus="Scapherpeton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Caudata" pageId="17" pageNumber="357" phylum="Chordata" rank="species" species="favosum" status="sp. nov .">
