<normalizedToken id="CEF4E3D280D599D946BEC6A017FEBB5D" originalValue="défaut">defaut</normalizedToken>
au 
<taxonomicName id="716727619614C194299837908CF29F9F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26433" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus dufouri Forel" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="species" species="dufouri">C. Dufouri</taxonomicName>
