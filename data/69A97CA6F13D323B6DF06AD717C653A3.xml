<taxonomicName id="F26FC75FFB507EDBCDB39B9C27697EFD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26433" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus dufouri Forel" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="species" species="dufouri">C. Dufouri</taxonomicName>
