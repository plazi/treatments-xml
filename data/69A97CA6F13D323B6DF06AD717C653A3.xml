<taxonomicName id="D446D364574A1471F6A6ED0A95EE80D0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24840" authorityName="Latreille" authorityYear="1804" class="Insecta" family="Formicidae" genus="Odontomachus" kingdom="Animalia" lsidName-HNS="Odontomachus Latreille" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="genus">Odontomachus</taxonomicName>
