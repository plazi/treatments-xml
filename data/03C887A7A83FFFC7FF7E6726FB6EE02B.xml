<taxonomicName id="4C614D32A83DFFC7FC096274FA67E2B6" authority="(Klautau &amp; Valentine, 2003)" authorityName="Klautau &amp; Valentine" authorityYear="2003" baseAuthorityName="Klautau &amp; Valentine" baseAuthorityYear="2003" box="[992,1431,367,392]" class="Calcarea" family="Clathrinidae" genus="Arturia" kingdom="Animalia" order="Clathrinida" pageId="10" pageNumber="311" phylum="Porifera" rank="species" species="hirsuta">
<emphasis id="B915EAA3A83DFFC7FC096274FBBEE2B6" box="[992,1102,368,392]" italics="true" pageId="10" pageNumber="311">A. hirsuta</emphasis>
