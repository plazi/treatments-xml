<taxonomicName id="5CB44D7AFFB4FF85FC9A5C89FB1BFC3C" authority="Shear, 1974" authorityName="Shear" authorityYear="1974" box="[977,1252,903,930]" class="Diplopoda" family="Conotylidae" genus="Bollmanella" kingdom="Animalia" order="Chordeumatida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="laminata">
