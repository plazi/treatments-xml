<taxonomicName id="5CB44D7AFFB4FF85FEF55B85FD2FFB00" authority="Shear, 1974" authorityName="Shear" authorityYear="1974" box="[446,720,1155,1182]" class="Diplopoda" family="Conotylidae" genus="Bifurcatella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chordeumatida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="bifurcata">
<emphasis id="A9C0EAEBFFB4FF85FEF55B85FDBBFB00" box="[446,580,1156,1182]" italics="true" pageId="29" pageNumber="30">B. bifurcata</emphasis>
<bibRefCitation id="FF254B08FFB4FF85FD005B82FD2FFB00" author="Shear, W. A." box="[587,720,1155,1182]" pageId="29" pageNumber="30" pagination="134 - 146" refId="ref44113" refString="Shear, W. A. (1974) The milliped genus Bollmanella (Diplopoda, Chordeumida, Conotylidae). Psyche, 81, 134 - 146. https: // doi. org / 10.1155 / 1974 / 51907" type="journal article" year="1974">Shear, 1974</bibRefCitation>
</taxonomicName>
from Wallowa Co., 
<collectingRegion id="5970F81BFFB4FF85FCFD5B85FBF2FB00" box="[950,1037,1156,1182]" country="United States of America" name="Oregon" pageId="29" pageNumber="30">Oregon</collectingRegion>
, are moved to 
<emphasis id="A9C0EAEBFFB4FF85FBFF5B82FA63FB00" bold="true" box="[1204,1436,1155,1182]" pageId="29" pageNumber="30">
<emphasis id="A9C0EAEBFFB4FF85FBFF5B82FAB9FB00" bold="true" box="[1204,1350,1155,1182]" italics="true" pageId="29" pageNumber="30">
<taxonomicName id="5CB44D7AFFB4FF85FBFF5B82FABFFB00" authority="Shear &amp; Richart &amp; Wong, 2020" authorityName="Shear &amp; Richart &amp; Wong" authorityYear="2020" box="[1204,1344,1155,1182]" class="Diplopoda" family="Conotylidae" genus="Bifurcatella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chordeumatida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus" status="n. gen.">Bifurcatella</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="B2F35790FFB4FF85FA065B85FA63FB00" box="[1357,1436,1156,1182]" pageId="29" pageNumber="30" rank="genus">n. gen.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="5CB44D7AFFB4FF85FF8C5BA9FE11FB5C" authority="Shear, 1974" authorityName="Shear" authorityYear="1974" box="[199,494,1191,1218]" class="Diplopoda" family="Conotylidae" genus="Complicatella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chordeumatida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="complicata">
<emphasis id="A9C0EAEBFFB4FF85FF8C5BA9FE9DFB5C" box="[199,354,1192,1218]" italics="true" pageId="29" pageNumber="30">B. complicata</emphasis>
<bibRefCitation id="FF254B08FFB4FF85FE225BA6FE11FB5C" author="Shear, W. A." box="[361,494,1191,1218]" pageId="29" pageNumber="30" pagination="134 - 146" refId="ref44113" refString="Shear, W. A. (1974) The milliped genus Bollmanella (Diplopoda, Chordeumida, Conotylidae). Psyche, 81, 134 - 146. https: // doi. org / 10.1155 / 1974 / 51907" type="journal article" year="1974">Shear, 1974</bibRefCitation>
</taxonomicName>
from Mason Co., 
<collectingRegion id="5970F81BFFB4FF85FDF05BA6FCBFFB5F" box="[699,832,1191,1217]" country="United States of America" name="Washington" pageId="29" pageNumber="30">Washington</collectingRegion>
