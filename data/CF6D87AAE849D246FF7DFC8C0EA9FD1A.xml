<taxonomicName id="80C44D3FE849D249FEA2F95A0E14F963" authority="Canu &amp; Bassler, 1929" authorityName="Canu &amp; Bassler" authorityYear="1929" box="[328,850,1748,1770]" class="Gymnolaemata" family="Membraniporidae" genus="Acanthodesia" kingdom="Animalia" order="Cheilostomatida" pageId="7" pageNumber="8" phylum="Bryozoa" rank="species" species="lamellosa">
