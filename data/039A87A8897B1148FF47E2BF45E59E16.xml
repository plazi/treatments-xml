<emphasis id="B947EAAC897D1148FEA5E2C447289FE6" bold="true" box="[373,530,152,179]" pageId="4" pageNumber="275">
<typeStatus id="5488881C897D1148FEA5E2C444929FE6" box="[373,424,153,179]" pageId="4" pageNumber="275">type</typeStatus>
locality.
</emphasis>
<location id="8EEC6065897D1148FDCEE2C446619FE6" LSID="urn:lsid:plazi:treatment:039A87A8897B1148FF47E2BF45E59E16:8EEC6065897D1148FDCEE2C446619FE6" box="[542,859,153,179]" country="China" county="With" latitude="19.144722" longLatPrecision="21" longitude="109.56833" municipality="South" name="Yinggeling Nature Reserve" pageId="4" pageNumber="275">Yinggeling Nature Reserve</location>
