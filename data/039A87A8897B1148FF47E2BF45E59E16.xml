<quantity id="4CCB9B5B897D1148FA97E2E040A69F82" box="[1351,1436,189,215]" metricMagnitude="3" metricUnit="m" metricValue="1.8119999999999998" pageId="4" pageNumber="275" unit="m" value="1812.0">
<elevation id="001ED18D897D1148FA97E2E040A69F82" box="[1351,1436,189,215]" metricMagnitude="3" metricUnit="m" metricValue="1.8119999999999998" pageId="4" pageNumber="275" unit="m" value="1812.0">1812 m</elevation>
</quantity>
in altitude. 
