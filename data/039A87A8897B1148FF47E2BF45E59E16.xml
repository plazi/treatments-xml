<location id="8EEC6065897D1148FB2AE2C440A69FE6" LSID="urn:lsid:plazi:treatment:039A87A8897B1148FF47E2BF45E59E16:8EEC6065897D1148FB2AE2C440A69FE6" box="[1274,1436,153,179]" country="China" county="With" latitude="19.144722" longLatPrecision="21" longitude="109.56833" municipality="South" name="Hainan Island" pageId="4" pageNumber="275">Hainan Island</location>
(109°11′27″– 
<geoCoordinate id="EE075079897D1148FEFBE2E144859F82" box="[299,447,188,215]" degrees="109" direction="east" minutes="34" orientation="longitude" pageId="4" pageNumber="275" precision="15" seconds="06" value="109.56833">109°34′06″E</geoCoordinate>
, 18°49′30″– 
<geoCoordinate id="EE075079897D1148FD9DE2E147E99F82" box="[589,723,188,215]" degrees="19" direction="north" minutes="08" orientation="latitude" pageId="4" pageNumber="275" precision="15" seconds="41" value="19.144722">19°08′41″N</geoCoordinate>
