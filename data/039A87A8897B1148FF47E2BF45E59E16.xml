<quantity id="4CCB9B5B897D1148FB0AE2E040189F82" box="[1242,1314,189,215]" metricMagnitude="2" metricUnit="m" metricValue="1.7" pageId="4" pageNumber="275" unit="m" value="170.0">
<elevation id="001ED18D897D1148FB0AE2E040189F82" box="[1242,1314,189,215]" metricMagnitude="2" metricUnit="m" metricValue="1.7" pageId="4" pageNumber="275" unit="m" value="170.0">170 m</elevation>
