<quantity id="4CCB9B5B897D1148FC1FE2E146CE9F82" box="[975,1012,188,215]" metricMagnitude="-2" metricUnit="m" metricValue="5.08" pageId="4" pageNumber="275" unit="in" value="2.0">
<superScript id="7C469BF6897D1148FC1FE2E146ED9F9F" attach="left" box="[975,983,188,202]" fontSize="6" pageId="4" pageNumber="275">2</superScript>
in
</quantity>
