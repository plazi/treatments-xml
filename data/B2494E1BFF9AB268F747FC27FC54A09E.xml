<taxonomicName id="FDE0848EFF9AB268F772FBADFC5FA173" authorityName="Dendy" authorityYear="1892" box="[827,955,1121,1145]" class="Calcarea" family="Heteropiidae" kingdom="Animalia" order="Leucosolenida" pageId="33" pageNumber="236" phylum="Porifera" rank="family">Heteropiidae</taxonomicName>
