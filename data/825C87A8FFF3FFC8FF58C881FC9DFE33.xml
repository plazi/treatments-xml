<taxonomicName id="CDF54D3DFFF3FFC8FF58C881FD07FF6C" authority="Gorochov, 2007" authorityName="Gorochov" authorityYear="2007" box="[151,733,221,247]" class="Insecta" family="Haglotettigoniidae" genus="Uvaroviella" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="nebulosa" subGenus="Holacla">
<emphasis id="3881EAACFFF3FFC8FF58C881FD07FF6C" bold="true" box="[151,733,221,247]" pageId="18" pageNumber="219">
<emphasis id="3881EAACFFF3FFC8FF58C881FEF9FF6C" bold="true" box="[151,291,221,247]" italics="true" pageId="18" pageNumber="219">Uvaroviella</emphasis>
(
<emphasis id="3881EAACFFF3FFC8FEFBC881FE4EFF6C" bold="true" box="[308,404,221,247]" italics="true" pageId="18" pageNumber="219">Holacla</emphasis>
) 
<emphasis id="3881EAACFFF3FFC8FE6AC881FDC9FF6C" bold="true" box="[421,531,221,247]" italics="true" pageId="18" pageNumber="219">nebulosa</emphasis>
<bibRefCitation id="6E644B4FFFF3FFC8FDD6C881FD07FF6C" author="Gorochov" box="[537,733,221,247]" pageId="18" pageNumber="219" refString="Gorochov, A. V. (2007) New and little known crickets of the subfamily Phalangopsinae (Orthoptera, Gryllidae). 4. Neotropical genus Uvaroviella. Zoologicheskii Zhurnal, 86 (10), 1183 - 1195. http: // dx. doi. org / 10.1134 / s 001387380707010 x" type="journal article" year="2007">Gorochov, 2007</bibRefCitation>
</emphasis>
</taxonomicName>
</heading>
</paragraph>
