<taxonomicName id="4C6844F76E1AD21CFDD6F887FCDEE432" authority="(Sowerby, 1841)" baseAuthorityName="Sowerby" baseAuthorityYear="1841" box="[514,801,1830,1847]" class="Gastropoda" family="Muricidae" genus="Attiliosa" kingdom="Animalia" order="Neogastropoda" pageId="13" pageNumber="450" phylum="Mollusca" rank="species" species="nodulifera">
