, 
<geoCoordinate id="B3C9502BFF85FFCEFB44F96D8905F88E" box="[1192,1339,1775,1803]" degrees="39" direction="west" minutes="40" orientation="longitude" pageId="1" pageNumber="469" precision="15" seconds="46" value="-39.679447">39°40′46ʺ W</geoCoordinate>
), Pedreira Três Irmãos; 
<collectingMunicipality id="3626AC96FF85FFCEFC52F889886CF8AA" box="[958,1106,1811,1838]" pageId="1" pageNumber="469">upper Aptian</collectingMunicipality>
