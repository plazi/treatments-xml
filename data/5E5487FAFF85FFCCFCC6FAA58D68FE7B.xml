<collectingCountry id="AEEA767CFF85FFCEFA8CF9578999F963" box="[1376,1447,1738,1764]" name="Brazil" pageId="1" pageNumber="469">Brazil</collectingCountry>
, 
<collectingRegion id="1439F80EFF85FFCEFCC6F9728F50F88E" box="[810,878,1775,1801]" country="Brazil" name="Ceara" pageId="1" pageNumber="469">Ceará</collectingRegion>
