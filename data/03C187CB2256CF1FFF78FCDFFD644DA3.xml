(
<taxonomicName id="4C684D5E2256CF1FFE3DFBE7FD704DA3" authority="Brues" authorityName="Brues" authorityYear="1907" box="[466,716,1148,1173]" class="Insecta" family="Chalcididae" genus="Parasolindenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
