<taxonomicName id="4C684D5E2256CF1FFEF3FBE7FE7E4DA2" box="[284,450,1148,1173]" class="Insecta" family="Eupelmidae" genus="Arachnophaga" kingdom="Animalia" order="Hymenoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
