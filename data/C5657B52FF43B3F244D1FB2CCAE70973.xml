<typeStatus id="927774E6FF7DB3F24015FF51CC8C0B9C" box="[1107,1162,152,176]" pageId="225" pageNumber="226">Type</typeStatus>
locality &quot; 
<collectingRegion id="8F0804A6FF7DB3F240B0FF5ECD540B83" box="[1270,1362,151,175]" country="United States of America" name="Oregon" pageId="225" pageNumber="226">Oregon</collectingRegion>
.”
