<taxonomicName id="E7D71BF48D344A092E62FB99FB4FFBC7" box="[1103,1213,1084,1109]" class="Insecta" family="Braconidae" genus="Apanteles" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
