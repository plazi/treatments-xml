<taxonomicName id="E7D71BF48D344A092ED7FB99FA6BFBC6" box="[1274,1433,1084,1108]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
