<taxonomicName id="CBC11EB0FFDDE508FF3BF96E862DFEAC" authority="Kovalov 1995:140" authorityName="Kovalov" authorityPageNumber="140" authorityYear="1995" box="[151,602,1696,1719]" class="Hexapoda" family="Figitidae" genus="Trjapitziniola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="popovi">
<emphasis id="3EB5B921FFDDE508FF3BF96E8500FEAC" box="[151,375,1696,1719]" italics="true" pageId="20" pageNumber="21">Trjapitziniola popovi</emphasis>
