<taxonomicName id="FCF9826E855296F5CA20CF2A308E4C05" class="Actinopterygii" family="Paradoxosomatidae" genus="Microdesmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Microdesmus" order="Polydesmida" pageId="0" pageNumber="43" phylum="Chordata" rank="genus">Microdesmus</taxonomicName>
