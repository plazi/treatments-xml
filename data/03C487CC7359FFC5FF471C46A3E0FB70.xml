<taxonomicName id="4C6D4D597359FFC5FBEF1A03A72FFBE4" box="[1128,1293,1123,1150]" class="Magnoliopsida" family="Cucurbitaceae" kingdom="Plantae" order="Cucurbitales" pageId="31" pageNumber="30" phylum="Tracheophyta" rank="family">Cucurbitaceae</taxonomicName>
