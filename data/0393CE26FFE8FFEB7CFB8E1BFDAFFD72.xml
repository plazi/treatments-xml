<subSubSection id="C3202CBBFFE9FFEB7CA88A56FDAFFD72" pageId="23" pageNumber="24" type="etymology">
<paragraph id="8B857F30FFE9FFEB7CA88A56FDAFFD72" blockId="23.[151,1436,152,663]" pageId="23" pageNumber="24">
<emphasis id="B94EA322FFE9FFEB7CA88A56FEB5FD94" bold="true" box="[196,340,599,625]" pageId="23" pageNumber="24">Etymology:</emphasis>
<emphasis id="B94EA322FFE9FFEB7D308A59FE28FD94" box="[348,457,600,625]" italics="true" pageId="23" pageNumber="24">Daimong</emphasis>
