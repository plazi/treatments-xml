<taxonomicName id="AFBF4D0F669329C731C8AE3EFC661D3F" box="[684,1009,1303,1329]" class="Reptilia" family="Phyllodactylidae" genus="Gymnodactylus" kingdom="Animalia" order="Squamata" pageId="147" pageNumber="148" phylum="Chordata" rank="species" species="louisiadensis">
