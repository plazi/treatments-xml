<taxonomicName id="7CFEC6EDFFE9FF82FC73FEEBD86AFEF1" box="[1003,1129,301,326]" class="Malacostraca" family="Ocypodidae" genus="Uca" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="typhoni">
