</taxonomicName>
and 
<taxonomicName id="7CFEC6EDFFE9FF82FDABFECEDED0FE96" box="[563,723,264,289]" class="Malacostraca" family="Ocypodidae" genus="Uca" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="coarctata">
