<taxonomicName id="585239D503C0968687054796F4099447" LSID="urn:lsid:zoobank.org:act:B823B199-72D0-499E-A595-68097693F7E4" authority="Aristov &amp; Storozhenko" class="Insecta" family="Permulidae" genus="Mezenalicula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mezenalicula" order="Grylloblattodea" pageId="7" pageNumber="118" phylum="Arthropoda" rank="genus">Mezenalicula Aristov &amp; Storozhenko</taxonomicName>
