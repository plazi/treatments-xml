<location id="4C6D6070FFB6BF2F9B87FC84FE8475D6" LSID="urn:lsid:plazi:treatment:C11B87BDFFB6BF2F9815FD3DFE317343:4C6D6070FFB6BF2F9B87FC84FE8475D6" name="Way Canguk Forest" pageId="20" pageNumber="432" stateProvince="Lampung">Way Canguk Forest</location>
