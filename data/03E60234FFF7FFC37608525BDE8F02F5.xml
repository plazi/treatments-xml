<collectingMunicipality id="6B942958FFF7FFC3765755FBDF640307" box="[334,512,1528,1565]" pageId="4" pageNumber="11">Machanao</collectingMunicipality>
, 
<date id="FFF195E2FFF7FFC3753955FBDFCB0307" box="[544,687,1528,1565]" pageId="4" pageNumber="11" value="1930-05">
<collectingDate id="EFB56C0AFFF7FFC3753955FBDFCB0307" box="[544,687,1528,1565]" pageId="4" pageNumber="11" value="1930-05">May 30</collectingDate>
