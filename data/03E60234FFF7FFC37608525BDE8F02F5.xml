<typeStatus id="54F40D80FFF7FFC3736955FBD8660307" box="[1136,1282,1528,1565]" pageId="4" pageNumber="11">holotype</typeStatus>
<specimenCount id="9D4978ABFFF7FFC3720C55FBD80E0307" box="[1301,1386,1528,1565]" pageId="4" pageNumber="11" type="male">male</specimenCount>
