<taxonomicName id="07D64CE23A5F1FD44868307697D4C9A6" authorityName="Li &amp; Chang" authorityYear="2020" class="Arachnida" family="Psilodercidae" genus="Psiloderces" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psiloderces cuyapoensis" order="Araneae" pageId="0" pageNumber="61" phylum="Arthropoda" rank="species" species="cuyapoensis">
