<taxonomicName id="4C684D74AE51B63CDE9145DEFC2100D8" authorityName="Sharp" authorityYear="1875" box="[920,1018,655,680]" class="Insecta" family="Scarabaeidae" genus="Cassolus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="nudus">P. nudus</taxonomicName>
