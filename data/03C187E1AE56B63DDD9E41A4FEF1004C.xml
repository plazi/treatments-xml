<taxonomicName id="4C684D74AE51B63CD88E45FAFEF70080" authorityName="Tarasov &amp; Keith" authorityYear="2011" class="Insecta" family="Scarabaeidae" genus="Parachorius" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="semsanganus">
