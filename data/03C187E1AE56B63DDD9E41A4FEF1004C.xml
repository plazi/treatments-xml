<taxonomicName id="4C684D74AE51B63CD8634597FEDC0165" pageId="12" pageNumber="113" rank="subSpecies" subSpecies="humeralis">
In 
<emphasis id="B91CEAE5AE51B63CD88E4596FEDC0165" italics="true" pageId="12" pageNumber="113">P. humeralis</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C684D74AE51B63CDC3545B2FE2A0164" authorityName="S. Tarasov" authorityYear="2017" box="[316,497,765,788]" class="Insecta" family="Scarabaeidae" genus="Parachorius" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="asymmetricus">
<emphasis id="B91CEAE5AE51B63CDC3545B2FE2A0164" box="[316,497,765,788]" italics="true" pageId="12" pageNumber="113">P. asymmetricus</emphasis>
</taxonomicName>
