Known from 
<collectingCountry id="F315F3FAFF840000FE5AB8E0B44C6C71" box="[495,559,766,790]" name="China" pageId="5" pageNumber="300">China</collectingCountry>
(
<collectingRegion id="49C67D88FF840000FD88B8E0B4E26C71" box="[573,641,766,790]" country="China" name="Hubei" pageId="5" pageNumber="300">Hubei</collectingRegion>
