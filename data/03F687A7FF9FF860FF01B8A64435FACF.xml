<taxonomicName id="4C5F4D32FF9FF860FD62BD214369FAEF" authorityName="Blahnik &amp; Holzenthal" authorityYear="2017" box="[675,866,1347,1371]" class="Magnoliopsida" family="Apocynaceae" genus="Mortoniella" kingdom="Plantae" order="Gentianales" pageId="103" pageNumber="102" phylum="Tracheophyta" rank="species" species="triangularis">
