<taxonomicName id="FBDD4D4880318142FC64F8A9FAC31C0E" box="[1282,1385,1827,1851]" class="Adenophorea" family="Longidoridae" genus="Xiphinema" kingdom="Animalia" order="Dorylaimida" pageId="33" pageNumber="34" phylum="Nematoda" rank="species" species="bakeri">
