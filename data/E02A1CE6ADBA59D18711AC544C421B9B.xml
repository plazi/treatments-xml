<taxonomicName id="263F50651B83290BB5C284BBA7336831" authority="Linnaeus, 1758" authorityName="Foerster" authorityYear="1856" class="Insecta" family="Ulmaceae" genus="Coleoptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="77092" phylum="Arthropoda" rank="genus">Coleoptera</taxonomicName>
