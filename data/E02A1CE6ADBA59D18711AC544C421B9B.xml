<taxonomicName id="B7C642BA2191B783A6353882782F9B23" authorityName="Foerster" authorityYear="1856" class="Insecta" family="Fagaceae" genus="Fagaceae" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="77092" phylum="Arthropoda">Fagaceae</taxonomicName>
