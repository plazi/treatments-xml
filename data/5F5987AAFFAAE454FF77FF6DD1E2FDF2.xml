<taxonomicName id="10F04D3FFFAAE454FD83FDD1D083FDAF" authority="Reiche, 1869" authorityName="Reiche" authorityYear="1869" box="[611,1016,548,573]" class="Insecta" family="Hydraenidae" genus="Ochthebius" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="278" phylum="Arthropoda" rank="species" species="maculatus">
