<taxonomicName id="9F984FD8FFBFFF9DFBE049025FD2E00F" box="[1041,1172,1537,1558]" class="Insecta" family="Coccidae" genus="Tillancoccus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="493" phylum="Arthropoda" rank="species" species="mexicanus">T. mexicanus</taxonomicName>
