<taxonomicName id="9B844D64FFE16248D60BFA8AFC72FA57" authority="(Herrich-Schaffer, 1838) (Frauenfeld 1861)" authorityName="Herrich-Schaffer" authorityYear="1838" baseAuthorityName="Herrich-Schaffer" baseAuthorityYear="1838" box="[349,1017,1406,1431]" class="Insecta" family="Gryllidae" genus="Gryllus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="apterus">
