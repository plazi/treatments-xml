<collectingCountry id="F347761FFFC6FFA7FC7C5C4246618320" box="[967,1060,1880,1901]" name="Morocco" pageId="17" pageNumber="1193">Morocco</collectingCountry>
and 
<collectingCountry id="F347761FFFC6FFA7FBE75C4D46F18320" box="[1116,1204,1879,1901]" name="Tunisia" pageId="17" pageNumber="1193">Tunisia</collectingCountry>
