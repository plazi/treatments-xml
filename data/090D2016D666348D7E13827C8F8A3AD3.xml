<taxonomicName id="0944FB4346C9500A6AB5EA4C4F154C5F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181713" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole reclusi Forel" lsidName-HNS="Pheidole reclusi Forel" order="Hymenoptera" pageNumber="255" phylum="Arthropoda" rank="species" species="reclusi">reclusi</taxonomicName>
