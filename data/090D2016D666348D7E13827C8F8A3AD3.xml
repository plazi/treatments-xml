<taxonomicName id="8A1A5B19103C56F1201AEB3EA5C4108D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33715" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole distorta Forel" lsidName-HNS="Pheidole distorta Forel" order="Hymenoptera" pageNumber="255" phylum="Arthropoda" rank="species" species="distorta">distorta</taxonomicName>
