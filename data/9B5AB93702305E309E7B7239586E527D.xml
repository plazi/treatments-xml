<taxonomicName id="FA7CC51087AE1163E8606517F6643F39" authority="Baehr, 2017" authorityName="Baehr" authorityYear="2017" class="Insecta" family="Carabidae" genus="Magnanillus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Magnanillus" order="Coleoptera" pageId="0" pageNumber="269" phylum="Arthropoda" rank="genus">
