<taxonomicName id="6D5677E668B708E246D575B822EEAA33" authority="Baehr, 2017" authorityName="Baehr" authorityYear="2017" class="Insecta" family="Carabidae" genus="Magnanillus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Magnanillus" order="Coleoptera" pageId="0" pageNumber="269" phylum="Arthropoda" rank="genus">
