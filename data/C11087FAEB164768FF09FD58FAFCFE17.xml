, 
<number id="FF7124CBEB154769FBF3FEFFFB30FEF7" box="[1133,1187,272,298]" pageId="2" pageNumber="64" value="0.9">0.90</number>
, 
<number id="FF7124CBEB154769FB2FFEFFFB74FEF7" box="[1201,1255,272,298]" pageId="2" pageNumber="64" value="0.6">0.60</number>
, 
<number id="FF7124CBEB154769FB6BFEFFFABEFEF7" box="[1269,1325,272,298]" pageId="2" pageNumber="64" value="0.4">0.40</number>
), III 
<number id="FF7124CBEB154769FAF7FEFFFA0FFEF7" box="[1385,1436,272,298]" pageId="2" pageNumber="64" value="1.9">1.90</number>
(
