<typeStatus id="9602884EEB16476AFE5CF835FDD3F829" box="[450,576,2010,2036]" pageId="1" pageNumber="63">paratypes</typeStatus>
, HNU-Tang 
<date id="3D07102CEB16476AFD41F835FCABF829" box="[735,824,2010,2036]" pageId="1" pageNumber="63" value="01-04-04">040401</date>
