<number id="FF7124CBEB154769FD2DFED7FD7AFE8F" box="[691,745,312,338]" pageId="2" pageNumber="64" value="0.4">0.40</number>
, 
<number id="FF7124CBEB154769FD69FED7FCBCFE8F" box="[759,815,312,338]" pageId="2" pageNumber="64" value="0.4">0.40</number>
