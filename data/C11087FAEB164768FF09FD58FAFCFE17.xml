, 
<number id="FF7124CBEB154769FEB7FED7FECCFE8F" box="[297,351,312,338]" pageId="2" pageNumber="64" value="0.3">0.30</number>
, 
<number id="FF7124CBEB154769FEF3FED7FE36FE8F" box="[365,421,312,338]" pageId="2" pageNumber="64" value="0.3">0.30</number>
), IV 
<number id="FF7124CBEB154769FE7AFED7FD84FE8F" box="[484,535,312,338]" pageId="2" pageNumber="64" value="2.3">2.30</number>
