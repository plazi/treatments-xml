<taxonomicName id="27CA4D35FEBEFED85A9CFA62FC9A6770" authorityName="Rondani" authorityYear="1870" box="[642,809,1519,1545]" class="Insecta" family="Tephritidae" genus="Oxyna" kingdom="Animalia" order="Diptera" pageId="371" pageNumber="372" phylum="Arthropoda" rank="species" species="guttella">
