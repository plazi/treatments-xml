(
<bibRefCitation id="EF9E24ABFF9EFFD4EED845CEB70EFE43" author="Zerny" box="[705,845,387,411]" pageId="4" pageNumber="509" refString="Zerny, H. (1928) Neue tropische Heteroceren aus dem Naturhistorischen Museum in Wien. Zeitschrift des Osterreichischen Entomologen-Vereines, 13 (9), 81 - 84." type="journal article" year="1928">
