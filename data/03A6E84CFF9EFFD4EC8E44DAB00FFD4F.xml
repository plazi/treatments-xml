<taxonomicName id="4C0F22D9FF9EFFD4E95A456CB5D3FE8E" authority="Zerny" authorityName="Zerny" class="Insecta" family="Arctiidae" genus="Eucyane" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="509" phylum="Arthropoda" rank="species" species="phanoptoides">Eucyane phanoptoides Zerny</taxonomicName>
