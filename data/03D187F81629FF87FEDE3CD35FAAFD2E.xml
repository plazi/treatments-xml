<quantity id="4C809B0B1629FF87FCA13F6B5D14FDD4" box="[836,890,518,537]" metricMagnitude="4" metricUnit="m" metricValue="1.0" pageId="13" pageNumber="272" unit="km" value="10.0">10 km</quantity>
