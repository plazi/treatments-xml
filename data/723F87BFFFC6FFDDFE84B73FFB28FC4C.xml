<quantity id="3D6E9B4CFFC5FFDDFB12B355FB2AFDE9" box="[1158,1276,543,567]" metricMagnitude="2" metricUnit="m" metricValue="6.29" metricValueMax="9.67" metricValueMin="2.91" pageId="50" pageNumber="265" unit="m" value="629.0" valueMax="967.0" valueMin="291.0">
<elevation id="71BBD19AFFC5FFDDFB12B355FB2AFDE9" box="[1158,1276,543,567]" metricMagnitude="2" metricUnit="m" metricValue="6.29" metricValueMax="9.67" metricValueMin="2.91" pageId="50" pageNumber="265" unit="m" value="629.0" valueMax="967.0" valueMin="291.0">291-967 m</elevation>
