<taxonomicName id="F179D365FAD0AFE864C7C8A7629027A9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34187" class="Insecta" family="Formicidae" genus="Plagiolepis" kingdom="Animalia" lsidName-HNS="Plagiolepis mediorufa Forel" order="Hymenoptera" pageNumber="212" phylum="Arthropoda" rank="species" species="mediorufa">Plagiolepis mediorufa</taxonomicName>
