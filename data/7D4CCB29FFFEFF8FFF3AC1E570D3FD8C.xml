<date id="815B5CFFFFFEFF8EFF73C1C271A7FA06" box="[222,237,1469,1493]" pageId="10" pageNumber="11">
<number id="432D6818FFFEFF8EFF73C1C271A7FA06" box="[222,237,1469,1493]" pageId="10" pageNumber="11" value="2.0">2</number>
</date>
</figureCitation>
I, 
<number id="432D6818FFFEFF8EFEA9C1C2705BFA06" box="[260,273,1469,1493]" pageId="10" pageNumber="11" value="3.0">3</number>
