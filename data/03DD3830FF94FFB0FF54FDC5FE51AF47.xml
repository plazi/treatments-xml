<taxonomicName id="4C74F2A5FF94FFB0FE64FC02FDA1A9FB" box="[423,569,896,920]" class="Insecta" family="Halictophagidae" genus="Coriophagus" kingdom="Animalia" order="Strepsiptera" pageId="4" pageNumber="83" phylum="Arthropoda" rank="genus">
<emphasis id="B9005534FF94FFB0FE64FC02FDA1A9FB" box="[423,569,896,920]" italics="true" pageId="4" pageNumber="83">Coriophagus</emphasis>
</taxonomicName>
