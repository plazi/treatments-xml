<taxonomicName id="4C74F2A5FF94FFB0FF23FC27FD28A9DF" authority="Kathirithamby, 1993: 188" authorityName="Kathirithamby" authorityPageNumber="188" authorityYear="1993" box="[224,688,932,956]" class="Insecta" family="Halictophagidae" genus="Coriophagus" kingdom="Animalia" order="Strepsiptera" pageId="4" pageNumber="83" phylum="Arthropoda" rank="species" species="adebratti">
