: 
<collectingMunicipality id="6B80AC9B7104FFF74555F9F38255F312" box="[883,1130,1618,1644]" pageId="4" pageNumber="25">San José del Palmar</collectingMunicipality>
, 
<collectingCounty id="62854E6D7104FFF7425DF9F38308F312" box="[1147,1335,1618,1644]" pageId="4" pageNumber="25">Cerro del Torrá</collectingCounty>
, vertiente nororiental, 
<quantity id="4CA39B047104FFF74737F9D78757F3EE" box="[273,360,1654,1681]" metricMagnitude="3" metricUnit="m" metricValue="2.05" pageId="4" pageNumber="25" unit="m" value="2050.0">
