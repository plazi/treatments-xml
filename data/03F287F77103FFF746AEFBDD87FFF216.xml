(
<collectionCode id="ED4AAE247104FFF7454EF9D78580F3EE" box="[872,959,1654,1680]" country="Colombia" lsid="urn:lsid:biocol.org:col:15729" name="Universidad del Valle" pageId="4" pageNumber="25" type="Herbarium">CUVC</collectionCode>
