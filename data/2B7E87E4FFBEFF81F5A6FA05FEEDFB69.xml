<taxonomicName id="64D74D71FFBBFF81F453FBE9FE71FBC4" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[354,424,1044,1069]" class="Insecta" family="Calamoceratidae" genus="Anisocentropus" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="fridae">
