<taxonomicName id="4C5F4D4DFFF9FFD21E2BFA14ADECF5D1" authority="BEIER, 1950" authorityName="in Beier" authorityYear="1950" class="Insecta" family="Phaneropteridae" genus="Panoploscelis" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="330" phylum="Arthropoda" rank="species" species="angusticauda">
