<number id="3F3E99726E0A3F79FC545C4DFC52540C" box="[994,1009,1857,1881]" pageId="4" pageNumber="205" value="4.0">4</number>
</figureCitation>
) as the 
<typeStatus id="564D35F76E0A3F79FBFD5C4DFB12540C" box="[1099,1201,1857,1881]" pageId="4" pageNumber="205">holotype</typeStatus>
. Mocsáry (
<number id="3F3E99726E0A3F79FA985C4DFACB540C" box="[1326,1384,1857,1881]" pageId="4" pageNumber="205" value="1909.0">1909</number>
