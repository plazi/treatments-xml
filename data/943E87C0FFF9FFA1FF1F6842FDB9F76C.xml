<taxonomicName id="DB974D55FFF9FFA1FE85688DFEB1F5BC" authority="Dumitrica, 1991" authorityName="Dumitrica" authorityYear="1991" class="Polycystina" family="Tripedurnulidae" genus="Tripedocorbis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="31" pageNumber="36" phylum="Radiozoa" rank="species" species="ramulispinus">
