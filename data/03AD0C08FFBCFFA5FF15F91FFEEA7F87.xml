<number id="3DCCAF39FFBFFFA5FC53FD31FBF77E98" box="[977,1045,654,680]" pageId="2" pageNumber="60" value="0.053">0.053</number>
long and 
<number id="3DCCAF39FFBFFFA5FB0DFD31FB317E98" box="[1167,1235,654,680]" pageId="2" pageNumber="60" value="0.025">0.025</number>
wide. Aedeagus 
<number id="3DCCAF39FFBFFFA5FF15FD0AFF3B7EFF" box="[151,217,693,719]" pageId="2" pageNumber="60" value="0.14">0.140</number>
