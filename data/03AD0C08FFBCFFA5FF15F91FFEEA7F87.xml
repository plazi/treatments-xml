<number id="3DCCAF39FFBFFFA5FDCEFD31FD727E98" box="[588,656,654,680]" pageId="2" pageNumber="60" value="0.088">0.088</number>
long, 
<number id="3DCCAF39FFBFFFA5FD5CFD31FCC07E98" box="[734,802,654,680]" pageId="2" pageNumber="60" value="0.053">0.053</number>
