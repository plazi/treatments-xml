<taxonomicName id="4C794D17FFDFC60276DE5EA54262F9D2" box="[470,632,958,978]" class="Insecta" family="Miridae" genus="Neaitkenia" kingdom="Animalia" order="Hemiptera" pageId="115" pageNumber="115" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="B90DEA86FFDFC60276DE5EA5425FF9D2" box="[470,581,958,978]" italics="true" pageId="115" pageNumber="115">Neaitkenia</emphasis>
