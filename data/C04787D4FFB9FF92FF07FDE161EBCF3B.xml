<taxonomicName id="8FEE4D41FFB9FF92FCFEFB996125CF1F" authorityName="Karsch" authorityYear="1879" box="[878,1126,1086,1112]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="arcanum">
