<emphasis id="7A9AEAD0FFB9FF92FCC9FBC561EBCF3B" box="[857,1192,1122,1148]" italics="true" pageId="80" pageNumber="81">
<taxonomicName id="8FEE4D41FFB9FF92FCC9FBC561E7CF3B" baseAuthorityName="Lucas" baseAuthorityYear="1846" box="[857,1188,1122,1148]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="flavomaculatum">Amblyomma flavomaculatum</taxonomicName>
