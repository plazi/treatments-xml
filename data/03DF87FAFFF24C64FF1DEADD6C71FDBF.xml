<taxonomicName id="4C764D6FFFF24C64FF5AEAB56EBEFEBB" authority="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord, 2022" authorityName="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord" authorityYear="2022" box="[208,398,255,279]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="ambigua" status="n. sp.">
