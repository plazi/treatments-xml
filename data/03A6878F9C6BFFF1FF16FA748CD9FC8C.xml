<taxonomicName id="4C0F4D1A9C68FFF0FE30F9458C95F952" authority="Fabricius" authorityName="Fabricius" box="[433,720,1716,1738]" class="Insecta" family="Notonectidae" genus="Buenoa" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="115" phylum="Arthropoda" rank="species" species="pallipes">
