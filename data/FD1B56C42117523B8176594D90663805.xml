<taxonomicName id="4AB10EEA5AC2BCA1ADEEAFFE28F817FF" class="Insecta" genus="P." kingdom="Animalia" lsidName="P. apollo" order="Coleoptera" pageId="0" pageNumber="193" phylum="Arthropoda" rank="species" species="apollo">
<emphasis id="27EF34233A9578495394260249DA43BC" italics="true" pageId="0" pageNumber="193">P. apollo</emphasis>
</taxonomicName>
