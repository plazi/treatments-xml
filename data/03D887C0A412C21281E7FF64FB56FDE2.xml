<collectionCode id="ED60AE13A412C2178536FDEDFB52FDCC" LSID="urn:lsid:biocol.org:col:34982" box="[1094,1175,542,566]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34982" name="South Australia Museum" pageId="1">SAMA</collectionCode>
C 
<number id="3DB924F1A412C21785C0FDEDFB29FDCC" box="[1200,1260,542,566]" pageId="1" value="8392.0">8392</number>
