<taxonomicName id="E30C9BFDCBCF85D0D67BDE61E5F446F8" class="Dicotyledoneae" family="Fabaceae" genus="Desmodium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Rosales" pageId="27" pageNumber="122" phylum="Angiospermae" rank="species" species="paniculatum">
