<taxonomicName id="4196E4B09E19EDFE005852328012003C" authorityName="D. Don" authorityYear="1834" baseAuthorityName="L." class="Globothalamea" family="Cassidulinidae" genus="Cassandra" higherTaxonomySource="GBIF,CoL" kingdom="Chromista" lsidName="" order="Rotaliida" pageId="27" pageNumber="122" phylum="Foraminifera" rank="species" species="calyculata">
