<taxonomicName id="A694403F10F9A369B7EEE3796DECBA77" class="Pinopsida" family="Cupressaceae" genus="Rosaceae" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Pinales" pageId="27" pageNumber="122" phylum="Tracheophyta" rank="genus">Rosaceae</taxonomicName>
), and tick-trefoil (
<taxonomicName id="5B9FB1D16A8C9E592603A4E7951A683A" class="Dicotyledoneae" family="Fabaceae" genus="Desmodium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Rosales" pageId="27" pageNumber="122" phylum="Angiospermae" rank="genus">
<emphasis id="276B299385B2C2D927F3E17CFE826482" italics="true" pageId="27" pageNumber="122">Desmodium</emphasis>
</taxonomicName>
