<taxonomicName id="4C7071ED9469FFF5FD8F2838BA42FB7A" authority=", Lendenfeld" authorityName="Lendenfeld" box="[527,1123,1142,1169]" class="Demospongiae" family="Axinellidae" genus="Chalinissa" kingdom="Animalia" order="Axinellida" pageId="60" pageNumber="481" phylum="Porifera" rank="species" species="oblata">
