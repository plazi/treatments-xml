by 
<bibRefCitation id="F6424B52FFFDFFABFE53F82BCB9146C1" author="Stark, B. P. &amp; R. W. Baumann" box="[454,747,1954,1978]" pageId="2" pageNumber="19" pagination="96 - 108" refId="ref1605" refString="Stark, B. P. and R. W. Baumann. 2004. The winter stonefly genus Paracapnia (Plecoptera: Capniidae). Monographs of the Western North American Naturalist, 2: 96 - 108." type="journal article" year="2004">Stark and Baumann (2004)</bibRefCitation>
as occurring only in 
<collectingCounty id="7B0D4E2FFFFDFFABFC63FE8DCC6E4067" box="[1014,1300,260,284]" pageId="2" pageNumber="19">
<collectingMunicipality id="7208ACD9FFFDFFABFC63FE8DCD3D4060" box="[1014,1095,260,283]" pageId="2" pageNumber="19">Benton</collectingMunicipality>
and 
<location id="970C6078FFFDFFABFBEBFE8DCC6E4067" LSID="urn:lsid:plazi:treatment:1A7A87B5FFFEFFABFEADF8B0CC064259:970C6078FFFDFFABFBEBFE8DCC6E4067" box="[1150,1300,260,284]" county="Benton and Lane County" latitude="12.23" municipality="Benton" name="Lane County" pageId="2" pageNumber="19" stateProvince="Oregon">Lane County</location>
</collectingCounty>
, 
<collectingRegion id="5017F841FFFDFFABFA8BFE8DCC024067" box="[1310,1400,260,284]" country="United States of America" name="Oregon" pageId="2" pageNumber="19">Oregon</collectingRegion>
. 
<location id="970C6078FFFDFFABFCB9FEAECAE34045" LSID="urn:lsid:plazi:treatment:1A7A87B5FFFEFFABFEADF8B0CC064259:970C6078FFFDFFABFCB9FEAECAE34045" box="[812,921,295,318]" county="Benton and Lane County" latitude="12.23" municipality="Benton" name="However" pageId="2" pageNumber="19" stateProvince="Oregon">However</location>
