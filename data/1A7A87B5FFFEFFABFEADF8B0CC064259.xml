<location id="970C6078FFFDFFABFCB9FEC0CD7B401A" LSID="urn:lsid:plazi:treatment:1A7A87B5FFFEFFABFEADF8B0CC064259:970C6078FFFDFFABFCB9FEC0CD7B401A" box="[812,1025,329,353]" county="Benton and Lane County" latitude="12.23" municipality="Benton" name="Humboldt County" pageId="2" pageNumber="19" stateProvince="Oregon">Humboldt County</location>
