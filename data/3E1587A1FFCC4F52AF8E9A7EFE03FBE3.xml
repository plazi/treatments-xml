<collectorName id="1B495361FFCC4F53AC9B995DFCB0FDDB" box="[722,837,536,555]" pageId="10" pageNumber="69">L. Cervantes’</collectorName>
(
<collectionCode id="D0ADAE72FFCC4F53AD19995DFC63FDDB" box="[848,918,536,555]" country="Mexico" httpUri="http://grbio.org/cool/10ei-x9nj" name="Universidad Nacional Autonoma de Mexico" pageId="10" pageNumber="69">UNAM</collectionCode>
