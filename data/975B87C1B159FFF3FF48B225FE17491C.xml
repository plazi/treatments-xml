<collectionCode id="79E3AE12B156FFF3FC34B658FBC24884" LSID="urn:lsid:biocol.org:col:34795" box="[1003,1090,632,658]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="8" pageNumber="47">FMNH</collectionCode>
