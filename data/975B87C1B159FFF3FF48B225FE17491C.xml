<collectionCode id="79E3AE12B156FFF3FEF0B6D0FE0B491C" LSID="urn:lsid:biocol.org:col:34871" box="[303,395,752,778]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="8" pageNumber="47">USNM</collectionCode>
