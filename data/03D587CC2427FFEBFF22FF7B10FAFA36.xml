<number id="3DB424FD2427FFEBFEDAFEDE10A1FEAE" box="[367,396,306,332]" pageId="9" pageNumber="51" value="30.0">30</number>
(
<number id="3DB424FD2427FFEBFE2BFEDE10C1FEAE" box="[414,492,306,332]" pageId="9" pageNumber="51" value="29.0" valueMax="30.0" valueMin="28.0">28–30</number>
), projecting obliquely down, pedipalp coxal setae (
<emphasis id="B908EAC82427FFEBFBE5FEDE1546FEA9" box="[1104,1131,306,331]" italics="true" pageId="9" pageNumber="51">ep</emphasis>
) 
<number id="3DB424FD2427FFEBFBCBFEDE15A0FEAE" box="[1150,1165,306,332]" pageId="9" pageNumber="51" value="3.0">3</number>
(
<number id="3DB424FD2427FFEBFB2AFEDE15E2FEAE" box="[1183,1231,306,332]" pageId="9" pageNumber="51" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
), dorsal pedipalp genual setae (
<emphasis id="B908EAC82427FFEBFE8EFEB61067FE91" box="[315,330,346,371]" italics="true" pageId="9" pageNumber="51">d</emphasis>
) 
<number id="3DB424FD2427FFEBFEE9FEB61047FE96" box="[348,362,346,372]" pageId="9" pageNumber="51" value="8.0">8</number>
(
<number id="3DB424FD2427FFEBFEC9FEB61081FE96" box="[380,428,346,372]" pageId="9" pageNumber="51" value="7.0" valueMax="8.0" valueMin="6.0">6–8</number>
), chelicerae 
<number id="3DB424FD2427FFEBFDF7FEB61372FE96" box="[578,607,346,372]" pageId="9" pageNumber="51" value="28.0">28</number>
(
<number id="3DB424FD2427FFEBFDC4FEB61392FE96" box="[625,703,346,372]" pageId="9" pageNumber="51" value="28.0" valueMax="29.0" valueMin="27.0">27–29</number>
