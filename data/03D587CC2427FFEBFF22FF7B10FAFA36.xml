<number id="3DB424FD2427FFEBFE44FEE61322FEC6" box="[497,527,266,292]" pageId="9" pageNumber="51" value="12.0">12</number>
) Body fusiform, 
<number id="3DB424FD2427FFEBFD6DFEE61229FEC6" box="[728,772,266,292]" pageId="9" pageNumber="51" value="231.0">231</number>
(
<number id="3DB424FD2427FFEBFCA6FEE61252FEC6" box="[787,895,266,292]" pageId="9" pageNumber="51" value="228.0" valueMax="231.0" valueMin="225.0">225–231</number>
), 
<number id="3DB424FD2427FFEBFC26FEE6129DFEC6" box="[915,944,266,292]" pageId="9" pageNumber="51" value="72.0">72</number>
(
<number id="3DB424FD2427FFEBFC0AFEE61526FEC6" box="[959,1035,266,292]" pageId="9" pageNumber="51" value="73.5" valueMax="75.0" valueMin="72.0">72–75</number>
) wide, 
<number id="3DB424FD2427FFEBFBD4FEE61553FEC6" box="[1121,1150,266,292]" pageId="9" pageNumber="51" value="60.0">60</number>
(
<number id="3DB424FD2427FFEBFB3BFEE615F4FEC6" box="[1166,1241,266,292]" pageId="9" pageNumber="51" value="61.5" valueMax="63.0" valueMin="60.0">60–63</number>
