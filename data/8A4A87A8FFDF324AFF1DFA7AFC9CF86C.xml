<taxonomicName id="C5E34D3DFFD8324BFE3BF831FDB8F849" box="[433,568,1961,1983]" class="Insecta" family="Lathiceridae" genus="Gryllotalpa" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="cycloptera">
