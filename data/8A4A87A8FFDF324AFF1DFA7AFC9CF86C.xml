<number id="B42B2499FFD8324BFBBAF810FBBEF856" box="[1072,1086,1931,1952]" pageId="4" pageNumber="45" value="1.0">1</number>
. 
<taxonomicName id="C5E34D3DFFD8324BFBC3F810FB51F856" box="[1097,1233,1931,1952]" class="Insecta" family="Lathiceridae" genus="Gryllotalpa" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="jinxiuensis">
<emphasis id="3097EAACFFD8324BFBC3F810FB51F856" box="[1097,1233,1931,1952]" italics="true" pageId="4" pageNumber="45">G. jinxiuensis</emphasis>
</taxonomicName>
; 
<number id="B42B2499FFD8324BFB55F810FB6EF856" box="[1247,1262,1931,1952]" pageId="4" pageNumber="45" value="2.0">2</number>
. 
<taxonomicName id="C5E34D3DFFD8324BFB73F810FADFF856" box="[1273,1375,1930,1952]" class="Insecta" family="Lathiceridae" genus="Gryllotalpa" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="henana">
<emphasis id="3097EAACFFD8324BFB73F810FADFF856" box="[1273,1375,1930,1952]" italics="true" pageId="4" pageNumber="45">G. henana</emphasis>
</taxonomicName>
; 
<number id="B42B2499FFD8324BFAE6F810FAFBF856" box="[1388,1403,1931,1952]" pageId="4" pageNumber="45" value="3.0">3</number>
. 
<taxonomicName id="C5E34D3DFFD8324BFA0CF810FEB4F849" class="Insecta" family="Lathiceridae" genus="Gryllotalpa" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="breviabdominis">
<emphasis id="3097EAACFFD8324BFA0CF810FEB4F849" italics="true" pageId="4" pageNumber="45">G. breviabdominis</emphasis>
</taxonomicName>
<emphasis id="3097EAACFFD8324BFEB0F831FE0BF849" bold="true" box="[314,395,1962,1983]" pageId="4" pageNumber="45">nov. sp.</emphasis>
; 
<number id="B42B2499FFD8324BFE12F831FE27F849" box="[408,423,1962,1983]" pageId="4" pageNumber="45" value="4.0">4</number>
