<quantity id="F361E0F3FFEBFFFBFC76BD53FBDC7FC4" box="[1006,1091,1151,1175]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="8" pageNumber="9" unit="m" value="1000.0">1000 m</quantity>
