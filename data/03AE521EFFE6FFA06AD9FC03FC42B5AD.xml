<taxonomicName id="4C07988BFFE5FFA66B6EF878FE51B0C1" authority="Taylor, 2009" authorityName="Taylor" authorityYear="2009" box="[288,456,1965,1988]" class="Malacostraca" family="Phoxocephalidae" genus="Birubius" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="802" phylum="Arthropoda" rank="species" species="banksi" status="sp. nov.">
<emphasis id="B9733F1AFFE5FFA66B6EF878FE51B0C1" box="[288,456,1965,1988]" italics="true" pageId="2" pageNumber="802">Birubius banksi</emphasis>
</taxonomicName>
<emphasis id="B9733F1AFFE5FFA66B81F879FDBCB0C1" bold="true" box="[463,549,1964,1988]" pageId="2" pageNumber="802">
