<collectingCountry id="36884E9BD510B26DFD3AFF2E891011AA" box="[718,875,152,178]" name="New Zealand" pageId="13" pageNumber="35">New Zealand</collectingCountry>
with 
<taxonomicName id="899F7588D510B26DFC44FF2E8EEA11AA" authority="Hammer" authorityName="Hammer" box="[944,1169,152,178]" class="Arachnida" family="Oppiidae" genus="Paroppia" kingdom="Animalia" order="Sarcoptiformes" pageId="13" pageNumber="35" phylum="Arthropoda" rank="species" species="lebruni">
