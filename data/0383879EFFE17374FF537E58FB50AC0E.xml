<subSubSection id="C3306503FFE37373FF037F03FE91AA88" pageId="3" pageNumber="56" type="etymology">
<paragraph id="8B953688FFE37373FF037F03FE91AA88" blockId="3.[151,1436,1058,2004]" pageId="3" pageNumber="56">
<emphasis id="B95EEA9AFFE37373FF037F03FEC4AAA7" bold="true" box="[199,340,1897,1923]" pageId="3" pageNumber="56">Etymology.</emphasis>
