<emphasis id="F2EA4553FFA7FFB6DAD5D1557B52D943" bold="true" box="[151,580,850,876]" pageId="9" pageNumber="50">
<taxonomicName id="079EE2C2FFA7FFB6DAD5D15578C9D943" authority="Brooks" authorityName="Brooks" box="[151,479,850,876]" class="Insecta" family="Dolichopodidae" genus="Cheiromyia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="50" phylum="Arthropoda" rank="species" species="laselva" status="sp. nov.">
<emphasis id="F2EA4553FFA7FFB6DAD5D1557869D943" bold="true" box="[151,383,850,876]" italics="true" pageId="9" pageNumber="50">Cheiromyia laselva</emphasis>
Brooks
</taxonomicName>
<taxonomicNameLabel id="E9D9F828FFA7FFB6DBA4D1557B52D943" box="[486,580,850,876]" pageId="9" pageNumber="50" rank="species">sp. nov.</taxonomicNameLabel>
