<emphasis id="B947EA9B50434D12FD35F8D557FDFB15" box="[657,761,1824,1849]" italics="true" pageId="5" pageNumber="60">Solidago</emphasis>
</taxonomicName>
sp., 
<taxonomicName id="4C334D0A50434D12FC94F8D55696FB15" box="[816,914,1824,1849]" class="Magnoliopsida" family="Asteraceae" genus="Achillea" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="60" phylum="Tracheophyta" rank="genus">
