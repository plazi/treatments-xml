<taxonomicName id="CD854D14215F717DFD9EC03ECAE7000C" box="[634,724,1080,1105]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="seducta">
