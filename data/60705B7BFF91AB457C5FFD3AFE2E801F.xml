<paragraph id="E866EA6DFF90AB467CC6F8ECFB088548" blockId="13.[401,1242,1794,1817]" box="[401,1242,1794,1817]" pageId="13" pageNumber="14">
<taxonomicName id="2FD991EEFF90AB467CC6F8ECFDE08548" box="[401,562,1794,1817]" class="Malacostraca" family="Amathillopsidae" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="14" phylum="Arthropoda" rank="subFamily" subFamily="Parepimeriinae">Parepimeriinae</taxonomicName>
(
<taxonomicName id="2FD991EEFF90AB467F8EF8ECFC718548" box="[729,931,1794,1817]" class="Malacostraca" family="Amathillopsidae" genus="Parepimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="perda">
<emphasis id="DAAD367FFF90AB467F8EF8ECFC718548" box="[729,931,1794,1817]" italics="true" pageId="13" pageNumber="14">Parepimeria perda</emphasis>
</taxonomicName>
