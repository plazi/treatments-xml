<taxonomicName id="FAA54D71FFA5FFEA447DFF05FE38FF42" box="[347,434,189,212]" class="Diplopoda" family="Pseudonannolenidae" genus="Pseudonannolene" kingdom="Animalia" order="Spirostreptida" pageId="18" pageNumber="379" phylum="Arthropoda" rank="species" species="gogo">
<emphasis id="0FD1EAE0FFA5FFEA447DFF05FE38FF42" box="[347,434,189,212]" italics="true" pageId="18" pageNumber="379">P. gogo</emphasis>
