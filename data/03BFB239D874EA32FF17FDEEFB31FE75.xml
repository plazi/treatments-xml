<taxonomicName id="4C1678ACD874EA36FF17FDEEFEE6FD8F" authority="Gagne" authorityName="Gagne" box="[151,357,573,599]" class="Insecta" family="Cecidomyiidae" genus="Pellacara" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="520" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B962DF3DD874EA36FF17FDEEFE88FD8F" bold="true" box="[151,267,573,599]" italics="true" pageId="9" pageNumber="520">Pellacara</emphasis>
Gagné
</taxonomicName>
