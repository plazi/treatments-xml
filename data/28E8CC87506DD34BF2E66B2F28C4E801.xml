<subSubSection id="5804527184F226EAD463F6F468520531" pageId="77" pageNumber="78" type="reference_group">
<paragraph id="433E785003020CE9601EC4789D9D02C7" pageId="77" pageNumber="78">
<taxonomicName id="36A971EF3C4AE1D1172933F209D69C9A" authority="Foerster, 1869" authorityName="Foerster" authorityYear="1869" class="Insecta" family="Figitidae" genus="Gronotoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gronotoma" order="Hymenoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="genus">Gronotoma</taxonomicName>
(synonym 
<taxonomicName id="E2DED9E8FB9C75CD1C1C74A3751A2377" authority="Ashmead, 1887" authorityName="Ashmead" authorityYear="1887" class="Insecta" family="Figitidae" genus="Eucoilidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eucoilidea" order="Hymenoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="genus">Eucoilidea</taxonomicName>
