<heading id="D0CBB611FF809011AFBEFD235856FD16" bold="true" box="[151,784,722,748]" fontSize="11" level="1" pageId="11" pageNumber="100" reason="1">
<emphasis id="B948DD6FFF809011AFBEFD235856FD16" bold="true" box="[151,784,722,748]" pageId="11" pageNumber="100">
3. 
<taxonomicName id="4C3C7AFEFF809011AF9DFD235AB1FD16" baseAuthorityName="Petrovitz" baseAuthorityYear="1981" box="[180,503,722,748]" class="Insecta" family="Scarabaeidae" genus="Protaetia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="100" phylum="Arthropoda" rank="species" species="jelineki" subGenus="Potosia">
<emphasis id="B948DD6FFF809011AF9DFD235A64FD16" bold="true" box="[180,290,722,748]" italics="true" pageId="11" pageNumber="100">Protaetia</emphasis>
(
<emphasis id="B948DD6FFF809011AE1DFD235ACBFD16" bold="true" box="[308,397,722,748]" italics="true" pageId="11" pageNumber="100">Potosia</emphasis>
) 
<emphasis id="B948DD6FFF809011AEB7FD235AB1FD16" bold="true" box="[414,503,722,748]" italics="true" pageId="11" pageNumber="100">jelineki</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3C7AFEFF809011AD1CFD235841FD16" box="[565,775,722,748]" class="Insecta" family="Scarabaeidae" genus="Protaetia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="100" phylum="Arthropoda" rank="species" species="spendidula" subGenus="Potosia">
