<taxonomicName id="4C444D78B41AFFDD1DF758ACFBFF78D6" box="[953,1100,186,211]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="spinevalva" status="sp. nov.">
