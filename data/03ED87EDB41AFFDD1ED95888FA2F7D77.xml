<taxonomicName id="4C444D78B41AFFDD1F965887FDED78B4" authorityName="Rondani" authorityYear="1876" box="[472,606,151,177]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
