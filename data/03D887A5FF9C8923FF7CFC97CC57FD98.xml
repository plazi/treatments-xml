<taxonomicName id="4C714D30FF9C8924FCD3FE75CAAEFE0E" authority="Cruz, 1991" authorityName="Cruz" authorityYear="1991" box="[783,1223,428,451]" class="Amphibia" family="Phyllomedusidae" genus="Phrynomedusa" kingdom="Animalia" order="Anura" pageId="115" pageNumber="116" phylum="Chordata" rank="species" species="vanzolinii">
