<taxonomicName id="4C714D30FF9C8924FBB0FEA8CD6BFE0E" authority="(Izecksohn and Cruz, 1976)" baseAuthorityName="Izecksohn and Cruz" baseAuthorityYear="1976" class="Amphibia" family="Phyllomedusidae" genus="Phrynomedusa" kingdom="Animalia" order="Anura" pageId="115" pageNumber="116" phylum="Chordata" rank="species" species="marginata">
