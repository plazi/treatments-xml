<specimenCount id="088DFD0BFFEDFFD379822421FF4FF85A" box="[151,222,1063,1085]" pageId="26" pageNumber="1171" type="generic">
<quantity id="D9739B67FFEDFFD379822421FF4BF85A" box="[151,218,1063,1085]" metricMagnitude="2" metricUnit="m" metricValue="2.15" pageId="26" pageNumber="1171" unit="m" value="215.0">215 m</quantity>
