<paragraph id="8BE3F107FF8E237CFF76FBEEFE3AFBEB" blockId="1.[151,503,1000,1060]" box="[151,397,1034,1060]" pageId="1" pageNumber="61">
(
<figureCitation id="1367ED82FF8E237CFF7EFBEEFF44FBEB" box="[159,243,1034,1060]" captionStart="FIGURES 1 – 3" captionStartId="2.[151,271,1835,1859]" captionTargetBox="[363,1211,262,1805]" captionTargetId="figure@2.[350,1227,246,1821]" captionTargetPageId="2" captionText="FIGURES 1 – 3. Wings. 1, Spartimas ornatipes Enderlein, male. 2, Spartimas apiciniger sp. nov., male. 3, Spartimas hainanensis sp. nov., male." httpUri="https://zenodo.org/record/196684/files/figure.png" pageId="1" pageNumber="61">Figs. 2</figureCitation>
, 
<figureCitation id="1367ED82FF8E237CFF1EFBEEFEB8FBEB" box="[255,271,1034,1060]" captionStart="FIGURES 4 – 6" captionStartId="2.[151,271,1899,1923]" captionText="FIGURES 4 – 6. Heads and thoraces, lateral view. 4, Spartimas ornatipes Enderlein, male. 5, Spartimas apiciniger sp. nov., male. 6, Spartimas hainanensis sp. nov., male." pageId="1" pageNumber="61">5</figureCitation>
, 
<figureCitation id="1367ED82FF8E237CFEFDFBEEFE9BFBEB" box="[284,300,1034,1060]" captionStart="FIGURES 7 – 9" captionStartId="2.[151,275,1963,1987]" captionText="FIGURES 7 – 9. Hind femora, tibiae and tarsi, lateral view. 7, Spartimas ornatipes Enderlein, male. 8, Spartimas apiciniger sp. nov., male. 9, Spartimas hainanensis sp. nov., male." pageId="1" pageNumber="61">8</figureCitation>
, 
<figureCitation id="1367ED82FF8E237CFEDBFBEEFE33FBEB" box="[314,388,1034,1060]" captionStart="FIGURES 10 – 13" captionStartId="3.[151,270,1599,1623]" captionTargetBox="[161,1425,201,1544]" captionTargetId="figure@3.[160,1431,194,1579]" captionTargetPageId="3" captionText="FIGURES 10 – 13. Spartimas apiciniger sp. nov., male. 10, tergites 9 – 10 and cercus, dorsal view. 11, tergites 9 – 10 and cercus, ventral view. 12, genital capsule, dorsal view. 13, genital capsule, ventral view." httpUri="https://zenodo.org/record/196685/files/figure.png" pageId="1" pageNumber="61">
10– 
<number id="3D94E320FF8E237CFE89FBEEFE33FBEB" box="[360,388,1034,1060]" pageId="1" pageNumber="61" value="13.0">13</number>
