<taxonomicName id="4C274D220D486924FE89CFEDFD44F8C1" authority="Agazzi, 1961" authorityName="Agazzi" authorityYear="1961" box="[332,718,1452,1477]" class="Insecta" family="Staphylinidae" genus="Bythinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="110" phylum="Arthropoda" rank="subSpecies" species="crepsensis" subSpecies="histrus">
