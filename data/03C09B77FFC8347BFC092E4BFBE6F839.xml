<taxonomicName id="4C6951E2FFC8347BFB622D0DFC03F899" authority="(DC.) DC. We" authorityName="DC. We" baseAuthorityName="DC." class="Magnoliopsida" family="Asteraceae" genus="Anaphalis" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="202" phylum="Tracheophyta" rank="species" species="wightiana">
<emphasis id="B91DF673FFC8347BFB622D0DFAECF8F9" box="[1238,1379,1810,1836]" italics="true" pageId="7" pageNumber="202">A. wightiana</emphasis>
