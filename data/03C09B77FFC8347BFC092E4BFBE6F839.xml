<bibRefCitation id="EFF85790FFC8347BFAFA2CCDFC34F8D9" author="De Candolle" firstAuthor="De Candolle" pageId="7" pageNumber="202" pagination="271 - 275" refId="ref4343" refString="DE CANDOLLE A. P. 1838. - Prodromus Systematis Naturalis Regni Vegetabilis. Vol. 6. Vevitque Eorumdem Bibliopolio, Paris: 271 - 275. https: // doi. org / 10.5962 / bhl. title. 286" type="book chapter" year="1838">De Candolle (1838)</bibRefCitation>
transferred 
<taxonomicName id="4C6951E2FFC8347BFBFF2CEDFAECF8D9" box="[1099,1379,1778,1804]" class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="202" phylum="Tracheophyta" rank="species" species="wightianum">
<emphasis id="B91DF673FFC8347BFBFF2CEDFAECF8D9" box="[1099,1379,1778,1804]" italics="true" pageId="7" pageNumber="202">Gnaphalium wightianum</emphasis>
</taxonomicName>
to the genus 
<taxonomicName id="4C6951E2FFC8347BFCC12D0DFB9DF8F9" authority="DC" authorityName="DC" box="[885,1042,1810,1836]" class="Magnoliopsida" family="Asteraceae" genus="Anaphalis" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="202" phylum="Tracheophyta" rank="genus">
