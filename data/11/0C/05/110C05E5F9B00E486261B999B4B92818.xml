<normalizedToken originalValue="paßt">passt</normalizedToken>
<taxonomicName class="Arachnida" family="Euphthiracaridae" genus="Euphthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageNumber="132" phylum="Arthropoda" rank="species" species="mixtus">E. mixtus</taxonomicName>
