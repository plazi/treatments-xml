<paragraph pageId="6" pageNumber="105">
<taxonomicName class="Insecta" family="Carabidae" genus="Wanoblemus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Wanoblemus wui" order="Coleoptera" pageId="6" pageNumber="105" phylum="Arthropoda" rank="species" species="wui">Wanoblemus wui</taxonomicName>
