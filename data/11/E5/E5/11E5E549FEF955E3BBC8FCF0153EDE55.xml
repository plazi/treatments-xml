. 
<taxonomicName authorityName="Povolny" authorityYear="1977" class="Insecta" family="Gelechiidae" genus="Caryocolum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Caryocolum syriacum" order="Lepidoptera" pageId="0" pageNumber="39" phylum="Arthropoda" rank="species" species="syriacum">
