<taxonomicName authorityName="Krefft" authorityYear="1871" box="[1104,1247,1901,1926]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="30" pageNumber="181" phylum="Nematoda" rank="genus">
