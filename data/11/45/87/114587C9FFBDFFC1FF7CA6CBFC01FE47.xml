<taxonomicName authority="Azevedo" authorityName="Azevedo" box="[843,1132,330,355]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="dentiformis">
<emphasis box="[843,1014,330,355]" italics="true" pageId="34" pageNumber="35">D. dentiformis</emphasis>
Azevedo
</taxonomicName>
, 
<taxonomicName authority="Evans" authorityName="Evans" box="[1153,1388,330,355]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="ulceratus">
<emphasis box="[1153,1301,330,355]" italics="true" pageId="34" pageNumber="35">D. ulceratus</emphasis>
