</taxonomicName>
, 
<taxonomicName authority="Azevedo" authorityName="Azevedo" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="concavatus">
<emphasis italics="true" pageId="34" pageNumber="35">D. concavatus</emphasis>
Azevedo
</taxonomicName>
, 
<taxonomicName authority="Azevedo" authorityName="Azevedo" box="[405,671,366,391]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="rectilineus">
<emphasis box="[405,560,366,391]" italics="true" pageId="34" pageNumber="35">D. rectilineus</emphasis>
