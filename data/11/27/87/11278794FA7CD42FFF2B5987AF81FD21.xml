<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[401,554,629,655]" class="Liliopsida" family="Asparagaceae" kingdom="Plantae" order="Asparagales" pageId="10" pageNumber="61" phylum="Tracheophyta" rank="family">Asparagaceae</taxonomicName>
