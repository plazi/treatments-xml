<paragraph blockId="56.[151,1437,259,826]" pageId="56" pageNumber="57">
<emphasis bold="true" box="[199,315,691,717]" pageId="56" pageNumber="57">Remarks.</emphasis>
<taxonomicName box="[323,537,691,718]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="56" pageNumber="57" phylum="Porifera" rank="species" species="laevis" subGenus="Mycale">
