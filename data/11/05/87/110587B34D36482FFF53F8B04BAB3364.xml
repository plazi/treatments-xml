<quantity box="[905,962,330,357]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="62" pageNumber="63" unit="m" value="15.0">15 m</quantity>
depth
</elevation>
, coll. 
<collectorName box="[1101,1258,330,356]" pageId="62" pageNumber="63">Diana Ugalde</collectorName>
, 
<date box="[1269,1423,330,357]" pageId="62" pageNumber="63" value="2016-06-25">
<collectingDate box="[1269,1423,330,357]" pageId="62" pageNumber="63" value="2016-06-25">25 June 2016</collectingDate>
