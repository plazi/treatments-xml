<subSubSection pageId="8" pageNumber="9" type="distribution">
<paragraph blockId="8.[151,1437,763,1402]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[199,353,1195,1221]" pageId="8" pageNumber="9">Distribution.</emphasis>
<collectingCountry box="[361,447,1195,1221]" name="Mexico" pageId="8" pageNumber="9">Mexico</collectingCountry>
(
<bibRefCitation author="Green, G." box="[463,602,1195,1221]" pageId="8" pageNumber="9" pagination="79 - 98" refId="ref76592" refString="Green, G. (1977) Sinopsis Taxonomica de esponjas de la Blanquilla. Anales del Centro de Ciencias del Mar y Limnologia, Universidad Nacional Autonoma de Mexico, 1, 79 - 98." type="journal article" year="1977">Green 1977</bibRefCitation>
: as 
<taxonomicName authorityName="Gray" authorityYear="1867" box="[647,766,1195,1221]" class="Demospongiae" family="Agelasidae" genus="Ectyon" kingdom="Animalia" order="Agelasida" pageId="8" pageNumber="9" phylum="Porifera" rank="species" species="sparsus">
<emphasis box="[647,766,1195,1221]" italics="true" pageId="8" pageNumber="9">A. sparsus</emphasis>
</taxonomicName>
; 
<bibRefCitation author="Gomez, P." box="[783,931,1195,1221]" pageId="8" pageNumber="9" refId="ref75998" refString="Gomez, P. (2002) Esponjas marinas del Golfo de Mexico y el Caribe. AGT Editors S. A, Mexico, D. F., 134 pp." type="book" year="2002">Gómez 2002</bibRefCitation>
, 
<bibRefCitation author="Gomez, P." box="[944,1003,1195,1221]" pageId="8" pageNumber="9" pagination="51 - 72" refId="ref76106" refString="Gomez, P. (2007) Inventario de las esponjas del Parque Nacional Sistema Arrecifal Veracruzano, con nuevos Registros de Especies (Porifera: Demospongiae). In: Jimenez-Hernandez, M. J.; Granados-Barba, A. &amp; Ortiz-Lozano, L. (Eds.), Investigaciones Cientificas en el Sistema Arrecifal Veracruzano. Universidad Autonoma de Campeche, Campeche, pp. 51 - 72." type="book chapter" year="2007">2007</bibRefCitation>
, 
<bibRefCitation author="Gomez, P." box="[1016,1074,1195,1221]" pageId="8" pageNumber="9" pagination="217 - 224" refId="ref76179" refString="Gomez, P. (2011) Esponjas marinas y de agua dulce. In: Cruz, A. (Ed.), La biodiversidad en Veracruz: Estudio de Estado. Vol II. Comision Nacional para el Conocimiento y uso de la Biodiversidad (CONABIO), Gobierno del Estado de Veracruz, Universidad Veracruzana, Instituto de Ecologia, Veracruz, pp. 217 - 224." type="book chapter" year="2011">2011</bibRefCitation>
