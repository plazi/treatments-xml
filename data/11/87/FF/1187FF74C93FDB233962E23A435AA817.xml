<taxonomicName class="Magnoliopsida" family="Theaceae" genus="Camellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Camellia reticulata" order="Ericales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="reticulata">Camellia reticulata</taxonomicName>
