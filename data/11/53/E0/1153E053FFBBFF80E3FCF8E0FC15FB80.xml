/ total 
<number box="[1311,1378,639,664]" pageId="8" pageNumber="285" value="23.64">23.64</number>
; II— 
<number box="[151,199,676,701]" pageId="8" pageNumber="285" value="6.47">6.47</number>
/ 
<number box="[215,263,676,700]" pageId="8" pageNumber="285" value="8.13">8.13</number>
