<paragraph blockId="7.[113,764,1292,1444]" pageId="7" pageNumber="176">
<keyLead pageId="7" pageNumber="176">18a. Inflorescences robust, lacking white woolly pubescence, bracts shorter than the flowers .................................. 19</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="7" pageNumber="176">
<paragraph blockId="7.[113,764,1463,1711]" pageId="7" pageNumber="176">
<keyLead pageId="7" pageNumber="176">
