<bibRefCitation author="Benavides-Serrato, M" journalOrPublisher="Serie de Publicaciones Especiales de Invemar, 22. Santa Marta" pageId="23" pageNumber="24" refId="B12" refString="Benavides-Serrato, M, Borrero-Perez, GH, Dias Sanchez, CM, 2011. Equinodermos del Caribe colombiano I: Crinoidea, Asteroidea y Ophiuroidea. Serie de Publicaciones Especiales de Invemar, 22. Santa Marta" title="Equinodermos del Caribe colombiano I: Crinoidea, Asteroidea y Ophiuroidea." year="2011">2011</bibRefCitation>
). In Brazil: CE, PB, PE, RJ, SP, SC, and RS (
<bibRefCitation author="Bernasconi, I" journalOrPublisher="Boletim do Instituto Oceanografico" pageId="23" pageNumber="24" pagination="51 - 57" publicationUrl="10.1590/S0373-55241955000100002" refId="B16" refString="Bernasconi, I, 1955. Equinoideos y Asteroideos de la Coleccion del Instituto Oceanografico de la Univerdidad de San Pablo. Primera contribucion. Boletim do Instituto Oceanografico 6: 51 - 57, DOI: 10.1590/S0373-55241955000100002" title="Equinoideos y Asteroideos de la Coleccion del Instituto Oceanografico de la Univerdidad de San Pablo. Primera contribucion." url="10.1590/S0373-55241955000100002" volume="6" year="1955">Bernasconi 1955</bibRefCitation>
, 
<bibRefCitation author="Brito, IM" journalOrPublisher="Universidade do Brasil, Faculdade Nacional de Filosofia, Centro de Estudos Zoologicos" pageId="23" pageNumber="24" pagination="1 - 10" refId="B21" refString="Brito, IM, 1962. Ensaio de catalogo dos equinodermas do Brasil. Universidade do Brasil, Faculdade Nacional de Filosofia, Centro de Estudos Zoologicos: 1 - 10" title="Ensaio de catalogo dos equinodermas do Brasil" year="1962">Brito 1962</bibRefCitation>
, 
<bibRefCitation author="Lima-Verde, JS" journalOrPublisher="Arquivos de Ciencias do Mar" pageId="26" pageNumber="27" pagination="9 - 13" refId="B72" refString="Lima-Verde, JS, 1969. Primeira contribuicao ao inventario dos Echinodermas do nordeste Brasileiro. Arquivos de Ciencias do Mar 9: 9 - 13" title="Primeira contribuicao ao inventario dos Echinodermas do nordeste Brasileiro." volume="9" year="1969">Lima-Verde 1969</bibRefCitation>
, 
<bibRefCitation author="Netto, LF" journalOrPublisher="University of Sao Paulo" pageId="27" pageNumber="28" refId="B105" refString="Netto, LF, 2006. Echinodermata do Canal de Sao Sebastiao, Sao Sebastiao (SP). University of Sao Paulo" title="Echinodermata do Canal de Sao Sebastiao, Sao Sebastiao (SP)" year="2006">Netto 2006</bibRefCitation>
, 
<bibRefCitation author="Gondim, AI" journalOrPublisher="Biota Neotropica" pageId="25" pageNumber="26" pagination="151 - 159" publicationUrl="10.1590/S1676-06032008000200016" refId="B46" refString="Gondim, AI, Lacouth, P, Alonso, C, Manso, CLC, 2008. Echinodermata da praia do Cabo Branco, Joao Pessoa, Paraiba, Brasil. Biota Neotropica 8: 151 - 159, DOI: 10.1590/S1676-06032008000200016" title="Echinodermata da praia do Cabo Branco, Joao Pessoa, Paraiba, Brasil." url="10.1590/S1676-06032008000200016" volume="8" year="2008">Gondim et al. 2008</bibRefCitation>
, 
<bibRefCitation author="Lima, EJB" journalOrPublisher="Revista Brasileira de Zoociencias" pageId="26" pageNumber="27" pagination="55 - 63" refId="B71" refString="Lima, EJB, Fernandes, MLB, 2009. Diversidade de equinodermos (Echinodermata) no Estado de Pernambuco (Brasil). Revista Brasileira de Zoociencias 11: 55 - 63" title="Diversidade de equinodermos (Echinodermata) no Estado de Pernambuco (Brasil)." volume="11" year="2009">Lima and Fernandes 2009</bibRefCitation>
, 
<bibRefCitation author="Xavier, LAR" journalOrPublisher="Brazilian Journal of Aquatic Science and Technology" pageId="30" pageNumber="31" pagination="73 - 78" publicationUrl="10.14210/bjast.v14n2.p73-78" refId="B157" refString="Xavier, LAR, 2010. Inventario dos equinodermos do Estado de Santa Catarina, Brasil. Brazilian Journal of Aquatic Science and Technology 14: 73 - 78, DOI: 10.14210/bjast.v14n2.p73-78" title="Inventario dos equinodermos do Estado de Santa Catarina, Brasil." url="10.14210/bjast.v14n2.p73-78" volume="14" year="2010">Xavier 2010</bibRefCitation>
