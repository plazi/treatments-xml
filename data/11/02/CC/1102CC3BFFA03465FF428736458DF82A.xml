<number box="[750,826,1196,1222]" pageId="4" pageNumber="66" value="16.5" valueMax="18.0" valueMin="15.0">15–18</number>
, EL 
<number box="[885,1005,1196,1222]" pageId="4" pageNumber="66" value="0.12000000000000001" valueMax="0.14" valueMin="0.1">0.10–0.14</number>
, HL 
<number box="[1067,1187,1196,1222]" pageId="4" pageNumber="66" value="0.79" valueMax="0.87" valueMin="0.71">0.71–0.87</number>
, HW 
<number box="[1258,1378,1196,1222]" pageId="4" pageNumber="66" value="0.73" valueMax="0.81" valueMin="0.65">0.65–0.81</number>
, ML 
<number box="[151,270,1234,1260]" pageId="4" pageNumber="66" value="0.88" valueMax="0.99" valueMin="0.77">0.77–0.99</number>
