<taxonomicName class="Sordariomycetes" family="Melanconiellaceae" genus="Oblongisporothyrium" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Oblongisporothyrium castanopsidis" order="Diaporthales" pageId="0" pageNumber="185" phylum="Ascomycota" rank="species" species="castanopsidis">
<emphasis italics="true" pageId="0" pageNumber="185">Oblongisporothyrium castanopsidis</emphasis>
</taxonomicName>
