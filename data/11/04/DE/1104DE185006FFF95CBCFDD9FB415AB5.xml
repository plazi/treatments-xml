<collectingRegion box="[900,1035,837,863]" country="Russia" name="Kamchatka" pageId="13" pageNumber="545">Kamtchatka</collectingRegion>
(leg. 
<collectorName box="[1099,1236,837,863]" pageId="13" pageNumber="545">Derschawin</collectorName>
