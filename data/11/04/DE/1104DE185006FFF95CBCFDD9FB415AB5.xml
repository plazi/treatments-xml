<specimenCount box="[1388,1431,801,827]" pageId="13" pageNumber="545" type="male">1 ♂</specimenCount>
, 
<specimenCount box="[151,193,837,863]" pageId="13" pageNumber="545" type="female">4 ♀</specimenCount>
