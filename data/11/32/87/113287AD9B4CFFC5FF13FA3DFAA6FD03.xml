<number box="[1171,1199,1497,1518]" pageId="24" pageNumber="25" value="77.0">77</number>
, penis; 
<number box="[1277,1305,1496,1518]" pageId="24" pageNumber="25" value="78.0">78</number>
, tegmen; 
<number box="[1404,1432,1496,1518]" pageId="24" pageNumber="25" value="79.0">79</number>
, S 
