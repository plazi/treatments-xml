<collectionCode box="[500,535,1701,1725]" pageId="20" pageNumber="21">BL</collectionCode>
/BW 
<quantity box="[598,664,1701,1726]" metricMagnitude="-2" metricUnit="m" metricValue="3.81" pageId="20" pageNumber="21" unit="in" value="1.5">1.5 in</quantity>
