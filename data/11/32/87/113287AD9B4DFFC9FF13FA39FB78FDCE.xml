. 
<collectionCode box="[310,346,1702,1725]" pageId="20" pageNumber="21">BL</collectionCode>
<quantity box="[356,486,1701,1725]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="1.8" metricValueMin="1.7" pageId="20" pageNumber="21" unit="mm" value="1.75" valueMax="1.8" valueMin="1.7">1.7–1.8mm</quantity>
