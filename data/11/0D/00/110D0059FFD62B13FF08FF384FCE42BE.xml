Huber, 
<number box="[793,848,403,427]" pageId="13" pageNumber="214" value="2011.0">2011</number>
</bibRefCitation>
</taxonomicName>
(see 
<bibRefCitation author="Huber" box="[908,1045,402,427]" pageId="13" pageNumber="214" refString="Huber, B. A. (2011) Revision and cladistic analysis of Pholcus and closely related taxa (Araneae, Pholcidae). Bonner Zoologische Monographien, 58, 1 - 509." type="journal article" year="2011">
Huber 
<number box="[986,1045,403,427]" pageId="13" pageNumber="214" value="2011.0">2011</number>
</bibRefCitation>
: 
<number box="[1056,1101,402,427]" pageId="13" pageNumber="214" value="285.0">285</number>
, figs 
<date box="[1162,1291,402,427]" pageId="13" pageNumber="214" value="1361" valueMax="1363">
<number box="[1162,1218,402,427]" pageId="13" pageNumber="214" value="1361.0">1361</number>
–1363
</date>
, 
<date box="[1303,1431,402,427]" pageId="13" pageNumber="214" value="1396" valueMax="1397">1396–1397</date>
, 
<date box="[151,278,438,463]" pageId="13" pageNumber="214" value="1425" valueMax="1429">
1425– 
<number box="[222,278,438,463]" pageId="13" pageNumber="214" value="1429.0">1429</number>
</date>
) and 
<emphasis box="[340,436,439,462]" italics="true" pageId="13" pageNumber="214">P. r ut e ng</emphasis>
<bibRefCitation author="Huber" box="[446,584,438,463]" pageId="13" pageNumber="214" refString="Huber, B. A. (2011) Revision and cladistic analysis of Pholcus and closely related taxa (Araneae, Pholcidae). Bonner Zoologische Monographien, 58, 1 - 509." type="journal article" year="2011">
Huber, 
<number box="[529,584,439,463]" pageId="13" pageNumber="214" value="2011.0">2011</number>
</bibRefCitation>
(see 
<bibRefCitation author="Huber" box="[642,777,438,463]" pageId="13" pageNumber="214" refString="Huber, B. A. (2011) Revision and cladistic analysis of Pholcus and closely related taxa (Araneae, Pholcidae). Bonner Zoologische Monographien, 58, 1 - 509." type="journal article" year="2011">
Huber 
<number box="[718,777,439,463]" pageId="13" pageNumber="214" value="2011.0">2011</number>
</bibRefCitation>
: 
<number box="[788,833,439,463]" pageId="13" pageNumber="214" value="287.0">287</number>
, figs 
<date box="[892,1021,438,463]" pageId="13" pageNumber="214" value="1364" valueMax="1366">
<number box="[892,949,438,463]" pageId="13" pageNumber="214" value="1364.0">1364</number>
–1366
</date>
, 
<date box="[1031,1160,439,463]" pageId="13" pageNumber="214" value="1398" valueMax="1399">1398–1399</date>
