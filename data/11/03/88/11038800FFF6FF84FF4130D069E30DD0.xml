<emphasis box="[374,432,1982,2007]" italics="true" pageId="25" pageNumber="26">et al.</emphasis>
<quantity box="[440,496,1982,2007]" metricMagnitude="1" metricUnit="m" metricValue="5.03936" pageId="25" pageNumber="26" unit="in" value="1984.0">1984</quantity>
</bibRefCitation>
in 
<collectingCountry box="[535,604,1982,2007]" name="China" pageId="25" pageNumber="26">China</collectingCountry>
