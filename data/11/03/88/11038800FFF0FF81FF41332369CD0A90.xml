, 
<collectingCountry box="[561,619,1772,1797]" name="India" pageId="31" pageNumber="32">India</collectingCountry>
(
<collectingRegion box="[634,715,1772,1797]" country="India" name="Assam" pageId="31" pageNumber="32">Assam</collectingRegion>
, 
<collectingRegion box="[727,811,1772,1797]" country="India" name="Sikkim" pageId="31" pageNumber="32">Sikkim</collectingRegion>
, W. Bengal), 
<collectingCountry box="[965,1022,1772,1795]" name="Laos" pageId="31" pageNumber="32">Laos</collectingCountry>
, 
<collectingCountry box="[1035,1146,1772,1796]" name="Myanmar" pageId="31" pageNumber="32">Myanmar</collectingCountry>
, 
<collectingCountry box="[1157,1271,1772,1797]" name="Sri Lanka" pageId="31" pageNumber="32">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[1283,1382,1772,1797]" name="Vietnam" pageId="31" pageNumber="32">Vietnam</collectingCountry>
. The species also occurs in 
<collectingCountry box="[400,467,1807,1832]" name="China" pageId="31" pageNumber="32">China</collectingCountry>
