<taxonomicName box="[770,970,655,680]" class="Magnoliopsida" family="Dipterocarpaceae" kingdom="Plantae" order="Malvales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Dipterocarpaceae</taxonomicName>
) in 
