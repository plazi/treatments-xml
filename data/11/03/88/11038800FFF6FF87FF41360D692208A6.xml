<collectingCountry box="[416,525,1094,1119]" name="Indonesia" pageId="25" pageNumber="26">Indonesia</collectingCountry>
(
<collectingRegion box="[541,595,1094,1119]" country="Indonesia" name="Jawa Timur" pageId="25" pageNumber="26">Java</collectingRegion>
, 
<collectingRegion box="[608,718,1094,1119]" country="Indonesia" name="Sumatera Utara" pageId="25" pageNumber="26">Sumatera</collectingRegion>
), 
<collectingCountry box="[738,841,1094,1119]" name="Malaysia" pageId="25" pageNumber="26">Malaysia</collectingCountry>
(E. &amp; W.). Intercepted in 
<collectingCountry box="[1141,1205,1095,1118]" name="Japan" pageId="25" pageNumber="26">Japan</collectingCountry>
