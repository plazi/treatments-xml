<typeStatus id="D11A882CFFCCFFBBFF26E2BDAE46FCA1" box="[255,301,756,775]" pageId="15" pageNumber="159">Type</typeStatus>
genus: 
<taxonomicName id="C9A14D0DFFCCFFBBFEADE2BDAD02FCA1" authority="Westwood, 1845" authorityName="Westwood" authorityYear="1845" box="[372,617,756,775]" class="Insecta" family="Aphodiidae" genus="Rhyparus" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="159" phylum="Arthropoda" rank="genus">
