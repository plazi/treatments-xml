<taxonomicName id="51CD2BBE3E4BFF847BFEB9CCD892FBCF" authorityName="Hosoishi &amp; Ogata" authorityYear="2016" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="599" phylum="Arthropoda" rank="species" species="vieti">
