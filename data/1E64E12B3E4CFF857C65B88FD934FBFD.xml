<taxonomicName id="51CD2BBE3E4BFF847F9BB9CCD9FCFC20" authorityName="Hosoishi &amp; Ogata" authorityYear="2016" box="[144,467,982,1002]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="599" phylum="Arthropoda" rank="species" species="sundalandensis">
