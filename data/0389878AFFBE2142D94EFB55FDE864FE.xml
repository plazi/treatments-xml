Basonym: 
<taxonomicName id="4C204D1FFFBE2142D9DFFAD6FDC6673F" authority="Wiese et al. 1996" authorityName="Wiese" authorityYear="1996" box="[232,617,1344,1368]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="51" pageNumber="2833" phylum="Firmicutes" rank="species" species="panis">
