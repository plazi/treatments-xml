<taxonomicName id="4C69B6D9FFA18A05FDC6A1CA19EAFF72" box="[632,687,151,175]" class="Insecta" family="Cantharidae" genus="Lycocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="28" phylum="Arthropoda" rank="species" species="vita" status="sp. nov.">Víťa</taxonomicName>
