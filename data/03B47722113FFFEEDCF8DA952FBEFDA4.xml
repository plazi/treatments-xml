<taxonomicName id="4C1DBDB7113FFFE9DD01DA942831FC03" authorityName="Lacordaire" authorityYear="1842" box="[366,531,910,937]" class="Insecta" family="Carabidae" genus="Physodeutera" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="154" phylum="Arthropoda" rank="genus">
