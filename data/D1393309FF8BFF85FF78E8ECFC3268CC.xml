<treatmentCitationGroup id="7980A531FF8BFF85FF78E92AFDA36A6A" pageId="5" pageNumber="148">
<taxonomicName id="9E90F99CFF8BFF85FF78E92AFE696A4D" authority="Bolivar, 1892: 29" authorityName="Bolivar" authorityPageNumber="29" authorityYear="1892" box="[151,433,1334,1356]" class="Insecta" family="Nocticolidae" genus="Nocticola" kingdom="Animalia" order="Blattodea" pageId="5" pageNumber="148" phylum="Arthropoda" rank="genus">
<emphasis id="6BE45E0DFF8BFF85FF78E92AFF226A4D" box="[151,250,1334,1356]" italics="true" pageId="5" pageNumber="148">Nocticola</emphasis>
<treatmentCitation id="D831A40EFF8BFF85FEEFE92AFE696A4D" author="Bolivar" box="[256,433,1334,1356]" page="29" pageId="5" pageNumber="148" year="1892">
<bibRefCitation id="3D01FFEEFF8BFF85FEEFE92AFE556A4D" author="Bolivar" box="[256,397,1334,1356]" pageId="5" pageNumber="148" refString="Bolivar, I. (1892) Voyage de M. E. Simon aux l'ile Philippines (Mars et Avril 1890). Etudes sur les Arthropodes cavernicoles de l'ile de Luzon. Annales de la Societe Entomologique de France, 61, 29 - 34." type="journal article" year="1892">Bolívar, 1892</bibRefCitation>
: 29
