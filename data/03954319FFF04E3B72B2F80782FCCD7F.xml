<taxonomicName id="4C3C898CFFF04E3B71D5FDDB82C8CA11" authorityName="A.Hay" authorityYear="1996" box="[975,1169,534,558]" class="Liliopsida" family="Araceae" genus="Colocasia" kingdom="Plantae" order="Alismatales" pageId="17" pageNumber="948" phylum="Tracheophyta" rank="species" species="oresbia">
