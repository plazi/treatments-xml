<emphasis id="B93EEAF1C204AF63C142D0E7FD1A47B3" bold="true" box="[358,710,736,759]" pageId="7" pageNumber="528">
<taxonomicName id="4C4A4D60C204AF63C142D0E7FD4E47B3" authority="WESMAEL 1844" authorityName="WESMAEL" authorityYear="1844" box="[358,658,736,759]" class="Insecta" family="Ichneumonidae" genus="Platylabus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="528" phylum="Arthropoda" rank="genus">
