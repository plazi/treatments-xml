<collectionCode id="199EAE37FFF0FFAAFC8BFEC7FC6A4D3F" box="[835,924,261,284]" country="Hungary" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="103" pageNumber="104">HNHM</collectionCode>
