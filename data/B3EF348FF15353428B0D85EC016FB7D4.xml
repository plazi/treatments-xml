<paragraph id="6CF712F95B9C79A4487DCBD1C708591A" pageId="18" pageNumber="43">
<taxonomicName id="CFA16EB9392A8C772A7771B87596D562" class="Magnoliopsida" family="Asteraceae" genus="Acilepis" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="18" pageNumber="43" phylum="Tracheophyta" rank="species" species="pseudosutepensis">
