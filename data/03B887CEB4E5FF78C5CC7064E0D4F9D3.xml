<emphasis id="B965EACAB4E5FF79C6C972F7E702F933" box="[959,1145,1688,1709]" italics="true" pageId="190" pageNumber="191">Centrina dorrigo</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A25657B1B4E5FF79C1FB72F7E18CF956" pageId="190" pageNumber="191" rank="species">new species</taxonomicNameLabel>
(circles) and 
<taxonomicName id="4C114D5BB4E5FF79C6DD72DCE768F956" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[939,1043,1715,1736]" class="Arachnida" family="Lamponidae" genus="Centrina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="190" pageNumber="191" phylum="Arthropoda" rank="species" species="bondi" status="new species">
<emphasis id="B965EACAB4E5FF79C6DD72DCE768F956" box="[939,1043,1715,1736]" italics="true" pageId="190" pageNumber="191">C. bondi</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A25657B1B4E5FF79C15D72DCE7C2F956" box="[1067,1209,1715,1736]" pageId="190" pageNumber="191" rank="species">new species</taxonomicNameLabel>
