<taxonomicName id="44EDF8453480287A4AB28CF81FD9AFEF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25181" class="Insecta" family="Formicidae" genus="Amblyopone" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amblyopone gaetulica Baroni Urbani" lsidName-HNS="Amblyopone gaetulica Baroni Urbani" order="Hymenoptera" pageNumber="47" phylum="Arthropoda" rank="species" species="gaetulica">Amblyopone gaetulica</taxonomicName>
