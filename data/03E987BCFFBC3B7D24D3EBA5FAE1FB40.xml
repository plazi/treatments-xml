<emphasis id="B934EAB8FFBC3B7D24D3EBA5FEECFE53" bold="true" box="[151,312,429,455]" italics="true" pageId="1" pageNumber="54">
<taxonomicName id="4C404D29FFBC3B7D24D3EBA5FEE7FE53" box="[151,307,429,455]" class="Diplopoda" family="Dalodesmidae" genus="Setoisenoton" kingdom="Animalia" order="Polydesmida" pageId="1" pageNumber="54" phylum="Arthropoda" rank="genus" status="n. gen.">Setoisenoton</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A20757C3FFBC3B7D2504EBA5FE46FE53" box="[320,402,429,455]" pageId="1" pageNumber="54" rank="genus">n. gen.</taxonomicNameLabel>
