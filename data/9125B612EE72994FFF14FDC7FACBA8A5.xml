</taxonomicName>
from Arvorezinha, State of 
<collectingRegion id="DB48C9E6EE72994FFC5FFC05FB61A8ED" box="[988,1199,906,931]" country="Brazil" name="Rio Grande do Sul" pageId="6" pageNumber="249">
Rio 
<collectingRegion id="DB48C9E6EE72994FFB88FC05FB61A8ED" box="[1035,1199,906,931]" inferredBySuffix="true" pageId="6" pageNumber="249">Grande do Sul</collectingRegion>
</collectingRegion>
