<taxonomicName id="D8DBC4B8FFFBD60CCACABC1F06CBF453" authority="Kroyer, 1864" authorityName="Kroyer" authorityYear="1864" baseAuthorityName="Kroyer" baseAuthorityYear="1864" box="[426,679,1854,1876]" pageId="16" pageNumber="17" rank="species" species="polynoes">
<taxonomicName id="D8DBC4B8FFFBD60CCACABC1F0668F453" box="[426,516,1854,1876]" pageId="16" pageNumber="17" rank="species" species="polynoes">
<emphasis id="2DAF6329FFFBD60CCACABC1F0668F453" box="[426,516,1854,1876]" italics="true" pageId="16" pageNumber="17">polynoes</emphasis>
