<taxonomicName id="21BFCCB6FFD2B9620E14F870962EF82F" authorityName="Zhou" authorityYear="2017" box="[301,377,1934,1955]" class="Insecta" family="Staphylinidae" genus="Ocypus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="300" phylum="Arthropoda" rank="genus">
