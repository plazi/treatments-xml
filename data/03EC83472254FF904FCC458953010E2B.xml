<emphasis id="B931EE432254FF964FCC40D8510C0C5F" bold="true" box="[140,286,1526,1548]" pageId="3" pageNumber="70">Figures 1-10.</emphasis>
<taxonomicName id="4C4549D22254FF964E6540D8518D0C5F" box="[293,415,1526,1548]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="70" phylum="Arthropoda" rank="subFamily" subFamily="Tersilochinae">Tersilochinae</taxonomicName>
spp., 
<specimenCount id="9D43F9D82254FF964E9C40D951E60C5F" box="[476,500,1527,1548]" pageId="3" pageNumber="70" type="female">♀♀</specimenCount>
, 
