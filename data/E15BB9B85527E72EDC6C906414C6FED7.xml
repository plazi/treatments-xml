<taxonomicName id="11326D7B204A6AF279C821252D23D5C2" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1860" class="Gastropoda" family="Orthalicidae" genus="Porphyrobaphe" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Porphyrobaphe (Porphyrobaphe) saturnus" order="Stylommatophora" pageId="29" pageNumber="46" phylum="Mollusca" rank="species" species="saturnus" subGenus="Porphyrobaphe">Porphyrobaphe (Porphyrobaphe) saturnus</taxonomicName>
