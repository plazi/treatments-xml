<taxonomicName id="F327F23432CF3C357E5E7E121A331A4D" class="Insecta" family="Phasmatidae" genus="Graeffea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Graeffea crouanii" order="Phasmida" pageId="13" pageNumber="72" phylum="Arthropoda" rank="species" species="crouanii">Graeffea crouanii</taxonomicName>
