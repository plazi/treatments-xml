<collectingCountry id="9E917650FF16FF45FEF9F86EFE30FCC3" box="[320,449,1978,2004]" name="Philippines" pageId="148" pageNumber="149">Philippines</collectingCountry>
, north to southern 
<collectingCountry id="9E917650FF16FF45FD28F86FFD25FCC2" box="[657,724,1979,2005]" name="Japan" pageId="148" pageNumber="149">Japan</collectingCountry>
, south to northern 
<collectingCountry id="9E917650FF16FF45FC18F86EFBF9FCC3" box="[929,1032,1978,2004]" name="Australia" pageId="148" pageNumber="149">Australia</collectingCountry>
and 
<collectingCountry id="9E917650FF16FF45FB87F86FFB1FFCC3" box="[1086,1262,1978,2005]" name="New Caledonia" pageId="148" pageNumber="149">New Caledonia</collectingCountry>
