<collectingRegion id="6A68F820FFB6285FFD1AFE1D2D01A2EC" box="[704,772,484,509]" country="Brazil" name="Bahia" pageId="15" pageNumber="318">Bahia</collectingRegion>
, 
<collectingRegion id="6A68F820FFB6285FFCCAFE1D2DB1A2ED" box="[784,948,484,509]" country="Brazil" name="Espirito Santo" pageId="15" pageNumber="318">Espírito Santo</collectingRegion>
, 
<collectingRegion id="6A68F820FFB6285FFC1AFE1D2A5FA2EC" box="[960,1114,484,509]" country="Brazil" name="Minas Gerais" pageId="15" pageNumber="318">Minas Gerais</collectingRegion>
, 
<collectingRegion id="6A68F820FFB6285FFBBCFE1D2B08A2EC" box="[1126,1293,484,509]" country="Brazil" name="Rio de Janeiro" pageId="15" pageNumber="318">Rio de Janeiro</collectingRegion>
, 
<collectingRegion id="6A68F820FFB6285FFAC3FE1D2EE7A131" country="Brazil" name="Rio Grande do Sul" pageId="15" pageNumber="318">Rio Grande do Sul</collectingRegion>
, 
<collectingRegion id="6A68F820FFB6285FFF37FDF12F94A131" box="[237,401,519,544]" country="Brazil" name="Santa Catarina" pageId="15" pageNumber="318">Santa Catarina</collectingRegion>
(new record), 
<collectingRegion id="6A68F820FFB6285FFDECFDF12CA2A131" box="[566,679,519,544]" country="Brazil" name="Sao Paulo" pageId="15" pageNumber="318">São Paulo</collectingRegion>
