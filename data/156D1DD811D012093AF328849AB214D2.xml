[[ 
<taxonomicName id="3426290FADE5D7D63BF9231E83B2655E" LSID-ZBK="urn:lsid:zoobank.org:act:B865A005-F1DE-4C23-BA3D-BFD3804C517A" class="Actinopterygii" family="Polypteridae" genus="Polypterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Polypterus Lacépède 1803:340" order="Polypteriformes" pageNumber="23" phylum="Chordata" rank="genus">Polypterus</taxonomicName>
