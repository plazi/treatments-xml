<collectingCountry id="F3244E9CFF8DFFB387FD76D4FE810281" box="[223,293,1189,1209]" name="Spain" pageId="13" pageNumber="1308">Spanien</collectingCountry>
(MAD), 
<collectingCountry id="F3244E9CFF8DFFB386A176D4FE690281" box="[387,461,1189,1209]" name="Switzerland" pageId="13" pageNumber="1308">Schweiz</collectingCountry>
