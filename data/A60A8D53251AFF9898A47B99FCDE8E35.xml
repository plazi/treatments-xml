<collectingCountry id="56B47CD5251AFF9899FD7EE1FD808FA4" box="[462,578,1231,1254]" name="Turkey" pageId="7" pageNumber="408">TURKEY</collectingCountry>
: 
<collectingRegion id="EC67F2A7251AFF989A7F7EE0FD5B8FA5" box="[588,665,1230,1255]" country="Turkey" name="Edirne" pageId="7" pageNumber="408">Edirne</collectingRegion>
, 
<collectingRegion id="EC67F2A7251AFF989A977EE0FCD78FA5" box="[676,789,1230,1255]" country="Turkey" name="Kirklareli" pageId="7" pageNumber="408">Kirklareli</collectingRegion>
