<taxonomicName id="E408FC849A3334EF1A00A6AEE13992E2" authority="" authorityName="" class="Gastropoda" family="Retusidae" genus="Volvula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Volvula" order="Cephalaspidea" pageId="0" pageNumber="505" phylum="Mollusca" rank="genus">Volvula</taxonomicName>
