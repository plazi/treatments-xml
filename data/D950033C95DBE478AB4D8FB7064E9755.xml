<taxonomicName id="5048F0725EA63C93E4304AE4F8A57718" class="Gastropoda" family="Rhizoridae" genus="Volvulella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Volvulella" order="Cephalaspidea" pageId="0" pageNumber="505" phylum="Mollusca" rank="genus">Volvulella</taxonomicName>
