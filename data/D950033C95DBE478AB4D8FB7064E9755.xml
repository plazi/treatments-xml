<paragraph id="B8DD676DE029BD160561B5FEBF91C4C5" pageId="0" pageNumber="505">
<taxonomicName id="5E91B50983D9132D034DE717D6FC0A7B" class="Gastropoda" family="Rhizoridae" genus="Volvulella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Volvulella minutissima" order="Cephalaspidea" pageId="0" pageNumber="505" phylum="Mollusca" rank="species" species="minutissima">Volvulella minutissima</taxonomicName>
