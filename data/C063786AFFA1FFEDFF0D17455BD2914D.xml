<quantity id="8F326499FFA1FFF2FECB16815E95928A" box="[337,436,1970,1994]" metricMagnitude="-3" metricUnit="m" metricValue="1.58" pageId="15" pageNumber="16" unit="mm" value="1.58">1.58 mm</quantity>
