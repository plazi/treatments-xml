<quantity id="4C8A9B706247FFDFFE57FD0A2B64A7EA" box="[510,594,739,762]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="3" pageNumber="648" unit="mm" value="3.2">3.2 mm</quantity>
