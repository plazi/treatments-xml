<taxonomicName id="4C748099C654FFB7C8FBFA4DFED12537" authority="(Pallas ex Pursh) G.L. Nesom &amp; Baird" authorityName="(Pallas ex Pursh) G.L. Nesom &amp; Baird" class="Magnoliopsida" family="Asteraceae" genus="Ericameria" kingdom="Plantae" order="Asterales" pageId="14" pageNumber="50" phylum="Tracheophyta" rank="species" species="nauseosa">
