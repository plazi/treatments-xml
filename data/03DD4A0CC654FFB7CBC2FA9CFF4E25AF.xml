<taxonomicName id="4C748099C654FFB7C92AF993FD662537" box="[639,767,1569,1593]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="14" pageNumber="50" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
