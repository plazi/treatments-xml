<taxonomicName id="4C5D4D75BD764656E9DFFB93CB72FBA9" authority="Linnavuori, 2009" authorityName="Linnavuori" authorityYear="2009" box="[174,343,1056,1074]" class="Insecta" family="Miridae" genus="Phytocoris" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="bavanus" status="sp. nov.">
