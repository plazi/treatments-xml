<quantity id="6616A9A9FF9E1E22472DFE3C7D9CFE2D" box="[774,926,484,510]" metricMagnitude="5" metricUnit="m" metricValue="4.32" metricValueMax="5.4399999999999995" metricValueMin="3.2" pageId="17" pageNumber="18" unit="km" value="432.0" valueMax="544.0" valueMin="320.0">320–544 km</quantity>
