<elevation id="000D506EFFAB3D3F3B18E61AFA51BE6E" box="[1377,1436,1375,1399]" metricMagnitude="1" metricUnit="m" metricValue="2.4" pageId="81" pageNumber="82" unit="m" value="24.0">
