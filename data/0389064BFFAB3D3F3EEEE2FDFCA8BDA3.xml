<collectingCounty id="62FECFD1FFAB3D3F3CA1E6C6FC6ABE83" box="[728,935,1410,1435]" pageId="81" pageNumber="82">eastern Timor Sea</collectingCounty>
</materialsCitation>
. 
<materialsCitation id="3B48BD00FFAB3D3F3DCAE6C7FD7DBEFB" ID-GBIF-Occurrence="1427553087" country="Cuba" location="Western Australia" pageId="81" pageNumber="82" specimenCount="1" stateProvince="Western Australia">
<bibRefCitation id="EFB1CAACFFAB3D3F3DCAE6C7FBF6BE83" author="Haig" box="[947,1083,1410,1435]" pageId="81" pageNumber="82" refString="Haig, D. W. (1997) Foraminifera from Exmouth Gulf, Western Australia. Journal of the Royal Society of Western Australia, 80, 263 - 280." type="journal article" year="1997">Haig (1997)</bibRefCitation>
