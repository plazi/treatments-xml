<taxonomicName id="4C674AF3FF8EFFE6FBC54944FA12C520" authority="(McAlpine)" authorityName="Gaimari" authorityYear="2020" baseAuthorityName="McAlpine, McAlpine" baseAuthorityYear="2020" box="[1118,1436,1380,1402]" class="Insecta" family="Chamaemyiidae" genus="Vitaleucopis" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="68" phylum="Arthropoda" rank="species" species="astonea">
