<taxonomicName id="C6019749644B431D0B96DB10614F0B8F" authorityName="Rudd" authorityYear="1968" class="Magnoliopsida" family="Fabaceae" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="subFamily" subFamily="Faboideae">Faboideae</taxonomicName>
