<paragraph id="54C1C7A0F230DE05A3051C67E81ECC61" pageId="0" pageNumber="103">
<taxonomicName id="000F45673EABA622F43983D7CD4598CC" authorityName="T. Deng, D. G. Zhang &amp; Jiao Sun" authorityYear="2020" class="Magnoliopsida" family="Primulaceae" genus="Primula" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Primula sunhangii" order="Ericales" pageId="0" pageNumber="103" phylum="Tracheophyta" rank="species" species="sunhangii">
