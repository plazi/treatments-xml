<taxonomicName id="4C570AE0FF86C165FAC4FB3AFF3BFB1B" authority="ski, 2009" authorityName="ski" authorityYear="2009" class="Insecta" family="Staphylinidae" genus="Horaeomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="299" phylum="Arthropoda" rank="species" species="fakfakensis" status="new species">
