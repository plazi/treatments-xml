<taxonomicName id="4C570AE0FF86C165FC4DFB3AFB4CFB06" authority="ski, 2009" authorityName="ski" authorityYear="2009" box="[939,1158,1165,1186]" class="Insecta" family="Staphylinidae" genus="Horaeomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="299" phylum="Arthropoda" rank="species" species="minor" status="new species">
