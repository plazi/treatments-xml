<quantity id="4CB79B6AFFC6FFB0F03CFF6A2DCD7490" box="[873,965,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="37" pageNumber="38" unit="mm" value="3.4">3.4 mm</quantity>
, body 
<quantity id="4CB79B6AFFC6FFB0F743FF6A2A7B7490" box="[1046,1139,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="37" pageNumber="38" unit="mm" value="1.6">1.6 mm</quantity>
), 
<collectingCountry id="F358761FFFC6FFB0F7DCFF6A2B2E7490" box="[1161,1318,151,175]" name="Indonesia" pageId="37" pageNumber="38">INDONESIA</collectingCountry>
