<materialsCitation id="3B273CD2FFC6FFB0F2D7FF652A2874EB" collectionCode="BPBM" collectorName="F. Muir" country="Indonesia" location="INDONESIA" pageId="37" pageNumber="38" specimenCount="1" specimenCount-female="1" typeStatus="lectotype">
<typeStatus id="54F4882DFFC6FFB0F2D7FF652FFA748F" box="[386,498,152,176]" pageId="37" pageNumber="38">Lectotype</typeStatus>
<specimenCount id="9D49FD06FFC6FFB0F153FF672C107490" box="[518,536,154,175]" pageId="37" pageNumber="38" type="female">♀</specimenCount>
