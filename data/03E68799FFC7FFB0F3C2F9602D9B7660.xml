<specimenCount id="9D49FD06FFC6FFB0F007FE4A2D7075F0" box="[850,888,439,463]" pageId="37" pageNumber="38" type="female">1 ♀</specimenCount>
(
<typeStatus id="54F4882DFFC6FFB0F0D2FE4A2A2875F0" box="[903,1056,439,463]" pageId="37" pageNumber="38">paralectotype</typeStatus>
of 
