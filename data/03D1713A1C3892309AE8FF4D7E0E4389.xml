<subSubSection id="C36293A71C3892309AB9FE057CA347CC" pageId="20" pageNumber="21" type="diagnosis">
<paragraph id="8BC7C02C1C3892309AB9FE057CA347CC" blockId="20.[151,1437,255,1738]" pageId="20" pageNumber="21">
<emphasis id="B90C1C3E1C3892309AB9FE057C3F44BA" bold="true" box="[198,326,479,505]" pageId="20" pageNumber="21">Diagnosis.</emphasis>
Rostrum 
<number id="3DB0D20B1C3892309BC0FE3A7C9A44B9" box="[447,483,480,506]" pageId="20" pageNumber="21" value="0.5">0.5</number>
