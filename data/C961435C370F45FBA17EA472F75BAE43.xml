<taxonomicName id="FBDD44DFB0194A0882D8EF354C815360" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:4041" class="Insecta" family="Scelionidae" genus="Archaeoteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Archaeoteleia novaezealandiae Masner" order="Hymenoptera" pageNumber="15" phylum="Arthropoda" rank="species" species="novaezealandiae">Archaeoteleia novaezealandiae Masner</taxonomicName>
