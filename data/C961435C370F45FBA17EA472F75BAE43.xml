<taxonomicName id="FD7671C81FE5D53BC73CC0AED32B74BD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:4041" class="Insecta" family="Scelionidae" genus="Archaeoteleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Archaeoteleia novaezealandiae Masner" order="Hymenoptera" pageNumber="15" phylum="Arthropoda" rank="species" species="novaezealandiae">Archaeoteleia novaezealandiae Masner</taxonomicName>
