<collectionCode id="ED6146FAFF89FFACFC81FEACFC690F38" box="[875,954,272,293]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="44" pageNumber="489" type="Museum">USNM</collectionCode>
