<taxonomicName id="E8F24D2CFF8833353BB038A7FCFE1583" box="[661,779,1763,1787]" class="Insecta" family="Cicadellidae" genus="Parallygus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="47" phylum="Arthropoda" rank="species" species="rameshi">
