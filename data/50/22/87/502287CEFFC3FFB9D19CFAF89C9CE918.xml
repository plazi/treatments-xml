<subSubSection box="[199,1132,1503,1528]" pageId="4" pageNumber="40" type="etymology">
<paragraph blockId="4.[151,1436,1431,2011]" box="[199,1132,1503,1528]" pageId="4" pageNumber="40">
<emphasis bold="true" box="[199,337,1504,1528]" pageId="4" pageNumber="40">Etymology:</emphasis>
