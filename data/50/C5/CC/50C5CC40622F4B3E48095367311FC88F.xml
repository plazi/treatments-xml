<subSubSection type="nomenclature">
<paragraph pageNumber="73">
<taxonomicName authorityName="Linnaeus" authorityYear="1753" class="Liliopsida" family="Poaceae" genus="Festuca" kingdom="Plantae" order="Poales" phylum="Tracheophyta" rank="species" species="ovina" status="spec. nov.">Festuca ovina</taxonomicName>
, 
<taxonomicNameLabel rank="species">spec. nov.</taxonomicNameLabel>
