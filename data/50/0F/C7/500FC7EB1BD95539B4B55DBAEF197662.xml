<taxonomicName genus="A." lsidName="A. (H.) kneuckeri" pageId="0" pageNumber="47" rank="subspecies" species="kneuckeri" subGenus="H." subspecies="zabranskyi">A. (H.) kneuckeri zabranskyi</taxonomicName>
<normalizedToken originalValue="Bílý">Bily</normalizedToken>
, 1995 (
<taxonomicName family="Buprestidae" lsidName="" pageId="0" pageNumber="47" rank="family">Buprestidae</taxonomicName>
