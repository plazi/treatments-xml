<taxonomicName authority="Des Gozis, 1886" authorityName="Des Gozis" authorityYear="1886" class="Insecta" family="Eucnemidae" genus="Hylis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hylis terminalis" order="Coleoptera" pageId="4" pageNumber="81" phylum="Arthropoda" rank="species" species="terminalis">Hylis terminalis</taxonomicName>
.
