<subSubSection lastPageId="9" lastPageNumber="12" pageId="7" pageNumber="10" type="diagnosis">
<paragraph blockId="7.[189,454,1771,1797]" box="[189,454,1771,1797]" pageId="7" pageNumber="10">
<heading bold="true" box="[189,454,1771,1797]" fontSize="11" level="3" pageId="7" pageNumber="10" reason="3">
