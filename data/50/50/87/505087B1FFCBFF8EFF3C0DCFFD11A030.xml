and 
<taxonomicName baseAuthorityName="Fabricius" baseAuthorityYear="1801" box="[862,998,914,935]" class="Insecta" family="Haliplidae" genus="Haliplus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="256" phylum="Arthropoda" rank="species" species="fulvus">
