<number box="[1393,1431,151,175]" pageId="16" pageNumber="53" value="3.4">3.4</number>
, 
<number box="[151,188,188,212]" pageId="16" pageNumber="53" value="1.5">1.5</number>
, 
<number box="[201,238,188,212]" pageId="16" pageNumber="53" value="2.5">2.5</number>
, -, 
<number box="[275,313,188,212]" pageId="16" pageNumber="53" value="3.6">3.6</number>
), leg I 
<number box="[393,442,188,212]" pageId="16" pageNumber="53" value="34.4">34.4</number>
(
<number box="[458,496,188,212]" pageId="16" pageNumber="53" value="9.3">9.3</number>
, 
<number box="[508,545,188,212]" pageId="16" pageNumber="53" value="3.5">3.5</number>
, 
<number box="[558,609,188,212]" pageId="16" pageNumber="53" value="10.4">10.4</number>
, 
<number box="[622,659,188,212]" pageId="16" pageNumber="53" value="9.1">9.1</number>
, 
<number box="[672,710,188,212]" pageId="16" pageNumber="53" value="2.1">2.1</number>
), leg II 
<number box="[799,848,188,212]" pageId="16" pageNumber="53" value="36.3">36.3</number>
(
<number box="[865,917,188,212]" pageId="16" pageNumber="53" value="10.3">10.3</number>
, 
<number box="[929,966,188,212]" pageId="16" pageNumber="53" value="3.7">3.7</number>
, 11.0, 
<number box="[1042,1080,188,212]" pageId="16" pageNumber="53" value="9.2">9.2</number>
, 
<number box="[1092,1130,188,212]" pageId="16" pageNumber="53" value="2.1">2.1</number>
), leg III 
<number box="[1228,1277,188,212]" pageId="16" pageNumber="53" value="32.2">32.2</number>
(
<number box="[1294,1332,188,212]" pageId="16" pageNumber="53" value="9.1">9.1</number>
, 
<number box="[1344,1382,188,212]" pageId="16" pageNumber="53" value="3.2">3.2</number>
, 
<number box="[1394,1431,188,212]" pageId="16" pageNumber="53" value="9.5">9.5</number>
, 
<number box="[151,188,223,247]" pageId="16" pageNumber="53" value="8.4">8.4</number>
, 2.0), leg IV 
<number box="[339,388,223,247]" pageId="16" pageNumber="53" value="34.6">34.6</number>
(
<number box="[404,443,223,247]" pageId="16" pageNumber="53" value="9.6">9.6</number>
, 
<number box="[454,492,223,247]" pageId="16" pageNumber="53" value="3.2">3.2</number>
, 
<number box="[504,541,223,247]" pageId="16" pageNumber="53" value="9.8">9.8</number>
, 
