<taxonomicName class="Gastropoda" family="Plectopylidae" genus="Gudeodiscus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gudeodiscus messageri" order="Stylommatophora" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="messageri">Gudeodiscus messageri</taxonomicName>
