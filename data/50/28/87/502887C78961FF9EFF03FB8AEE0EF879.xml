<taxonomicName class="Insecta" family="Encyrtidae" genus="Pseudectroma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="288" phylum="Arthropoda" rank="species" species="samare">
