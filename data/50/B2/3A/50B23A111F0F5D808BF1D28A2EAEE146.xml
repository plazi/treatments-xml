Benth., 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. plumieri" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="plumieri">
<emphasis italics="true" pageId="0" pageNumber="69">C. plumieri</emphasis>
</taxonomicName>
Benth., 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. pubescens" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="pubescens">
<emphasis italics="true" pageId="0" pageNumber="69">C. pubescens</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="C." higherTaxonomySource="CoL" kingdom="Plantae" lsidName="C. virginianum" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="species" species="virginianum">
<emphasis italics="true" pageId="0" pageNumber="69">C. virginianum</emphasis>
</taxonomicName>
(L.) Benth, 
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Centrosema" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Centrosema" order="Fabales" pageId="0" pageNumber="69" phylum="Tracheophyta" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="69">Centrosema</emphasis>
</taxonomicName>
