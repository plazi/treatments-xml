<taxonomicName baseAuthorityName="Dejean" baseAuthorityYear="1828" class="Hexapoda" family="Carabidae" genus="Laemostenus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="729" pageNumber="730" phylum="Arthropoda" rank="subSpecies" species="terricola" subSpecies="punctatus">
