<paragraph blockId="1.[189,1398,1168,1231]" pageId="1" pageNumber="2">
<taxonomicName authorityName="Johanson" authorityYear="2017" box="[189,454,1168,1194]" class="Insecta" family="Ecnomidae" genus="Caledomina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="paniensis">
<emphasis box="[189,454,1168,1194]" italics="true" pageId="1" pageNumber="2">Caledomina paniensis</emphasis>
</taxonomicName>
