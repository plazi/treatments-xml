<paragraph id="B53836C3F81AFF92FF5CFA79FE4FFA4F" blockId="20.[189,1398,1415,1477]" pageId="20" pageNumber="21">
<collectionCode id="D396AE06F81AFF92FF5CFA79FE92FA2B" box="[189,303,1415,1441]" pageId="20" pageNumber="21">BATHUS</collectionCode>
3: Stn. DW809, single sterile stem, 
<quantity id="727F9B26F81AFF92FD10FA79FCF9FA2B" box="[753,836,1415,1441]" metricMagnitude="-2" metricUnit="m" metricValue="2.4" pageId="20" pageNumber="21" unit="cm" value="2.4">2.4 cm</quantity>
