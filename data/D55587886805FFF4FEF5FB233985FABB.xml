<taxonomicName id="9AFC4D1D6806FFF4FB41FB533EF6FB43" box="[1315,1436,1221,1245]" class="Insecta" family="Encyrtidae" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="31" phylum="Arthropoda" rank="family">Encyrtidae</taxonomicName>
