<taxonomicName id="9AFC4D1D6806FFF4FA16FB533FB1FB43" box="[1140,1243,1221,1245]" class="Insecta" family="Psyllidae" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="31" phylum="Arthropoda" rank="family">Psyllidae</taxonomicName>
