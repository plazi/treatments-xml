<emphasis id="B9215978FFC6FFDDFF5AFEADBC6EFE10" bold="true" box="[151,269,367,392]" pageId="8" pageNumber="9">Holotype:</emphasis>
USNM 
<quantity id="4CAD288FFFC6FFDDFEA1FEADBF49FE1F" box="[364,554,367,392]" metricMagnitude="5" metricUnit="m" metricValue="3.53113167" pageId="8" pageNumber="9" unit="mm" value="3.53113167E8">353113, 167 mm</quantity>
