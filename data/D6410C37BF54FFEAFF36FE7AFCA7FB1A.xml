<subSubSection id="16F2EEAABF54FFEAFF36FDEFFBB6FDE2" box="[151,1102,546,571]" pageId="31" pageNumber="32" type="materials_examined">
<paragraph id="5E57BD21BF54FFEAFF36FDEFFBB6FDE2" blockId="31.[151,1437,546,1219]" box="[151,1102,546,571]" pageId="31" pageNumber="32">
<emphasis id="6C9C6133BF54FFEAFF36FDEFFE81FDE3" bold="true" box="[151,377,546,571]" pageId="31" pageNumber="32">Material examined</emphasis>
. 
<emphasis id="6C9C6133BF54FFEAFE26FDEFFE2FFDE3" bold="true" box="[391,471,546,571]" pageId="31" pageNumber="32">Stn. 16</emphasis>
, 
<date id="2A569BE1BF54FFEAFE47FDEFFDA2FDE2" box="[486,602,546,570]" pageId="31" pageNumber="32" value="2012-02-11">11.ii.2012</date>
