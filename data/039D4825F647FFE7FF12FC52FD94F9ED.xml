<collectionCode id="ED2561F6F647FFE7FD0BFBC5FD6CFB76" box="[654,734,1149,1175]" httpUri="http://grbio.org/cool/c7yt-hh77" name="Naturhistorisches Museum, Wien" pageId="4" pageNumber="5">NMW</collectionCode>
