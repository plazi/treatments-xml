<paragraph id="582C36E6FFB7FFD5FF5E867C1B3D6E26" blockId="2.[151,573,1363,1389]" box="[151,573,1363,1389]" pageId="2" pageNumber="3">
<heading id="0364818AFFB7FFD5FF5E867C1B3D6E26" bold="true" box="[151,573,1363,1389]" fontSize="11" level="1" pageId="2" pageNumber="3" reason="1">
<emphasis id="6AE7EAF4FFB7FFD5FF5E867C1B3D6E26" bold="true" box="[151,573,1363,1389]" pageId="2" pageNumber="3">
Family 
<taxonomicName id="9F934D65FFB7FFD5FF3F867C1B3D6E26" authority="Dana, 1852" authorityName="Dana" authorityYear="1852" box="[246,573,1363,1389]" class="Malacostraca" family="Callianassidae" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="family">
Callianassidae 
<bibRefCitation id="3C024B17FFB7FFD5FE7B867C1B3D6E26" author="Dana" box="[434,573,1363,1389]" pageId="2" pageNumber="3" refString="Dana, J. D. (1852) Crustacea. Part I. United States Exploring Expedition, during the years 1838, 1839, 1840, 1841, 1842, under the command of Charles Wilkes, U. S. N. 13, viii + 685 pp. Philadelphia: C. Sherman." type="book" year="1852">
Dana, 
<number id="EE5B24C1FFB7FFD5FDCA867C1B3D6E26" box="[515,573,1363,1389]" pageId="2" pageNumber="3" value="1852.0">1852</number>
