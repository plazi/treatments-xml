<number id="3DCF24B6FF92FFDAFBA0FB39E8F7FB20" box="[1089,1206,1208,1234]" pageId="16" pageNumber="17" value="2.19" valueMax="2.34" valueMin="2.04">2.04–2.34</number>
(
<number id="3DCF24B6FF92FFDAFB28FB39E8A7FB20" box="[1225,1254,1208,1234]" pageId="16" pageNumber="17" value="11.0">11</number>
measured). 
<collectingCountry id="F3107601FF92FFDAFA9AFB39E9DDFB20" box="[1403,1436,1208,1234]" name="American Samoa" pageId="16" pageNumber="17">As</collectingCountry>
<typeStatus id="54BC8833FF92FFDAFF76FB61ECBFFB08" box="[151,254,1248,1274]" pageId="16" pageNumber="17">holotype</typeStatus>
