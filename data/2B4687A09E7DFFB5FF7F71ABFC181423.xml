<collectingRegion id="612BF8549E7DFFB5FD0E75D9FCF71794" box="[701,769,451,473]" country="Nigeria" name="Rivers" pageId="121" pageNumber="121">River</collectingRegion>
, 
<emphasis id="919BEAA49E7DFFB5FCAB75D9FBC01795" bold="true" box="[792,1078,451,472]" pageId="121" pageNumber="121">AMNH 721283–721292</emphasis>
, six males, four females, 1 and 
<date id="D75110769E7DFFB5FC7F75FBFD011459" pageId="121" pageNumber="121" value="1907-06-05" valueMax="1907-07-24" valueMin="1907-06-05">5 June and 24 July 1907</date>
