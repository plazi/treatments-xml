La 
<taxonomicName id="2536D6A25B387F46B13FB38B5F77BE10" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:231157" class="Insecta" family="Formicidae" genus="Messor" kingdom="Animalia" lsidName-HNS="Messor meridionalis (André)" order="Hymenoptera" pageNumber="70" phylum="Arthropoda" rank="variety" species="arenarius" variety="meridionalis">
