<quantity id="4CF39B0BFFE9084647DBFD6F633FFCDD" box="[989,1097,764,790]" metricMagnitude="-4" metricUnit="m" metricValue="3.5" pageId="6" pageNumber="7" unit="mm" value="0.35">0.35 mm</quantity>
, left side 
<quantity id="4CF39B0BFFE9084640BFFD6F6253FCDD" box="[1209,1317,764,790]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="6" pageNumber="7" unit="mm" value="0.15">0.15 mm</quantity>
; aedeagus length 
<quantity id="4CF39B0BFFE9084645CDFCB7655EFCF5" box="[459,552,804,830]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="6" pageNumber="7" unit="mm" value="2.2">2.2 mm</quantity>
, width 
<quantity id="4CF39B0BFFE90846467BFCB765ADFCF5" box="[637,731,804,830]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="6" pageNumber="7" unit="mm" value="0.6">0.6 mm</quantity>
, rounded; everted vesica length 
<quantity id="4CF39B0BFFE908464052FCB763C7FCF5" box="[1108,1201,804,830]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="6" pageNumber="7" unit="mm" value="2.3">2.3 mm</quantity>
