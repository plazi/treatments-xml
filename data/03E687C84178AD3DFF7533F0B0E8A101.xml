species: 
<taxonomicName id="4C4F4D5D4178AD3DFEF33232B3AEA63A" authority="GRAVENHORST" authorityName="GRAVENHORST" box="[308,695,282,302]" class="Insecta" family="Ichneumonidae" genus="Ichneumon" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="644" phylum="Arthropoda" rank="species" species="subcylindricus">
