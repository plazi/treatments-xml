</taxonomicName>
(= 
<taxonomicName id="4C4F4D5D4178AD3DFD163232B519A63A" authority="HEINRICH" authorityName=": HEINRICH" authorityYear="1936" box="[721,1024,282,302]" class="Insecta" family="Ichneumonidae" genus="Bureschias" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="644" phylum="Arthropoda" rank="species" species="balcanicus">
