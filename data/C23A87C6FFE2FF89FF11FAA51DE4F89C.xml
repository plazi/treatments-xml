<quantity id="8D6B9B35FFE2FF89FB0CF8B718F6F8C0" box="[1162,1264,1831,1855]" metricMagnitude="1" metricUnit="m" metricValue="1.95" metricValueMax="2.1" metricValueMin="1.8" pageId="81" pageNumber="82" unit="m" value="19.5" valueMax="21.0" valueMin="18.0">
18– 
<specimenCount id="5C95FD59FFE2FF89FB33F8B718F6F8C0" box="[1205,1264,1831,1855]" pageId="81" pageNumber="82" type="generic">21 m</specimenCount>
