<number id="9B607708FFD8FFF6FCA3FF075C553AD2" box="[849,863,224,248]" pageId="4" pageNumber="150" value="1.0">1</number>
♀ (
<collectionCode id="4BB9FDEAFFD8FFF6FC76FF075CC33AD2" box="[900,969,224,248]" pageId="4" pageNumber="150">IEBR</collectionCode>
), 
<date id="591643EFFFD8FFF6FC2FFF385B5F3AD2" box="[989,1109,223,248]" pageId="4" pageNumber="150" value="2003-04-23">
<number id="9B607708FFD8FFF6FC2FFF385CF03AD2" box="[989,1018,223,248]" pageId="4" pageNumber="150" value="23.0">23</number>
