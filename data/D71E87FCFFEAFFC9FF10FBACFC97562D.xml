<paragraph id="5F0836EAFFEAFFC9FF10F90CFE46578D" blockId="12.[192,1440,1763,1883]" box="[192,448,1763,1787]" pageId="12" pageNumber="11">
<emphasis id="6DC3EAF8FFEAFFC9FF10F90CFEEC578D" bold="true" box="[192,362,1763,1787]" pageId="12" pageNumber="11">New records</emphasis>
