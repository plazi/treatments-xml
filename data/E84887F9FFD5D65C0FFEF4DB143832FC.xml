/ 
<emphasis id="5295EAFDFFD5D65B0D4CF49B164E3D95" box="[1798,1889,2935,2956]" pageId="3" pageNumber="694">German:</emphasis>
<vernacularName id="EEE246C1FFD5D65B0D21F49B19BB3D95" box="[1899,2196,2935,2956]" pageId="3" pageNumber="694">Ryukyu-Langfligelfledermaus</vernacularName>
/ 
<emphasis id="5295EAFDFFD5D65B02E3F49B182B3D95" box="[2217,2308,2935,2956]" pageId="3" pageNumber="694">Spanish:</emphasis>
<vernacularName id="EEE246C1FFD5D65B0344F49B18C53D95" box="[2318,2538,2935,2956]" pageId="3" pageNumber="694">Minioptero de Ryukyu</vernacularName>
<emphasis id="5295EAFDFFD5D65B0FFEF47217843DAA" box="[1460,1707,2974,2995]" pageId="3" pageNumber="694">Other common names:</emphasis>
<vernacularName id="EEE246C1FFD5D65B0CF9F47216E03DAA" box="[1715,1999,2974,2995]" pageId="3" pageNumber="694">Japanese Long-fingered Bat</vernacularName>
, 
<vernacularName id="EEE246C1FFD5D65B0D97F47219FA3DAA" box="[2013,2261,2974,2995]" pageId="3" pageNumber="694">Ryukyu Bent-winged Bat</vernacularName>
