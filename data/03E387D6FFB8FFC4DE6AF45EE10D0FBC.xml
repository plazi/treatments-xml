MHNG-INVE-60142, 
<specimenCount id="9D4CFD49FFB8FFC4D9FAF5CEE0000992" box="[1260,1392,993,1016]" pageId="20" pageNumber="77" type="generic" typeStatus="syntype">one syntype</specimenCount>
