from 
<taxonomicName id="4C624D664434787BFBB33F11FB47F963" authorityName="Gunther" authorityYear="1876" box="[1048,1155,1639,1664]" class="Actinopterygii" family="Carangidae" genus="Seriola" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="61" phylum="Chordata" rank="species" species="hippos">
