<taxonomicName id="4C624D664434787BFE863FF9FDD2F94B" authorityName="McIntosh" authorityYear="1934" box="[301,534,1679,1704]" class="Trematoda" family="Sanguinicolidae" genus="Paradeontacylix" kingdom="Animalia" order="Diplostomida" pageId="6" pageNumber="61" phylum="Platyhelminthes" rank="species" species="sanguinicoloides">
