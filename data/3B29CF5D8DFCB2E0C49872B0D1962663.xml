<taxonomicName id="29A31EA29C38AADC4C370140878B8583" LSID="http://zoobank.org/A0E3370C-6203-485E-8193-28BE25129337" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus siete" order="Hymenoptera" pageId="408" pageNumber="409" phylum="Arthropoda" rank="species" species="siete">Heterospilus siete Marsh</taxonomicName>
