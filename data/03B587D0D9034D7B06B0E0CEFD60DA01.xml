<taxonomicName id="4C1C4D45D9034D7B032AE1CAFE65DA01" authority="Gratshev et Zherikhin, 1995" authorityName="Gratshev et Zherikhin" authorityYear="1995" class="Insecta" family="Anthribidae" genus="Glaesotropis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="161" phylum="Arthropoda" rank="genus">
