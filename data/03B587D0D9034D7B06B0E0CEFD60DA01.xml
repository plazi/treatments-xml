<quantity id="4CE49B23D9034D7B044AE1EEFD74DA01" box="[621,711,1519,1546]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="19" pageNumber="161" unit="mm" value="3.4">3.4 mm</quantity>
