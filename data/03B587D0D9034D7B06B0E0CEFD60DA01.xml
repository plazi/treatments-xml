<taxonomicName id="4C1C4D45D9034D7B0453E1CAFD49D9EE" authorityName="Billberg" authorityYear="1820" box="[628,762,1483,1509]" class="Insecta" family="Anthribidae" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="161" phylum="Arthropoda" rank="family">Anthribidae</taxonomicName>
