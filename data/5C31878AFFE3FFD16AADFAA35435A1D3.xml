<th id="29795DA2FFE100356AA5FC21579AA7C6" box="[159,179,925,953]" gridcol="0" gridrow="7" pageId="27" pageNumber="28">p3</th>
<td id="29795DA2FFE100356ACDFC2156A3A7C6" box="[247,394,925,953]" gridcol="1" gridrow="7" pageId="27" pageNumber="28">32–47, 41</td>
<td id="29795DA2FFE100356BF9FC215542A7C6" box="[451,619,925,953]" gridcol="2" gridrow="7" pageId="27" pageNumber="28">0.54–0.61, 0.58</td>
<td id="29795DA2FFE1003568A8FC215455A7C6" box="[658,892,925,953]" gridcol="3" gridrow="7" pageId="27" pageNumber="28">3.39–3.98, 3.60</td>
<td id="29795DA2FFE100356980FC2153EDA7C6" box="[954,1220,925,953]" colspan="2" colspanRight="1" gridcol="4" gridrow="7" pageId="27" pageNumber="28">3.20–3.57, 3.36</td>
<td id="29795DA2FFE100356EE3FC215253A7C6" box="[1241,1402,925,953]" gridcol="6" gridrow="7" pageId="27" pageNumber="28">3.4–4.8, 4.4</td>
