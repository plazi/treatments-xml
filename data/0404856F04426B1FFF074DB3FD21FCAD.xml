<taxonomicName id="4BAD4FFA04436B1FFECB4E0CFDA0FDF0" box="[347,582,600,625]" class="Insecta" family="Encyrtidae" genus="Adelencyrtus" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="noyesi">
