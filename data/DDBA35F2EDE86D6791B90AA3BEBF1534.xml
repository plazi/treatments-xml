<taxonomicName id="27535C0E09FE855B79071B77A6AF16D2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:238796" class="Insecta" family="Formicidae" genus="Vollenhovia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Vollenhovia pwidikidika Clouse" order="Hymenoptera" pageNumber="17" phylum="Arthropoda" rank="species" species="pwidikidika">Vollenhovia pwidikidika</taxonomicName>
<taxonomicNameLabel id="B9F736FA2F7B10BB443874542DEA7599" rank="species">n. sp.</taxonomicNameLabel>
