<taxonomicName id="11A8B73D2DED00E3E54B7BF33C0E3BF4" authority="Panzer, 1801" authorityName="Panzer" authorityYear="1801" class="Insecta" family="Chrysididae" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Omalus aeneus" order="Hymenoptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="aeneus">Omalus aeneus</taxonomicName>
