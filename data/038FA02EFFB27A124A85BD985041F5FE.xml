<taxonomicName id="4C266ABBFFB27A124B5BBE8E55A3F5FE" box="[329,467,1689,1712]" class="Heterotardigrada" family="Hypsibiidae" genus="Platicrista" kingdom="Animalia" order="Parachela" pageId="46" pageNumber="47" phylum="Tardigrada" rank="species" species="angustata">
