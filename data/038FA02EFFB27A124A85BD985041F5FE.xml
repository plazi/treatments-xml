<taxonomicName id="4C266ABBFFB27A1248D7BE8E574BF5FE" box="[709,827,1689,1712]" class="Heterotardigrada" family="Hypsibiidae" genus="Platicrista" kingdom="Animalia" order="Parachela" pageId="46" pageNumber="47" phylum="Tardigrada" rank="species" species="ramsayi">
