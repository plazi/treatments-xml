<emphasis id="B92BEAA9FFB67E38C887F8B9BC57BDB8" bold="true" box="[151,549,1907,1933]" pageId="7" pageNumber="158">
Genus 
<taxonomicName id="4C5F4D38FFB67E38C8FEF8B9BC57BDB8" authority="Bolivar, 1887" authorityName="Bolivar" authorityYear="1887" box="[238,549,1907,1933]" class="Insecta" family="Tetrigidae" genus="Mazarredia" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="158" phylum="Arthropoda" rank="genus">
