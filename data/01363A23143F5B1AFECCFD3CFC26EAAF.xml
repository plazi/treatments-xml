<paragraph id="89208B35143F5B1DFECCFD53FD95EA9F" blockId="66.[264,844,691,758]" box="[264,557,732,758]" pageId="66" pageNumber="67">
(
<figureCitation id="11A497B0143F5B1DFED5FD53FE0FEA9F" box="[273,439,732,758]" captionStart="FIGURES 101 - 108" captionStartId="68.[264,382,1708,1731]" captionTargetBox="[281,1306,594,1686]" captionTargetId="figure@68.[281,1309,594,1686]" captionTargetPageId="68" captionText="FIGURES 101 - 108. Metamasius species. 101 - 102. Metamasius richdeboeri Anderson. 101. Dorsal habitus. 102. Lateral habitus. 103 - 104. Metamasius shchepaneki Anderson. 103. Dorsal habitus. 104. Lateral habitus. 105 - 106. Metamasius vaurieae Anderson. 105. Dorsal habitus. 106. Lateral habitus. 107 - 108. Metamasius wolfensohni Anderson. 107. Dorsal habitus. 108. Lateral habitus." httpUri="https://zenodo.org/record/168667/files/figure.png" pageId="66" pageNumber="67">
Figs. 
<number id="3F579912143F5B1DFE97FD53FE0FEA9F" box="[339,439,732,758]" pageId="66" pageNumber="67" value="51105.0" valueMax="102109.0" valueMin="101.0">101­102</number>
</figureCitation>
, 
<figureCitation id="11A497B0143F5B1DFE07FD53FD9DEA9F" box="[451,549,732,758]" captionStart="FIGURES 109 ­ 114" captionStartId="69.[264,383,1305,1329]" captionTargetBox="[276,1310,769,1276]" captionTargetId="figure@69.[275,1311,769,1278]" captionTargetPageId="69" captionText="FIGURES 109 ­ 114. Metamasius richdeboeri Anderson. 109. Lateral view of head, male. 110. Lateral view of head, female. 111. Antenna. 112. Hind leg, male. 113. Pygidium, male. 114. Pygidium, female." httpUri="https://zenodo.org/record/156057/files/figure.png" pageId="66" pageNumber="67">
109­ 
<number id="3F579912143F5B1DFE3CFD53FD9DEA9F" box="[504,549,732,758]" pageId="66" pageNumber="67" value="114.0">114</number>
</figureCitation>
