<taxonomicName id="D140B89FFB18ACDD8F30CB10E043C7B2" authority="" authorityName="" class="Insecta" family="Aphididae" genus="P." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="P. interrupta" order="Hemiptera" pageId="0" pageNumber="5447" phylum="Arthropoda" rank="species" species="interrupta">P. interrupta</taxonomicName>
(
<bibRefCitation id="B87FA073C92D669846CBFA9E10681190" author="Prout, L. B." journalOrPublisher="Novitates Zoologicae Tring" pageId="0" pageNumber="5447" pagination="1 - 77" title="New genera and species of indo-australian Geometridae." volume="23" year="1916">Prout 1916</bibRefCitation>
) 
<taxonomicName id="74F34F16E3B487F3D72E592FA2F564DC" authority="" authorityName="" class="Insecta" family="Aphididae" genus="P." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="P. phanoides" order="Hemiptera" pageId="0" pageNumber="5447" phylum="Arthropoda" rank="species" species="phanoides">P. phanoides</taxonomicName>
(
<bibRefCitation id="C42636223BBEEC9C5C1E0438B5E87B4B" author="Debauche, H." journalOrPublisher="Memoires du Musee Histoire Naturelle Belgique Brussels" pageId="0" pageNumber="5447" pagination="1 - 46" title="Geometridae de Celebes (Lepidoptera Heterocora)" volume="2" year="1941">Debauche 1941</bibRefCitation>
) and 
<taxonomicName id="5D697E9B135D8F630A908C76E646521B" authority="" authorityName="" class="Insecta" family="Aphididae" genus="P." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="P. praelustris" order="Hemiptera" pageId="0" pageNumber="5447" phylum="Arthropoda" rank="species" species="praelustris">P. praelustris</taxonomicName>
(
<bibRefCitation id="C0D64F02ED7586241112D6F43A63E52A" author="Prout, L. B." journalOrPublisher="Stylops London" pageId="0" pageNumber="5447" pagination="25 - 27" title="New Geometridae (Lep.) from Buru." volume="2" year="1933">Prout 1933</bibRefCitation>
