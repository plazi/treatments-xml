<taxonomicName id="4C0974AE6401FFB4FE06F852FDA450B1" authorityName=": Thomas" authorityYear="1906" box="[478,613,1988,2011]" class="Mammalia" family="Leporidae" genus="Lepus" kingdom="Animalia" order="Lagomorpha" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="coreanus">
