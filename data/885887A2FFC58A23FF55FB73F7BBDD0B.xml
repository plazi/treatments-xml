<taxonomicName id="C7F14D37FFC58A23FBB5F6AFFA03D056" authorityName="Peters" authorityYear="1878" box="[1145,1414,2318,2357]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="10" pageNumber="289" phylum="Chordata" rank="species" species="hildebrandtii">
<emphasis id="3285EAA6FFC58A23FBB5F6AFFA03D056" box="[1145,1414,2318,2357]" italics="true" pageId="10" pageNumber="289">R hildebrandtii</emphasis>
