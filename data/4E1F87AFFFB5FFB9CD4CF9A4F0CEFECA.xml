<taxonomicName id="01B64D3AFFB7FFB7CDC2FB3BF1EBFB68" authority="BoloAvar" authorityName="BoloAvar" authorityYear="1899" box="[458,664,1151,1176]" class="Insecta" family="Tettigoniidae" genus="Platycleis" kingdom="Animalia" order="Orthoptera" pageId="23" pageNumber="218" phylum="Arthropoda" rank="species" species="taurica">
