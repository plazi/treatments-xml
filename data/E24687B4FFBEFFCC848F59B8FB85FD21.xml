' N
</geoCoordinate>
, 
<geoCoordinate id="0FDB5065FF83FFCC80965E5EFAFFFE99" box="[1166,1285,272,298]" direction="west" orientation="longitude" pageId="34" pageNumber="35" precision="925" value="-99.666664">
<number id="DC272485FF83FFCC80965E5EFB57FE99" box="[1166,1197,272,298]" pageId="34" pageNumber="35" value="99.0">99</number>
