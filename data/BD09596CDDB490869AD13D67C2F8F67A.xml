4. 
<taxonomicName id="A1342E2B23E4698CBFF93637F21479AE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145193" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" lsidName-HNS="Tetramorium (Xiphomyrmex) schaufussii Forel" order="Hymenoptera" pageNumber="158" phylum="Arthropoda" rank="species" species="schaufussii" status="n. sp." subGenus="XIPHOMYRMEX">TETRAMORIUM (XIPHOMYRMEX) SCHAUFUSSII</taxonomicName>
