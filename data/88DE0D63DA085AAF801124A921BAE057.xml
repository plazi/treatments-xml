<taxonomicName id="691BFD8C6D22345A9EA5DC7F1FC4011A" authority="Zhao &amp; Li, 2020" authorityName="Zhao &amp; Li" authorityYear="2020" class="Arachnida" family="Telemidae" genus="Zhuanlema" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zhuanlema" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="genus">
