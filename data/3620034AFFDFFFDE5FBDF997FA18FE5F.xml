<taxonomicName id="7989C9DFFFDEFFD15BBEFE77FACDFE7C" authority="Hancock &amp; Wicksten, 2018" authorityName="Hancock &amp; Wicksten" authorityYear="2018" box="[1172,1353,415,436]" class="Malacostraca" family="Haustoriidae" genus="Haustorius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="23" pageNumber="124" phylum="Arthropoda" rank="species" species="galvezi" status="sp. nov.">
