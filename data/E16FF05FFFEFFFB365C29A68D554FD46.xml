Central 
<collectingCountry id="11D101D9FFE9FFB364959BDDD46AFD46" box="[448,545,692,716]" name="United States of America" pageId="4" pageNumber="83">America</collectingCountry>
(
<collectingCountry id="11D101D9FFE9FFB3677A9BDDD4E6FD46" box="[559,685,692,716]" name="Guatemala" pageId="4" pageNumber="83">Guatemala</collectingCountry>
, 
<collectingCountry id="11D101D9FFE9FFB367ED9BDDD558FD46" box="[696,787,692,716]" name="Mexico" pageId="4" pageNumber="83">Mexico</collectingCountry>
).
</paragraph>
