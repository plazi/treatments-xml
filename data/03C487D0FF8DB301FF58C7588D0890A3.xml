<taxonomicName id="4C6D4D45FF8DB301FA4BC7DF8D049194" authorityName="Boheman" authorityYear="1840" class="Insecta" family="Curculionidae" genus="Naupactus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="332" phylum="Arthropoda" rank="species" species="sellatus">
