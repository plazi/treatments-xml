<taxonomicName id="4C6D4D45FF8DB301FEF5C7DF8E1491F5" authority="Germar" authorityName="Germar" box="[314,516,669,691]" class="Insecta" family="Curculionidae" genus="Naupactus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="332" phylum="Arthropoda" rank="species" species="obliquus">
