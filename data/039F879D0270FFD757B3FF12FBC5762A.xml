<emphasis id="B942EA990270FFD757E3FE16FE6074DA" bold="true" box="[199,433,403,429]" pageId="10" pageNumber="111">Material examined.</emphasis>
[
<collectionCode id="ED27AE4E0270FFD7569BFE17FDCF74DA" box="[447,542,402,429]" pageId="10" pageNumber="111">NHMD</collectionCode>
]: Large sample of males and females from: 
<collectingCountry id="F321761B0270FFD7532DFE11FB8474D9" box="[1033,1109,404,430]" name="France" pageId="10" pageNumber="111">France</collectingCountry>
(incl. 
<collectingCountry id="F321761B0270FFD753BAFE16FB2974D9" box="[1182,1272,403,430]" name="France" pageId="10" pageNumber="111">Corsica</collectingCountry>
), 
<collectingCountry id="F321761B0270FFD7522FFE16FA8B74DA" box="[1291,1370,403,429]" name="Greece" pageId="10" pageNumber="111">Greece</collectingCountry>
(incl. 
<collectingRegion id="49F2F8690270FFD757B3FE32FF0974A6" box="[151,216,439,465]" country="Greece" name="Kriti" pageId="10" pageNumber="111">Crete</collectingRegion>
), 
<collectingCountry id="F321761B0270FFD757CFFE32FE9C74A6" box="[235,333,439,465]" name="Portugal" pageId="10" pageNumber="111">Portugal</collectingCountry>
, 
<collectingCountry id="F321761B0270FFD7567CFE32FE4974A6" box="[344,408,439,465]" name="Spain" pageId="10" pageNumber="111">Spain</collectingCountry>
