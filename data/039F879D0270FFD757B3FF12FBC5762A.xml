<collectingCountry id="F321761B0270FFD7557CFE32FD5A74A6" box="[600,651,439,465]" name="Italy" pageId="10" pageNumber="111">Italy</collectingCountry>
(
<collectingRegion id="49F2F8690270FFD755BEFE32FD3074A5" box="[666,737,439,466]" country="Italy" name="Sicilia" pageId="10" pageNumber="111">Sicily</collectingRegion>
