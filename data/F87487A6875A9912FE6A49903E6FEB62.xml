<materialsCitation id="C0B53CED875A9912FF6149E03C8AEA52" box="[212,983,1020,1045]" location="Yunnan Province" pageId="10" pageNumber="2561" specimenCount="1" specimenCount-egg="1" stateProvince="Yunnan">
