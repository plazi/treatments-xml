<number id="163524D6FF82FFB9FE8EFE79A478FE27" box="[261,275,428,452]" pageId="9" pageNumber="596" value="2.0">2</number>
K, 
<number id="163524D6FF82FFB9FEBEFE79A428FE27" box="[309,323,428,452]" pageId="9" pageNumber="596" value="2.0">2</number>
L, 
<number id="163524D6FF82FFB9FEE8FE79A41BFE27" box="[355,368,428,452]" pageId="9" pageNumber="596" value="3.0">3</number>
