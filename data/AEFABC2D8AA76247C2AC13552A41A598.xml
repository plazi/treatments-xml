<paragraph id="261E8016F6565D30A8CB7521F71E90A1" pageId="0" pageNumber="23">
<taxonomicName id="EE7CA15EF421039FD66D1DD31439EC3A" LSID="http://zoobank.org/A6F8BDA7-F19C-4529-808F-AB51C1C10B16" authority="Song &amp; Li, 2019" authorityName="Song &amp; Li" authorityYear="2019" class="Insecta" family="Cicadellidae" genus="Undulivena" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Undulivena" order="Hemiptera" pageId="0" pageNumber="23" phylum="Arthropoda" rank="genus">Undulivena</taxonomicName>
