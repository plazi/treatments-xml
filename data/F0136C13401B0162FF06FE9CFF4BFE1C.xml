<quantity id="BF4270E04019016DFF43FB21FEA7FB1C" box="[201,260,1220,1242]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="55" pageNumber="56" unit="mm" value="1.0">1 mm</quantity>
