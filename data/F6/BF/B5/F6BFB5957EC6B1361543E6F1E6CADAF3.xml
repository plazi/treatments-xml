<paragraph pageId="103" pageNumber="104">
<taxonomicName authorityName="Tryon" authorityYear="1867" class="Gastropoda" family="Bulimulidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="103" pageNumber="104" phylum="Mollusca" rank="family">Bulimulidae</taxonomicName>
, 
<taxonomicName baseAuthorityName="d'Orbigny" baseAuthorityYear="1835" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="103" pageNumber="104" phylum="Mollusca" rank="species" species="torallyi">
<emphasis italics="true" pageId="103" pageNumber="104">Bostryx torallyi</emphasis>
</taxonomicName>
(
