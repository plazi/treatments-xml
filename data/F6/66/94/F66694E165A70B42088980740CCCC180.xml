<subSubSection pageId="10" pageNumber="11" type="nomenclature">
<paragraph pageId="10" pageNumber="11">
<taxonomicName LSID="urn:lsid:zoobank.org:act:D3B0B038-D4A8-41E8-803A-0D2CFE97A8CE" class="Insecta" family="Aphididae" genus="Aphidura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphidura pakistanensis" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="pakistanensis">Aphidura pakistanensis</taxonomicName>
