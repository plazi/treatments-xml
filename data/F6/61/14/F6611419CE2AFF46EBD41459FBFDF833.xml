(
<bibRefCitation author="Ehrenberg" box="[453,664,1655,1681]" pageId="10" pageNumber="60" refString="Ehrenberg, C. G. (1853) Diagnoses novarum formarum. Monatsberichte der Koniglich Preussischen Akademie der Wissenshaften zu Berlin, Berlin, 8, 526 - 533." type="journal article" year="1853">
