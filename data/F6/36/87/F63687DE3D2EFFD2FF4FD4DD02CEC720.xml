<subSubSection box="[199,1142,630,655]" pageId="12" pageNumber="313" type="etymology">
<paragraph blockId="12.[151,1437,526,1314]" box="[199,1142,630,655]" pageId="12" pageNumber="313">
<emphasis bold="true" box="[199,334,630,655]" pageId="12" pageNumber="313">Etymology.</emphasis>
