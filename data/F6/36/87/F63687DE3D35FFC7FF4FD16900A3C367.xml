<emphasis bold="true" box="[151,308,1110,1135]" pageId="23" pageNumber="324">Type species:</emphasis>
<taxonomicName authority="Bustamante &amp; Ruiz, 2017" authorityName="Bustamante &amp; Ruiz" authorityYear="2017" box="[315,533,1110,1135]" class="Arachnida" family="Salticidae" genus="Tartamura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="324" phylum="Arthropoda" rank="species" species="agatelin" status="sp. nov.">
<emphasis box="[315,533,1110,1135]" italics="true" pageId="23" pageNumber="324">Tartamura agatelin</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[540,629,1110,1135]" pageId="23" pageNumber="324" rank="species">
