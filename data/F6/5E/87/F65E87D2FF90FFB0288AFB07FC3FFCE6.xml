<number box="[411,425,188,212]" pageId="9" pageNumber="491" value="2.0">2</number>
a
</emphasis>
, 
<emphasis box="[453,481,188,212]" italics="true" pageId="9" pageNumber="491">
<number box="[453,467,188,212]" pageId="9" pageNumber="491" value="3.0">3</number>
a
