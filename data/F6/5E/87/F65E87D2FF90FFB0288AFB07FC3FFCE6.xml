b
</emphasis>
, 
<emphasis box="[1359,1385,152,176]" italics="true" pageId="9" pageNumber="491">
<number box="[1359,1373,152,176]" pageId="9" pageNumber="491" value="3.0">3</number>
c
</emphasis>
, 
<emphasis box="[1401,1429,152,176]" italics="true" pageId="9" pageNumber="491">
