<taxonomicName authorityName="Wainstein" authorityYear="1958" box="[1043,1144,188,213]" class="Arachnida" family="Tetranychidae" genus="Tetranychus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="15" pageNumber="336" phylum="Arthropoda" rank="species" species="similis">
