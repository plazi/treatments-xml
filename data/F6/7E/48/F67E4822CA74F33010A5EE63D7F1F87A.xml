<taxonomicName authorityName="sensu Wainstein" authorityYear="1960" box="[564,692,934,959]" class="Arachnida" family="Tetranychidae" genus="Georgiobia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="324" phylum="Arthropoda" rank="genus">
