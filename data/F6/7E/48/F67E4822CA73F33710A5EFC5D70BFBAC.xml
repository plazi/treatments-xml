<collectingRegion box="[791,874,1039,1064]" country="Ukraine" name="Crimea" pageId="4" pageNumber="325">Crimea</collectingRegion>
associated with 
<taxonomicName box="[1093,1283,1039,1064]" class="Magnoliopsida" family="Rubiaceae" genus="Galium" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="4" pageNumber="325" phylum="Magnoliopsida" rank="species" species="aparine">
<emphasis box="[1093,1283,1039,1064]" italics="true" pageId="4" pageNumber="325">Galium aparine</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="" authorityYear="1789" box="[1308,1428,1039,1064]" class="Magnoliopsida" family="Rubiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="4" pageNumber="325" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
) (
<bibRefCitation author="Mitrofanov, V. I. &amp; Strunkova, Z. I. &amp; Livshits, I. Z." box="[158,438,1076,1101]" pageId="4" pageNumber="325" refId="ref9948" refString="Mitrofanov, V. I., Strunkova, Z. I. &amp; Livshits, I. Z. (1987) Keys to the tetranychid mites (Tetranychidae, Bryobiidae) fauna of the USSR and adjacent countries. Donish, Dushanbe, 224 pp. [in Russian]" type="book" year="1987">
Mitrofanov 
<emphasis box="[299,352,1076,1101]" italics="true" pageId="4" pageNumber="325">et al</emphasis>
., 1987
</bibRefCitation>
