<taxonomicName authorityName="Ohaus" authorityYear="1905" class="Insecta" family="Scarabaeidae" genus="Pseudochlorota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudochlorota" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
