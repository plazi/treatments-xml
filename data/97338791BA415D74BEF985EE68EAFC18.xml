BL 
<number id="A95224A0BA475D74BFC084976AA9FC19" box="[430,481,908,933]" pageId="5" pageNumber="448" value="17.5">17.5</number>
, HW 
<number id="A95224A0BA475D74BC4D8497697BFC18" box="[547,563,908,932]" pageId="5" pageNumber="448" value="4.0">4</number>
, PL 
<number id="A95224A0BA475D74BC09849769C4FC19" box="[615,652,908,933]" pageId="5" pageNumber="448" value="2.5">2.5</number>
, PW 
<number id="A95224A0BA475D74BCA784976991FC18" box="[713,729,908,932]" pageId="5" pageNumber="448" value="4.0">4</number>
, FWL 
<number id="A95224A0BA475D74BD498497687FFC19" box="[807,823,908,933]" pageId="5" pageNumber="448" value="6.0">6</number>
, HFL 
<number id="A95224A0BA475D74BD11849768D6FC18" box="[895,926,908,932]" pageId="5" pageNumber="448" value="10.0">10</number>
.
</paragraph>
