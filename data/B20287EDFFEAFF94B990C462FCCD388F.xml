<number id="8C6324DCFFEAFF94BDD6C5D2FB0D3B13" box="[1233,1268,1014,1038]" pageId="14" pageNumber="15" value="7.2">7.2</number>
+ 
<number id="8C6324DCFFEAFF94BC15C5D2FACE3B13" box="[1298,1335,1014,1038]" pageId="14" pageNumber="15" value="1.3">1.3</number>
