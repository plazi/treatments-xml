2003
</bibRefCitation>
; 
<location id="8EFFE186FF993D0D3CF0E39AFCE3BBE0" LSID="urn:lsid:plazi:treatment:0389064BFF983D0D3EEEE097FB74B904:8EFFE186FF993D0D3CF0E39AFCE3BBE0" box="[649,814,223,248]" country="Mauritius" county="Caribbean" municipality="Xisha Island" name="Sesoko Island" pageId="99" pageNumber="100" stateProvince="Southern">Sesoko Island</location>
, 
