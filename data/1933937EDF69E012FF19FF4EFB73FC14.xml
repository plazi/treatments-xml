, 
<collectingCountry id="E98D62F8DF69E012FE42FCA8FDCDFC4C" box="[460,539,881,904]" name="France" pageId="21" pageNumber="422">France</collectingCountry>
, 
<collectingCountry id="E98D62F8DF69E012FDA8FCB6FD8CFC43" box="[550,602,879,903]" name="Italy" pageId="21" pageNumber="422">Italy</collectingCountry>
