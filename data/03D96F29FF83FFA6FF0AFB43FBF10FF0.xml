, 
<collectingRegion id="49B410DDFF83FFA6FDA3FA21FF600BCC" country="Canada" name="British Columbia" pageId="38" pageNumber="483">British Columbia</collectingRegion>
, to 
<location id="8EAF88E4FF83FFA6FF35FA07FE8B0BCC" LSID="urn:lsid:plazi:treatment:03D96F29FF83FFA6FF0AFB43FBF10FF0:8EAF88E4FF83FFA6FF35FA07FE8B0BCC" box="[223,344,1467,1489]" municipality="North of Banks Island" name="San Diego" pageId="38" pageNumber="483" stateProvince="British Columbia">San Diego</location>
