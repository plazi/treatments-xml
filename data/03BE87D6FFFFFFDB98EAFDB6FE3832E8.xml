<quantity id="4CEF9B25FFFFFFDB9D68FD7EFFDD32A2" box="[1301,1436,652,679]" metricMagnitude="2" metricUnit="m" metricValue="5.805" metricValueMax="11.51" metricValueMin="0.1" pageId="110" pageNumber="111" unit="m" value="580.5" valueMax="1151.0" valueMin="10.0">10–1,151 m</quantity>
