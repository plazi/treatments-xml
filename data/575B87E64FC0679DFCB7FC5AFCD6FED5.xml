<typeStatus id="004988524FC0679CFD50FBADFCC9FBB9" box="[728,786,1053,1077]" pageId="172" pageNumber="173">TYPE</typeStatus>
SPECIES: 
<taxonomicName id="18F24D734FC0679CFC1FFBAEFB51FBBF" authority="Platnick &amp; Baehr, 2006" authorityName="Platnick &amp; Baehr" authorityYear="2006" box="[919,1162,1054,1077]" class="Arachnida" family="Gnaphosidae" genus="Wesmaldra" kingdom="Animalia" order="Araneae" pageId="172" pageNumber="173" phylum="Arthropoda" rank="species" species="bidgemia" status="new species">
<emphasis id="ED86EAE24FC0679CFC1FFBAEFB51FBBF" box="[919,1162,1054,1077]" italics="true" pageId="172" pageNumber="173">Wesmaldra bidgemia</emphasis>
