<collectingCountry id="F36D760CB746FFF3661A7CB1DFCCFE06" box="[846,956,387,413]" name="Bermuda" pageId="23" pageNumber="24">Bermuda</collectingCountry>
Islands?, 
<collectingCountry id="F36D760CB746FFF3616E7CB1D8F8FE06" box="[1082,1160,387,413]" name="Brazil" pageId="23" pageNumber="24">Brazil</collectingCountry>
?, 
<collectingCountry id="F36D760CB746FFF361F77CB1D975FE06" box="[1187,1285,387,413]" name="Panama" pageId="23" pageNumber="24">Panama</collectingCountry>
, 
<collectingCountry id="F36D760CB746FFF360477CB1D902FE06" box="[1299,1394,387,413]" name="Mexico" pageId="23" pageNumber="24">Mexico</collectingCountry>
, 
<collectingCountry id="F36D760CB746FFF365E97C9ADD55FE59" box="[189,293,424,450]" name="Uruguay" pageId="23" pageNumber="24">Uruguay</collectingCountry>
, 
