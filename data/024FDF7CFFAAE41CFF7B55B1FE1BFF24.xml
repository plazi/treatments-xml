species: 
<taxonomicName id="4DE615E9FFAAE41BFED955F9FD28FCE5" authority="Kieffer, 1921" authorityName="Kieffer" authorityYear="1921" box="[309,692,762,788]" class="Insecta" family="Chironomidae" genus="Kribiodosis" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="563" phylum="Arthropoda" rank="species" species="fasciata">
<emphasis id="B892B278FFAAE41BFED955F9FD84FCE5" box="[309,536,762,788]" italics="true" pageId="3" pageNumber="563">Kribiodosis fasciata</emphasis>
<bibRefCitation id="EE77139BFFAAE41BFDF355F9FD28FCE5" author="Kieffer, J. J." box="[543,692,762,788]" pageId="3" pageNumber="563" pagination="1 - 56" refId="ref5440" refString="Kieffer, J. J. (1921) Chironomides de l'Afrique Equatoriale (1 e partie). Annales de la Societe entomologique de France, 90, 1 - 56." type="journal article" year="1921">Kieffer, 1921</bibRefCitation>
