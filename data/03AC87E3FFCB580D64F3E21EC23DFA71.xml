<emphasis id="B971EAE7FFCD580D64F3E449C0FFFCFA" bold="true" box="[140,251,772,794]" pageId="85" pageNumber="86">Figure 62.</emphasis>
<taxonomicName id="4C054D76FFCD580D657DE449C24EFCF9" baseAuthorityName="Banks" baseAuthorityYear="1924" box="[258,586,772,794]" class="Insecta" family="Chrysopidae" genus="Chrysopodes" kingdom="Animalia" order="Neuroptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="nigripilosus" subGenus="Neosuarius">
