<collectingCountry id="11C9A9D9C6201B15FF3CFB7CFE10FBAF" box="[151,405,1114,1138]" name="Philippines Singapore" pageId="38" pageNumber="39">Philippines Singapore</collectingCountry>
, 
<collectingCountry id="11C9A9D9C6201B15FE0EFB7CFD90FBAF" box="[421,533,1114,1138]" name="Indonesia" pageId="38" pageNumber="39">Indonesia</collectingCountry>
, 
<collectingCountry id="11C9A9D9C6201B15FD8EFB7CFD0EFBAF" box="[549,651,1114,1138]" name="Australia" pageId="38" pageNumber="39">Australia</collectingCountry>
(
<collectingRegion id="AB1A27ABC6201B15FD34FB7CFCACFBAF" box="[671,809,1114,1138]" country="Australia" name="Queensland" pageId="38" pageNumber="39">Queensland</collectingRegion>
), 
<collectingCountry id="11C9A9D9C6201B15FCEAFB7DFC72FBAF" box="[833,1015,1114,1138]" name="New Caledonia" pageId="38" pageNumber="39">New Caledonia</collectingCountry>
