<taxonomicName id="6649E677472E8507AC7EFCF5247311BE" class="Insecta" family="Staphylinidae" genus="Mocyta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mocyta" order="Coleoptera" pageId="15" pageNumber="64" phylum="Arthropoda" rank="genus">Mocyta</taxonomicName>
