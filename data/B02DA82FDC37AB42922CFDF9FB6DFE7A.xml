<emphasis id="0AF0C52BDC30AB4290A4FE0FFB6DFE7A" box="[929,1224,412,438]" italics="true" pageId="13" pageNumber="14">
, 
<taxonomicName id="FF8462BADC30AB4290A8FE0FFB88FE7A" baseAuthorityName="Timberlake" baseAuthorityYear="1980" box="[941,1069,412,438]" class="Insecta" family="Apidae" genus="Anthophorula" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="consobrina">consobrina</taxonomicName>
, 
<taxonomicName id="FF8462BADC30AB42973FFE0EFB24FE7A" baseAuthorityName="Cockerell" baseAuthorityYear="1915" box="[1082,1153,413,438]" class="Insecta" family="Apidae" genus="Anthophorula" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="nitens">nitens</taxonomicName>
, 
<taxonomicName id="FF8462BADC30AB42978BFE0FFB6DFE7A" baseAuthorityName="Cockerell" baseAuthorityYear="1897" box="[1166,1224,412,438]" class="Insecta" family="Apidae" genus="Anthophorula" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="sidae">sidae</taxonomicName>
</emphasis>
</keyLead>
</paragraph>
