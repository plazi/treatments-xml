, coll. P.P. Soysouvan, on 
<taxonomicName id="46DD42D9B65CFFFEFAD2FA1C073FFA07" box="[1321,1396,1465,1489]" class="Magnoliopsida" family="Rubiaceae" genus="Coffea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="genus">
<emphasis id="B3A9E548B65CFFFEFAD2FA1C073FFA07" box="[1321,1396,1465,1489]" italics="true" pageId="47" pageNumber="48">Coffea</emphasis>
</taxonomicName>
sp. (
<taxonomicName id="46DD42D9B65CFFFEFF65FA780352FA23" authorityName="" authorityYear="1789" box="[158,281,1501,1525]" class="Magnoliopsida" family="Rubiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
