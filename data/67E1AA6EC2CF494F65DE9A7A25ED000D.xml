<taxonomicName id="17EEEBF779C8AC4C9BB6E3B9A90A9665" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Anthemis" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="549" phylum="Tracheophyta" rank="species" species="arvensis">
