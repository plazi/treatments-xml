<taxonomicName id="04B211ABC947FF8C5CBCF922FB871CC7" baseAuthorityName="Forsskal" baseAuthorityYear="1775" box="[948,1134,1757,1781]" class="Actinopterygii" family="Mugilidae" genus="Valamugil" kingdom="Animalia" order="Mugiliformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="seheli">
