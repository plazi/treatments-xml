<paragraph id="8B9F369CFFCC2130DA0BF972FB9A655E" blockId="65.[826,1467,1532,1895]" pageId="65" pageNumber="2847">
<taxonomicName id="4C204D1FFFCC2130DA0BF972FC62649B" box="[828,973,1764,1788]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="daqingensis">
