1.60 (1.30); HW = 1.25 
<emphasis id="BAAEEAC6FFDFFFACC89A115D0FAC7E25" bold="true" box="[465,480,1631,1657]" pageId="11" pageNumber="12">–</emphasis>
1.95 (1.56); EL = 0.70 
<emphasis id="BAAEEAC6FFDFFFACCBAC115D0CBA7E25" bold="true" box="[743,758,1631,1657]" pageId="11" pageNumber="12">–</emphasis>
1.10 (0.86); EW = 0.50 
<emphasis id="BAAEEAC6FFDFFFACCD4C115D0A5A7E25" bold="true" box="[1031,1046,1631,1657]" pageId="11" pageNumber="12">–</emphasis>
