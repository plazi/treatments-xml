<typeStatus id="4C523FE1FF8DFFF3FF51F8D1FE97F8E4" box="[197,261,1857,1883]" pageId="7" pageNumber="64">Type</typeStatus>
material.
</emphasis>
<typeStatus id="4C523FE1FF8DFFF3FE07F8D2FD9AF8E3" box="[403,520,1858,1884]" pageId="7" pageNumber="64">Holotype</typeStatus>
male, 
<collectingCountry id="EBFEC1D3FF8DFFF3FDF4F8D2FD5AF8E3" box="[608,712,1858,1884]" name="China" pageId="7" pageNumber="64">CHINA</collectingCountry>
: Yunnan, Xishuangbanna, Jinghong, 
<quantity id="54112CA6FF8DFFF3FB34F8D2FB61F8E3" box="[1184,1267,1858,1884]" metricMagnitude="2" metricUnit="m" metricValue="5.7" pageId="7" pageNumber="64" unit="m" value="570.0">570 m</quantity>
, 
<date id="E757A783FF8DFFF3FA96F8D2FA0AF8E3" box="[1282,1432,1858,1884]" pageId="7" pageNumber="64" value="2001-11-04">
