<treatmentCitation id="584310B20121370676E5FF0DED92056F" author="Haliday, A. H." box="[151,452,250,274]" page="268" pageId="17" pageNumber="18" year="1833">
<taxonomicName id="1EE24D200121370676E5FF0DED92056F" authority="Haliday, 1833 a: 268" authorityName="Haliday" authorityPageNumber="268" authorityYear="1833" box="[151,452,250,274]" class="Insecta" family="Mymaridae" genus="Anaphes" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis id="EB96EAB10121370676E5FF0DECB8056F" box="[151,238,250,274]" italics="true" pageId="17" pageNumber="18">Anaphes</emphasis>
<bibRefCitation id="BD734B52012137067687FF0DED92056F" author="Haliday, A. H." box="[245,452,250,274]" pageId="17" pageNumber="18" pagination="259 - 276" refId="ref50013" refString="Haliday, A. H. (1833 a) Essay on the classification of the parasitic Hymenoptera of Britain, which correspond with the Ichneumones minuti of Linnaeus. Entomological Magazine, 1 (3), 259 - 276. https: // www. biodiversitylibrary. org / page / 25334028" type="journal article" year="1833">Haliday, 1833a: 268</bibRefCitation>
</taxonomicName>
</treatmentCitation>
, 
<treatmentCitation id="584310B20121370677BDFF0DEE13056F" author="Haliday, A. H." box="[463,581,250,274]" page="346" pageId="17" pageNumber="18" year="1833">
<bibRefCitation id="BD734B520121370677BDFF0DEE13056F" author="Haliday, A. H." box="[463,581,250,274]" pageId="17" pageNumber="18" pagination="333 - 350" refId="ref50067" refString="Haliday, A. H. (1833 b) An essay on the classification of the parasitic Hymenoptera, etc. (continued). Entomological Magazine, 1 (4), 333 - 350. https: // www. biodiversitylibrary. org / page / 25334028" type="journal article" year="1833">1833b: 346</bibRefCitation>
</treatmentCitation>
. 
<typeStatus id="0659880101213706743CFF0CEED6056E" box="[590,640,251,275]" pageId="17" pageNumber="18">Type</typeStatus>
species: 
<taxonomicName id="1EE24D200121370674ACFF0DE813056F" authority="Haliday, 1833" authorityName="Haliday" authorityYear="1833" box="[734,1093,250,274]" class="Insecta" family="Mymaridae" genus="Anaphes" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="fuscipennis">
