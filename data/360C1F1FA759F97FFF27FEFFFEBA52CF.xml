<taxonomicName id="79A5D58AA759F97FFEAFFE52FD8751ED" authority="Kramer, 1886: 255" authorityName="Kramer" authorityPageNumber="255" authorityYear="1886" box="[287,583,457,479]" class="Arachnida" family="Urodinychidae" genus="Dinychus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="6" pageNumber="353" phylum="Arthropoda" rank="genus">
<emphasis id="8CD1721BA759F97FFEAFFE52FEBF51ED" box="[287,383,457,479]" italics="true" pageId="6" pageNumber="353">Dinychus</emphasis>
<bibRefCitation id="DA34D3F8A759F97FFE35FE51FDD451ED" author="Kramer" box="[389,532,457,479]" pageId="6" pageNumber="365" refString="Kramer, P. (1886) Ueber Milben. Archiv fur Naturgeschichte, 52, 241 - 268 + pl. XII." type="journal article" year="1886">
