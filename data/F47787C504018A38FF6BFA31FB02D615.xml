</taxonomicName>
; B— 
<taxonomicName id="BBDE4D5004008A3BFBCEFAC5FB39D65D" box="[1074,1188,1307,1329]" class="Arachnida" family="Araneidae" genus="Larinioides" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="ixobolus">
<emphasis id="4EAAEAC104008A3BFBCEFAC5FB39D65D" box="[1074,1188,1307,1329]" italics="true" pageId="9" pageNumber="70">L. ixobolus</emphasis>
</taxonomicName>
; C— 
<taxonomicName id="BBDE4D5004008A3BFB20FAC5FACED65D" box="[1244,1363,1308,1329]" class="Arachnida" family="Araneidae" genus="Larinioides" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="sericatus">
<emphasis id="4EAAEAC104008A3BFB20FAC5FACED65D" box="[1244,1363,1308,1329]" italics="true" pageId="9" pageNumber="70">L. sericatus</emphasis>
</taxonomicName>
, arrow pointed to the spur from different view; D— 
<taxonomicName id="BBDE4D5004008A3BFDA5FAE2FD43D63C" box="[601,734,1339,1360]" class="Arachnida" family="Araneidae" genus="Larinioides" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="patagiatus">
<emphasis id="4EAAEAC104008A3BFDA5FAE2FD43D63C" box="[601,734,1339,1360]" italics="true" pageId="9" pageNumber="70">L. patagiatus</emphasis>
</taxonomicName>
; E— 
<taxonomicName id="BBDE4D5004008A3BFCE8FAE2FC09D63C" box="[788,916,1338,1360]" class="Arachnida" family="Araneidae" genus="Larinioides" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="chabarovi">
<emphasis id="4EAAEAC104008A3BFCE8FAE2FC09D63C" box="[788,916,1338,1360]" italics="true" pageId="9" pageNumber="70">L. chabarovi</emphasis>
</taxonomicName>
; F— 
<taxonomicName id="BBDE4D5004008A3BFC36FAE2FBA0D63C" box="[970,1085,1339,1360]" class="Arachnida" family="Araneidae" genus="Larinioides" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="cornutus">
<emphasis id="4EAAEAC104008A3BFC36FAE2FBA0D63C" box="[970,1085,1339,1360]" italics="true" pageId="9" pageNumber="70">L. cornutus</emphasis>
</taxonomicName>
; G— 
<taxonomicName id="BBDE4D5004008A3BFB8AFAE2FB4FD63C" box="[1142,1234,1338,1360]" class="Arachnida" family="Araneidae" genus="Araneus" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="folium">
<emphasis id="4EAAEAC104008A3BFB8AFAE2FB4FD63C" box="[1142,1234,1338,1360]" italics="true" pageId="9" pageNumber="70">L. folium</emphasis>
</taxonomicName>
.
</paragraph>
