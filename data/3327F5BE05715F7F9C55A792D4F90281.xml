<materialsCitation id="17B8DBADCF1D746D50A45A66874E35C7" ID-GBIF-Occurrence="3063651310" collectingDate="2021-01-01" collectingDateMax="2021-12-31" collectingDateMin="2021-01-01" collectionCode="R" specimenCode="R5" specimenCount="1">
<specimenCode id="BB1217790C9792B8A583495697C8A310" collectionCode="R" country="Chile" name="Departamento de Geologia, Universidad de Chile">R5</specimenCode>
