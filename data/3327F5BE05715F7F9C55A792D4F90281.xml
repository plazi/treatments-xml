<taxonomicName id="614141662F6946C8B448BC846E3490EE" authorityName="Shelford" authorityYear="1910" class="Insecta" family="Blaberidae" genus="Morphna" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Morphna" order="Blattodea" pageId="0" pageNumber="127" phylum="Arthropoda" rank="genus">
<emphasis id="B7D0F895874F69CFC2E415365479D1E6" italics="true" pageId="0" pageNumber="127">Morphna</emphasis>
</taxonomicName>
Shelford, 
<taxonomicName id="5BC65ACC2E37A86ED64A6B4412FF590B" authorityName="Kirby" authorityYear="1903" class="Insecta" family="Blaberidae" genus="Pseudophoraspis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Pseudophoraspis" order="Blattodea" pageId="0" pageNumber="127" phylum="Arthropoda" rank="genus">
<emphasis id="738DBBB4B5E19163A2084CEA83A2021F" italics="true" pageId="0" pageNumber="127">Pseudophoraspis</emphasis>
</taxonomicName>
Kirby, 
<taxonomicName id="57493605778A7C20DA437F04E5DC4E39" authorityName="Kirby" authorityYear="1903" class="Insecta" family="Blaberidae" genus="Rhabdoblatta" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Rhabdoblatta" order="Blattodea" pageId="0" pageNumber="127" phylum="Arthropoda" rank="genus">
<emphasis id="909D6E2CB298FE064C73CF78540DB98A" italics="true" pageId="0" pageNumber="127">Rhabdoblatta</emphasis>
</taxonomicName>
Kirby, and 
<taxonomicName id="A0981CB8BC6752122DD0710071911D76" authorityName="Hanitsch" authorityYear="1930" class="Insecta" family="Blaberidae" genus="Stictolampra" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Stictolampra" order="Blattodea" pageId="0" pageNumber="127" phylum="Arthropoda" rank="genus">
