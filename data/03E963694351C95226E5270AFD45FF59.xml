<taxonomicName id="4C40A9FC4351C95324F5272CFC75F8EC" authority="Zhiltzova, 1956" authorityName="Zhiltzova" authorityYear="1956" box="[647,1001,1844,1869]" class="Insecta" family="Perlodidae" genus="Isoperla" kingdom="Animalia" order="Plecoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="rhododendri">
