<quantity id="4CEF9B25FF1FFF3B9A09FD95F8933287" box="[628,722,615,642]" metricMagnitude="3" metricUnit="m" metricValue="2.036" pageId="142" pageNumber="143" unit="m" value="2036.0">2,036 m</quantity>
