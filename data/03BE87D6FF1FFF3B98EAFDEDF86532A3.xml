<quantity id="4CEF9B25FF1FFF3B9D67FDD2FF29323E" box="[1306,1384,544,571]" metricMagnitude="-2" metricUnit="m" metricValue="2.7" pageId="142" pageNumber="143" unit="cm" value="2.7">2.7 cm</quantity>
