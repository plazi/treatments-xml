</specimenCount>
): PW/PL = 
<number box="[690,796,533,556]" pageId="2" pageNumber="292" value="1.69" valueMax="1.8" valueMin="1.58">1.58–1.80</number>
(mean 
<number box="[884,935,533,556]" pageId="2" pageNumber="292" value="1.7">1.70</number>
); EL/EW = 
<number box="[1073,1180,533,556]" pageId="2" pageNumber="292" value="1.355" valueMax="1.43" valueMin="1.28">1.28–1.43</number>
(mean 
<number box="[1266,1316,533,556]" pageId="2" pageNumber="292" value="1.36">1.36</number>
); EL/PL = 
<number box="[151,257,568,591]" pageId="2" pageNumber="292" value="2.46" valueMax="2.62" valueMin="2.3">2.30–2.62</number>
(mean 
<number box="[339,390,568,591]" pageId="2" pageNumber="292" value="2.46">2.46</number>
); EW/PW = 
<number box="[528,634,568,591]" pageId="2" pageNumber="292" value="1.0699999999999998" valueMax="1.13" valueMin="1.01">1.01–1.13</number>
(mean 
<number box="[716,767,568,591]" pageId="2" pageNumber="292" value="1.06">1.06</number>
); HWmax/PW = 
<number box="[954,1061,568,591]" pageId="2" pageNumber="292" value="0.825" valueMax="0.86" valueMin="0.79">0.79–0.86</number>
(mean 
<number box="[1142,1193,568,591]" pageId="2" pageNumber="292" value="0.82">0.82</number>
); HWmin/PW = 
<number pageId="2" pageNumber="292" value="0.755" valueMax="0.8" valueMin="0.71">0.71– 0.80</number>
(mean 
<number box="[277,328,602,625]" pageId="2" pageNumber="292" value="0.76">0.76</number>
); HWmax/HWmin = 
<number box="[559,666,602,625]" pageId="2" pageNumber="292" value="1.085" valueMax="1.14" valueMin="1.03">1.03–1.14</number>
(mean 
