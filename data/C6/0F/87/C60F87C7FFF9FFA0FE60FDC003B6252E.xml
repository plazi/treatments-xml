<subSubSection pageId="5" pageNumber="6" type="diagnosis">
<paragraph blockId="5.[189,1399,750,1105]" box="[189,310,750,776]" pageId="5" pageNumber="6">
<heading bold="true" box="[189,310,750,776]" fontSize="11" level="2" pageId="5" pageNumber="6" reason="6">
