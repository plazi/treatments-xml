<emphasis box="[581,599,790,813]" italics="true" pageId="15" pageNumber="3964">„</emphasis>
, 
<collectionCode box="[618,699,790,814]" pageId="15" pageNumber="3964">SCAU</collectionCode>
; 4 
<emphasis box="[730,748,790,813]" italics="true" pageId="15" pageNumber="3964">„</emphasis>
, 
