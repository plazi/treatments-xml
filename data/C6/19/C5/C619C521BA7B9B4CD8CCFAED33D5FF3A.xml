<subSubSection box="[151,505,1291,1317]" pageId="12" pageNumber="513" type="nomenclature">
<paragraph blockId="12.[151,505,1291,1317]" box="[151,505,1291,1317]" pageId="12" pageNumber="513">
<heading bold="true" box="[151,505,1291,1317]" fontSize="11" level="1" pageId="12" pageNumber="513" reason="1">
<taxonomicName authority="Bussau, 1993" authorityName="Bussau" authorityYear="1993" box="[151,505,1291,1317]" class="Adenophorea" family="Xyalidae" genus="Manganonema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monhysterida" pageId="12" pageNumber="513" phylum="Nematoda" rank="genus">
