<taxonomicName authority="Jeannel, 1916" authorityName="Jeannel" authorityYear="1916" class="Insecta" family="Reduviidae" genus="Pachysandalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="208" phylum="Arthropoda" rank="species" species="orientalis">
