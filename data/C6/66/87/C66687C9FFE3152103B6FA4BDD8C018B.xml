<taxonomicName baseAuthorityName="Stal" baseAuthorityYear="1855" box="[510,606,1664,1690]" class="Insecta" family="Reduviidae" genus="Lestomerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="220" phylum="Arthropoda" rank="species" species="ochripes">
