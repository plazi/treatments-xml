<taxonomicName authority="Berlese, 1884: 5" authorityName="Berlese" authorityPageNumber="5" authorityYear="1884" box="[226,644,587,612]" class="Arachnida" family="Uropodidae" genus="Discopoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="venusta">
