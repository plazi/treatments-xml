and M 
<subScript attach="left" box="[391,399,242,256]" fontSize="6" pageId="5" pageNumber="356">1</subScript>
. 
<collectionCode box="[415,434,225,251]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="5" pageNumber="356">R</collectionCode>
<subScript attach="left" box="[434,459,242,256]" fontSize="6" pageId="5" pageNumber="356">2+3</subScript>
