<quantity box="[1224,1362,1653,1681]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" metricValueMax="5.7" metricValueMin="4.5" pageId="11" pageNumber="362" unit="mm" value="5.1" valueMax="5.7" valueMin="4.5">4.5–5.7 mm</quantity>
, CuA 
<subScript attach="left" box="[1428,1436,1672,1686]" fontSize="6" pageId="11" pageNumber="362">2</subScript>
<quantity box="[151,289,1689,1717]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" metricValueMax="4.5" metricValueMin="3.3" pageId="11" pageNumber="362" unit="mm" value="3.9" valueMax="4.5" valueMin="3.3">3.3–4.5 mm</quantity>
.
</paragraph>
