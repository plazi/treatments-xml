<location LSID="urn:lsid:plazi:treatment:C61E7211FFC64C40FF4030D7332EB739:4B6895DCFFC64C41FEEB338D33BAB3B2" box="[316,402,738,764]" country="Ukraine" name="Podolia" pageId="70" pageNumber="71">Podolia</location>
