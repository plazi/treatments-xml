, 
<collectingCountry box="[390,495,1161,1187]" name="Germany" pageId="73" pageNumber="74">Germany</collectingCountry>
, 
<collectingCountry box="[508,589,1161,1187]" name="Poland" pageId="73" pageNumber="74">Poland</collectingCountry>
, 
<collectingCountry box="[598,683,1161,1187]" name="Austria" pageId="73" pageNumber="74">Austria</collectingCountry>
, 
<collectingCountry box="[696,877,1161,1187]" name="Czech Republic" pageId="73" pageNumber="74">Czech Republic</collectingCountry>
, 
<collectingCountry box="[889,990,1161,1187]" name="Slovakia" pageId="73" pageNumber="74">Slovakia</collectingCountry>
, 
<collectingCountry box="[1002,1101,1161,1187]" name="Hungary" pageId="73" pageNumber="74">Hungary</collectingCountry>
