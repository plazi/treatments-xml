<taxonomicName authority="Angelov" authorityName="Angelov. One" authorityYear="1978" box="[364,591,1721,1747]" class="Insecta" family="Curculionidae" genus="Cionus" kingdom="Animalia" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="galanus">
