<paragraph blockId="45.[151,1437,1404,2043]" pageId="45" pageNumber="46">
<emphasis bold="true" box="[199,339,1440,1467]" pageId="45" pageNumber="46">Type series.</emphasis>
<materialsCitation ID-GBIF-Occurrence="2273923470" collectionCode="NHMW" collectorName="In &amp; Wingelmuller" determinerName="Wingelmull." pageId="45" pageNumber="46" specimenCount="5" specimenCount-female="2" specimenCount-male="3" typeStatus="syntype">
