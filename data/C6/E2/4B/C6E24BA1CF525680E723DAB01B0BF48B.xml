<subSubSection pageId="null" pageNumber="790" type="nomenclature">
<paragraph pageId="null" pageNumber="790">
<taxonomicName authority="L." class="Magnoliopsida" family="Caryophyllaceae" genus="Saponaria" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="790" phylum="Tracheophyta" rank="species" species="ocymoides">
