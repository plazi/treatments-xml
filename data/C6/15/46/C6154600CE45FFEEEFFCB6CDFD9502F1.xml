<typeStatus box="[151,200,225,246]" pageId="1" pageNumber="54">Type</typeStatus>
species: 
<taxonomicName authority="Kleine, 1920" authorityName="Kleine" authorityYear="1920" box="[294,723,224,246]" class="Insecta" family="Brentidae" genus="Myrmecobrenthus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="54" phylum="Arthropoda" rank="species" species="wasmanni">
