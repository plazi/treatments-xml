.VI 
<number box="[1074,1134,786,810]" pageId="5" pageNumber="6" value="1961.0">1961</number>
</date>
); Irkutsk (
<number box="[1258,1292,786,810]" pageId="5" pageNumber="6" value="13.0">13</number>
, no date, V. Jakowlew [leg.]); Tibel´ti [Slydyanskij rajon, Irkutskaja oblast] (
<number box="[919,934,822,846]" pageId="5" pageNumber="6" value="2.0">2</number>
Ƥ, 
<date box="[975,1235,822,846]" pageId="5" pageNumber="6" value="1963-07-02" valueMax="1963-07-04" valueMin="1963-07-02">
