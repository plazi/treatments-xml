<taxonomicName box="[1256,1428,499,523]" class="Insecta" family="Hymenoptera" genus="Dolerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="subfasciatus">
<emphasis box="[1256,1428,499,523]" italics="true" pageId="4" pageNumber="5">D. subfasciatus</emphasis>
</taxonomicName>
. A 
<typeStatus box="[178,265,534,558]" pageId="4" pageNumber="5">syntype</typeStatus>
male of 
<taxonomicName box="[364,558,533,557]" class="Insecta" family="Hymenoptera" genus="Dolerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="pseudoanticus">
<emphasis box="[364,558,533,557]" italics="true" pageId="4" pageNumber="5">D. pseudoanticus</emphasis>
</taxonomicName>
