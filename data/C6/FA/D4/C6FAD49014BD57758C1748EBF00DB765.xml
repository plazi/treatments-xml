<paragraph pageId="0" pageNumber="1">
<taxonomicName LSID="C6FAD490-14BD-5775-8C17-48EBF00DB765" authority="Stop-Bowitz, 1948" authorityName="Stop-Bowitz" authorityYear="1948" class="Polychaeta" family="Capitellidae" genus="Ophelina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ophelina abranchiata" order="Capitellida" pageId="0" pageNumber="1" phylum="Annelida" rank="species" species="abranchiata">
