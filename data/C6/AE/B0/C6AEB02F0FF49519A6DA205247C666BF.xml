<subSubSection pageId="null" pageNumber="488" type="reference_group">
<paragraph pageId="null" pageNumber="488">
(
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Doronicum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="488" phylum="Tracheophyta" rank="species" species="cordatum">
