, 
<taxonomicName box="[314,412,897,921]" class="Amphibia" family="Plethodontidae" genus="Bolitoglossa" kingdom="Animalia" order="Caudata" pageId="11" pageNumber="80" phylum="Chordata" rank="species" species="pandi">
