<collectingCountry box="[616,691,1344,1368]" name="Russia" pageId="12" pageNumber="39">Russia</collectingCountry>
and 
<collectingCountry box="[749,882,1344,1368]" name="Kazakhstan" pageId="12" pageNumber="39">Kazakhstan</collectingCountry>
