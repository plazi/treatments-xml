<subSubSection pageId="0" pageNumber="27" type="taxonomic history">
<paragraph pageId="0" pageNumber="27">Taxonomic history.</paragraph>
<paragraph pageId="0" pageNumber="27">
<taxonomicName class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crematogaster flaviventris" order="Hymenoptera" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="flaviventris">
<emphasis italics="true" pageId="0" pageNumber="27">Crematogaster flaviventris</emphasis>
</taxonomicName>
Santschi, 1910: 370 (w, q, m) Democratic Republic of Congo.
</paragraph>
<paragraph pageId="0" pageNumber="27">
Subspecies of 
<taxonomicName genus="C." lsidName="C. inversa" pageId="0" pageNumber="27" rank="species" species="inversa">
<emphasis italics="true" pageId="0" pageNumber="27">C. inversa</emphasis>
</taxonomicName>
