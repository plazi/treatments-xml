, EW 
<number box="[1005,1057,439,464]" pageId="28" pageNumber="543" value="0.5">0.50</number>
, OL 
<number box="[1125,1239,440,464]" pageId="28" pageNumber="543" value="0.225" valueMax="0.23" valueMin="0.22">0.22–0.23</number>
, OES 
<number box="[1323,1375,440,464]" pageId="28" pageNumber="543" value="0.13">0.13</number>
, WL 
<number box="[151,266,476,501]" pageId="28" pageNumber="543" value="2.465" valueMax="2.5" valueMin="2.43">2.43–2.50</number>
