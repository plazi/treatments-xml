<paragraph blockId="28.[151,1437,151,1616]" pageId="28" pageNumber="543">
Male: HL 
<number box="[328,442,440,464]" pageId="28" pageNumber="543" value="1.025" valueMax="1.07" valueMin="0.98">0.98–1.07</number>
, HW 
<number box="[520,635,439,464]" pageId="28" pageNumber="543" value="1.375" valueMax="1.45" valueMin="1.3">1.30–1.45</number>
, SL 
