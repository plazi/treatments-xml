<emphasis id="B9633218FFECFFEBFF74FA8BCC46FAB4" bold="true" box="[151,283,1330,1352]" pageId="1" pageNumber="272">
<typeStatus id="54AC50A8FFECFFEBFF74FA8BCD97FAB4" box="[151,202,1330,1352]" pageId="1" pageNumber="272">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="4C179589FFECFFEBFEC9FA8BCE44FAB4" authority="Sowerby, 1903" authorityName="Sowerby" authorityYear="1903" box="[298,793,1330,1352]" class="Gastropoda" family="Turridae" genus="Pleurotoma" higherTaxonomySource="CoL" kingdom="Animalia" order="Neogastropoda" pageId="1" pageNumber="272" phylum="Mollusca" rank="species" species="scitecostata" subGenus="Drillia">
