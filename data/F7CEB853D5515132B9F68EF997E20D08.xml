<taxonomicName id="4ACD2910940016BD8F71DBFEE479BE4E" LSID="F7CEB853-D551-5132-B9F6-8EF997E20D08" authority="Duftschmid, 1812" authorityName="Duftschmid" authorityYear="1812" class="Insecta" family="Carabidae" genus="Harpalus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Harpalus (Harpalus) distinguendus" order="Coleoptera" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="distinguendus" subGenus="Harpalus">Harpalus (Harpalus) distinguendus Duftschmid, 1812</taxonomicName>
