<taxonomicName id="00F766379A947CEDA3C6915E94A43101" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:37220" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium severini (Emery)" lsidName-HNS="Tetramorium severini (Emery)" order="Hymenoptera" phylum="Arthropoda" rank="species" species="severini">Tetramorium severini (Emery)</taxonomicName>
