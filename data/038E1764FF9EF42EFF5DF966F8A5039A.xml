<typeStatus id="549C18D0FF9DF42FFF0CFCD9FC3A016A" box="[198,259,786,812]" pageId="2" pageNumber="391">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C27DDF1FF9DF42FFEBAFCD9FF5C016D" box="[368,613,786,811]" class="Malacostraca" family="Alpheidae" genus="Bruceopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="391" phylum="Arthropoda" rank="species" species="projectus" status="sp. nov.">
<emphasis id="B9537A60FF9DF42FFEBAFCD9FF5C016D" box="[368,613,786,811]" italics="true" pageId="2" pageNumber="391">Bruceopsis projectus</emphasis>
</taxonomicName>
