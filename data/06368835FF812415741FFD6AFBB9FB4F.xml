<quantity id="496794C6FF812415742DFB95FEACFB21" box="[305,379,1139,1164]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="8" pageNumber="69" unit="mm" value="11.0">11 mm</quantity>
