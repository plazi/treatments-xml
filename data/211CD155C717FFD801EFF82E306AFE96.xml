<taxonomicName id="6EB51BC0C717FFDB01B7FA28340BF898" box="[207,334,1803,1828]" class="Amphibia" family="Hylidae" genus="Hyla" kingdom="Animalia" order="Anura" pageId="5" pageNumber="32" phylum="Chordata" rank="species" species="faber">
