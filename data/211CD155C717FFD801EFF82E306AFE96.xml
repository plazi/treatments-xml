</taxonomicName>
(
<taxonomicName id="6EB51BC0C717FFDB0027FA293487F898" box="[351,450,1802,1828]" class="Amphibia" family="Hylidae" kingdom="Animalia" order="Anura" pageId="5" pageNumber="32" phylum="Chordata" rank="family">Hylidae</taxonomicName>
