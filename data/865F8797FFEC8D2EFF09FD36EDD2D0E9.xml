<quantity id="C90E9B64FFEC8D2EFDBCFC68EC64D0CC" box="[519,598,958,980]" metricMagnitude="1" metricUnit="m" metricValue="5.0291999999999994" pageId="35" pageNumber="40" unit="in" value="1980.0">1980 in</quantity>
