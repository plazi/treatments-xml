<subSubSection id="C34B79F8FFBFFF82FF134958FC0FBBCC" box="[151,933,1642,1667]" pageId="10" pageNumber="11" type="distribution">
<paragraph id="8BEE2A73FFBFFF82FF134958FC0FBBCC" blockId="10.[151,1436,1462,1739]" box="[151,933,1642,1667]" pageId="10" pageNumber="11">
<emphasis id="B925F661FFBFFF82FF134958FE9ABBCD" bold="true" box="[151,304,1642,1667]" pageId="10" pageNumber="11">Distribution.</emphasis>
Eastern 
<collectingCountry id="F3466AE3FFBFFF82FE1D4958FE5ABBCD" box="[409,496,1642,1667]" name="Algeria" pageId="10" pageNumber="11">Algeria</collectingCountry>
, 
<collectingCountry id="F3466AE3FFBFFF82FE7F4958FDFBBBCD" box="[507,593,1642,1667]" name="Tunisia" pageId="10" pageNumber="11">Tunisia</collectingCountry>
