<taxonomicName id="4C4F4D0CFF9DFFEBF723F8982B5E7345" authority="(Fennah)" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Fennah" baseAuthorityYear="1956" box="[1142,1366,1893,1914]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="species" species="guamana">
