<taxonomicName id="F768EB87ACF1D93BA4FD9B1FCADA550C" authorityName="Sharp" authorityYear="1882" class="Insecta" family="Hydrophilidae" genus="Chasmogenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chasmogenus" order="Coleoptera" pageId="0" pageNumber="25" phylum="Arthropoda" rank="genus">Chasmogenus</taxonomicName>
