<heading id="D0AB81BDF53FFFD8FF6CFC02DB4DCE66" bold="true" box="[151,780,1013,1039]" fontSize="11" level="1" pageId="3" pageNumber="454" reason="1">
<taxonomicName id="4C5C4D52F53FFFD8FF6CFC02DB4DCE66" authority="Piersig, 1896" authorityName="Piersig" authorityYear="1896" box="[151,780,1013,1039]" class="Arachnida" family="Sperchonidae" genus="Sperchon" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="454" phylum="Arthropoda" rank="species" species="clupeifer" subGenus="Hispidosperchon">
<emphasis id="B928EAC3F53FFFD8FF6CFC02DB4DCE66" bold="true" box="[151,780,1013,1039]" pageId="3" pageNumber="454">
<emphasis id="B928EAC3F53FFFD8FF6CFC02D94ACE66" bold="true" box="[151,267,1013,1039]" italics="true" pageId="3" pageNumber="454">Sperchon</emphasis>
(
<emphasis id="B928EAC3F53FFFD8FEE7FC02D9A8CE66" bold="true" box="[284,489,1013,1039]" italics="true" pageId="3" pageNumber="454">Hispidosperchon</emphasis>
) 
<emphasis id="B928EAC3F53FFFD8FE01FC02DA27CE66" bold="true" box="[506,614,1013,1039]" italics="true" pageId="3" pageNumber="454">clupeifer</emphasis>
<bibRefCitation id="EFCD4B20F53FFFD8FD97FC02DB4DCE66" author="Piersig" box="[620,780,1013,1039]" pageId="3" pageNumber="454" refString="Piersig, R. G. (1896 - 1899) Deutschlands Hydrachniden. Zoologica, 19, 601 pp. http: // dx. doi. org / 10.5962 / bhl. title. 14043" type="book" year="1896">Piersig, 1896</bibRefCitation>
</emphasis>
</taxonomicName>
