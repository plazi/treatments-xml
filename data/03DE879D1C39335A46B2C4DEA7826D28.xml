<taxonomicName id="4C774D081C39335A4795C121A16E6C5C" authority="Stechow, 1907" authorityName="Stechow" authorityYear="1907" box="[432,875,1096,1122]" class="Dothideomycetes" family="Venturiaceae" genus="Antennularia" kingdom="Animalia" order="Venturiales" pageId="40" pageNumber="452" phylum="Ascomycota" rank="species" species="dendritica">
