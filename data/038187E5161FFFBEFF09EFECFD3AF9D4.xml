<subSubSection id="C3326578161FFFBEFF59E8A6FD3AF9D4" pageId="23" pageNumber="24" type="discussion">
<paragraph id="8B9736F3161FFFBEFF59E8A6FD3AF9D4" blockId="23.[151,1437,793,1609]" pageId="23" pageNumber="24">
<emphasis id="B95CEAE1161FFFBEFF59E8A6FE8BFA40" bold="true" box="[199,315,1477,1501]" pageId="23" pageNumber="24">Remarks.</emphasis>
<taxonomicName id="4C284D70161FFFBEFEDDE8A6FDE8FA40" box="[323,600,1477,1501]" class="Insecta" family="Mutillidae" genus="Odontophotopsis" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="sonora">
