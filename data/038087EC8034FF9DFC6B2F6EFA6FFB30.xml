Fontes
</taxonomicName>
............... 
<taxonomicName id="4C294D798034FF9DFAC42C2EFA6CFDF0" authority="Kistner" authorityName="Kistner" authorityYear="1993" box="[1362,1493,570,595]" class="Insecta" family="Staphylinidae" genus="Fonsechellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="fontesi">
