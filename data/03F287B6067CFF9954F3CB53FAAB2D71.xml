<subSubSection id="C341652B067CFF9A54F3CBCDFBE32E84" box="[151,1103,1762,1784]" pageId="1" pageNumber="378" type="materials_examined">
<paragraph id="8BE436A0067CFF9A54F3CBCDFBE32E84" blockId="1.[151,1103,1732,1784]" box="[151,1103,1762,1784]" pageId="1" pageNumber="378">
Type species: 
<taxonomicName id="4C5B4D23067CFF9A5542CBCCFDD82E84" authority="Pascoe" authorityName="Pascoe" box="[294,628,1762,1784]" class="Insecta" family="Curculionidae" genus="Nyphaeba" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="378" phylum="Arthropoda" rank="species" species="monommoides">
