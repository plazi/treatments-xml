<taxonomicName id="4C1235C26118FFEDFBABFE0F7139C6B4" authority="Smith, 2008" authorityName="Smith" authorityYear="2008" box="[1096,1289,451,475]" class="Insecta" family="Melolonthidae" genus="Pusiodactylus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="genus" status="new genus">
