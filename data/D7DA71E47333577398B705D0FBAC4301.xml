, 
<taxonomicName id="38DC3CA766F454217C8450D8A973D399" baseAuthorityName="J.E.Gray" baseAuthorityYear="1828" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="30" pageNumber="31" phylum="Mollusca" rank="species" species="hennahi">
