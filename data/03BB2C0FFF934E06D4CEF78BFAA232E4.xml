</taxonomicName>
similar to 
<taxonomicName id="4C12E69AFF934E05D453F41EFD6A31F1" authorityName="Geis" authorityYear="1933" box="[538,732,1652,1678]" class="Ostracoda" family="Pachydomellidae" genus="Microcheilinella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
