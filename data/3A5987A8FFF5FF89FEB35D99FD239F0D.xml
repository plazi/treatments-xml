<taxonomicName id="75F04D3DFFF6FF88FE3D5E2BFE1F991B" authorityName="Mound &amp; Masumoto" authorityYear="2005" box="[390,484,1843,1864]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="martini">
