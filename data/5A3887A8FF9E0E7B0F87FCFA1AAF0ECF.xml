2019a); 
<taxonomicName id="15914D3DFF9E0E7B0FC9FB551A660D70" authority="Torres" authorityName="Torres" box="[216,312,1275,1299]" class="Liliopsida" family="Poaceae" genus="Nicoraella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="15" pageNumber="390" phylum="Tracheophyta" rank="genus">
<emphasis id="E0E5EAACFF9E0E7B0FC9FB551A660D70" box="[216,312,1275,1299]" italics="true" pageId="15" pageNumber="390">Nicoraella</emphasis>
</taxonomicName>
