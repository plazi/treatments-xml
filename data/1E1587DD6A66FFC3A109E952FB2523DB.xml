<collectingCountry id="EEAB765B6A66FFC3A54AE919FAD8239D" box="[1302,1355,1391,1412]" name="United States of America" pageId="4" pageNumber="264">USA</collectingCountry>
(
<collectingRegion id="5478F8296A66FFC3A505E919FC1023BD" country="United States of America" name="Washington" pageId="4" pageNumber="264">Washington</collectingRegion>
