<taxonomicName id="51BC4D486A66FFC3A405E927FCE2239D" authority="Newell, 1943" authorityName="Newell" authorityYear="1943" class="Arachnida" family="Sironidae" genus="Neosiro" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="4" pageNumber="264" phylum="Arthropoda" rank="species" species="kamiakensis">
<emphasis id="A4C8EAD96A66FFC3A405E927FAD7237F" box="[1113,1348,1361,1382]" italics="true" pageId="4" pageNumber="264">Neosiro kamiakensis</emphasis>
