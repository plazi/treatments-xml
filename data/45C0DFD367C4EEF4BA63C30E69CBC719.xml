<taxonomicName id="F743D93175B9437F74CF9F0F44A252DF" class="Insecta" family="Tettigoniidae" genus="Psorodonotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psorodonotus ebneri" order="Orthoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="ebneri">Psorodonotus ebneri</taxonomicName>
