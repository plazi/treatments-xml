<subSubSection id="9AB40C8AFFECDC5AFF01A74514B4F926" box="[151,419,1654,1680]" pageId="1" pageNumber="427" type="nomenclature">
<paragraph id="D2115F01FFECDC5AFF01A74514B4F926" blockId="1.[151,419,1654,1680]" box="[151,419,1654,1680]" pageId="1" pageNumber="427">
<heading id="8959E86DFFECDC5AFF01A74514B4F926" bold="true" box="[151,419,1654,1680]" fontSize="11" level="1" pageId="1" pageNumber="427" reason="1">
<emphasis id="E0DA8313FFECDC5AFF01A74514B4F926" bold="true" box="[151,419,1654,1680]" pageId="1" pageNumber="427">
<taxonomicName id="15AE2482FFECDC5AFF01A745145DF926" box="[151,330,1654,1680]" class="Maxillopoda" family="Jasmineiricolidae" genus="Jasmineiricola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poecilostomatoida" pageId="1" pageNumber="427" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="E0DA8313FFECDC5AFF01A745145DF926" bold="true" box="[151,330,1654,1680]" italics="true" pageId="1" pageNumber="427">Jasmineiricola</emphasis>
</taxonomicName>
<taxonomicNameLabel id="FBE93E68FFECDC5AFEC7A74514B4F926" box="[337,419,1654,1680]" pageId="1" pageNumber="427" rank="genus">n. gen.</taxonomicNameLabel>
