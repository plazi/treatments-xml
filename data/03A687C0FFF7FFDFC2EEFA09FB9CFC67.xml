<quantity id="4CF79B33FFF6FFDFC2ACFE69FE81FE28" box="[213,279,432,452]" metricMagnitude="3" metricUnit="m" metricValue="1.3199999999999998" pageId="25" pageNumber="180" unit="m" value="1320.0">1320m</quantity>
