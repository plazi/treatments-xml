<quantity id="4CF79B33FFF6FFDFC0EDFE9FFD43FEB6" box="[660,725,326,346]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="25" pageNumber="180" unit="m" value="1300.0">1300m</quantity>
