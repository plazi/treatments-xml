<taxonomicName id="4C6EDFA6FFF42F73EAD59203FDC6A1C5" authority="Dalman 1819" authorityName="Dalman" authorityYear="1819" box="[238,542,1405,1432]" class="Insecta" family="Hydroptilidae" genus="Hydroptila" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="497" phylum="Arthropoda" rank="genus">
