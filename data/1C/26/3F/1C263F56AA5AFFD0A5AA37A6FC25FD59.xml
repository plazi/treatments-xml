<taxonomicName authorityName="Rougemont" authorityYear="2000" class="Insecta" family="Staphylinidae" genus="Lesteva" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="huabeiensis">
