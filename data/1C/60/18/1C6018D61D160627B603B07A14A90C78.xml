<subSubSection pageId="10" pageNumber="179" type="preys">
<paragraph pageId="10" pageNumber="179">Preys.</paragraph>
<paragraph pageId="10" pageNumber="179">
<taxonomicName class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cacopsylla melanoneura" order="Hemiptera" pageId="10" pageNumber="179" phylum="Arthropoda" rank="species" species="melanoneura">Cacopsylla melanoneura</taxonomicName>
and 
<taxonomicName class="Insecta" family="Psyllidae" genus="Cacopsylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cacopsylla peregrina" order="Hemiptera" pageId="10" pageNumber="179" phylum="Arthropoda" rank="species" species="peregrina">Cacopsylla peregrina</taxonomicName>
, from 
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Crataegus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Crataegus monogyna" order="Rosales" pageId="10" pageNumber="179" phylum="Tracheophyta" rank="species" species="monogyna">Crataegus monogyna</taxonomicName>
