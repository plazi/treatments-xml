<paragraph blockId="13.[990,1265,1156,1180]" box="[990,1265,1156,1180]" pageId="13" pageNumber="14">
<heading box="[990,1265,1156,1180]" centered="true" fontSize="9" level="2" pageId="13" pageNumber="14" reason="2">
<taxonomicName authorityName="Enderlein" baseAuthorityName="Enderlein" baseAuthorityYear="1912" box="[990,1137,1157,1180]" class="Insecta" family="Phoridae" genus="Melaloncha" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="28" phylum="Arthropoda" rank="species" species="colossia">
