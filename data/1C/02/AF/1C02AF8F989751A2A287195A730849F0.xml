<geoCoordinate degrees="03" direction="south" minutes="01.528" orientation="latitude" precision="1" value="-3.0254667">03°01.528'S</geoCoordinate>
, 
<geoCoordinate degrees="38" direction="east" minutes="12.091" orientation="longitude" precision="1" value="38.201515">38°12.091'E</geoCoordinate>
