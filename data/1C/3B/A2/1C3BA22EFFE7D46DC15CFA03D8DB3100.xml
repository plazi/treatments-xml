<emphasis italics="true" pageId="4" pageNumber="93">
<taxonomicName baseAuthorityName="Miyosi" baseAuthorityYear="1947" box="[496,630,1957,1981]" class="Diplopoda" family="Polyxenidae" genus="Eudigraphis" kingdom="Animalia" order="Polyxenida" pageId="4" pageNumber="93" phylum="Arthropoda" rank="species" species="takakuwai">E. takakuwai</taxonomicName>
, 
<taxonomicName authorityName="Karasawa &amp; Kawano &amp; Fukaya &amp; Tsurusaki" authorityYear="2020" baseAuthorityName="Miyosi" baseAuthorityYear="1947" box="[642,764,1958,1981]" class="Diplopoda" family="Polyxenidae" genus="Eudigraphis" kingdom="Animalia" order="Polyxenida" pageId="4" pageNumber="93" phylum="Arthropoda" rank="species" species="nigricans">E. nigricans</taxonomicName>
, 
<taxonomicName authorityName="Karasawa &amp; Kawano &amp; Fukaya &amp; Tsurusaki" authorityYear="2020" baseAuthorityName=": Ishii" baseAuthorityYear="1950" box="[156,281,1990,2014]" class="Diplopoda" family="Polyxenidae" genus="Eudigraphis" kingdom="Animalia" order="Polyxenida" pageId="4" pageNumber="93" phylum="Arthropoda" rank="species" species="kinutensis">E. kinutensis</taxonomicName>
</emphasis>
, 
<taxonomicName authority="Ishii and Liang, 1990" authorityName="Ishii and Liang" authorityYear="1990" box="[293,616,1990,2014]" class="Diplopoda" family="Polyxenidae" genus="Eudigraphis" kingdom="Animalia" order="Polyxenida" pageId="4" pageNumber="93" phylum="Arthropoda" rank="species" species="sinensis">
<emphasis box="[293,395,1991,2014]" italics="true" pageId="4" pageNumber="93">E. sinensis</emphasis>
Ishii and Liang, 1990
</taxonomicName>
, 
<taxonomicName authority="Ishii, 1990" authorityName="Ishii" authorityYear="1990" class="Diplopoda" family="Polyxenidae" genus="Eudigraphis" kingdom="Animalia" order="Polyxenida" pageId="4" pageNumber="93" phylum="Arthropoda" rank="species" species="taiwanensis">
<emphasis box="[625,767,1991,2014]" italics="true" pageId="4" pageNumber="93">E. taiwanensis</emphasis>
Ishii, 1990
</taxonomicName>
, and 
<taxonomicName authority="Ishii and Yin, 2000" authorityName="Ishii and Yin" authorityYear="2000" box="[316,691,2022,2047]" class="Diplopoda" family="Polyxenidae" genus="Eudigraphis" kingdom="Animalia" order="Polyxenida" pageId="4" pageNumber="93" phylum="Arthropoda" rank="species" species="xishuangbanna">
<emphasis box="[316,494,2022,2046]" italics="true" pageId="4" pageNumber="93">E. xishuangbanna</emphasis>
Ishii and Yin, 2000
</taxonomicName>
.
