(
<bibRefCitation author="Berland L." box="[482,655,1208,1234]" firstAuthor="Berland" journalOrPublisher="Libraire des Sciences Naturelles, Paris" pageId="6" pageNumber="7" refId="ref20425" refString="Berland L. 1920. Voyage de Ch. Alluaud et R. Jeannel en Afrique Orientale (1911 - 1912). Araneae (2 e partie). Libraire des Sciences Naturelles, Paris." title="Voyage de Ch. Alluaud et R. Jeannel en Afrique Orientale (1911 - 1912). Araneae (2 e partie)" type="book chapter" year="1920">Berland, 1920</bibRefCitation>
)
</taxonomicName>
. Kenya (
<bibRefCitation author="Berland L." box="[767,932,1208,1234]" firstAuthor="Berland" journalOrPublisher="Libraire des Sciences Naturelles, Paris" pageId="6" pageNumber="7" refId="ref20425" refString="Berland L. 1920. Voyage de Ch. Alluaud et R. Jeannel en Afrique Orientale (1911 - 1912). Araneae (2 e partie). Libraire des Sciences Naturelles, Paris." title="Voyage de Ch. Alluaud et R. Jeannel en Afrique Orientale (1911 - 1912). Araneae (2 e partie)" type="book chapter" year="1920">Berland 1920</bibRefCitation>
