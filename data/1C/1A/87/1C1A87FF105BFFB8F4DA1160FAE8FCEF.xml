<taxonomicName box="[873,994,743,769]" class="Arachnida" family="Pholcidae" genus="Quamtana" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="17" phylum="Arthropoda" rank="species" species="kitahurira">Kitahurira</taxonomicName>
(
<geoCoordinate box="[1009,1092,744,770]" direction="south" orientation="latitude" pageId="2" pageNumber="3" precision="555" value="-0.97">
0.97° 
<collectionCode box="[1077,1092,744,770]" pageId="2" pageNumber="3">S</collectionCode>
</geoCoordinate>
, 
<geoCoordinate box="[1104,1203,743,769]" direction="east" orientation="longitude" pageId="2" pageNumber="3" precision="555" value="29.68">
29.68° 
<collectionCode box="[1184,1203,743,769]" pageId="2" pageNumber="3">E</collectionCode>
</geoCoordinate>
