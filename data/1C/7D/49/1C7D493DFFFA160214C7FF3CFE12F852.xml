<quantity box="[1015,1103,1483,1510]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="4" pageNumber="596" unit="mm" value="4.4">4.4 mm</quantity>
, EL 
<quantity box="[1155,1243,1483,1510]" metricMagnitude="-3" metricUnit="m" metricValue="9.4" pageId="4" pageNumber="596" unit="mm" value="9.4">9.4 mm</quantity>
