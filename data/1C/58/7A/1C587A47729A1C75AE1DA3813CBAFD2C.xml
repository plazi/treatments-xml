<subSubSection pageId="16" pageNumber="133" type="multiple">
<paragraph pageId="16" pageNumber="133">Taxon classification Animalia Lepidoptera Crambidae</paragraph>
</subSubSection>
<subSubSection pageId="16" pageNumber="133" type="nomenclature">
<paragraph pageId="16" pageNumber="133">
<taxonomicName LSID="http://zoobank.org/FECB8543-F50F-4CB3-A3D7-5C9ABAA3D9CC" class="Insecta" family="Crambidae" genus="Leucinodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leucinodes malawiensis" order="Lepidoptera" pageId="16" pageNumber="133" phylum="Arthropoda" rank="species" species="malawiensis">Leucinodes malawiensis</taxonomicName>
