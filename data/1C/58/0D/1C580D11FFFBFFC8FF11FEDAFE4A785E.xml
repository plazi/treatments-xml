<paragraph blockId="14.[136,1059,344,597]" box="[136,277,344,368]" pageId="14" pageNumber="221">
<taxonomicName box="[136,225,344,368]" class="Arachnida" family="Hersiliidae" genus="Hersilia" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="221" phylum="Arthropoda" rank="genus">
