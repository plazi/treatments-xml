</figureCitation>
); 
<number box="[424,452,1853,1877]" pageId="10" pageNumber="11" value="19.0">19</number>
localities between 
<number box="[667,709,1853,1877]" pageId="10" pageNumber="11" value="890.0">890</number>
and 
<quantity box="[764,849,1853,1877]" metricMagnitude="3" metricUnit="m" metricValue="3.432" pageId="10" pageNumber="11" unit="m" value="3432.0">3432 m</quantity>
