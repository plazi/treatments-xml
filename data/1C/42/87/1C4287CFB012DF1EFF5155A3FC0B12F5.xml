<number box="[1368,1399,1356,1380]" pageId="10" pageNumber="11" value="18.0">18</number>
</figureCitation>
); 
<number box="[1422,1436,1356,1380]" pageId="10" pageNumber="11" value="8.0">8</number>
localities between 
<date box="[359,525,1391,1415]" pageId="10" pageNumber="11" value="1423" valueMax="2659">
<number box="[359,415,1391,1415]" pageId="10" pageNumber="11" value="1423.0">1423</number>
