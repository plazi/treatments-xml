</figureCitation>
); 
<number box="[506,534,1519,1543]" pageId="18" pageNumber="19" value="37.0">37</number>
localities between 
<number box="[749,791,1519,1543]" pageId="18" pageNumber="19" value="719.0">719</number>
and 
<quantity box="[846,930,1519,1544]" metricMagnitude="3" metricUnit="m" metricValue="3.432" pageId="18" pageNumber="19" unit="m" value="3432.0">3432 m</quantity>
