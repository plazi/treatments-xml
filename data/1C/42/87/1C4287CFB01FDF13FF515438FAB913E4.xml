<number box="[617,632,1006,1030]" pageId="7" pageNumber="8" value="8.0">8</number>
</figureCitation>
); 
<number box="[655,683,1006,1030]" pageId="7" pageNumber="8" value="23.0">23</number>
localities between 
<date box="[898,1064,1006,1030]" pageId="7" pageNumber="8" value="1423" valueMax="2923">
<number box="[898,954,1006,1030]" pageId="7" pageNumber="8" value="1423.0">1423</number>
and 
<quantity box="[1008,1064,1006,1030]" metricMagnitude="3" metricUnit="m" metricValue="2.923" pageId="7" pageNumber="8" unit="m" value="2923.0">2923</quantity>
</date>
