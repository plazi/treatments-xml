<number box="[1146,1177,874,898]" pageId="17" pageNumber="18" value="36.0">36</number>
</figureCitation>
); 
<number box="[1199,1227,874,898]" pageId="17" pageNumber="18" value="25.0">25</number>
localities between 
<date box="[151,315,910,934]" pageId="17" pageNumber="18" value="1113" valueMax="2526">
<number box="[151,205,910,934]" pageId="17" pageNumber="18" value="1113.0">1113</number>
and 
<quantity box="[259,315,910,934]" metricMagnitude="3" metricUnit="m" metricValue="2.526" pageId="17" pageNumber="18" unit="m" value="2526.0">2526</quantity>
</date>
