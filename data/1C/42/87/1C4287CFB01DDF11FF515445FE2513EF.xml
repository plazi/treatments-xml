<number box="[1167,1183,1033,1057]" pageId="5" pageNumber="6" value="5.0">5</number>
</figureCitation>
); 
<number box="[1208,1222,1033,1057]" pageId="5" pageNumber="6" value="6.0">6</number>
localities between 
<number box="[151,207,1069,1093]" pageId="5" pageNumber="6" value="2604.0">2604</number>
and 
<quantity box="[261,346,1069,1093]" metricMagnitude="3" metricUnit="m" metricValue="3.5239999999999996" pageId="5" pageNumber="6" unit="m" value="3524.0">3524 m</quantity>
