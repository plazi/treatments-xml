<taxonomicName id="4C0E4D5AFF9F8570BE9DFCD90EA1FC68" authority="Felt, 1904" authorityName="Felt" authorityYear="1904" box="[913,1016,877,901]" class="Insecta" family="Culicidae" genus="Culicella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="32" phylum="Arthropoda" rank="genus">
