. 
<taxonomicName id="4C524D64244B2525FC5FFF6FFAAAFF2C" authority="Stebbing, 1912" authorityName="Stebbing" authorityYear="1912" box="[948,1373,202,226]" class="Malacostraca" family="Diastylidae" genus="Makrokylindrus" kingdom="Animalia" order="Cumacea" pageId="46" pageNumber="46" phylum="Arthropoda" rank="species" species="fragilis">
