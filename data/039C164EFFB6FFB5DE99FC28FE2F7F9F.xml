<taxonomicName id="4C35DCDBFFB6FFBADE99FC28FC6B7B4A" box="[683,1009,319,342]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="maculipennis" status="new species">
<emphasis id="B9417B4AFFB6FFBADE99FC28FC6B7B4A" bold="true" box="[683,1009,319,342]" italics="true" pageId="55" pageNumber="56">Paramblynotus maculipennis</emphasis>
</taxonomicName>
