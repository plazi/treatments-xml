inland of beach, Playa de Sardinia, 
<collectingCountry id="F31A765495500845E4F0FEF2ECEAFE85" box="[1174,1214,295,321]" name="Iceland" pageId="3" pageNumber="4">Isla</collectingCountry>
Mona (
<geoCoordinate id="EE39500395500845E574FEF3EDC8FE85" box="[1298,1436,294,321]" degrees="18" direction="north" minutes="04" orientation="latitude" pageId="3" pageNumber="4" precision="15" seconds="56" value="18.082224">18°04’56”N</geoCoordinate>
<geoCoordinate id="EE39500395500845E0F1FE9FE97CFEA1" box="[151,296,330,357]" degrees="67" direction="west" minutes="56" orientation="longitude" pageId="3" pageNumber="4" precision="15" seconds="17" value="-67.93806">67°56’17”W</geoCoordinate>
