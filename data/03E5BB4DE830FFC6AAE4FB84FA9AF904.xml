<location id="8E935C80E830FFC6AB0AF95FFE72F905" LSID="urn:lsid:plazi:treatment:03E5BB4DE830FFC6AAE4FB84FA9AF904:8E935C80E830FFC6AB0AF95FFE72F905" box="[377,424,1735,1759]" municipality="Boucomont" name="One" pageId="20" pageNumber="321" stateProvince="Manilla">One</location>
