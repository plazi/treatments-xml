<taxonomicName id="BFFD4D10FF80FFBA7706DA460942F835" box="[280,557,1919,1941]" class="Trematoda" family="Anenterotrematidae" genus="Anenterotrema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="12" pageNumber="62" phylum="Platyhelminthes" rank="species" species="mesolecitha">
