<taxonomicName id="07993B8FFFFBFFBBFC4DFA0DFEB0E8B8" authority="(Kirby)" baseAuthorityName="Kirby" class="Insecta" family="Curculionidae" genus="Polygraphus" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="211" phylum="Arthropoda" rank="species" species="rufipennis">
