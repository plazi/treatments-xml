<taxonomicName id="07993B8FFFFBFFBBFDA5FA0DFC7BE89B" authority="(Kirby)" baseAuthorityName="Kirby" box="[572,925,1469,1496]" class="Insecta" family="Curculionidae" genus="Dendroctonus" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="211" phylum="Arthropoda" rank="species" species="rufipennis">
