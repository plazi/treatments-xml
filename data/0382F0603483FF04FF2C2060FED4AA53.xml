<taxonomicName id="4C2B3AF53481FF05FCA5230DFBF6A801" box="[828,1066,1614,1638]" class="Insecta" family="Miridae" genus="Ngullamiris" kingdom="Animalia" order="Hemiptera" pageId="190" pageNumber="190" phylum="Arthropoda" rank="species" species="whadjuk">
