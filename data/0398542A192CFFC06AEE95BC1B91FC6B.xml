<collectorName id="26C480EA192CFFC06E1B92151C04FDC8" pageId="19" pageNumber="19">
<location id="8EEEB3E7192CFFC06E1B92151C20FDC8" LSID="urn:lsid:plazi:treatment:0398542A192CFFC06AEE95BC1B91FC6B:8EEEB3E7192CFFC06E1B92151C20FDC8" county="In" municipality="Rothschild Collection" name="Soronko River" pageId="19" pageNumber="19" stateProvince="Kisumu">Soronko River</location>
