<date id="FF8FC3FC192CFFC06E1591AB1DD6FE5E" pageId="19" pageNumber="19" value="1916-04-28">
<collectingDate id="EFCB3A14192CFFC06E1591AB1DD6FE5E" pageId="19" pageNumber="19" value="1916-04-28">28 April 1916</collectingDate>
</date>
