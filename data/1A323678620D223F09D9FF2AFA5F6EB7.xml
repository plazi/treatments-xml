<taxonomicName id="559BFCED620D223F0A8BF87FFADE6EB7" authority="Gunter &amp; Weir" authorityName="Gunter &amp; Weir" authorityYear="2021" box="[965,1308,1985,2007]" class="Insecta" family="Scarabaeidae" genus="Lepanus" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="williamsi" status="new species">
