<taxonomicName id="27CA4D35FF4DFF2B5C12FDE6FB7B63FC" authorityName="Rondani" authorityYear="1856" box="[1036,1224,619,645]" class="Insecta" family="Hybotidae" genus="Oedalea" kingdom="Animalia" order="Diptera" pageId="128" pageNumber="129" phylum="Arthropoda" rank="species" species="bracata">
