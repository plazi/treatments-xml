; 
<materialsCitation id="3B5C9E28BA179D7EFC54FE5EFBA5FC95" collectingDate="1918-04-20" collectionCode="AMNH" country="Vietnam" county="Rothschild" elevation="2057" location="Rothschild" municipality="The" pageId="98" pageNumber="99" specimenCode="AMNH 590909" specimenCount="12" specimenCount-female="1" specimenCount-male="1" typeStatus="paratype">
