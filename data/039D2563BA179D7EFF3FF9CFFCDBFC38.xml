<location id="8EEBC2AEBA179D7EFBBAFE36FBCBFE35" LSID="urn:lsid:plazi:treatment:039D2563BA179D7EFF3FF9CFFCDBFC38:8EEBC2AEBA179D7EFBBAFE36FBCBFE35" box="[1109,1140,486,509]" country="Vietnam" county="Rothschild" municipality="The" name="Of" pageId="98" pageNumber="99">Of</location>
the 
<specimenCount id="9D325FFCBA179D7EFB44FE36FCBFFDD3" pageId="98" pageNumber="99" type="male">six males</specimenCount>
and 
<specimenCount id="9D325FFCBA179D7EFCD1FDD4FC7CFDD3" box="[830,963,516,539]" pageId="98" pageNumber="99" type="female">six females</specimenCount>
