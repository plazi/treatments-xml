<taxonomicName id="4C34EFF6BA179D7EFBC5FD8CFB26FDBB" box="[1066,1177,604,627]" class="Aves" family="Timaliidae" genus="Lioptila" kingdom="Animalia" order="Passeriformes" pageId="98" pageNumber="99" phylum="Chordata" rank="species" species="robinsoni">
<emphasis id="B9404867BA179D7EFBC5FD8CFB26FDBB" box="[1066,1177,604,627]" italics="true" pageId="98" pageNumber="99">robinsoni</emphasis>
</taxonomicName>
