<location id="8EEBC2AEBA179D7EFC36FDEEFBCDFD9D" LSID="urn:lsid:plazi:treatment:039D2563BA179D7EFF3FF9CFFCDBFC38:8EEBC2AEBA179D7EFC36FDEEFBCDFD9D" box="[985,1138,574,597]" country="Vietnam" county="Rothschild" municipality="The" name="Nevertheless" pageId="98" pageNumber="99">Nevertheless</location>
