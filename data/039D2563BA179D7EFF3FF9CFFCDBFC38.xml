, 
<location id="8EEBC2AEBA179D7EFC34FDF1FBE6FDF0" LSID="urn:lsid:plazi:treatment:039D2563BA179D7EFF3FF9CFFCDBFC38:8EEBC2AEBA179D7EFC34FDF1FBE6FDF0" box="[987,1113,545,568]" country="Vietnam" county="Rothschild" municipality="The" name="Rothschild" pageId="98" pageNumber="99">Rothschild</location>
