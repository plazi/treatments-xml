<taxonomicName id="C0D24D6CCF10EB7C8CF8D6FCFD1A95AC" box="[444,661,669,693]" class="Arachnida" family="Uloboridae" genus="Orinomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="203" phylum="Arthropoda" rank="species" species="lamprus">
