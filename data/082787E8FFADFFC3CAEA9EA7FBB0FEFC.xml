<taxonomicName id="478E4D7DFFABFFC3CBA19CC2FD43F87E" baseAuthorityName="Lobl" baseAuthorityYear="1971" box="[668,756,1956,1979]" class="Insecta" family="Staphylinidae" genus="Baeocera" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="87" phylum="Arthropoda" rank="species" species="lenta">
