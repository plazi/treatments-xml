<taxonomicName id="4D17C1FBC7110789F5FE5FA74F3BAC9D" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" class="Insecta" family="Chrysididae" genus="Hedychrum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hedychrum aureicolle" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="aureicolle">
