<paragraph id="E91A400DE00DFFD5FEA022D6FDBEFA36" blockId="2.[323,588,1469,1493]" box="[323,588,1469,1493]" pageId="2" pageNumber="47">
<heading id="B252F761E00DFFD5FEA022D6FDBEFA36" bold="true" box="[323,588,1469,1493]" centered="true" fontSize="10" level="2" pageId="2" pageNumber="47" reason="2">
