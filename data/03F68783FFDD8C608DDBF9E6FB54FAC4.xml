<taxonomicName id="4C5F4D16FFD18C638E49FE1DFED7FE04" box="[283,354,408,433]" class="Magnoliopsida" family="Asteraceae" genus="Picris" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="161" phylum="Tracheophyta" rank="genus">
