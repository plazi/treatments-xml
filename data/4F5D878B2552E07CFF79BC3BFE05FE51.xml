<taxonomicName id="00F44D1E2555E07CFF79BBFBFE88FE51" authorityName="Sukaczev" authorityYear="1911" box="[151,357,441,467]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="platyphylla">
