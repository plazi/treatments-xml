<emphasis id="F580EA8F2555E07CFCCDBADBFC81FF31" box="[803,876,153,179]" italics="true" pageId="20" pageNumber="21">Betula</emphasis>
</taxonomicName>
spp. (
<taxonomicName id="00F44D1E2555E07CFC5DBADBFBDCFF36" authorityName="S.F.Gray" authorityYear="1822" box="[947,1073,153,180]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
) (
<bibRefCitation id="A3654B6C2555E07CFBA7BADBFB29FF31" author="Ellis, W." box="[1097,1220,153,179]" pageId="20" pageNumber="21" refId="ref31004" refString="Ellis, W. (2018) Leafminers and plant galls of Europe. Plant parasites of Europe: leafminers, gallers and fungi. Available from: http: // bladmineerders. nl / (accessed 2 August 2018)" type="url" year="2018">Ellis 2018</bibRefCitation>
