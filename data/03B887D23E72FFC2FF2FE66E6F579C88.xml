<number id="3DD924E33E72FFC2FB9CE75E690E994D" box="[1060,1130,860,885]" pageId="13" pageNumber="188" value="10707.0">10707</number>
); st. 
<quantity id="4CE99B213E72FFC2FB13E75E685D994C" box="[1195,1337,860,884]" metricMagnitude="3" metricUnit="m" metricValue="1.82324" pageId="13" pageNumber="188" unit="m" value="1823.24">1823, 24 m</quantity>
, 
