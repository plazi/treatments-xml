<quantity id="4CE99B213E72FFC2FDD5E75E6FCA994C" box="[621,686,860,884]" metricMagnitude="1" metricUnit="m" metricValue="2.4" pageId="13" pageNumber="188" unit="m" value="24.0">24 m</quantity>
