<materialsCitation id="85AC3CF2FF8A792EFC781AA7FEC32550" collectionCode="T" pageId="6" pageNumber="429" specimenCount="1" typeStatus="allotype">
460; t1= 168; t2 = 52; t3 = 60; length of phallosome = 250. โ 
<emphasis id="07B0EABDFF8A792EFD601AC7FC2C26B0" box="[747,920,461,485]" italics="true" pageId="6" pageNumber="429">
Female 
<typeStatus id="EA7F880DFF8A792EFCC91AC7FC2C26B0" box="[834,920,461,485]" pageId="6" pageNumber="429">allotype</typeStatus>
</emphasis>
: BL = 1510; FW = 220; F = 350; 
<collectionCode id="53D5AE6AFF8A792EFEEC1AE4FEC32550" box="[359,375,494,517]" name="Tavera, Department of Geology and Geophysics" pageId="6" pageNumber="429">T</collectionCode>
</materialsCitation>
= 560; t1= 180; t2 = 56; t3 = 65.
