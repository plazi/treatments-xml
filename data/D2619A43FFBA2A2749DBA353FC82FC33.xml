<collectingCountry id="22DF6BC5FFBC2A274BD6A7CFFD00FC16" box="[666,765,942,968]" name="Belgium" pageId="69" pageNumber="70">Belgium</collectingCountry>
, the British Isles, 
<collectingCountry id="22DF6BC5FFBC2A274A86A7CFFBDFFC16" box="[970,1058,942,968]" name="France" pageId="69" pageNumber="70">Corsica</collectingCountry>
, 
<collectingCountry id="22DF6BC5FFBC2A274D63A7CFFB7AFC16" box="[1071,1159,942,968]" name="Finland" pageId="69" pageNumber="70">Finland</collectingCountry>
, 
<collectingCountry id="22DF6BC5FFBC2A274DDFA7CEFB1FFC17" box="[1171,1250,943,969]" name="France" pageId="69" pageNumber="70">France</collectingCountry>
, 
<collectingCountry id="22DF6BC5FFBC2A274DA2A7CFFAAAFC16" box="[1262,1367,942,968]" name="Germany" pageId="69" pageNumber="70">Germany</collectingCountry>
, 
<collectingCountry id="22DF6BC5FFBC2A274C28A7CFFA65FC16" box="[1380,1432,942,968]" name="Italy" pageId="69" pageNumber="70">Italy</collectingCountry>
, 
<collectingCountry id="22DF6BC5FFBC2A2749DBA7B3FF2AFC32" box="[151,215,978,1004]" name="Spain" pageId="69" pageNumber="70">Spain</collectingCountry>
and also 
<collectingCountry id="22DF6BC5FFBC2A27480EA7B2FE7FFC33" box="[322,386,979,1005]" name="Japan" pageId="69" pageNumber="70">Japan</collectingCountry>
(
<bibRefCitation id="3E5956A4FFBC2A2748DCA7B3FD8BFC32" author="Papp, L. &amp; Cerny, M." box="[400,630,978,1007]" pageId="69" pageNumber="70" refId="ref74240" refString="Papp, L. &amp; Cerny, M. (2019) Agromyzidae (Diptera) of Hungary. Vol. 4. Phytomyzinae III. Pars Ltd, Nagycovacsi, 708 pp." type="book" year="2019">Papp &amp; Černý 2019</bibRefCitation>
). 
<collectingCountry id="22DF6BC5FFBC2A274BC4A7B3FD1FFC32" box="[648,738,978,1004]" name="Ukraine" pageId="69" pageNumber="70">Ukraine</collectingCountry>
(first record).
</paragraph>
