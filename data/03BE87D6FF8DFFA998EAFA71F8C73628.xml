<quantity id="4CEF9B25FF8DFFA99D1EFA1DFFDD360F" box="[1379,1436,1519,1546]" metricMagnitude="1" metricUnit="m" metricValue="6.4" pageId="28" pageNumber="29" unit="m" value="64.0">64 m</quantity>
(
<quantity id="4CEF9B25FF8DFFA998B3F9E1FB52362B" box="[206,275,1555,1582]" metricMagnitude="1" metricUnit="m" metricValue="6.4008" pageId="28" pageNumber="29" unit="ft" value="210.0">210 ft</quantity>
