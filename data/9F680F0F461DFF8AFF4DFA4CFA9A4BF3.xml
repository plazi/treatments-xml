</emphasis>
<typeStatus id="C87A00BB461DFF8BFE80F9D8FE0E4914" box="[346,457,1545,1571]" pageId="36" pageNumber="37">Holotype</typeStatus>
male, 
<emphasis id="25B5620B461DFF8BFDC1F9D8FD504914" bold="true" box="[539,663,1545,1571]" pageId="36" pageNumber="37">
<collectingCountry id="6FD6FE89461DFF8BFDC1F9D8FD554914" box="[539,658,1545,1571]" name="Brazil" pageId="36" pageNumber="37">BRAZIL</collectingCountry>
