<paragraph id="F3C8943DE7AB97303A513B052E4E6903" pageId="7" pageNumber="300">
<taxonomicName id="77FC73332D6F192133845DAD48A4F6A9" authority="Martins-Neto &amp; Kucera-Santos, 1994" authorityName="Martins-Neto &amp; Kucera-Santos" authorityYear="1994" class="Insecta" family="Tabanidae" genus="Cratotabanus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cratotabanus" order="Diptera" pageId="7" pageNumber="300" phylum="Arthropoda" rank="genus">Cratotabanus</taxonomicName>
