<specimenCount id="F097FD24FFDCFFE1FE814008A2550760" box="[342,358,541,566]" pageId="5" pageNumber="230" type="female">♀</specimenCount>
, ‘Bayr. Alpen / Osterseegebiet / Anf. 
<date id="922F106DFFDCFFE1FD6E4036A0340760" box="[697,775,547,566]" pageId="5" pageNumber="230" value="1969-09">IX. 1969</date>
// Gesiebe von / Riedgräsern / leg. Wellschmied // 
<taxonomicName id="21914D2EFFDCFFE1FF204028A24A0706" authorityName="Grouvelle" authorityYear="1913" box="[247,377,573,592]" class="Insecta" family="Kateretidae" genus="Brachypterolus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="234" phylum="Arthropoda" rank="genus">Brachypterolus</taxonomicName>
/ 
<taxonomicName id="21914D2EFFDCFFE1FE5C4028A1270706" authority="(Steph.)" baseAuthorityName="Stephens" baseAuthorityYear="1830" box="[395,532,573,592]" class="Insecta" family="Kateretidae" genus="Brachypterolus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="230" phylum="Arthropoda" rank="species" species="linariae">linariae (Steph.)</taxonomicName>
