<taxonomicName id="F780EBFDC575FC66FF7A23968C0CF80E" baseAuthorityName="Ortmann" baseAuthorityYear="1889" box="[173,370,1928,1949]" class="Anthozoa" family="Fungiidae" genus="Cycloseris" kingdom="Animalia" order="Scleractinia" pageId="4" pageNumber="570" phylum="Cnidaria" rank="species" species="costulata">
