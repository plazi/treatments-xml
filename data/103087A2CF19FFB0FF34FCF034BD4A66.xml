</bibRefCitation>
). 
<taxonomicName id="5F994D37CF19FFB7FC6BF97F31F44C1A" box="[968,1189,1773,1798]" class="Insecta" family="Asilidae" genus="Molobratia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="youngi">
<emphasis id="AAEDEAA6CF19FFB7FC6BF97F31F44C1A" box="[968,1189,1773,1798]" italics="true" pageId="3" pageNumber="4">Molobratia youngi</emphasis>
</taxonomicName>
inserted into couplet 
<number id="2E512493CF19FFB7FF34F88735F64C33" box="[151,167,1813,1839]" pageId="3" pageNumber="4" value="1.0">1</number>
