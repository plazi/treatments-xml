<taxonomicName id="4C3E4D58FF87CD3EFD1642E4FC7F4AB5" authority="Maulik" authorityName="Maulik" box="[716,996,152,178]" class="Insecta" family="Lampyridae" genus="Lamellipalpus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="manipurensis">
