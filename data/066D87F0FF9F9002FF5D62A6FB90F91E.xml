<subSubSection id="C6DE656DFF9F9002FF5D63F3FBF9FA62" pageId="34" pageNumber="35" type="diagnosis">
<paragraph id="8E7B36E6FF9F9002FF5D63F3FBF9FA62" blockId="34.[151,1436,1449,1690]" pageId="34" pageNumber="35">
<emphasis id="BCB0EAF4FF9F9002FF5D63F3FEBBFA46" bold="true" box="[151,266,1449,1474]" pageId="34" pageNumber="35">Diagnosis</emphasis>
