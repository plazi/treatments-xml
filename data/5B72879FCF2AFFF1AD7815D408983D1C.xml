<taxonomicName id="14DB4D0ACF2AFFF6AB30159A0E153824" authority="Sars, 1896" authorityName="Sars" authorityYear="1896" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="11" pageNumber="334" phylum="Arthropoda" rank="species" species="tenellus">
