<paragraph id="D3643689CF2AFFF6AD2D159A0EB23824" blockId="11.[826,1441,956,1038]" pageId="11" pageNumber="334">
<taxonomicName id="14DB4D0ACF2AFFF6AD2D159A0EBE3FC5" authority="Stebbing, 1899" authorityName="STEBBING" authorityYear="1899" box="[826,1202,985,1007]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="11" pageNumber="334" phylum="Arthropoda" rank="species" species="ischnus">
