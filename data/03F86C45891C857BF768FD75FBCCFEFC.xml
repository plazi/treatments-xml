<emphasis id="B9250141891C857BF6D4FC31FDFAFC7F" box="[144,531,960,981]" italics="true" pageId="6" pageNumber="613">
<typeStatus id="54EA63F1891C857BF6D4FC31FF2DFC7F" box="[144,196,960,981]" pageId="6" pageNumber="613">Type</typeStatus>
