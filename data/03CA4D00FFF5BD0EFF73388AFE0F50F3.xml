<taxonomicName id="4C638795FFF6BD0EFEA3388BFDA750BA" authorityName="Mlk., Cotype" authorityYear="1923" box="[327,551,152,178]" class="Insecta" family="Chrysomelidae" genus="Chaetocnema" kingdom="Animalia" order="Coleoptera" pageId="102" pageNumber="103" phylum="Arthropoda" rank="species" species="sticta">
