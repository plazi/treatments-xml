<collectingCountry id="FEAE9BD0DF72FFDDFEEFFC6589D230D3" box="[359,423,948,974]" name="Japan" pageId="3" pageNumber="287">Japan</collectingCountry>
(Honshu, Shikoku, Kyushu); 
<collectingCountry id="FEAE9BD0DF72FFDDFD7FFC628B4B30D0" box="[759,830,947,973]" name="China" pageId="3" pageNumber="287">China</collectingCountry>
