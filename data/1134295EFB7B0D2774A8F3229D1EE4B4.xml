<taxonomicName id="A9BAE820B0998DA2C4FF27844C1AAA0E" class="Insecta" family="Tenebrionidae" genus="Platydema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platydema teleops" order="Coleoptera" pageId="8" pageNumber="265" phylum="Arthropoda" rank="species" species="teleops">Platydema teleops</taxonomicName>
