<typeStatus id="549504BB01440725FE2C67B2FE47FA4E" box="[455,505,1480,1504]" pageId="1" pageNumber="560">Type</typeStatus>
species: 
<taxonomicName id="4C2EC19A01440725FDBC67B2FCCEFA71" authority="Stal, 1863" authorityName="Stal" authorityYear="1863" box="[599,880,1479,1504]" class="Insecta" family="Lophopidae" genus="Acarna" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="560" phylum="Arthropoda" rank="species" species="pupillata">
