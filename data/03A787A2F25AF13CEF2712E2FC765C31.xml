[
<taxonomicName id="4C0E4D37F25AF13CEFDE12E2FDC559D0" authority="Stresemann" authorityName="Stresemann" authorityYear="1923" box="[132,580,1164,1188]" class="Aves" family="Dicaeidae" genus="Dicaeum" kingdom="Animalia" order="Passeriformes" pageId="110" pageNumber="110" phylum="Chordata" rank="subSpecies" species="minullum" subSpecies="sinense">
<emphasis id="B97AEAA6F25AF13CEFDE12E2FE3259D0" box="[132,435,1165,1188]" italics="true" pageId="110" pageNumber="110">Dicaeum minullum sinense</emphasis>
