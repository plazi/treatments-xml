<taxonomicName id="59E304AABE0CFFB8FEB7F893FD81B22B" authority="Ruffo, 1979" authorityName="Ruffo" authorityYear="1979" box="[299,582,1870,1894]" class="Malacostraca" family="Paracrangonyctidae" genus="Dussartiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
