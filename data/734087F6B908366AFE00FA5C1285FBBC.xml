<paragraph id="FB5636E0B909366AFF0AFF4C12CBFF58" blockId="15.[146,727,168,192]" box="[146,727,168,192]" pageId="15" pageNumber="2570">
<materialsCitation id="4B813CBDB909366AFF0AFF4C12CFFF58" accessionNumber="D53321" box="[146,723,168,192]" pageId="15" pageNumber="2570" specimenCount="2" typeStatus="paratype">
