<taxonomicName id="C56157DF9D3BB48296D8141063456F4F" authority="L. (1753)" authorityName="Linnaeus" authorityYear="1753" baseAuthorityName="Linnaeus" class="Magnoliopsida" family="Penaeaceae" genus="Penaea" kingdom="Plantae" order="Myrtales" phylum="Tracheophyta" rank="species" species="sarcocolla">
