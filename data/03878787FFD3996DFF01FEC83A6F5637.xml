), 
<collectingCountry id="F3397601FFD3996AFDEAF8D73DEF505B" box="[636,736,1880,1905]" name="Thailand" pageId="3" pageNumber="4">Thailand</collectingCountry>
(
<collectingRegion id="49EAF873FFD3996AFD78F8D73C78505B" box="[750,887,1880,1905]" country="Thailand" name="Phetchabun" pageId="3" pageNumber="4">Phetchabun</collectingRegion>
