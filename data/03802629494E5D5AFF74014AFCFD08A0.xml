<emphasis id="B95D4B2D494D5D5BFCB5010EFC3F0D08" box="[854,964,296,320]" italics="true" pageId="2" pageNumber="445">Adocimus</emphasis>
</taxonomicName>
-complex (
<bibRefCitation id="EFB8EACE494D5D5BFBA70101FA630D08" author="Kirejtshuk" box="[1092,1432,295,320]" pageId="2" pageNumber="445" refString="Kirejtshuk, A. G. &amp; Kovalev, A. V. (2016) A new genus of the subfamily Cillaeinae (Coleoptera, Nitidulidae) from the Philippines and New Guinea with notes on the taxonomy and phylogeny of the subfamily. Zootaxa, 4205 (3), 226 - 242. https: // doi. org / 10.11646 / zootaxa. 4205.3.3" type="journal article" year="2016">Kirejtshuk &amp; Kovalev 2016</bibRefCitation>
: 
<taxonomicName id="4C29ECBC494D5D5BFF74016AFE520D2D" authority="Murray, 1864" authorityName="Murray" authorityYear="1864" box="[151,425,332,357]" class="Insecta" family="Nitidulidae" genus="Adocimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="445" phylum="Arthropoda" rank="genus">
<emphasis id="B95D4B2D494D5D5BFF74016AFEFE0D2C" box="[151,261,332,356]" italics="true" pageId="2" pageNumber="445">Adocimus</emphasis>
Murray, 1864
</taxonomicName>
, 
<taxonomicName id="4C29ECBC494D5D5BFE55016AFD2A0D2C" authority="Brittonoma" authorityName="Brittonoma" authorityYear="2016" box="[438,721,332,356]" class="Insecta" family="Nitidulidae" genus="Allenipeplus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="445" phylum="Arthropoda" rank="genus">
<emphasis id="B95D4B2D494D5D5BFE55016AFDB90D2C" box="[438,578,332,356]" italics="true" pageId="2" pageNumber="445">Allenipeplus</emphasis>
, 
<emphasis id="B95D4B2D494D5D5BFDB2016BFD2A0D2C" box="[593,721,333,356]" italics="true" pageId="2" pageNumber="445">Brittonoma</emphasis>
</taxonomicName>
<emphasis id="B95D4B2D494D5D5BFD3B016AFCB50D2D" bold="true" box="[728,846,332,357]" pageId="2" pageNumber="445">nom. nov.</emphasis>
and 
<taxonomicName id="4C29ECBC494D5D5BFC65016AFB5A0D2D" authority="Murray, 1864" authorityName="Murray" authorityYear="1864" box="[902,1185,332,357]" class="Insecta" family="Nitidulidae" genus="Ithyphenes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="445" phylum="Arthropoda" rank="genus">
