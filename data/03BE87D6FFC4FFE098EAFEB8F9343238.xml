<quantity id="4CEF9B25FFC4FFE09B78FE61F90C31A8" box="[773,845,403,429]" metricMagnitude="2" metricUnit="m" metricValue="4.64" pageId="85" pageNumber="86" unit="m" value="464.0">464 m</quantity>
(
