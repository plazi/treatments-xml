<taxonomicName id="4C174D43FFC4FFE0999CFE29F80E31F1" authorityName="Girard" authorityYear="1854" box="[481,591,475,500]" class="Actinopterygii" family="Batrachoididae" genus="Porichthys" kingdom="Animalia" order="Batrachoidiformes" pageId="85" pageNumber="86" phylum="Chordata" rank="species" species="notatus">
