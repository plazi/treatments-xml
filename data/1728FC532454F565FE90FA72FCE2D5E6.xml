<collectionCode id="F990D5802454F566FCACF9CDFC66D210" LSID="urn:lsid:biocol.org:col:34925" box="[820,901,1625,1648]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="1" pageNumber="64">AMNH</collectionCode>
