<taxonomicName id="588136C62454F566FE90F9CEFD9DD210" authority="Hy. Edwards, 1881" authorityName="Hy. Edwards" authorityYear="1881" box="[264,638,1625,1649]" class="Insecta" family="Noctuidae" genus="Euros" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="proprius">
