<taxonomicName id="84E6C7E85A02FFAE4A91FD5E4F88FD6D" box="[734,937,701,726]" class="Magnoliopsida" family="Rubiaceae" genus="Coprosma" kingdom="Plantae" order="Gentianales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="lucida">
