<taxonomicName id="84E6C7E85A02FFAE4D6CFD5E4CD2FD45" class="Magnoliopsida" family="Rubiaceae" genus="Coprosma" kingdom="Plantae" order="Gentianales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="acerosa">
