<collectingRegion id="0D28F83FAF57FFD18DB0FBC9FEDAFBC6" box="[196,361,1109,1135]" country="New Zealand" name="Bay of Plenty" pageId="10" pageNumber="11">Bay of Plenty</collectingRegion>
at a depth of 
<quantity id="08149B38AF57FFD18F60FBC9FD28FBC6" box="[532,667,1109,1135]" metricMagnitude="2" metricUnit="m" metricValue="3.66" metricValueMax="3.82" metricValueMin="3.5" pageId="10" pageNumber="11" unit="m" value="366.0" valueMax="382.0" valueMin="350.0">350–382 m</quantity>
(
<bibRefCitation id="AB7D4B2CAF57FFD18FD8FBC9FCE5FBC6" author="Dawson, E. W." box="[684,854,1109,1135]" pageId="10" pageNumber="11" pagination="1 - 21" refId="ref44863" refString="Dawson, E. W. (2002) First record of the primitive crab Homolodromia in New Zealand waters (Crustacea, Decapoda, Brachyura, Homolodromiidae), with a provisional new key to the extant species of Homolodromia. Hutton Foundation New Zealand, Special Papers, 1, 1 - 21." type="journal article" year="2002">Dawson 2002</bibRefCitation>
). 
<taxonomicName id="08EC4D5EAF57FFD18E18FBC9FBFFFBC7" authorityName="Guinot, 1993:" authorityYear="1228" box="[876,1100,1109,1134]" class="Malacostraca" family="Homolodromiidae" genus="Homolodromia" kingdom="Animalia" order="Decapoda" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="kai">
