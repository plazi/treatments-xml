<paragraph id="8BED835BFFAFFFEC5F8BF8898E51F89E" blockId="6.[151,1050,1804,1826]" box="[151,1050,1804,1826]" pageId="6" pageNumber="555">
<taxonomicName id="4C52F8D8FFAFFFEC5F8BF889887FF89E" authority="Parent, 1934: 295" authorityName="Parent" authorityPageNumber="295" authorityYear="1934" box="[151,564,1804,1826]" class="Insecta" family="Dolichopodidae" genus="Sciapus" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="555" phylum="Arthropoda" rank="species" species="latilamellatus">
