of 
<location id="C7416060544BA57FFE3702C14555FD3E" LSID="urn:lsid:plazi:treatment:4A3787AD5454A57FFF3C027643C0FC34:C7416060544BA57FFE3702C14555FD3E" box="[412,587,692,717]" country="United States of America" county="Madison County" municipality="Madison County" name="Natural History" pageId="16" pageNumber="511" stateProvince="Florida">Natural History</location>
