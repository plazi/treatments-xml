<taxonomicName id="992FB8B1275CB6FA3AE112BB8B73D83F" class="Insecta" family="Dolichopodidae" genus="Liancalus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Liancalus" order="Diptera" pageId="6" pageNumber="103" phylum="Arthropoda" rank="genus">Liancalus</taxonomicName>
