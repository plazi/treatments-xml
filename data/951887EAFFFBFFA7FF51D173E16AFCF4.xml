<paragraph id="1D0E36FCFFFAFFA7FF01D27DE5C1FE37" blockId="57.[151,1437,357,816]" pageId="57" pageNumber="293">
P 
<specimenCount id="0BB7FD75FFFAFFA7FF1ED27DE541FEA4" box="[216,257,357,384]" pageId="57" pageNumber="293" type="female">5 ♀</specimenCount>
