<taxonomicName id="5DD24D38FFC4FFD364F4F817FB41F831" authorityName="Thomas" authorityYear="1917" box="[944,1183,1978,2000]" class="Mammalia" family="Cricetidae" genus="Urocricetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="12" pageNumber="343" phylum="Chordata" rank="species" species="alticola">
<emphasis id="A8A6EAA9FFC4FFD364F4F817FB41F831" box="[944,1183,1978,2000]" italics="true" pageId="12" pageNumber="343">Urocricetus aff.alticola</emphasis>
</taxonomicName>
(ZMMU S-155362), (e) 
<taxonomicName id="5DD24D38FFC4FFD367D3F877FE41F810" baseAuthorityName="Brandt" baseAuthorityYear="1859" box="[151,415,2011,2033]" class="Mammalia" family="Cricetidae" genus="Allocricetulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="12" pageNumber="343" phylum="Chordata" rank="species" species="eversmanni">
<emphasis id="A8A6EAA9FFC4FFD367D3F877FE41F810" box="[151,415,2011,2033]" italics="true" pageId="12" pageNumber="343">Allocricetulus eversmanni</emphasis>
