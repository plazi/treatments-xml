</taxonomicName>
, 
<taxonomicName id="84804D2870737300351322D1FB2B0CB7" box="[1027,1187,1498,1521]" class="Insecta" family="Colletidae" genus="Chilicola" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="45" phylum="Arthropoda" rank="species" species="mirzamalae">
