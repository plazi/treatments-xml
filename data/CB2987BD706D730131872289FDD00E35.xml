</taxonomicName>
, 
<emphasis id="71F4EAB970737300349422D1FF510F53" italics="true" pageId="16" pageNumber="45">
<taxonomicName id="84804D2870737300349422D1FF5E0F53" class="Insecta" family="Colletidae" genus="Chilicola" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="45" phylum="Arthropoda" rank="species" species="penai">C. penai</taxonomicName>
