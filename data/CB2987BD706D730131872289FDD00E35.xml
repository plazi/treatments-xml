<taxonomicName id="84804D28706C731F303221C9FE400F9F" box="[290,456,1730,1753]" class="Insecta" family="Colletidae" genus="Chilicola" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="44" phylum="Arthropoda" rank="species" species="penai">
