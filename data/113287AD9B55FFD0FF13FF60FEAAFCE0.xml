<quantity id="5E639B5E9B55FFD0FDD0FEBCFD55FE91" box="[596,736,330,355]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" metricValueMax="2.1" metricValueMin="1.9" pageId="12" pageNumber="13" unit="mm" value="2.0" valueMax="2.1" valueMin="1.9">
