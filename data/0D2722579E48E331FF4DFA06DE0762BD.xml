<quantity id="42763EA49E48E332FDA1F90BD9EC6459" box="[635,720,1778,1802]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="45" pageNumber="46" unit="mm" value="4.4">4.4 mm</quantity>
