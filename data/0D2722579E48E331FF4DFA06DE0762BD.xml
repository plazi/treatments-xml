<quantity id="42763EA49E48E332FC68F90BDF376459" box="[946,1035,1778,1802]" metricMagnitude="-3" metricUnit="m" metricValue="7.8" pageId="45" pageNumber="46" unit="mm" value="7.8">7.8 mm</quantity>
