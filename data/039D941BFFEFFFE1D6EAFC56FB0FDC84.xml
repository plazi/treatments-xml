<taxonomicName id="4C345E8EFFEFFFE2D6EAFC56FE62D8ED" authority="Shear" authorityName="Shear" box="[151,450,933,959]" class="Insecta" family="Lycaenidae" genus="Taracus" kingdom="Animalia" order="Lepidoptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="audisioae" status="new species">
<emphasis id="B940F91FFFEFFFE2D6EAFC56FED2D8ED" bold="true" box="[151,370,933,959]" italics="true" pageId="49" pageNumber="50">Taracus audisioae</emphasis>
