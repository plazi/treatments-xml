<location id="E33C59E2FFA7FF853237734225DAA206" LSID="urn:lsid:plazi:treatment:6E4ABE2FFFA7FF8530C375E22526A296:E33C59E2FFA7FF853237734225DAA206" box="[611,648,1071,1094]" country="Guadeloupe" county="Monroe" municipality="Levy" name="We" pageId="9" pageNumber="210" stateProvince="Florida">We</location>
