<taxonomicName id="4CF74D27FF98FFB6FD283781FCEDD248" authority="LACMIP" authorityName="LACMIP" box="[701,854,752,778]" class="Insecta" family="Bibionidae" genus="Bibio" kingdom="Animalia" order="Diptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="auripes">
