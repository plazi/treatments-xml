<taxonomicName id="69DA6224E33C7302402A42E1F58B02F7" LSID-ZBK="urn:lsid:zoobank.org:act:DC5E825F-03C8-4F47-80C6-350B61232BD1" class="Actinopterygii" family="Apteronotidae" genus="Apteronotus" kingdom="Animalia" lsidName-ZBK="Apteronotus Lacépède 1800:208" order="Gymnotiformes" pageNumber="6" phylum="Chordata" rank="genus">Apteronotus</taxonomicName>
