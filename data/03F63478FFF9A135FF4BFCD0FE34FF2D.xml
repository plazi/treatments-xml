; 
<quantity id="4CA7288BFFF9A134FD0FFA32FCBFFA3B" box="[723,844,1488,1514]" metricMagnitude="2" metricUnit="m" metricValue="7.5" metricValueMax="15.0" metricValueMin="0.0" pageId="50" pageNumber="51" unit="m" value="750.0" valueMax="1500.0" valueMin="0.0">0–1500 m</quantity>
