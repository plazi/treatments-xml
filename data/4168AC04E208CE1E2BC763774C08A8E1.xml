<taxonomicName id="D53D9DB27BF52ACF09CB8E64C6FBD049" LSID-ZBK="urn:lsid:zoobank.org:act:CEBC96E6-EC0E-4B79-B0E1-C693E445CD64" class="Actinopterygii" family="Sisoridae" genus="Pseudecheneis" kingdom="Animalia" lsidName-ZBK="Pseudecheneis sulcatoides Zhou &amp; Chu 1992:111 [115]" order="Siluriformes" pageNumber="51" phylum="Chordata" rank="species" species="sulcatoides">P. sulcatoides</taxonomicName>
