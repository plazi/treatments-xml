<quantity id="4C8A9B0400329F3BFDD2FAAC701DFADA" box="[549,608,1346,1368]" metricMagnitude="4" metricUnit="m" metricValue="2.5" pageId="24" pageNumber="125" unit="km" value="25.0">25km</quantity>
off the shore of Ubatuba, 
<date id="FFCC102100329F3BFC9DFAAC7192FADA" box="[874,1007,1346,1369]" pageId="24" pageNumber="125" value="1964-03-26">
<number id="3DBA24C600329F3BFC9DFAAC71F9FADA" box="[874,900,1346,1368]" pageId="24" pageNumber="125" value="26.0">26</number>
