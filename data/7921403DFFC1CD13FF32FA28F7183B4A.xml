<taxonomicName id="36888AA8FFC0CD13FC74FC78F6DA3DBF" authority="Chen, Song &amp; Li, 2001" authorityName="Chen, Song &amp; Li" authorityYear="2001" class="Arachnida" family="Lycosidae" genus="Pardosa" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="34" phylum="Arthropoda" rank="species" species="aciculifera">
