</taxonomicName>
; 37 – 
<taxonomicName id="4C42B2B9FFBE0F47FF04EE1ED6650732" authority="Stehlík &amp; Jindra, 2008" authorityName="Stehlík &amp; Jindra" authorityYear="2008" box="[254,598,1554,1573]" class="Insecta" family="Pyrrhocoridae" genus="Brancucciana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="637" phylum="Arthropoda" rank="species" species="orientalis" status="sp. nov." subGenus="Rubriascopus">
