<taxonomicNameLabel id="A205A853FFBE0F47FDA0EE1FD6AC0732" box="[602,671,1554,1573]" pageId="26" pageNumber="637" rank="species">sp. nov.</taxonomicNameLabel>
; 38 – 
<taxonomicName id="4C42B2B9FFBE0F47FD2BEE1ED7D00732" authority="(Distant, 1903)" baseAuthorityName="Distant" baseAuthorityYear="1903" box="[721,995,1554,1573]" class="Insecta" family="Pyrrhocoridae" genus="Brancucciana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="637" phylum="Arthropoda" rank="species" species="pygmaea" subGenus="Rubriascopus">
