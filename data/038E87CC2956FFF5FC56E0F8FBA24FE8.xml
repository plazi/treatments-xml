<quantity id="4CDF9B3F2956FFF4FBC9E16CFB414AC4" box="[1055,1164,1916,1945]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="22" pageNumber="21" unit="mm" value="0.6">0.60mm</quantity>
, width 
<quantity id="4CDF9B3F2956FFF4FACEE16CFA484AC4" box="[1304,1413,1916,1945]" metricMagnitude="-4" metricUnit="m" metricValue="4.4" pageId="22" pageNumber="21" unit="mm" value="0.44">0.44mm</quantity>
