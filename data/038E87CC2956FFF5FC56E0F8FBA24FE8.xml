<quantity id="4CDF9B3F2957FFF5FFBEE660FF1B4DD0" box="[104,214,112,141]" metricMagnitude="-4" metricUnit="m" metricValue="3.1" pageId="23" pageNumber="22" unit="mm" value="0.31">0.31mm</quantity>
