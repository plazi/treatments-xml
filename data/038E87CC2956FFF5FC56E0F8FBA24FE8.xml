<quantity id="4CDF9B3F2957FFF5FE05E660FD8D4DD0" box="[467,576,112,141]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="23" pageNumber="22" unit="mm" value="0.8">0.80mm</quantity>
