<quantity id="72626396E6308E5501113509FAB9F89E" box="[1301,1376,1891,1913]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="5" pageNumber="412" unit="mm" value="1.5">1.5 mm</quantity>
