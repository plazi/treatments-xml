<quantity id="72626396E6308E550100352CFAE4F8BD" box="[1284,1341,1862,1883]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="5" pageNumber="412" unit="mm" value="2.0">2 mm</quantity>
