<quantity id="72626396E6308E55068F34FCFD1BF94D" box="[651,706,1686,1707]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="412" unit="mm" value="1.0">1 mm</quantity>
