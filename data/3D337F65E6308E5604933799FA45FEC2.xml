<quantity id="72626396E6308E550751352FFC47F8BD" box="[853,926,1861,1883]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="5" pageNumber="412" unit="mm" value="1.5">1.5 mm</quantity>
