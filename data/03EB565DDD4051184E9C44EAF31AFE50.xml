<taxonomicName id="4C429CC8DD4E511A4F1542F2F372F862" authority="Ceballos" authorityName="Ceballos" box="[286,607,1951,1973]" class="Liliopsida" family="Araceae" genus="Gonatopus" kingdom="Plantae" order="Alismatales" pageId="8" phylum="Tracheophyta" rank="species" species="baeticus">
