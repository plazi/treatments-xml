<taxonomicName id="12C34D156D345279FDA85BD38EE5FF21" box="[585,710,228,252]" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="36" pageNumber="1207" phylum="Tracheophyta" rank="genus">
<emphasis id="E7B7EA846D345279FDA85BD38EE5FF21" box="[585,710,228,252]" italics="true" pageId="36" pageNumber="1207">Nothofagus</emphasis>
