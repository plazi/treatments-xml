<subSubSection id="C31D43AEFFEBFFA5FFD0CFE9FD0244B8" box="[113,760,1225,1248]" pageId="1" pageNumber="40" type="type_taxon">
<paragraph id="8BB81025FFEBFFA5FFD0CFE9FD0244B8" blockId="1.[113,1090,1225,1632]" box="[113,760,1225,1248]" pageId="1" pageNumber="40">
<emphasis id="B973CC37FFEBFFA5FFD0CFE9FEFF44B8" bold="true" box="[113,261,1225,1248]" pageId="1" pageNumber="40">
<typeStatus id="54BCAE87FFEBFFA5FFD0CFE9FF5344B8" box="[113,169,1225,1248]" pageId="1" pageNumber="40">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C076BA6FFEBFFA5FEADCFE9FE1044B8" authority="Davranoglou &amp; Ox, 2016" authorityName="Davranoglou &amp; Ox" authorityYear="2016" box="[268,490,1225,1248]" class="Insecta" family="Reduviidae" genus="Redeicephala" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="taylori" status="sp. nov">
<emphasis id="B973CC37FFEBFFA5FEADCFE9FE1044B8" box="[268,490,1225,1248]" italics="true" pageId="1" pageNumber="40">Redeicephala taylori</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A240714CFFEBFFA5FE50CFE9FDC544B8" box="[497,575,1225,1248]" pageId="1" pageNumber="40" rank="species">sp. nov</taxonomicNameLabel>
