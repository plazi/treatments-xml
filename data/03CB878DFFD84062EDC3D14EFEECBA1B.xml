<quantity id="4C9A9B7EFFD84062E9EBD1BFFAAFBBAC" box="[1215,1381,1255,1280]" metricMagnitude="-2" metricUnit="m" metricValue="1.38" metricValueMax="1.61" metricValueMin="1.15" pageId="1" pageNumber="434" unit="mm" value="13.8" valueMax="16.1" valueMin="11.5">11.5–16.1 mm</quantity>
