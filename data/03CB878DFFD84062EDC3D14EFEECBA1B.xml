<quantity id="4C9A9B7EFFD84062EEF0D1B0FBC3BBAC" box="[932,1033,1256,1280]" metricMagnitude="-2" metricUnit="m" metricValue="1.09" pageId="1" pageNumber="434" unit="mm" value="10.9">10.9 mm</quantity>
