<taxonomicName id="71BB131146F491C2F999849FBDB83F4D" authority="" authorityName="" class="Arachnida" family="Oribatulidae" genus="Oribates" genus.outerRound="1" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageNumber="164" phylum="Arthropoda" rank="species" species="major">O. major Mihelcic</taxonomicName>
