<taxonomicName id="9EBE4F3AD8DD1E8C83735EA49A78B16F" authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" class="Arachnida" family="Ceratozetidae" genus="Oribata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageNumber="164" phylum="Arthropoda" rank="species" species="tecta">Oribata tecta Michael</taxonomicName>
