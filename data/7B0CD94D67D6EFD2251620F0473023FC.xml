<taxonomicName id="4D282EA560E569D087A555A64453A2A5" class="Arachnida" family="Tegoribatidae" genus="Tectoribates" genus.bestMatchDistance="519" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageNumber="164" phylum="Arthropoda" rank="species" species="punctatus">T. punctatus Sellnick</taxonomicName>
