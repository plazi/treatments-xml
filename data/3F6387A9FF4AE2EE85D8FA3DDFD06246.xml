<taxonomicName id="70CA4D3CFF4AE2EE84EBF9E5DFDA6366" box="[420,724,1590,1615]" class="Hydrozoa" family="Campanulariidae" genus="Campanularia" kingdom="Animalia" order="Leptothecata" pageId="167" pageNumber="168" phylum="Cnidaria" rank="species" species="calceolifera">
