<taxonomicName id="10D7CC1BB3876AE9C27CA44836A2227E" authorityName="Kjaerandsen, Polevoi &amp; Salmela" authorityYear="2020" genus="Coelosynapha" lsidName="Coelosynapha" pageId="0" pageNumber="54834" rank="genus">Coelosynapha</taxonomicName>
<taxonomicName id="5A8B910A7EFAC9C004996A178AA53209" authorityName="Kjaerandsen, Polevoi &amp; Salmela" authorityYear="2020" genus="Coelosynapha" lsidName="Coelosynapha loici" pageId="0" pageNumber="54834" rank="species" species="loici">Coelosynapha loici</taxonomicName>
