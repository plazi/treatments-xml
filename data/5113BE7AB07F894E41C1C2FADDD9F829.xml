<number id="6F721D4BB07F894E451BC4D2DD2FFA15" box="[1101,1143,1465,1489]" pageId="11" pageNumber="12" value="122.0">122</number>
specimens
</specimenCount>
from BIOICE sample 
<number id="6F721D4BB07F894E41BBC4B6D870FA31" box="[237,296,1501,1525]" pageId="11" pageNumber="12" value="3282.0">3282</number>
, 
<collectingCountry id="A1AD4FFCB07F894E4065C4B6D8DDFA31" box="[307,389,1501,1525]" name="Iceland" pageId="11" pageNumber="12">Iceland</collectingCountry>
Basin, at 
<quantity id="1E42A289B07F894E40A0C4B6DB15FA31" box="[502,589,1501,1525]" metricMagnitude="3" metricUnit="m" metricValue="1.811" pageId="11" pageNumber="12" unit="m" value="1811.0">1811 m</quantity>
