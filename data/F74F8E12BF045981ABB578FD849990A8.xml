<taxonomicName id="77978D70992A44C6F76EC8DB9F8F3717" class="Dicotyledoneae" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Fagales" pageId="2" pageNumber="95" phylum="Angiospermae" rank="family">Fagaceae</taxonomicName>
