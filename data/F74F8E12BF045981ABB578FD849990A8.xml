<taxonomicName id="E3313775A165D6C95427A80B52B4B1DD" class="Equisetopsida" family="Aristolochiaceae" genus="A." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="A. utriformis" order="Piperales" pageId="0" pageNumber="93" phylum="Magnoliophyta" rank="species" species="utriformis">
