<paragraph id="8B947A5ECB55C264CEA7F933FC7BC7D6" blockId="73.[151,1436,192,2018]" box="[198,1011,1752,1778]" pageId="73" pageNumber="74">
<emphasis id="B95FA64CCB55C264CEA7F933FEDDC7D5" box="[198,341,1752,1777]" italics="true" pageId="73" pageNumber="74">Distribution</emphasis>
. 
<collectingCountry id="F33C3ACECB55C264CF0DF933FE63C7D6" box="[364,491,1752,1778]" name="Costa Rica" pageId="73" pageNumber="74">Costa Rica</collectingCountry>
(
<collectingRegion id="49EFB4BCCB55C264CF9AF933FDEBC7D6" box="[507,611,1752,1778]" country="Costa Rica" name="Alajuela" pageId="73" pageNumber="74">Alajuela</collectingRegion>
