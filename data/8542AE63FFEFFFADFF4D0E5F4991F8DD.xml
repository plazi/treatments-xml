<number id="BB230D52FFEFFFADFA4A0CD64FECFBBD" box="[1424,1436,1006,1030]" pageId="1" pageNumber="74" value="3.0">3</number>
<number id="BB230D52FFEFFFADFF4D0B2A4AC6FB91" box="[151,182,1042,1066]" pageId="1" pageNumber="74" value="68.0">68</number>
