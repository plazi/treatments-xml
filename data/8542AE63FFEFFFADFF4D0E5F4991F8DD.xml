<number id="BB230D52FFEFFFADFAED0CD64F34FBBD" box="[1335,1348,1006,1030]" pageId="1" pageNumber="74" value="2.0">2</number>
<number id="BB230D52FFEFFFADFA910CD64F19FBBD" box="[1355,1385,1006,1030]" pageId="1" pageNumber="74" value="94.0">94</number>
