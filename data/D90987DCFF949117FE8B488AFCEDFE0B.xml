<paragraph id="511F36CAFF949111FEBB49FAFBBEF9E7" blockId="3.[264,1324,857,1673]" pageId="3" pageNumber="4">
FEMALE: (n = 
<number id="E76824EDFF949111FE7B49FAFDDBFC46" box="[504,520,897,923]" pageId="3" pageNumber="4" value="4.0">4</number>
) Body fusiform, 
<number id="E76824EDFF949111FD5449FAFCD0FC46" box="[727,771,897,923]" pageId="3" pageNumber="4" value="210.0">210</number>
(
<number id="E76824EDFF949111FC9749FAFCAEFC46" box="[788,893,897,923]" pageId="3" pageNumber="4" value="205.5" valueMax="220.0" valueMin="191.0">191–220</number>
) long, 
<number id="E76824EDFF949111FC5049FAFC23FC46" box="[979,1008,897,923]" pageId="3" pageNumber="4" value="69.0">69</number>
(
<number id="E76824EDFF949111FB8249FAFB9EFC46" box="[1025,1101,897,923]" pageId="3" pageNumber="4" value="68.0" valueMax="71.0" valueMin="65.0">65–71</number>
) wide, 
<number id="E76824EDFF949111FB2449FAFB17FC46" box="[1191,1220,897,923]" pageId="3" pageNumber="4" value="63.0">63</number>
(
<number id="E76824EDFF949111FB5549FAFAF1FC46" box="[1238,1314,897,923]" pageId="3" pageNumber="4" value="62.5" valueMax="64.0" valueMin="61.0">61–64</number>
) thick; light yellow. Gnathosoma 
<number id="E76824EDFF949111FD0B49D3FD76FC1F" box="[648,677,936,962]" pageId="3" pageNumber="4" value="30.0">30</number>
(
<number id="E76824EDFF949111FD3549D3FCD7FC1F" box="[694,772,936,962]" pageId="3" pageNumber="4" value="30.5" valueMax="32.0" valueMin="29.0">29–32</number>
), projecting obliquely down. 
<emphasis id="63D4EAD8FF949111FBDD49D3FAF8FC1F" bold="true" box="[1118,1323,936,962]" pageId="3" pageNumber="4">Prodorsal shield</emphasis>
<number id="E76824EDFF949111FE8B49ABFEF6FC37" box="[264,293,976,1002]" pageId="3" pageNumber="4" value="46.0">46</number>
(
<number id="E76824EDFF949111FEBA49ABFE56FC37" box="[313,389,976,1002]" pageId="3" pageNumber="4" value="46.0" valueMax="47.0" valueMin="45.0">45–47</number>
) long, 
<number id="E76824EDFF949111FE6349ABFE2EFC37" box="[480,509,976,1002]" pageId="3" pageNumber="4" value="50.0">50</number>
(
<number id="E76824EDFF949111FD9149ABFD8EFC37" box="[530,605,976,1002]" pageId="3" pageNumber="4" value="49.5" valueMax="51.0" valueMin="48.0">48–51</number>
) wide; subtriangular; frontal lobe broad; smooth. Scapular tubercles on rear margin, 
<number id="E76824EDFF949111FDB0498CFD83FBCC" box="[563,592,1015,1041]" pageId="3" pageNumber="4" value="35.0">35</number>
(
<number id="E76824EDFF949111FDE3498CFD7FFBCC" box="[608,684,1015,1041]" pageId="3" pageNumber="4" value="35.0" valueMax="36.0" valueMin="34.0">34–36</number>
