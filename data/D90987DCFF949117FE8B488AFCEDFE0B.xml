<emphasis id="63D4EAD8FF949111FC3A4983FC02FBCC" box="[953,977,1016,1041]" italics="true" pageId="3" pageNumber="4">sc</emphasis>
) 
<number id="E76824EDFF949111FC61498CFC22FBCC" box="[994,1009,1015,1041]" pageId="3" pageNumber="4" value="8.0">8</number>
(
<number id="E76824EDFF949111FB82498CFBFCFBCC" box="[1025,1071,1015,1041]" pageId="3" pageNumber="4" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
