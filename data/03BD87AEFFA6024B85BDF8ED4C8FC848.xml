<collectingCountry id="F3037628FFA602488007F8A64C7CCCAA" box="[1325,1384,1913,1935]" name="Brazil" pageId="1" pageNumber="122">Brazil</collectingCountry>
(
<figureCitation id="132F2A3DFFA60248805FF8A649EECC88" captionStart="FIGURE 16" captionStartId="24.[151,250,1349,1370]" captionTargetBox="[164,1422,205,1299]" captionTargetId="figure@24.[151,1436,193,1327]" captionTargetPageId="24" captionText="FIGURE 16. Distribution of Nephepeltia species." httpUri="https://zenodo.org/record/227952/files/figure.png" pageId="1" pageNumber="122">
