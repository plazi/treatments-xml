<taxonomicName id="C9A14D0DFFE2FF95FE80E10FAD3FFEFE" authority="Fabricius, 1775: 42" authorityName="Fabricius" authorityPageNumber="42" authorityYear="1775" box="[345,596,325,345]" class="Insecta" family="Cetoniidae" genus="Cetonia" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="177" phylum="Arthropoda" rank="genus">
