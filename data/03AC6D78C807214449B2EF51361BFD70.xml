; PW 
<quantity id="4CFD718BC80621444BC9E8B2363DFE49" box="[748,851,476,501]" metricMagnitude="-4" metricUnit="m" metricValue="5.4" pageId="7" pageNumber="458" unit="mm" value="0.54">0.54 mm</quantity>
; PL 
<quantity id="4CFD718BC80621444AA3E8B23683FE49" box="[902,1005,476,500]" metricMagnitude="-4" metricUnit="m" metricValue="2.7" pageId="7" pageNumber="458" unit="mm" value="0.27">0.27 mm</quantity>
; EL 
<quantity id="4CFD718BC80621444D07E8B231E7FE49" box="[1058,1161,476,500]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="7" pageNumber="458" unit="mm" value="1.2">1.20 mm</quantity>
