<taxonomicName id="9F964D71D11D5057FD34DC744F08AF1D" baseAuthorityName="Uvarov" baseAuthorityYear="1916" box="[737,850,813,836]" class="Insecta" family="Tettigoniidae" genus="Uvarovistia" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="157" phylum="Arthropoda" rank="species" species="satunini">U. satunini</taxonomicName>
</emphasis>
group
