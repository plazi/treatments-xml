<taxonomicName id="9F964D71D11D5057FA88DB0C49C8A834" baseAuthorityName="Uvarov" baseAuthorityYear="1934" box="[1373,1426,1109,1133]" class="Insecta" family="Tettigoniidae" genus="Uvarovistia" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="157" phylum="Arthropoda" rank="species" species="iraka">
<emphasis id="6AE2EAE0D11D5057FA88DB0C49C8A834" box="[1373,1426,1109,1133]" italics="true" pageId="5" pageNumber="157">iraka</emphasis>
</taxonomicName>
