<taxonomicName id="48E161A1FFB4FFAFFF113CB9FDD0ED85" authority="Stal, 1859" authorityName="Stal" authorityYear="1859" box="[151,545,1221,1247]" class="Insecta" family="Reduviidae" genus="Brontostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="42" phylum="Arthropoda" rank="species" species="basalis">
