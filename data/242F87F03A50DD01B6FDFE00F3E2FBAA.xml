<taxonomicName id="6B864D653A50DD01B417FC18F03AFB82" authorityName="Kociolek &amp; Kulikovskiy &amp; Solak" authorityYear="2013" box="[637,791,1004,1030]" class="Bacillariophyceae" family="Gomphonemataceae" genus="Gomphoneis" higherTaxonomySource="GBIF" kingdom="Chromista" order="Cymbellales" pageId="10" pageNumber="11" phylum="Ochrophyta" rank="species" species="witkowskii">
