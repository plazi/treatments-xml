; width of head including eyes 
<quantity id="4CC59B651C4D3476542BFDE2FE459E69" box="[332,509,534,560]" metricMagnitude="-3" metricUnit="m" metricValue="1.26" metricValueMax="1.32" metricValueMin="1.2" pageId="7" pageNumber="8" unit="mm" value="1.26" valueMax="1.32" valueMin="1.2">1.20–1.32 mm</quantity>
; length of vertex 
<quantity id="4CC59B651C4D347657A8FDE2FCC79E69" box="[719,895,534,560]" metricMagnitude="-4" metricUnit="m" metricValue="5.4" metricValueMax="6.0" metricValueMin="4.8" pageId="7" pageNumber="8" unit="mm" value="0.54" valueMax="0.6" valueMin="0.48">0.48–0.60 mm</quantity>
