; width of pronotum 
<quantity id="4CC59B651C4D34765472FDC9FE739E0E" box="[277,459,573,599]" metricMagnitude="-3" metricUnit="m" metricValue="1.18" metricValueMax="1.24" metricValueMin="1.12" pageId="7" pageNumber="8" unit="mm" value="1.18" valueMax="1.24" valueMin="1.12">1.12–1.24 mm</quantity>
