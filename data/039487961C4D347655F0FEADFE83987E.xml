<quantity id="4CC59B651C4D34765451FD90FE5F9E27" box="[310,487,612,638]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" metricValueMax="4.2" metricValueMin="3.8" pageId="7" pageNumber="8" unit="mm" value="0.4" valueMax="0.42" valueMin="0.38">0.38–0.42 mm</quantity>
