<collectingCountry id="F37842F9FFFAC822FE87FD5FFE29B6A9" box="[331,419,654,680]" name="Finland" pageId="17" pageNumber="168">Finland</collectingCountry>
, 
<collectingCountry id="F37842F9FFFAC822FE62FD5FFD9DB6A9" box="[430,535,654,680]" name="Germany" pageId="17" pageNumber="168">Germany</collectingCountry>
, 
<collectingCountry id="F37842F9FFFAC822FDEEFD5FFD1DB6A9" box="[546,663,654,680]" name="Greenland" pageId="17" pageNumber="168">Greenland</collectingCountry>
