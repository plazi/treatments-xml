<paragraph id="4E7336856B18FFE2FF5C1F8EFD9CDA9B" blockId="0.[151,708,1980,2033]" box="[151,540,1980,2003]" pageId="0" pageNumber="81">
<taxonomicName id="89CC4D066B18FFE2FF5C1F8EFEE4DA9B" authority="Kuoh" authorityName="Kuoh" box="[151,356,1980,2003]" class="Insecta" family="Cicadellidae" genus="Parafagocyba" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="81" phylum="Arthropoda" rank="genus">
