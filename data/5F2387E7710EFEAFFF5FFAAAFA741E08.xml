<taxonomicName id="108A4D72710EFEAFFAD7FA43FA741DC6" authorityName="Tillyard" authorityYear="1916" box="[1311,1436,1510,1531]" class="Insecta" family="Myrmeleontidae" genus="Periclystus" kingdom="Animalia" order="Neuroptera" pageId="269" pageNumber="270" phylum="Arthropoda" rank="species" species="aureolatus">
