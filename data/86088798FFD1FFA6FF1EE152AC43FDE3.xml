<taxonomicName id="C9A14D0DFFD1FFA6FD36E253AC3FFD8B" authorityName="Linnaeus" authorityYear="1758" box="[751,852,538,557]" class="Insecta" family="Scarabaeidae" genus="Scarabaeus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="162" phylum="Arthropoda" rank="genus">
<emphasis id="3CD5EA9CFFD1FFA6FD36E253AC3FFD8B" box="[751,852,538,557]" italics="true" pageId="18" pageNumber="162">Scarabaeus</emphasis>
</taxonomicName>
