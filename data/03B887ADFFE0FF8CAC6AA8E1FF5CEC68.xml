<taxonomicName id="4C114D38FFE2FF82AE6AA874FDBBED63" box="[383,607,1542,1564]" class="Insecta" family="Miridae" genus="Cyrtorhinus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="286" phylum="Arthropoda" rank="species" species="undefined-A">
<emphasis id="B965EAA9FFE2FF82AE6AA874FE17ED63" box="[383,499,1542,1563]" italics="true" pageId="6" pageNumber="286">Cyrtorhinus</emphasis>
species. A
</taxonomicName>
, B, 
<taxonomicName id="4C114D38FFE2FF82AD9EA874FC91ED63" authorityName="Reuter" authorityYear="1885" box="[651,885,1542,1563]" class="Insecta" family="Miridae" genus="Cyrtorhinus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="286" phylum="Arthropoda" rank="species" species="lividipennis">
<emphasis id="B965EAA9FFE2FF82AD9EA874FC91ED63" box="[651,885,1542,1563]" italics="true" pageId="6" pageNumber="286">Cyrtorhinus lividipennis</emphasis>
