, pd 
<number id="3D9C24E5FF9B3D51FDCEC68DF873FE08" box="[567,582,476,500]" pageId="44" pageNumber="545" value="1.0">1</number>
; patella I rv 
<number id="3D9C24E5FF9B3D51FD2AC68DF8D7FE08" box="[723,738,476,500]" pageId="44" pageNumber="545" value="1.0">1</number>
; tibia I pv 
<number id="3D9C24E5FF9B3D51FCA2C68DF95EFE08" box="[859,875,476,500]" pageId="44" pageNumber="545" value="3.0">3</number>
, rv 
<number id="3D9C24E5FF9B3D51FC6CC68DF990FE08" box="[917,933,476,500]" pageId="44" pageNumber="545" value="3.0">3</number>
