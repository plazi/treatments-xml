<number id="3D9C24E5FF9B3D51FBC6C551FE59FDE4" box="[1087,1132,512,536]" pageId="44" pageNumber="545" value="0.5" valueMax="1.0" valueMin="0.0">0–1</number>
; femur IV md 
<number id="3D9C24E5FF9B3D51FAF6C551FF28FDE4" box="[1295,1309,512,536]" pageId="44" pageNumber="545" value="1.0">1</number>
