<emphasis id="B953E614D83DFFA9BF96E0E62952FB51" bold="true" box="[192,324,1155,1179]" pageId="41" pageNumber="40">Remarks.</emphasis>
<taxonomicName id="4C274185D83DFFA9BE1AE0E12A13FB51" authorityName="Gordon and Hanley" authorityYear="2017" box="[332,517,1156,1179]" class="Insecta" family="Coccinellidae" genus="Neaporia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="40" phylum="Arthropoda" rank="species" species="sonia">
<emphasis id="B953E614D83DFFA9BE1AE0E12A13FB51" box="[332,517,1156,1179]" italics="true" pageId="41" pageNumber="40">Neaporia sonia</emphasis>
</taxonomicName>
