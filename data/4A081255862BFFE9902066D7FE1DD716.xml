holotype except 
<emphasis id="F0D57F518636FFE992A36316FC18D050" box="[898,1010,867,888]" italics="true" pageId="18" pageNumber="103">U1, U2, U3</emphasis>
and 
<emphasis id="F0D57F518636FFE9953E6316FBD0D050" box="[1055,1082,867,888]" italics="true" pageId="18" pageNumber="103">P2</emphasis>
