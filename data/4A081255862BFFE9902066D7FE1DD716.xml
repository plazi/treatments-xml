<taxonomicName id="05A1D8C08635FFEA91E865F2FE2AD6B4" authority="Jørgen Berge, 2003" authorityName="Jørgen Berge" authorityYear="2003" box="[201,448,1415,1436]" class="Malacostraca" family="Stegocephalidae" genus="Stegocephaloides" kingdom="Animalia" order="Amphipoda" pageId="17" pageNumber="102" phylum="Arthropoda" rank="species" species="gunnae" status="n.sp.">
