<taxonomicName id="60F287A67069FFB5B1FF4764FB8BF80F" authorityName="Schenkel" authorityYear="1950" box="[993,1097,1901,1925]" class="Arachnida" family="Cybaeidae" genus="Cybaeus" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="267" phylum="Arthropoda" rank="species" species="grizzlyi">
