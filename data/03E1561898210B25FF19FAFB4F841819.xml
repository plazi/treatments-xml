<taxonomicName id="4C489C8D98200B25FE6DFE1A4F021BB9" authorityName="Melsheimer" authorityYear="1844" box="[436,577,482,506]" class="Insecta" family="Heteroceridae" genus="Heterocerus" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="26" phylum="Arthropoda" rank="species" species="undatus">
