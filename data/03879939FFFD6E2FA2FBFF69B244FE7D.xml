<taxonomicName id="4C2E53ACFFFD6E2FA373FEBEB2B8FECA" authority="Brothers, 1974" authorityName="Brothers" authorityYear="1974" box="[287,721,320,342]" class="Insecta" family="Plumariidae" genus="Plumaroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="65" phylum="Arthropoda" rank="species" species="andalgalensis">
