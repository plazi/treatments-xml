<taxonomicName id="4C22AFCEFFCEB91E3CB51FD42D36B993" box="[944,1101,1714,1736]" class="Actinopterygii" family="Claroteidae" genus="Notoglanidium" kingdom="Animalia" order="Siluriformes" pageId="12" pageNumber="177" phylum="Chordata" rank="species" species="boutchangai">
