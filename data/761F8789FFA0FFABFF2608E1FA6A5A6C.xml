<heading id="A54181F3FFA0FFAAFF2608E1FDE05969" bold="true" box="[151,519,511,537]" fontSize="11" level="1" pageId="6" reason="1">
<emphasis id="CCC2EA8DFFA0FFAAFF2608E1FDE05969" bold="true" box="[151,519,511,537]" pageId="6">
<number id="487E24B8FFA0FFAAFF2608E1FF405969" box="[151,167,511,537]" pageId="6" value="2.0">2</number>
. 
<taxonomicName id="39B64D1CFFA0FFAAFF0508E1FE445969" box="[180,419,511,537]" class="Insecta" family="Cicadellidae" genus="Oniella" kingdom="Animalia" order="Hemiptera" pageId="6" phylum="Arthropoda" rank="species" species="shaanxiana" status="sp. nov.">
<emphasis id="CCC2EA8DFFA0FFAAFF0508E1FE445969" bold="true" box="[180,419,511,537]" italics="true" pageId="6">Oniella shaanxiana</emphasis>
</taxonomicName>
