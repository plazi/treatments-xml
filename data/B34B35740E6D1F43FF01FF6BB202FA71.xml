<taxonomicName id="FCE2FFE10E6D1F43FC72FEDBB223FEDD" authority="Smith &amp; Sidik, 1998" authorityName="Smith &amp; Sidik" authorityYear="1998" box="[996,1381,358,383]" class="Reptilia" family="Cylindrophiidae" genus="Cylindrophis" kingdom="Animalia" order="Squamata" pageId="1" pageNumber="2" phylum="Chordata" rank="species" species="yamdena">
