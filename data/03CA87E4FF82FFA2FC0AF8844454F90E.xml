<taxonomicName id="4C634D71FF82FFA3FC0AF88442C6FA61" authorityName="Rea" authorityYear="1922" box="[988,1193,1829,1853]" class="Agaricomycetes" higherTaxonomySource="GBIF" kingdom="Fungi" order="Aphyllophorales" pageId="6" pageNumber="378" phylum="Basidiomycota" rank="order">Aphyllophorales</taxonomicName>
