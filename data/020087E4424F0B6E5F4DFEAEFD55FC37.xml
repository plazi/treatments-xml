, 
<geoCoordinate id="EF9D5035424F0B6E5CFAFE2EFCB6FE17" box="[703,812,412,438]" direction="north" orientation="latitude" pageId="14" pageNumber="15" precision="555" value="39.43">
<number id="3C6124D5424F0B6E5CFAFE2EFC9EFE17" box="[703,772,412,438]" pageId="14" pageNumber="15" value="39.43">39.43</number>
° N
</geoCoordinate>
, 
<geoCoordinate id="EF9D5035424F0B6E5D7DFE2EFC00FE17" box="[824,922,412,438]" direction="west" orientation="longitude" pageId="14" pageNumber="15" precision="5555" value="-70.6">
<number id="3C6124D5424F0B6E5D7DFE2EFCF4FE17" box="[824,878,412,438]" pageId="14" pageNumber="15" value="70.6">70.6</number>
