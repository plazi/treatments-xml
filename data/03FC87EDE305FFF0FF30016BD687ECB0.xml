<taxonomicName id="4C554D78E306FFF1FEF0010DD066E9D0" authorityName="Guignot" authorityYear="1957" box="[343,529,1655,1677]" class="Insecta" family="Dytiscidae" genus="Agabus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="47" phylum="Arthropoda" rank="species" species="turcmenus">
