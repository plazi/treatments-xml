<taxonomicName id="605981C57E6B876903BF1F6D027AB0C6" genus="P." lsidName="P. torresi" pageId="0" pageNumber="1" rank="species" species="torresi">
<emphasis id="7C6CF832A9C50EC469D665C9F8C3EF24" italics="true" pageId="0" pageNumber="1">P. torresi</emphasis>
</taxonomicName>
sp. nov. Photographs of preserved holotypes of 
<emphasis id="608E5BD0AF625D67AA71EA07A94CBB0E" bold="true" pageId="0" pageNumber="1">A</emphasis>
<taxonomicName id="FA8C416AF6DFE2BE495B64A217037062" genus="P." lsidName="P. teslai" pageId="0" pageNumber="1" rank="species" species="teslai">
<emphasis id="7499AF49715C0B7E33B1D499EC508A9C" italics="true" pageId="0" pageNumber="1">P. teslai</emphasis>
</taxonomicName>
(QCAZ 46213, male) and 
<emphasis id="20F7AADBD114473316A52800442A8AC2" bold="true" pageId="0" pageNumber="1">B</emphasis>
<taxonomicName id="BDAE5833CA32227164F52B9242131E1B" genus="P." lsidName="P. torresi" pageId="0" pageNumber="1" rank="species" species="torresi">
<emphasis id="54756C11EFA921EA1E4F3AE336EA583A" italics="true" pageId="0" pageNumber="1">P. torresi</emphasis>
