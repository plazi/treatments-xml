<taxonomicName id="4C774D081C0233614291C042A7746D7A" authorityName=": Billard" authorityYear="1906" box="[1204,1393,1323,1349]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="19" pageNumber="431" phylum="Cnidaria" rank="species" species="gayi">
