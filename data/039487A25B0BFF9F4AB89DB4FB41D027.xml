<taxonomicName id="4C3D4D375B0BFF9E4B309C29FDAAD151" authorityName="Published" authorityYear="2007" box="[287,558,1834,1859]" class="Insecta" family="Rangomaramidae" genus="Colonomyia" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="brasiliana">
