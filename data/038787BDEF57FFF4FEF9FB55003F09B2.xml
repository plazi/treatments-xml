<taxonomicName id="4C2E4D28EF57FFF4FD5FF9A807E1099A" authority="Banks" authorityName="Banks" box="[686,970,1641,1667]" class="Magnoliopsida" family="Linaceae" genus="Linum" kingdom="Plantae" order="Malpighiales" pageId="9" pageNumber="10" phylum="Tracheophyta" rank="species" species="pubescens">
