<paragraph id="8BEBD46B105B057F8F65FA64FBED01BD" blockId="36.[855,1074,1436,1460]" box="[855,1074,1436,1460]" pageId="36" pageNumber="37">
<heading id="D0A36307105B057F8F65FA64FBED01BD" box="[855,1074,1436,1460]" centered="true" fontSize="36" level="3" pageId="36" pageNumber="37" reason="7">
