<taxonomicName id="4C024D5BFFE3FFA3DE2E279B51BFF97A" authority="(Koenike, 1895)" authorityName="Koenike" authorityYear="1895" baseAuthorityName="Koenike" baseAuthorityYear="1895" box="[346,697,1709,1734]" class="Arachnida" family="Pionidae" genus="Piona" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="403" phylum="Arthropoda" rank="species" species="discrepans">
