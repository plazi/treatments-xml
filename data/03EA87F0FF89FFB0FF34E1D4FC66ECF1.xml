<number id="3D8B24C1FF87FFB0FF4FE53AFED4EA24" box="[236,267,548,572]" pageId="8" pageNumber="9" value="45.0">45</number>
; ovipositor length, 
<number id="3D8B24C1FF87FFB0FE46E53AFDDBEA24" box="[485,516,548,572]" pageId="8" pageNumber="9" value="35.0">35</number>
; third valvula length, 
<number id="3D8B24C1FF87FFB0FD58E53AFCD4EA24" box="[763,779,548,572]" pageId="8" pageNumber="9" value="6.0">6</number>
; mid tibia length, 
<number id="3D8B24C1FF87FFB0FC79E53AFC26EA24" box="[986,1017,548,572]" pageId="8" pageNumber="9" value="82.0">82</number>
; mid basitarsus length, 
<number id="3D8B24C1FF87FFB0FAA7E53AFAFCEA24" box="[1284,1315,548,572]" pageId="8" pageNumber="9" value="24.0">24</number>
