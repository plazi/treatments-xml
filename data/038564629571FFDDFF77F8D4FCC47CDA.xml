<taxonomicName id="4C2CAEF79572FFDDFC36FE3CFB757C82" authority="Gray, 1860" authorityName="Macdonaldii Gray" authorityYear="1860" box="[982,1199,404,429]" class="Gastropoda" family="Athoracophoridae" genus="Aneitea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="22" pageNumber="223" phylum="Mollusca" rank="genus">
