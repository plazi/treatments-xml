<emphasis id="B96AEAE0FFDFFFCDFC92F84DA2777CBA" bold="true" box="[805,817,1958,1979]" pageId="2" pageNumber="161">1</emphasis>
, dorsal habitus; 
<emphasis id="B96AEAE0FFDFFFCDFC6DF84DA2A07CBA" bold="true" box="[986,998,1958,1979]" pageId="2" pageNumber="161">2</emphasis>
, ventral habitus; 
<emphasis id="B96AEAE0FFDFFFCDFB21F84DA5E57CBA" bold="true" box="[1174,1187,1958,1979]" pageId="2" pageNumber="161">3</emphasis>
