<subSubSection id="3899CDDB68361D5BFF77FEBCFE48401F" box="[151,503,271,298]" pageId="5" pageNumber="206" type="nomenclature">
<paragraph id="703C9E5068361D5BFF77FEBCFE48401F" blockId="5.[151,503,271,298]" box="[151,503,271,298]" pageId="5" pageNumber="206">
<heading id="2B74293C68361D5BFF77FEBCFE48401F" bold="true" box="[151,503,271,298]" fontSize="11" level="1" pageId="5" pageNumber="206" reason="1">
<emphasis id="42F7424268361D5BFF77FEBCFE48401F" bold="true" box="[151,503,271,298]" pageId="5" pageNumber="206">
<taxonomicName id="B783E5D368361D5BFF77FEBCFE96401C" baseAuthorityName="Sars" baseAuthorityYear="1835" box="[151,297,271,297]" class="Polychaeta" family="Nereididae" genus="Alitta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="5" pageNumber="206" phylum="Annelida" rank="species" species="virens">
