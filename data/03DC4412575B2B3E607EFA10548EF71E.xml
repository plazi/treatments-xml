<paragraph id="8BCAF504575B2B3F607EFA105741F7AA" blockId="4.[559,825,1499,1524]" box="[559,825,1499,1524]" pageId="4" pageNumber="1861">
<heading id="D0824268575B2B3F607EFA105741F7AA" box="[559,825,1499,1524]" centered="true" fontSize="36" level="2" pageId="4" pageNumber="1861" reason="7">
<taxonomicName id="4C758E87575B2B3F607EFA1056B6F7AA" authority="Monné &amp; Monné, 2011" authorityName="Monné &amp; Monné" authorityYear="2011" box="[559,718,1499,1523]" class="Insecta" family="Cerambycidae" genus="Acanthocornis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="1861" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B9012916575B2B3F607EFA1056B6F7AA" bold="true" box="[559,718,1499,1523]" italics="true" pageId="4" pageNumber="1861">Acanthocornis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A232946D575B2B3F6085FA175741F7AA" box="[724,825,1500,1524]" pageId="4" pageNumber="1861" rank="genus">gen. nov.</taxonomicNameLabel>
