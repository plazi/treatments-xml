<taxonomicName id="4C4EDDAE4169FFF6BF8E4712FE70FBC3" authority="Malicky &amp; Chantaramongkol 2000" authorityName="Malicky &amp; Chantaramongkol" authorityYear="2000" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="342" phylum="Arthropoda" rank="species" species="battos">
