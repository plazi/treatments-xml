<caption id="30F4E7A0627E622386C241D698D9F83F" ID-DOI="http://doi.org/10.5281/zenodo.3814393" ID-Zenodo-Dep="3814393" httpUri="https://zenodo.org/record/3814393/files/figure.png" pageId="19" pageNumber="20" startId="19.[189,232,1966,1992]" targetBox="[196,1398,843,1926]" targetPageId="19">
