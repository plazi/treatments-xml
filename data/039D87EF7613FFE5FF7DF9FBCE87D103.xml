<quantity id="4CCC9B1C7613FFE5FAE2FBA7C833D0B3" box="[1288,1361,708,732]" metricMagnitude="2" metricUnit="m" metricValue="2.17" pageId="6" pageNumber="59" unit="m" value="217.0">217 m</quantity>
