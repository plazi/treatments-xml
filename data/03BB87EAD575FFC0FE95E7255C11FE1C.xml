<taxonomicName id="4C124D7FD576FFC1FE03E26C5C65F9E7" box="[414,607,1598,1621]" class="Insecta" family="Scatopsidae" genus="Anapausis" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="mourei">
<emphasis id="B966EAEED576FFC1FE03E26C5C65F9E7" box="[414,607,1598,1621]" italics="true" pageId="18" pageNumber="19">Anapausis mourei</emphasis>
