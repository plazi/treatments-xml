<paragraph id="A86036A8D642B2636EE0FD7AFDE04627" blockId="9.[151,518,764,791]" box="[151,518,764,791]" pageId="9" pageNumber="360">
<heading id="F32881C4D642B2636EE0FD7AFDE04627" bold="true" box="[151,518,764,791]" fontSize="11" level="1" pageId="9" pageNumber="360" reason="1">
<taxonomicName id="6FDF4D2BD642B2636EE0FD7AFDE04627" authority="(Morley)" baseAuthorityName="Morley" baseAuthorityYear="1926" box="[151,518,764,791]" class="Insecta" family="Ichneumonidae" genus="Venturia" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="360" phylum="Arthropoda" rank="species" species="crassicaput">
