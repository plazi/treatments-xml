<taxonomicName id="4C714D30FF9F8927FCAFFCD5CD61FC8D" authority="(Taylor, 1955)" baseAuthorityName="Taylor" baseAuthorityYear="1955" class="Amphibia" family="Phyllomedusidae" genus="Agalychnis" kingdom="Animalia" order="Anura" pageId="112" pageNumber="113" phylum="Chordata" rank="species" species="saltator">
<emphasis id="B905EAA1FF9F8927FCAFFCD5CA09FCEE" box="[883,1120,780,803]" italics="true" pageId="112" pageNumber="113">Agalychnis saltator</emphasis>
