<taxonomicName id="4C714D30FF9F8927FC8AFD08CDE5FCC8" authority="(Duellman and Trueb, 1967)" baseAuthorityName="Duellman and Trueb" baseAuthorityYear="1967" class="Amphibia" family="Phyllomedusidae" genus="Agalychnis" kingdom="Animalia" order="Anura" pageId="112" pageNumber="113" phylum="Chordata" rank="species" species="litodryas">
<emphasis id="B905EAA1FF9F8927FC8AFD08CA21FD25" box="[854,1096,721,744]" italics="true" pageId="112" pageNumber="113">Agalychnis litodryas</emphasis>
(Duellman and Trueb, 1967)
</taxonomicName>
; 
<taxonomicName id="4C714D30FF9F8927FC46FD36CD32FCEE" authority="(Dumeril, 1853)" baseAuthorityName="Dumeril" baseAuthorityYear="1853" class="Amphibia" family="Phyllomedusidae" genus="Agalychnis" kingdom="Animalia" order="Anura" pageId="112" pageNumber="113" phylum="Chordata" rank="species" species="moreletii">
<emphasis id="B905EAA1FF9F8927FC46FD36CAE4FCCB" box="[922,1165,751,774]" italics="true" pageId="112" pageNumber="113">Agalychnis moreletii</emphasis>
