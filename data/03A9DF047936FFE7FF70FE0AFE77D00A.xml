<number id="3DC87C357928FFE7FDF4FF0DFDABD38F" box="[531,615,189,215]" pageId="16" pageNumber="17" value="0.1025">0.1025</number>
FL + 
<number id="3DC87C357928FFE7FD4EFF0DFD30D38F" box="[681,764,189,215]" pageId="16" pageNumber="17" value="0.0712">0.0712</number>
MW – 
<number id="3DC87C357928FFE7FCB6FF0DFC6ED38F" box="[849,930,189,215]" pageId="16" pageNumber="17" value="0.0942">0.0942</number>
<collectionCode id="ED11F6D77928FFE7FC4CFF0DFC21D38F" box="[939,1005,189,215]" pageId="16" pageNumber="17">SPSP</collectionCode>
+ 
<number id="3DC87C357928FFE7FBE3FF0DFB9BD38F" box="[1028,1111,189,215]" pageId="16" pageNumber="17" value="8.1089">8.1089</number>
for 
<taxonomicName id="4C0015917928FFE7FB6DFF0DFA87D38E" box="[1162,1355,189,214]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="densopilosum">
<emphasis id="B974B2007928FFE7FB6DFF0DFA87D38E" box="[1162,1355,189,214]" italics="true" pageId="16" pageNumber="17">T. densopilosum</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C0015917928FFE7FA61FF0DFEC5D3A3" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="feroxoide">
<emphasis id="B974B2007928FFE7FA61FF0DFEC5D3A3" italics="true" pageId="16" pageNumber="17">T. feroxoide</emphasis>
</taxonomicName>
workers resulted in 
<number id="3DC87C357928FFE7FDE2FF52FDFFD3A4" box="[517,563,226,252]" pageId="16" pageNumber="17" value="100.0">100</number>
% success, and each case (
<number id="3DC87C357928FFE7FC9EFF52FC55D3A4" box="[889,921,226,252]" pageId="16" pageNumber="17" value="56.0">56</number>
