<taxonomicName id="4C0015917928FFE7FE46FEB8FD92D279" box="[417,606,264,289]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="densopilosum">
<emphasis id="B974B2007928FFE7FE46FEB8FD92D279" box="[417,606,264,289]" italics="true" pageId="16" pageNumber="17">T. densopilosum</emphasis>
