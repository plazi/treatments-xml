<materialsCitation id="3B348076FFE2030D7646FCAEFA803C2A" collectingDate="2015-12-15" collectorName="Lat" country="Saint Pierre and Miquelon" elevation="450" latitude="-21.303888" location="Armeflhor Station" longLatPrecision="21" longitude="55.519165" municipality="Long" pageId="64" pageNumber="174" specimenCount="15" specimenCount-female="15">
