<taxonomicName id="58B187F0FFB46515509CFC43FC1879B6" authorityName="Mosely. The" authorityYear="1934" box="[738,910,958,982]" class="Insecta" family="Hydroptilidae" genus="Leucotrichia" kingdom="Animalia" order="Trichoptera" pageId="21" pageNumber="20" phylum="Arthropoda" rank="species" species="melleopicta">
<emphasis id="ADC52061FFB46515509CFC43FC1879B6" box="[738,910,958,982]" italics="true" pageId="21" pageNumber="20">L. melleopicta</emphasis>
</taxonomicName>
