<subSubSection id="C342654AFFB38445FF11FB45FF1ABC5E" pageId="56" pageNumber="57" type="distribution">
<paragraph id="8BE736C1FFB38445FF11FB45FF1ABC5E" blockId="56.[151,1437,1253,2034]" pageId="56" pageNumber="57">
<emphasis id="B92CEAD3FFB38445FF11FB45FEF0BDE6" bold="true" box="[151,297,1253,1278]" pageId="56" pageNumber="57">Distribution</emphasis>
. Found in 
<collectingCountry id="F34F7651FFB38445FE3BFB45FDC9BDE6" box="[445,528,1253,1278]" name="Bolivia" pageId="56" pageNumber="57">Bolivia</collectingCountry>
