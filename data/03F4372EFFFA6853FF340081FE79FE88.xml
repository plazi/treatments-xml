<collectionCode id="ED4C1EFDFFFA6852FB1F06F8FAD3F809" LSID="urn:lsid:biocol.org:col:34871" box="[1212,1295,1961,1985]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="28" pageNumber="429">USNM</collectionCode>
