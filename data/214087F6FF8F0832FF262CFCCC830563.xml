<keyLead id="121895D5FF8F0832FF262FB7CC8304AC" pageId="2" pageNumber="593">
<number id="1F2124C7FF8F0832FF262FB7C9BE04CB" box="[151,166,663,685]" pageId="2" pageNumber="593" value="2.0">2</number>
