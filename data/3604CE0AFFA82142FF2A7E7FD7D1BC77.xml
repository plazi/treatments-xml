<taxonomicName id="79AD049FFFA9215DFEF77A06D382BA44" authorityName="♂ Draudt" authorityYear="1950" box="[330,476,1859,1881]" class="Insecta" family="Noctuidae" genus="Miracopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="prodigiosa">
