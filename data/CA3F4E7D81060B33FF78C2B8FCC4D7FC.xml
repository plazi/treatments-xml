<subSubSection id="0A8CACE081070B0CFF28C31DFC43D3AD" pageId="31" pageNumber="182" type="distribution">
<paragraph id="4229FF6B81070B0CFF28C31DFC43D3AD" blockId="31.[151,1437,1397,2034]" pageId="31" pageNumber="182">
<emphasis id="70E2237981070B0CFF28C31DFED5D3F5" bold="true" box="[199,352,1397,1422]" inLineHeading="true" pageId="31" pageNumber="182" reason="1">Distribution.</emphasis>
<taxonomicName id="859684E881070B0CFE83C31DFDB4D3F5" authorityName="Smith" authorityYear="1954" box="[364,513,1397,1422]" class="Actinopterygii" family="Apogonidae" genus="Pseudamiops" kingdom="Animalia" order="Perciformes" pageId="31" pageNumber="182" phylum="Chordata" rank="genus">
