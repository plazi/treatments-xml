<taxonomicName id="4C1F4D624D0AFFFCBED90556FDE5FE25" authorityName="Carter" authorityYear="1875" box="[403,540,359,385]" class="Demospongiae" family="Axinellidae" kingdom="Animalia" order="Halichondrida" pageId="2" pageNumber="3" phylum="Porifera" rank="family">Axinellidae</taxonomicName>
