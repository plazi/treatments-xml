<number id="3DF124F6FFA6FF8FE0E14BC9363F6FAE" box="[762,790,188,212]" pageId="9" pageNumber="10" value="13.0">13</number>
PTT ♀ (CAl, CCa, CGi, CSc, 
<collectionCode id="ED28AE14FFA6FF8FE6694BC831FE6FAE" box="[1138,1239,189,212]" pageId="9" pageNumber="10">MNHNP</collectionCode>
, 
<collectionCode id="ED28AE14FFA6FF8FE6F94BC9301D6FAE" box="[1250,1332,188,212]" pageId="9" pageNumber="10">MRSN</collectionCode>
, 
<collectionCode id="ED28AE14FFA6FF8FE7244BC930B16FAE" box="[1343,1432,188,212]" pageId="9" pageNumber="10">MSNM</collectionCode>
, 
<collectionCode id="ED28AE14FFA6FF8FE28C4B9535C46F82" LSID="urn:lsid:biocol.org:col:34404" box="[151,237,224,248]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34404" name="University of Alberta, E.H. Strickland Entomological Museum" pageId="9" pageNumber="10">UASM</collectionCode>
