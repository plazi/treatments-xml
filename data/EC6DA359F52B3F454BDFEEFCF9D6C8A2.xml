<quantity id="A33CBFAAF5243F45485CEA99FD25C77D" box="[572,705,1521,1557]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" pageId="40" pageNumber="51" unit="mm" value="2.75">2.75 mm</quantity>
