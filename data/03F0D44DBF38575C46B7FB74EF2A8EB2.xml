<taxonomicName id="4C591ED8BF38575C430DF9C5E9BE8E4F" authorityName="Linnaeus" authorityYear="1753" box="[1325,1435,1587,1610]" class="Magnoliopsida" family="Acanthaceae" genus="Avicennia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="1" pageNumber="222" phylum="Tracheophyta" rank="genus">
<emphasis id="B92DB949BF38575C430DF9C5E9BE8E4F" box="[1325,1435,1587,1610]" italics="true" pageId="1" pageNumber="222">Avicennia</emphasis>
</taxonomicName>
