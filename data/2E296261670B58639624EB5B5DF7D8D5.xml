<taxonomicName id="D3F0F7046B103CB9DD3BB504869A9D94" authorityName="Foerster" authorityYear="1856" class="Insecta" family="Scelionidae" genus="Hadronotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hadronotus" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="genus">
<emphasis id="4D8D221215C96B18080AF999C6CC2ED7" italics="true" pageId="0" pageNumber="323">Hadronotus</emphasis>
</taxonomicName>
