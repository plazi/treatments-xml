<taxonomicName id="D1B3F7ABBE02E155AC1AFD34670AFD30" authority="Gates, 1930" authorityName="Gates" authorityYear="1930" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="49" pageNumber="782" phylum="Annelida" rank="species" species="papilio">
