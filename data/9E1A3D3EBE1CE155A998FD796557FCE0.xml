<bibRefCitation id="7222F1D9BE02E155A8ADFD11670AFD30" author="Gates GE" box="[238,365,628,653]" pageId="49" pageNumber="782" pagination="257 - 356" refId="ref33135" refString="Gates GE. 1930. The earthworms of Burma I. Rec Indian Mus. 32: 257 - 356." type="journal article" year="1930">Gates, 1930</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="D1B3F7ABBE02E155A939FD11641DFD30" authority="Chen, 1933" authorityName="Chen" authorityYear="1933" box="[378,634,628,653]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="49" pageNumber="782" phylum="Annelida" rank="species" species="zyosiae">
