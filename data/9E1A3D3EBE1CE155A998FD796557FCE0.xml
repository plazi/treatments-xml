</taxonomicName>
, 
<taxonomicName id="D1B3F7ABBE02E155AB89FFEB67F8FF77" authority="Michaelsen, 1899" authorityName="Michaelsen" authorityYear="1899" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="49" pageNumber="782" phylum="Annelida" rank="species" species="lompobatangensis">
