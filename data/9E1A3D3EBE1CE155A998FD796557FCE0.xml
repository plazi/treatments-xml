</taxonomicName>
, 
<taxonomicName id="D1B3F7ABBE02E155A9AEFFEB64B9FF1A" authority="Chen, 1938" authorityName="Chen" authorityYear="1938" box="[493,734,142,167]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="49" pageNumber="782" phylum="Annelida" rank="species" species="mutus">
<emphasis id="24C7503ABE02E155A9AEFFEB6438FF1A" box="[493,607,142,167]" italics="true" pageId="49" pageNumber="782">Am. mutus</emphasis>
<bibRefCitation id="7222F1D9BE02E155AA26FFEB64B9FF1A" author="Chen Y" box="[613,734,142,167]" pageId="49" pageNumber="782" pagination="375 - 427" refId="ref32442" refString="Chen Y. 1938. Oligochaeta from Hainan, Kwangtung. Contri Biol Lab Sci Soc China, Zool Ser. 12: 375 - 427." type="book chapter" year="1938">Chen, 1938</bibRefCitation>
