<bibRefCitation id="7222F1D9BE02E155A9BCFD11641DFD30" author="Chen Y" box="[511,634,628,653]" pageId="49" pageNumber="782" pagination="178 - 296" refId="ref32347" refString="Chen Y. 1933. A preliminary survey of the earthworms of the Lower Yangtze valley. Contri Biol Lab Sci Soc China, Zool Ser. 9: 178 - 296." type="book chapter" year="1933">Chen, 1933</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="D1B3F7ABBE02E155AAC4FD1165C9FD30" authority="Thai, 1984" authorityName="Thai" authorityYear="1984" box="[647,942,628,653]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="49" pageNumber="782" phylum="Annelida" rank="species" species="infantiloides">
<emphasis id="24C7503ABE02E155AAC4FD116551FD30" box="[647,822,628,653]" italics="true" pageId="49" pageNumber="782">Am. infantiloides</emphasis>
<bibRefCitation id="7222F1D9BE02E155AB7DFD1165C9FD30" author="Thai TB" box="[830,942,628,653]" pageId="49" pageNumber="782" pagination="284 - 288" refId="ref36371" refString="Thai TB. 1984. New data on taxonomy of the genus Pheretima (Oligochaeta, Megascolecidae) of the fauna of Vietnam. Zool J. 63: 284 - 288." type="journal article" year="1984">Thai, 1984</bibRefCitation>
