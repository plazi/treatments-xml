<collectingCountry id="F32F41DEFFB3AD1C713AA86AFC755C1D" box="[887,939,776,797]" name="Laos" pageId="31" pageNumber="2056">Laos</collectingCountry>
, Sumatra and 
<collectingCountry id="F32F41DEFFB3AD1C72FFA846FED25C3A" box="[178,268,804,826]" name="Vietnam" pageId="31" pageNumber="2056">Vietnam</collectingCountry>
