<paragraph id="C48BD4C383FC5DEDED9CEB0AE8527780" pageId="1" pageNumber="2">
<taxonomicName id="BC70FA599E47DB0EAC1EC056F4621D91" class="Insecta" family="Issidae" genus="Tetricodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetricodes polyphemus" order="Hemiptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="polyphemus">Tetricodes polyphemus</taxonomicName>
