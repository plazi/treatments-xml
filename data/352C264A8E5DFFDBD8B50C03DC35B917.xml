<taxonomicName id="7A85ECDF8E5DFFDBD8930A44DB3BBB51" authority="Woolacot, 1954: 38" authorityName="Woolacot" authorityPageNumber="38" authorityYear="1954" box="[212,524,1340,1363]" class="Gastropoda" family="Muricidae" genus="Rhombothais" kingdom="Animalia" order="Neogastropoda" pageId="4" pageNumber="485" phylum="Mollusca" rank="genus">
<emphasis id="8FF14B4E8E5DFFDBD8930A44D87DBB52" box="[212,330,1341,1362]" italics="true" pageId="4" pageNumber="485">Rhombothais</emphasis>
Woolacot, 1954: 38
</taxonomicName>
. 
<typeStatus id="623E29FE8E5DFFDBDA520A45DB74BB51" box="[533,579,1340,1361]" pageId="4" pageNumber="485">Type</typeStatus>
species (by original designation): 
<taxonomicName id="7A85ECDF8E5DFFDBDBCD0A44DC0FBB51" authority="Woolacot, 1954 Recent" authorityName="Woolacot" authorityYear="1954" box="[906,1336,1340,1363]" class="Gastropoda" family="Muricidae" genus="Rhombothais" kingdom="Animalia" order="Neogastropoda" pageId="4" pageNumber="485" phylum="Mollusca" rank="species" species="arbutum">
