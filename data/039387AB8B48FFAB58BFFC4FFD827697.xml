<taxonomicName id="4C3A4D3E8B48FFAB5931FBB1FD387603" box="[281,652,1092,1116]" class="Hexactinellida" family="Hyalonematidae" genus="Hyalonema" kingdom="Animalia" order="Amphidiscosida" pageId="2" pageNumber="416" phylum="Porifera" rank="species" species="poculum" subGenus="Prionema">
