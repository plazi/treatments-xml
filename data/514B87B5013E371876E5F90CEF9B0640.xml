<taxonomicName id="1EE24D20013F3718747FFF60EF6504CF" authority="(Naude)" baseAuthorityName="Naude" box="[525,819,151,178]" class="Insecta" family="Cicadellidae" genus="Cicadulina" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="mbila">
<emphasis id="EB96EAB1013F3718747FFF60EE8604CC" box="[525,720,151,177]" italics="true" pageId="15" pageNumber="16">Cicadulina mbila</emphasis>
(NaudÃ©)
</taxonomicName>
on 
<taxonomicName id="1EE24D20013F3718752FFF60EF9E04CD" box="[861,968,151,176]" class="Liliopsida" family="Poaceae" genus="Zea" kingdom="Plantae" order="Poales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="mays">
<emphasis id="EB96EAB1013F3718752FFF60EF9E04CD" box="[861,968,151,176]" italics="true" pageId="15" pageNumber="16">Zea mays</emphasis>
</taxonomicName>
.
</paragraph>
