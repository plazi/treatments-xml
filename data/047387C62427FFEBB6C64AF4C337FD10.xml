<taxonomicName id="4BDA4D532427FFEBB0984BA4C4DEFD93" authority="Chanabun &amp; Bantaowong &amp; Sutcharit &amp; Tongkerd &amp; James &amp; Panha, 2012" authorityName="Chanabun &amp; Bantaowong &amp; Sutcharit &amp; Tongkerd &amp; James &amp; Panha" authorityYear="2012" class="Clitellata" family="Glossoscolecidae" genus="Glyphidrilus" kingdom="Animalia" order="Haplotaxida" pageId="9" pageNumber="274" phylum="Annelida" rank="species" species="mekongensis" status="new species">
