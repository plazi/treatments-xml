, 
<collectionCode box="[350,410,1823,1846]" collectionName="Brazil, Rio de Janeiro, Rio de Janeiro, Fundacao Instituto Oswaldo Cruz" country="Brazil" httpUri="http://grbio.org/cool/2sxf-cwg4" name="Fundacao Instituto Oswaldo Cruz" pageId="22" pageNumber="187">FIOC</collectionCode>
, 
<collectionCode box="[418,497,1823,1846]" collectionName="UFRRJ" pageId="22" pageNumber="187">UFRRJ</collectionCode>
, 
<collectionCode box="[505,564,1823,1846]" collectionName="Brazil, Amazonas, Manaus, Instituto Nacional de Pesquisas da Amazoonia, Colecao Sistematica da Entomologia" country="Brazil" httpUri="http://grbio.org/cool/rm88-499z" name="Instituto Nacional de Pesquisas da Amazonia" pageId="22" pageNumber="187">INPA</collectionCode>
