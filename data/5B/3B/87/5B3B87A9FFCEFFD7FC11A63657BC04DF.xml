<taxonomicName authorityName="Fabricius" authorityYear="1803" box="[1286,1429,1170,1194]" class="Insecta" family="Pentatomidae" genus="Edessa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="59" pageNumber="224" phylum="Arthropoda" rank="species" species="sepulcralis">
