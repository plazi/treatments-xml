e 
<collectionCode box="[1221,1306,1074,1097]" collectionName="USA, Washington D.C., National Museum of Natural History, [formerly, United States National Museum]" country="USA" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="59" pageNumber="224" type="Museum">NMNH</collectionCode>
