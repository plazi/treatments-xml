male. Tanzania, Nguru Mountains, submontane forest, 1100-1200 m, 
<geoCoordinate degrees="6.05336" direction="south" orientation="latitude" precision="1" value="-6.05336">6.05336째S</geoCoordinate>
, 
<geoCoordinate degrees="37.56076" direction="east" orientation="longitude" precision="1" value="37.56076">37.56076째E</geoCoordinate>
- 
<geoCoordinate degrees="6.06702" direction="south" orientation="latitude" precision="1" value="-6.06702">6.06702째S</geoCoordinate>
, 
<geoCoordinate degrees="37.57339" direction="east" orientation="longitude" precision="1" value="37.57339">37.57339째E</geoCoordinate>
