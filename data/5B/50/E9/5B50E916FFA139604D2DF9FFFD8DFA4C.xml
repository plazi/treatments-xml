<paragraph blockId="65.[264,1323,284,545]" pageId="65" pageNumber="66">
<taxonomicName box="[312,568,444,469]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="carteriensis">
<emphasis box="[312,568,444,469]" italics="true" pageId="65" pageNumber="66">Geostiba carteriensis</emphasis>
