<paragraph pageId="null" pageNumber="485">
<taxonomicName authority="Gaudin" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="485" phylum="Tracheophyta" rank="species" species="punctata">
