<taxonomicName authorityName="Balogh" authorityYear="1983" box="[151,260,2004,2029]" class="Arachnida" family="Oppiidae" genus="Kokoppia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="2" pageNumber="196" phylum="Arthropoda" rank="genus">
<emphasis box="[151,260,2004,2029]" italics="true" pageId="2" pageNumber="196">Kokoppia</emphasis>
</taxonomicName>
, and 
<bibRefCitation author="Subias" box="[321,594,2004,2029]" pageId="2" pageNumber="196" refString="Subias, L. S. &amp; Balogh, P. (1989) Identification keys to the genera of Oppiidae Grandjean, 1951 (Acari: Oribatei). Acta Zoologica Academiae Scientiarum Hungaricae, 35 (3 - 4), 355 - 412." type="journal article" year="1989">Sub√≠as &amp; Balogh (1989)</bibRefCitation>
transferred 
<taxonomicName authorityName=": Hammer" authorityYear="1968" box="[729,822,2004,2029]" class="Arachnida" family="Oppiidae" genus="Brachioppiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="2" pageNumber="196" phylum="Arthropoda" rank="species" species="rafalskii">
<emphasis box="[729,822,2004,2029]" italics="true" pageId="2" pageNumber="196">rafalskii</emphasis>
</taxonomicName>
