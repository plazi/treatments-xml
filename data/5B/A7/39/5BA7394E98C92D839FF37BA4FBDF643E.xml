<paragraph pageId="null" pageNumber="627">
<taxonomicName authority="L." class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="627" phylum="Tracheophyta" rank="species" species="papilionacea">
