<taxonomicName class="Insecta" family="Carabidae" genus="Ega" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="783" pageNumber="784" phylum="Arthropoda" rank="genus">
