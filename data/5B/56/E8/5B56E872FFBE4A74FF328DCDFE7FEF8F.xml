<typeStatus box="[1041,1100,152,176]" pageId="63" pageNumber="64">types</typeStatus>
of 
<taxonomicName authorityName="Santschi" authorityYear="1923" box="[1137,1220,152,176]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="vafra">
<emphasis box="[1137,1220,152,176]" italics="true" pageId="63" pageNumber="64">P. vafra</emphasis>
</taxonomicName>
match the 
<typeStatus box="[1347,1406,152,176]" pageId="63" pageNumber="64">types</typeStatus>
of Wilson's 
<taxonomicName authorityName="Wilson" authorityYear="2003" box="[253,388,188,213]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="laticornis">
