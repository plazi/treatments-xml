<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33948" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole punctithorax Borgmeier" lsidName-HNS="Pheidole punctithorax Borgmeier" order="Hymenoptera" pageNumber="335" phylum="Arthropoda" rank="species" species="punctithorax">punctithorax</taxonomicName>
