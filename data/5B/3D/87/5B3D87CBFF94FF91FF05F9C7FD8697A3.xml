(
<bibRefCitation author="Poisson" box="[562,718,511,535]" pageId="21" pageNumber="22" refString="Poisson, R. (1955) Contributions a l'etude de la faune entomologique du Ruanda-Urundi (Mission P. Basilewsky 1953). XLV." type="journal volume" year="1955">
<taxonomicName box="[562,651,511,535]" class="Insecta" family="Naucoridae" genus="Neomacrocoris" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="8" phylum="Arthropoda" rank="species" species="poisson">Poisson</taxonomicName>
<number box="[658,718,511,535]" pageId="21" pageNumber="22" value="1955.0">1955</number>
</bibRefCitation>
