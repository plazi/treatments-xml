HW 
<number box="[542,573,440,464]" pageId="11" pageNumber="462" value="48.0">48</number>
, HL 
<number box="[628,658,440,464]" pageId="11" pageNumber="462" value="39.0">39</number>
, FV 
<number box="[712,742,440,464]" pageId="11" pageNumber="462" value="17.0">17</number>
