<emphasis bold="true" box="[151,345,1491,1514]" pageId="31" pageNumber="32">FIGURES 54–56.</emphasis>
<taxonomicName box="[356,652,1492,1514]" class="Insecta" family="Naucoridae" genus="Neomacrocoris" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="usambaricus">
