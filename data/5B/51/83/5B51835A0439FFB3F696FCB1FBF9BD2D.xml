<quantity box="[151,279,252,275]" metricMagnitude="-3" metricUnit="m" metricValue="2.45" metricValueMax="2.5" metricValueMin="2.4" pageId="4" pageNumber="241" unit="mm" value="2.45" valueMax="2.5" valueMin="2.4">2.4–2.5 mm</quantity>
(
<typeStatus box="[293,388,252,275]" pageId="4" pageNumber="241">holotype</typeStatus>
<quantity box="[395,481,252,275]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" pageId="4" pageNumber="241" unit="mm" value="2.5">2.5 mm</quantity>
