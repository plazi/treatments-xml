<heading bold="true" box="[151,605,151,178]" fontSize="11" level="1" pageId="9" pageNumber="380" reason="1">
<emphasis bold="true" box="[151,605,151,178]" pageId="9" pageNumber="380">
[
<taxonomicName authority="Chevrolat, 1863" authorityName="Chevrolat" authorityYear="1863" box="[161,598,151,178]" class="Insecta" family="Dytiscidae" genus="Copelatus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="380" phylum="Arthropoda" rank="species" species="insolitus">
