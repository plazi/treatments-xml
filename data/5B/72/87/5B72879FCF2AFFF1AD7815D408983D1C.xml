<taxonomicName authority="G. O. Sars, 1894" authorityName="G. O. Sars" authorityYear="1894" box="[826,1305,1141,1162]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Tanaidacea" pageId="11" pageNumber="334" phylum="Arthropoda" rank="species" species="warpachowskii">
