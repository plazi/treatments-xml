<quantity id="766A4291FFD68709FCA35494FCC990EB" box="[791,875,672,695]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="2" pageNumber="598" unit="mm" value="0.4">0.4 mm</quantity>
