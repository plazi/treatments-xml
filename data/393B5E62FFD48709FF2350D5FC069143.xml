<quantity id="766A4291FFD68709FD2854D1FD4D90A0" box="[668,751,741,764]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="2" pageNumber="598" unit="mm" value="5.9">5.9 mm</quantity>
