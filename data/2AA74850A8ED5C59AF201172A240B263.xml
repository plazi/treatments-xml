<paragraph id="A80D4D602361B0B17584D5272CB9C7DC" pageId="0" pageNumber="1">
<taxonomicName id="7D47E73C8A39EB1AF279B6A2584C4849" authorityName="Songsangchote, Sippawat, Khaikaew &amp; Chomphuphuang" authorityYear="2022" family="Theraphosidae" genus="Taksinus" kingdom="Animalia" lsidName="Taksinus bambus" order="Araneae" pageId="0" pageNumber="1" rank="species" species="bambus">
