<taxonomicName id="4C0EEBFFFFD1FFB193D634F0DD0FFBC6" box="[930,1103,1012,1036]" class="Insecta" family="Gryllidae" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="tribe" tribe="Modicogryllini">Modicogryllini</taxonomicName>
