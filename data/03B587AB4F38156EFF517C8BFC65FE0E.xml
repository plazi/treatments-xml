<taxonomicName id="4C1C4D3E4F27156EFE5A789FFD3CFF7E" authority="(de Koninck, 1843)" baseAuthorityName="de Koninck" baseAuthorityYear="1843" box="[412,759,225,251]" class="Gastropoda" family="Pleurotomariidae" genus="Pleurotomaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Pleurotomariida" pageId="16" pageNumber="17" phylum="Mollusca" rank="species" species="callosa">
<emphasis id="B968EAAF4F27156EFE5A789FFE7BFF7E" box="[412,432,225,251]" italics="true" pageId="16" pageNumber="17">G</emphasis>
. 
<emphasis id="B968EAAF4F27156EFE06789FFDDAFF7E" box="[448,529,225,251]" italics="true" pageId="16" pageNumber="17">callosa</emphasis>
(
<bibRefCitation id="EF8D4B4C4F27156EFDE7789FFD24FF7E" author="Koninck, L. G. de" box="[545,751,225,251]" firstAuthor="de Koninck" pageId="16" pageNumber="17" refId="ref25479" refString="Koninck, L. G. de (1843) [1842 - 44]. Description des animaux fossiles qui se trouvent dans le terrain carbonifere de Belgique. Dessain, Liege, 651 pp." type="book" year="1843">de Koninck, 1843</bibRefCitation>
)
</taxonomicName>
(Carboniferous, 
<collectingCountry id="F30B762D4F27156EFC7C789FFBD4FF79" box="[954,1055,225,252]" name="Belgium" pageId="16" pageNumber="17">Belgium</collectingCountry>
), the 
<typeStatus id="54A7881F4F27156EFB98789FFB45FF7E" box="[1118,1166,225,251]" pageId="16" pageNumber="17">type</typeStatus>
