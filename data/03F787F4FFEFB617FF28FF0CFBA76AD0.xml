<number id="3D9624C5FFECB616FD48FF0CFC8669A7" box="[759,802,152,176]" pageId="2" pageNumber="277" value="2.2">2, 2</number>
, 
<number id="3D9624C5FFECB616FC91FF0CFCFD69A7" box="[814,857,152,176]" pageId="2" pageNumber="277" value="2.1">2, 1</number>
