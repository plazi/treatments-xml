<taxonomicName id="4C4A4D60C200AF67C14CD15EFD38462B" authority="sensu HEINRICH (1962)" authorityName="sensu HEINRICH" authorityYear="1962" box="[360,740,857,879]" class="Insecta" family="Ichneumonidae" genus="Asthenolabus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="524" phylum="Arthropoda" rank="genus">
