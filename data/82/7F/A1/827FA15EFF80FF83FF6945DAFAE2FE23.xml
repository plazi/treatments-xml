<paragraph blockId="6.[168,775,1203,1362]" pageId="6" pageNumber="131">
<emphasis box="[168,551,1203,1228]" inLineHeading="true" pageId="6" pageNumber="131" reason="1">
<emphasis bold="true" box="[168,423,1203,1227]" pageId="6" pageNumber="131">Diagnostic characters.</emphasis>
Apart from
</emphasis>
<taxonomicName authority="Borgmeier, 1960" authorityName="Borgmeier" authorityYear="1960" class="Insecta" family="Phoridae" genus="Johowia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="131" phylum="Arthropoda" rank="species" species="hexachaeta">
