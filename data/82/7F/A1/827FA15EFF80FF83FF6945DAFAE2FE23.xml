<taxonomicName baseAuthorityName="Borgmeier" baseAuthorityYear="1962" box="[457,568,1744,1768]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="131" phylum="Arthropoda" rank="genus">
