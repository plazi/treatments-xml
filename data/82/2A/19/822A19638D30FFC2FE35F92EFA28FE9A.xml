<taxonomicName box="[449,702,1724,1753]" class="Mammalia" family="Hominidae" genus="Homo" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Primates" pageId="25" pageNumber="26" phylum="Chordata" rank="species" species="rudolfensis">
<heading box="[449,702,1724,1753]" fontSize="12" level="3" pageId="25" pageNumber="26" reason="1">
