</taxonomicName>
(
<taxonomicName box="[529,627,151,176]" class="Branchiopoda" genus="Daphnia" kingdom="Animalia" order="Diplostraca" pageId="14" pageNumber="265" phylum="Arthropoda" rank="genus">
<emphasis box="[529,627,151,176]" italics="true" pageId="14" pageNumber="265">Daphnia</emphasis>
</taxonomicName>
) species. 
<quantity box="[750,828,151,176]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="14" pageNumber="265" unit="mm" value="0.6">0.6mm</quantity>
in length, 
<quantity box="[966,1044,151,176]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="14" pageNumber="265" unit="mm" value="0.5">0.5mm</quantity>
