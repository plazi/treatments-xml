<paragraph blockId="14.[151,1436,151,357]" pageId="14" pageNumber="265">
<emphasis bold="true" box="[199,273,151,176]" pageId="14" pageNumber="265">Notes.</emphasis>
Unknown 
<taxonomicName box="[409,507,151,176]" class="Branchiopoda" genus="Daphnia" kingdom="Animalia" order="Diplostraca" pageId="14" pageNumber="265" phylum="Arthropoda" rank="genus">
