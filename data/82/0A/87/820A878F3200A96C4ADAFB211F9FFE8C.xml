<collectingCountry box="[1367,1435,1545,1570]" name="Brazil" pageId="9" pageNumber="267">Brazil</collectingCountry>
(
<bibRefCitation author="Travassos" box="[158,338,1582,1606]" pageId="9" pageNumber="267" refString="Travassos, L. (1922) Informacoes sobre a fauna helminthologica de Matto Grosso. Folha Medica, 3, 187 - 190." type="journal article" year="1922">
Travassos 
<number box="[278,338,1582,1606]" pageId="9" pageNumber="267" value="1922.0">1922</number>
</bibRefCitation>
), 
<collectingCountry box="[357,416,1581,1606]" name="Cuba" pageId="9" pageNumber="267">Cuba</collectingCountry>
(
<bibRefCitation author="Perez" box="[431,668,1581,1606]" pageId="9" pageNumber="267" refString="Perez Vigueras, I. (1940) Notas sobre algunas especies nuevas de trematodes y sobre otras poco conocidas. Revista Universidad de La Habana, 5, 1 - 28." type="journal article" year="1940">Perez Vigueras 1940</bibRefCitation>
