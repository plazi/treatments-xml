<taxonomicName authorityName="X. H. Jin" authorityYear="2020" class="Liliopsida" family="Orchidaceae" genus="Bulbophyllum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Bulbophyllum papuaense" order="Asparagales" pageId="0" pageNumber="125" phylum="Tracheophyta" rank="species" species="papuaense">
