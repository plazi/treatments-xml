<taxonomicName authority="Olivier 1808" authorityName="Olivier" authorityYear="1808" class="Insecta" family="Chrysomelidae" genus="Pachybrachis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="93" phylum="Arthropoda" rank="species" species="pubescens">
