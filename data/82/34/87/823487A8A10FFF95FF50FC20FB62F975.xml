<subSubSection pageId="3" pageNumber="34" type="materials_examined">
<paragraph blockId="3.[151,1437,1081,1682]" box="[151,743,1081,1106]" pageId="3" pageNumber="34">
<emphasis bold="true" box="[151,306,1081,1106]" pageId="3" pageNumber="34">
