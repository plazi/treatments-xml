and 
<taxonomicName class="Dicotyledoneae" family="Myristicaceae" genus="V." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="V. macrocarpa" order="Magnoliales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="macrocarpa">
