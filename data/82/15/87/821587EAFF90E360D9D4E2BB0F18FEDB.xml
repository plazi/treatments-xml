<subSubSection pageId="3" pageNumber="26" type="etymology">
<paragraph blockId="3.[151,1437,560,1993]" pageId="3" pageNumber="26">
<emphasis bold="true" box="[199,339,1926,1952]" pageId="3" pageNumber="26">Etymology.</emphasis>
