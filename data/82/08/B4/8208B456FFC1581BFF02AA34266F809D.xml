<subSubSection pageId="1" pageNumber="210" type="diagnosis">
<paragraph blockId="1.[136,779,743,1132]" pageId="1" pageNumber="210">
<emphasis bold="true" box="[189,306,962,986]" pageId="1" pageNumber="210">Diagnosis.</emphasis>
