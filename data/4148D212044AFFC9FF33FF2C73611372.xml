</taxonomicName>
(= 
<taxonomicName id="0EE11887044AFFC9FC2EFD2E721112CA" authorityName="$ Koehler" authorityYear="1906" box="[906,1027,739,765]" class="Ophiuroidea" family="Ophiochitonidae" genus="Ophiochiton" kingdom="Animalia" order="Ophiurida" pageId="26" pageNumber="27" phylum="Echinodermata" rank="species" species="solutum">
