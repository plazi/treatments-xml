<taxonomicName id="0EE11887044AFFC9FCD0FCE672011372" authorityName="Lyman" authorityYear="1883" box="[884,1043,811,837]" class="Ophiuroidea" family="Ophiochitonidae" genus="Ophiochiton" kingdom="Animalia" order="Ophiurida" pageId="26" pageNumber="27" phylum="Echinodermata" rank="species" species="ternispinus">
<emphasis id="FB95BF16044AFFC9FCD0FCE672011372" box="[884,1043,811,837]" italics="true" pageId="26" pageNumber="27">O. ternispinus</emphasis>
</taxonomicName>
