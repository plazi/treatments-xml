<quantity id="0E19CEE1044AFFC9FAB4FCCB76A31372" metricMagnitude="0" metricUnit="m" metricValue="2.1875" metricValueMax="2.22" metricValueMin="2.155" pageId="26" pageNumber="27" unit="m" value="2.1875" valueMax="2.22" valueMin="2.155">
