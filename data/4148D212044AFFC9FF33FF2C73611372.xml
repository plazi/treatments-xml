<geoCoordinate id="ACD505C3044AFFC9FBD1FCCA73141316" box="[1141,1286,775,801]" degrees="27" direction="west" minutes="02" orientation="longitude" pageId="26" pageNumber="27" precision="15" seconds="46" value="-27.04611">27°02’46”W</geoCoordinate>
