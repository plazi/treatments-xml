<geoCoordinate id="ACD505C3044AFFC9FBACFCCA72781316" box="[1032,1130,775,801]" degrees="38" direction="north" minutes="00" orientation="latitude" pageId="26" pageNumber="27" precision="925" value="38.0">38°00’N</geoCoordinate>
