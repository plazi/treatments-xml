<taxonomicName id="0EE11887044AFFC9FF33FCCA76ED1316" box="[151,255,775,801]" class="Gastropoda" family="Bursidae" genus="Talisman" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="26" pageNumber="27" phylum="Mollusca" rank="genus">
