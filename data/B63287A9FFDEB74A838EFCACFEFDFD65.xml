<taxonomicName id="F99B4D3CFFD9B74A8162FEF7FCA0FE95" box="[635,772,345,370]" class="Polychaeta" family="Ampharetidae" genus="Anobothrus" kingdom="Animalia" order="Terebellida" pageId="28" pageNumber="29" phylum="Annelida" rank="genus">
