<taxonomicName id="F99B4D3CFFD9B74A838EFE09FE84FE27" box="[151,288,423,448]" class="Polychaeta" family="Ampharetidae" genus="Anobothrus" kingdom="Animalia" order="Terebellida" pageId="28" pageNumber="29" phylum="Annelida" rank="genus">
