<taxonomicName id="F99B4D3CFFD9B74A8025FEF7FC15FE95" authority="Hartmann-Schroeder, 1965" authorityName="Hartmann-Schroeder" authorityYear="1965" box="[828,945,345,370]" class="Polychaeta" family="Ampharetidae" genus="Sosanides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Terebellida" pageId="28" pageNumber="29" phylum="Annelida" rank="genus">
