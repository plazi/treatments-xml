<subSubSection id="76816534FFD9B74A83DFFDB4FBD1FDD3" box="[198,1141,538,564]" pageId="28" pageNumber="29" type="etymology">
<paragraph id="3E2436BFFFD9B74A83DFFDB4FBD1FDD3" blockId="28.[151,1437,152,642]" box="[198,1141,538,564]" pageId="28" pageNumber="29">
<emphasis id="0CEFEAADFFD9B74A83DFFDB4FEF7FDD3" bold="true" box="[198,339,538,564]" pageId="28" pageNumber="29">Etymology.</emphasis>
