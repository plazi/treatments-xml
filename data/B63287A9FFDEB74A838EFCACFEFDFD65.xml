<taxonomicName id="F99B4D3CFFD9B74A8788FE9CFA30FEAC" authority="Hartmann-Schroeder, 1965" authorityName="Hartmann-Schroeder" authorityYear="1965" box="[1169,1428,306,331]" class="Polychaeta" family="Ampharetidae" genus="Sosanides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Terebellida" pageId="28" pageNumber="29" phylum="Annelida" rank="species" species="glandularis">
