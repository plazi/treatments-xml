<taxonomicName id="4C4AEEB3FFD48E28FDAB5182AC74E55C" authority="Diesing, 1850" authorityName="Diesing" authorityYear="1850" box="[633,809,506,531]" class="Hoplonemertea" family="Emplectonematidae" genus="Colpocephalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monostilifera" pageId="2" pageNumber="45" phylum="Nemertea" rank="genus">
