, 
<geoCoordinate id="EE229CB3FFA5E02F1D27FEB48100FEDF" box="[801,865,335,362]" degrees="3, 5" direction="east" orientation="longitude" pageId="27" pageNumber="28" precision="5555" value="3.5">
