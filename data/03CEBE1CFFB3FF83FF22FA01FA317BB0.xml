</subSubSection>
<subSubSection id="C37D5C81FFB3FF8DFF72F8ECFBF37E03" box="[199,1114,1832,1859]" pageId="6" pageNumber="467" type="diagnosis">
<paragraph id="8BD80F0AFFB3FF8DFF72F8ECFBF37E03" blockId="6.[151,1437,1580,2039]" box="[199,1114,1832,1859]" pageId="6" pageNumber="467">
