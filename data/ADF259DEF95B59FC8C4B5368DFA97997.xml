3. 
<taxonomicName id="4E8FB9993E6A137A3B1982828D3769BF" LSID="ADF259DE-F95B-59FC-8C4B-5368DFA97997" class="Insecta" family="Baetidae" genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis sumbensis" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="sumbensis">Labiobaetis sumbensis</taxonomicName>
