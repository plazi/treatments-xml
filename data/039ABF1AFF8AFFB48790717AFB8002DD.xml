<taxonomicName id="4C33758FFF8AFFB486217651FD74020D" authority="(DENIS &amp; SCHIFFERMULLER)" baseAuthorityName="DENIS &amp; SCHIFFERMULLER" box="[259,720,1055,1077]" class="Insecta" family="Geometridae" genus="Eupithecia" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="1305" phylum="Arthropoda" rank="species" species="subumbrata">
<emphasis id="B947D21EFF8AFFB486217651FEB6020D" box="[259,274,1056,1077]" italics="true" pageId="10" pageNumber="1305">E</emphasis>
