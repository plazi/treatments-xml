<quantity id="8E4A05E0322EFFB4502FFB9BFDC7046A" box="[545,637,1121,1147]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="1" pageNumber="495" unit="mm" value="2.8">2.8 mm</quantity>
