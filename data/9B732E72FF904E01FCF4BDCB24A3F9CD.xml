</taxonomicName>
e 
<taxonomicName id="D4DAE4E7FF904E01FBB9BF1124A9F9CD" authorityName="THOMSON" authorityYear="1858" box="[1138,1207,1609,1633]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="100" phylum="Arthropoda" rank="genus">
<emphasis id="21AE4376FF904E01FBB9BF1124A9F9CD" box="[1138,1207,1609,1633]" italics="true" pageId="7" pageNumber="100">Atheta</emphasis>
