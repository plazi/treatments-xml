<geoCoordinate id="6F8B1D7B233735D21675C38C4ED3D5DD" direction="north" orientation="latitude" precision="925" value="19.016666">19Â°01'N</geoCoordinate>
, 
<geoCoordinate id="703CA53D897DD7C638F403E7DB3F9568" direction="east" orientation="longitude" precision="925" value="109.55">109Â°33'E</geoCoordinate>
