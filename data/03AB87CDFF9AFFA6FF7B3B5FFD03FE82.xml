<taxonomicName id="4C024D58FF9AFFA1FB903127FB18F7A4" box="[1123,1222,2060,2086]" class="Tremellomycetes" family="Tremellaceae" genus="Tremella" kingdom="Fungi" order="Tremellales" pageId="3" pageNumber="211" phylum="Basidiomycota" rank="genus" sensu="stricto">
