<taxonomicName id="4C6008C7355CFFD5C1C6FE73FF2EFE2D" box="[151,343,470,495]" class="Insecta" family="Acroceridae" genus="Ogcodes" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="138" phylum="Arthropoda" rank="species" species="reginae">
