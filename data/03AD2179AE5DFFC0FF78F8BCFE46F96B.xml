<number id="3DCC8248AE5DFFC3FEFAF851FEC9F86D" box="[277,352,1930,1956]" pageId="5" pageNumber="50" value="15.0" valueMax="16.0" valueMin="14.0">14–16</number>
</figureCitation>
)
