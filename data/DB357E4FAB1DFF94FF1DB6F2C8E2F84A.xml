<taxonomicName id="949CB4DAAB1DFF94FC87B74BCF9EF896" box="[781,988,1860,1885]" class="Magnoliopsida" family="Chrysobalanaceae" kingdom="Plantae" order="Malpighiales" pageId="18" pageNumber="489" phylum="Tracheophyta" rank="family">Chrysobalanaceae</taxonomicName>
