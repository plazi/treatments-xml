<taxonomicName id="949CB4DAAB1DFF94FDB6B74ACEB8F896" authorityName="Sabine" authorityYear="1824" box="[572,762,1860,1885]" class="Magnoliopsida" family="Chrysobalanaceae" genus="Parinari" kingdom="Plantae" order="Malpighiales" pageId="18" pageNumber="489" phylum="Tracheophyta" rank="species" species="excelsa">
