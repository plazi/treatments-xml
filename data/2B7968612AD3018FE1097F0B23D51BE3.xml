<taxonomicName id="1A5690D84CBFB3DFCA48B39A533DD0E7" class="Polychaeta" family="Spionidae" genus="Nerinides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nerinides gilchristi" order="Spionida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="gilchristi">Nerinides gilchristi</taxonomicName>
