<quantity id="241A9BADFFD6ED12FE515DD5D76FB9ED" box="[480,634,707,729]" metricMagnitude="-3" metricUnit="m" metricValue="9.93" metricValueMax="10.61" metricValueMin="9.25" pageId="1" pageNumber="120" unit="mm" value="9.93" valueMax="10.61" valueMin="9.25">9.25–10.61 mm</quantity>
