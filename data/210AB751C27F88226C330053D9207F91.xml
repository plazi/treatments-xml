<taxonomicName id="EE756075181DCE0980D28881DA441E74" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia psittacorum" order="Zingiberales" pageId="135" pageNumber="136" phylum="Tracheophyta" rank="species" species="psittacorum">Heliconia psittacorum</taxonomicName>
Sw. (
<taxonomicName id="B7936D539BA13E3F1F637CEC4B05A9FD" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="135" pageNumber="136" rank="genus">Heliconiaceae</taxonomicName>
) (
<bibRefCitation id="1CFD43528831FC769550E175A6914B6F" author="Remillet, M" journalOrPublisher="Editions de l'ORSTOM, Paris" pageId="291" pageNumber="292" title="Catalogue des insectes ravageurs des cultures en Guyane Francaise. Institut Francaise de recherche scientifique pour le developpement en cooperation. Etudes et Theses." year="1988">Remillet 1988</bibRefCitation>
).
</paragraph>
</subSubSection>
