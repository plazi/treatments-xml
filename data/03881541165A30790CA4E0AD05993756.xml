<taxonomicName id="4C21DFD4165A30780CA4E0AD01A734C5" box="[151,418,671,697]" class="Insecta" family="Tettigoniidae" genus="Xiphidiola" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="lobaticerca" status="n. sp.">
<emphasis id="B9557845165A30780CA4E0AD01A734C5" bold="true" box="[151,418,671,697]" italics="true" pageId="14" pageNumber="15">Xiphidiola lobaticerca</emphasis>
</taxonomicName>
