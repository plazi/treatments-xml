<collectingCountry id="AF9D76617027FF86FDB3FA98FD0D1D6A" box="[635,741,1341,1367]" name="Australia" pageId="36" pageNumber="37">Australia</collectingCountry>
, 
<collectingRegion id="154EF8137027FF86FD27FA98FC511D6A" box="[751,953,1341,1367]" country="Australia" name="Western Australia" pageId="36" pageNumber="37">Western Australia</collectingRegion>
