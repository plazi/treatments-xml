<taxonomicName id="108A4D727027FF86FE0AFABCFCF41D0E" authorityName="Esben-Petersen" authorityYear="1918" box="[450,796,1305,1331]" class="Insecta" family="Myrmeleontidae" genus="Anomaloplectron" kingdom="Animalia" order="Neuroptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="lineatipenne">Anomaloplectron lineatipenne</taxonomicName>
:
