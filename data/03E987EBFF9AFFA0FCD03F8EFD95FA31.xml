<geoCoordinate id="EE74503AFF9BFFA0FEFF3C7DFE3CFDCE" box="[381,442,591,615]" degrees="2" direction="south" minutes="26" orientation="latitude" pageId="9" pageNumber="293" precision="925" value="-2.4333334">S2 26</geoCoordinate>
