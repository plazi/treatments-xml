<geoCoordinate id="EE74503AFF9BFFA0FDAC3C7DFDECFDCE" box="[558,618,591,615]" degrees="4" direction="south" minutes="16" orientation="latitude" pageId="9" pageNumber="293" precision="925" value="-4.266667">S4 16</geoCoordinate>
