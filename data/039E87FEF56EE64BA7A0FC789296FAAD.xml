<taxonomicName id="4C374D6BF56EE64BA6DBFC799554FC75" authorityName="Cameron" authorityYear="1943" box="[492,606,942,969]" class="Insecta" family="Staphylinidae" genus="Iotarphia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="421" phylum="Arthropoda" rank="genus">
