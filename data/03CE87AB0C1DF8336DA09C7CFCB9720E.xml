<emphasis id="B913EAAF0C1DF8306DA09C7CFCA57096" bold="true" box="[192,846,1643,1667]" pageId="13" pageNumber="12">
Subgenus 
<taxonomicName id="4C674D3E0C1DF8306C2C9C7CFDFC7096" box="[332,535,1643,1667]" class="Insecta" family="Erotylidae" genus="Dacne" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="13" phylum="Arthropoda" rank="subGenus" subGenus="Ameridacne">
<emphasis id="B913EAAF0C1DF8306C2C9C7CFDFC7096" bold="true" box="[332,535,1643,1667]" italics="true" pageId="13" pageNumber="12">AMERIDACNE</emphasis>
</taxonomicName>
<taxonomicName id="4C674D3E0C1DF8306F7C9C7CFD697096" authority="Skelley, 2009" authorityName="Skelley" authorityYear="2009" box="[540,642,1643,1667]" pageId="13" pageNumber="12" rank="subGenus" status="new subgenus" subGenus="Skelley">Skelley</taxonomicName>
