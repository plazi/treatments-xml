47. 
<taxonomicName id="A3C469CCF5273F464B9DEB76FC8FC521" authority="Usinger, 1946" authorityName="Usinger" authorityYear="1946" box="[509,875,1054,1097]" class="Insecta" family="Anthocoridae" genus="Lasiochilus" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="54" phylum="Arthropoda" rank="species" species="swezeyi" status="new species">Lasiochilus swezeyi</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="4D837326F5273F4649E3EB76FBBFC521" box="[899,1115,1054,1097]" pageId="43" pageNumber="54" rank="species">new species</taxonomicNameLabel>
