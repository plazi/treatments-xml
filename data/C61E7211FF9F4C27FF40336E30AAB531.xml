<subSubSection id="06AD908CFFA04C27FF1035723041B515" pageId="32" pageNumber="33" type="etymology">
<paragraph id="4E08C307FFA04C27FF1035723041B515" blockId="32.[151,1437,153,1151]" pageId="32" pageNumber="33">
<emphasis id="7CC31F15FFA04C27FF1035723366B579" bold="true" box="[199,334,1053,1079]" pageId="32" pageNumber="33">Etymology.</emphasis>
