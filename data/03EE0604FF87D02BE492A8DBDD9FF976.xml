<collectingRegion id="498379F0FF87D02BE5F0AC66DD03FAA9" box="[212,300,1322,1346]" country="United States of America" name="Florida" pageId="4" pageNumber="2059">Florida</collectingRegion>
, Gulf of 
<materialsCitation id="3B2FBD4FFF87D02BE480AC66DE65FA89" county="Lee County" location="Biscayne Bay" municipality="Hillsborough County" pageId="4" pageNumber="2059" specimenCount="1" stateProvince="Mexico">
