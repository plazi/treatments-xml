, 
<collectingCounty id="6299CF9EFF87D02BE72AAC67DEB3FAA9" box="[526,668,1322,1346]" pageId="4" pageNumber="2059">Lee County</collectingCounty>
, 
<collectingMunicipality id="6B9C2D68FF87D02BE78AAC66DF84FAA9" box="[686,939,1322,1346]" pageId="4" pageNumber="2059">Hillsborough County</collectingMunicipality>
; 
<typeStatus id="54FC09B0FF87D02BE699AC67DFC1FAA8" box="[957,1006,1323,1347]" pageId="4" pageNumber="2059">type</typeStatus>
locality from 
<location id="8E98E1C9FF87D02BE5F0AC06DD45FA88" LSID="urn:lsid:plazi:treatment:03EE0604FF87D02BE492A8DBDD9FF976:8E98E1C9FF87D02BE5F0AC06DD45FA88" box="[212,362,1354,1379]" county="Lee County" municipality="Hillsborough County" name="Biscayne Bay" pageId="4" pageNumber="2059" stateProvince="Mexico">Biscayne Bay</location>
south to the Lower
</materialsCitation>
<collectingRegion id="498379F0FF87D02BE774AC06DE8AFA89" box="[592,677,1354,1378]" country="United States of America" name="Florida" pageId="4" pageNumber="2059">Florida</collectingRegion>
