<location id="8EE8E6BA4B96D882FF673710B6D3FBAA" LSID="urn:lsid:plazi:treatment:039E01774B95D885FC863176B794FD5F:8EE8E6BA4B96D882FF673710B6D3FBAA" box="[208,413,1102,1125]" country="Brazil" county="Amazonas" municipality="Pref. Tefe'" name="upper Rio Urucu" pageId="194" pageNumber="195">upper Rio Urucu</location>
