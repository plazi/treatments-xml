<collectingMunicipality id="6BEC2A1B4B96D882FE1C3710B56DFBAA" box="[427,547,1102,1125]" pageId="194" pageNumber="195">Pref. Tefe´</collectingMunicipality>
, 
<collectingCounty id="62E9C8ED4B96D882FD863710B5E1FBAA" box="[561,687,1102,1125]" pageId="194" pageNumber="195">Amazonas</collectingCounty>
