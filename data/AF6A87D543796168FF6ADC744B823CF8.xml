<taxonomicName id="E0C34D4043746168FEE3DFA149813EBB" authorityName="Wilson &amp; Reels" authorityYear="2003" box="[286,393,1232,1256]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="24" pageNumber="525" phylum="Arthropoda" rank="species" species="galbina">
<emphasis id="15B7EAD143746168FEE3DFA149813EBB" box="[286,393,1232,1256]" italics="true" pageId="24" pageNumber="525">C. galbina</emphasis>
</taxonomicName>
; (e) 
<taxonomicName id="E0C34D4043746168FE44DFA14A3D3EBB" authorityName="Kompier &amp; Dow &amp; Steinhoff" authorityYear="2020" box="[441,565,1232,1256]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="24" pageNumber="525" phylum="Arthropoda" rank="species" species="pulchella">
<emphasis id="15B7EAD143746168FE44DFA14A3D3EBB" box="[441,565,1232,1256]" italics="true" pageId="24" pageNumber="525">C. pulchella</emphasis>
</taxonomicName>
; (f) 
<taxonomicName id="E0C34D4043746168FD9FDFA14ACC3EBB" authorityName="Dow" authorityYear="2016" box="[610,708,1233,1256]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="24" pageNumber="525" phylum="Arthropoda" rank="species" species="suoitia">
<emphasis id="15B7EAD143746168FD9FDFA14ACC3EBB" box="[610,708,1233,1256]" italics="true" pageId="24" pageNumber="525">C. suoitia</emphasis>
