<taxonomicName id="E0C34D4043746168FB32DFDE4D583E96" authorityName="Kompier &amp; Dow &amp; Steinhoff" authorityYear="2020" box="[1231,1360,1197,1221]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="24" pageNumber="525" phylum="Arthropoda" rank="species" species="diomedea">
<emphasis id="15B7EAD143746168FB32DFDE4D583E96" box="[1231,1360,1197,1221]" italics="true" pageId="24" pageNumber="525">C. diomedea</emphasis>
