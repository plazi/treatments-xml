spp. [a,c,e] frontal view, [b,d,f] oblique dorsal view. (a,b) 
<taxonomicName id="E0C34D40437B6167FADFDCD84D9D3D93" authorityName="Phan &amp; Kompier" authorityYear="2016" box="[1314,1429,1960,1984]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="23" pageNumber="524" phylum="Arthropoda" rank="species" species="hayashii">
<emphasis id="15B7EAD1437B6167FADFDCD84D9D3D93" box="[1314,1429,1960,1984]" italics="true" pageId="23" pageNumber="524">C. hayashii</emphasis>
</taxonomicName>
; (c,d) 
<taxonomicName id="E0C34D40437B6167FF30DCBC49443DB7" authorityName="Kompier &amp; Dow &amp; Steinhoff" authorityYear="2020" box="[205,332,1996,2020]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="23" pageNumber="524" phylum="Arthropoda" rank="species" species="diomedea">
<emphasis id="15B7EAD1437B6167FF30DCBC49443DB7" box="[205,332,1996,2020]" italics="true" pageId="23" pageNumber="524">C. diomedea</emphasis>
</taxonomicName>
; (e,f) 
<taxonomicName id="E0C34D40437B6167FE76DCBC4A0C3DB7" authorityName="Kompier &amp; Dow &amp; Steinhoff" authorityYear="2020" box="[395,516,1996,2020]" class="Insecta" family="Platycnemididae" genus="Coeliccia" kingdom="Animalia" order="Odonata" pageId="23" pageNumber="524" phylum="Arthropoda" rank="species" species="coronata">
<emphasis id="15B7EAD1437B6167FE76DCBC4A0C3DB7" box="[395,516,1996,2020]" italics="true" pageId="23" pageNumber="524">C. coronata</emphasis>
</taxonomicName>
.
</paragraph>
</caption>
<caption id="73BC664B43746168FF6ADFDD4AC23EBB" ID-DOI="http://doi.org/10.5281/zenodo.3765690" ID-Zenodo-Dep="3765690" httpUri="https://zenodo.org/record/3765690/files/figure.png" pageId="24" pageNumber="525" startId="24.[151,250,1197,1222]" targetBox="[182,1384,181,1171]" targetPageId="24">
