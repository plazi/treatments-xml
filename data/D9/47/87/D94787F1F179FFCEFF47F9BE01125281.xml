<subSubSection pageId="8" pageNumber="240" type="discussion">
<paragraph blockId="8.[151,1436,1827,2032]" pageId="8" pageNumber="240">
<emphasis bold="true" box="[199,315,1900,1924]" inLineHeading="true" pageId="8" pageNumber="240" reason="1">Remarks.</emphasis>
<taxonomicName authorityName="Manning" authorityYear="1971" box="[328,562,1900,1924]" class="Malacostraca" family="Gonodactylidae" genus="Gonodactylus" kingdom="Animalia" order="Stomatopoda" pageId="8" pageNumber="240" phylum="Arthropoda" rank="species" species="childi">
<emphasis box="[328,562,1900,1924]" italics="true" pageId="8" pageNumber="240">Gonodactylus childi</emphasis>
