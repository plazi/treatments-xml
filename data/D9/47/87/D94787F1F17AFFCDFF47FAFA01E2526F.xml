<taxonomicName baseAuthorityName="Moosa" baseAuthorityYear="1991" box="[389,493,1726,1750]" class="Malacostraca" family="Pseudosquillidae" genus="Pseudosquillana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stomatopoda" pageId="11" pageNumber="243" phylum="Arthropoda" rank="species" species="richeri">
