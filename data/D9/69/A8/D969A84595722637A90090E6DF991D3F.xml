<taxonomicName authority="Eichler, 1949" authorityName="Eichler" authorityYear="1949" baseAuthorityName="Balat" baseAuthorityYear="1981" class="Insecta" family="Philopteridae" genus="Guimaraesiella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Guimaraesiella haftorni" order="Passeriformes" pageId="16" pageNumber="33" phylum="Arthropoda" rank="species" species="haftorni">
