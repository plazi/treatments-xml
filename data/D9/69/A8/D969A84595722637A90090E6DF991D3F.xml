<taxonomicName class="Aves" family="Muscicapidae" genus="Turdus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Turdus iliacus" order="Passeriformes" pageId="16" pageNumber="33" phylum="Chordata" rank="species" species="iliacus">
