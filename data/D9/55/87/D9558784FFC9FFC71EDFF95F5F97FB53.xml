0 
<geoCoordinate box="[1173,1209,548,575]" direction="east" orientation="longitude" pageId="34" pageNumber="35" precision="1" value="30.358694">
<number box="[1173,1201,551,575]" pageId="34" pageNumber="35" value="30.0">30</number>
o
</geoCoordinate>
