<paragraph blockId="15.[151,426,943,1003]" box="[151,426,943,969]" pageId="15" pageNumber="16">
<heading bold="true" box="[151,426,943,969]" fontSize="11" level="1" pageId="15" pageNumber="16" reason="1">
