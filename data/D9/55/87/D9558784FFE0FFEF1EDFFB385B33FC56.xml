</geoCoordinate>
<number box="[1014,1044,802,826]" pageId="10" pageNumber="11" value="33.0">33</number>
’54,2’’ N, 
<geoCoordinate box="[1159,1209,799,826]" direction="west" orientation="longitude" pageId="10" pageNumber="11" precision="925" value="-0.5">
0 
<geoCoordinate box="[1173,1209,799,826]" direction="east" orientation="longitude" pageId="10" pageNumber="11" precision="1" value="30.358694">
