<number box="[1379,1387,242,263]" pageId="3" pageNumber="4" value="1.0">1</number>
/ 
<number box="[1397,1410,242,263]" pageId="3" pageNumber="4" value="2.0">2</number>
to 
<number box="[225,237,271,292]" pageId="3" pageNumber="4" value="2.0">2</number>
