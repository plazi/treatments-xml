<emphasis italics="true" pageId="0" pageNumber="1">
<taxonomicName class="Magnoliopsida" family="Convolvulaceae" genus="Ipomoea" kingdom="Plantae" order="Solanales" phylum="Tracheophyta" rank="genus">Ipomoea</taxonomicName>
<normalizedToken originalValue="×">x</normalizedToken>
