<subSubSection pageId="4" pageNumber="105" type="nomenclature">
<paragraph pageId="4" pageNumber="105">
<taxonomicName authority="Wiedemann, 1819" authorityName="Wiedemann" authorityYear="1819" class="Insecta" family="Calliphoridae" genus="Chrysomya" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysomya albiceps" order="Diptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="species" species="albiceps">Chrysomya albiceps (Wiedemann, 1819)</taxonomicName>
Figs 2G, 3D
</paragraph>
</subSubSection>
<subSubSection pageId="4" pageNumber="105" type="materials_examined">
<paragraph pageId="4" pageNumber="105">Material examined.</paragraph>
<paragraph pageId="4" pageNumber="105">
