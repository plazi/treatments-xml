<quantity box="[380,455,957,981]" metricMagnitude="3" metricUnit="m" metricValue="1.68" pageId="23" pageNumber="602" unit="m" value="1680.0">1680 m</quantity>
, montane rainforest, 
<date box="[111,217,983,1007]" pageId="23" pageNumber="602" value="1993-10-23">23.X.1993</date>
