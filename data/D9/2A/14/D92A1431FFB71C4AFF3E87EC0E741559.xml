<collectionCode box="[385,467,1010,1034]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="23" pageNumber="602" type="Museum">FMNH</collectionCode>
