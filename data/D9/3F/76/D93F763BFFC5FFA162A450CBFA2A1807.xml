<taxonomicName authority="Aczel" authorityName="Aczel" box="[1260,1436,603,625]" class="Insecta" family="Neriidae" genus="Glyphidops" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="142" phylum="Arthropoda" rank="species" species="etele" subGenus="Glyphidops">
