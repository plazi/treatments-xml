<heading bold="true" box="[151,880,792,819]" fontSize="11" level="1" pageId="1" pageNumber="24" reason="1">
<emphasis bold="true" box="[151,880,792,819]" pageId="1" pageNumber="24">
Genus 
<taxonomicName authority="Brescovit, Ferreira &amp; Rheims" authorityName="Brescovit, Ferreira &amp; Rheims" box="[238,763,792,819]" class="Arachnida" family="Prodidomidae" genus="Brasilomma" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="24" phylum="Arthropoda" rank="genus">
