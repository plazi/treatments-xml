<title pageId="null" pageNumber="333">
<subSubSection pageId="null" pageNumber="333" type="nomenclature">
<paragraph pageId="null" pageNumber="333">
<taxonomicName authority="L." class="Liliopsida" family="Poaceae" genus="Poa" kingdom="Plantae" order="Poales" pageId="null" pageNumber="333" phylum="Tracheophyta" rank="species" species="bulbosa">
<pageBreakToken pageId="null" pageNumber="333" start="start">Poa</pageBreakToken>
<normalizedToken originalValue="bulbósa" pageId="null" pageNumber="333">bulbosa</normalizedToken>
<authorityName pageId="null" pageNumber="333">L.</authorityName>
</taxonomicName>
</paragraph>
