, 
<taxonomicName authority="Aube, 1838" authorityName="Aube" authorityYear="1838" box="[609,954,732,756]" class="Insecta" family="Curculionidae" genus="Hydaticus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="species" species="pacificus">
<emphasis box="[609,820,733,756]" italics="true" pageId="7" pageNumber="72">Hydaticus pacificus</emphasis>
<bibRefCitation author="AUBE C." box="[826,954,732,756]" pageId="7" pageNumber="72" refId="ref3291" refString="References AUBE C. 1838: Hydrocanthares et gyriniens. In: DEJEAN P. F.: Species general des coleopteres de la collection de M. le Comte Dejean. Vol. 6. Mequignon Pere et Fils, Paris, xvi + 804 pp." type="book" year="1838">Aubé, 1838</bibRefCitation>
</taxonomicName>
, 
<taxonomicName class="Insecta" family="Dytiscidae" genus="Platynectes" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis box="[966,1089,733,756]" italics="true" pageId="7" pageNumber="72">Platynectes</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName authority="(W. S. Macleay, 1825)" baseAuthorityName="W. S. Macleay" baseAuthorityYear="1825" box="[158,596,763,788]" class="Insecta" family="Dytiscidae" genus="Rhantus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="species" species="suturalis">
<emphasis box="[158,346,765,788]" italics="true" pageId="7" pageNumber="72">Rhantus suturalis</emphasis>
(W. S. Macleay, 1825)
</taxonomicName>
and 
<taxonomicName authority="Aube, 1838" authorityName="Aube" authorityYear="1838" box="[649,1038,764,788]" class="Insecta" family="Dytiscidae" genus="Sandracottus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="species" species="chevrolati">
<emphasis box="[649,904,765,788]" italics="true" pageId="7" pageNumber="72">Sandracottus chevrolati</emphasis>
<bibRefCitation author="AUBE C." box="[910,1038,764,788]" pageId="7" pageNumber="72" refId="ref3291" refString="References AUBE C. 1838: Hydrocanthares et gyriniens. In: DEJEAN P. F.: Species general des coleopteres de la collection de M. le Comte Dejean. Vol. 6. Mequignon Pere et Fils, Paris, xvi + 804 pp." type="book" year="1838">Aubé, 1838</bibRefCitation>
</taxonomicName>
, and some unidentified 
<taxonomicName authorityName="Latreille" authorityYear="1802" box="[303,454,796,820]" class="Insecta" family="Hydrophilidae" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="family">Hydrophilidae</taxonomicName>
