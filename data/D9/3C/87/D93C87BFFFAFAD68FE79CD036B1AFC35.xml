<taxonomicName authority="Thomson, 1859" authorityName="Thomson" authorityYear="1859" box="[596,870,795,820]" class="Insecta" family="Hydrophilidae" genus="Anacaena" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[596,701,797,820]" italics="true" pageId="7" pageNumber="72">Anacaena</emphasis>
Thomson, 1859
</taxonomicName>
, 
<taxonomicName authority="Thomson, 1859" authorityName="Thomson" authorityYear="1859" class="Insecta" family="Hydrophilidae" genus="Enochrus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[878,978,797,820]" italics="true" pageId="7" pageNumber="72">Enochrus</emphasis>
Thomson, 1859
</taxonomicName>
, and 
