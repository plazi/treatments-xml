<subSubSection pageId="33" pageNumber="34" type="materials_examined">
<paragraph blockId="33.[151,1437,151,464]" pageId="33" pageNumber="34">
<emphasis bold="true" box="[199,373,151,176]" pageId="33" pageNumber="34">
<typeStatus box="[199,258,151,176]" pageId="33" pageNumber="34">Type</typeStatus>
material
</emphasis>
. 
<emphasis bold="true" box="[394,553,151,176]" pageId="33" pageNumber="34">
<typeStatus box="[394,553,151,176]" pageId="33" pageNumber="34">HOLOTYPE</typeStatus>
</emphasis>
♂, 
