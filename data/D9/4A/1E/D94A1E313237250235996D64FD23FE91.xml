+ 
<number box="[762,798,1973,1999]" pageId="13" pageNumber="14" value="4.2">4.2</number>
+ 
<number box="[830,867,1973,1999]" pageId="13" pageNumber="14" value="3.9">3.9</number>
+ 2.0); IV: 
<number box="[1004,1055,1973,1999]" pageId="13" pageNumber="14" value="17.6">17.6</number>
(
<number box="[1071,1109,1973,1999]" pageId="13" pageNumber="14" value="4.3">4.3</number>
+ 
<number box="[1140,1177,1973,1999]" pageId="13" pageNumber="14" value="5.6">5.6</number>
