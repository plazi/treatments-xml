<taxonomicName authority="Gravier, 1901" authorityName="Gravier" authorityYear="1901" box="[1029,1319,649,674]" class="Polychaeta" family="Nereididae" genus="Pseudonereis" kingdom="Animalia" order="Phyllodocida" pageId="25" pageNumber="232" phylum="Annelida" rank="species" species="anomala">
