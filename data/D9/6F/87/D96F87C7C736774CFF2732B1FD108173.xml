<quantity box="[1176,1278,686,710]" metricMagnitude="1" metricUnit="m" metricValue="1.4" metricValueMax="1.8" metricValueMin="1.0" pageId="25" pageNumber="232" unit="m" value="14.0" valueMax="18.0" valueMin="10.0">
