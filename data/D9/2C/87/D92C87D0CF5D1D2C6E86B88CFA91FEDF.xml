<collectingRegion box="[926,1071,280,307]" country="Mexico" name="Morelos" pageId="58" pageNumber="559">Morelos state</collectingRegion>
(
<taxonomicName baseAuthorityName="Caballero" baseAuthorityYear="1938" box="[1088,1197,280,306]" class="Reptilia" family="Kinosternidae" genus="Kinosternon" kingdom="Animalia" order="Testudines" pageId="58" pageNumber="559" phylum="Chordata" rank="species" species="hirtipes">
<emphasis box="[1088,1197,280,306]" italics="true" pageId="58" pageNumber="559">K. hirtipes</emphasis>
