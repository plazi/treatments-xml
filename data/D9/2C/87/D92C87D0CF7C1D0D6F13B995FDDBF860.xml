<taxonomicName authorityName="Mizelle &amp; Price" authorityYear="1964" box="[383,520,2001,2027]" class="Monogenea" family="Ancyrocephalidae" genus="Urocleidoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dactylogyridea" pageId="27" pageNumber="528" phylum="Platyhelminthes" rank="genus">
