<emphasis box="[910,962,438,464]" italics="true" pageId="26" pageNumber="527">et al.</emphasis>
(2006) as 
<taxonomicName authorityName="Paperna &amp; Thurston" authorityYear="1969" box="[1078,1337,438,464]" class="Monogenea" family="Ancyrocephalidae" genus="Cichlidogyrus" kingdom="Animalia" order="Dactylogyridea" pageId="26" pageNumber="527" phylum="Platyhelminthes" rank="species" species="longicornis">
