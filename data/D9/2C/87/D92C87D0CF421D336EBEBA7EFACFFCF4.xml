</taxonomicName>
+ 
<taxonomicName authorityName="Bravo-Hollis" authorityYear="1970" class="Monogenea" family="Loimoidae" genus="Loimosina" kingdom="Animalia" order="Monocotylidea" pageId="37" pageNumber="538" phylum="Platyhelminthes" rank="species" species="parawilsoni">
<emphasis italics="true" pageId="37" pageNumber="538">Loimosina parawilsoni</emphasis>
