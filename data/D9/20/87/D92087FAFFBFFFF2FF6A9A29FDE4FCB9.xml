<number box="[159,226,367,391]" pageId="2" pageNumber="64" value="0.23">0.230</number>
, n = 
<number box="[280,295,367,391]" pageId="2" pageNumber="64" value="2.0">2</number>
) mm. Respiratory organ length 
<number box="[651,763,367,391]" pageId="2" pageNumber="64" value="0.25" valueMax="0.26" valueMin="0.24">0.24–0.26</number>
(
<number box="[778,831,367,391]" pageId="2" pageNumber="64" value="0.25">0.25</number>
, n = 
<number box="[884,899,367,391]" pageId="2" pageNumber="64" value="2.0">2</number>
) mm. Anal segment (Fig. 
<number box="[1194,1223,367,391]" pageId="2" pageNumber="64" value="20.0">20</number>
) length 
<number box="[1314,1377,367,391]" pageId="2" pageNumber="64" value="0.342">0.342</number>
