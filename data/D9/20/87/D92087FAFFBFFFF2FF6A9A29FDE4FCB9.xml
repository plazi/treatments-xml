<number box="[151,166,404,428]" pageId="2" pageNumber="64" value="2.0">2</number>
) mm, width 
<number box="[309,421,404,428]" pageId="2" pageNumber="64" value="0.235" valueMax="0.25" valueMin="0.22">0.22–0.25</number>
(
<number box="[436,503,404,428]" pageId="2" pageNumber="64" value="0.235">0.235</number>
