<taxonomicName authorityName="Clayton" authorityYear="1969" box="[1112,1232,1491,1512]" class="Liliopsida" family="Poaceae" genus="Parahyparrhenia" kingdom="Plantae" order="Poales" pageId="3" pageNumber="250" phylum="Tracheophyta" rank="species" species="siamensis">
