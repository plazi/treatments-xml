<quantity box="[721,836,1635,1657]" metricMagnitude="-2" metricUnit="m" metricValue="6.75" metricValueMax="10.0" metricValueMin="3.5" pageId="3" pageNumber="250" unit="mm" value="67.5" valueMax="100.0" valueMin="35.0">35–100 mm</quantity>
