<number box="[775,803,879,905]" pageId="7" pageNumber="8" value="11.0">11</number>
(
<number box="[824,900,879,905]" pageId="7" pageNumber="8" value="10.5" valueMax="11.0" valueMin="10.0">10–11</number>
), basiventral femoral setae (
<emphasis box="[1246,1274,879,904]" italics="true" pageId="7" pageNumber="8">bv</emphasis>
