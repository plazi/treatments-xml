<number box="[1191,1220,599,625]" pageId="7" pageNumber="8" value="65.0">65</number>
(
<number box="[1238,1314,599,625]" pageId="7" pageNumber="8" value="65.5" valueMax="67.0" valueMin="64.0">64–67</number>
