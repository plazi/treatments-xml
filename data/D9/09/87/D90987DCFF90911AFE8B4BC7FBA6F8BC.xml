<number box="[727,771,599,625]" pageId="7" pageNumber="8" value="210.0">210</number>
(
<number box="[788,893,599,625]" pageId="7" pageNumber="8" value="202.5" valueMax="215.0" valueMin="190.0">190–215</number>
) long, 
<number box="[979,1008,599,625]" pageId="7" pageNumber="8" value="70.0">70</number>
(
<number box="[1025,1102,599,625]" pageId="7" pageNumber="8" value="70.0" valueMax="72.0" valueMin="68.0">68–72</number>
