<paragraph id="AF656C2FFF91FFFF98D4063150D18FFD" blockId="35.[1475,2285,271,394]" lastBlockId="35.[1472,2679,872,3029]" pageId="35" pageNumber="606">
<subSubSection id="E7C03FA4FF91FFFF98D40631516A8DC7" pageId="35" pageNumber="606" type="vernacular_name">
<heading id="F42DDB43FF91FFFF98D40631516A8DC7" pageId="35" pageNumber="606">
<emphasis id="9DAEB03DFF91FFFF98D4063150118D2E" box="[1477,1554,334,355]" pageId="35" pageNumber="606">French:</emphasis>
<vernacularName id="21D91C01FF91FFFF9B0A063150B48D2E" box="[1563,1719,334,355]" pageId="35" pageNumber="606">
Sitatunga du 
<collectingRegion id="6D1EA2CDFF91FFFF9BB1063150B48D2E" box="[1696,1719,334,355]" country="Sudan" name="Nahr an Nil" pageId="35" pageNumber="606">Nil</collectingRegion>
</vernacularName>
/ 
<emphasis id="9DAEB03DFF91FFFF9BDC0631512B8D2E" box="[1741,1832,334,355]" pageId="35" pageNumber="606">German:</emphasis>
<vernacularName id="21D91C01FF91FFFF9A23063151AD8D2E" box="[1842,1966,334,355]" pageId="35" pageNumber="606">Nil-Sitatunga</vernacularName>
/ 
<emphasis id="9DAEB03DFF91FFFF9AD206315E1D8D2E" box="[1987,2078,334,355]" pageId="35" pageNumber="606">Spanish:</emphasis>
