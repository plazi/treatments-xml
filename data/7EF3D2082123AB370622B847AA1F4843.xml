<subSubSection id="0B8071E7590B3168B25677CE0CA645F9" pageId="0" pageNumber="1" type="nomenclature">
<paragraph id="22D4590713265151DFDA6882EF4F40F7" pageId="0" pageNumber="1">
<taxonomicName id="A0D72DDB49C9E4779308A8BF3E8DB47D" LSID="828725" authority="S. X. Zhou, J. C. Kang &amp; K. D. Hyde" class="Sordariomycetes" family="Glomerellaceae" genus="Colletotrichum" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Colletotrichum tongrenense" order="Glomerellales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="tongrenense">Colletotrichum tongrenense S.X. Zhou, J.C. Kang &amp; K.D. Hyde</taxonomicName>
<taxonomicNameLabel id="F90822C294BA5585FE27B7F86DD5BD5F" pageId="0" pageNumber="1">sp. nov.</taxonomicNameLabel>
