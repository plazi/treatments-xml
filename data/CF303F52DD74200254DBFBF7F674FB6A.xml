<taxonomicName id="8099F5C7DD74200257DBFB6DF161FB24" authorityName="ASSING" authorityYear="2012" box="[946,1078,1157,1179]" class="Insecta" family="Staphylinidae" genus="Lobrathium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="431" phylum="Arthropoda" rank="species" species="flavipenne">
