<taxonomicName id="8099F5C7DD7420025652FB2BF797FB6A" authorityName="ASSING" authorityYear="2012" box="[571,704,1215,1237]" class="Insecta" family="Staphylinidae" genus="Lobrathium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="431" phylum="Arthropoda" rank="species" species="flavipenne">
