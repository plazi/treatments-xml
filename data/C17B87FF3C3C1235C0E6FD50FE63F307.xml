): “ 
<location id="4C0D60323C3D1235C4D2FB97FADAF6C7" LSID="urn:lsid:plazi:treatment:C17B87FF3C3C1235C0E6FD50FE63F307:4C0D60323C3D1235C4D2FB97FADAF6C7" box="[1187,1352,477,503]" municipality="Matto Grosso" name="Bras. Corumba" pageId="3" pageNumber="152">Bras.Corumba</location>
[
<collectingMunicipality id="A909AC933C3D1235C527FB97FF7BF52B" pageId="3" pageNumber="152">Matto Grosso</collectingMunicipality>
] // 
<determinerName id="BCB1DA2D3C3D1235C17DF84BFED0F52B" box="[268,322,513,539]" pageId="3" pageNumber="152">Kniž</determinerName>
