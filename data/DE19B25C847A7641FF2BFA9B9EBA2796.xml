<taxonomicName id="91B078C9847A764EFF2BFA9B9D6B217F" authority="Morrison" authorityName="Morrison" box="[151,532,1331,1357]" class="Insecta" family="Braconidae" genus="Cremnops" kingdom="Animalia" order="Hymenoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="kelloggii">
