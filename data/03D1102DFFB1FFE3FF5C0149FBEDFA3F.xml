<quantity id="4C800CDEFFB1FFE3FF2900D5FF47FA3A" box="[166,230,1420,1443]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="45" pageNumber="248" unit="m" value="100.0">100 m</quantity>
