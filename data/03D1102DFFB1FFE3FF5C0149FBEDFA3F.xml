<quantity id="4C800CDEFFB1FFE3FF2A0082FF40FA6B" box="[165,225,1499,1522]" metricMagnitude="0" metricUnit="m" metricValue="7.0" metricValueMax="9.0" metricValueMin="5.0" pageId="45" pageNumber="248" unit="m" value="7.0" valueMax="9.0" valueMin="5.0">5-9 m</quantity>
