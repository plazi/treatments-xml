<taxonomicName id="E5F34D33FFE8FFCDFBC52B9E89B2FE51" box="[1116,1275,439,464]" class="Insecta" family="Limnephilidae" genus="Drusus" kingdom="Animalia" order="Trichoptera" pageId="8" pageNumber="559" phylum="Arthropoda" rank="species" species="dardanicus" status="sp. nov.">
