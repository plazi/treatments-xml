<subSubSection id="47F4E1936D448185DD321D7F5FD7AE56" pageId="null" pageNumber="539" type="nomenclature">
<paragraph id="C65DA826516DFB6850999B0DEBF8F481" pageId="null" pageNumber="539">
<pageBreakToken id="99310F3F28F19643E1E6DFCEB7EDD357" pageId="null" pageNumber="539" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="21BD1C8B31B601DDE4E0339F801EBD6A" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Erigeron" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="539" phylum="Tracheophyta" rank="species" species="alpinus">
Erigeron 
<normalizedToken id="33586D1D5863BD6A51435984044F48F0" originalValue="alpínus" pageId="null" pageNumber="539">alpinus</normalizedToken>
<authorityName id="A797F49DFB16A3CB61008874875C65B7" pageId="null" pageNumber="539">L.</authorityName>
</taxonomicName>
</paragraph>
