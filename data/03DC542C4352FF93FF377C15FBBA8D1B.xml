<collectionCode id="ED647DFF4352FF96FF497CA6FEE1899B" box="[233,337,1424,1450]" httpUri="http://grbio.org/cool/6yx0-3nm7" name="Museu de Zoologia da Universidade de Sao Paulo" pageId="1" pageNumber="338">MZUSP</collectionCode>
