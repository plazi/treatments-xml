<tr id="9E0FE1421E9DAD1158F5DC00830E23B0" pageId="35" pageNumber="148">
<td id="014FDCC6A740FE293CFA044FFCD3343F" colspan="1" pageId="35" pageNumber="148" rowspan="1">-</td>
<td id="66D575CDEE14889CFC7A29EBAD4DB98F" colspan="1" pageId="35" pageNumber="148" rowspan="1">Eyes present</td>
<td id="5E2D1A9AC3171107BD9B11136B7BB399" colspan="1" pageId="35" pageNumber="148" rowspan="1">
<emphasis id="2E79C847D8FB76FE3981ECFA1FD4ACA3" bold="true" pageId="35" pageNumber="148">2</emphasis>
