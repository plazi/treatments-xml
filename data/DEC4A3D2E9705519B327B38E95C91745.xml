<taxonomicName id="D2AD4C2B39C1F4E77B26303AD124B504" class="Liliopsida" family="Tofieldiaceae" genus="Harperocallis" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Alismatales" pageId="5" pageNumber="42" phylum="Tracheophyta" rank="species" species="flava">
