<subSubSection id="0DD76571211CF769A095F1C94EC0C7D2" box="[199,1239,692,716]" pageId="6" pageNumber="42" type="etymology">
<paragraph id="457236FA211CF769A095F1C94EC0C7D2" blockId="6.[151,1436,152,1256]" box="[199,1239,692,716]" pageId="6" pageNumber="42">
<emphasis id="77B9EAE8211CF769A095F1C94B46C7D2" bold="true" box="[199,337,692,716]" pageId="6" pageNumber="42">Etymology:</emphasis>
