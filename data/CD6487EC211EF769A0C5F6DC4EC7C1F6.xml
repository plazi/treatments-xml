</taxonomicName>
<taxonomicName id="82CD4D79211CF769A1D9F0CD484BC6D6" box="[395,604,944,968]" class="Insecta" family="Braconidae" genus="Aphidius" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="42" phylum="Arthropoda" rank="species" species="staticobii">
