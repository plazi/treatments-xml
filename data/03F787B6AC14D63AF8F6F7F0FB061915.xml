<taxonomicName id="4C5E4D23AC14D63BF8D7F640FEE31915" authorityName="Poche" authorityYear="1926" box="[182,282,295,321]" class="Trematoda" family="Faustulidae" genus="Faustula" kingdom="Animalia" order="Plagiorchiida" pageId="6" pageNumber="237" phylum="Platyhelminthes" rank="genus">
