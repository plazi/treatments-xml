<taxonomicName id="BBC84D137A0A005F65A310E5364A5A19" authority="Haliday" authorityName="Haliday" box="[761,1042,1524,1547]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="0" pageNumber="484" phylum="Arthropoda" rank="species" species="phaleratus">
<emphasis id="4EBCEA827A0A005F65A310E531EF5A19" box="[761,951,1524,1547]" italics="true" pageId="0" pageNumber="484">Thrips phaleratus</emphasis>
