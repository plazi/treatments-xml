Eastern 
<collectingCountry id="F345765DFF8EA31718BFFF05FDE8FF27" box="[465,596,223,249]" name="Kazakhstan" pageId="4" pageNumber="121">Kazakhstan</collectingCountry>
(Ivanovsky Mountain Range, West 
<collectingRegion id="4996F82FFF8EA3171D65FF05FBF9FF24" box="[1035,1093,223,250]" country="Russia" name="Altay" pageId="4" pageNumber="121">Altai</collectingRegion>
) and 
<collectingRegion id="4996F82FFF8EA3171DFEFF05FA83FF27" box="[1168,1343,223,249]" country="Russia" name="Altay" pageId="4" pageNumber="121">Altai Republic</collectingRegion>
, 
<collectingCountry id="F345765DFF8EA3171C3CFF05FA21FF27" box="[1362,1437,223,249]" name="Russia" pageId="4" pageNumber="121">Russia</collectingCountry>
