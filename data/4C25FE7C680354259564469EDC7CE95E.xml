<taxonomicName id="1B7BAF995A3DCE6FFF146719AC94E0DE" authorityName="Friese" authorityYear="1914" class="Insecta" family="Halictidae" genus="Sphecodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphecodes brunneipes" order="Hymenoptera" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="brunneipes">Sphecodes brunneipes</taxonomicName>
