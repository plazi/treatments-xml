<taxonomicName id="4C0974AE6405FFB0FF2CFBB3FE1E5356" authorityName=": Han" authorityYear="1994" box="[244,479,1061,1084]" class="Mammalia" family="Erinaceidae" genus="Erinaceus" kingdom="Animalia" order="Erinaceomorpha" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="amurensis">
