<taxonomicName id="4C264D40FF9EFF8CFD7A21D4FC2599AD" authority="(Shear, 1990)" authorityName="Mikhaljova" authorityYear="2021" baseAuthorityName="Shear" baseAuthorityYear="1990" box="[640,937,963,987]" class="Diplopoda" family="Diplomaragnidae" genus="Litovkia" kingdom="Animalia" order="Chordeumatida" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="lysaya">
