<collectingCountry id="E09A7623FFB6FFD418F6FD62FF0CFC93" box="[170,250,762,787]" name="Mexico" pageId="32" pageNumber="33">Mexico</collectingCountry>
(
<collectingRegion id="5A49F851FFB6FFD41955FD62FE86FC93" box="[265,368,762,787]" country="Mexico" name="Veracruz de Ignacio de la Llave" pageId="32" pageNumber="33">Veracruz</collectingRegion>
