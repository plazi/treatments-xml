<emphasis id="AAF9EAA1FFB6FFD418F6FD02FEEEFD33" bold="true" box="[170,280,666,691]" pageId="32" pageNumber="33">Remarks:</emphasis>
<taxonomicName id="5F8D4D30FFB6FFD4197EFD02FE0BFD33" authorityName="Bates" authorityYear="1885" box="[290,509,666,691]" class="Insecta" family="Cerambycidae" genus="Plagiosarus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="literatus">
