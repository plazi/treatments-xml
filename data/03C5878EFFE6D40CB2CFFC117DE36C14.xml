<heading id="D09B81F4FFE6D409B2CFFC117C156B82" bold="true" box="[151,844,927,953]" fontSize="11" level="1" pageId="3" pageNumber="4" reason="1">
<taxonomicName id="4C6C4D1BFFE6D409B2CFFC117C156B82" authority="Michaelsen, 1921" authorityName="Michaelsen" authorityYear="1921" box="[151,844,927,953]" class="Clitellata" family="Lumbriculidae" genus="Rhynchelmis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lumbriculida" pageId="3" pageNumber="4" phylum="Annelida" rank="species" species="tetratheca" subGenus="Rhynchelmis">
<emphasis id="B918EA8AFFE6D409B2CFFC117C156B82" bold="true" box="[151,844,927,953]" pageId="3" pageNumber="4">
<emphasis id="B918EA8AFFE6D409B2CFFC117E6C6B82" bold="true" box="[151,309,927,953]" italics="true" pageId="3" pageNumber="4">Rhynchelmis</emphasis>
(
<emphasis id="B918EA8AFFE6D409B31EFC117EBD6B82" bold="true" box="[326,484,927,953]" italics="true" pageId="3" pageNumber="4">Rhynchelmis</emphasis>
) 
<emphasis id="B918EA8AFFE6D409B3ADFC117D346B82" bold="true" box="[501,621,927,953]" italics="true" pageId="3" pageNumber="4">tetratheca</emphasis>
Michaelsen, 
<number id="3DA424BFFFE6D409B149FC117C156B82" box="[785,844,927,953]" pageId="3" pageNumber="4" value="1921.0">1921</number>
</emphasis>
