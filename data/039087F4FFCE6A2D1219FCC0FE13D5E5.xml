<typeStatus id="54828840FFCE6A2D1112FC45FD94D3CF" box="[527,569,918,941]" pageId="15" pageNumber="124">type</typeStatus>
locality of 
<taxonomicName id="4C394D61FFCE6A2D1396FC63FEBAD3A5" authority="Glover &amp; Taylor, 2007" authorityName="Glover &amp; Taylor" authorityYear="2007" box="[139,279,944,967]" class="Bivalvia" family="Lucinidae" genus="Poumea" kingdom="Animalia" order="Lucinida" pageId="15" pageNumber="124" phylum="Mollusca" rank="species" species="coselia" status="n. sp.">
<emphasis id="B94DEAF0FFCE6A2D1396FC63FEBAD3A5" box="[139,279,944,967]" italics="true" pageId="15" pageNumber="124">Poumea coselia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27E578BFFCE6A2D1200FC63FEFED3A5" box="[285,339,944,967]" pageId="15" pageNumber="124" rank="species">n. sp.</taxonomicNameLabel>
