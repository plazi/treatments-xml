11. 
<taxonomicName id="4C4A9AB9991F856DE8D0F8CAD774F721" authority="Becker, 1907" authorityName="Becker" authorityYear="1907" box="[192,549,1844,1871]" class="Insecta" family="Dolichopodidae" genus="Argyra" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="discedens">
