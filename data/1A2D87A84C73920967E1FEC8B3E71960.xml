<number id="244C24994C74920965A6FF77B2EA1B7D" box="[720,761,295,320]" pageId="12" pageNumber="355" value="341.0">341</number>
; large dorsal anteromedial plate length/width 
<number id="244C24994C749209624FFF77B5771B7D" box="[1337,1380,295,320]" pageId="12" pageNumber="355" value="274.0">274</number>
