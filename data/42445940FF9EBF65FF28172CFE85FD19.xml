<emphasis id="F8993444FF9EBF62FEB91786FE92FD18" box="[271,374,654,678]" italics="true" pageId="11" pageNumber="80">
<typeStatus id="155656F4FF9EBF62FEB91786FE92FD18" box="[271,374,654,678]" pageId="11" pageNumber="80">Holotype</typeStatus>
</emphasis>
male: 
<collectingCountry id="B2FAA8C6FF9EBF62FE751786FDCFFD18" box="[451,555,654,678]" name="Tanzania" pageId="11" pageNumber="80">Tanzania</collectingCountry>
, 
<collectingCountry id="B2FAA8C6FF9EBF62FD831786FD7EFD18" box="[565,666,654,678]" name="Tanzania" pageId="11" pageNumber="80">Zanzibar</collectingCountry>
