<paragraph id="8BA2CA0CFF97CF5999BEFAC4EF3DFAFC" blockId="5.[151,567,1267,1327]" box="[151,333,1303,1327]" pageId="5" pageNumber="40">
(tab. 
<number id="3DD5D82BFF97CF5999F9FAC4EE90FAFC" box="[208,224,1303,1327]" pageId="5" pageNumber="40" value="3.0">3</number>
