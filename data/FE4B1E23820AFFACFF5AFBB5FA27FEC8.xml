<typeStatus id="A9591197820AFFAFFF5AFBDCFF7AFB47" box="[151,200,1150,1171]" pageId="1" pageNumber="62">Type</typeStatus>
species 
<taxonomicName id="B1E2D4B6820AFFAFFED2FBDFFDD0FB47" authority="Forsskal, 1775" authorityName="Forsskal" authorityYear="1775" box="[287,610,1149,1171]" class="Actinopterygii" family="Mullidae" genus="Mullus" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="62" phylum="Chordata" rank="species" species="vittatus">
<emphasis id="44967327820AFFAFFED2FBDFFE04FB47" box="[287,438,1149,1171]" italics="true" pageId="1" pageNumber="62">Mullus vittatus</emphasis>
(Forsskål, 
<number id="C02ABD12820AFFAFFDEAFBDFFDE9FB47" box="[551,603,1149,1171]" pageId="1" pageNumber="62" value="1775.0">1775</number>
)
</taxonomicName>
