<taxonomicName id="70B873E1FFC2FFFF3EA19DE9F415309F" authorityName="USMFC" baseAuthorityName="Lacepede" baseAuthorityYear="1802" box="[675,823,2012,2032]" class="Actinopterygii" family="Lethrinidae" genus="Lethrinus" kingdom="Animalia" order="Perciformes" pageId="11" pageNumber="612" phylum="Chordata" rank="species" species="lentjan">
