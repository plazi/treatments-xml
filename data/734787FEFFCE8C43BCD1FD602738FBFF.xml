, southern 
<collectingCountry id="83F97678FFCE8C43BF13FCF325CAFCFB" box="[853,987,797,821]" name="Kazakhstan" pageId="19" pageNumber="320">Kazakhstan</collectingCountry>
, 
<collectingCountry id="83F97678FFCE8C43BFACFCF32299FCFB" box="[1002,1160,797,821]" name="Turkmenistan" pageId="19" pageNumber="320">Turkmenistan</collectingCountry>
, 
<collectingCountry id="83F97678FFCE8C43B8D1FCF32309FCFB" box="[1175,1304,797,821]" name="Uzbekistan" pageId="19" pageNumber="320">Uzbekistan</collectingCountry>
, 
<collectingCountry id="83F97678FFCE8C43B961FCF32389FCFB" box="[1319,1432,797,821]" name="Tajikistan" pageId="19" pageNumber="320">Tajikistan</collectingCountry>
, 
<collectingCountry id="83F97678FFCE8C43BCD1FCAC26D6FC97" box="[151,199,834,857]" name="Iran" pageId="19" pageNumber="320">Iran</collectingCountry>
, 
<collectingCountry id="83F97678FFCE8C43BC94FCAF274FFC97" box="[210,350,833,857]" name="Afghanistan" pageId="19" pageNumber="320">Afghanistan</collectingCountry>
, northern 
<collectingCountry id="83F97678FFCE8C43BD88FCAF2421FC97" box="[462,560,833,857]" name="Pakistan" pageId="19" pageNumber="320">Pakistan</collectingCountry>
