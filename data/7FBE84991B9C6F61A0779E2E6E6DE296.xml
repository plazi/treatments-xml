<taxonomicName id="80449AC2A2BC7FEF6C0071ECD3A4B9FA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33838" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole longipes (Latreille)" lsidName-HNS="Pheidole longipes (Latreille)" order="Hymenoptera" pageNumber="360" phylum="Arthropoda" rank="species" species="longipes">longipes Pergande</taxonomicName>
