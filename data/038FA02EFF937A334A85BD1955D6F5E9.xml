<taxonomicName id="4C266ABBFF937A334AD5BE98553CF5E8" box="[199,332,1679,1702]" class="Heterotardigrada" family="Echiniscidae" genus="Echiniscus" kingdom="Animalia" order="Echiniscoidea" pageId="15" pageNumber="16" phylum="Tardigrada" rank="species" species="arctomys">
