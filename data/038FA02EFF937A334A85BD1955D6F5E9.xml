<taxonomicName id="4C266ABBFF937A334851BE7B56D9F5CD" box="[579,681,1644,1667]" class="Heterotardigrada" family="Echiniscidae" genus="Echiniscus" kingdom="Animalia" order="Echiniscoidea" pageId="15" pageNumber="16" phylum="Tardigrada" rank="species" species="arctomys">
