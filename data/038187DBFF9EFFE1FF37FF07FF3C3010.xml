<taxonomicName id="4C284D4EFF9EFFE2FF15FBB0FEC7352B" authorityName="Published" authorityYear="2007" box="[181,303,1055,1081]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="antaria">
