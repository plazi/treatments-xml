<keyLead id="7A50461EA537FFB5FF6CF9D9FA35F9D8" pageId="3" pageNumber="504">
<number id="7769F70CA537FFB5FF6CF9D9FF18F9FA" box="[151,177,1541,1563]" pageId="3" pageNumber="504" value="13.0">13</number>
. Sternite 
<number id="7769F70CA537FFB5FEF0F9D9FEBEF9FA" box="[267,279,1541,1563]" pageId="3" pageNumber="504" value="5.0">5</number>
