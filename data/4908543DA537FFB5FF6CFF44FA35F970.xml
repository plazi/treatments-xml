<keyLead id="7A50461EA537FFB5FF6CFBECFA32FB82" pageId="3" pageNumber="504">
<number id="7769F70CA537FFB5FF6CFBECFF1BFBA7" box="[151,178,1072,1094]" pageId="3" pageNumber="504" value="10.0">10</number>
