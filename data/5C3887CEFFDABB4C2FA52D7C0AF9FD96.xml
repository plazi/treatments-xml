<taxonomicName id="13914D5BFFDABB4C2F812D7C0B46FE92" authority="(Schiecke, 1977)" baseAuthorityName="Schiecke" baseAuthorityYear="1977" box="[179,588,259,286]" class="Malacostraca" family="Cyproideidae" genus="Peltocoxa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="10" pageNumber="383" phylum="Arthropoda" rank="species" species="gibbosa">
