<taxonomicName id="4C4E4D60B12A6370FBCC06A7C89FFDEC" authority="Viraktamath &amp; Wesley" authorityName="Viraktamath &amp; Wesley" box="[1103,1435,500,522]" class="Insecta" family="Cicadellidae" genus="Sophonia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="227" phylum="Arthropoda" rank="species" species="complicata">
