</taxonomicName>
, 
<taxonomicName id="4C717C8D180AFFB74365FDE5FE62233C" box="[369,465,564,589]" class="Magnoliopsida" family="Primulaceae" genus="Myrsine" kingdom="Plantae" order="Ericales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="genus">
<emphasis id="B905DB1C180AFFB74365FDE5FE62233C" box="[369,465,564,589]" italics="true" pageId="34" pageNumber="35">Myrsine</emphasis>
</taxonomicName>
bark, 
<taxonomicName id="4C717C8D180AFFB74037FDE5FCA4233C" box="[547,791,564,589]" class="Magnoliopsida" family="Primulaceae" genus="Myrsine" kingdom="Plantae" order="Ericales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="chathamica">
<emphasis id="B905DB1C180AFFB74037FDE5FCA4233C" box="[547,791,564,589]" italics="true" pageId="34" pageNumber="35">Myrsine chathamica</emphasis>
</taxonomicName>
