<taxonomicName id="4C717C8D180AFFB7431CFE6DFD8420A4" box="[264,567,444,469]" class="Magnoliopsida" family="Polygonaceae" genus="Muehlenbeckia" kingdom="Plantae" order="Polygonales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="australis">Muehlenbeckia australis</taxonomicName>
</emphasis>
, 
<taxonomicName id="4C717C8D180AFFB74040FE6DFC8A20A4" box="[596,825,444,469]" class="Magnoliopsida" family="Scrophulariaceae" genus="Myoporum" kingdom="Plantae" order="Lamiales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="laetum">
