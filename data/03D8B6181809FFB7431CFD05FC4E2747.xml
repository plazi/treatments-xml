<emphasis id="B905DB1C180AFFB74131FE45FB3920DC" box="[805,1162,404,429]" italics="true" pageId="34" pageNumber="35">
<taxonomicName id="4C717C8D180AFFB74131FE45FCC620DC" box="[805,885,404,429]" class="Magnoliopsida" family="Apiaceae" genus="Apium" kingdom="Plantae" order="Apiales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="genus">Apium</taxonomicName>
