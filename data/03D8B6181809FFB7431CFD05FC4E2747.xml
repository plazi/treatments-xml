<taxonomicName id="4C717C8D180AFFB7431CFE35FE15208C" box="[264,422,484,509]" class="Magnoliopsida" family="Araliaceae" genus="Pseudopanax" kingdom="Plantae" order="Apiales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="genus">
