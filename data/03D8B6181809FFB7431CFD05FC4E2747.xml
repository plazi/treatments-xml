</taxonomicName>
, 
<taxonomicName id="4C717C8D180AFFB74142FE6DFC5020A4" box="[854,995,444,469]" class="Magnoliopsida" family="Malvaceae" genus="Plagianthus" kingdom="Plantae" order="Malvales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="genus">
