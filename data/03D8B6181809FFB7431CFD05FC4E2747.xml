<taxonomicName id="4C717C8D180AFFB74173FDE5FBB8233C" box="[871,1035,564,589]" class="Magnoliopsida" family="Primulaceae" genus="Myrsine" kingdom="Plantae" order="Ericales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="coxii">
<emphasis id="B905DB1C180AFFB74173FDE5FBB8233C" box="[871,1035,564,589]" italics="true" pageId="34" pageNumber="35">Myrsine coxii</emphasis>
</taxonomicName>
