<taxonomicName id="4C2A4D4BD153B460FD2A51E58D33E1B9" authorityName="Dumortier" authorityYear="1829" box="[718,815,449,475]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="10" pageNumber="231" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
