<taxonomicName id="4C2A4D4BD153B460FE2D51E58CD8E1B8" authority="Ehrh." authorityName="Ehrh." box="[457,708,448,474]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" kingdom="Plantae" order="Fagales" pageId="10" pageNumber="231" phylum="Tracheophyta" rank="species" species="grandifolia">
