<subSubSection id="C369656A3B4CFFB2FF55F9D5FA8BF908" pageId="38" pageNumber="39" type="biology_ecology">
<paragraph id="8BCC36E13B4CFFB2FF55F9D5FA8BF908" blockId="38.[151,1437,1362,2000]" pageId="38" pageNumber="39">
<emphasis id="B907EAF33B4CFFB2FF55F9D5FE80F9E4" bold="true" box="[199,295,1614,1639]" pageId="38" pageNumber="39">Biology.</emphasis>
<taxonomicName id="4C734D623B4CFFB2FEBDF9D5FD86F9E4" box="[303,545,1614,1639]" class="Insecta" family="Eulophidae" genus="Pediobius" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="bruchicida">
