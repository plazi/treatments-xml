<taxonomicName id="45DA7CE0A87AFFE44271FBF7FC6D8522" box="[777,906,1105,1130]" class="Insecta" family="Noctuidae" genus="Spodoptera" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
