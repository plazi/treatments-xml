<taxonomicName id="45DA7CE0A87AFFE44374FBF7FD708522" box="[524,663,1105,1130]" class="Insecta" family="Noctuidae" genus="Helicoverpa" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
