) and 
<taxonomicName id="45DA7CE0A87AFFE441BEFB3FFDB885FA" authority="Rao &amp; Kurian" authorityName="Rao &amp; Kurian" box="[198,607,1177,1202]" class="Insecta" family="Braconidae" genus="Microplitis" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="prodeniae">
<emphasis id="B0AEDB71A87AFFE441BEFB3FFE5F85FA" box="[198,440,1177,1202]" italics="true" pageId="10" pageNumber="11">Microplitis prodeniae</emphasis>
Rao &amp; Kurian
</taxonomicName>
