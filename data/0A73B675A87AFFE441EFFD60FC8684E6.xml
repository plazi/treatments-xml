<taxonomicName id="45DA7CE0A87AFFE441EFFBF7FDE68522" authority="Hubner" authorityName="Hubner" box="[151,513,1105,1130]" class="Insecta" family="Noctuidae" genus="Helicoverpa" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="armigera">
