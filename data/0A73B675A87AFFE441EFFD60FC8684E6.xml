<taxonomicName id="45DA7CE0A87AFFE4426FFBD0FBCF85C6" authority="Wilkinson" authorityName="Wilkinson" box="[791,1064,1141,1166]" class="Insecta" family="Braconidae" genus="Microplitis" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="demolitor">
<emphasis id="B0AEDB71A87AFFE4426FFBD0FC4E85C6" box="[791,937,1141,1166]" italics="true" pageId="10" pageNumber="11">M. demolitor</emphasis>
Wilkinson
</taxonomicName>
, 
<taxonomicName id="45DA7CE0A87AFFE4454BFBD0FAC385C6" authority="Ashmead" authorityName="Ashmead" box="[1075,1316,1141,1166]" class="Insecta" family="Braconidae" genus="Microplitis" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="manilae">
<emphasis id="B0AEDB71A87AFFE4454BFBD0FB5485C6" box="[1075,1203,1141,1166]" italics="true" pageId="10" pageNumber="11">M. manilae</emphasis>
Ashmead
