<taxonomicName id="4C464D18FFF9ED3251D3D02FA493508D" box="[730,920,1385,1410]" class="Insecta" family="Cicadellidae" genus="Scaphoideus" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="asymmetricus">
