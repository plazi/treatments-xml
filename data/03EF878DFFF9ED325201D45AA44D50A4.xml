<taxonomicName id="4C464D18FFF9ED3252A4D02FA55D508D" box="[429,598,1385,1410]" class="Insecta" family="Cicadellidae" genus="Scaphoideus" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="sabourensis">
