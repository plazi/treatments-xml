<emphasis id="B955F31CFF7855D1FF5C46C1FEEAF864" bold="true" box="[151,364,1952,1975]" pageId="199" pageNumber="200">FIGURES 311–315.</emphasis>
<taxonomicName id="4C21548DFF7855D1FEBF46C0FD0FF865" box="[372,649,1953,1975]" class="Arachnida" family="Chthoniidae" genus="Occidenchthonius" kingdom="Animalia" order="Pseudoscorpiones" pageId="199" pageNumber="200" phylum="Arthropoda" rank="species" species="serranoi" status="n. sp.">
<emphasis id="B955F31CFF7855D1FEBF46C0FD0FF865" box="[372,649,1953,1975]" italics="true" pageId="199" pageNumber="200">Occidenchthonius serranoi</emphasis>
</taxonomicName>
<emphasis id="B955F31CFF7855D1FD5B46C0FD4CF864" bold="true" box="[656,714,1953,1974]" pageId="199" pageNumber="200">
