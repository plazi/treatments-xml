<taxonomicName id="7984E2A7DA2A3E4C0B52FE84FE2FFE9C" baseAuthorityName="Macleay" baseAuthorityYear="1883" box="[260,417,260,285]" class="Chondrichthyes" family="Dasyatidae" genus="Urogymnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="16" pageNumber="217" phylum="Chordata" rank="species" species="granulatus">
<emphasis id="8CF04536DA2A3E4C0B52FE84FE2FFE9C" box="[260,417,260,285]" italics="true" pageId="16" pageNumber="217">U. granulatus</emphasis>
</taxonomicName>
and 
<taxonomicName id="7984E2A7DA2A3E4C0B8DFE84FDF3FE9D" baseAuthorityName="Bloch &amp; Schneider" baseAuthorityYear="1801" box="[475,637,261,284]" class="Chondrichthyes" family="Dasyatidae" genus="Urogymnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="16" pageNumber="217" phylum="Chordata" rank="species" species="asperrimus">
<emphasis id="8CF04536DA2A3E4C0B8DFE84FDF3FE9D" box="[475,637,261,284]" italics="true" pageId="16" pageNumber="217">U. asperrimus</emphasis>
</taxonomicName>
