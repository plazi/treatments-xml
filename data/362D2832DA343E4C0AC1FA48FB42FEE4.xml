<collectingCountry id="C693D9B4DA2A3E4C0959FE85FC0FFE9C" box="[783,897,260,285]" name="Sri Lanka" pageId="16" pageNumber="217">Sri Lanka</collectingCountry>
, and a specimen consistent with 
<taxonomicName id="7984E2A7DA2A3E4C0EACFE84FA12FE9D" baseAuthorityName="Bloch &amp; Schneider" baseAuthorityYear="1801" box="[1274,1436,261,284]" class="Chondrichthyes" family="Dasyatidae" genus="Urogymnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="16" pageNumber="217" phylum="Chordata" rank="species" species="asperrimus">
<emphasis id="8CF04536DA2A3E4C0EACFE84FA12FE9D" box="[1274,1436,261,284]" italics="true" pageId="16" pageNumber="217">U. asperrimus</emphasis>
</taxonomicName>
