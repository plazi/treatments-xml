<quantity id="4CEF9B25FF01FF259890FAEBFB113531" box="[237,336,1305,1332]" metricMagnitude="1" metricUnit="m" metricValue="2.6060399999999997" metricValueMax="4.99872" metricValueMin="0.21336" pageId="144" pageNumber="145" unit="ft" value="85.5" valueMax="164.0" valueMin="7.0">7–164 ft</quantity>
) (min.: Personal communication: Scripps Institution of Oceanography Fish Collection, La Jolla, California; max.: 
<bibRefCitation id="EF864B31FF01FF2599F1FACFF80D3552" author="Craig" box="[396,588,1341,1367]" etAl="et al." firstAuthor="Craig" pageId="144" pageNumber="145" refId="ref272592" refString="Craig, M. T., Sadovy de Mitcheson, Y. J. &amp; Heemstra, P. C. (2011) Groupers of the World. A Field and Market Guide. CRC Press, Boca Raton." type="book" year="2011">
Craig 
<emphasis id="B963EAD2FF01FF2599ACFACCF84B3552" box="[465,522,1341,1367]" italics="true" pageId="144" pageNumber="145">et al.</emphasis>
2011
</bibRefCitation>
). 
