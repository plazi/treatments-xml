<quantity id="4CEF9B25FF01FF259D51FB07FF183515" box="[1324,1369,1269,1296]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="144" pageNumber="145" unit="m" value="2.0">2 m</quantity>
to 
<quantity id="4CEF9B25FF01FF259DFDFB07FA9D3531" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="144" pageNumber="145" unit="m" value="50.0">50 m</quantity>
