<taxonomicName id="FADBB3A4FFA47912FCCF22F361D48B89" box="[868,979,1970,1995]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="21" pageNumber="22" phylum="Annelida" rank="genus">
