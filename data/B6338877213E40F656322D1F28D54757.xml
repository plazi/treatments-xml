8. 
<taxonomicName id="A60C3B4CAB30F71363CE8B0CE4587534" LSID="http://zoobank.org/B88459B2-D6F8-486A-B85A-37EA2EA0135A" authority="Liebherr, 2016" authorityName="Liebherr" authorityYear="2016" class="Insecta" family="Carabidae" genus="Cyphocoleus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyphocoleus monteithi" order="Coleoptera" pageId="25" pageNumber="236" phylum="Arthropoda" rank="species" species="monteithi" status="sp. n.">Cyphocoleus monteithi</taxonomicName>
