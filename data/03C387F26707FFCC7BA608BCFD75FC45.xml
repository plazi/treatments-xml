<collectingCountry id="F37D76746704FFCC7A8C0A60FD9FFD8D" box="[445,512,587,610]" name="Japan" pageId="6" pageNumber="87">Japan</collectingCountry>
. 
<taxonomicName id="4C6A4D676704FFCC793B0A60FCB8FD8C" authority="Linnaeus" authorityName="Linnaeus" box="[522,807,586,611]" class="Malacostraca" family="Oniscidae" genus="Oniscus" kingdom="Animalia" order="Isopoda" pageId="6" pageNumber="87" phylum="Arthropoda" rank="species" species="asellus">
