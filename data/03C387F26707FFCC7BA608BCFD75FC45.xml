<taxonomicName id="4C6A4D676704FFCC7E2C0A61FAE8FD8C" box="[1309,1399,586,611]" class="Malacostraca" kingdom="Animalia" order="Isopoda" pageId="6" pageNumber="87" phylum="Arthropoda" rank="order">Isopoda</taxonomicName>
