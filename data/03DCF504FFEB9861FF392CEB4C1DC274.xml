<subSubSection id="C36F1799FFEB9861FF392CEB4B2FC5AF" box="[151,591,905,931]" pageId="30" pageNumber="31" type="nomenclature">
<paragraph id="8BCA4412FFEB9861FF392CEB4B2FC5AF" blockId="30.[151,591,905,931]" box="[151,591,905,931]" pageId="30" pageNumber="31">
<heading id="D082F37EFFEB9861FF392CEB4B2FC5AF" bold="true" box="[151,591,905,931]" fontSize="11" level="1" pageId="30" pageNumber="31" reason="1">
<emphasis id="B9019800FFEB9861FF392CEB4B2FC5AF" bold="true" box="[151,591,905,931]" pageId="30" pageNumber="31">
Genus 20. 
<taxonomicName id="4C753F91FFEB9861FEB42CEB4B2FC5AF" authority="Lelej, 2005" authorityName="Lelej" authorityYear="2005" box="[282,591,905,931]" class="Insecta" family="Mutillidae" genus="Kurzenkotilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
<emphasis id="B9019800FFEB9861FEB42CEB48DFC5AF" bold="true" box="[282,447,905,931]" italics="true" pageId="30" pageNumber="31">Kurzenkotilla</emphasis>
<bibRefCitation id="EFE439E3FFEB9861FE6B2CEB4B2FC5AF" author="Lelej, A. S." box="[453,591,905,931]" pageId="30" pageNumber="31" refId="ref59074" refString="Lelej, A. S. (2005) Catalogue of the Mutillidae (Hymenoptera) of the Oriental Region. Dalnauka, Vladivostok, 252 pp." type="book" year="2005">Lelej, 2005</bibRefCitation>
