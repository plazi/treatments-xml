<date id="FFE0101CFFC9FF9DFF45FC637E5258F9" box="[151,280,977,1001]" pageId="5" pageNumber="442" value="2011-07-26">
<number id="3D9624FBFFC9FF9DFF45FC637FFF58F9" box="[151,181,977,1001]" pageId="5" pageNumber="442" value="26.0">26</number>
