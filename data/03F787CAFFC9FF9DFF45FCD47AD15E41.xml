<date id="FFE0101CFFC9FF9DFB2CFC637ACA58F9" box="[1278,1408,977,1001]" pageId="5" pageNumber="442" value="2012-07-27">
<number id="3D9624FBFFC9FF9DFB2CFC637A5158F9" box="[1278,1307,977,1001]" pageId="5" pageNumber="442" value="27.0">27</number>
