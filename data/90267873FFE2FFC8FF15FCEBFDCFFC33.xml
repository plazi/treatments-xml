<emphasis id="2AFB1577FFE3FFCFFCA4FF21FBA9FF54" box="[873,1130,212,235]" italics="true" pageId="27" pageNumber="27">Bunomys chrysocomus</emphasis>
</taxonomicName>
, 
<taxonomicName id="DF8FB2E6FFE3FFCFFBB6FF21FD35FEB7" authorityName="Thomas" authorityYear="1896" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="coelestis">
<emphasis id="2AFB1577FFE3FFCFFBB6FF21FD35FEB7" italics="true" pageId="27" pageNumber="27">B. coelestis</emphasis>
</taxonomicName>
, 
<taxonomicName id="DF8FB2E6FFE3FFCFFCDCFF07FC59FEB7" authorityName="Musser" authorityYear="1991" box="[785,922,241,264]" class="Mammalia" family="Muridae" genus="Bunomys" kingdom="Animalia" order="Rodentia" pageId="27" pageNumber="27" phylum="Chordata" rank="species" species="prolatus">
<emphasis id="2AFB1577FFE3FFCFFCDCFF07FC59FEB7" box="[785,922,241,264]" italics="true" pageId="27" pageNumber="27">B. prolatus</emphasis>
</taxonomicName>
, 
<emphasis id="2AFB1577FFE3FFCFFC78FF07FBFAFEB7" box="[949,1081,243,264]" italics="true" pageId="27" pageNumber="27">
