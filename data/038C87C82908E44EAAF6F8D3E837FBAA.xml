<number id="3DED24F9290AE44EAE50FCF8E854FCA9" box="[1073,1111,799,823]" pageId="7" pageNumber="8" value="3.9">3.9</number>
; PW 
<number id="3DED24F9290AE44EAEF5FCF8E8B9FCA9" box="[1172,1210,799,823]" pageId="7" pageNumber="8" value="2.9">2.9</number>
; ID 
<number id="3DED24F9290AE44EAE8AFCF8E912FCA9" box="[1259,1297,799,823]" pageId="7" pageNumber="8" value="1.8">1.8</number>
; chelicera II 
<number id="3DED24F9290AE44EAAF6FCA3ECBEFCC2" box="[151,189,836,860]" pageId="7" pageNumber="8" value="2.9">2.9</number>
; III 
<number id="3DED24F9290AE44EAA8DFCA3ED11FCC2" box="[236,274,836,860]" pageId="7" pageNumber="8" value="1.8">1.8</number>
; pedipalpus 
<number id="3DED24F9290AE44EABFFFCA3EDC7FCC2" box="[414,452,836,860]" pageId="7" pageNumber="8" value="9.6">9.6</number>
; leg 
<date id="FF9B101E290AE44EAB98FCA2EE2BFCC2" box="[505,552,836,860]" pageId="7" pageNumber="8" value="1916-01">
I 
<number id="3DED24F9290AE44EA868FCA3EE2BFCC2" box="[521,552,836,860]" pageId="7" pageNumber="8" value="16.0">16</number>
</date>
; 
<date id="FF9B101E290AE44EA855FCA2EE6EFCC2" box="[564,621,836,860]" pageId="7" pageNumber="8" value="1921-02">
II 
<number id="3DED24F9290AE44EA82FFCA3EE6EFCC2" box="[590,621,836,860]" pageId="7" pageNumber="8" value="21.0">21</number>
</date>
