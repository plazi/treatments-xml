<date id="FF9B101E290AE44EA819FCA2EEB9FCC2" box="[632,698,836,860]" pageId="7" pageNumber="8" value="1916-03">
III 
<number id="3DED24F9290AE44EA8FAFCA3EEB9FCC2" box="[667,698,836,860]" pageId="7" pageNumber="8" value="16.0">16</number>
</date>
; 
<date id="FF9B101E290AE44EA8A7FCA2EF0DFCC2" box="[710,782,836,860]" pageId="7" pageNumber="8">
IV 
<number id="3DED24F9290AE44EA88AFCA3EF09FCC2" box="[747,778,836,860]" pageId="7" pageNumber="8" value="18.0">18</number>
