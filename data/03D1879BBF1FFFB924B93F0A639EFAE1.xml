<taxonomicName id="4C784D0EBF1FFFB9278A3E796182FAC1" box="[758,919,1283,1306]" class="Insecta" family="Stylopidae" genus="Stylops" kingdom="Animalia" order="Strepsiptera" pageId="22" pageNumber="327" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="B90CEA9FBF1FFFB9278A3E796156FAC1" box="[758,835,1283,1306]" italics="true" pageId="22" pageNumber="327">Stylops</emphasis>
