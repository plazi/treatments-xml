<taxonomicName id="3EFF26851A26F47E3CCA4C5F1E08FC95" authorityName="Dumortier" authorityYear="1829" box="[479,590,853,879]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
) (
<bibRefCitation id="9D6E20F71A26F47E3F704C5F1F48FC95" author="Godfrey, G. L." box="[613,782,853,879]" pageId="54" pageNumber="55" pagination="1 - 265" refId="ref30677" refString="Godfrey, G. L. (1972) A review and reclassification of larvae of the subfamily Hadeninae (Lepidoptera, Noctuidae) of American north of Mexico. U. S. Department of Agriculture Technical Bulletin, 1450, 1 - 265." type="journal article" year="1972">Godfrey 1972</bibRefCitation>
