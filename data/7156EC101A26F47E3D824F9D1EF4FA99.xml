<taxonomicName id="3EFF26851A26F47E38304C3A19D2FCB0" authorityName="Dumortier" authorityYear="1829" box="[1317,1428,816,842]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
