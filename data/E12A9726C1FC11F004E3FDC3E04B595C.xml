<paragraph id="1885EE984184A403A8702789F4991F61" pageId="null" pageNumber="376">
Artengruppe des 
<taxonomicName id="66AA22FBF39344C161B82D8AD05F7217" authority="L." class="Magnoliopsida" family="Campanulaceae" genus="Phyteuma" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="376" phylum="Tracheophyta" rank="species" species="spicatum">
