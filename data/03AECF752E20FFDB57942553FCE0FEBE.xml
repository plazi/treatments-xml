<geoCoordinate id="EE3318A42E21FFDB562D22DAFE7FFF49" box="[302,446,153,180]" degrees="43" direction="west" minutes="10" orientation="longitude" pageId="7" pageNumber="579" precision="15" seconds="46" value="-43.179447">43º10’46”W</geoCoordinate>
