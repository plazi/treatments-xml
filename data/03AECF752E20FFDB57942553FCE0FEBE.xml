<geoCoordinate id="EE3318A42E21FFDB520C22DAFA56FF4E" box="[1295,1431,153,179]" degrees="23" direction="south" minutes="02" orientation="latitude" pageId="7" pageNumber="579" precision="15" seconds="37" value="-23.043612">23°02’37”S</geoCoordinate>
; 
<geoCoordinate id="EE3318A42E21FFDB579422FFFEEBFF2A" box="[151,298,188,215]" degrees="43" direction="west" minutes="35" orientation="longitude" pageId="7" pageNumber="579" precision="15" seconds="47" value="-43.596386">43°35’47”W</geoCoordinate>
