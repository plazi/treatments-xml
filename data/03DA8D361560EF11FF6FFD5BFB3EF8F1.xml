<collectionCode id="ED62A4E51560EF11FCC1FAB9FCC9FAA9" box="[825,882,1287,1311]" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="6" pageNumber="28">ZSM</collectionCode>
Sp 
<number id="3DBB2E071560EF11FC5AFAB9FC66FAA9" box="[930,989,1287,1311]" pageId="6" pageNumber="28" value="1508.0">1508</number>
; Namanyere, 
<quantity id="4C8B91C51560EF11FB85FAB9FB76FAA9" box="[1149,1229,1287,1311]" metricMagnitude="3" metricUnit="m" metricValue="1.29" pageId="6" pageNumber="28" unit="m" value="1290.0">1290m</quantity>
, 
<number id="3DBB2E071560EF11FB23FAB9FB52FAA9" box="[1243,1257,1287,1311]" pageId="6" pageNumber="28" value="1.0">1</number>
ɗ; Livingstone Mts., 
<quantity id="4C8B91C51560EF11FF2FFA93FE93FAF3" box="[215,296,1325,1349]" metricMagnitude="3" metricUnit="m" metricValue="2.05" pageId="6" pageNumber="28" unit="m" value="2050.0">2050m</quantity>
, Mlangani, 
