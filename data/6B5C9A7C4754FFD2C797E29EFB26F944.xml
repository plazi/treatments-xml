<collectingCountry id="9BE26BFA4754FFD2C659E163FE5CF945" box="[345,393,1663,1686]" name="Iran" pageId="5" pageNumber="406">Iran</collectingCountry>
, it has been recorded from 
<taxonomicName id="24F550E94754FFD2C5B9E162FCACF944" box="[697,889,1662,1687]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="species" species="avellana">
<emphasis id="D181F7784754FFD2C5B9E162FCACF944" box="[697,889,1662,1687]" italics="true" pageId="5" pageNumber="406">Corylus avellana</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E94754FFD2C488E162FBD3F944" box="[904,1030,1662,1687]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
