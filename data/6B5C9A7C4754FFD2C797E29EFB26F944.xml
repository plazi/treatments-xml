<paragraph id="E34A2B6A4754FFD2C7C7E2F2FC6CF9D4" blockId="5.[151,1436,1482,1687]" box="[199,953,1518,1543]" pageId="5" pageNumber="406">
<emphasis id="D181F7784754FFD2C7C7E2F2FE16F9D4" bold="true" box="[199,451,1518,1543]" pageId="5" pageNumber="406">General distribution.</emphasis>
<collectingCountry id="9BE26BFA4754FFD2C6CAE2F2FDF8F9D4" box="[458,557,1518,1543]" name="Bulgaria" pageId="5" pageNumber="406">Bulgaria</collectingCountry>
