<taxonomicName id="24F550E94754FFD2C583E147FD3EF9A1" box="[643,747,1627,1650]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F7784754FFD2C583E147FD3EF9A1" box="[643,747,1627,1650]" italics="true" pageId="5" pageNumber="406">Carpinus</emphasis>
</taxonomicName>
, 
<taxonomicName id="24F550E94754FFD2C5FBE146FC81F9A0" box="[763,852,1626,1651]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F7784754FFD2C5FBE146FC81F9A0" box="[763,852,1626,1651]" italics="true" pageId="5" pageNumber="406">Corylus</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E94754FFD2C464E146FC30F9A0" box="[868,997,1626,1651]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
), and 
<taxonomicName id="24F550E94754FFD2C32BE140FBA4F9A0" box="[1067,1137,1628,1651]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F7784754FFD2C32BE140FBA4F9A0" box="[1067,1137,1628,1651]" italics="true" pageId="5" pageNumber="406">Fagus</emphasis>
</taxonomicName>
(
<taxonomicName id="24F550E94754FFD2C382E147FB3BF9A0" box="[1154,1262,1627,1651]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="406" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
) (
<bibRefCitation id="8764569B4754FFD2C205E146FA42F9A0" author="Schedl" box="[1285,1431,1626,1651]" pageId="5" pageNumber="406" refString="Schedl, K. E. (1961) Borkenkafer aus der Turkei. II. Mitteilung. Anzeiger fur Schadlingskunde, 34, 184 - 188." type="journal article" year="1961">
