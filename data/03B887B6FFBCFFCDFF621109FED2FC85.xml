<taxonomicName id="4C114D23FFBDFFCDFF3215FBFEA4FF4C" authority="Lesson, 1832" authorityName="Lesson" authorityYear="1832" box="[199,347,224,248]" class="Ascidiacea" family="Holozoidae" genus="Holozoa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aplousobranchia" pageId="17" pageNumber="18" phylum="Chordata" rank="species" species="cylindrica">
