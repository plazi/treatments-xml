<quantity id="4CE99B45FFBDFFCDFB0E158DFAB1FF1B" box="[1275,1358,151,175]" metricMagnitude="0" metricUnit="m" metricValue="1.08" pageId="17" pageNumber="18" unit="cm" value="108.0">108 cm</quantity>
long. One sequence for specimen A 
<number id="3DD92487FFBDFFCDFDED15A6FDDBFF60" box="[536,548,188,212]" pageId="17" pageNumber="18" value="3.0">3</number>
<collectionCode id="ED00AE65FFBDFFCDFDDE15A6FDA7FF60" box="[555,600,188,212]" httpUri="http://grbio.org/cool/z70e-zmfg" name="Dinamation International Society" pageId="17" pageNumber="18">DIS</collectionCode>
