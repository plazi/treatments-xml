<paragraph id="6E3E35F08A9E2110547428D97D7B6281">
<taxonomicName id="A6788C3F4FC4485D3DEE241800A14CFE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32756" class="Insecta" family="Formicidae" genus="Mystrium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mystrium mysticum Roger" lsidName-HNS="Mystrium mysticum Roger" order="Hymenoptera" phylum="Arthropoda" rank="species" species="mysticum">Mystrium mysticum Roger</taxonomicName>
