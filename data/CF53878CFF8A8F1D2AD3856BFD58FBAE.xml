<taxonomicName id="80FA4D19FF8A8F1D2AF784F6FEDBFC24" authorityName="Fauvel" authorityYear="1898" box="[179,276,962,984]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="762" phylum="Arthropoda" rank="species" species="filaria">
