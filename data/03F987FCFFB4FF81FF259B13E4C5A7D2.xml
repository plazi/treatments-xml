<taxonomicName id="4C504D69FFB4FF81FF259B9EE049A5E7" box="[151,285,1419,1440]" class="Insecta" family="Pseudococcidae" genus="Puto" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="vaccinii">
