<quantity id="3A1D9B70FFF7FFB6FE9AFDDFFE24FD0B" box="[340,398,623,647]" metricMagnitude="1" metricUnit="m" metricValue="3.0" pageId="19" pageNumber="1287" unit="m" value="30.0">30 m</quantity>
