<treatmentCitationGroup id="AB207240FFBAFFA6FF7A1AA9FDA1FC5C" box="[151,563,989,1012]" pageId="28" pageNumber="29">
<treatmentCitation id="0A91737FFFBAFFA6FF7A1AA9FDBDFC5C" author="Mosely, M. E." box="[151,559,989,1012]" page="158" pageId="28" pageNumber="29" year="1937">
<taxonomicName id="4C302EEDFFBAFFA6FF7A1AA9FDBDFC5C" authority="Mosely, 1937: 158" authorityName="Mosely" authorityPageNumber="158" authorityYear="1937" box="[151,559,989,1012]" class="Insecta" family="Glossosomatidae" genus="Mexitrichia" kingdom="Animalia" order="Trichoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="leroda">
