<taxonomicName id="7E12DA26187119393D083D238D3839AA" authorityName="P. H. Hoekstra" authorityYear="2016" class="Magnoliopsida" family="Annonaceae" genus="Monanthotaxis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Monanthotaxis latistamina" order="Magnoliales" pageId="7" pageNumber="78" phylum="Tracheophyta" rank="species" species="latistamina">
