<taxonomicName id="4C774D01FF8E8F6FE624FC1CFD3C611E" authority="(Frey, 1975)" authorityName="Fuhrmann" authorityYear="2021" baseAuthorityName="Frey" baseAuthorityYear="1975" box="[346,696,1005,1030]" class="Insecta" family="Scarabaeidae" genus="Zagurya" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="400" phylum="Arthropoda" rank="species" species="eusquamosa" status="new combination">
