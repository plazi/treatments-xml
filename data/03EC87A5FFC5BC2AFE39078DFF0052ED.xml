S346, S347, 
<collectingCountry id="F3527623FFC6BC2AFDC800CFFC7A55CF" box="[618,928,926,950]" name="South Africa" pageId="6" pageNumber="1992">Republic of South Africa</collectingCountry>
, 
<location id="8E9A6068FFC6BC2AFC1500CFFBE455CF" LSID="urn:lsid:plazi:treatment:03EC87A5FFC5BC2AFE39078DFF0052ED:8E9A6068FFC6BC2AFC1500CFFBE455CF" box="[951,1086,926,950]" country="South Africa" municipality="Durban" name="Park Rynie" pageId="6" pageNumber="1992">Park Rynie</location>
