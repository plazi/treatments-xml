</paragraph>
<paragraph id="4A4436AD500E1C78FF75FA96FB10FAE1" blockId="9.[151,1229,1350,1373]" box="[151,1229,1350,1373]" pageId="9" pageNumber="258">
<taxonomicName id="8DFB4D2E500E1C78FF75FA96FE1AFAE1" authority="LeConte, 1878:416" authorityName="LeConte" authorityPageNumber="416" authorityYear="1878" box="[151,455,1350,1373]" class="Insecta" family="Chrysomelidae" genus="Argopistes" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="258" phylum="Arthropoda" rank="species" species="scyrtoides">
