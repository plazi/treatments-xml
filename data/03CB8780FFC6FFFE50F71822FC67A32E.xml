<quantity id="4C9A9B73FFC7FFFC54BB1D31FAD1A50C" box="[1243,1346,512,536]" metricMagnitude="-2" metricUnit="m" metricValue="4.12" pageId="5" unit="mm" value="41.2">41.2 mm</quantity>
