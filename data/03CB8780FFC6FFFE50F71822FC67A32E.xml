<taxonomicName id="4C624D15FFC7FFFC53E91D31FB99A503" box="[905,1034,512,536]" class="Reptilia" family="Sphaerodactylidae" genus="Sphaerodactylus" kingdom="Animalia" order="Squamata" pageId="5" phylum="Chordata" rank="species" species="rosaurae">
