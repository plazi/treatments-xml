<emphasis id="A8EC5247FF8B6A1AF7DBF944FE6CF95C" box="[137,385,1736,1759]" italics="true" pageId="34" pageNumber="34">
<taxonomicName id="5D98F5D6FF8B6A1AF7DBF944FE90F95C" authorityName="Markham" authorityYear="1985" box="[137,381,1736,1759]" class="Malacostraca" family="Bopyridae" genus="Bopyrella" kingdom="Animalia" order="Isopoda" pageId="34" pageNumber="34" phylum="Arthropoda" rank="species" species="tanytelson">Bopyrella tanytelson</taxonomicName>
