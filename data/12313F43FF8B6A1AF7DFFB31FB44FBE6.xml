<taxonomicName id="5D98F5D6FF8B6A1AF7A6F907FDC5F921" authorityName="Coutiere" authorityYear="1905" box="[244,552,1675,1698]" class="Malacostraca" family="Alpheidae" genus="Synalpheus" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="34" phylum="Arthropoda" rank="species" species="streptodactylus">
