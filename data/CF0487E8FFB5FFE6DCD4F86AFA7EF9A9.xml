<collectingCountry id="3FBA766EFFB5FFE6D8B2F8C9FAD4FA49" box="[1190,1290,1442,1466]" name="Albania" pageId="11" pageNumber="10">Albania</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6D902F8C9FE80FA29" name="Bosnia and Herzegovina" pageId="11" pageNumber="10">Bosnia and Herzegovina</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6DD7DF8A9FE06FA29" box="[361,472,1474,1498]" name="Bulgaria" pageId="11" pageNumber="10">Bulgaria</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6DDF7F8A9FD9DFA29" box="[483,579,1474,1498]" name="Croatia" pageId="11" pageNumber="10">Croatia</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6DE59F8A9FD7AFA29" box="[589,676,1474,1498]" name="Greece" pageId="11" pageNumber="10">Greece</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6DEA4F8A9FC99FA29" box="[688,839,1474,1498]" name="Serbia" pageId="11" pageNumber="10">Montenegro</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6DF45F8A9FC1DFA29" box="[849,963,1474,1498]" name="Romania" pageId="11" pageNumber="10">Romania</collectingCountry>
, 
<collectingCountry id="3FBA766EFFB5FFE6DFDAF8A9FBC0FA29" box="[974,1054,1474,1498]" name="Serbia" pageId="11" pageNumber="10">Serbia</collectingCountry>
