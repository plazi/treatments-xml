<taxonomicName id="A6834D44FFC5FFE6CA41FCD76BEDFCB4" authorityName="Rondani" authorityYear="1840" box="[857,975,838,863]" class="Insecta" family="Cecidomyiidae" genus="Dasineura" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="430" phylum="Arthropoda" rank="genus">
<emphasis id="53F7EAD5FFC5FFE6CA41FCD76BEDFCB4" box="[857,975,838,863]" italics="true" pageId="29" pageNumber="430">Dasineura</emphasis>
</taxonomicName>
