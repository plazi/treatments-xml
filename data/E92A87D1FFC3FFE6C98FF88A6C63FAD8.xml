<subSubSection id="2999654CFFC5FFE6C9DFFCD46C12FC43" pageId="29" pageNumber="430" type="biology_ecology">
<paragraph id="613C36C7FFC5FFE6C9DFFCD46C12FC43" blockId="29.[151,1437,153,1331]" pageId="29" pageNumber="430">
<emphasis id="53F7EAD5FFC5FFE6C9DFFCD469B9FCB4" bold="true" box="[199,411,836,863]" pageId="29" pageNumber="430">Host information.</emphasis>
<taxonomicName id="A6834D44FFC5FFE6C8B8FCD76A7CFCB4" authorityName="Matsuo" authorityYear="2020" box="[416,606,838,863]" class="Insecta" family="Torymidae" genus="Torymus" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="430" phylum="Arthropoda" rank="species" species="rugosus">
