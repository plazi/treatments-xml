<taxonomicName id="4C734D5DFF95FF97FD120AD1FC532EC0" authority="Kuang, 1991" authorityName="Kuang" authorityYear="1991" box="[694,1018,1759,1785]" class="Arachnida" family="Eriophyidae" genus="Tetra" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="46" phylum="Arthropoda" rank="species" species="cleomis">
<emphasis id="B907EACCFF95FF97FD120AD1FCFF2EC1" box="[694,854,1759,1784]" italics="true" pageId="9" pageNumber="46">Tetra cleomis</emphasis>
