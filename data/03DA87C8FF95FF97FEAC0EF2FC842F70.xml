<number id="3DBB24F9FF95FF97FD320F49FD6B2B58" box="[662,706,839,865]" pageId="9" pageNumber="46" value="198.0">198</number>
long, 
<number id="3DBB24F9FF95FF97FCAA0F49FC822B58" box="[782,811,839,865]" pageId="9" pageNumber="46" value="60.0">60</number>
