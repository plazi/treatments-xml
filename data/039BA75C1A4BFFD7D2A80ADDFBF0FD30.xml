<emphasis id="B946CA581A48FFD7D3930BCCFD99FD30" bold="true" box="[458,522,677,694]" italics="true" pageId="22" pageNumber="333">FIG. 10.</emphasis>
<taxonomicName id="4C326DC91A48FFD7D0490BCCFC9AFD30" authority="(Karsch)" baseAuthorityName="Karsch" baseAuthorityYear="1893" box="[528,777,677,694]" class="Insecta" family="Acrididae" genus="Eucoptacra" kingdom="Animalia" order="Orthoptera" pageId="22" pageNumber="333" phylum="Arthropoda" rank="species" species="anguliflava">
