<taxonomicName id="84EE38583864448D39F9A81A0535B430" authorityName="Aublet" authorityYear="1775" class="Magnoliopsida" family="Myristicaceae" genus="Virola" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Virola" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">Virola</taxonomicName>
