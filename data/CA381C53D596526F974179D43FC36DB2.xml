<taxonomicName id="148DE47FB3E31F8D6E456800BBD568A0" class="Dicotyledoneae" family="Myristicaceae" genus="V." higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="V. laevigata" order="Magnoliales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="laevigata">
<emphasis id="6761B9C5B3BCADD5DD0FA7D17861FB25" italics="true" pageId="0" pageNumber="1">V. laevigata</emphasis>
