<taxonomicName id="FD6C273066144FB7DED1F044BA06331F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:30612" class="Insecta" family="Formicidae" genus="Meranoplus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Meranoplus diversus Smith" order="Hymenoptera" pageNumber="386" phylum="Arthropoda" rank="species" species="diversus">Meranoplus diversus F. Smith</taxonomicName>
