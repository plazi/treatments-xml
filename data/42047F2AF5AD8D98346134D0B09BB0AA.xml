<taxonomicName id="23479357922943C63BFE7BCCA9052905" class="Insecta" family="Carabidae" genus="Geocharidius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Geocharidius minimus" order="Coleoptera" pageId="25" pageNumber="86" phylum="Arthropoda" rank="species" species="minimus">Geocharidius minimus</taxonomicName>
