<taxonomicName id="9A2732B277898B1A7DC1204E29DE0054" class="Insecta" family="Carabidae" genus="Geocharidius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Geocharidius antigua" order="Coleoptera" pageId="25" pageNumber="86" phylum="Arthropoda" rank="species" species="antigua">Geocharidius antigua</taxonomicName>
