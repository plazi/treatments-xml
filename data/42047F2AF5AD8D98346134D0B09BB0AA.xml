<taxonomicName id="76442DDA3E92C2FB1B08A75D361B6C25" class="Insecta" family="Carabidae" genus="Geocharidius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Geocharidius vignatagliantii" order="Coleoptera" pageId="25" pageNumber="86" phylum="Arthropoda" rank="species" species="vignatagliantii">Geocharidius vignatagliantii</taxonomicName>
