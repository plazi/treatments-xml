<quantity id="4CC6B59C2F3DFF87068BFD08E5782C8F" box="[1236,1394,696,723]" metricMagnitude="2" metricUnit="m" metricValue="7.25" metricValueMax="12.5" metricValueMin="2.0" pageId="80" pageNumber="81" unit="m" value="725.0" valueMax="1250.0" valueMin="200.0">200–1250 m</quantity>
