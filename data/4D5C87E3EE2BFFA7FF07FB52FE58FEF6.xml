</taxonomicNameLabel>
, 
<taxonomicName id="02F54D76EE2AFFA7FA95FF33FAEEFF34" box="[1285,1391,152,177]" class="Adenophorea" family="Tripylidae" genus="Tripylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="21" pageNumber="22" phylum="Nematoda" rank="species" species="tamaki" status="sp. nov.">
<emphasis id="F781EAE7EE2AFFA7FA95FF33FAEEFF34" box="[1285,1391,152,177]" italics="true" pageId="21" pageNumber="22">T. tamaki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="ECB2579CEE2AFFA7FAE9FF3CFF4AFF5D" pageId="21" pageNumber="22" rank="species">
<emphasis id="F781EAE7EE2AFFA7FAE9FF3CFF4AFF5D" bold="true" pageId="21" pageNumber="22">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="02F54D76EE2AFFA7FF4CFF14FEB4FF5D" box="[220,309,191,216]" class="Adenophorea" family="Tripylidae" genus="Tripyla" kingdom="Animalia" order="Enoplida" pageId="21" pageNumber="22" phylum="Nematoda" rank="species" species="sheri">
