(
<taxonomicName id="4C4B14961706ED70FF33FAE2FE8FFA95" authorityName="S.F.Gray" authorityYear="1822" box="[179,291,1346,1368]" class="Magnoliopsida" family="Betulaceae" kingdom="Plantae" order="Fagales" pageId="1" pageNumber="226" phylum="Tracheophyta" rank="family">Betulaceae</taxonomicName>
