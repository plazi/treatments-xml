<paragraph id="3342956C5306AB6075EABFC0BD015381" pageId="null" pageNumber="199">
<taxonomicName id="D7827AE9843094FEFED8940D1D002E5A" authority="L." class="Magnoliopsida" family="Plantaginaceae" genus="Veronica" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="199" phylum="Tracheophyta" rank="species" species="agrestis">
Veronica 
<normalizedToken id="2F56099B599A37BF47A3ECF55C6A158A" originalValue="agréstis" pageId="null" pageNumber="199">agrestis</normalizedToken>
<authorityName id="FE38085E7BF38A1A985F0A815D756760" pageId="null" pageNumber="199">L.</authorityName>
</taxonomicName>
</paragraph>
