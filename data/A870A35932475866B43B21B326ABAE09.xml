<taxonomicName id="D5AC7D842F4BF888DD2FBAC98DF6A570" authorityName="Fujii &amp; Reimer" authorityYear="2013" class="Anthozoa" family="Nanozoanthidae" genus="Nanozoanthus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nanozoanthus" order="Zoantharia" pageId="11" pageNumber="12" phylum="Cnidaria" rank="genus">
