<collectingCountry id="F3322AA7FF9BA222FE4AFEB16380FE86" box="[440,504,333,356]" name="Japan" pageId="12" pageNumber="13">Japan</collectingCountry>
in 832– 
<quantity id="4CDDC7D2FF9BA222FDA4FEB060D8FE86" box="[598,672,332,357]" metricMagnitude="2" metricUnit="m" metricValue="7.359999999999999" pageId="12" pageNumber="13" unit="m" value="736.0">736 m</quantity>
