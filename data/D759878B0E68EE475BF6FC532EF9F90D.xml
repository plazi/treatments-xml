<materialsCitation id="EF983CC00E68EE475BF6FB8F2BC1FB69" ID-GBIF-Occurrence="1416805650" collectionCode="ANIC" collectorName="Illawara District, N. S. W." location="Mount Kembla" municipality="Illawara District" pageId="23" pageNumber="324" specimenCode="ANIC 29-38491, ANIC 29-38492" specimenCount="3" typeStatus="holotype">
