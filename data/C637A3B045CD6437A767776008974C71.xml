<taxonomicName id="A608A3A679FC56DFAF1AE826B90241EA" authority="Sharkey, 2017" authorityName="Sharkey" authorityYear="2017" class="Insecta" family="Braconidae" genus="Chimaeragathis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chimaeragathis eurysoma" order="Hymenoptera" pageId="8" pageNumber="115" phylum="Arthropoda" rank="species" species="eurysoma">Chimaeragathis eurysoma</taxonomicName>
