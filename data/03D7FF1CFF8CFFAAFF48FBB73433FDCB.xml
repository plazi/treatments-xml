<taxonomicName id="4C7E3589FF8CFFAAFC0DFA2834DCFEAE" authority="fulgidus Redtenbacher, 1868" authorityName="Redtenbacher" authorityYear="1868" box="[901,1440,326,350]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="70" phylum="Arthropoda" rank="subSpecies" species="fulgidus" subGenus="Goniocanthon" subSpecies="fulgidus">
