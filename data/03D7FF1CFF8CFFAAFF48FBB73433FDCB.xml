<collectingCountry id="F3690E9AFF8CFFAAFEF2FA0830BAFE8E" box="[378,454,358,382]" name="Brazil" pageId="71" pageNumber="70">Brazil</collectingCountry>
) and 
<taxonomicName id="4C7E3589FF8CFFAAFD82FA08355BFE8E" authority="Nunes et al., 2018" authorityName="Nunes" authorityYear="2018" box="[522,1063,358,382]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="70" phylum="Arthropoda" rank="subSpecies" species="fulgidus" subGenus="Goniocanthon" subSpecies="martinezi">
