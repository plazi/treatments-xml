<taxonomicName id="4C7E3589FF8CFFAAFC8DFA4835E1FECE" authorityName="Redtenbacher" authorityYear="1868" box="[773,1181,294,318]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="70" phylum="Arthropoda" rank="species" species="fulgidus" subGenus="Goniocanthon">
