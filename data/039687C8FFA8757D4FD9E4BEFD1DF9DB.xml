<subSubSection id="C3256555FFA8757D4FD9E4DAFE02FEEB" pageId="10" pageNumber="196" type="etymology">
<paragraph id="8B8036DEFFA8757D4FD9E4DAFE02FEEB" blockId="10.[151,1437,252,1546]" pageId="10" pageNumber="196">
<emphasis id="B94BEACCFFA8757D4FD9E4DAFEA1FEC4" bold="true" box="[151,289,252,277]" pageId="10" pageNumber="196">Etymology:</emphasis>
