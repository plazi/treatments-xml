<taxonomicName id="4C3F4D5DFFA8757D4C66E1A7FBA2FA48" authority="Ruan &amp; Konstantinov &amp; Prathapan &amp; Zhang &amp; Jiang &amp; Yang, 2018" authorityName="Ruan &amp; Konstantinov &amp; Prathapan &amp; Zhang &amp; Jiang &amp; Yang" authorityYear="2018" box="[808,1058,1409,1433]" class="Insecta" family="Chrysomelidae" genus="Meishania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="196" phylum="Arthropoda" rank="species" species="flavipennis" status="sp. nov.">
