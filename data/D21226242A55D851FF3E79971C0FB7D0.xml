<caption id="0EC4C7BA2A54D852FF3E7D611FEBB286" httpUri="https://zenodo.org/record/1302731/files/figure.png" pageId="21" pageNumber="22" startId="21.[151,209,1134,1156]" targetBox="[151,1436,195,1112]" targetPageId="21">
