<taxonomicName id="459E7A87FFF4FFFEFEC8FD5633755FCC" authority="Bukit Sarang, Bintulu Divison" authorityName="Bukit Sarang, Bintulu Divison" box="[294,777,735,758]" class="Insecta" family="Protoneuridae" genus="Elattoneura" kingdom="Animalia" order="Odonata" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="longispina">
<emphasis id="B0EADD16FFF4FFFEFEC8FD5631C95FCC" box="[294,437,735,758]" italics="true" pageId="1" pageNumber="66">E. longispina</emphasis>
