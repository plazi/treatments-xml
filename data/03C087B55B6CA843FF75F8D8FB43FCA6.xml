<emphasis id="B91DEAB15B6CA840FF75F8D8FF66F83C" bold="true" box="[151,243,1909,1935]" pageId="9" pageNumber="510">
4. 
<taxonomicName id="4C694D205B6CA840FF56F8D8FF66F83C" authorityName="Rothschild" authorityYear="1898" box="[180,243,1909,1935]" class="Aves" family="Monarchidae" genus="Ifrita" kingdom="Animalia" order="Passeriformes" pageId="9" pageNumber="510" phylum="Chordata" rank="genus">
