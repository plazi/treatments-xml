<collectingCountry id="F3626D898255FFB8FFF8B205FF7F0869" box="[92,196,1120,1144]" name="Australia" pageId="18" pageNumber="1701">Australia</collectingCountry>
. 
<location id="8EAA7BC28255FFB8FF70B205FEB00869" LSID="urn:lsid:plazi:treatment:03DC9C0F8256FFB8FE0BB4EFFD090A87:8EAA7BC28255FFB8FF70B205FEB00869" box="[212,267,1120,1144]" country="Australia" name="Only" pageId="18" pageNumber="1701">Only</location>
<specimenCount id="9D73E6908255FFB8FEB0B204FE170869" box="[276,428,1120,1144]" pageId="18" pageNumber="1701" type="generic">one specimen</specimenCount>
with name 
<taxonomicName id="4C75569A8255FFB8FD90B205FCAB0869" authorityName="Pascoe. Since" authorityYear="1863" box="[564,784,1120,1144]" class="Insecta" family="Cerambycidae" genus="Phoracantha" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="1701" phylum="Arthropoda" rank="species" species="scitula">
<emphasis id="B901F10B8255FFB8FD90B205FCAB0869" box="[564,784,1120,1144]" italics="true" pageId="18" pageNumber="1701">Phoracantha scitula</emphasis>
