<subSubSection id="A8B2ACB3FD39FFA7B88CFEA7FA46FEDE" box="[198,1409,338,364]" pageId="5" pageNumber="6" type="etymology">
<paragraph id="E017FF38FD39FFA7B88CFEA7FA46FEDE" blockId="5.[151,1436,152,364]" box="[198,1409,338,364]" pageId="5" pageNumber="6">
<emphasis id="D2DC232AFD39FFA7B88CFEA7FE94FEDE" bold="true" box="[198,339,338,364]" pageId="5" pageNumber="6">Etymology.</emphasis>
