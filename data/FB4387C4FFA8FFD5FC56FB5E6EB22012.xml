<quantity id="B4129B37FFAAFFD5FE4FFD756E2527C3" box="[426,559,709,733]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" metricValueMax="3.5" metricValueMin="3.1" pageId="10" pageNumber="256" unit="mm" value="3.3" valueMax="3.5" valueMin="3.1">3.1–3.5 mm</quantity>
; length of thorax, 
<quantity id="B4129B37FFAAFFD5FF9FFD526CF727E5" box="[122,253,738,762]" metricMagnitude="-3" metricUnit="m" metricValue="1.65" metricValueMax="1.7" metricValueMin="1.6" pageId="10" pageNumber="256" unit="mm" value="1.65" valueMax="1.7" valueMin="1.6">1.6–1.7 mm</quantity>
; length of wing, 4.0– 
<quantity id="B4129B37FFAAFFD5FE05FD526E3E27E5" box="[480,564,738,762]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="10" pageNumber="256" unit="mm" value="4.3">4.3 mm</quantity>
.
</paragraph>
