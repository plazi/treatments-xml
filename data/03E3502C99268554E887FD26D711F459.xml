<taxonomicName id="4C4A9AB999268554EA9BFD26D610F29A" authorityName="Latreille" authorityYear="1809" box="[651,833,729,756]" class="Insecta" family="Dolichopodidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="subFamily" subFamily="Dolichopodinae">Dolichopodinae</taxonomicName>
