<emphasis id="5A6D8A11DC9C6D82D3D4E7C292C9D721" italics="true" pageId="0" pageNumber="1">Coptodryas mus</emphasis>
</taxonomicName>
, 
<taxonomicName id="E8A2511458C3B3E93B79AA6CB518BA02" authority="Cognato, Smith &amp; Beaver, 2020" authorityName="Cognato, Smith &amp; Beaver" authorityYear="2020" baseAuthorityName="Schedl" baseAuthorityYear="1958" class="Insecta" family="Curculionidae" genus="Fraudatrix" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Fraudatrix cuneiformis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="cuneiformis">
<emphasis id="1C0B021812D8C9AED52DE3CEFAEC832C" italics="true" pageId="0" pageNumber="1">Fraudatrix cuneiformis</emphasis>
</taxonomicName>
