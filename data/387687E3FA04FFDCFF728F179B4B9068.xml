<date id="C4611035FA04FFDFFBFC8D109BD196BD" box="[1049,1177,1901,1927]" pageId="1" pageNumber="45" value="2000-06-19">
<number id="061724D2FA04FFDFFBFC8D109B7E96BD" box="[1049,1078,1901,1927]" pageId="1" pageNumber="45" value="19.0">19</number>
