<collectingRegion id="49F3F81FFF9503477A69E829324EF8AE" box="[264,361,1865,1891]" country="Tanzania" name="Kigoma" pageId="50">Kigoma</collectingRegion>
, 
<collectingCountry id="F320766DFF9503477A14E82932C5F8AE" box="[373,482,1865,1891]" name="Tanzania" pageId="50">Tanzania</collectingCountry>
