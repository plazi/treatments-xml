<paragraph pageId="null" pageNumber="712">
<taxonomicName authority="Jan" class="Insecta" family="Staphylinidae" genus="Thesium" kingdom="Animalia" order="Coleoptera" pageId="null" pageNumber="712" phylum="Arthropoda" rank="species" species="divaricatum">
