<paragraph blockId="17.[151,1436,1925,2023]" box="[199,644,1925,1950]" pageId="17" pageNumber="302">
<emphasis bold="true" box="[199,352,1925,1950]" pageId="17" pageNumber="302">Distribution.</emphasis>
<collectingCountry box="[359,427,1927,1950]" name="South Korea" pageId="17" pageNumber="302">Korea</collectingCountry>
(South) and 
<collectingCountry box="[573,640,1927,1950]" name="Japan" pageId="17" pageNumber="302">Japan</collectingCountry>
