7 (6 – 8), J 
<subScript attach="left" box="[716,725,699,716]" fontSize="7" pageId="5" pageNumber="55">5</subScript>
11 (10 – 11), z 
<subScript attach="left" box="[285,294,735,752]" fontSize="7" pageId="5" pageNumber="55">2</subScript>
35 (32 – 40), z 
<subScript attach="left" box="[464,473,735,752]" fontSize="7" pageId="5" pageNumber="55">4</subScript>
27 (21 – 33), z 
<subScript attach="left" box="[643,652,734,751]" fontSize="7" pageId="5" pageNumber="55">5</subScript>
6 (5 – 6), Z 
<subScript attach="left" box="[176,185,770,787]" fontSize="7" pageId="5" pageNumber="55">1</subScript>
