<taxonomicName authority="Evenhuis et al. (2015: 64)" authorityName="Evenhuis" authorityPageNumber="64" authorityYear="2015" box="[1052,1407,908,933]" class="Insecta" family="Anthomyiidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="teste">
