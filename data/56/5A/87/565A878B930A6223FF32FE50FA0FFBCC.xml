<taxonomicName authority="Rondani, 1848" authorityName="Rondani" authorityYear="1848" box="[444,787,1052,1077]" class="Insecta" family="Syrphidae" genus="Phalacromya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
