<emphasis box="[345,592,1556,1580]" italics="true" pageId="4">Oryssomus guillermo</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[606,702,1556,1581]" pageId="4" rank="species">
<emphasis bold="true" box="[606,702,1556,1581]" pageId="4">sp. nov.</emphasis>
</taxonomicNameLabel>
resembles 
<taxonomicName box="[841,937,1556,1580]" class="Insecta" family="Coccinellidae" genus="Oryssomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" phylum="Arthropoda" rank="species" species="calix" status="sp. nov.">
<emphasis box="[841,937,1556,1580]" italics="true" pageId="4">O. calix</emphasis>
