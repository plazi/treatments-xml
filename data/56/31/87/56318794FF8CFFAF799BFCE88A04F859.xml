<taxonomicNameLabel box="[1245,1341,1556,1581]" pageId="4" rank="species">
<emphasis bold="true" box="[1245,1341,1556,1581]" pageId="4">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName class="Insecta" family="Coccinellidae" genus="Oryssomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" phylum="Arthropoda" rank="species" species="unimaculatus">
