<collectorName box="[442,661,1562,1588]" pageId="3" pageNumber="107">Perrier de la Bâthie</collectorName>
in 
<collectingRegion box="[698,850,1562,1588]" country="Madagascar" name="Antananarivo" pageId="3" pageNumber="107">Antananarivo</collectingRegion>
