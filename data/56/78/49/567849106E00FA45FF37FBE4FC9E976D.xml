<paragraph blockId="6.[151,1437,1397,2034]" box="[151,1130,1397,1422]" pageId="6" pageNumber="561">
<emphasis bold="true" box="[151,216,1397,1422]" pageId="6" pageNumber="561">Note:</emphasis>
