<taxonomicName authorityName="Shi, Pan and Zhang" authorityYear="2010" box="[181,334,331,356]" class="Collembola" family="Entomobryidae" genus="Homidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="11" pageNumber="190" phylum="Arthropoda" rank="species" species="apigmenta">
<emphasis box="[181,334,331,356]" italics="true" pageId="11" pageNumber="190">H. apigmenta</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Pan and Shi" authorityYear="2015" box="[390,605,330,356]" class="Collembola" family="Entomobryidae" genus="Homidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="11" pageNumber="190" phylum="Arthropoda" rank="species" species="triangulimacula">
<emphasis box="[390,605,330,356]" italics="true" pageId="11" pageNumber="190">H. triangulimacula</emphasis>
</taxonomicName>
