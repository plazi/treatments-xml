<taxonomicName authority="Szeptycki, 1973" authorityName="Szeptycki" authorityYear="1973" box="[765,1148,366,392]" class="Collembola" family="Entomobryidae" genus="Homidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="11" pageNumber="190" phylum="Arthropoda" rank="species" species="phjongjangica">
<emphasis box="[765,961,366,392]" italics="true" pageId="11" pageNumber="190">H. phjongjangica</emphasis>
