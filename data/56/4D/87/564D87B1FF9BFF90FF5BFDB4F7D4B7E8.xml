<subSubSection pageId="6" pageNumber="643" type="reference_group">
<paragraph blockId="6.[160,1047,282,772]" box="[250,947,282,304]" pageId="6" pageNumber="643">
<taxonomicName authority="H.R, 2007" authorityName="H.R" authorityYear="2007" box="[250,398,282,304]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="643" phylum="Arthropoda" rank="species" species="pastellensis" status="nov.sp.">
<emphasis bold="true" box="[250,398,282,304]" italics="true" pageId="6" pageNumber="643">A. pastellensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[405,479,283,304]" pageId="6" pageNumber="643" rank="species">nov.sp.</taxonomicNameLabel>
<taxonomicName authority="GIRAUD 1861" authorityName="GIRAUD, Genital" authorityYear="1861" box="[688,947,282,304]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="643" phylum="Arthropoda" rank="species" species="taraxaci">
