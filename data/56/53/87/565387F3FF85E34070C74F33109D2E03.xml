<quantity box="[789,856,1524,1550]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" pageId="19" pageNumber="62" unit="mm" value="7.0">7 mm</quantity>
male from Alb. Sta. 
<number box="[1101,1160,1524,1550]" pageId="19" pageNumber="62" value="4803.0">4803</number>
(
<geoCoordinate box="[1175,1287,1524,1550]" direction="north" orientation="latitude" pageId="19" pageNumber="62" precision="925" value="46.7">
<number box="[1175,1208,1524,1550]" pageId="19" pageNumber="62" value="46.0">46</number>
° 
<number box="[1218,1251,1524,1550]" pageId="19" pageNumber="62" value="42.0">42</number>
’ N
</geoCoordinate>
, 
<geoCoordinate box="[1299,1422,1524,1550]" direction="east" orientation="longitude" pageId="19" pageNumber="62" precision="925" value="151.75">
<number box="[1299,1345,1524,1550]" pageId="19" pageNumber="62" value="151.0">151</number>
° 
