<caption httpUri="https://zenodo.org/record/191840/files/figure.png" pageId="18" pageNumber="61" targetBox="[213,1373,194,1932]" targetPageId="18">
<paragraph blockId="18.[151,1435,1956,2014]" pageId="18" pageNumber="61">
<emphasis bold="true" box="[151,294,1956,1980]" pageId="18" pageNumber="61">FIGURE 14.</emphasis>
<taxonomicName authority="Shoemaker, 1955" authorityName="Shoemaker" authorityYear="1955" box="[301,687,1957,1980]" class="Malacostraca" family="Stenothoidae" genus="Metopa" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="61" phylum="Arthropoda" rank="species" species="spinicoxa">
