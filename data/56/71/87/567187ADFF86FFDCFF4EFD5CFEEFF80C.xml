<number box="[650,702,1682,1706]" pageId="12" pageNumber="56" value="0.46">0.46</number>
, 
<collectionCode box="[713,794,1682,1706]" pageId="12" pageNumber="56">MOQP</collectionCode>
<number box="[801,853,1682,1706]" pageId="12" pageNumber="56" value="0.6">0.60</number>
.
</paragraph>
<paragraph blockId="12.[151,1436,890,2030]" pageId="12" pageNumber="56">
<emphasis box="[199,252,1720,1743]" italics="true" pageId="12" pageNumber="56">Legs</emphasis>
