<number box="[442,494,1142,1166]" pageId="12" pageNumber="56" value="0.13">0.13</number>
, ALE 
<number box="[570,622,1142,1166]" pageId="12" pageNumber="56" value="0.15">0.15</number>
, PME 
<number box="[701,750,1142,1166]" pageId="12" pageNumber="56" value="0.25">0.25</number>
, PLE 
<number box="[825,878,1142,1166]" pageId="12" pageNumber="56" value="0.12">0.12</number>
. AME–AME 
<number box="[1037,1089,1142,1166]" pageId="12" pageNumber="56" value="0.13">0.13</number>
, AME–ALE 
<number box="[1242,1294,1142,1166]" pageId="12" pageNumber="56" value="0.07">0.07</number>
, PME–PME 
<number box="[151,203,1178,1202]" pageId="12" pageNumber="56" value="0.3">0.30</number>
, PME–PLE 
<number box="[342,394,1178,1202]" pageId="12" pageNumber="56" value="0.21">0.21</number>
, 
<collectionCode box="[405,487,1178,1202]" pageId="12" pageNumber="56">MOQL</collectionCode>
<number box="[494,546,1178,1202]" pageId="12" pageNumber="56" value="0.38">0.38</number>
, 
<collectionCode box="[557,638,1178,1202]" pageId="12" pageNumber="56">MOQP</collectionCode>
<number box="[645,697,1178,1202]" pageId="12" pageNumber="56" value="0.53">0.53</number>
, 
<collectionCode box="[708,794,1178,1202]" pageId="12" pageNumber="56">MOQA</collectionCode>
<number box="[801,853,1178,1202]" pageId="12" pageNumber="56" value="0.38">0.38</number>
