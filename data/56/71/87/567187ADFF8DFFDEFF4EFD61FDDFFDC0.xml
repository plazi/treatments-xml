<paragraph blockId="12.[151,1437,151,572]" pageId="12" pageNumber="56">
<emphasis bold="true" box="[199,304,476,500]" pageId="12" pageNumber="56">Remark.</emphasis>
<taxonomicName box="[310,460,476,500]" class="Arachnida" family="Clubionidae" genus="Clubiona" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="56" phylum="Arthropoda" rank="species" species="linzhiensis">
