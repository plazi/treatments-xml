<paragraph blockId="12.[151,1437,151,572]" pageId="12" pageNumber="56">
<emphasis bold="true" box="[199,395,331,356]" pageId="12" pageNumber="56">Natural History.</emphasis>
<taxonomicName box="[401,552,332,356]" class="Arachnida" family="Clubionidae" genus="Clubiona" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="56" phylum="Arthropoda" rank="species" species="linzhiensis">
<emphasis box="[401,552,332,356]" italics="true" pageId="12" pageNumber="56">C. linzhiensis</emphasis>
</taxonomicName>
