<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:231955" authority="Emery, 1901" authorityName="Emery" authorityYear="1901" class="Hexapoda" family="Formicidae" genus="Neoponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Uploaded - Neoponera flavicornis" lsidName-HNS="Neoponera flavicornis" order="Hymenoptera" pageNumber="161" phylum="Arthropoda" rank="species" species="flavicornis">flavicornis Fab</taxonomicName>
