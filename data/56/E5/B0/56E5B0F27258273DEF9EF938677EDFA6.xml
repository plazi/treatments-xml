<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142090" authority="Emery, 1901" authorityName="Emery" authorityYear="1901" class="Hexapoda" family="Formicidae" genus="Neoponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neoponera latreillei Forel" lsidName-HNS="Neoponera latreillei Forel" order="Hymenoptera" pageNumber="161" phylum="Arthropoda" rank="species" species="latreillei">Latreillei n. nom</taxonomicName>
