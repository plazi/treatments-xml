<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147137" authority="Emery, 1901" authorityName="Emery" authorityYear="1901" class="Hexapoda" family="Formicidae" genus="Neoponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neoponera Emery" lsidName-HNS="Neoponera Emery" order="Hymenoptera" pageNumber="161" phylum="Arthropoda" rank="genus">Neoponera</taxonomicName>
