<taxonomicName authorityName="Herbst" authorityYear="1806" class="Hexapoda" family="Carabidae" genus="Cicindela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="244" pageNumber="245" phylum="Arthropoda" rank="species" species="tranquebarica">
