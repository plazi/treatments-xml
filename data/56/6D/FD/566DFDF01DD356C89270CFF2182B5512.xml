<taxonomicName authorityName="Casey" authorityYear="1913" class="Hexapoda" family="Carabidae" genus="Cicindela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="245" pageNumber="246" phylum="Arthropoda" rank="species" species="crinifrons">Cicindela crinifrons</taxonomicName>
