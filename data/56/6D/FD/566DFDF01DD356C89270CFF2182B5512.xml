<taxonomicName authorityName="Herbst" authorityYear="1806" class="Hexapoda" family="Carabidae" genus="Cicindela" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="245" pageNumber="246" phylum="Arthropoda" rank="species" species="tranquebarica">Cicindela tranquebarica</taxonomicName>
