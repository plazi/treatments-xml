</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8BF7369F193BAD423AEBFEC1EEA0FE50" blockId="27.[151,1437,151,436]" pageId="27" pageNumber="328">
<keyLead id="30B995AA193BAD423AEBFEC1EEA0FE50" pageId="27" pageNumber="328">
<number id="3D8024B8193BAD423AEBFEC1EB9EFEB9" box="[151,163,327,349]" pageId="27" pageNumber="328" value="8.0">8</number>
