<taxonomicName id="4C094D634222FFB68E63F820FDDBA8B9" box="[363,582,1950,1973]" class="Insecta" family="Chironomidae" genus="Parapsectra" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="mendli">
