<taxonomicName id="4C234D202170BE3AFAC4FA3B1944B947" authorityName="Walker" authorityYear="1871" box="[470,601,1533,1557]" class="Insecta" family="Acrididae" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="615" phylum="Arthropoda" rank="subFamily" subFamily="Oedipodinae">Oedipodinae</taxonomicName>
