<typeStatus id="CF1FCDDBE325FFCBFCDFFB539661FB11" box="[882,1037,1186,1210]" pageId="14" pageNumber="13">HOLOTYPE</typeStatus>
[male symbol] 
<taxonomicName id="D7A408FAE325FFCBFB65FB539073FB72" authority="Larochelle &amp; Lariviere, 2015" authorityName="Larochelle &amp; Lariviere" authorityYear="2015" class="Insecta" family="Carabidae" genus="Bembidion" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="13" phylum="Arthropoda" rank="species" species="tepaki">
