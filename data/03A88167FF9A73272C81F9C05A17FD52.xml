<taxonomicName id="4C014BF2FF9E73272CA3FEA65913FE5E" baseAuthorityName="Kirti &amp; Singh" baseAuthorityYear="2016" box="[181,338,370,394]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="450" phylum="Arthropoda" rank="species" species="cacharensis">
<emphasis id="B975EC63FF9E73272CA3FEA65913FE5E" box="[181,338,370,394]" italics="true" pageId="5" pageNumber="450">B. cacharensis</emphasis>
</taxonomicName>
