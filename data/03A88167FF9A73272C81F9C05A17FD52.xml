<taxonomicName id="4C014BF2FF9E7327281EFDD65C26FDCD" authorityName="Volynkin &amp; Huang &amp; Dubatolov &amp; Kishida" authorityYear="2019" box="[1032,1127,514,537]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="450" phylum="Arthropoda" rank="species" species="wangi">
