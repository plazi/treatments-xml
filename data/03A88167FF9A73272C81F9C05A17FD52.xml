<taxonomicName id="4C014BF2FF9E73272E24FEA65AD6FE5D" baseAuthorityName="Bremer &amp; Grey" baseAuthorityYear="1852" box="[562,663,370,393]" class="Insecta" family="Erebidae" genus="Barsine" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="450" phylum="Arthropoda" rank="species" species="striata">
<emphasis id="B975EC63FF9E73272E24FEA65AD6FE5D" box="[562,663,370,393]" italics="true" pageId="5" pageNumber="450">B. striata</emphasis>
</taxonomicName>
by its smaller 1 
<superScript id="7C749D39FF9E73272F51FEBB5B10FEA9" attach="left" box="[839,849,367,381]" fontSize="6" pageId="5" pageNumber="450">st</superScript>
medial diverticulum, bilobate 2 
<superScript id="7C749D39FF9E732728BAFEBA5CFDFEA9" attach="left" box="[1196,1212,366,381]" fontSize="6" pageId="5" pageNumber="450">nd</superScript>
