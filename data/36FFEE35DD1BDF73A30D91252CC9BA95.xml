<taxonomicName id="D2761B628296B8F420F03AD216F2F523" authority="McDunnough, 1937" authorityName="McDunnough" authorityYear="1937" class="Insecta" family="Noctuidae" genus="Aseptis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aseptis" order="Lepidoptera" pageId="6" pageNumber="63" phylum="Arthropoda" rank="genus">Aseptis McDunnough, 1937</taxonomicName>
