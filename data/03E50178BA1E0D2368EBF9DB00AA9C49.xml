<emphasis id="B9386C7CBA1E0D2668EBF9DB04A39A1C" bold="true" box="[151,385,1015,1041]" italics="true" pageId="3" pageNumber="4">Stenopsyche dakpri</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20BD107BA1E0D2669F4F9DB04C49A1C" box="[392,486,1015,1041]" pageId="3" pageNumber="4" rank="species">sp. nov.</taxonomicNameLabel>
(
<figureCitation id="1377ACEBBA1E0D266989F9DB076B9A1C" box="[501,585,1015,1041]" captionStart="FIGURE 2" captionStartId="4.[151,255,1627,1651]" captionTargetBox="[182,1408,195,1605]" captionTargetId="figure@4.[172,1414,179,1620]" captionTargetPageId="4" captionText="FIGURE 2. Stenopsyche dakpri, sp. nov. a – d: wings. a: male forewing; b: male hind wing; c: female forewing; d: female hind wing. e – g: male genitalia. e: dorsal; f: lateral; g: ventral. h – j: female genitalia. h: dorsal; i: lateral; j: ventral." httpUri="https://zenodo.org/record/179265/files/figure.png" pageId="3" pageNumber="4">
Figs. 
<number id="3D84A249BA1E0D266A46F9DB076B9A1C" box="[570,585,1015,1041]" pageId="3" pageNumber="4" value="2.0">2</number>
</figureCitation>
a– 
<number id="3D84A249BA1E0D266A1BF9DB07549A1C" box="[615,630,1015,1041]" pageId="3" pageNumber="4" value="2.0">2</number>
j, 
<number id="3D84A249BA1E0D266AF0F9DB07B99A1C" box="[652,667,1015,1041]" pageId="3" pageNumber="4" value="3.0">3</number>
a– 
<quantity id="4CB41D8BBA1E0D266AC6F9DB07F59A1C" box="[698,727,1015,1041]" metricMagnitude="-3" metricUnit="kg" metricValue="3.0" pageId="3" pageNumber="4" unit="g" value="3.0">3g</quantity>
)
