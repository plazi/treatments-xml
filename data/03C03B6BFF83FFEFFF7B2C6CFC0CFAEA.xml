<taxonomicName id="4C69F1FEFF83FFEEFF2B2CC8FD5FF825" authority="Hoshina" authorityName="Hoshina" authorityYear="2013" box="[199,697,1950,1974]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="kumejimensis" subGenus="Eupentarius">
