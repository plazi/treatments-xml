<taxonomicName id="4C69F1FEFF83FFEEFB402CC9FAD3F825" box="[1196,1333,1950,1974]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="33" phylum="Arthropoda" rank="subGenus" subGenus="Euconophron">
