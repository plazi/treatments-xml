<taxonomicName id="6CA9AE459B783CD2315241E4E8B693DB" class="Insecta" family="Carabidae" genus="Apotomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apotomus clypeonitens" order="Coleoptera" pageId="0" pageNumber="8135" phylum="Arthropoda" rank="subSpecies" species="clypeonitens" subSpecies="adanensis">Apotomus clypeonitens adanensis</taxonomicName>
