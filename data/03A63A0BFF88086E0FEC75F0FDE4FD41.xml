<quantity id="4CF726F8FF8808610B5575B3FF11FA9A" metricMagnitude="3" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="7" pageNumber="1160" unit="m" value="1400.0" valueMax="1500.0" valueMin="1300.0">1300- 1500 m</quantity>
