<quantity id="4CEF9B25FF03FF279AECFB59F88834C0" box="[657,713,1195,1221]" metricMagnitude="1" metricUnit="m" metricValue="7.0" pageId="146" pageNumber="147" unit="m" value="70.0">70 m</quantity>
