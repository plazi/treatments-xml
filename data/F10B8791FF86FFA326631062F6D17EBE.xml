<date id="0D1C1047FF86FFA3202A10F3F68F79D1" box="[1026,1215,704,729]" pageId="17" pageNumber="2739" value="1999-10-28">28 October 1999</date>
, 
<specimenCount id="6FA4FD0EFF86FFA320E310F3F3A479F0" pageId="17" pageNumber="2739" type="juvenile">one juvenile</specimenCount>
; 
<quantity id="BE5A9B62FF86FFA3258810D2F3FB79F0" box="[416,459,736,760]" metricMagnitude="0" metricUnit="m" metricValue="6.0" pageId="17" pageNumber="2739" unit="m" value="6.0">6 m</quantity>
, 
<date id="0D1C1047FF86FFA325F010D2F04879F0" box="[472,632,736,760]" pageId="17" pageNumber="2739" value="2000-04-19">19 April 2000</date>
