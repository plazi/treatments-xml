<emphasis id="4BD6EA95FF86FFA3268810D3F08279F0" bold="true" box="[672,690,737,760]" italics="true" pageId="17" pageNumber="2739">W</emphasis>
, 24 
<emphasis id="4BD6EA95FF86FFA326F410D3F0DB79F0" bold="true" box="[732,747,737,760]" italics="true" pageId="17" pageNumber="2739">X</emphasis>
, 
<specimenCount id="6FA4FD0EFF86FFA326D310D2F1B479F0" box="[763,900,736,760]" pageId="17" pageNumber="2739" type="juvenile">11 juveniles</specimenCount>
