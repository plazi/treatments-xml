<quantity id="BE5A9B62FF86FFA3272910F2F11B79D0" box="[769,811,704,729]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="17" pageNumber="2739" unit="m" value="5.0">5 m</quantity>
