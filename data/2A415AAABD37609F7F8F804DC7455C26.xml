<paragraph id="FF4491B61F90F3BB9541DBD39A3E506E" pageId="30" pageNumber="31">Type species.</paragraph>
<paragraph id="9B9658EBCA2B06E7FE92D270E4DC92F9" pageId="30" pageNumber="31">
<taxonomicName id="9BC2D31B61D707C017BF11FCCF35C86A" class="Insecta" family="Curculionidae" genus="Laparocerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Laparocerus grossepunctatus" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="grossepunctatus">Laparocerus grossepunctatus</taxonomicName>
