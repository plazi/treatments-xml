<taxonomicName id="2B8862CCFFC74F2BE3830D46B260FC98" authority="d'Orbigny, 1850 b: 323" authorityName="d'Orbigny" authorityPageNumber="323" authorityYear="1850" box="[677,1133,843,867]" class="Bivalvia" family="Crassatellidae" genus="Crassatella" kingdom="Animalia" order="Carditida" pageId="40" pageNumber="57" phylum="Mollusca" rank="species" species="michelottii">
