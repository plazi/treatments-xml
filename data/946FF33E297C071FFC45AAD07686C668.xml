<taxonomicName id="DBC639AB2963071FFEB3A8A17667C328" authorityName="Stal" authorityYear="1872" box="[328,572,253,277]" class="Insecta" family="Pentatomidae" genus="Cosmopepla" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="291" phylum="Arthropoda" rank="species" species="cruciaria">
<emphasis id="2EB29E3A2963071FFEB3A8A17667C328" bold="true" box="[328,572,253,277]" italics="true" pageId="16" pageNumber="291">Cosmopepla cruciaria</emphasis>
