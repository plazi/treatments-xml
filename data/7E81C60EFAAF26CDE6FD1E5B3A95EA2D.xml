<taxonomicName id="BC93AE16FD39D0BA90609743A653452C" authorityName="Morrone" authorityYear="1992" class="Insecta" family="Curculionidae" genus="Lamiarhinus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="34" pageNumber="49" phylum="Arthropoda" rank="genus">
