<taxonomicName id="97BC4D35FFFFFF240A9EFE97F05DFEEC" authorityName="DISNEY &amp; WINQVIST" authorityYear="2018" box="[413,543,287,311]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="14" phylum="Arthropoda" rank="species" species="confusus">
