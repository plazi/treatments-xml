<taxonomicName id="4C784D597A40FFC6FD7FFD44FCA1FD0F" authorityName="Otto &amp; Hill" authorityYear="2012" box="[724,867,647,677]" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="calcitrans">
