<taxonomicName id="319A4D32FF92F206032F8AFB72ADEE16" box="[589,709,1771,1792]" class="Insecta" family="Anthocoridae" genus="Bilia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="548" phylum="Arthropoda" rank="species" species="castanea">
