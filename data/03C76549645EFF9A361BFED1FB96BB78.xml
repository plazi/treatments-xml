Neotropical Region: 
<collectingCountry id="F37994CF645EFF9A3483FB94FC62B860" box="[912,1011,1113,1139]" name="Grenada" pageId="6" pageNumber="7">Grenada</collectingCountry>
(
<bibRefCitation id="EFFFA9AE645EFF9A3315FB94FB50B860" author="Ashmead" box="[1030,1217,1113,1139]" pageId="6" pageNumber="21" refString="Ashmead, W. H. (1896) Report on the parasitic Hymenoptera of the Island of Grenada, comprising the families Cynipidae, Ichneumonidae, Braconidae and Proctotrupidae. Proceedings of the Zoological Society of London, 1895, 742 - 812." type="proceedings paper" year="1896">
Ashmead 
<number id="3DA6C678645EFF9A3391FB94FB50B860" box="[1154,1217,1113,1139]" pageId="6" pageNumber="7" value="1896.0">1896</number>
</bibRefCitation>
); 
<collectingCountry id="F37994CF645EFF9A33CAFB94FEA5B888" name="Argentina" pageId="6" pageNumber="7">Argentina</collectingCountry>
(
<bibRefCitation id="EFFFA9AE645EFF9A3650FB4CFE2EB888" author="Diaz" box="[323,447,1153,1179]" pageId="6" pageNumber="21" refString="Diaz, N. (1974) Anotaciones sobre cinipoideos Argentinos. I. (Hymenoptera). Neotropica, 20 (61), 17 - 20." type="journal article" year="1974">
Diaz 
<number id="3DA6C678645EFF9A3690FB4CFE2EB888" box="[387,447,1153,1179]" pageId="6" pageNumber="7" value="1974.0">1974</number>
