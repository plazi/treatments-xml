<taxonomicName id="4E519C94DB6332B18337FD95F7BB0574" LSID="http://zoobank.org/7227C05D-E910-44EE-AC65-49CACF572032" authority="Chakrabarti &amp; Pandit" authorityName="Chakrabarti &amp; Pandit" authorityYear="2019" class="Arachnida" family="Eriophyidae" genus="Mesoshieldophyes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Mesoshieldophyes" order="Trombidiformes" pageId="5" pageNumber="44" phylum="Arthropoda" rank="genus">
<pageBreakToken id="5CCC715119C8B63B9F09D9B67E6F0740" pageId="5" pageNumber="44" start="start">Mesoshieldophyes</pageBreakToken>
Chakrabarti &amp; Pandit
</taxonomicName>
<taxonomicNameLabel id="F64950413AF018B0CDF012CA6DEE9A77" pageId="5" pageNumber="44">gen. n.</taxonomicNameLabel>
