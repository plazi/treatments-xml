(38–39), 
<taxonomicName id="4C074D42FFE7FF81FEE3FCF2FE4DA6C7" box="[264,504,1670,1693]" class="Arachnida" family="Stygnidae" genus="Protimesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="12" pageNumber="231" phylum="Arthropoda" rank="species" species="boibumba" status="n. sp.">
