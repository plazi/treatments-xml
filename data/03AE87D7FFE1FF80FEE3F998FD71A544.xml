<taxonomicName id="4C074D42FFE7FF81FBEFFCF2FB5DA6C7" box="[1028,1256,1670,1693]" class="Arachnida" family="Stygnidae" genus="Protimesius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="12" pageNumber="231" phylum="Arthropoda" rank="species" species="carnaval" status="n. sp.">
