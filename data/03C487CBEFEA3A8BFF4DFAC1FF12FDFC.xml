, 
<geoCoordinate id="EE59501AEFEA3A8EFAEAFB49FA5BFB60" box="[1327,1438,1159,1185]" degrees="121" direction="east" minutes="43" orientation="longitude" pageId="187" pageNumber="188" precision="925" value="121.71667">121°43 É</geoCoordinate>
), MUSORSTOM Stn 131, depth 
