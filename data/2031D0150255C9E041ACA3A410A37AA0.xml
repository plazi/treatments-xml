<taxonomicName id="1A8D11E5E5A56F15491E93A191E3F401" class="Insecta" family="Scarabaeidae" genus="Onthophagus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Onthophagus clypeatus" order="Coleoptera" pageId="254" pageNumber="255" phylum="Arthropoda" rank="species" species="clypeatus">Onthophagus clypeatus</taxonomicName>
