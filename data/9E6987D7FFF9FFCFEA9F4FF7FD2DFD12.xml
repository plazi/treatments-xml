<taxonomicName id="D1C04D42FFFAFFCEE8D14E55FC82F8C9" baseAuthorityName="Hope" baseAuthorityYear="1840" box="[654,779,1833,1859]" class="Insecta" family="Rutelidae" genus="Chrysina" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="adelaida">
