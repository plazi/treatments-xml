<quantity id="E8549B1BFFB7FC370B5E8E6CDBFD01DD" box="[1138,1201,628,654]" metricMagnitude="-1" metricUnit="m" metricValue="1.4" pageId="5" pageNumber="862" unit="cm" value="14.0">14cm</quantity>
