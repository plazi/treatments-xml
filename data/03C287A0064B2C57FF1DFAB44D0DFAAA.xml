<quantity id="4C939B53064B2C52FD5AF9804C54F9B1" box="[715,809,1540,1566]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="3" pageNumber="46" unit="mm" value="2.7">2.7 mm</quantity>
