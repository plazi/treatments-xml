<taxonomicName id="4C3A4D82FFA9FFF1FBCD79B0FF04F9B1" authority="BOSSELAERS &amp; JOCQUÉ, 2000" authorityName="BOSSELAERS &amp; JOCQUÉ" authorityYear="2000" class="Arachnida" family="Corinnidae" genus="Hortipes" kingdom="Animalia" order="Araneae" pageId="91" phylum="Arthropoda" rank="species" species="calliblepharus" status="new species">
