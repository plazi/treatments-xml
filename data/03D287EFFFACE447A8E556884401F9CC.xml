<subSubSection id="C3616572FFACE446A8B5544541ECF986" pageId="12" pageNumber="13" type="etymology">
<paragraph id="8BC436F9FFACE446A8B5544541ECF986" blockId="12.[151,1437,1362,2038]" pageId="12" pageNumber="13">
<emphasis id="B90FEAEBFFACE446A8B55445404DF9A2" bold="true" box="[199,334,1578,1603]" pageId="12" pageNumber="13">Etymology.</emphasis>
