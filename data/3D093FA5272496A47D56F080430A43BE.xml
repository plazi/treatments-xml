<taxonomicName id="24F43407B72032E02766DB8BE0BF569E" class="Agaricomycetes" family="Polyporaceae" genus="Nigrofomes" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Nigrofomes melanoporus" order="Polyporales" pageId="3" pageNumber="4" phylum="Basidiomycota" rank="species" species="melanoporus">Nigrofomes melanoporus</taxonomicName>
