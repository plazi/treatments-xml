<taxonomicName id="4C4AAE9AFF89FFE9D10228AEAB558F9A" authorityName="Hsiao &amp; Li &amp; Ren &amp; Pang" authorityYear="2021" box="[313,576,1411,1433]" class="Insecta" family="Cantharidae" genus="Palaeocantharis" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="1283" phylum="Arthropoda" rank="species" species="panna">
<emphasis id="B93E090BFF89FFE9D10228AEAB558F9A" box="[313,576,1411,1433]" italics="true" pageId="12" pageNumber="1283">Palaeocantharis panna</emphasis>
</taxonomicName>
