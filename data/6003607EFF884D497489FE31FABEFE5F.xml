leg 
<number id="5E62C34FFF884D4C763CFA67FDD2FA42" box="[546,590,1492,1513]" pageId="1" pageNumber="65" value="2.23">2.23</number>
<number id="5E62C34FFF884D4C76E8FA67FCBEFA42" box="[758,802,1492,1513]" pageId="1" pageNumber="65" value="1.21">1.21</number>
