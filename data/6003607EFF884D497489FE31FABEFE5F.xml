<number id="5E62C34FFF884D4C763CFA19FDD2FA6B" box="[546,590,1450,1472]" pageId="1" pageNumber="65" value="2.05">2.05</number>
<number id="5E62C34FFF884D4C76E8FA19FCBEFA6B" box="[758,802,1450,1472]" pageId="1" pageNumber="65" value="2.56">2.56</number>
<number id="5E62C34FFF884D4C77B1FA18FC47FA6B" box="[943,987,1451,1472]" pageId="1" pageNumber="65" value="0.88">0.88</number>
