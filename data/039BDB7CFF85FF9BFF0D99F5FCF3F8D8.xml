); III 
<number id="3DFA784DFF85FF9BFCCB9EAEFC02FA56" box="[849,901,1452,1478]" pageId="9" pageNumber="32" value="0.84">0.84</number>
(
<number id="3DFA784DFF85FF9BFC0C9EAEFC49FA56" box="[918,974,1452,1478]" pageId="9" pageNumber="32" value="0.26">0.26</number>
, 
<number id="3DFA784DFF85FF9BFC419EAEFB95FA56" box="[987,1042,1452,1478]" pageId="9" pageNumber="32" value="0.13">0.13</number>
, 
<number id="3DFA784DFF85FF9BFB859EAEFBD1FA56" box="[1055,1110,1452,1478]" pageId="9" pageNumber="32" value="0.16">0.16</number>
, 
<number id="3DFA784DFF85FF9BFBF89EAEFB1EFA56" box="[1122,1177,1452,1478]" pageId="9" pageNumber="32" value="0.18">0.18</number>
