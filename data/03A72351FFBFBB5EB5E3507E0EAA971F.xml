<taxonomicName id="4C0EE9C4FFBABB5FB42F51CC0F2594C8" box="[347,563,1963,1985]" class="Insecta" family="Tenebrionidae" genus="Atrocrates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="583" phylum="Arthropoda" rank="species" species="matthewsi" status="sp. nov.">
<emphasis id="B97A4E55FFBABB5FB42F51CC0F2594C8" box="[347,563,1963,1985]" italics="true" pageId="12" pageNumber="583">
<taxonomicName id="4C0EE9C4FFBABB5FB42F51CC0CD394C9" box="[347,453,1963,1984]" genus="Atrocrates" pageId="12" pageNumber="583" rank="genus">Atrocrates</taxonomicName>
matthewsi
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A249F32EFFBABB5FB74E51CC0F9D94C9" box="[570,651,1963,1984]" pageId="12" pageNumber="583" rank="species">
