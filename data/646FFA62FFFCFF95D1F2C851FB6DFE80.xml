<quantity id="2B3EE691FFFCFF94D341C8CDFDB5F838" box="[548,639,1989,2015]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="4" pageNumber="281" unit="mm" value="1.1">1.1 mm</quantity>
