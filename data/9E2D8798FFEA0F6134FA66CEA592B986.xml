<taxonomicName id="D1844D0DFFE50F61357A65DCA678BBC9" authorityName="Shattuck &amp; Gunawardene &amp; Heterick" authorityYear="2012" box="[279,514,1448,1469]" class="Insecta" family="Formicidae" genus="Probolomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="48" phylum="Arthropoda" rank="species" species="simplex">
