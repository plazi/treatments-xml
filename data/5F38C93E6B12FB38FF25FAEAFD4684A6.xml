<paragraph id="D72E78286B12FB3BFF25FA0DFDBF809E" blockId="5.[151,639,1431,1484]" box="[151,639,1461,1484]" pageId="5" pageNumber="375">
<taxonomicName id="109103AB6B12FB3BFF25FA0DFDF1809E" authority="Zhang &amp; Tong, 1992: 82" authorityName="Zhang &amp; Tong" authorityPageNumber="82" authorityYear="1992" box="[151,561,1462,1484]" class="Insecta" family="Thripidae" genus="Graminothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="375" phylum="Arthropoda" rank="genus">
