<taxonomicName id="9BD307EBD7EF6F73D12C5723FAAE26D1" class="Insecta" family="Bombycidae" genus="Bombyx" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bombyx amilia" order="Lepidoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="amilia">Bombyx amilia</taxonomicName>
