<taxonomicName id="25CD917AEA8F8B1B70077A4183E7BFC5" authorityName="H. W. Shen, Y. X. Xu, H. Y. Su &amp; Z. L. Luo" authorityYear="2021" class="Dothideomycetes" family="Lamiaceae" genus="Cladosporium" kingdom="Fungi" lsidName="Cladosporium yunnanensis" order="Capnodiales" pageId="0" pageNumber="77224" phylum="Ascomycota" rank="species" species="yunnanensis">
