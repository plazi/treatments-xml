<number id="3DED24F92908E44CA8A5FA17EEFBF996" box="[708,760,1520,1544]" pageId="5" pageNumber="6" value="15.8">15.8</number>
; 
<date id="FF9B101E2908E44CA965FA16EF4FF996" box="[772,844,1520,1544]" pageId="5" pageNumber="6">
IV 
<number id="3DED24F92908E44CA948FA17EF4FF996" box="[809,844,1520,1544]" pageId="5" pageNumber="6" value="21.9">21.</number>
