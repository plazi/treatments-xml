<number id="3DED24F92908E44CAAF6FEE3ECBEFE82" box="[151,189,260,284]" pageId="5" pageNumber="6" value="1.6">1.6</number>
; pedipalpus 
<number id="3DED24F92908E44CAB28FEE3ED6CFE82" box="[329,367,260,284]" pageId="5" pageNumber="6" value="6.8">6.8</number>
; leg I 
<number id="3DED24F92908E44CABD5FEE3EDEBFE82" box="[436,488,260,284]" pageId="5" pageNumber="6" value="13.3">13.3</number>
; II 
<number id="3DED24F92908E44CA86FFEE3EE40FE82" box="[526,579,260,284]" pageId="5" pageNumber="6" value="25.3">25.3</number>
