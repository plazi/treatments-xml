<taxonomicName id="4C390E873447FFE7FDE5E174FF52756B" authority="(Taschenberg, 1880)" baseAuthorityName="Taschenberg" baseAuthorityYear="1880" class="Insecta" family="Pulicidae" genus="Synosternus" kingdom="Animalia" order="Siphonaptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="pallidus">
