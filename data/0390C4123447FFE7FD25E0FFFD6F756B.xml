<taxonomicName id="4C390E873447FFE7FFC3E114FD6A756B" authority="(Rothschild, 1903)" baseAuthorityName="Rothschild" baseAuthorityYear="1903" box="[234,650,854,877]" class="Insecta" family="Pulicidae" genus="Xenopsylla" kingdom="Animalia" order="Siphonaptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="cheopis">
