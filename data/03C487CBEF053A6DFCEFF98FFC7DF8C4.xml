<taxonomicName id="4C6D4D5EEF053A61FC11F876FB06F813" authorityName="Kim &amp; Boxshall" authorityYear="2020" baseAuthorityName="Marchenkov &amp; Boxshall" baseAuthorityYear="2003" box="[980,1219,1976,2002]" class="Maxillopoda" family="Notodelphyidae" genus="Notodelphyopsis" kingdom="Animalia" order="Cyclopoida" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="illgi">
