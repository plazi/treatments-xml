<taxonomicName id="4C2CAEF79569FFC6FEE1FEC7FE227CA7" authority="Gray, 1853" authorityName="Gray" authorityYear="1853" box="[257,504,367,392]" class="Gastropoda" family="Janellidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="13" pageNumber="214" phylum="Mollusca" rank="family">Janellidae Gray, 1853</taxonomicName>
