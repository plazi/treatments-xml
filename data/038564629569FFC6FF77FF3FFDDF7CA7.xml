<taxonomicName id="4C2CAEF79569FFC6FAA9FEE4FA417C4A" authorityName="Gray" authorityYear="1850" box="[1353,1435,332,357]" class="Gastropoda" family="Athoracophoridae" genus="Janella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="13" pageNumber="214" phylum="Mollusca" rank="genus">
