<treatmentCitationGroup id="AB490203FFD54D1D0BF8FAE2FC86152A" box="[151,832,1356,1378]" pageId="9" pageNumber="10">
<taxonomicName id="4C595EAEFFD54D1D0BF8FAE2FE89152A" baseAuthorityName="Quoy &amp; Gaimard" baseAuthorityYear="1827" box="[151,335,1356,1378]" class="Hydrozoa" family="Tiarannidae" genus="Modeeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="9" pageNumber="10" phylum="Cnidaria" rank="species" species="rotunda">
