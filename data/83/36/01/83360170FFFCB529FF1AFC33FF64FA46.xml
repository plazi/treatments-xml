; prothorax: length, 
<number box="[1187,1270,799,823]" pageId="3" pageNumber="64" value="1.35" valueMax="1.5" valueMin="1.2">1.2–1.5</number>
/ 
<number box="[1279,1366,799,823]" pageId="3" pageNumber="64" value="1.9" valueMax="2.4" valueMin="1.4">1.4–2.4</number>
; anterior width, 
<number box="[279,315,836,860]" pageId="3" pageNumber="64" value="0.8">0.8</number>
–1.0/1.0– 
<number box="[424,458,836,860]" pageId="3" pageNumber="64" value="1.6">1.6</number>
; posterior width, 
<number box="[658,745,836,860]" pageId="3" pageNumber="64" value="1.0" valueMax="1.1" valueMin="0.9">0.9–1.1</number>
/ 
<number box="[754,838,836,860]" pageId="3" pageNumber="64" value="1.4" valueMax="1.7" valueMin="1.1">1.1–1.7</number>
; humeral width, 
<number box="[1030,1113,836,860]" pageId="3" pageNumber="64" value="1.25" valueMax="1.4" valueMin="1.1">1.1–1.4</number>
/ 
<number box="[1122,1209,836,860]" pageId="3" pageNumber="64" value="1.75" valueMax="2.2" valueMin="1.3">1.3–2.2</number>
; elytral length, 
<number pageId="3" pageNumber="64" value="4.2" valueMax="4.8" valueMin="3.6">3.6– 4.8</number>
/ 
<number box="[195,281,871,895]" pageId="3" pageNumber="64" value="5.5" valueMax="6.8" valueMin="4.2">4.2–6.8</number>
.
</paragraph>
<paragraph blockId="3.[151,1437,151,1436]" pageId="3" pageNumber="64">
