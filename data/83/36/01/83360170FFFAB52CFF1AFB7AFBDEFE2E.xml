<emphasis bold="true" box="[798,810,1967,1988]" pageId="5" pageNumber="66">6</emphasis>
, male, lateral; 
<emphasis bold="true" box="[963,975,1968,1989]" pageId="5" pageNumber="66">7</emphasis>
; female, dorsal. 
<emphasis bold="true" box="[1146,1184,1967,1989]" pageId="5" pageNumber="66">8–9</emphasis>
, 
<taxonomicName box="[1196,1436,1967,1989]" class="Insecta" family="Cerambycidae" genus="Stultutragus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="endoluteus" status="sp. nov.">
<emphasis box="[1196,1436,1967,1989]" italics="true" pageId="5" pageNumber="66">
<taxonomicName box="[1196,1319,1967,1989]" class="Insecta" family="Cerambycidae" genus="Stultutragus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="genus">Stultutragus</taxonomicName>
