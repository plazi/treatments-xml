[
<date box="[267,426,439,464]" pageId="106" pageNumber="107" value="1965-06-08" valueMax="1965-06-09" valueMin="1965-06-08">
<number box="[267,311,440,464]" pageId="106" pageNumber="107" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
Jun 
<number box="[365,426,439,464]" pageId="106" pageNumber="107" value="1965.0">1965</number>
</date>
], L. Pena/ 
<typeStatus box="[541,644,439,464]" pageId="106" pageNumber="107">Holotype</typeStatus>
<taxonomicName authority="Mathis" authorityName="Mathis" box="[652,967,439,464]" class="Insecta" family="Ephydridae" genus="Setacera" kingdom="Animalia" order="Diptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="trichoscelis">Setacera trichoscelis Mathis</taxonomicName>
