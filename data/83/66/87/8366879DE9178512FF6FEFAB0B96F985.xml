<number box="[411,423,1036,1057]" pageId="7" pageNumber="8" value="5.0">5</number>
(
<number box="[438,447,1036,1057]" pageId="7" pageNumber="8" value="1.0">1</number>
+ 
<number box="[462,476,1036,1057]" pageId="7" pageNumber="8" value="4.0">4</number>
) present (anterior setae reduced in 
<taxonomicName box="[821,948,1036,1058]" class="Insecta" family="Ephydridae" genus="Austrocoenia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
