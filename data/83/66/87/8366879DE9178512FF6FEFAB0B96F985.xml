<keyLead pageId="7" pageNumber="8">
<number box="[151,176,978,999]" pageId="7" pageNumber="8" value="11.0">11</number>
. Dorsocentral setae 
<number box="[410,422,978,999]" pageId="7" pageNumber="8" value="4.0">4</number>
(
<number box="[437,446,978,999]" pageId="7" pageNumber="8" value="1.0">1</number>
+ 
<number box="[462,474,978,999]" pageId="7" pageNumber="8" value="3.0">3</number>
