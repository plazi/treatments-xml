<caption ID-Table-UUID="5FD86636FFF7FFDAEB7B9CF6F6DEBEBF" box="[119,1666,712,736]" httpUri="http://table.plazi.org/id/5FD86636FFF7FFDAEB7B9CF6F6DEBEBF" pageId="6" pageNumber="157" targetBox="[119,1735,859,1616]" targetIsTable="true" targetPageId="6">
<paragraph blockId="6.[119,1666,712,846]" box="[119,1666,712,736]" pageId="6" pageNumber="157">
