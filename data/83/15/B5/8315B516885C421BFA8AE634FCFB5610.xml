<taxonomicName baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[519,615,364,385]" class="Insecta" family="Tortricidae" genus="Tortrix" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="683" phylum="Arthropoda" rank="species" species="viridana">
