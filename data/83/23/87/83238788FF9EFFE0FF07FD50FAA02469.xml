<quantity box="[469,596,226,252]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" metricValueMax="3.6" metricValueMin="2.4" pageId="18" pageNumber="19" unit="mm" value="30.0" valueMax="36.0" valueMin="24.0">24–36 mm</quantity>
