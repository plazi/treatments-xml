<subSubSection pageId="273" pageNumber="274" type="materials_examined">
<paragraph blockId="273.[151,1133,1086,1170]" pageId="273" pageNumber="274">
<emphasis bold="true" box="[151,261,1086,1108]" pageId="273" pageNumber="274">Type host.</emphasis>
<taxonomicName authority="(Meyer, 1885)" authorityName="Meyer" authorityYear="1885" baseAuthorityName="A. B. Meyer" baseAuthorityYear="1885" box="[268,883,1086,1108]" class="Aves" family="Paradisaeidae" genus="Phonygammus" kingdom="Animalia" order="Passeriformes" pageId="273" pageNumber="274" phylum="Chordata" rank="subSpecies" species="keraudrenii" subSpecies="purpureoviolaceus">
<emphasis box="[268,736,1086,1108]" italics="true" pageId="273" pageNumber="274">Phonygammus keraudrenii purpureoviolaceus</emphasis>
(Meyer, 1885)
</taxonomicName>
