<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[798,867,1194,1216]" class="Insecta" family="Philopteridae" genus="Brueelia" kingdom="Animalia" order="Phthiraptera" pageId="254" pageNumber="255" phylum="Arthropoda" rank="species" species="quelea">quelea</taxonomicName>
