ex 
<taxonomicName authorityName="Bush" authorityYear="2017" baseAuthorityName="Bocage" baseAuthorityYear="1870" box="[775,1151,1951,1973]" class="Aves" family="Sturnidae" genus="Cinnyricinclus" kingdom="Animalia" order="Passeriformes" pageId="253" pageNumber="254" phylum="Chordata" rank="subSpecies" species="leucogaster" subSpecies="verreauxi">
