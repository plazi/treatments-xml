. (
<emphasis box="[827,874,224,247]" italics="true" pageId="179" pageNumber="180">Cm.</emphasis>
) 
<emphasis box="[890,1018,223,248]" italics="true" pageId="179" pageNumber="180">paulbrowni</emphasis>
</taxonomicName>
, see 
<taxonomicName authorityName="Bush" authorityYear="2017" box="[1074,1314,223,248]" class="Insecta" family="Philopteridae" genus="Priceiella" kingdom="Animalia" order="Phthiraptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="species" species="paulbrowni" subGenus="Camurnirmus">
<emphasis box="[1074,1101,225,248]" italics="true" pageId="179" pageNumber="180">Pr</emphasis>
. (
<emphasis box="[1124,1170,224,247]" italics="true" pageId="179" pageNumber="180">Cm.</emphasis>
) 
<emphasis box="[1186,1314,223,248]" italics="true" pageId="179" pageNumber="180">paulbrowni</emphasis>
</taxonomicName>
, below.
</paragraph>
</subSubSection>
