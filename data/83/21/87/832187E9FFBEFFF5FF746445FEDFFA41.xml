Ex 
<emphasis bold="true" box="[431,793,725,750]" pageId="54" pageNumber="55">
<taxonomicName baseAuthorityName="Sundevall" baseAuthorityYear="1850" box="[431,662,725,750]" class="Aves" family="Remizidae" genus="Auriparus" kingdom="Animalia" order="Passeriformes" pageId="54" pageNumber="55" phylum="Chordata" rank="species" species="flaviceps">
