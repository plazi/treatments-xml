: 
<quantity id="38249B0C0918FFB2FE28559FB261FC01" box="[487,548,906,932]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="6" pageNumber="19" unit="km" value="4.0">4 km</quantity>
