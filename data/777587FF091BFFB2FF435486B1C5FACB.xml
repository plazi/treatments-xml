<taxonomicName id="38DC4D6A091BFFB1FDEC5318B37BFA82" box="[547,830,1293,1319]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="18" phylum="Arthropoda" rank="subSpecies" species="delilah" subSpecies="utahensis">
<emphasis id="CDA8EAFB091BFFB1FDEC5318B37BFA82" box="[547,830,1293,1319]" italics="true" pageId="5" pageNumber="18">Catocala delilah utahensis</emphasis>
</taxonomicName>
form 
