<td id="C38E6A81B67663B673D459EEDAE3884B" colspan="1" pageId="2" pageNumber="131" rowspan="1">(Holotype ♂ / Paratype ♀)</td>
<td id="3AA02D960EE50B74CB6EFCE0120C2667" colspan="1" pageId="2" pageNumber="131" rowspan="1">Fe</td>
<td id="9ACA83435A1312755DAE030D2C3DE896" colspan="1" pageId="2" pageNumber="131" rowspan="1">Pa</td>
