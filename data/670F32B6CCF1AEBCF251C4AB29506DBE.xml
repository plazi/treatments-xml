<taxonomicName id="CF26D47334167417E7CE2E4495839859" LSID="http://zoobank.org/0EC4E275-EF1A-4987-9F05-242EB6D6FD2D" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus gracilipes" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="gracilipes">Trigonopterus gracilipes Riedel</taxonomicName>
