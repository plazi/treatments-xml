<subSubSection box="[113,861,826,849]" pageId="71" pageNumber="366" type="type_taxon">
<paragraph blockId="71.[113,1090,826,1585]" box="[113,861,826,849]" pageId="71" pageNumber="366">
<emphasis bold="true" box="[113,261,826,849]" pageId="71" pageNumber="366">
<typeStatus box="[113,169,826,849]" pageId="71" pageNumber="366">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Colonnelli, 2014" authorityName="Colonnelli" authorityYear="2014" box="[268,511,826,849]" class="Insecta" family="Curculionidae" genus="Socotracerus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="366" phylum="Arthropoda" rank="species" species="delumbis" status="sp. nov.">
<emphasis box="[268,511,826,849]" italics="true" pageId="71" pageNumber="366">Socotracerus delumbis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[518,603,826,849]" pageId="71" pageNumber="366" rank="species">sp. nov.</taxonomicNameLabel>
, by present designation.
</paragraph>
