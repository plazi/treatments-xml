<paragraph pageId="0" pageNumber="1008">Distribution</paragraph>
<paragraph pageId="0" pageNumber="1008">
Finland (
<bibRefCitation author="Hellen, W." journalOrPublisher="Notulae Entomologicae" pageId="0" pageNumber="1008" pagination="5 - 13" title="Fuer die Fauna Finnlands neue Ichneumoniden: II. Cryptinae, Pimplinae" volume="17" year="1937">
<normalizedToken originalValue="Hellén">Hellen</normalizedToken>
1937
</bibRefCitation>
), Hungary (
<bibRefCitation author="Kiss von Zilah, A." journalOrPublisher="Verhandlungen und Mitteilungen des Siebenburgischen Vereins fuer Naturwissenschaften in Hermannstadt" pageId="0" pageNumber="1008" pagination="74 - 120" title="Zweiter Beitrag zur Kenntnis der ungarischen und siebenbuergischen Ichneumoniden- (Schlupfwespen-) Fauna" volume="75 / 76" year="1926">Kiss von Zilah 1926</bibRefCitation>
), Romania (
<bibRefCitation author="Constantineanu, M. I." journalOrPublisher="Fauna Republicii Socialiste Romania" pageId="0" pageNumber="1008" title="Hymenoptera, Ichneumonidae. Subfamiliile Ephialtinae, Lycorininae, Xoridinae si Acaenitinae" year="1977">Constantineanu and Pisica 1977</bibRefCitation>
