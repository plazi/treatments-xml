<geoCoordinate degrees="18.355536" direction="north" orientation="latitude" precision="1" value="18.355536">18.355536N</geoCoordinate>
, 
<geoCoordinate degrees="68.6182518" direction="west" orientation="longitude" precision="1" value="-68.61825">68.6182518W</geoCoordinate>
