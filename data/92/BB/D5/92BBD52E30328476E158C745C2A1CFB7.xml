<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2458" class="Insecta" family="Formicidae" genus="Strumigenys" kingdom="Animalia" lsidName-HNS="Strumigenys Smith" order="Hymenoptera" pageNumber="37" phylum="Arthropoda" rank="genus">Strumigenys F. Smith</taxonomicName>
