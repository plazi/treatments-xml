<taxonomicName authorityName="Casey" authorityYear="1918" class="Insecta" family="Carabidae" genus="Feronina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="493" pageNumber="494" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="493" pageNumber="494">Feronina</emphasis>
</taxonomicName>
(two species), 
<taxonomicName authorityName="Casey" authorityYear="1918" class="Insecta" family="Carabidae" genus="Gastrellarius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="493" pageNumber="494" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="493" pageNumber="494">Gastrellarius</emphasis>
</taxonomicName>
(three species), 
<taxonomicName authorityName="Casey" authorityYear="1918" class="Insecta" family="Carabidae" genus="Gastrosticta" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="493" pageNumber="494" phylum="Arthropoda" rank="genus">
