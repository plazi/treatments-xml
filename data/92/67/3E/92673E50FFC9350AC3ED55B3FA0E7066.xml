<subSubSection box="[199,673,1500,1525]" pageId="8" pageNumber="531" type="etymology">
<paragraph blockId="8.[151,1437,1500,1633]" box="[199,673,1500,1525]" pageId="8" pageNumber="531">
<emphasis bold="true" box="[199,326,1500,1525]" pageId="8" pageNumber="531">Etymology</emphasis>
