<collectingRegion box="[911,1028,1516,1540]" country="Argentina" name="Corrientes" pageId="13" pageNumber="428">Corrientes</collectingRegion>
, 
<collectorName box="[1053,1159,1516,1540]" pageId="13" pageNumber="428">La Cruz</collectorName>
