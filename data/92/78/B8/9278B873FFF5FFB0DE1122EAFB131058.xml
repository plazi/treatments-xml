<collectingRegion box="[948,1065,1624,1648]" country="Argentina" name="Corrientes" pageId="11" pageNumber="426">Corrientes</collectingRegion>
, 
<collectorName box="[1082,1181,1624,1648]" pageId="11" pageNumber="426">La Cruz</collectorName>
