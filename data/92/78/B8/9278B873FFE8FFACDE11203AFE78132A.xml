<materialsCitation collectorName="La Cruz &amp; Paraje Tres Cerros" country="Argentina" latitude="-29.113335" location="Multiple" longLatPrecision="20" longitude="-56.921112" pageId="22" pageNumber="437" specimenCount="2" specimenCount-female="1" specimenCount-male="1">
