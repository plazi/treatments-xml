and 
<specimenCount box="[1337,1357,1894,1917]" pageId="22" pageNumber="437" type="female">♀</specimenCount>
; Cor- rientes, 
<collectorName box="[899,995,1927,1951]" pageId="22" pageNumber="437">La Cruz</collectorName>
, 
<collectorName box="[1009,1218,1927,1951]" pageId="22" pageNumber="437">Paraje Tres Cerros</collectorName>
; 
<geoCoordinate box="[1232,1356,1926,1951]" degrees="29" direction="south" minutes="06" orientation="latitude" pageId="22" pageNumber="437" precision="15" seconds="48" value="-29.113335">29°06′48′′S</geoCoordinate>
