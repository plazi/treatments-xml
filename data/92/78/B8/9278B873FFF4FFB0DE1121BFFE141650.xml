<taxonomicName authority="(Cassin, 1845)" baseAuthorityName="Cassin" baseAuthorityYear="1845" box="[168,589,159,184]" class="Aves" family="Cathartidae" genus="Cathartes" kingdom="Animalia" order="Accipitriformes" pageId="11" pageNumber="426" phylum="Chordata" rank="species" species="burrovianus">
