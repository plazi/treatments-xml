, 
<collectorName box="[385,478,233,257]" pageId="11" pageNumber="426">La Cruz</collectorName>
, Paraje Tres Cerros; 
<geoCoordinate degrees="29" direction="south" minutes="09" orientation="latitude" pageId="11" pageNumber="426" precision="15" seconds="03" value="-29.150833">29°09′ 03′′S</geoCoordinate>
