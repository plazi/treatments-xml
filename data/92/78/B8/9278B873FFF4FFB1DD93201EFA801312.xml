<collectingRegion box="[269,386,1926,1950]" country="Argentina" name="Corrientes" pageId="10" pageNumber="425">Corrientes</collectingRegion>
, 
<collectorName box="[411,517,1926,1950]" pageId="10" pageNumber="425">La Cruz</collectorName>
