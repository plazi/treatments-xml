<taxonomicName class="Insecta" family="Tetratomidae" genus="Pseudoholostrophus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudoholostrophus" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">Pseudoholostrophus</taxonomicName>
