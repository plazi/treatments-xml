<taxonomicName authority="Nikitsky, 1998" authorityName="Nikitsky" authorityYear="1998" class="Insecta" family="Tetratomidae" genus="Holostrophinus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Holostrophinus" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">Holostrophinus</taxonomicName>
