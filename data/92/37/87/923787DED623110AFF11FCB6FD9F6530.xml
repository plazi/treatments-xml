<subSubSection box="[199,1055,962,987]" pageId="4" pageNumber="487" type="etymology">
<paragraph blockId="4.[151,1436,890,1310]" box="[199,1055,962,987]" pageId="4" pageNumber="487">
<emphasis bold="true" box="[199,337,963,987]" pageId="4" pageNumber="487">Etymology:</emphasis>
