<taxonomicName authorityName="Piran" authorityYear="1959" class="Insecta" family="Pentatomidae" genus="Mecocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="181" phylum="Arthropoda" rank="species" species="curculionoides">
