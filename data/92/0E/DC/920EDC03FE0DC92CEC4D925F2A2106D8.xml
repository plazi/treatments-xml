17. 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143805" class="Insecta" family="Formicidae" genus="Pseudomyrma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Pseudomyrma cladoica Smith" order="Hymenoptera" pageId="157" pageNumber="157" phylum="Arthropoda" rank="species" species="cladoica">Pseudomyrma cladoica</taxonomicName>
