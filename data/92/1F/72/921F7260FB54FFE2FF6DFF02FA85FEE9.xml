</taxonomicName>
; 
<taxonomicName authority="Gonzalez, Carrejo &amp; Garcia Aldrete, 2017" authorityName="Gonzalez, Carrejo &amp; Garcia Aldrete" authorityYear="2017" class="Insecta" family="Ptiloneuridae" genus="Triplocania" kingdom="Animalia" order="Psocodea" pageId="4" pageNumber="541" phylum="Arthropoda" rank="species" species="asisensis">
