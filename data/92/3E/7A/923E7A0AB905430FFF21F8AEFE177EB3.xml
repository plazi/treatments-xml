<geoCoordinate box="[1194,1343,231,252]" degrees="18" direction="south" minutes="47" orientation="latitude" pageId="4" pageNumber="33" precision="1" seconds="44.3" value="-18.795639">S 18° 47’ 44.3’’</geoCoordinate>
<geoCoordinate degrees="048" direction="east" minutes="25" orientation="longitude" pageId="4" pageNumber="33" precision="1" seconds="36.9" value="48.426918">E 048° 25’ 36.9</geoCoordinate>
