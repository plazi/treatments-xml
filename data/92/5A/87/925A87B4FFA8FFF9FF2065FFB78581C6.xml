<geoCoordinate box="[294,398,1575,1599]" direction="east" orientation="longitude" pageId="19" pageNumber="320" precision="925" value="138.66667">
<number box="[294,336,1575,1599]" pageId="19" pageNumber="320" value="138.0">138</number>
° 
<number box="[345,376,1575,1599]" pageId="19" pageNumber="320" value="40.0">40</number>
