<geoCoordinate box="[452,526,1575,1599]" direction="south" orientation="latitude" pageId="19" pageNumber="320" precision="925" value="-4.3333335">
<number box="[452,467,1575,1599]" pageId="19" pageNumber="320" value="4.0">4</number>
