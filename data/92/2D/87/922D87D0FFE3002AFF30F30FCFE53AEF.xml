<collectingCountry box="[672,785,1327,1352]" name="Sri Lanka" pageId="19" pageNumber="20">Sri Lanka</collectingCountry>
(
<collectingCountry box="[802,886,1327,1352]" name="Sri Lanka" pageId="19" pageNumber="20">Ceylon</collectingCountry>
) (
<bibRefCitation author="Attems" box="[912,1064,1327,1352]" pageId="19" pageNumber="20" refString="Attems, C. (1936) Diplopoda of India. Memoirs of the Indian Museum, 11 (4), 133 - 323." type="journal article" year="1936">Attems 1936</bibRefCitation>
