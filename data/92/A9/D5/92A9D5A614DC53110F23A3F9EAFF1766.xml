<paragraph pageId="173" pageNumber="174">
Tribe 
<taxonomicName authority="Poey, 1854" authorityName="Poey" authorityYear="1854" lsidName="" pageId="173" pageNumber="174" rank="tribe" tribe="Parmulini">Parmulini Poey, 1854</taxonomicName>
