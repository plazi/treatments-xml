<taxonomicName id="4C394D48F90A843CFD2B3119B1E3FDAB" authority="Stephens, 1835" authorityName="Stephens" authorityYear="1835" box="[671,999,531,558]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
