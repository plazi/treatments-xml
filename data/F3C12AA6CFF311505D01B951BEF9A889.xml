<subSubSection id="B4F4E7F4FC8B4656E2B84C0540FBB47D" pageId="0" pageNumber="87" type="type species">
<paragraph id="AB3E443460CBBEB0B767A5729F8AFAEC" pageId="0" pageNumber="87">Type species.</paragraph>
<paragraph id="78685A529C2A11EBC81932F06C4B2D5A" pageId="0" pageNumber="87">
<taxonomicName id="9A6718508618169A9375CAB4BABECD45" authority="Wang, Chen &amp; Wu, 2017" authorityName="Wang, Chen &amp; Wu" authorityYear="2017" class="Insecta" family="Pyralidae" genus="Arcanusa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Arcanusa apexiarcanusa" order="Lepidoptera" pageId="0" pageNumber="87" phylum="Arthropoda" rank="species" species="apexiarcanusa">Arcanusa apexiarcanusa</taxonomicName>
