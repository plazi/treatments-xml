<taxonomicName id="C9854D43FFF9FFC0253EF97DB7C7F8F2" authority="J. L. Barnard, 1972" authorityName="J. L. Barnard" authorityYear="1972" box="[780,1170,1778,1802]" class="Malacostraca" family="Phliantidae" genus="Quasimodia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Amphipoda" pageId="0" pageNumber="449" phylum="Arthropoda" rank="species" species="enna">
