<taxonomicName id="9FDA4D0E4F177B37FEE8C1A0FDD781F3" box="[324,633,1924,1945]" class="Insecta" family="Scydmaenidae" genus="Cephennodes" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="430" phylum="Arthropoda" rank="species" species="mussauinsularis" status="sp. n.">
<emphasis id="6AAEEA9F4F177B37FEE8C1A0FDD781F3" box="[324,633,1924,1945]" italics="true" pageId="3" pageNumber="430">Cephennodes mussauinsularis</emphasis>
