<taxonomicName id="4C204D24DF18FF89C1AEFAFEFDDB5BE2" authority="Schoedler, 1858" authorityName="Schoedler" authorityYear="1858" box="[238,609,1386,1412]" class="Branchiopoda" family="Daphniidae" genus="Scapholeberis" kingdom="Animalia" order="Diplostraca" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis id="B954EAB5DF18FF89C1AEFAFEFE2D5BE2" bold="true" box="[238,407,1386,1412]" italics="true" pageId="9" pageNumber="10">Scapholeberis</emphasis>
