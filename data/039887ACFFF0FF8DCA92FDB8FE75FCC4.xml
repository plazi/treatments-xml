<emphasis id="B945EAA8FFF2FF8DC810FE22FD52FE4B" box="[533,641,441,464]" italics="true" pageId="6" pageNumber="487">E. specca</emphasis>
</taxonomicName>
); body deep in 
<taxonomicName id="4C314D39FFF2FF8DC93DFE22FC4FFE4B" box="[824,924,440,464]" class="Actinopterygii" family="Gobiidae" genus="Eviota" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="487" phylum="Chordata" rank="species" species="herrei">
<emphasis id="B945EAA8FFF2FF8DC93DFE22FC4FFE4B" box="[824,924,440,464]" italics="true" pageId="6" pageNumber="487">E. herrei</emphasis>
</taxonomicName>
, 
<number id="3DF9249DFFF2FF8DC9A9FE2CFBCEFE4B" box="[940,1053,439,464]" pageId="6" pageNumber="487" value="26.65" valueMax="28.5" valueMin="24.8">24.8â€“28.5</number>
