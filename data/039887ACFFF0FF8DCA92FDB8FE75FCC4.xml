<caption id="DF4E6632FFF1FF8ECA92F9C7FB30F9E9" box="[151,1251,1628,1650]" httpUri="https://zenodo.org/record/228735/files/figure.png" pageId="5" pageNumber="486" targetBox="[151,1436,1042,1606]" targetPageId="5">
<paragraph id="8B8E36BAFFF1FF8ECA92F9C7FB30F9E9" blockId="5.[151,1251,1628,1650]" box="[151,1251,1628,1650]" pageId="5" pageNumber="486">
