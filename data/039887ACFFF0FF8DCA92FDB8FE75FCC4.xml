<number id="3DF9249DFFF2FF8DCB13FF7BFEF0FF63" box="[278,291,224,248]" pageId="6" pageNumber="487" value="8.0">8</number>
/ 
<number id="3DF9249DFFF2FF8DCB28FF7BFEE9FF63" box="[301,314,224,248]" pageId="6" pageNumber="487" value="8.0">8</number>
with 
<taxonomicName id="4C314D39FFF2FF8DCB79FF7AFE3BFF63" box="[380,488,225,248]" class="Actinopterygii" family="Gobiidae" genus="Eviota" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="487" phylum="Chordata" rank="species" species="specca">
<emphasis id="B945EAA8FFF2FF8DCB79FF7AFE3BFF63" box="[380,488,225,248]" italics="true" pageId="6" pageNumber="487">E. specca</emphasis>
