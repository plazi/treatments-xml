<taxonomicName id="FDA76E596D49800F6A1E70D0871ED502" baseAuthorityName="Geoffroy in Fourcroy" baseAuthorityYear="1785" class="Hexapoda" family="Carabidae" genus="Carabus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="185" pageNumber="186" phylum="Arthropoda" rank="species" species="bicolor">
