</taxonomicName>
, 
<taxonomicName id="4C696DD9FFE9FFC5FCD56EE5E7E591E7" authority="Molero &amp; Bach &amp; Sendra &amp; Montagud &amp; Barranco &amp; Gaju, 2013" authorityName="Molero &amp; Bach &amp; Sendra &amp; Montagud &amp; Barranco &amp; Gaju" authorityYear="2013" box="[781,918,1412,1436]" class="Insecta" family="Nicoletiidae" genus="Coletinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Zygentoma" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="redetecta" status="n. sp.">
<emphasis id="B91DCA48FFE9FFC5FCD56EE5E7E591E7" box="[781,918,1412,1436]" italics="true" pageId="34" pageNumber="35">C. redetecta</emphasis>
</taxonomicName>
<emphasis id="B91DCA48FFE9FFC5FC476EE5E7AC91E7" bold="true" box="[927,991,1412,1436]" pageId="34" pageNumber="35">
<taxonomicNameLabel id="A22E7733FFE9FFC5FC476EE5E7AC91E7" box="[927,991,1412,1436]" pageId="34" pageNumber="35" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
