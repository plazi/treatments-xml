<quantity id="4CAC5FB3FFE41806FC400B0FFBC6FB86" box="[1008,1142,1081,1107]" metricMagnitude="2" metricUnit="m" metricValue="5.5" metricValueMax="7.0" metricValueMin="4.0" pageId="94" pageNumber="95" unit="m" value="550.0" valueMax="700.0" valueMin="400.0">
