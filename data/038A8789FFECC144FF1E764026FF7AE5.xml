<taxonomicName id="4C234D1CFFEFC145FE1E7595253D7CE6" box="[407,552,1730,1753]" class="Hydrozoa" family="Bougainvilliidae" genus="Bougainvillia" kingdom="Animalia" order="Anthoathecata" pageId="6" pageNumber="7" phylum="Cnidaria" rank="genus">
