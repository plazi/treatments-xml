<taxonomicName id="4C234D1CFFEFC145FD7C75B223E97CC4" authority="Millard &amp; Bouillon, 1973" authorityName="Millard &amp; Bouillon" authorityYear="1973" box="[757,1276,1764,1788]" class="Hydrozoa" family="Bougainvilliidae" genus="Silhouetta" kingdom="Animalia" order="Anthoathecata" pageId="6" pageNumber="7" phylum="Cnidaria" rank="species" species="uvacarpa">
