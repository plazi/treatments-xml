<taxonomicName id="4C234D1CFFEFC145FD34747D23737D7E" authority="McCrady, 1857" authorityName="McCrady" authorityYear="1857" box="[701,1126,1834,1857]" class="Hydrozoa" family="Oceaniidae" genus="Turritopsis" kingdom="Animalia" order="Anthoathecata" pageId="6" pageNumber="7" phylum="Cnidaria" rank="species" species="nutricula">
<emphasis id="B957EA8DFFEFC145FD34747D24277D7E" box="[701,818,1834,1857]" italics="true" pageId="6" pageNumber="7">Turritopsis</emphasis>
