<taxonomicName id="44ADA3CBFFF06646FEE9F9AAFDA0F9C7" authority="Wheeler, 1915" authorityName="Wheeler" authorityYear="1915" box="[302,621,1638,1660]" class="Insecta" family="Formicidae" genus="Liometopum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="136" phylum="Arthropoda" rank="species" species="oligocenicum">
<emphasis id="B1D9045AFFF06646FEE9F9AAFE00F9C7" bold="true" box="[302,461,1639,1660]" italics="true" pageId="5" pageNumber="136">L. oligocenicum</emphasis>
<bibRefCitation id="C75AB22D1D8BF8A670E912F5C76FDF8B" author="Wheeler W. M." journalOrPublisher="Schriften der Physikalisch- Okonomischen Gesellschaft zu Konigsberg" pagination="1 - 142" part="55" refId="ref21906" refString="Wheeler W. M. 1915. The ants of the Baltic Amber. Schriften der Physikalisch- Ökonomischen Gesellschaft zu Königsberg. 55: 1 – 142." title="The ants of the Baltic Amber" type="journal article" year="1915">Wheeler, 1915</bibRefCitation>
</taxonomicName>
(Baltic amber, Late Eocene), 
<taxonomicName id="44ADA3CBFFF06646FE8AF949FD41F922" authority="Carpenter, 1930" authorityName="Carpenter" authorityYear="1930" box="[333,652,1667,1689]" class="Insecta" family="Formicidae" genus="Liometopum" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="136" phylum="Arthropoda" rank="species" species="miocenicum">
