<taxonomicName id="E3A7FE88FFE88318FB255E04FAE0F980" box="[1249,1392,1636,1659]" class="Magnoliopsida" family="Brassicaceae" genus="Fibigia" kingdom="Plantae" order="Brassicales" pageId="85" pageNumber="85" phylum="Tracheophyta" rank="species" species="macrocarpa">
