<taxonomicName id="4C1E4D2D1D38FFB6FEB8E657B6A6FEEC" box="[369,488,348,369]" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="431" phylum="Arthropoda" rank="genus">
