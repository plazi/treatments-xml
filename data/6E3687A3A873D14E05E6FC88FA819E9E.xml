<collectingRegion id="245BF857A873D141013BFA7DFB049AD8" box="[1098,1189,1482,1508]" country="Ireland" name="Monaghan" pageId="22" pageNumber="23">MNHN</collectingRegion>
<location id="E340606EA873D14101C1FA7DFB759AD8" LSID="urn:lsid:plazi:treatment:6E3687A3A873D14E05E6FC88FA819E9E:E340606EA873D14101C1FA7DFB759AD8" box="[1200,1236,1482,1508]" name="Ga" pageId="22" pageNumber="23" stateProvince="Monaghan">Ga</location>
5724)
