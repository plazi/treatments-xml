</taxonomicName>
; 33, 
<taxonomicName id="3ED91DDA6A25FFF0FB7CFC2A8EE8DFA5" authority="Knutson &amp; Bratt" authorityName="Knutson &amp; Bratt" class="Insecta" family="Sciomyzidae" genus="Colobaea" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="deemingi">
<emphasis id="CBADBA4B6A25FFF0FB7CFC2A8B04DF81" box="[1188,1313,897,921]" italics="true" pageId="39" pageNumber="40">C. deemingi</emphasis>
Knutson &amp; Bratt
</taxonomicName>
; 34, 
<taxonomicName id="3ED91DDA6A25FFF0FF25FC0E8FD6DFA5" authority="(Lundbeck)" baseAuthorityName="Lundbeck" baseAuthorityYear="1923" box="[253,499,933,957]" class="Insecta" family="Sciomyzidae" genus="Colobaea" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="punctata">
<emphasis id="CBADBA4B6A25FFF0FF25FC0E8F51DFA5" box="[253,372,934,957]" italics="true" pageId="39" pageNumber="40">C. punctata</emphasis>
