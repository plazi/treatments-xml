<taxonomicName id="4C1DC1F2FFB37C12FAE3F97419439721" class="Insecta" family="Ichneumonidae" genus="Megastylus" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="vagabundus">
