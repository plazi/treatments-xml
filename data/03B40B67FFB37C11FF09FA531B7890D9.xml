<taxonomicName id="4C1DC1F2FFB37C12FB9EF8901D529721" authority="Benoit" authorityName="Benoit" box="[1024,1295,1812,1836]" class="Insecta" family="Ichneumonidae" genus="Megastylus" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="mascarenscis">
