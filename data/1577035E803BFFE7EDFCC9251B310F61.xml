<quantity id="5A261FAD8044FFE7EC36CF2D189D0F60" box="[349,448,260,284]" metricMagnitude="1" metricUnit="m" metricValue="6.15" metricValueMax="12.1" metricValueMin="0.2" pageId="64" pageNumber="65" unit="m" value="61.5" valueMax="121.0" valueMin="2.0">2–121 m</quantity>
