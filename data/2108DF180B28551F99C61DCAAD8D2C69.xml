<paragraph id="9735D0AC2B1F5D6BFDBE376F60272E6C" pageId="0" pageNumber="323">
<taxonomicName id="677CFADB1CDF39A48C4DFFD2A87F8D48" authorityName="Kononova &amp; Petrov" authorityYear="2001" class="Insecta" family="Scelionidae" genus="Gryon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gryon grande" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="species" species="grande">Gryon grande</taxonomicName>
