<number id="DB4624B1FFEEFFCAFF3B5A14FF6BF982" box="[151,213,1592,1618]" pageId="3" pageNumber="34" value="1997.0">1997</number>
a
</bibRefCitation>
); 
<number id="DB4624B1FFEEFFCAFF545A14FEBDF982" box="[248,259,1592,1618]" pageId="3" pageNumber="34" value="1.0">1</number>
F, 
<collectionCode id="0B9FAE53FFEEFFCAFE8F5A14FEF3F982" box="[291,333,1592,1618]" pageId="3" pageNumber="34">TN</collectionCode>
, Sevier Co., Clingman’s Dome, 
<date id="19301056FFEEFFCAFD645A14FCE4F982" box="[712,858,1592,1618]" pageId="3" pageNumber="34" value="2001-07-17">
<number id="DB4624B1FFEEFFCAFD645A14FD5BF982" box="[712,741,1592,1618]" pageId="3" pageNumber="34" value="17.0">17</number>
