<taxonomicName class="Magnoliopsida" family="Primulaceae" genus="Lysimachia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Ericales" pageId="0" pageNumber="75" phylum="Tracheophyta" rank="subGenus" subGenus="Idiophyton">Idiophyton</taxonomicName>
