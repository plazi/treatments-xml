<taxonomicName authority="sinecus" class="Insecta" family="Valerianaceae" genus="T." kingdom="Animalia" lsidName="T. sinecus subsp. sinecus" order="Orthoptera" pageId="0" pageNumber="163" rank="subSpecies" species="sinecus" subSpecies="sinecus">
