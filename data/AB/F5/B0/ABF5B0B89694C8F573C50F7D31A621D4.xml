<taxonomicName class="Insecta" family="Carabidae" genus="Eobroscus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="307" pageNumber="308" phylum="Arthropoda" rank="genus">
