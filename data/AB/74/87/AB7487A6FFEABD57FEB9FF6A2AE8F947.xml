, 
<bibRefCitation author="Singh, L." box="[983,1042,692,718]" pageId="5" pageNumber="6" pagination="419 - 429" refId="ref6173" refString="Singh, L. 1974. Chromosomes of six species of Indian snakes. Herpetologica 30: 419 - 429." type="journal article" year="1974">1974</bibRefCitation>
; 
<bibRefCitation author="Ivanov, V. G." box="[1056,1208,692,719]" pageId="5" pageNumber="6" pagination="993 - 996" refId="ref5563" refString="Ivanov, V. G. 1975. A description of the karyotype and peculiarities of heteromorphism in the sex chromosomes of Elaphe dione Pall (Serpentes, Colubridae). Tsitologiia 17: 993 - 996." type="journal article" year="1975">Ivanov, 1975</bibRefCitation>
; 
<bibRefCitation author="Hardy, L. M." box="[108,254,729,755]" pageId="5" pageNumber="6" pagination="189 - 191" refId="ref5402" refString="Hardy, L. M. 1976. The chromosomes of a rare Mexican colubrid snake. Copeia 1976: 189 - 191." type="journal article" year="1976">Hardy, 1976</bibRefCitation>
; 
<bibRefCitation author="Van Devender, R. W. &amp; C. J. Cole" box="[267,629,729,756]" pageId="5" pageNumber="6" pagination="1 - 12" refId="ref6316" refString="Van Devender, R. W., and C. J. Cole. 1977. Notes on a colubrid snake, Tantilla vermiformis, from Central America. American Museum Novitates 2625: 1 - 12." type="journal article" year="1977">Van Devender and Cole, 1977</bibRefCitation>
; 
