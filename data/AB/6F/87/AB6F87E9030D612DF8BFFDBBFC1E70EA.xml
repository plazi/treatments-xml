<materialsCitation ID-GBIF-Occurrence="2610627301" country="Russia" location="Olenek River" pageId="4" pageNumber="83" specimenCount="1" typeStatus="holotype">
<bibRefCitation author="Tshistjakov YA" box="[456,655,167,193]" pageId="4" pageNumber="83" pagination="632 - 643" refId="ref3415" refString="Tshistjakov YA (1988) Tiger moths of the genus Dodia Dyar, 1901 (Lepidoptera, Arctiidae) of the fauna of the USSR. Entomologicheskoe Obozrenie 67: 632 - 643. (In Russian)." type="journal article" year="1988">Tshistjakov, 1988</bibRefCitation>
– Type locality: [
<location LSID="urn:lsid:plazi:treatment:AB6F87E9030D612DF8BFFDBBFC1E70EA:26196024030B612DFB67FF50FC4771F1" box="[852,1006,167,193]" country="Russia" name="Olenek River" pageId="4" pageNumber="83">Olenek River</location>
<locationDeviation firstLocation="lower Tomba" pageId="4" pageNumber="83" secondLocation="Alakit River">between lower Tomba and Alakit River</locationDeviation>
, 
<collectingCountry box="[495,574,201,228]" name="Russia" pageId="4" pageNumber="83">Russia</collectingCountry>
]. 
<typeStatus box="[592,699,202,228]" pageId="4" pageNumber="83">Holotype</typeStatus>
male [
<collectionCode box="[776,832,202,228]" country="Russia" httpUri="http://grbio.org/cool/z2y8-2jfr" name="Russian Academy of Sciences, Zoological Institute, Zoological Museum" pageId="4" pageNumber="83" type="Museum">ZIN</collectionCode>
