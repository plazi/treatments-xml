<paragraph blockId="27.[164,779,1207,1260]" pageId="27" pageNumber="388">
<materialsCitation collectionCode="CCMGE" pageId="27" pageNumber="388" specimenCode="CCMGE 70" specimenCount="1" typeStatus="holotype">
<emphasis box="[164,270,1208,1229]" italics="true" pageId="27" pageNumber="388">
