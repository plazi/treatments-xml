<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Magnoliopsida" family="Cyclophoridae" genus="Pearsonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pearsonia" order="Cycloneritida" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">Pearsonia</taxonomicName>
