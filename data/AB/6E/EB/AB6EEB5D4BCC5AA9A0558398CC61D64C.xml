<subSubSection pageId="0" pageNumber="1" type="reference_group">
<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Magnoliopsida" family="Cyclophoridae" genus="Pearsonia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pearsonia vilvensi" order="Cycloneritida" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="vilvensi">Pearsonia vilvensi</taxonomicName>
