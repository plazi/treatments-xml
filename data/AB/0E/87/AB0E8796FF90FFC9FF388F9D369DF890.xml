<taxonomicNameLabel box="[676,735,1189,1210]" pageId="2" pageNumber="235" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
(A–B) and 
<taxonomicName box="[860,951,1190,1211]" class="Arachnida" family="Lycosidae" genus="Mongolicosa" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="235" phylum="Arthropoda" rank="species" species="uncia" status="sp. n.">
<emphasis box="[860,951,1190,1211]" italics="true" pageId="2" pageNumber="235">M. uncia</emphasis>
</taxonomicName>
<emphasis bold="true" box="[960,1018,1189,1210]" pageId="2" pageNumber="235">
