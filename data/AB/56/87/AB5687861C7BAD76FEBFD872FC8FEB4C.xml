<paragraph blockId="8.[264,1325,284,1870]" pageId="8" pageNumber="9">
<materialsCitation collectingDate="2000-12-31" collectionCode="NZAC" collectorName="Waitakere Ra. &amp; Taumata Track &amp; N. A. Martin" country="New Zealand" location="Titirangi" municipality="Ngaio Road" pageId="8" pageNumber="9" specimenCode="NZAC04-039" specimenCount="1" typeStatus="paratype">
