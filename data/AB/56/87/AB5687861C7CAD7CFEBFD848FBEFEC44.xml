<taxonomicName authority="Henderson &amp; Hodgson" authorityName="Henderson &amp; Hodgson" box="[923,1318,1644,1670]" class="Insecta" family="Coccidae" genus="Umbonichiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="adelus">
<emphasis box="[923,1035,1644,1669]" italics="true" pageId="2" pageNumber="3">U. adelus</emphasis>
Henderson &amp; Hodgson
</taxonomicName>
, 
<taxonomicName authority="Henderson &amp; Hodgson, 2005" authorityName="Henderson &amp; Hodgson" authorityYear="2005" box="[264,422,1684,1709]" class="Insecta" family="Coccidae" genus="Umbonichiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="bispinatus" status="sp. nov.">
<emphasis box="[264,422,1684,1709]" italics="true" pageId="2" pageNumber="3">U. bispinatus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[431,526,1684,1710]" pageId="2" pageNumber="3">
<taxonomicNameLabel box="[431,526,1684,1710]" pageId="2" pageNumber="3" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="Henderson &amp; Hodgson" authorityName="Henderson &amp; Hodgson" box="[543,960,1684,1710]" class="Insecta" family="Coccidae" genus="Umbonichiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="bullatus">
<emphasis box="[543,675,1684,1709]" italics="true" pageId="2" pageNumber="3">U. bullatus</emphasis>
Henderson &amp; Hodgson
</taxonomicName>
, 
<taxonomicName authority="(Maskell)" baseAuthorityName="Maskell" box="[974,1318,1684,1710]" class="Insecta" family="Coccidae" genus="Ctenochiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="hymenantherae">
<emphasis box="[974,1193,1684,1709]" italics="true" pageId="2" pageNumber="3">U. hymenantherae</emphasis>
(Maskell)
</taxonomicName>
, 
<taxonomicName authority="Henderson &amp; Hodgson" authorityName="Henderson &amp; Hodgson" box="[264,676,1724,1750]" class="Insecta" family="Coccidae" genus="Umbonichiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="jubatus">
<emphasis box="[264,388,1724,1749]" italics="true" pageId="2" pageNumber="3">U. jubatus</emphasis>
Henderson &amp; Hodgson
</taxonomicName>
, 
<taxonomicName authority="Henderson &amp; Hodgson" authorityName="Henderson &amp; Hodgson" box="[690,1119,1724,1750]" class="Insecta" family="Coccidae" genus="Umbonichiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="pellaspis">
<emphasis box="[690,831,1724,1749]" italics="true" pageId="2" pageNumber="3">U. pellaspis</emphasis>
Henderson &amp; Hodgson
</taxonomicName>
, and 
<taxonomicName authority="Henderson &amp; Hodgson, 2005" authorityName="Henderson &amp; Hodgson" authorityYear="2005" box="[1185,1278,1724,1749]" class="Insecta" family="Coccidae" genus="Umbonichiton" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="rimu" status="sp. nov.">
<emphasis box="[1185,1278,1724,1749]" italics="true" pageId="2" pageNumber="3">U. rimu</emphasis>
</taxonomicName>
<emphasis bold="true" pageId="2" pageNumber="3">
<taxonomicNameLabel pageId="2" pageNumber="3" rank="species">sp. nov.</taxonomicNameLabel>
