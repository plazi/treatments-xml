, 
<geoCoordinate degrees="99" direction="east" minutes="49" orientation="longitude" precision="1" seconds="4.76" value="99.817986">99°49'4.76&quot;E</geoCoordinate>
; Sai Nuan 
<geoCoordinate degrees="10" direction="north" minutes="4" orientation="latitude" precision="1" seconds="43.24" value="10.078678">10°4'43.24&quot;N</geoCoordinate>
, 
