<taxonomicName id="FADBB3A4FF867930FC2920A461F189BC" box="[898,1014,1509,1534]" class="Oligochaeta" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="55" pageNumber="56" phylum="Annelida" rank="genus">
