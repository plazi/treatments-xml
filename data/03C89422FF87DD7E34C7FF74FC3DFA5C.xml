<typeStatus id="54DA9B96FF87DD7E3016FA81FBCAFAEC" box="[1094,1144,1378,1404]" pageId="86" pageNumber="87">type</typeStatus>
from the Pacific Ocean, west off 
<collectingCountry id="F37665A4FF87DD7E34AFFA69FEF3FA34" box="[255,321,1418,1444]" name="Japan" pageId="86" pageNumber="87">Japan</collectingCountry>
[
<geoCoordinate id="EE5543F3FF87DD7E3502FA69FE08FA34" box="[338,442,1418,1444]" degrees="38" direction="north" minutes="03" orientation="latitude" pageId="86" pageNumber="87" precision="925" value="38.05">38°03’N</geoCoordinate>
