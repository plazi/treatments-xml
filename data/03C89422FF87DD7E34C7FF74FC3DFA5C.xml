<quantity id="4C9988D1FF87DD7E3690FE99FC8DFE04" box="[704,831,378,404]" metricMagnitude="3" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="86" pageNumber="87" unit="m" value="1500.0" valueMax="3000.0" valueMin="0.0">0–3000 m</quantity>
