<typeStatus id="54B6E18B5626BC08FF72FDDCDDFFFDDE" box="[151,212,551,577]" pageId="2" pageNumber="43">Type</typeStatus>
–species
</emphasis>
. 
<taxonomicName id="4C0D24AA5626BC08FEADFDD3DFC1FDDD" authority="Edwards, 1928" authorityName="Edwards" authorityYear="1928" box="[328,746,552,578]" class="Insecta" family="Scatopsidae" genus="Scatopse" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="43" phylum="Arthropoda" rank="species" species="flavipalpis">
