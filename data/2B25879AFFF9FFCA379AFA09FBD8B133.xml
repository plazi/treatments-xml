<location id="A6536057FFF9FFCB36EAF921FDFEB295" LSID="urn:lsid:plazi:treatment:2B25879AFFF9FFCA379AFA09FBD8B133:A6536057FFF9FFCB36EAF921FDFEB295" box="[487,536,1751,1777]" country="Brazil" county="Label" municipality="Note" name="Tefe" pageId="4" pageNumber="527" stateProvince="Amazonas">Tefé</location>
in the 
<location id="A6536057FFF9FFCB3565F921FD29B295" LSID="urn:lsid:plazi:treatment:2B25879AFFF9FFCA379AFA09FBD8B133:A6536057FFF9FFCB3565F921FD29B295" box="[616,719,1751,1777]" country="Brazil" county="Label" municipality="Note" name="Brazilian" pageId="4" pageNumber="527" stateProvince="Amazonas">Brazilian</location>
