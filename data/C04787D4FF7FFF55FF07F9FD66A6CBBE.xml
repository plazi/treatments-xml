<taxonomicName id="8FEE4D41FF7EFF55FC76FF1D6002CB93" authorityName="Warburton and Nuttall" authorityYear="1909" box="[998,1345,186,212]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="koningsbergeri">
