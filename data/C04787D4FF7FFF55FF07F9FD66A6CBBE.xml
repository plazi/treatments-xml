<subSubSection id="00F46549FF7FFF54FD1DF9FC643ECDDE" pageId="150" pageNumber="151" type="discussion">
<paragraph id="485136C2FF7FFF54FD1DF9FC643ECDDE" blockId="150.[151,1437,1626,2013]" pageId="150" pageNumber="151">
An 
<collectingRegion id="8A2AF820FF7FFF54FD27F9FC6650CD32" box="[695,787,1627,1653]" country="Saudi Arabia" name="Ash Sharqiyah" pageId="150" pageNumber="151">Oriental</collectingRegion>
