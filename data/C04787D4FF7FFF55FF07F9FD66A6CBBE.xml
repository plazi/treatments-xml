<paragraph id="485136C2FF7EFF55FF07FF1D66A6CBBE" blockId="151.[151,1437,150,249]" pageId="151" pageNumber="152">
<emphasis id="7A9AEAD0FF7EFF55FF07FF1D659BCB93" bold="true" box="[151,216,186,212]" pageId="151" pageNumber="152">Note:</emphasis>
<taxonomicName id="8FEE4D41FF7EFF55FF4EFF1D64A4CB93" authorityName="Schulze" authorityYear="1938" box="[222,487,186,212]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="151" pageNumber="152" phylum="Arthropoda" rank="species" species="bartelsi">
