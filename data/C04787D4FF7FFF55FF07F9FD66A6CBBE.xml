<taxonomicName id="8FEE4D41FF7FFF54FF07F9D86442CDDE" box="[151,257,1663,1689]" class="Mammalia" kingdom="Animalia" order="Rodentia" pageId="150" pageNumber="151" phylum="Chordata" rank="order">Rodentia</taxonomicName>
