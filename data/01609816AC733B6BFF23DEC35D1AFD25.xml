<taxonomicName id="4EC95283AC733B14FEA6DEC35ECFF93C" authority="Leach, 1815" authorityName="Leach" authorityYear="1815" box="[375,652,1670,1693]" class="Insecta" family="Elateridae" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="456" phylum="Arthropoda" rank="subFamily" subFamily="Elaterinae">
ELATERINAE 
<bibRefCitation id="ED5854F1AC733B14FDDCDEC35ECFF93C" author="LEACH W. E." box="[525,652,1670,1693]" pageId="63" pageNumber="456" pagination="145" refId="ref99130" refString="LEACH W. E. 1815. - Entomology, in Brewster, D. Edinburgh Encyclopaedia, Zeitschrift 1: 145." type="journal article" year="1815">Leach, 1815</bibRefCitation>
