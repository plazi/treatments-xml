<emphasis bold="true" box="[151,586,915,941]" pageId="5" pageNumber="58">
Subgenus 
<taxonomicName authority="Knab, 1914" authorityName="Knab" authorityYear="1914" box="[279,586,915,941]" class="Insecta" family="Drosophilidae" genus="Titanochaeta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="58" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[279,436,915,941]" italics="true" pageId="5" pageNumber="58">Titanochaeta</emphasis>
Knab, 
<number box="[528,586,915,941]" pageId="5" pageNumber="58" value="1914.0">1914</number>
</taxonomicName>
</emphasis>
</heading>
