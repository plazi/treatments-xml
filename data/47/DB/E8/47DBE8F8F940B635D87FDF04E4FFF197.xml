<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Albizia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Albizia lebbeck" order="Fabales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="lebbeck">
