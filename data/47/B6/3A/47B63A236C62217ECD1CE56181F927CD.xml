<taxonomicName class="Insecta" family="Cynipidae" genus="Diplolepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diplolepis rosaefolii" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="rosaefolii">Diplolepis rosaefolii</taxonomicName>
