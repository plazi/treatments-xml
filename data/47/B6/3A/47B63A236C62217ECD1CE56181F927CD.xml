<taxonomicName class="Insecta" family="Cynipidae" genus="Diplolepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diplolepis nodulosa" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="nodulosa">Diplolepis nodulosa</taxonomicName>
on 
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Rosa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rosa virginiana" order="Rosales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="virginiana">Rosa virginiana</taxonomicName>
