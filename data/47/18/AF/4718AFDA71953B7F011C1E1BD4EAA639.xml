<taxonomicName baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1853" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="15" pageNumber="16" phylum="Mollusca" rank="species" species="baranguillanus" subGenus="Drymaeus">
