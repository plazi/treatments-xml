<collectingCountry box="[986,1051,1333,1355]" name="Sudan" pageId="1" pageNumber="1392">Sudan</collectingCountry>
) and Palaearctic (
<collectingCountry box="[327,455,1363,1385]" name="Afghanistan" pageId="1" pageNumber="1392">Afghanistan</collectingCountry>
, 
<collectingCountry box="[470,533,1363,1384]" name="Egypt" pageId="1" pageNumber="1392">Egypt</collectingCountry>
, 
<collectingCountry box="[547,590,1363,1384]" name="Iran" pageId="1" pageNumber="1392">Iran</collectingCountry>
, 
<collectingCountry box="[605,693,1363,1385]" name="Pakistan" pageId="1" pageNumber="1392">Pakistan</collectingCountry>
