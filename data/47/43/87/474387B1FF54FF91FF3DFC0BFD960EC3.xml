<paragraph blockId="1.[151,1058,1333,1414]" pageId="1" pageNumber="1392">
<taxonomicName authorityName="Radoszkowski" authorityYear="1888" box="[151,381,1333,1355]" class="Insecta" family="Bradynobaenidae" genus="Macroocula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="1392" phylum="Arthropoda" rank="species" species="morawitzi">
