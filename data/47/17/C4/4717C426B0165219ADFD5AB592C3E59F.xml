<subSubSection pageId="15" pageNumber="16" type="remarks">
<paragraph pageId="15" pageNumber="16">Remarks.</paragraph>
<paragraph pageId="15" pageNumber="16">
<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Mammalia" family="Dasyproctidae" genus="Dasyprocta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dasyprocta leporina" order="Rodentia" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="leporina">
