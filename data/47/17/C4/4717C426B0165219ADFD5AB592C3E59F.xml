<taxonomicName class="Mammalia" genus="D." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="D. fuliginosa" order="Rodentia" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="fuliginosa">
