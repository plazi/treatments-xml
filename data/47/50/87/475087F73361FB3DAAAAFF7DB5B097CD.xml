<taxonomicName authorityName="Jordan" authorityYear="1906" box="[1113,1224,309,331]" class="Insecta" family="Anthribidae" genus="Stenocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="487" phylum="Arthropoda" rank="species" species="sigillatus">
