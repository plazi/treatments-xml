<subSubSection pageId="14" pageNumber="23" type="nomenclature">
<paragraph pageId="14" pageNumber="23">
<taxonomicName LSID="http://zoobank.org/0BB6C9B6-08D5-411A-BCC8-9818E0BCF4EC" class="Gastropoda" family="Pomatiopsidae" genus="Tricula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tricula davisi" order="Littorinimorpha" pageId="14" pageNumber="23" phylum="Mollusca" rank="species" species="davisi">Tricula davisi</taxonomicName>
