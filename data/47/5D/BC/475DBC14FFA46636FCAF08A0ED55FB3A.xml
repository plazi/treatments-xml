<subSubSection box="[788,1099,1261,1288]" pageId="9" pageNumber="14" type="nomenclature">
<paragraph blockId="9.[788,1099,1261,1288]" box="[788,1099,1261,1288]" pageId="9" pageNumber="14">
<heading box="[788,1099,1261,1288]" centered="true" fontSize="11" level="2" pageId="9" pageNumber="14" reason="2">
<taxonomicName authority="(Peters, 1852)" baseAuthorityName="Peters" baseAuthorityYear="1852" box="[788,1099,1261,1288]" class="Mammalia" family="Muridae" genus="Pelomys" kingdom="Animalia" order="Rodentia" pageId="9" pageNumber="14" phylum="Chordata" rank="species" species="fallax">
