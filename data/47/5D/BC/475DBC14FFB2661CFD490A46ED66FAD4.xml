<taxonomicName authority="Peter, 1852: 274" authorityName="Peters" authorityPageNumber="274" authorityYear="1852" box="[677,1012,1603,1627]" class="Mammalia" family="Sciuridae" genus="Sciurus" kingdom="Animalia" order="Rodentia" pageId="31" pageNumber="36" phylum="Chordata" rank="species" species="flavovittis">
