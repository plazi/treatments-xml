<emphasis box="[388,445,747,771]" italics="true" pageId="15" pageNumber="16">et al.</emphasis>
, 
<number box="[460,520,746,770]" pageId="15" pageNumber="16" value="2011.0">2011</number>
</bibRefCitation>
