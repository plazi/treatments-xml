<taxonomicName box="[708,780,710,734]" class="Insecta" family="Curculionidae" genus="Debus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
