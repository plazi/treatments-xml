<paragraph blockId="23.[151,741,151,177]" box="[151,741,151,177]" pageId="23" pageNumber="24">
<heading bold="true" box="[151,741,151,177]" fontSize="11" level="1" pageId="23" pageNumber="24" reason="1">
<emphasis bold="true" box="[151,741,151,177]" pageId="23" pageNumber="24">
Genus 
<taxonomicName authority="Hulcr and Cognato" authorityName="Hulcr and Cognato" box="[238,646,151,177]" class="Insecta" family="Curculionidae" genus="Truncaudum" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,396,151,177]" italics="true" pageId="23" pageNumber="24">Truncaudum</emphasis>
