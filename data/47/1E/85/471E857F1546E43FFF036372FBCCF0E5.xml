, 
<taxonomicName authority="(Karsch 1885)" authorityName="Karsch" authorityYear="1885" baseAuthorityName="Karsch" baseAuthorityYear="1885" box="[691,1071,1189,1214]" class="Arachnida" family="Rhagodidae" genus="Rhagodoca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="termes">
