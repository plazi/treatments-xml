<specimenCount box="[795,830,1185,1210]" pageId="34" pageNumber="35" type="male">
7 
<emphasis box="[809,830,1185,1210]" italics="true" pageId="34" pageNumber="35">♂</emphasis>
</specimenCount>
and 
<specimenCount box="[890,924,1186,1210]" pageId="34" pageNumber="35" type="female">3♀</specimenCount>
(
<typeStatus box="[942,1047,1185,1210]" pageId="34" pageNumber="35">lectotype</typeStatus>
(
<specimenCount box="[1065,1087,1186,1210]" pageId="34" pageNumber="35" type="male">♂</specimenCount>
) and paratypoide) at the 
<collectionCode box="[1380,1436,1186,1210]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="34" pageNumber="35">SMF</collectionCode>
