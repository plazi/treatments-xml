, 
<taxonomicName authority="Birula 1893" authorityName="Birula" authorityYear="1893" box="[682,1263,1722,1747]" class="Arachnida" family="Gylippidae" genus="Gylippus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="ferganensis" subGenus="Anoplogylippus">
