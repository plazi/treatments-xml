<subSubSection pageId="41" pageNumber="42" type="materials_examined">
<paragraph blockId="41.[151,1436,151,464]" box="[199,722,223,248]" pageId="41" pageNumber="42">
<emphasis bold="true" box="[199,373,223,248]" pageId="41" pageNumber="42">Type material.</emphasis>
