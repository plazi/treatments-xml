<taxonomicName authority="Kudo" authorityName="Kudo" authorityYear="1985" box="[151,394,1734,1760]" class="Insecta" family="Thripidae" genus="Yoshinothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="6" pageNumber="85" phylum="Arthropoda" rank="genus">
