<taxonomicName id="4C6B4D6DFB51B3056CD49A32FAF88D7B" box="[1245,1379,1372,1397]" class="Insecta" family="Coleophoridae" genus="Coleophora" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="52" phylum="Arthropoda" rank="genus">
<emphasis id="B91FEAFCFB51B3056CD49A32FAF88D7B" box="[1245,1379,1372,1397]" italics="true" pageId="5" pageNumber="52">Coleophora</emphasis>
</taxonomicName>
