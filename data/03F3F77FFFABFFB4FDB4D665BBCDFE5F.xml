<taxonomicName id="4C5A3DEAFFABFFB7FBEBD6B4BBB0F97F" baseAuthorityName="Horsfield" baseAuthorityYear="1855" box="[1130,1275,1733,1757]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="53" pageNumber="54" phylum="Chordata" rank="species" species="siligorensis">
