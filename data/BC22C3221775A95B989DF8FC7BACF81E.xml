<heading id="6F7CC5581775A95A989DF8FC7B50F8AD" bold="true" box="[151,787,1847,1873]" fontSize="11" level="1" pageId="68" pageNumber="69" reason="1">
<emphasis id="06FFAE261775A95A989DF8FC7B50F8AD" bold="true" box="[151,787,1847,1873]" pageId="68" pageNumber="69">
<taxonomicName id="F38B09B71775A95A989DF8FC7A35F8AD" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,630,1847,1873]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="tanung" status="new species">
<emphasis id="06FFAE261775A95A989DF8FC79D1F8AD" bold="true" box="[151,402,1847,1873]" italics="true" pageId="68" pageNumber="69">Hydropsyche tanung</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D1775A95A9A8EF8FC7B50F8AD" box="[644,787,1847,1873]" pageId="68" pageNumber="69" rank="species">new species</taxonomicNameLabel>
