</treatmentCitation>
[
<collectingRegion id="49B9F837FF8DFFBC6DBBFC0BFBDAFAA4" box="[974,1048,934,958]" country="India" name="Assam" pageId="16" pageNumber="261">Assam</collectingRegion>
, 
<collectingCountry id="F36A7645FF8DFFBC6A57FC0BFB9EFAA4" box="[1058,1116,934,958]" name="India" pageId="16" pageNumber="261">India</collectingCountry>
