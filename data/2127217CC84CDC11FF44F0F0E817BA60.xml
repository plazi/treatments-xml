<collectingDate id="CD744F42C84CDC11FAFEF48CED0DBB48" pageId="4" pageNumber="55" value="2018-07-19" valueMax="2018-08-08" valueMin="2018-07-19">19.VII. to 08.VIII.2018</collectingDate>
</date>
, 17 ind. from 
<date id="DD30B6AAC84CDC11FE19F770EEEBBB48" box="[458,714,1555,1581]" pageId="4" pageNumber="55" value="2018-08-29" valueMax="2018-09-19" valueMin="2018-08-29">
<collectingDate id="CD744F42C84CDC11FE19F770EEEBBB48" box="[458,714,1555,1581]" pageId="4" pageNumber="55" value="2018-08-29" valueMax="2018-09-19" valueMin="2018-08-29">29.VIII. to 19.IX.2018</collectingDate>
</date>
, and 10 ind. from 
<date id="DD30B6AAC84CDC11FC44F770E85BBB48" box="[919,1146,1555,1581]" pageId="4" pageNumber="55" value="2018-09-20" valueMax="2018-10-11" valueMin="2018-09-20">
<collectingDate id="CD744F42C84CDC11FC44F770E85BBB48" box="[919,1146,1555,1581]" pageId="4" pageNumber="55" value="2018-09-20" valueMax="2018-10-11" valueMin="2018-09-20">20.IX to 11.X. 2018</collectingDate>
</date>
, 
<collectionCode id="CF9F08AFC84CDC11FB56F770E8ECBB48" box="[1157,1229,1555,1581]" country="France" httpUri="http://grbio.org/cool/imw9-4q84" name="Institut National de la Recherche Agronomique - Orleans. Unite de Zoologie Forestiere" pageId="4" pageNumber="55">URZF</collectionCode>
<emphasis id="9BFA4C78C84CDC11FB07F770E8DCBB48" box="[1236,1277,1555,1581]" italics="true" pageId="4" pageNumber="55">leg.</emphasis>
; 
<emphasis id="9BFA4C78C84CDC11FADFF770E979BB48" box="[1292,1368,1555,1581]" italics="true" pageId="4" pageNumber="55">ibidem</emphasis>
, from 
<date id="DD30B6AAC84CDC11FF44F754ED50BB34" box="[151,369,1591,1617]" pageId="4" pageNumber="55" value="2019-05-21" valueMax="2019-10-22" valueMin="2019-05-21">
