<taxonomicName id="4C544D76716D092AFEFBFB69FD62BAD3" authority="Naumov" authorityName="Naumov" box="[313,684,1230,1251]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="16" pageNumber="2668" phylum="Cnidaria" rank="species" species="amphorophora">
