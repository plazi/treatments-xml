<taxonomicName id="4C593EF0C55D5E350A0F426E52C41174" authorityName=": Parenti" authorityYear="2009" box="[588,727,801,827]" class="Insecta" family="Elachistidae" genus="Svenssonia" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="adscitella">
