<taxonomicName id="C5E75111C98691E746375ACA204DB3A6" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Lamiaceae" genus="Scutellaria" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Lamiales" phylum="Tracheophyta" rank="species" species="galericulata">Scutellaria galericulata</taxonomicName>
