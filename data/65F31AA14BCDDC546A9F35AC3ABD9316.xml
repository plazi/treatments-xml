<taxonomicName id="168072A5DC45A2A26CE1FEC88BB6DCE7" class="Insecta" family="Eriococcidae" genus="Opisthoscelis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opisthoscelis spinosa" order="Hemiptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="spinosa">Opisthoscelis spinosa</taxonomicName>
