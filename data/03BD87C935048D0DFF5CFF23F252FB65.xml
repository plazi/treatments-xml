</emphasis>
<taxonomicName id="4C144D5C35048D0DFEF2FF64F2DFFCBC" authority="Lowry &amp; Stoddart, 1983" authorityName="Lowry &amp; Stoddart" authorityYear="1983" box="[313,898,862,887]" class="Malacostraca" family="Conicostomatidae" genus="Stomacontion" kingdom="Animalia" order="Amphipoda" pageId="3" pageNumber="46" phylum="Arthropoda" rank="species" species="pungapunga">
