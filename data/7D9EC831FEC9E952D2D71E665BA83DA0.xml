<subSubSection id="64C487624AB4A768BDA03747F159001C" pageId="null" pageNumber="201" type="nomenclature">
<paragraph id="FF89C500224C0A515326196446162C93" pageId="null" pageNumber="201">
<pageBreakToken id="9296E11BD213E2DFE32D038B42A72166" pageId="null" pageNumber="201" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="268CC96CCE9E416877C6735DE00E4B24" authority="L." class="Magnoliopsida" family="Plantaginaceae" genus="Veronica" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="201" phylum="Tracheophyta" rank="species" species="hederifolia">
Veronica 
<normalizedToken id="E53BD3F2D69B0C18726E4320D4651D05" originalValue="hederifólia" pageId="null" pageNumber="201">hederifolia</normalizedToken>
<authorityName id="195805259ED1C3D6E7FFFAFD48B3FA7C" pageId="null" pageNumber="201">L.</authorityName>
</taxonomicName>
</paragraph>
