<taxonomicName id="4C2A8184FFF7FFA3FF49E37B4E8DB998" authority="T. H. Nguyen &amp; Yakovlev" authorityName="T. H. Nguyen &amp; Yakovlev" box="[136,690,626,653]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="2" pageNumber="295" phylum="Tracheophyta" rank="species" species="caudatisepalus">
