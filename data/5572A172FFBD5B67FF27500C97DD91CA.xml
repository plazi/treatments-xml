<taxonomicName id="1ADB6BE7FFBD5B67FE9350BE94CA9072" baseAuthorityName="Herz" baseAuthorityYear="1904" box="[291,396,1520,1543]" class="Insecta" family="Noctuidae" genus="Cranionycta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="63" phylum="Arthropoda" rank="species" species="albonigra">
<emphasis id="EFAFCC76FFBD5B67FE9350BE94CA9072" box="[291,396,1520,1543]" italics="true" pageId="15" pageNumber="63">albonigra</emphasis>
</taxonomicName>
; 
<bibRefCitation id="B94A6D95FFBD5B67FE2950BE97029072" author="Hampson, G. F." box="[409,580,1520,1543]" pageId="15" pageNumber="63" pagination="222 - 239" refId="ref10411" refString="Hampson, G. F. (1909) Catalogue of the Lepidoptera Phalaenae in the British Museum. (Acronyctinae), 8, I - XV + 709 p, pl. 222 - 239. London." type="book chapter" year="1909">Hampson, 1909</bibRefCitation>
