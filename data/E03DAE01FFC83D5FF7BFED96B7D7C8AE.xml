<taxonomicName id="AF946494FFC93D50F670EE14B691C8C2" box="[344,431,1394,1416]" class="Insecta" family="Braconidae" genus="Aleiodes" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="106" phylum="Arthropoda" rank="genus">Aleiodes</taxonomicName>
(
<taxonomicName id="AF946494FFC93D50F694EE14B50CC8C2" box="[444,562,1394,1416]" genus="Athacryvac" pageId="23" pageNumber="106" rank="genus">Athacryvac</taxonomicName>
