<quantity id="4C8512EDFFD61F0EAF9A0C2DFDF76A12" box="[511,599,523,547]" metricMagnitude="0" metricUnit="m" metricValue="9.5" metricValueMax="12.0" metricValueMin="7.0" pageId="36" pageNumber="473" unit="m" value="9.5" valueMax="12.0" valueMin="7.0">
7– 
<specimenCount id="9D7B7481FFD61F0EAC790C2DFDF76A12" box="[540,599,523,547]" pageId="36" pageNumber="473" type="generic">12 m</specimenCount>
