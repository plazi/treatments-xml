<collectingRegion id="49C8F84C2A5979482D63DE41F28B2D0D" box="[1091,1183,1285,1308]" country="Panama" name="Panama" pageId="6" pageNumber="7">
<collectingCountry id="F31B763E2A5979482D63DE41F28B2D0D" box="[1091,1183,1285,1308]" name="Panama" pageId="6" pageNumber="7">Panama</collectingCountry>
