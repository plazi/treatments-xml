<taxonomicName id="AC834BA7FF99FFFDFABDF851C5EFF818" authorityName="Wesołowska &amp; Azarkina &amp; Russell-Smith" authorityYear="2014" box="[1336,1436,1954,1975]" class="Arachnida" family="Salticidae" genus="Rumburak" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
