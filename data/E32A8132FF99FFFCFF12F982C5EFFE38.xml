<taxonomicName id="AC834BA7FF99FFFDFAAFF8DEC5EFF8ED" authorityName="Wesołowska &amp; Azarkina &amp; Russell-Smith" authorityYear="2014" box="[1322,1436,1837,1858]" class="Arachnida" family="Salticidae" genus="Yimbulunga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
