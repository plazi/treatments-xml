<taxonomicName id="96854D45CF461D376AE1B9D5FACCFA81" authorityName="Paperna" authorityYear="1968" box="[1181,1338,1267,1294]" class="Monogenea" family="Gyrodactylidae" genus="Gyrodactylus" kingdom="Animalia" order="Gyrodactylidea" pageId="33" pageNumber="534" phylum="Platyhelminthes" rank="species" species="cichlidarum">
