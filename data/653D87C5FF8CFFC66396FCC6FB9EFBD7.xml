<taxonomicName id="2A944D50FF8CFFC663C6FC76FB68FC77" authorityName="ORR" baseAuthorityName="Reitter" baseAuthorityYear="1875" box="[983,1264,934,957]" class="Insecta" family="Nitidulidae" genus="Glischrochilus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="573" phylum="Arthropoda" rank="species" species="flavipennis">
