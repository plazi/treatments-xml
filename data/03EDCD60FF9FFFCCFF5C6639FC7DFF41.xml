<quantity id="4CBCD193FF9CFFCCFC2D60DFFBE8FF3A" box="[998,1062,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="2" pageNumber="277" unit="mm" value="9.0">9 mm</quantity>
