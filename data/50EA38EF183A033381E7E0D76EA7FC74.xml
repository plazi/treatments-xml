<subSubSection id="C2A1D4132DF6605F88DA55A5CB882A39" pageId="0" pageNumber="75" type="nomenclature">
<paragraph id="DE5B2F547130788109A222F977B1A594" pageId="0" pageNumber="75">
<taxonomicName id="742BD38C9BF3DB2829A043CDC165C1DE" authority="Caldwell, 1940" authorityName="Caldwell" authorityYear="1940" class="Insecta" family="Triozidae" genus="Swezeyana" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Swezeyana elongagena" order="Hemiptera" pageId="0" pageNumber="75" phylum="Arthropoda" rank="species" species="elongagena">Swezeyana elongagena Caldwell, 1940</taxonomicName>
