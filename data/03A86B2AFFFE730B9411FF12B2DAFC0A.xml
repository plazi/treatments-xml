, 
<collectingCountry id="F3169AACFFFE730B93E3FC8DB32DFCE9" box="[1247,1316,864,888]" name="Brazil" pageId="5" pageNumber="602">Brazil</collectingCountry>
, 
<collectingCountry id="F3169AACFFFE730B9209FC8DB38EFCE9" box="[1333,1415,864,888]" name="Bolivia" pageId="5" pageNumber="602">Bolivia</collectingCountry>
