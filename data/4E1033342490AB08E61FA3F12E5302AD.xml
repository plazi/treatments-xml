<taxonomicName id="490ABEACAD716B5D210A376EF802DB6B" authority="Blainville, 1824" authorityName="Blainville" authorityYear="1824" class="Gastropoda" family="Lymnaeidae" genus="Limnaea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Limnaea cornea" order="Hygrophila" pageId="9" pageNumber="140" phylum="Mollusca" rank="species" species="cornea">Limnaea cornea</taxonomicName>
<normalizedToken id="849B77D81E54F4C3F7FA80558B323013" originalValue="“Ziegler”">&quot;Ziegler&quot;</normalizedToken>
. 56 - 
