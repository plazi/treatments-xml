<quantity id="AD179B47FFBAFFF585B259E4FDFEF977" box="[426,516,1706,1732]" metricMagnitude="5" metricUnit="m" metricValue="2.0" pageId="27" pageNumber="28" unit="km" value="200.0">200 km</quantity>
