<quantity id="AD179B47FFBAFFF5879F599CFC34F95F" box="[903,974,1746,1772]" metricMagnitude="4" metricUnit="m" metricValue="6.437376" pageId="27" pageNumber="28" unit="mi" value="40.0">40 mi</quantity>
