<quantity id="AD179B47FFBAFFF58736599CFC82F95F" box="[814,888,1746,1772]" metricMagnitude="4" metricUnit="m" metricValue="6.4" pageId="27" pageNumber="28" unit="km" value="64.0">64 km</quantity>
