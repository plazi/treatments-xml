<taxonomicName id="438A4D63FFAEFF85FDE980B3FC85FD51" authority="Low, 1879" authorityName="Low" authorityYear="1879" box="[618,874,731,758]" class="Insecta" family="Liviidae" genus="Psyllopsis" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="151" phylum="Arthropoda" rank="genus">
