<taxonomicName id="4C0263CEFFAFE72777E6FF52FE6A74AC" authority="Meinert, 1872" authorityName="Meinert" authorityYear="1872" class="Chilopoda" family="Lithobiidae" genus="Bothropolys" kingdom="Animalia" order="Lithobiomorpha" pageId="29" pageNumber="180" phylum="Arthropoda" rank="species" species="rugosus">
