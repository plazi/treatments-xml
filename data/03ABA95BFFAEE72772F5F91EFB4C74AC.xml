<collectingCountry id="F31558DDFFAEE7267397F817FDC47395" box="[501,568,2002,2028]" name="Japan" pageId="28" pageNumber="179">Japan</collectingCountry>
, 
<collectingCountry id="F31558DDFFAEE7267026F817FD297395" box="[580,725,2002,2028]" name="North Korea" pageId="28" pageNumber="179">North Korea</collectingCountry>
, the 
<collectingCountry id="F31558DDFFAEE726716EF817FC717395" box="[780,909,2002,2028]" name="Philippines" pageId="28" pageNumber="179">Philippines</collectingCountry>
, New 
<collectingCountry id="F31558DDFFAEE72671B5F817FBD77395" box="[983,1067,2002,2028]" name="Guinea-Bissau" pageId="28" pageNumber="179">Guinea</collectingCountry>
