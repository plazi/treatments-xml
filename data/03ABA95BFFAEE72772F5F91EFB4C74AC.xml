</taxonomicName>
from 
<collectingCountry id="F31558DDFFAFE72773B8FF7EFDE174AC" box="[474,541,187,213]" name="Japan" pageId="29" pageNumber="180">Japan</collectingCountry>
, 
<collectingCountry id="F31558DDFFAFE727704AFF7EFD9274AC" box="[552,622,187,213]" name="China" pageId="29" pageNumber="180">China</collectingCountry>
