<collectingCountry id="0CF103D7FFB1FFCEFEBF38CDFE68F6C9" box="[352,416,442,466]" name="China" pageId="2" pageNumber="395">China</collectingCountry>
(
<collectingRegion id="B6228DA5FFB1FFCEFE7238CDFDE6F6C9" box="[429,558,442,466]" country="China" name="Guangdong" pageId="2" pageNumber="395">Guangdong</collectingRegion>
