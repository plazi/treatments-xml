<taxonomicName id="4C114D5BB460FFFCC59D72A1E23DF97D" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[235,326,1742,1763]" class="Arachnida" family="Lamponidae" genus="Lampona" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="walsh" status="new species">
