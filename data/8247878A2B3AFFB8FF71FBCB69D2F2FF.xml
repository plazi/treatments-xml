Synonyms: 
<taxonomicName id="CDEE4D1F2B3AFFB8FED6FB5668F2F3EA" authority="Plotnikow, 1906" authorityName="Plotnikow" authorityYear="1906" box="[304,753,1194,1217]" class="Rhabditophora" family="Typhloplanidae" genus="Mesostoma" kingdom="Animalia" order="Rhabdocoela" pageId="11" pageNumber="12" phylum="Platyhelminthes" rank="species" species="bologoviense">
