<geoCoordinate id="EE235007FFBEFF9A99AEF918F8763700" box="[467,567,1770,1797]" degrees="59" direction="north" minutes="33" orientation="latitude" pageId="47" pageNumber="48" precision="925" value="59.55">59°33’N</geoCoordinate>
, 
<geoCoordinate id="EE235007FFBEFF9A9A3FF918F8F63700" box="[578,695,1770,1797]" degrees="144" direction="west" minutes="15" orientation="longitude" pageId="47" pageNumber="48" precision="925" value="-144.25">144°15’W</geoCoordinate>
) (
<bibRefCitation id="EF864B31FFBEFF9A9AB3F919F9933700" author="Maslenikov, K. P. &amp; Orr, J. W. &amp; Stevenson, D. W." box="[718,978,1771,1797]" pageId="47" pageNumber="48" pagination="1 - 21" refId="ref299182" refString="Maslenikov, K. P., Orr, J. W. &amp; Stevenson, D. W. (2013) Range extensions and significant distributional records for eighty-two species of fishes in Alaskan marine waters. Northwestern Naturalist, 94, 1 - 21. https: // doi. org / 10.1898 / 12 - 23.1" type="journal article" year="2013">
