<taxonomicName id="D7F64D08DF1C725DC386FBECFD86736F" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[460,614,1125,1148]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="species" species="eos">
