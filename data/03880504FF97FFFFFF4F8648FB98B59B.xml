<typeStatus id="549A0AB0FF95FFFFFD8C8598FD30B50A" box="[596,644,1269,1293]" pageId="6" pageNumber="299">type</typeStatus>
locality, 
<collectingRegion id="49E57AF0FF95FFFFFD2E8599FC0DB50A" box="[758,953,1268,1293]" country="China" name="Hainan" pageId="6" pageNumber="299">Hainan Province</collectingRegion>
