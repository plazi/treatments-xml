<emphasis id="B9556800FF96FFFCFF4F86CAFEA6B7BB" bold="true" box="[151,274,1959,1981]" pageId="5" pageNumber="298">FIGURE 4.</emphasis>
<taxonomicName id="4C21CF91FF96FFFCFEC086CAFDAAB7BA" box="[280,542,1959,1981]" class="Insecta" family="Nemouridae" genus="Sphaeronemoura" kingdom="Animalia" order="Plecoptera" pageId="5" pageNumber="298" phylum="Arthropoda" rank="species" species="hainana" status="sp. n.">
<emphasis id="B9556800FF96FFFCFEC086CAFDAAB7BA" box="[280,542,1959,1981]" italics="true" pageId="5" pageNumber="298">
Sphaeronemoura 
<taxonomicName id="4C21CF91FF96FFFCFE1386CAFDAAB7BA" box="[459,542,1959,1981]" pageId="5" pageNumber="298" rank="species" species="hainana" status="sp. n.">hainana</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A266D57BFF96FFFCFDFC86CAFDE8B7BB" box="[548,604,1959,1980]" pageId="5" pageNumber="298" rank="species">
<emphasis id="B9556800FF96FFFCFDFC86CAFDE8B7BB" bold="true" box="[548,604,1959,1980]" pageId="5" pageNumber="298">sp. n.</emphasis>
