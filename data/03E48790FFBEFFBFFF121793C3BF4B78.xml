<collectionCode id="ED5CAE43FFBEFFBEFEEC15FEC6D14BEE" box="[361,446,1698,1722]" country="Belgium" httpUri="http://grbio.org/cool/c7r2-61q8" name="Institut Royal des Sciences Naturelles de Belgique" pageId="60" pageNumber="61">IRSNB</collectionCode>
, 
<collectionCode id="ED5CAE43FFBEFFBEFE4C15FEC5494BEE" box="[457,550,1698,1722]" pageId="60" pageNumber="61">WAMA</collectionCode>
