<number id="A93A24F0B15FFFFAFDE1B145FDC94F69" box="[574,585,1381,1407]" pageId="1" pageNumber="40" value="1.0">1</number>
</figureCitation>
A, 
<number id="A93A24F0B15FFFFAFDAEB145FDFC4F69" box="[625,636,1381,1407]" pageId="1" pageNumber="40" value="1.0">1</number>
C, 
<number id="A93A24F0B15FFFFAFD7DB145FD314F69" box="[674,689,1381,1407]" pageId="1" pageNumber="40" value="4.0">4</number>
A, 
<number id="A93A24F0B15FFFFAFD0BB145FD634F69" box="[724,739,1381,1407]" pageId="1" pageNumber="40" value="4.0">4</number>
C)....................................................................................... 
<number id="A93A24F0B15FFFFAFAA7B145FA154F69" box="[1400,1429,1381,1407]" pageId="1" pageNumber="40" value="2.0">2</number>
</keyLead>
