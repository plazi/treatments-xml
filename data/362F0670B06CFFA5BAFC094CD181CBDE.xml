<taxonomicName id="7986CCE5B06CFFA3BB3B0878D72ACB60" box="[336,606,1307,1332]" class="Actinopterygii" family="Gobiidae" genus="Elacatinus" kingdom="Animalia" order="Perciformes" pageId="3" pageNumber="48" phylum="Chordata" rank="species" species="redimiculus">
