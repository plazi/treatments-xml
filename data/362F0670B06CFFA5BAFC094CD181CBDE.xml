<taxonomicName id="7986CCE5B06AFFA5BB040E0BD708CDD5" box="[367,636,872,897]" class="Actinopterygii" family="Gobiidae" genus="Elacatinus" kingdom="Animalia" order="Perciformes" pageId="5" pageNumber="50" phylum="Chordata" rank="species" species="redimiculus">
