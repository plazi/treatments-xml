<taxonomicName id="6A8214C0167CFFDAD2EDF917FE4DDAC2" box="[255,497,1685,1711]" class="Entognatha" family="Acerentomidae" genus="Gracilentulus" kingdom="Animalia" order="Protura" pageId="21" pageNumber="292" phylum="Arthropoda" rank="species" species="undetermined">
