<quantity id="4CDB35C42C67FF8BFE4DB329FDF1FE9A" box="[465,595,283,307]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" metricValueMax="3.6" metricValueMin="2.8" pageId="38" pageNumber="665" unit="mm" value="3.2" valueMax="3.6" valueMin="2.8">2.8–3.6 mm</quantity>
