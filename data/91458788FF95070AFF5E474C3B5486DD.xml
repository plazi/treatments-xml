<subSubSection id="51F66515FF95070AFF6B44233B5486DD" pageId="17" pageNumber="182" type="etymology">
<paragraph id="1953369EFF95070AFF6B44233B5486DD" blockId="17.[136,1459,1356,1707]" pageId="17" pageNumber="182">
<emphasis id="2B98EA8CFF95070AFF6B4423391B86F1" bold="true" box="[189,328,1644,1670]" pageId="17" pageNumber="182">Etymology:</emphasis>
