<caption id="DF386602FFE93D028EE2F886B0728585" httpUri="https://zenodo.org/record/281866/files/figure.png" pageId="2" pageNumber="32" targetBox="[151,1435,510,1794]" targetPageId="2">
<paragraph id="8BF8368AFFE93D028EE2F886B0728585" blockId="2.[151,1437,1816,2023]" pageId="2" pageNumber="32">
