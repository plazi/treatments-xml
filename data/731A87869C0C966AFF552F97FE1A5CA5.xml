<subSubSection id="B3A9651B9C0D966AFF05290FFE1A5CA5" pageId="11" pageNumber="281" type="etymology">
<paragraph id="FB0C36909C0D966AFF05290FFE1A5CA5" blockId="11.[151,1437,151,680]" pageId="11" pageNumber="281">
<emphasis id="C9C7EA829C0D966AFF05290FFEE05C88" bold="true" box="[199,334,620,645]" pageId="11" pageNumber="281">Etymology.</emphasis>
