<taxonomicName id="2EAF2399DF6702501219C176390E3E6D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:247370" class="Insecta" family="Formicidae" genus="Mycetophylax" genus.bestMatchDistance="1163" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Mycetophylax morschi (Emery)" order="Hymenoptera" pageNumber="15" phylum="Arthropoda" rank="species" species="morschi">M. morschi</taxonomicName>
and 
<taxonomicName id="CAE71F1934BF63E0211313D07E64DB19" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31774" class="Insecta" family="Formicidae" genus="Mycetophylax" genus.bestMatchDistance="1200" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Mycetophylax simplex (Emery)" order="Hymenoptera" pageNumber="15" phylum="Arthropoda" rank="species" species="simplex">M. simplex</taxonomicName>
at the isle of Florianopolis b) 
<taxonomicName id="BB6B8116556FBFA1BCB3891E7492A872" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:247369" authority="Klingenberg &amp; Brandão, 2009" authorityName="Klingenberg &amp; Brandão" authorityYear="2009" class="Insecta" family="Formicidae" genus="Kalathomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Kalathomyrmex Klingenberg &amp; Brandão" order="Hymenoptera" pageNumber="15" phylum="Arthropoda" rank="genus">Kalathomyrmex</taxonomicName>
and c) 
<taxonomicName id="3D9ACD3EA98C2AE5284D6878F353871C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147170" authority="Kusnezov" authorityName="Kusnezov" class="Hexapoda" family="Formicidae" genus="Paramycetophylax" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Paramycetophylax Kusnezov" order="Hymenoptera" pageNumber="15" phylum="Arthropoda" rank="genus">Paramycetophylax</taxonomicName>
