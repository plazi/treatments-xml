<taxonomicName id="8DB24D26FFB4143F5AF0DDDA4A29695B" authority="Imajima, 1972" authorityName="Imajima" authorityYear="1972" class="Polychaeta" family="Nereididae" genus="Leonnates" kingdom="Animalia" order="Phyllodocida" pageId="3" pageNumber="368" phylum="Annelida" rank="species" species="nipponicus">
