<taxonomicName id="CE8F4D68D7253944FD5FF8DCFC8A9F47" box="[706,841,1836,1860]" class="Insecta" family="Pseudococcidae" genus="Polystomophora" kingdom="Animalia" order="Hemiptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="arakensis">
