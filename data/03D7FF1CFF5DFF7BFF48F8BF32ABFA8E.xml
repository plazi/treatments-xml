<taxonomicName id="4C7E3589FF5DFF7BFF48FE0830CBFA8E" baseAuthorityName="Gory" baseAuthorityYear="1834" box="[192,439,1382,1406]" class="Insecta" family="Rutelidae" genus="Pelidnota" kingdom="Animalia" order="Coleoptera" pageId="150" pageNumber="149" phylum="Arthropoda" rank="species" species="granulata">
