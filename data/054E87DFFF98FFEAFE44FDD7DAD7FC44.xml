<caption id="D9986641FF9AFFF4FD43FC21D9DCFC0A" ID-DOI="http://doi.org/10.5281/zenodo.5341755" ID-Zenodo-Dep="5341755" httpUri="https://zenodo.org/record/5341755/files/figure.png" pageId="15" pageNumber="606" startId="15.[711,742,941,960]" targetBox="[113,687,218,984]" targetPageId="15">
