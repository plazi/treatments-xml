<taxonomicName id="4C134D63FFCDFFA8FC26FC1BFBF2FC0F" box="[951,1064,963,987]" class="Insecta" family="Curculionidae" genus="Synorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="80" phylum="Arthropoda" rank="species" species="indicus">
