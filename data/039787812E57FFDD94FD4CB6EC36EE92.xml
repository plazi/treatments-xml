<taxonomicName id="4C3E4D142E57FFDB94FD4CB6EBFFE9AD" authority="Bates, 1891" authorityName="Bates" authorityYear="1891" box="[151,728,1693,1719]" class="Insecta" family="Carabidae" genus="Harpalus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="indicus" subGenus="Pseudoophonus">
<emphasis id="B94AEA852E57FFDB94FD4CB6EBFFE9AD" bold="true" box="[151,728,1693,1719]" pageId="26" pageNumber="27">
<emphasis id="B94AEA852E57FFDB94FD4CB6E82EE9AD" bold="true" box="[151,265,1693,1719]" italics="true" pageId="26" pageNumber="27">Harpalus</emphasis>
(
<emphasis id="B94AEA852E57FFDB95704CB6E8FDE9AD" bold="true" box="[282,474,1693,1719]" italics="true" pageId="26" pageNumber="27">Pseudoophonus</emphasis>
