<materialsCitation id="08C6546DBD29C937BB238F9CC1AFFCCB" collectingDate="1998-09-07" collectionCode="CWOB" collectorName="J. E. Eger &amp; Cyclanthaceae'" country="Ecuador" county="Rio Blanco Ecol." elevation="518" location="Misahuali" municipality="Reserve" pageId="16" pageNumber="575" specimenCount="1" stateProvince="Napo" typeStatus="holotype">
<typeStatus id="6715E092BD29C937BB238F9CC2FEFD6F" box="[318,418,721,743]" pageId="16" pageNumber="575">Holotype</typeStatus>
male (dissected) ‘ 
