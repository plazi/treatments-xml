<subSubSection id="C30E653FC664A216FC1EFD5DFB57401E" pageId="18" pageNumber="245" type="discussion">
<paragraph id="8BAB36B4C664A216FC1EFD5DFB57401E" blockId="18.[996,1797,605,1174]" pageId="18" pageNumber="245">
<emphasis id="B960EAA6C664A216FC1EFD5DFAD041A8" box="[998,1343,711,750]" italics="true" pageId="18" pageNumber="245">
<taxonomicName id="4C144D37C664A216FC1EFD5DFB2E41A8" authorityName="ZyfetThomas" authorityYear="1913" box="[998,1217,711,750]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="245" phylum="Chordata" rank="genus">Hipposideros</taxonomicName>
