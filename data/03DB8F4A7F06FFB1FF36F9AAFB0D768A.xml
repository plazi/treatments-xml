<quantity id="4C8A93B97F06FFB3FCCAF84BFC2973BB" box="[875,983,1946,1974]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="20" pageNumber="145" unit="mm" value="7.5" valueMax="10.0" valueMin="5.0">5‒10 mm</quantity>
