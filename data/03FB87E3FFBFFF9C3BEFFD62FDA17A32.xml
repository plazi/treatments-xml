<taxonomicName id="4C524D76FFBFFF9C3B86FB49FE257977" box="[254,462,1267,1291]" class="Insecta" family="Pipunculidae" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="family">PIPUNCULIDAE</taxonomicName>
