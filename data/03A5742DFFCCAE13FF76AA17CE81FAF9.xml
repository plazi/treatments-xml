<subSubSection id="C31696B0FFCCAE13FF26AD41CE4EFBFC" box="[199,830,1044,1069]" pageId="25" pageNumber="226" type="diagnosis">
<paragraph id="8BB3C53BFFCCAE13FF26AD41CE4EFBFC" blockId="25.[151,1437,972,1320]" box="[199,830,1044,1069]" pageId="25" pageNumber="226">
<emphasis id="B9781929FFCCAE13FF26AD41CC31FBFC" bold="true" box="[199,321,1044,1069]" pageId="25" pageNumber="226">Diagnosis.</emphasis>
