<taxonomicName id="2EBD9282244EFFE5FEB2049EFD128493" authority="(REEVE, 1845)" authorityName="Herrmann &amp; Stossier &amp; Salisbury" authorityYear="2014" baseAuthorityName="REEVE" baseAuthorityYear="1845" box="[293,737,1243,1261]" class="Gastropoda" family="Costellariidae" genus="Vexillum" kingdom="Animalia" order="Neogastropoda" pageId="14" pageNumber="15" phylum="Mollusca" rank="species" species="corallinum" status="comb. nov." subGenus="Protoelongata">
