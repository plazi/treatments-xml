<taxonomicName id="0EE118870447FFC4FEE1FB98741C1458" authorityName="$ Lyman" authorityYear="1879" box="[325,526,1109,1135]" class="Ophiuroidea" family="Ophiactidae" genus="Ophiactis" kingdom="Animalia" order="Ophiurida" pageId="23" pageNumber="24" phylum="Echinodermata" rank="species" species="canotia">
