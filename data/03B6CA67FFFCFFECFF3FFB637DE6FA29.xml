PAL 641993, 
<taxonomicName id="4C1F00F2FFFCFFECFE57FAAB7D84FABD" box="[511,707,1341,1367]" pageId="11" pageNumber="122" rank="subSpecies" subSpecies="tarsometatarsus">r tarsometatarsus</taxonomicName>
