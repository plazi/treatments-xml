<taxonomicName id="4C1F00F2FFFCFFECFB98FA137BADFA75" baseAuthorityName="Latham" baseAuthorityYear="1790" box="[1072,1258,1413,1439]" class="Aves" family="Tyrannidae" genus="Sayornis" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="122" phylum="Chordata" rank="species" species="phoebe">
<emphasis id="B96BA763FFFCFFECFB98FA137BADFA75" box="[1072,1258,1413,1439]" italics="true" pageId="11" pageNumber="122">Sayornis phoebe</emphasis>
</taxonomicName>
