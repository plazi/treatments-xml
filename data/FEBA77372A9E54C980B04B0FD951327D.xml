<geoCoordinate id="13F3FAD4643EA71710DE4A4C05436938" degrees="01" direction="north" minutes="13.32" orientation="latitude" precision="9" value="1.222">01°13.32'N</geoCoordinate>
; 
<geoCoordinate id="C0AF75BFE375738A745294A7FB409DDC" degrees="113" direction="east" minutes="06.2433" orientation="longitude" precision="1" value="113.10406">113°06.2433'E</geoCoordinate>
