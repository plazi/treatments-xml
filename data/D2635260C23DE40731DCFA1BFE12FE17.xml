<quantity id="9D324E93C23DE40E3303F9F7FD61F9A9" box="[584,687,1582,1609]" metricMagnitude="-5" metricUnit="m" metricValue="1.0" pageId="23" pageNumber="324" unit="mm" value="0.01">0.01 mm</quantity>
; CL = 0.79 ± 
<quantity id="9D324E93C23DE40E3201F9F7FC6DF9A9" box="[842,931,1582,1609]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="23" pageNumber="324" unit="mm" value="0.2">0.2 mm</quantity>
