<number id="3DBE2471FFD0FFE7FCC3FAE3FC9EFA1E" box="[776,895,1399,1425]" pageId="9" pageNumber="54" value="0.9550000000000001" valueMax="1.03" valueMin="0.88">0.88–1.03</number>
