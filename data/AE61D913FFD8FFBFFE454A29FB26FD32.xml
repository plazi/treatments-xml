<subSubSection id="6ED23B8EFFD8FFBFFF1049D0FB26FD32" pageId="121" pageNumber="122" type="biology_ecology">
<paragraph id="26776805FFD8FFBFFF1049D0FE90FD43" blockId="121.[189,285,642,668]" box="[189,285,642,668]" pageId="121" pageNumber="122">
<heading id="7D3FDF69FFD8FFBFFF1049D0FE90FD43" bold="true" box="[189,285,642,668]" fontSize="11" level="3" pageId="121" pageNumber="122" reason="3">
