<taxonomicName id="6B3608F84A5057C70146D54A557A7B76" class="Arachnida" family="Linyphiidae" genus="Callosa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Callosa ciliata" order="Araneae" pageId="0" pageNumber="109" phylum="Arthropoda" rank="species" species="ciliata">Callosa ciliata</taxonomicName>
