<heading id="D0942F0AE264FF8AFF65FB56FD8BFB70" bold="true" box="[151,534,1159,1185]" fontSize="11" level="1" pageId="4" pageNumber="583" reason="1">
<emphasis id="B9174474E264FF8AFF65FB56FD8BFB70" bold="true" box="[151,534,1159,1185]" pageId="4" pageNumber="583">
Genus 
<taxonomicName id="4C63E3E5E264FF8AFF1CFB56FD8BFB70" authority="Vella, 1954" authorityName="Vella" authorityYear="1954" box="[238,534,1159,1185]" class="Gastropoda" family="Raphitomidae" genus="Mioawateria" kingdom="Animalia" order="Neogastropoda" pageId="4" pageNumber="583" phylum="Mollusca" rank="genus">
<emphasis id="B9174474E264FF8AFF1CFB56FE1AFB70" bold="true" box="[238,391,1159,1185]" italics="true" pageId="4" pageNumber="583">Mioawateria</emphasis>
Vella, 
<number id="3DAB8A41E264FF8AFE29FB56FD8BFB70" box="[475,534,1159,1185]" pageId="4" pageNumber="583" value="1954.0">1954</number>
</taxonomicName>
