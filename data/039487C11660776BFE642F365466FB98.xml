<paragraph id="8B8236D716607754FF352AA2550AF865" blockId="30.[189,1399,1929,2027]" box="[234,1051,1929,1956]" pageId="30" pageNumber="31">
<emphasis id="B949EAC516607754FF352AA2503BF862" bold="true" box="[234,298,1929,1955]" pageId="30" pageNumber="31">Male</emphasis>
(from 
<collectingCountry id="F32A764716607754FEA42AA250F9F862" box="[379,488,1929,1955]" name="Tanzania" pageId="30" pageNumber="31">Tanzania</collectingCountry>
