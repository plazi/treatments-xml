<taxonomicName id="4C4C77AAFF93FFB5FF3CFB775A38F44B" box="[151,373,1268,1294]" class="Insecta" family="Dascillidae" genus="Notodascillus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="254" phylum="Arthropoda" rank="species" species="iviei" status="sp. n.">
<emphasis id="B938D03BFF93FFB5FF3CFB775A38F44B" bold="true" box="[151,373,1268,1294]" italics="true" pageId="9" pageNumber="254">Notodascillus iviei</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20B6D40FF93FFB5FED7FB705AF3F448" box="[380,446,1267,1293]" pageId="9" pageNumber="254" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
