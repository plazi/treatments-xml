<quantity id="4CFE9B6E19B5FF726B5CA74571D6F8A8" box="[362,506,1826,1852]" metricMagnitude="-3" metricUnit="m" metricValue="6.05" metricValueMax="6.2" metricValueMin="5.9" pageId="149" pageNumber="150" unit="mm" value="6.05" valueMax="6.2" valueMin="5.9">5.9–6.2 mm</quantity>
