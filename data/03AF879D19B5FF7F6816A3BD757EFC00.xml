<taxonomicName id="4C064D0819B5FF7269AEA4F37472FB39" authority="Steenis &amp; Hippa &amp; Mutin, 2018" authorityName="Steenis &amp; Hippa &amp; Mutin" authorityYear="2018" box="[920,1118,1171,1198]" class="Insecta" family="Syrphidae" genus="Sphegina" kingdom="Animalia" order="Diptera" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="brevipilis" status="sp. nov." subGenus="Asiosphegina">
