<taxonomicName id="4C064D0819B5FF726E82A4DF755BFB44" authority="Steenis &amp; Hippa &amp; Mutin, 2018" authorityName="Steenis &amp; Hippa &amp; Mutin" authorityYear="2018" box="[1204,1399,1206,1234]" class="Insecta" family="Syrphidae" genus="Sphegina" kingdom="Animalia" order="Diptera" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="brevipilis" status="sp. nov." subGenus="Asiosphegina">
