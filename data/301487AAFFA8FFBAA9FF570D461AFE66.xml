<number id="0E75249BFFA9FFBAADED525241DEFEB6" box="[1157,1267,290,313]" pageId="1" pageNumber="67" value="0.875" valueMax="0.89" valueMin="0.86">0.86–0.89</number>
; IV, 
<number id="0E75249BFFA9FFBAAC42525240B5FEB6" box="[1322,1432,290,313]" pageId="1" pageNumber="67" value="0.615" valueMax="0.63" valueMin="0.6">0.60–0.63</number>
; lacking pilose setae. 
