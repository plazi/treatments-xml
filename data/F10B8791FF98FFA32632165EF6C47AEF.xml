<paragraph id="791D3687FF98FFBD251116D1F3EC7E1C" blockId="15.[313,1268,1132,1300]" pageId="15" pageNumber="2737">
<taxonomicName id="BEA24D04FF98FFBD251116D1F10E7FF0" authority=": Stock, 1958: 114" authorityName=": Stock" authorityPageNumber="114" authorityYear="1958" box="[313,830,1250,1273]" class="Pycnogonida" family="Colossendeidae" genus="Rhopalorhynchus" kingdom="Animalia" order="Pantopoda" pageId="15" pageNumber="2737" phylum="Arthropoda" rank="species" species="tenuissimum">
