<materialsCitation id="0792FF6BFFD74A070CCDFAEEFED4011B" collectionCode="NHMW" collectorName="Another &amp; Hugel &amp; Box A" country="Sri Lanka" county="Carl Hugel in Kashmir" location="The" municipality="There" pageId="66" pageNumber="511" specimenCount="3" specimenCount-female="2" typeStatus="syntype">
