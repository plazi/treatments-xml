<bibRefCitation id="EC55E0E23878AE00FBA56F92FB09FC06" author="Fukui, T." box="[1105,1262,938,964]" pageId="19" pageNumber="20" pagination="332 - 333" refId="ref23511" refString="Fukui, T. (1933) Teloporia (Tremat.) = Opisthoporus. Zoologischer Anzeiger, 103, 332 - 333." type="journal article" year="1933">
