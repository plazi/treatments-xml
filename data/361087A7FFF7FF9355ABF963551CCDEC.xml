<taxonomicName id="79B94D32FFF7FF9255ABF88F518BCA57" baseAuthorityName="Ridley &amp; Dendy" baseAuthorityYear="1886" box="[151,481,1834,1858]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="50" pageNumber="51" phylum="Porifera" rank="species" species="anomala" subGenus="Arenochalina">
