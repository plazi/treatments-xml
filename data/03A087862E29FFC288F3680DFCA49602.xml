<emphasis id="B97DEA822E29FFC288F36E8CFEAF95AD" box="[136,331,1484,1505]" italics="true" pageId="1" pageNumber="284">Limonium confusum</emphasis>
subsp. 
<emphasis id="B97DEA822E29FFC289EF6E8CFDEB95AD" box="[404,527,1484,1505]" italics="true" pageId="1" pageNumber="284">densissimum</emphasis>
<bibRefCitation id="EF984B612E29FFC28A6E6E8CFD3295AE" author="Pignatti, S." box="[533,726,1484,1506]" pageId="1" pageNumber="284" pagination="377 - 383" refId="ref4102" refString="Pignatti, S. (1953) Su alcune Plumbaginaceae interessanti raccolte alla foce dell'Ebro. Collectanea Botanica (Barcelona) 3 (3): 377 - 383." type="journal article" year="1953">Pignatti (1953: 379)</bibRefCitation>
</taxonomicName>
</treatmentCitation>
≡ 
<treatmentCitation id="0AA810812E29FFC28A8B6E8CFAEC95AE" author="Pignatti, S." box="[752,1288,1483,1506]" page=")" pageId="1" pageNumber="284" year="1971">
<taxonomicName id="4C094D132E29FFC28A8B6E8CFAEC95AE" authority="(Pignatti) Pignatti (1971: 367)" authorityName="Pignatti" authorityPageNumber="367" authorityYear="1971" baseAuthorityName="Pignatti" box="[752,1288,1483,1506]" class="Magnoliopsida" family="Plumbaginaceae" genus="Limonium" kingdom="Plantae" order="Caryophyllales" pageId="1" pageNumber="284" phylum="Tracheophyta" rank="species" species="densissimum">
