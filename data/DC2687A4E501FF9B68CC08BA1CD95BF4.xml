<number id="E2472495E502FF9A6C420B6B19325C45" box="[1049,1061,1581,1605]" pageId="62" pageNumber="63" value="8.0">8</number>
(
<number id="E2472495E502FF9A6C630B6B195E5C45" box="[1080,1097,1581,1605]" pageId="62" pageNumber="63" value="2.0">2</number>
