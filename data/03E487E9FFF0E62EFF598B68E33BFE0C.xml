<emphasis id="B939EAEDFFF0E62BFF598BA2E104F912" bold="true" box="[151,293,1720,1742]" pageId="3" pageNumber="504">
<typeStatus id="54F6885DFFF0E62BFF598BA2E0EDF912" box="[151,204,1720,1742]" pageId="3" pageNumber="504">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C4D4D7CFFF0E62BFEE58BA2E23BF912" box="[299,538,1720,1742]" class="Arachnida" family="Salticidae" genus="Ancepitilobus" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="504" phylum="Arthropoda" rank="species" species="howensis" status="sp. nov.">
<emphasis id="B939EAEDFFF0E62BFEE58BA2E23BF912" box="[299,538,1720,1742]" italics="true" pageId="3" pageNumber="504">Ancepitilobus howensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20A5796FFF0E62BFDEE8BA2E251F912" box="[544,624,1720,1742]" pageId="3" pageNumber="504" rank="species">
