<collectingCountry id="E0CD87F3FFD98F5EB977279B61E3AF41" box="[1104,1205,1256,1281]" name="United Kingdom" pageId="1" pageNumber="102">Scotland</collectingCountry>
, Edinburgh district, Corstorphine, 
<date id="EC64E1A3FFD98F5EBC10267864DEAF64" box="[311,392,1291,1316]" pageId="1" pageNumber="102" value="1935-02">
ii. 
<number id="2E12D544FFD98F5EBC6B267864DEAF64" box="[332,392,1291,1316]" pageId="1" pageNumber="102" value="1935.0">1935</number>
</date>
