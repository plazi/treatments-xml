<typeStatus id="476179C1FFD98F5EBDB0279B6584AF41" box="[151,210,1256,1281]" pageId="1" pageNumber="102">Type</typeStatus>
material
</emphasis>
. Five 
<typeStatus id="476179C1FFD98F5EBCB2279A6756AF41" box="[405,512,1257,1281]" pageId="1" pageNumber="102">paratypes</typeStatus>
(females mounted on two slides); 
<collectingCountry id="E0CD87F3FFD98F5EBE83279B6116AF41" box="[932,1088,1256,1281]" name="United Kingdom" pageId="1" pageNumber="102">Great Britain</collectingCountry>
