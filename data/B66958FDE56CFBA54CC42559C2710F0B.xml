<taxonomicName id="640897DE319919DE5ABD85E3F441AF0E" authorityName="Gistel" authorityYear="1850" class="Insecta" family="Scarabaeidae" genus="Rutela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rutela tristis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="tristis">Rutela tristis</taxonomicName>
