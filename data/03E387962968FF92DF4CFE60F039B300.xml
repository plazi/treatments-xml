<taxonomicName id="4C4A4D032969FF92DF6EFE87F085B0C7" authority="Cho, 2017" authorityName="Cho" authorityYear="2017" box="[382,699,380,404]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="179" phylum="Arthropoda" rank="species" species="kuatunensis" status="sp. nov." subGenus="Brachyphytodecta">
<emphasis id="B93EEA922969FF92DF6EFE87F3C2B0C0" box="[382,508,380,403]" italics="true" pageId="3" pageNumber="176">Gonioctena</emphasis>
