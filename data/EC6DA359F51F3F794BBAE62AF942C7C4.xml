<typeStatus id="BB7FACEDF5183F794B12EB55FDFFC50D" box="[370,539,1085,1125]" pageId="20" pageNumber="31">Holotype</typeStatus>
male, 
<typeStatus id="BB7FACEDF5183F7948F5EB55FCC1C50D" box="[661,805,1085,1125]" pageId="20" pageNumber="31">allotype</typeStatus>
female, and 
<specimenCount id="72C2D9C6F5183F794E70EB55FB1CC50D" box="[1040,1272,1085,1125]" pageId="20" pageNumber="31" type="generic" typeStatus="paratypes">44 paratypes</specimenCount>
