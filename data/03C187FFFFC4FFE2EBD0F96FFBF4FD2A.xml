<emphasis id="B91CEAFBFFC5FFE1EBD0F8B7FE9DF87E" bold="true" box="[151,269,1911,1933]" pageId="1" pageNumber="480">FIGURE 2.</emphasis>
<taxonomicName id="4C684D6AFFC5FFE1EA54F8B7FD1DF87F" authority="Forsskal, 1775" authorityName="Forsskal" authorityYear="1775" box="[275,653,1911,1932]" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="480" phylum="Chordata" rank="species" species="areolatus">
<emphasis id="B91CEAFBFFC5FFE1EA54F8B7FE79F87F" box="[275,489,1911,1932]" italics="true" pageId="1" pageNumber="480">Epinephelus areolatus</emphasis>
