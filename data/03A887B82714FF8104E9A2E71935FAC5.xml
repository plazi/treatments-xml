<taxonomicName id="4C014D2D2714FF8107E4A0E41F6DFC66" baseAuthorityName="Endrodi" baseAuthorityYear="1978" box="[973,1090,898,921]" class="Insecta" family="Dynastidae" genus="Phileurus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="carinatus">
