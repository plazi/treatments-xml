<specimenCount id="87B1FD41FFB3FFAAD7388878FDCEFBA4" box="[544,612,1123,1149]" pageId="15" pageNumber="16" type="generic">
<quantity id="564F9B2DFFB3FFAAD7388878FDCAFBA4" box="[544,608,1123,1149]" metricMagnitude="1" metricUnit="m" metricValue="4.0" pageId="15" pageNumber="16" unit="m" value="40.0">40 m</quantity>
