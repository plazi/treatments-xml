<collectingRegion id="5373F82AFFB3FFAAD4D0889CFDFEFB78" box="[456,596,1159,1185]" country="Nicaragua" name="Rio San Juan" pageId="15" pageNumber="16">río San Juan</collectingRegion>
, at Las Chorreras, Cutris de San Carlos, 
<collectingRegion id="5373F82AFFB3FFAAD10B889CFBDFFB78" box="[1043,1141,1159,1185]" country="Costa Rica" name="Alajuela" pageId="15" pageNumber="16">Alajuela</collectingRegion>
, (
<geoCoordinate id="F483500FFFB3FFAAD19F889DFB5EFB78" box="[1159,1268,1158,1185]" degrees="10.91566" direction="north" orientation="latitude" pageId="15" pageNumber="16" precision="1" value="10.91566">10.91566</geoCoordinate>
