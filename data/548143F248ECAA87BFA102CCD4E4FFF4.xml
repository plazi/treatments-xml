<taxonomicName id="3A731F5B957E0E708CE5B544CC5DB7D8" authority="Oudemans, 1910" authorityName="Oudemans" authorityYear="1910" class="Arachnida" family="Erythraeidae" genus="Charletonia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Charletonia feideri" order="Prostigmata" pageId="5" pageNumber="82" phylum="Arthropoda" rank="species" species="feideri">Charletonia feideri</taxonomicName>
