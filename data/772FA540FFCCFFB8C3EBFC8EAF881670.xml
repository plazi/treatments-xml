<specimenCount id="E980DFDFFFCCFFBCC1DBFB6FA81E10E0" box="[951,1066,1181,1205]" pageId="9" pageNumber="1916" type="male">two males</specimenCount>
on slides in 
<collectingCountry id="879154C6FFCCFFBCC2F1FB4FACC21080" box="[157,246,1213,1237]" name="Canada" pageId="9" pageNumber="1916">Canada</collectingCountry>
