<taxonomicName id="19F34D1E9306622FFC24FE3BFB65FE44" authority="Robineau-Desvoidy, 1830" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[897,1260,439,464]" class="Insecta" family="Tachinidae" genus="Pales" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="genus">
