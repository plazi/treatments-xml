<taxonomicName id="03FD4D09B628CD405836FD8DFD9DA6C6" authority="Distant, 1905 d" authorityName="Distant" authorityYear="1905" box="[287,616,621,647]" class="Insecta" family="Cicadidae" kingdom="Animalia" order="Hemiptera" pageId="117" pageNumber="118" phylum="Arthropoda" rank="subFamily" subFamily="Tibicininae">
