<taxonomicName id="4C5C4D14FFA7EB6C54A2FE70B66D39D2" authority="(C. L. Koch)" authorityName="Bosmans" authorityYear="2006" baseAuthorityName="C. L. Koch" baseAuthorityYear="1836" box="[161,531,450,476]" class="Arachnida" family="Linyphiidae" genus="Meioneta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="rurestris">
<emphasis id="B928EA85FFA7EB6C54A2FE70B50439D5" box="[161,378,450,475]" italics="true" pageId="30" pageNumber="31">Meioneta rurestris</emphasis>
(C.L. Koch)
</taxonomicName>
]
</paragraph>
