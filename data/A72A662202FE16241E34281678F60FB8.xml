<taxonomicName id="388D3109EE295B9B208A925E6CD3BDA6" LSID="http://zoobank.org/CEE8FE95-36F0-40B3-824A-C3BCFD29EDCD" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus pseudofulvicornis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pseudofulvicornis">Trigonopterus pseudofulvicornis Riedel</taxonomicName>
