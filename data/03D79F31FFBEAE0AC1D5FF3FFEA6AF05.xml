<taxonomicName id="4C7E55A4FFBEAE0AC5B8FF57FE2DAAE6" authority="Macquart, 1834" authorityName="Macquart" authorityYear="1834" class="Insecta" family="Tachinidae" genus="Dexia" kingdom="Animalia" order="Diptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="hirsuta">
<emphasis id="B90AF235FFBEAE0AC5B8FF57FA01AA0F" box="[1274,1436,350,375]" italics="true" pageId="77" pageNumber="78">Dexia hirsuta</emphasis>
Macquart, 1834
</taxonomicName>
