<taxonomicName id="7FE54D58FFDDFFC5FDB1B170FD7BC324" baseAuthorityName="Riley &amp; Howard" baseAuthorityYear="1890" box="[525,746,1081,1106]" class="Insecta" family="Margarodidae" genus="Crypticerya" kingdom="Animalia" order="Hemiptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="montserratensis">
