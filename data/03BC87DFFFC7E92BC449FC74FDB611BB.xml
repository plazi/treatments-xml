<taxonomicName id="4C154D4AFFC6E928C37BFC1DFD5610D5" box="[544,708,919,938]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="49" pageNumber="108" phylum="Arthropoda" rank="species" species="undetermined">
