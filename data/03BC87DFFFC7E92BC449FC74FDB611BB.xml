<taxonomicName id="4C154D4AFFC7E929C357F8D6FD221410" box="[524,688,1884,1903]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="48" pageNumber="107" phylum="Arthropoda" rank="species" species="undetermined">
