<geoCoordinate id="1440B29AB4C53552C694E88ED721E738" direction="north" orientation="latitude" precision="5" value="45.686">45.6860°N</geoCoordinate>
, 
<geoCoordinate id="E7BCBAF9780A6DDA695D367DF26F302C" direction="west" orientation="longitude" precision="5" value="-66.8668">66.8668°W</geoCoordinate>
