<taxonomicName id="66A2FE6EA03977D04889446CA6D00015" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="95" pageNumber="180" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
