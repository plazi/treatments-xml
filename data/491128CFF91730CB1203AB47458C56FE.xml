<taxonomicName id="430744EE84B31CA52828A6776153C3B8" class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex" order="Poales" pageId="95" pageNumber="180" phylum="Tracheophyta" rank="genus">Carex</taxonomicName>
hummock (2 ♂, RWC); 14 km WSW of Tracy, S of Rt. 645, 
<geoCoordinate id="67A96B3CE5E90EC4FD1CFB4AB3DDC672" direction="north" orientation="latitude" precision="5" value="45.6603">45.6603°N</geoCoordinate>
, 
<geoCoordinate id="71C64F20DA52EAB00A1F2E0F730A0F71" direction="west" orientation="longitude" precision="5" value="-66.8607">66.8607°W</geoCoordinate>
