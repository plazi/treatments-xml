<taxonomicName id="F5BDC535A601B0067AABE02549422E73" class="Insecta" family="Tettigoniidae" genus="Atlasacris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atlasacris" order="Orthoptera" pageId="9" pageNumber="86" phylum="Arthropoda" rank="genus">Atlasacris</taxonomicName>
