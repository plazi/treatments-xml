<taxonomicName id="DECE077F2E67191AEC41D85346A8AC92" class="Insecta" family="Tettigoniidae" genus="Atlasacris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atlasacris" order="Orthoptera" pageId="9" pageNumber="86" phylum="Arthropoda" rank="genus">Atlasacris</taxonomicName>
