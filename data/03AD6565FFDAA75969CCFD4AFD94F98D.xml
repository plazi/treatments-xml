<taxonomicName id="4C04AFF0FFDAA7596994FD4AFE15FCB6" authorityName="Gauld" authorityYear="1991" box="[207,419,761,787]" class="Insecta" family="Ichneumonidae" genus="Clistopyga" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="104" phylum="Arthropoda" rank="species" species="henryi">
<emphasis id="B9700861FFDAA7596994FD4AFE15FCB6" bold="true" box="[207,419,761,787]" italics="true" pageId="1" pageNumber="104">Clistopyga henryi</emphasis>
