<taxonomicName id="4C6131FCD73FFFDD0BF7AAA77D9F75E9" baseAuthorityName="Smith" baseAuthorityYear="1905" box="[500,761,1469,1495]" class="Insecta" family="Noctuidae" genus="Bryolymnia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="191" phylum="Arthropoda" rank="species" species="viridimedia">
