<collectionCode id="99B0D8A9B81CE52263F1F80263BEF8EB" box="[151,210,1946,1972]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" lsid="urn:lsid:biocol.org:col:34700" name="California Academy of Sciences" pageId="62" pageNumber="63" type="Museum">CAS</collectionCode>
