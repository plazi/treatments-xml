) 
<number id="9C3CEE31D65B763279BDFA147CD0FA1C" box="[729,743,1505,1530]" pageId="23" pageNumber="224" value="6.0">6</number>
(
<number id="9C3CEE31D65B76327993FA177D23FA1C" box="[759,788,1506,1530]" pageId="23" pageNumber="224" value="12.0">12</number>
