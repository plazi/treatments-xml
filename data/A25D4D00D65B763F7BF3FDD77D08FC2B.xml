(
<number id="9C3CEE31D65B76327A8FFA147C3FFA1C" box="[491,520,1505,1530]" pageId="23" pageNumber="224" value="15.0">15</number>
) 
<number id="9C3CEE31D65B76327973FA177C12FA1C" box="[535,549,1506,1530]" pageId="23" pageNumber="224" value="2.0">2</number>
