<taxonomicName id="9DB1853ED6BB0D8AE5D996EEC1C1F19D" class="Insecta" family="Gelechiidae" genus="Megacraspedus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megacraspedus andreneli" order="Lepidoptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="species" species="andreneli">Megacraspedus andreneli</taxonomicName>
