<date id="F56E10474D34FF8C0E3F35E45EB7E395" box="[1086,1253,924,950]" pageId="4" pageNumber="39" value="2004-08-11">11­Aug.­2004</date>
, from 
<taxonomicName id="46D04D044D34FF8C0B0935BC58B7E3FD" authority="(Roxb. ex Willd.) Baill." authorityName="Baill." baseAuthorityName="Willd." box="[264,741,964,990]" class="Magnoliopsida" family="Phyllanthaceae" genus="Fluggea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="4" pageNumber="39" phylum="Tracheophyta" rank="species" species="virosa">
