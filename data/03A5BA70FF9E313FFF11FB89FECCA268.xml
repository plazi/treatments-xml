<quantity id="4CF4A683FF9E313EFD3CFA76FCCDA6DE" box="[701,789,1478,1499]" metricMagnitude="-5" metricUnit="m" metricValue="2.0" pageId="2" pageNumber="35" unit="mm" value="0.02">0.02 mm</quantity>
