<location id="8ED35DBDFF9E313EFE0AFAC3FE26A68D" LSID="urn:lsid:plazi:treatment:03A5BA70FF9E313FFF11FB89FECCA268:8ED35DBDFF9E313EFE0AFAC3FE26A68D" box="[395,510,1395,1417]" county="Head" municipality="Elytra" name="Middle area" pageId="2" pageNumber="35">Middle area</location>
