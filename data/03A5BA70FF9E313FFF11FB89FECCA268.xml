<location id="8ED35DBDFF9E313EFE06FA91FE3EA633" LSID="urn:lsid:plazi:treatment:03A5BA70FF9E313FFF11FB89FECCA268:8ED35DBDFF9E313EFE06FA91FE3EA633" box="[391,486,1313,1334]" county="Head" municipality="Elytra" name="Pronotum" pageId="2" pageNumber="35">Pronotum</location>
