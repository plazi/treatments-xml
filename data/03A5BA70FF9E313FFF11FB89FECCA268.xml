<location id="8ED35DBDFF9E313EFF52FA8CFED5A657" LSID="urn:lsid:plazi:treatment:03A5BA70FF9E313FFF11FB89FECCA268:8ED35DBDFF9E313EFF52FA8CFED5A657" box="[211,269,1340,1362]" county="Head" municipality="Elytra" name="Elytra" pageId="2" pageNumber="35">Elytra</location>
