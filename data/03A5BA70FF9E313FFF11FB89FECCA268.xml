<location id="8ED35DBDFF9E313EFC0BFA90FC17A633" LSID="urn:lsid:plazi:treatment:03A5BA70FF9E313FFF11FB89FECCA268:8ED35DBDFF9E313EFC0BFA90FC17A633" box="[906,975,1312,1334]" county="Head" municipality="Elytra" name="Minute" pageId="2" pageNumber="35">Minute</location>
