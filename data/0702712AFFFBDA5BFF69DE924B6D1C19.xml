species: 
<taxonomicName id="48ABBBBFFFFBDA5BFE9EDED24D4A1B58" authorityName="Goulding &amp; Dayrat" authorityYear="2018" box="[301,536,760,784]" class="Gastropoda" family="Onchidiidae" genus="Alionchis" kingdom="Animalia" order="Systellommatophora" pageId="4" pageNumber="341" phylum="Mollusca" rank="species" species="jailoloensis">
<emphasis id="BDDF1C2EFFFBDA5BFE9EDED24D4A1B58" bold="true" box="[301,536,760,784]" italics="true" pageId="4" pageNumber="341">Alionchis jailoloensis</emphasis>
