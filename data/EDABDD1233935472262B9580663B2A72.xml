<taxonomicName id="547AE3966B4F8E5D7ACFDD91822AA56F" LSID="http://zoobank.org/B56E6C09-F0EB-4302-8BAA-D799C323B189" authority="Jordal &amp; Kirkendall" class="Insecta" family="Curculionidae" genus="Scolytodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scolytodes squamatifrons" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="squamatifrons">Scolytodes squamatifrons Jordal &amp; Kirkendall</taxonomicName>
<taxonomicNameLabel id="24EA0080B371F41495CCA81A091B335F" pageId="0" pageNumber="1">sp. nov.</taxonomicNameLabel>
