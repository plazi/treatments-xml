<taxonomicName id="51A94D4EFFDED53FFBEAFE50F4C913EF" authority="Cameron, 1950" authorityName="Cameron" authorityYear="1950" class="Insecta" family="Staphylinidae" genus="Oxytelus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="201" phylum="Arthropoda" rank="species" species="machadoi">
