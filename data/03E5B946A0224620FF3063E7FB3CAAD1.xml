<taxonomicName id="4C4C73D3A0224620FF3063E7FED4ADDA" authorityName="Cameron" authorityYear="1884" box="[160,364,410,436]" class="Insecta" family="Eucharitidae" genus="Orasema" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="737" phylum="Arthropoda" rank="genus">
<emphasis id="B938D442A0224620FF3063E7FED4ADDA" bold="true" box="[160,364,410,436]" italics="true" pageId="3" pageNumber="737">Orasema lasallei</emphasis>
