<quantity id="4CEF9B25FF1EFF3A9D10FB03FFD43509" box="[1389,1429,1265,1292]" metricMagnitude="0" metricUnit="m" metricValue="1.8288" pageId="143" pageNumber="144" unit="ft" value="6.0">6 ft</quantity>
