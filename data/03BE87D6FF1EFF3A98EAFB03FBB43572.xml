<taxonomicName id="4C174D43FF1EFF3A9D36FAC8FBB03572" authority="(Walbaum, 1792)" baseAuthorityName="Walbaum" baseAuthorityYear="1792" class="Actinopterygii" family="Moronidae" genus="Roccus" kingdom="Animalia" order="Perciformes" pageId="143" pageNumber="144" phylum="Chordata" rank="species" species="saxatilis">
<emphasis id="B963EAD2FF1EFF3A9D36FAC8FB623572" italics="true" pageId="143" pageNumber="144">Roccus saxatilis</emphasis>
