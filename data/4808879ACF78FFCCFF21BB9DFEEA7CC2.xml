<taxonomicName id="07A14D0FCF7BFFCCFDEDBE14FC827C56" box="[603,770,1309,1333]" class="Insecta" family="Hybotidae" genus="Crossopalpus" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="quadrispina">
