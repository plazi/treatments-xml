<collectorName id="26D9537BE353FFC5FA8DFA02B800FA66" pageId="84" pageNumber="85">J.H. Randriamihaja</collectorName>
&amp; 
<collectorName id="26D9537BE353FFC5FEBDFA6FBB23FA66" box="[351,543,1482,1507]" pageId="84" pageNumber="85">T.Ranarilalatiana</collectorName>
(ZAH13-06) (2 
<collectionCode id="ED3DAE68E353FFC5FD3BFA6EBA15FA66" box="[729,809,1483,1507]" country="Sweden" httpUri="http://grbio.org/cool/rsbi-vwmq" name="Swedish Museum of Natural History, Entomology Collections" pageId="84" pageNumber="85">NHRS</collectionCode>
)
</materialsCitation>
