<taxonomicName id="4C7612BBFFF2FFC7588AFB109C5DFB23" box="[999,1101,1209,1234]" class="Insecta" family="Tingidae" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="50" phylum="Arthropoda" rank="subFamily" subFamily="Tinginae">Tinginae</taxonomicName>
