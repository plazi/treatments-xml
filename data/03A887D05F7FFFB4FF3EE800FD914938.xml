<taxonomicName id="4C014D455F7FFFB4FC43EE89FBFF4AD4" authorityName="De Meyer" authorityYear="1995" box="[1002,1104,1482,1508]" class="Insecta" family="Pipunculidae" genus="Tomosvaryella" kingdom="Animalia" order="Diptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="jubata">
