<taxonomicName id="6184E1A5FF92FFFDFD555C889DC106A9" box="[646,916,1467,1492]" class="Arachnida" family="Momoniidae" genus="Canterburaturus" kingdom="Animalia" order="Trombidiformes" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="cooki">
