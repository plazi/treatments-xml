<taxonomicName id="DBC64D79FF812C7AFE09FB06FD2CFB56" authorityName="Attems" authorityYear="1930" box="[480,709,1238,1264]" class="Chilopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scolopendromorpha" pageId="22" pageNumber="99" phylum="Arthropoda" rank="order">scolopendromorph</taxonomicName>
