<collectionCode id="ED207DF91937FFDB69C094FE1B3BFA83" box="[956,1050,1264,1285]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="8" pageNumber="8">AMNH</collectionCode>
