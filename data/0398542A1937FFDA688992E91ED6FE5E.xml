<geoCoordinate id="EE0583FB1936FFDA6B4391CC1EB4FE51" box="[319,405,450,471]" degrees="34.18" direction="north" orientation="latitude" pageId="9" pageNumber="9" precision="555" value="34.18">34.18N</geoCoordinate>
