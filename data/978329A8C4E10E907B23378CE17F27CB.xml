<paragraph id="C949B3D1122DEBC1297AF90948A95214" pageId="0" pageNumber="177">
<taxonomicName id="D361083EE8B7A3D2593CB23C75F8A454" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax punctatus" order="Coleoptera" pageId="0" pageNumber="177" phylum="Arthropoda" rank="species" species="punctatus">Mecyclothorax punctatus</taxonomicName>
