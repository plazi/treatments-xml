<location id="8EAF88E4FF96FFB3FAD5FF7FFC5C0EE5" LSID="urn:lsid:plazi:treatment:03D96F29FF96FFB3FF11F957FAC20D0C:8EAF88E4FF96FFB3FAD5FF7FFC5C0EE5" country="Japan" county="Kamchatka" municipality="Oregon" name="Aleutian Islands" pageId="51" pageNumber="496" stateProvince="Hokkaido">Aleutian Islands</location>
, eastward and southward to 
<location id="8EAF88E4FF96FFB3FB31FF5EFA720EE5" LSID="urn:lsid:plazi:treatment:03D96F29FF96FFB3FF11F957FAC20D0C:8EAF88E4FF96FFB3FB31FF5EFA720EE5" box="[1243,1441,226,248]" country="Japan" county="Kamchatka" municipality="Oregon" name="British Columbia" pageId="51" pageNumber="496" stateProvince="Hokkaido">British Columbia</location>
(
<bibRefCitation id="EFE1A3CEFF96FFB3FCA8FEBDFC3C0F0A" author="Rathbun MJ" box="[834,1007,257,279]" pageId="51" pageNumber="496" pagination="1 - 5" refId="ref75249" refString="Rathbun MJ. 1924. New species and subspecies of spider crabs. Proceedings of the United States National Museum 64: 1 - 5." type="journal article" year="1924">Rathbun, 1924</bibRefCitation>
