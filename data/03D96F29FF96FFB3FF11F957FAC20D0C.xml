<quantity id="4C8873DAFF96FFB3FC92FE82FC270F4E" box="[888,1012,318,340]" metricMagnitude="2" metricUnit="m" metricValue="2.4" metricValueMax="4.74" metricValueMin="0.06" pageId="51" pageNumber="496" unit="m" value="240.0" valueMax="474.0" valueMin="6.0">
<elevation id="005D390CFF96FFB3FC92FE82FC270F4E" box="[888,1012,318,340]" metricMagnitude="2" metricUnit="m" metricValue="2.4" metricValueMax="4.74" metricValueMin="0.06" pageId="51" pageNumber="496" unit="m" value="240.0" valueMax="474.0" valueMin="6.0">6 to 474 m</elevation>
</quantity>
, usually on soft mud or sand
</materialsCitation>
.
