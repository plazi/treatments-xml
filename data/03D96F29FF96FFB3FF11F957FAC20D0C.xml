<collectingRegion id="49B410DDFF96FFB3FA80FEBDFCA90F28" country="Japan" name="Hokkaido" pageId="51" pageNumber="496">Hokkaido</collectingRegion>
, 
<collectingCountry id="F3679EAFFF96FFB3FC6CFE9CFC1F0F28" box="[902,972,288,309]" name="Japan" pageId="51" pageNumber="496">Japan</collectingCountry>
