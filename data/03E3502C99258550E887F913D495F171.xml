<taxonomicName id="4C4A9AB999258557E8D1F913D73CF769" authority="Meigen, 1824" authorityName="Meigen" authorityYear="1824" box="[193,621,1772,1799]" class="Insecta" family="Dolichopodidae" genus="Dolichopus" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="vitripennis">
