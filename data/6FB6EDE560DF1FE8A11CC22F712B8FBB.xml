<taxonomicName id="EF3596FED71357EEC7E5B44434542BBB" LSID-ZBK="urn:lsid:zoobank.org:act:BAA795C9-B135-46E7-9B4A-56C328A92833" class="Actinopterygii" family="Poeciliidae" genus="Cnesterodon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Cnesterodon Garman 1895:43" order="Cyprinodontiformes" pageNumber="22" phylum="Chordata" rank="genus">Cnesterodon Garman</taxonomicName>
