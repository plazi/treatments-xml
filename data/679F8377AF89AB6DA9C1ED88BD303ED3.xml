<taxonomicName id="168524D702A1E2A20532D7998469C6BB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145345" authority="Andrei" authorityName="Andrei" class="Hexapoda" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sima exasciata Forel" order="Hymenoptera" pageNumber="261" phylum="Arthropoda" rank="species" species="exasciata">SIMA EXASCIATA</taxonomicName>
