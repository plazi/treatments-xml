<figureCitation id="B1DE78B81741FFB6FF56FB5DFF3356C8" box="[151,167,1247,1271]" captionStart="FIGURES 8 – 9" captionStartId="8.[151,264,1729,1752]" captionTargetBox="[151,1385,1019,1667]" captionTargetId="figure@8.[151,1436,983,1707]" captionTargetPageId="8" captionText="FIGURES 8 – 9. Gracilaclopus caceresi, male." httpUri="https://zenodo.org/record/210635/files/figure.png" pageId="5" pageNumber="6">8</figureCitation>
, 
<figureCitation id="B1DE78B81741FFB6FF72FB5DFF4556C8" box="[179,209,1247,1271]" captionStart="FIGURES 10 – 11" captionStartId="10.[151,264,828,851]" captionTargetBox="[163,1404,193,793]" captionTargetId="figure@10.[151,1436,147,807]" captionTargetPageId="10" captionText="FIGURES 10 – 11. Gracilaclopus candelariae, male." httpUri="https://zenodo.org/record/210636/files/figure.png" pageId="5" pageNumber="6">10</figureCitation>
