<taxonomicName id="4C58A0B51E10FFADFEDCA4FCFF0CDAEB" authority="(Covell et al. 1984)" baseAuthorityName="Covell" baseAuthorityYear="1984" class="Magnoliopsida" family="Asteraceae" genus="Chrysoma" kingdom="Plantae" order="Asterales" pageId="15" pageNumber="272" phylum="Tracheophyta" rank="species" species="pauciflosculosa">
