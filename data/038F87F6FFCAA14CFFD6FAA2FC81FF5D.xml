NAMES IN 
<taxonomicName id="4C264D63FFCAA14DFEA5FAA2FCDDFAEA" authority="FROM" authorityName="FROM" box="[257,462,1325,1351]" class="Magnoliopsida" family="Cunoniaceae" genus="Geissois" kingdom="Plantae" order="Oxalidales" pageId="4" pageNumber="315" phylum="Tracheophyta" rank="genus">
<emphasis id="B952EAF2FFCAA14DFEA5FAA2FC65FAEA" box="[257,374,1325,1351]" italics="true" pageId="4" pageNumber="315">GEISSOIS</emphasis>
FROM
</taxonomicName>
