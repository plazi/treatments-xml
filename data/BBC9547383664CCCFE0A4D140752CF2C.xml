<taxonomicName id="C0D278EFF43EA9DBD37076854F70C598" class="Insecta" family="Cerambycidae" genus="Dolichostyrax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dolichostyrax" order="Coleoptera" pageId="2" pageNumber="51" phylum="Arthropoda" rank="genus">Dolichostyrax</taxonomicName>
