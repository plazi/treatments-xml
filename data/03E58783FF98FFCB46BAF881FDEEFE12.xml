species.— 
<taxonomicName id="4C4C4D16FF98FFCA4714F827FCEEF84B" authority="Walker 1850: 14 (Natal)" authorityName="Walker" authorityYear="1850" box="[313,804,2012,2037]" class="Insecta" family="Cicadidae" genus="Platypleura" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="punctigera">
