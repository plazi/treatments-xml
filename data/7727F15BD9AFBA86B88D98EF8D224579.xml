<taxonomicName id="2B77AB5DAFBA1696B52B93F5FECAC92A" LSID-ZBK="urn:lsid:zoobank.org:act:EB97BE42-AECC-453C-8FE2-D4E553CCB37F" class="Actinopterygii" family="Cobitidae" genus="Botia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Botia modesta Bleeker 1864:11" order="Cypriniformes" pageNumber="6" phylum="Chordata" rank="species" species="modesta">B. modesta</taxonomicName>
