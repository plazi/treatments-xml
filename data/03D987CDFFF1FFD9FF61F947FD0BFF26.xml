<typeStatus id="54CB8879FFF1FFD8FF61F926FF3DF90E" box="[151,200,1767,1788]" pageId="2" pageNumber="15">Type</typeStatus>
species: 
<taxonomicName id="4C704D58FFF1FFD8FED0F926FD78F90E" authority="Kethley, 1973" authorityName="Kethley" authorityYear="1973" box="[294,653,1766,1789]" class="Arachnida" family="Syringophilidae" genus="Colinophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="15" phylum="Arthropoda" rank="species" species="wilsoni">
<emphasis id="B904EAC9FFF1FFD8FED0F926FE0DF90F" box="[294,504,1767,1789]" italics="true" pageId="2" pageNumber="15">Colinophilus wilsoni</emphasis>
<bibRefCitation id="EFE14B2AFFF1FFD8FE08F927FD78F90E" author="Kethley" box="[510,653,1766,1788]" pageId="2" pageNumber="15" refString="Kethley, J. B. (1973) A new genus and species of quill mites (Acarina: Syringophilidae) from Colinus virginianus (Galliformes: Phasianidae) with notes on developmental chaetotaxy. Fieldiana (Zoology), 65, 1 - 8." type="journal article" year="1973">
Kethley, 
<number id="3DB824FCFFF1FFD8FDACF926FD78F90E" box="[602,653,1767,1788]" pageId="2" pageNumber="15" value="1973.0">1973</number>
</bibRefCitation>
</taxonomicName>
