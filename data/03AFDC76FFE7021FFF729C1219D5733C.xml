<subSubSection id="C31C3EEBFFE7021FFF269A281942708F" pageId="43" pageNumber="44" type="etymology">
<paragraph id="8BB96D60FFE7021FFF269A281942708F" blockId="43.[151,1437,1008,1578]" pageId="43" pageNumber="44">
<emphasis id="B972B172FFE7021FFF269A281B667063" bold="true" box="[195,330,1373,1398]" pageId="43" pageNumber="44">Etymology.</emphasis>
