<taxonomicName id="4C0616E3FFE7021FFB5E9AD21FB770AB" baseAuthorityName="Linnaeus" baseAuthorityYear="1766" box="[1211,1435,1447,1470]" class="Mammalia" family="Cercopithecidae" genus="Macaca" kingdom="Animalia" order="Primates" pageId="43" pageNumber="44" phylum="Chordata" rank="species" species="nemestrina">
