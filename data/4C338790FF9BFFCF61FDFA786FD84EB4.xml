[= 
<taxonomicName id="039A4D05FF9CFFCF61FDFD2D6A2E4BB0" authority="Suarez-Morales &amp; Iliffe, 2005" authorityName="Suarez-Morales &amp; Iliffe" authorityYear="2005" box="[151,1049,693,719]" class="Hexanauplia" family="Ameiridae" genus="Stygonitocrella" kingdom="Animalia" order="Harpacticoida" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="mexicana" subGenus="Eustygonitocrella">
