<taxonomicName id="4C414D07FFE4F232FB07FA921493FA2C" authority="Simon 1873" authorityName="Simon" authorityYear="1873" class="Arachnida" family="Theraphosidae" genus="Cratorrhagus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="36" phylum="Arthropoda" rank="species" species="concolor" status="n. comb.">
