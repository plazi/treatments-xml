<taxonomicName id="32B34D30FFA8F947E45A4494FDC3FB0B" authority="Ashmead, 1906: 294 - 295" authorityName="Ashmead" authorityPageNumber="294 - 295" authorityYear="2016" box="[120,541,1197,1221]" class="Insecta" family="Ichneumonidae" genus="Nesolinoceras" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
