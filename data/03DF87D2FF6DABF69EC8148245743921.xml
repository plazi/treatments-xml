<taxonomicName id="4C764D47FF6FABF69D1310E545EA38FA" authorityName="Lane" authorityYear="1939" box="[844,951,192,217]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="infuscata">
