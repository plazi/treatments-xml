<taxonomicName id="EBF135A3FFD07C426DC464C97643FE29" authorityName="Bergquist" authorityYear="1968" box="[705,874,384,410]" class="Demospongiae" family="Vulcanellidae" genus="Lamellomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tetractinellida" pageId="16" pageNumber="17" phylum="Porifera" rank="species" species="strongylata">
<emphasis id="1E859232FFD07C426DC464C97643FE29" box="[705,874,384,410]" italics="true" pageId="16" pageNumber="17">L. strongylata</emphasis>
</taxonomicName>
; 3 bp difference with the 28S (C1-C2) of 
<taxonomicName id="EBF135A3FFD07C426FB864EB74F9FE0E" baseAuthorityName="Bowerbank" baseAuthorityYear="1866" box="[189,464,419,445]" class="Demospongiae" family="Pachastrellidae" genus="Poecillastra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astrophorida" pageId="16" pageNumber="17" phylum="Porifera" rank="species" species="compressa">
