<typeStatus id="F34AF082FFD07C426B65641571E4FEC4" box="[1120,1229,349,375]" pageId="16" pageNumber="17">Holotype</typeStatus>
(
<accessionNumber id="33A2D3C3FFD07C426BD964157043FEC4" box="[1244,1386,349,375]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/MK033144" pageId="16" pageNumber="17">
