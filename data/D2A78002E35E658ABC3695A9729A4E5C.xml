<paragraph id="70619087F851293664FA30DA0AD17B07" pageId="90" pageNumber="115">
<taxonomicName id="1ACAD11015125B275A61FEB18B6169E3" LSID="http://zoobank.org/144D32AA-7B76-483E-A0F4-9159CDDD60D5" authority="Fernandez-Triana" authorityName="Fernandez-Triana" authorityYear="2018" class="Insecta" family="Braconidae" genus="Zachterbergius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zachterbergius" order="Hymenoptera" pageId="90" pageNumber="115" phylum="Arthropoda" rank="genus">Zachterbergius Fernandez-Triana</taxonomicName>
<taxonomicNameLabel id="BBBADC63503C73E0D792BBA0EA4A9F1A" pageId="90" pageNumber="115">gen. n.</taxonomicNameLabel>
