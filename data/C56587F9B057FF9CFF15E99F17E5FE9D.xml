<collectingCountry id="35DB767FB056FF9CFE80ED871778FF6D" box="[314,374,194,215]" name="China" pageId="5" pageNumber="316">China</collectingCountry>
: 
<collectingRegion id="8F08F80DB056FF9CFE38ED871417FF6D" box="[386,537,194,215]" country="China" name="Hebei" pageId="5" pageNumber="316">Hebei Province</collectingRegion>
, Xiaowutaishan; 
<collectingRegion id="8F08F80DB056FF9CFD7AED871565FF6D" box="[704,875,194,215]" country="China" name="Yunnan" pageId="5" pageNumber="316">Yunnan Province</collectingRegion>
