<taxonomicName id="C9B84D6B787AFFE675B64BDAE0FAF90C" authority="Pict." authorityName="Pict." authorityYear="1841" box="[913,1276,1682,1706]" class="Insecta" family="Perlodidae" genus="Dictyopteryx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="293" phylum="Arthropoda" rank="species" species="microcephala">
