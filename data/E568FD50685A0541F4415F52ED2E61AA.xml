<taxonomicName id="5ACC504BD89D65D1822A8EB0087F7B1E" baseAuthorityName="Reeve" baseAuthorityYear="1849" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="151" pageNumber="152" phylum="Mollusca" rank="species" species="spectatus" subGenus="Drymaeus">
