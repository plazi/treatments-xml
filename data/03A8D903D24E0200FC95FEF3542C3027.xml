<taxonomicName id="4C011396D24E0200FC02FE36545D32C4" authority="Long et al. 2020" authorityName="Long" authorityYear="2020" box="[939,1375,450,474]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="65" pageNumber="2847" phylum="Firmicutes" rank="species" species="mulanensis">
<emphasis id="B975B407D24E0200FC02FE3655B232C4" box="[939,1200,450,474]" italics="true" pageId="65" pageNumber="2847">Lactobacillus mulanensis</emphasis>
Long 
<emphasis id="B975B407D24E0200FB5BFE36541C32C4" box="[1266,1310,450,474]" italics="true" pageId="65" pageNumber="2847">et al</emphasis>
. 2020
</taxonomicName>
, 14 
<superScript id="7C74C55DD24E0200FA2AFE34549732D0" attach="none" box="[1411,1429,448,462]" fontSize="6" pageId="65" pageNumber="2847">VP</superScript>
