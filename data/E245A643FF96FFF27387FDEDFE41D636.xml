plant from which 
<taxonomicName id="ADEC6CD6FF97FFFD700DFBE4FD68D401" box="[521,696,1079,1102]" class="Insecta" family="Psychodidae" genus="Mucomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="141" phylum="Arthropoda" rank="species" species="emersa">
