(Vera Cruz), 
<collectingCountry id="CB8876467C449B13FC72F7859C9AFA0D" box="[898,1023,1465,1490]" name="Costa Rica" pageId="55" pageNumber="56">
<collectingRegion id="715BF8347C449B13FC72F7859CA7FA0D" box="[898,962,1466,1490]" country="New Zealand" name="West Coast" pageId="55" pageNumber="56">Costa</collectingRegion>
Rica
</collectingCountry>
and 
<collectingRegion id="715BF8347C449B13FBCBF7859BF1FA0E" box="[1083,1172,1466,1489]" country="Panama" name="Panama" pageId="55" pageNumber="56">
<collectingCountry id="CB8876467C449B13FBCBF7859BF1FA0E" box="[1083,1172,1466,1489]" name="Panama" pageId="55" pageNumber="56">Panama</collectingCountry>
</collectingRegion>
