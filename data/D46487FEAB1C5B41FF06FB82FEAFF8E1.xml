<quantity id="9B359B0DAB1C5B40FDA6FA9EFD2AFA80" box="[567,671,1356,1380]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="34" pageNumber="185" unit="mm" value="2.1">2.10 mm</quantity>
