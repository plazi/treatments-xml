<taxonomicName id="0E23AEF018DFCE6394D23A791367247A" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" lsidName="Betula" order="Fagales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">
