<taxonomicName id="5207209EF612BE956BAC51DD9787D768" authorityName="S. H. Long &amp; Q. R. Li" authorityYear="2021" class="Pyrenomycetes" family="Diatrypaceae" genus="Diatrype" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Diatrype lancangensis" order="Xylariales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="lancangensis">
