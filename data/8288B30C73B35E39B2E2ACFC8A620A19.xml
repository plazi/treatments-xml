<taxonomicName id="2BFB5A495188F3624073B86CD78E6E07" class="Pyrenomycetes" family="Diatrypaceae" genus="Diatrype" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Diatrype" order="Xylariales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="genus">
<emphasis id="C09572942A8651BD5893305380A305F6" italics="true" pageId="0" pageNumber="1">Diatrype</emphasis>
</taxonomicName>
.
