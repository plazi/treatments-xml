<taxonomicName id="4C264D52FFFEFFCEFED30EE4FDAFF84E" box="[287,516,1954,1976]" class="Adenophorea" family="Oxystominidae" genus="Litinium" kingdom="Animalia" order="Enoplida" pageId="13" pageNumber="70" phylum="Nematoda" rank="species" species="profundorum" status="sp. n.">
<emphasis id="B952EAC3FFFEFFCEFED30EE4FDAFF84E" box="[287,516,1954,1976]" italics="true" pageId="13" pageNumber="70">
Litinium 
<taxonomicName id="4C264D52FFFEFFCEFEB30EE5FDAFF84E" box="[383,516,1954,1976]" pageId="13" pageNumber="70" rank="species" species="profundorum" status="sp. n.">profundorum</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="B952EAC3FFFEFFCEFDC10EE5FDE2F841" bold="true" box="[525,585,1954,1975]" pageId="13" pageNumber="70">
