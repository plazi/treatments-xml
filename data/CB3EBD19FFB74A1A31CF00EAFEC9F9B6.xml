<quantity id="846FA1EAFFB24A1A35F7021DFACEFE86" box="[1199,1288,260,284]" metricMagnitude="-3" metricUnit="m" metricValue="2.7" pageId="4" pageNumber="369" unit="mm" value="2.7">2.7 mm</quantity>
