<typeStatus id="2F1B882D2A50FF8C7C2F955EFCB86209" box="[828,883,271,294]" pageId="2" pageNumber="175">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="37A04D0C2A50FF8C7CC39541FAF16209" authority="Ahrens, 1810" authorityName="Ahrens" authorityYear="1810" box="[976,1338,271,295]" class="Insecta" family="Chrysomelidae" genus="Donacia" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="175" phylum="Arthropoda" rank="species" species="brevicornis">
