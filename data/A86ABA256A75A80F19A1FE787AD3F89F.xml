<taxonomicName id="E7C370B06A75A80F1A4AFAF87B9DFABF" authority="Warner and Ratcliffe" authorityName="Warner and Ratcliffe" authorityYear="2011" box="[811,1262,1282,1307]" class="Insecta" family="Cetoniidae" genus="Gymnetina" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="howdeni" status="new species">
