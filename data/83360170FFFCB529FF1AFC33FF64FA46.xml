<collectionCode id="6D8E28A3FFFDB529FAD4FBCEFA02FB82" box="[1369,1436,1088,1112]" pageId="3" pageNumber="64">BRA-</collectionCode>
<collectionCode id="6D8E28A3FFFDB529FF1AFBEBFF5BFBA6" box="[151,197,1125,1148]" httpUri="http://grbio.org/cool/74rs-wjen" name="Academy of Sciences, Zoological Institute" pageId="3" pageNumber="64">ZIL</collectionCode>
