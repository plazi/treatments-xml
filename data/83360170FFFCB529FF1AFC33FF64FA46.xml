<collectionCode id="6D8E28A3FFFDB529FCC4FB06FC06FB7A" box="[841,920,1160,1184]" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="3" pageNumber="64">MZSP</collectionCode>
