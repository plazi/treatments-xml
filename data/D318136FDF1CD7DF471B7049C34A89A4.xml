, B 
<taxonomicName id="4CD98CB13D013564FE2C0D530B44ECE7" class="Hydrozoa" family="Stylasteridae" genus="Stylaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stylaster parageus subsp. columbiensis" order="Anthoathecata" pageId="44" pageNumber="45" phylum="Cnidaria" rank="subSpecies" species="parageus" subSpecies="columbiensis">Stylaster parageus columbiensis</taxonomicName>
C 
<taxonomicName id="C1AC75AF37138020F7C3F4A6EC55A5F1" class="Hydrozoa" family="Stylasteridae" genus="Stylaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stylaster elassotomus" order="Anthoathecata" pageId="44" pageNumber="45" phylum="Cnidaria" rank="species" species="elassotomus">Stylaster elassotomus</taxonomicName>
D 
<taxonomicName id="A2A55C2955A4B8610DC13AE83F1A669F" class="Hydrozoa" family="Stylasteridae" genus="Stylaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stylaster crassiseptum" order="Anthoathecata" pageId="44" pageNumber="45" phylum="Cnidaria" rank="species" species="crassiseptum">Stylaster crassiseptum</taxonomicName>
<normalizedToken id="69B54FA203F760813FD1E5B126AC124E" originalValue="E–G">E-G</normalizedToken>
