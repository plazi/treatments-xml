<taxonomicName id="4C234D28FF82677AFF78830FFC8EFA2E" authority="Boeters, Gloer &amp; Pesic, 2014" authorityName="Boeters, Gloer &amp; Pesic" authorityYear="2014" box="[189,801,1389,1415]" class="Gastropoda" family="Hydrobiidae" genus="Arganiella" kingdom="Animalia" order="Littorinimorpha" pageId="6" pageNumber="146" phylum="Mollusca" rank="species" species="tabanensis">
