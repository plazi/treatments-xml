<taxonomicName id="6B1E160FE1F6068AE017B63D300FE550" class="Polychaeta" family="Sabellidae" genus="Branchiomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Branchiomma vesiculosum" order="Sabellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="vesiculosum">Branchiomma vesiculosum</taxonomicName>
(Montagu, 1813) | 
<taxonomicName id="BCFEB6C42C07DE13CA29E3BFB1053EAE" class="Polychaeta" family="Sabellidae" genus="Megalomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megalomma vesiculosum" order="Sabellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="vesiculosum">Megalomma vesiculosum</taxonomicName>
