<taxonomicName id="0D0B39E9247BD4D20ABD49FAF052A915" authority="" authorityName="" class="Arachnida" family="Oribatulidae" genus="Oribates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageNumber="403" phylum="Arthropoda" rank="species" species="pusillus">Oribates pusillus Berlese</taxonomicName>
