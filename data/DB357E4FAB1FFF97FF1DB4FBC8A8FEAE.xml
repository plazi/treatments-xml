<taxonomicName id="949CB4DAAB1FFF96FF1DB4FBCE84FAC5" authority="(Hampson, 1891)" authorityName="Hampson" authorityYear="1891" baseAuthorityName="Hampson" baseAuthorityYear="1891" box="[151,710,1268,1294]" class="Insecta" family="Hesperiidae" genus="Apallaga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="487" phylum="Arthropoda" rank="subSpecies" species="biseriata" subSpecies="maculata">
