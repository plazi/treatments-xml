, 
<collectorName id="50185311FF84FFD3FEEFF962FE37FDE2" box="[371,485,536,559]" pageId="3" pageNumber="248">Miyaneh</collectorName>
city, 
<collectingRegion id="3F29F825FF84FFD3FDB1F962FCA1FDE2" box="[557,883,536,559]" country="Iran" name="Azarbayjan-e Sharqi" pageId="3" pageNumber="248">East Azerbaijan province</collectingRegion>
, 
<collectingCountry id="85FA7657FF84FFD3FC1CF962FC66FDE2" box="[896,948,536,559]" name="Iran" pageId="3" pageNumber="248">Iran</collectingCountry>
were collected by 
<collectorName id="50185311FF84FFD3FB39F962FABBFDE2" box="[1189,1385,536,559]" pageId="3" pageNumber="248">Aylar Rostami.</collectorName>
