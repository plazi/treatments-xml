<location id="F832601CFF84FFD3FA80FAB5FF23FDC6" LSID="urn:lsid:plazi:treatment:754487D1FF86FFD3FE36FE18FABBFDE2:F832601CFF84FFD3FA80FAB5FF23FDC6" country="Iran" name="Seyyedlar village" pageId="3" pageNumber="248" stateProvince="Azarbayjan-e Sharqi">Seyyedlar village</location>
, 
<date id="89531007FF84FFD3FE9AFA8EFDDEFDC6" box="[262,524,500,523]" pageId="3" pageNumber="248" value="2015-12-11">
<collectingDate id="9917E9EFFF84FFD3FE9AFA8EFDDEFDC6" box="[262,524,500,523]" pageId="3" pageNumber="248" value="2015-12-11">11 December 2015</collectingDate>
</date>
; one male from bark of the apple tree, Sowmaeh-ye Kabudin, 
<date id="89531007FF84FFD3FAE0FA8EFEB4FDE2" pageId="3" pageNumber="248" value="2015-12-19">
<collectingDate id="9917E9EFFF84FFD3FAE0FA8EFEB4FDE2" pageId="3" pageNumber="248" value="2015-12-19">19 December 2015</collectingDate>
