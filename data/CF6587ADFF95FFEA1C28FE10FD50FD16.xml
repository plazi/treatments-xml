<taxonomicName id="80CC4D38FF95FFEF1E93FD0BFB61FCE1" authority="Fabr." authorityName="Fabr." box="[947,1179,767,793]" class="Insecta" family="Mantidae" genus="Popa" kingdom="Animalia" order="Mantodea" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="undata">
