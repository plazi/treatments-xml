<taxonomicName id="80CC4D38FF92FFE81984F8F2FAD0F8E5" box="[1188,1322,1798,1821]" class="Gordioida" family="Chordodidae" genus="Chordodes" kingdom="Animalia" order="Gordioidea" pageId="4" pageNumber="53" phylum="Nematomorpha" rank="species" species="africanus">
