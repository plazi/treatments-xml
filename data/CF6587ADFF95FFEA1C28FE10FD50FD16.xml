<collectingCountry id="3FDB762BFF90FFEA1CF7FDF8FDD9FDDE" box="[471,547,524,550]" name="Sudan" pageId="6" pageNumber="55">Sudan</collectingCountry>
, 
<collectingRegion id="8508F859FF90FFEA1F0FFDF8FD47FDDE" box="[559,701,524,550]" country="Sudan" name="An Nil al Abyad" pageId="6" pageNumber="55">Nilo Blanco</collectingRegion>
(
