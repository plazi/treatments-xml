<normalizedToken id="AA96467C22A720F7EAC1C00423B29A97" originalValue="Rosablühendes" pageId="null" pageNumber="780">Rosabluehendes</normalizedToken>
<normalizedToken id="A1A105CFB49E3CF583ADC2F38B335638" originalValue="Weidenröschen" pageId="null" pageNumber="780">Weidenroeschen</normalizedToken>
</paragraph>
