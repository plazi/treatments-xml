<taxonomicName id="215DB0A767966B795E9BFBF693A66C4F" family="Scelionidae" genus="G." kingdom="Animalia" lsidName="G. canum" order="Hymenoptera" pageId="0" pageNumber="323" rank="species" species="canum">
