(
<figureCitation id="4FB12A74709AFF3BFF57FF18FF1518EA" box="[159,253,189,215]" captionStart="FIGURE 69" captionStartId="144.[151,250,1642,1667]" captionTargetBox="[151,1434,561,1617]" captionTargetId="figure@144.[151,1435,561,1617]" captionTargetPageId="144" captionText="FIGURE 69. Distribution of Austrogymnocnemia species (part)." figureDoi="http://doi.org/10.5281/zenodo.3896956" httpUri="https://zenodo.org/record/3896956/files/figure.png" pageId="153" pageNumber="154">Figs. 69</figureCitation>
, 
<figureCitation id="4FB12A74709AFF3BFEC1FF18FEB918E5" box="[265,337,189,216]" captionStart-0="FIGURE 74" captionStart-1="FIGURE 75" captionStartId-0="154.[151,250,1929,1954]" captionStartId-1="155.[151,250,1643,1668]" captionTargetBox-0="[151,1435,564,1896]" captionTargetBox-1="[151,1428,186,1618]" captionTargetId-0="figure@154.[151,1437,561,1904]" captionTargetId-1="figure@155.[151,1436,181,1618]" captionTargetPageId-0="154" captionTargetPageId-1="155" captionText-0="FIGURE 74. Austrogymnocnemia tatarnici sp. nov.: a) head, anterior; b) head and thorax, dorsal; c) wings, dorsal." captionText-1="FIGURE 75. Austrogymnocnemia tatarnici sp. nov.: male: a) terminalia, lateral; b) genitalia, lateral; c) genitalia, posteroventral; d) 9th sternite and genitalia, ventral. Scale bars = 0.5 mm." figureDoi-0="http://doi.org/10.5281/zenodo.3896966" figureDoi-1="http://doi.org/10.5281/zenodo.3896968" httpUri-0="https://zenodo.org/record/3896966/files/figure.png" httpUri-1="https://zenodo.org/record/3896968/files/figure.png" pageId="153" pageNumber="154">74–75</figureCitation>
)
</paragraph>
<paragraph id="D73536F1709AFF3BFF5FFF44FB9B18C1" blockId="153.[151,1139,153,252]" box="[151,1139,225,252]" pageId="153" pageNumber="154">
