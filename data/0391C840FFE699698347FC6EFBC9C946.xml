<taxonomicName id="4C3802D5FFE699768347FBE2FD31C908" authority="Moulton, 1936: 264" authorityName="Moulton" authorityPageNumber="264" authorityYear="1936" box="[264,723,1073,1097]" class="Insecta" family="Thripidae" genus="Scirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="dobroskyi">
<emphasis id="B94CA544FFE699768347FBE2FE16C909" box="[264,500,1074,1097]" italics="true" pageId="15" pageNumber="16">Scirtothrips dobroskyi</emphasis>
<treatmentCitation id="0A995F47FFE6997683B3FBE1FD31C908" author="Moulton" box="[508,723,1073,1096]" page="264" pageId="15" pageNumber="16" year="1936">
