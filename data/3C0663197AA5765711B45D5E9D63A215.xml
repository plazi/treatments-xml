<taxonomicName id="918C25143EFA5F57FE4B6C8C06425179" LSID="urn:lsid:zoobank.org:act:16D3A62E-1BE8-41B5-9C3B-64B925010BAC" class="Insecta" family="Staphylinidae" genus="Antimerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Antimerus bellus" order="Coleoptera" pageId="21" pageNumber="42" phylum="Arthropoda" rank="species" species="bellus">Antimerus bellus</taxonomicName>
<taxonomicNameLabel id="393567289508799EAC178785A234B259" pageId="21" pageNumber="42">sp. n.</taxonomicNameLabel>
