8. 
<taxonomicName id="76C7D9119D1AF16CC8CFE8A737212F37" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136695" authority="Emery, 1882" authorityName="Emery" authorityYear="1882" class="Insecta" family="Formicidae" genus="Parasyscia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Parasyscia peringueyi Emery" lsidName-HNS="Parasyscia peringueyi Emery" order="Hymenoptera" pageNumber="360" phylum="Arthropoda" rank="species" species="peringueyi">Parasyscia peringueyi</taxonomicName>
