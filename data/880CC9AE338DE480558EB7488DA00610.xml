<taxonomicName id="B8A1883CFD8BDF2F39E493925792F6F0" authority="L. (1753)" authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Cistaceae" genus="Cistus" kingdom="Plantae" order="Malvales" phylum="Tracheophyta" rank="species" species="hirtus">
