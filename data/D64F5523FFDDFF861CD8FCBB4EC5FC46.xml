(
<emphasis id="6C923827FFDDFF861D42FCBB4C79FF52" bold="true" box="[269,367,151,177]" italics="true" pageId="7" pageNumber="101">Vetur ius</emphasis>
) 
<taxonomicName id="99E69FB6FFDDFF861DCCFCBB4FA7FF51" authority="Kuwert, 1891" authorityName="Kuwert" authorityYear="1891" box="[387,689,151,178]" class="Insecta" family="Passallidae" genus="Veturius" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="101" phylum="Arthropoda" rank="species" species="simillimus">
