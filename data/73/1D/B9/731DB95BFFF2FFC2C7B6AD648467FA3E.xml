<taxonomicName authority="Sankolli, 1962" authorityName="Sankolli" authorityYear="1962" box="[450,754,1369,1394]" class="Malacostraca" family="Paguridae" genus="Pagurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="917" phylum="Arthropoda" rank="species" species="kulkarnii">
