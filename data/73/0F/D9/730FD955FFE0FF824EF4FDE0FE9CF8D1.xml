Berlese, 
<number box="[861,919,1585,1609]" pageId="10" pageNumber="32" value="1923.0">1923</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Womersley, 1956" authorityName="Womersley" authorityYear="1956" box="[931,1291,1585,1609]" class="Arachnida" family="Laelapidae" genus="Gymnolaelaps" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="32" phylum="Arthropoda" rank="species" species="australicus">
