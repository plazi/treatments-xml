<taxonomicName authorityName="Gravenhorst" authorityYear="1829" box="[732,948,1583,1625]" class="Insecta" family="Agaonidae" genus="Blastophaga" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="211" phylum="Arthropoda" rank="genus">
