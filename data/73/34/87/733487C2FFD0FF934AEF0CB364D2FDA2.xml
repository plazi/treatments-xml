<number box="[151,163,339,360]" pageId="4" pageNumber="131" value="2.0">2</number>
Body relatively small (
<tableCitation box="[451,526,339,360]" captionStart="TABLE 1" captionStartId="3.[151,239,799,823]" captionText="TABLE 1. Measurements." pageId="4" pageNumber="131">
Table 
<number box="[515,526,339,360]" pageId="4" pageNumber="131" value="1.0">1</number>
</tableCitation>
