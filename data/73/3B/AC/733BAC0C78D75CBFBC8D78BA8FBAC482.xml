<normalizedToken originalValue="“HOLOTYPUS”">&quot;HOLOTYPUS&quot;</normalizedToken>
[red], &quot; 
<taxonomicName class="Insecta" family="Dytiscidae" genus="Copelatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Copelatus" order="Coleoptera" pageId="0" pageNumber="73" phylum="Arthropoda" rank="genus">Copelatus</taxonomicName>
<taxonomicName lsidName="takime" pageId="0" pageNumber="73" rank="species" species="takime">takime</taxonomicName>
