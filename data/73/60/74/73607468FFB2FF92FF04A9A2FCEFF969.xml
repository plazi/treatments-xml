</taxonomicName>
(
<taxonomicName box="[784,878,1688,1714]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="genus">
