<collectingCountry box="[359,426,511,536]" name="China" pageId="6" pageNumber="276">China</collectingCountry>
(
<collectingRegion box="[441,576,511,536]" country="China" name="Guangdong" pageId="6" pageNumber="276">Guangdong</collectingRegion>
, 
