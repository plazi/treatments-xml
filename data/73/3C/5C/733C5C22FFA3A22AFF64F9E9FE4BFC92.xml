<taxonomicName box="[699,817,704,728]" class="Insecta" family="Conopidae" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="subFamily" subFamily="Conopinae">Conopinae</taxonomicName>
<taxonomicName authorityName="Stuke" authorityYear="2019" box="[212,390,736,760]" class="Insecta" family="Conopidae" genus="Gellergrimmellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="307" phylum="Arthropoda" rank="genus">
<emphasis box="[212,390,736,760]" italics="true" pageId="6" pageNumber="307">Gellergrimmellus</emphasis>
