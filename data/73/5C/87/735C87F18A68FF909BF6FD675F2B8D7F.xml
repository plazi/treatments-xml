<taxonomicName baseAuthorityName="Bloch &amp; Schneider" baseAuthorityYear="1801" class="Chondrichthyes" family="Dasyatidae" genus="Urogymnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="62" pageNumber="63" phylum="Chordata" rank="species" species="asperrimus">
