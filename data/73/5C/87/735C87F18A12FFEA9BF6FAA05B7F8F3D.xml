<taxonomicName authority="(Gilchrist, 1922)" authorityName="Gilchrist" authorityYear="1922" baseAuthorityName="Gilchrist" baseAuthorityYear="1922" box="[151,599,1599,1624]" class="Chondrichthyes" family="Chimaeridae" genus="Hydrolagus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chimaeriformes" pageId="68" pageNumber="69" phylum="Chordata" rank="species" species="africanus">
