<taxonomicName baseAuthorityName="Jordan &amp; Snyder" baseAuthorityYear="1904" box="[660,811,1599,1624]" class="Chondrichthyes" family="Chimaeridae" genus="Hydrolagus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chimaeriformes" pageId="68" pageNumber="69" phylum="Chordata" rank="species" species="mitsukurii">
