<emphasis box="[604,895,1410,1435]" italics="true" pageId="62" pageNumber="63">
<taxonomicName authorityName="Muller &amp; Henle" authorityYear="1837" box="[604,727,1412,1435]" class="Chondrichthyes" family="Dasyatidae" genus="Himantura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="62" pageNumber="63" phylum="Chordata" rank="genus">Himantura</taxonomicName>
