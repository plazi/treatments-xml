<emphasis box="[996,1255,1446,1471]" italics="true" pageId="62" pageNumber="63">
<taxonomicName authorityName="Muller &amp; Henle" authorityYear="1837" box="[996,1119,1447,1470]" class="Chondrichthyes" family="Dasyatidae" genus="Himantura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myliobatiformes" pageId="62" pageNumber="63" phylum="Chordata" rank="genus">Himantura</taxonomicName>
