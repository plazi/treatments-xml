<subSubSection box="[199,1128,1573,1598]" pageId="8" pageNumber="227" type="etymology">
<paragraph blockId="8.[151,1437,1501,2030]" box="[199,1128,1573,1598]" pageId="8" pageNumber="227">
<emphasis bold="true" box="[199,326,1573,1598]" pageId="8" pageNumber="227">Etymology</emphasis>
