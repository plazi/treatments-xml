and 
<taxonomicName authorityName="Makino" authorityYear="1914" box="[1108,1312,367,393]" class="Liliopsida" family="Poaceae" genus="Chimonobambusa" kingdom="Plantae" order="Poales" pageId="11" pageNumber="531" phylum="Tracheophyta" rank="genus">
