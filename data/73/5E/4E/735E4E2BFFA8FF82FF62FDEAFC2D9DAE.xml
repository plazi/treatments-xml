</taxonomicName>
, 
<taxonomicName authorityName="Nakai" authorityYear="1925" box="[919,1057,367,393]" class="Liliopsida" family="Poaceae" genus="Pleioblastus" kingdom="Plantae" order="Poales" pageId="11" pageNumber="531" phylum="Tracheophyta" rank="genus">
