, 
<collectingCountry box="[440,521,582,607]" name="Bhutan" pageId="53" pageNumber="54">Bhutan</collectingCountry>
and 
<collectingCountry box="[575,636,582,607]" name="India" pageId="53" pageNumber="54">India</collectingCountry>
