<taxonomicName authorityName="Redtenbacher" authorityYear="1908" box="[792,923,512,535]" class="Insecta" family="Diapheromeridae" genus="Pericentrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phasmida" pageId="55" pageNumber="56" phylum="Arthropoda" rank="genus">
<emphasis box="[792,923,512,535]" italics="true" pageId="55" pageNumber="56">Pericentrus</emphasis>
