<taxonomicName authorityName="Latreille" authorityYear="1802" box="[1113,1275,476,501]" class="Insecta" family="Cerambycidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="family">Cerambycidae</taxonomicName>
