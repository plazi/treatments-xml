</taxonomicName>
<emphasis bold="true" box="[520,610,1794,1819]" pageId="5" pageNumber="6">
<taxonomicNameLabel box="[520,610,1794,1819]" pageId="5" pageNumber="6" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
[
<collectingCountry box="[624,694,1794,1819]" name="China" pageId="5" pageNumber="6">China</collectingCountry>
(
<collectingRegion box="[709,801,1795,1819]" country="China" name="Yunnan" pageId="5" pageNumber="6">Yunnan</collectingRegion>
)] is similar to 
<taxonomicName authority="Ho, 2013" authorityName="Ho" authorityYear="2013" box="[963,1170,1795,1819]" class="Insecta" family="Diapheromeridae" genus="Dianphasma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phasmida" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="cheni">
<emphasis box="[963,983,1795,1818]" italics="true" pageId="5" pageNumber="6">D</emphasis>
. 
<emphasis box="[997,1057,1795,1819]" italics="true" pageId="5" pageNumber="6">cheni</emphasis>
Ho, 2013
</taxonomicName>
[
<collectingCountry box="[1184,1254,1794,1819]" name="China" pageId="5" pageNumber="6">China</collectingCountry>
