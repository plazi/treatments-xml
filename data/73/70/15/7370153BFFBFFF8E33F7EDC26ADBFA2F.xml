<subSubSection pageId="5" pageNumber="6" type="etymology">
<paragraph blockId="5.[151,1437,535,1496]" pageId="5" pageNumber="6">
<emphasis bold="true" box="[199,334,1436,1461]" pageId="5" pageNumber="6">Etymology.</emphasis>
