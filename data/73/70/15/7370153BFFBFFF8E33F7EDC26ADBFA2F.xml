<taxonomicName authority="Chen &amp; He, 1997" authorityName="Chen &amp; He" authorityYear="1997" class="Insecta" family="Diapheromeridae" genus="Dianphasma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phasmida" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="microptera">
