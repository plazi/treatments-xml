</taxonomicName>
, 
<taxonomicName authorityName="* Cockerell" authorityYear="1910" box="[635,950,617,642]" class="Insecta" family="Colletidae" genus="Paracolletes" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="nigroclypeatus">
