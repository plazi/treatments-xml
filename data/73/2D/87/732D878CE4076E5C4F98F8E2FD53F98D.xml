<taxonomicName authorityName="* Cockerell" authorityYear="1905" box="[1267,1429,653,678]" class="Insecta" family="Colletidae" genus="Paracolletes" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="roseoviridis">
