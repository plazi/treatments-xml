from 
<location LSID="urn:lsid:plazi:treatment:731987BCFFDFFFEAFEA026F55F07D7F9:FE6F6071FFDFFFECFDBE24355CE6D598" box="[521,672,875,901]" name="Rainbow falls" pageId="2" pageNumber="453">Rainbow falls</location>
