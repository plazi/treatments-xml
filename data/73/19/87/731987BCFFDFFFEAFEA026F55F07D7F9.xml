<paragraph blockId="2.[151,684,778,965]" box="[151,296,778,804]" pageId="2" pageNumber="453">DESCRIPTION</paragraph>
<paragraph blockId="2.[151,684,778,965]" box="[151,297,810,835]" pageId="2" pageNumber="453">
<emphasis box="[151,297,810,835]" italics="true" pageId="2" pageNumber="453">Measurements</emphasis>
</paragraph>
<paragraph blockId="2.[151,684,778,965]" pageId="2" pageNumber="453">
