(
<location LSID="urn:lsid:plazi:treatment:733A87FA033AFFB9FF2FFAD8FD0616AC:FE4C6037033BFFB9FBC0FCE0FAED1656" box="[1144,1313,848,874]" name="South Central" pageId="21" pageNumber="22" stateProvince="Xizang">South Central</location>
