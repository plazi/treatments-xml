<taxonomicName authority="Trouessart, 1885" authorityName="Trouessart" authorityYear="1885" box="[293,697,1386,1408]" class="Arachnida" family="Xolalgidae" genus="Dubininia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="18" pageNumber="469" phylum="Arthropoda" rank="species" species="psittacina">
