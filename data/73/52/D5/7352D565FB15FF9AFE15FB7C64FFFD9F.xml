, 
<specimenCode box="[975,1136,1405,1429]" collectionCode="WAM" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34981" lsid="urn:lsid:biocol.org:col:34981" name="Western Australian Museum" pageId="39" pageNumber="2447" type="Museum">WAM119.90</specimenCode>
