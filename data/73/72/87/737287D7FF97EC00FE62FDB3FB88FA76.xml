; 
<bibRefCitation author="Neuherz" box="[279,448,1107,1134]" firstAuthor="Neuherz" pageId="46" pageNumber="47" pagination="415 - 427" refId="ref74368" refString="Neuherz H. 1984. Torocampa holzeli n. gen., n. spec. - eine klasobionte Campodeide (Diplura, Apterygota) aus der Hafnerhohle in den Karawanken, Karnten. Carinthia II 174 / 94: 415 - 427." type="journal article" year="1984">Neuherz 1984</bibRefCitation>
) and 
<taxonomicName authorityName="Conde" authorityYear="1955" box="[519,661,1108,1134]" class="Entognatha" family="Campodeidae" genus="Plusiocampa" kingdom="Animalia" order="Diplura" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="fagei" subGenus="Plusiocampa">
