<emphasis box="[357,380,1073,1098]" italics="true" pageId="46" pageNumber="47">P.</emphasis>
(
<emphasis box="[397,416,1073,1098]" italics="true" pageId="46" pageNumber="47">P.</emphasis>
) 
<emphasis box="[438,543,1072,1098]" italics="true" pageId="46" pageNumber="47">strouhali</emphasis>
</taxonomicName>
, 
