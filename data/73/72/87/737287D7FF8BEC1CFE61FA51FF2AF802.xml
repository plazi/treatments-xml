(
<bibRefCitation author="Silvestri" box="[325,509,1660,1686]" firstAuthor="Silvestri" pageId="50" pageNumber="51" pagination="1 - 4" refId="ref76360" refString="Silvestri F. 1933 b. Descrizione di una nuova specie cavernicola di Campodeidae (Thysanura Entothropha) del Trentino. Bollettino del Laboratorio di Entomologia agraria de Bologna 6: 1 - 4." type="journal article" year="1933" yearSuffix="b">Silvestri 1933b</bibRefCitation>
; 
<bibRefCitation author="Conde" box="[521,678,1660,1687]" firstAuthor="Conde" pageId="50" pageNumber="51" pagination="644 - 649" refId="ref72311" refString="Conde B. 1954 a. Ordnung: Entotropha (Diplura). In: Franz H. (ed.) Der Nordost im Spiegel ihrer Landtierwelt: 644 - 649. Universitatsverlag Wagner, Innsbruck, Austria." type="book chapter" year="1954" yearSuffix="a">Condé 1954a</bibRefCitation>
