<collectingRegion country="Spain" name="Illes Baleares" pageId="36" pageNumber="37">Balearic Islands</collectingRegion>
. 
<taxonomicName authorityName="Conde" authorityYear="1955" box="[292,639,1283,1310]" class="Entognatha" family="Campodeidae" genus="Plusiocampa" kingdom="Animalia" order="Diplura" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="breuili" subGenus="Plusiocampa">
