<taxonomicName authority="Sendra &amp; Delic" authorityName="Sendra &amp; Antić &amp; Barranco &amp; Borko &amp; Christian &amp; Delić &amp; Fadrique &amp; Faille &amp; Galli &amp; Gasparo &amp; Georgiev &amp; Giachino &amp; Lukić &amp; Marcia &amp; Miculinić &amp; Nicolosi &amp; Palero &amp; Paragamian &amp; Pérez &amp; Polak &amp; Prieto &amp; Turbanov &amp; Vailati &amp; Reboleira" authorityYear="2020" box="[361,964,1895,1922]" class="Entognatha" family="Campodeidae" genus="Plusiocampa" kingdom="Animalia" order="Diplura" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="ferrani" status="sp. nov." subGenus="Venetocampa">
