<collectingCountry box="[1061,1170,619,645]" name="Myanmar" pageId="4" pageNumber="137">Myanmar</collectingCountry>
(
<collectingCountry box="[1185,1268,619,645]" name="Myanmar" pageId="4" pageNumber="137">Burma</collectingCountry>
