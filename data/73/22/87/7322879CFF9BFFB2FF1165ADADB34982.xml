<taxonomicName authority="Lubbock 1980" authorityName="Lubbock" authorityYear="1980" box="[160,518,1641,1665]" class="Actinopterygii" family="Pseudochromidae" genus="Pseudochromis" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="59" phylum="Chordata" rank="species" species="andamanensis">
