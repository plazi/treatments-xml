<number box="[795,869,1783,1807]" pageId="6" pageNumber="59" value="24.0" valueMax="25.0" valueMin="23.0">23–25</number>
), 
<number box="[888,916,1783,1807]" pageId="6" pageNumber="59" value="25.0">25</number>
