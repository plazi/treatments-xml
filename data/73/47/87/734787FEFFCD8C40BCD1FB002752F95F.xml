<taxonomicName authority="Golubev &amp; Szczerbak" authorityName="Golubev &amp; Szczerbak" authorityYear="1984" box="[729,1144,1297,1321]" class="Reptilia" family="Gekkonidae" genus="Asiocolotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="317" phylum="Chordata" rank="species" species="levitoni">
<emphasis box="[729,850,1297,1321]" italics="true" pageId="16" pageNumber="317">A. levitoni</emphasis>
(Golubev &amp; Szczerbak)
</taxonomicName>
, 
<taxonomicName authority="Golubev &amp; Szczerbak" authorityName="Golubev &amp; Szczerbak" class="Reptilia" family="Gekkonidae" genus="Cyrtodactylus" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="317" phylum="Chordata" rank="species" species="mintoni">
<emphasis box="[1160,1282,1297,1321]" italics="true" pageId="16" pageNumber="317">A. mintoni</emphasis>
(Golubev &amp; Szczerbak)
</taxonomicName>
, 
<taxonomicName authority="Steindachner" authorityName="Steindachner" box="[300,633,1333,1357]" genus="Altigekko" kingdom="Animalia" pageId="16" pageNumber="317" rank="species" species="stoliczkai">
<emphasis box="[300,448,1333,1357]" italics="true" pageId="16" pageNumber="317">A. stoliczkai</emphasis>
(Steindachner)
</taxonomicName>
, 
<taxonomicName authority="Yeriomtschenko &amp; Szczerbak" authorityName="Yeriomtschenko &amp; Szczerbak" box="[654,1207,1333,1357]" class="Reptilia" family="Gekkonidae" genus="Altiphylax" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="317" phylum="Chordata" rank="species" species="tokobajevi">
