<paragraph blockId="18.[151,1436,1874,2006]" box="[151,535,1874,1900]" pageId="18" pageNumber="319">
<heading bold="true" box="[151,535,1874,1900]" fontSize="11" level="1" pageId="18" pageNumber="319" reason="1">
<emphasis bold="true" box="[151,535,1874,1900]" pageId="18" pageNumber="319">
