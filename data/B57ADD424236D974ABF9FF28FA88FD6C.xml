<materialsCitation id="8DBB66094236D974AF01FD73FA5EFD48" ID-GBIF-Occurrence="1915525736" country="Mexico" location="San Luis Potosi" pageId="14" pageNumber="203" specimenCount="1" stateProvince="San Luis Potosi">
<collectingCountry id="45C42CC44236D974AF01FD73FB03FD14" box="[1135,1220,646,671]" name="Mexico" pageId="14" pageNumber="203">Mexico</collectingCountry>
