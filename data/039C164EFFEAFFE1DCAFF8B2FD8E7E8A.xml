<paragraph id="8B8AA758FFEAFFE6DCAFF8B2FD807FA0" blockId="107.[157,538,1445,1468]" box="[157,538,1445,1468]" pageId="107" pageNumber="108">
<heading id="D0C21034FFEAFFE6DCAFF8B2FD807FA0" box="[157,538,1445,1468]" centered="true" fontSize="9" level="2" pageId="107" pageNumber="108" reason="2">
<taxonomicName id="4C35DCDBFFEAFFE6DCAFF8B2FE1E7FA0" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[157,388,1445,1468]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="asae" status="new species">
<emphasis id="B9417B4AFFEAFFE6DCAFF8B2FE1E7FA0" bold="true" box="[157,388,1445,1468]" italics="true" pageId="107" pageNumber="108">Paramblynotus asae</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFEAFFE6DDA6F8B0FD807FA0" box="[404,538,1446,1468]" pageId="107" pageNumber="108" rank="species">new species</taxonomicNameLabel>
