<emphasis id="88AA3D77FFCEFF888CC0FA1E20CAFA00" bold="true" box="[209,242,1434,1455]" pageId="33" pageNumber="34">28.</emphasis>
<taxonomicName id="7DDE9AE6FFCEFF888CEAFA1F21A4FA1F" authorityName="Milne-Edwards &amp; A. Grandidier" authorityYear="1872" box="[251,412,1435,1456]" class="Mammalia" family="Tenrecidae" genus="Geogale" kingdom="Animalia" order="Afrosoricida" pageId="33" pageNumber="34" phylum="Chordata" rank="species" species="aurita">
