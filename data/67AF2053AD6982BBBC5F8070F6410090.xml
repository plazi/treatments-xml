(Erichson, 1842), 
<taxonomicName id="9C488EB5D8FBEA222AD0039CB82C9D98" authority="Motschoulsky, 1860" authorityName="Motschoulsky" authorityYear="1860" baseAuthorityName="Chapuis" baseAuthorityYear="1877" class="Hexapoda" family="Chrysomelidae" genus="Paropsisterna" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paropsisterna variicollis" order="Hymenoptera" pageId="6" pageNumber="147" phylum="Arthropoda" rank="species" species="variicollis">
<emphasis id="883E237185C3673B4C8E33E8C4B31394" italics="true" pageId="6" pageNumber="147">Paropsisterna variicollis</emphasis>
</taxonomicName>
*, 
<taxonomicName id="022F52C407096599C4A333F6D27835F5" authority="Motschoulsky, 1860" authorityName="Reid &amp; de Little" authorityYear="2013" class="Hexapoda" family="Chrysomelidae" genus="Paropsisterna" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paropsisterna selmani" order="Hymenoptera" pageId="6" pageNumber="147" phylum="Arthropoda" rank="species" species="selmani">
<emphasis id="C311F28E07BD830809D3CE7675C65CED" italics="true" pageId="6" pageNumber="147">Paropsisterna selmani</emphasis>
