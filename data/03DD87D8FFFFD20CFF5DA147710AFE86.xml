</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="4C744D4DFFFFD20DFC0CA147714DF890" authority="Karanovic &amp; Sitnikova, 2017" authorityName="Karanovic &amp; Sitnikova" authorityYear="2017" class="Ostracoda" family="Candonidae" genus="Mazepovacandona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
