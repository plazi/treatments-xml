<typeStatus id="D030882CFF9D6C436C12F916FD8AF913" box="[526,639,1764,1788]" pageId="23" pageNumber="22">paratype</typeStatus>
, 
<quantity id="C8739B6BFF9D6C436C96F911FD05F914" box="[650,752,1763,1787]" metricMagnitude="-3" metricUnit="m" metricValue="1.63" pageId="23" pageNumber="22" unit="mm" value="1.63">1.63mm</quantity>
, otherwise very similar to 
<typeStatus id="D030882CFF9D6C436A33F911FB69F914" box="[1071,1180,1763,1787]" pageId="23" pageNumber="22">holotype</typeStatus>
