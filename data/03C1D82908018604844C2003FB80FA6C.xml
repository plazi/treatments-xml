Male (
<typeStatus id="54D3D79D0806860485CD25EBFE97FB01" box="[235,340,1218,1243]" pageId="14" pageNumber="2084">paratype</typeStatus>
) (
<figureCitation id="135375BA08068604844B25EBFE1AFB01" box="[365,473,1218,1243]" captionStart="Figure 6" captionStartId="13.[160,225,1713,1736]" captionTargetBox="[165,1153,155,1687]" captionTargetId="figure-1@13.[164,1153,142,1690]" captionTargetPageId="13" captionText="Figure 6. Leucospis arabica sp. nov., paratype (male). (a) Habitus, lateral view; (b,c) head, frontal view; (d) head and mesosoma, dorsal view." figureDoi="http://doi.org/10.5281/zenodo.5177134" httpUri="https://zenodo.org/record/5177134/files/figure.png" pageId="14" pageNumber="2084">Figures 6</figureCitation>
and 
