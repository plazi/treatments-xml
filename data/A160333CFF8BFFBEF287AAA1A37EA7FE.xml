<taxonomicName id="EEC9F9A9FF8BFFBEF287A93BA04AA138" authority="Lea, 1928: 128" authorityName="Lea" authorityPageNumber="128" authorityYear="1928" box="[151,538,582,604]" class="Insecta" family="Curculionidae" genus="Melanterius" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="latipennis">
