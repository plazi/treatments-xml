<paragraph id="337BCF59CD71383E44C15939FCA17913" blockId="1.[151,1437,1596,2033]" pageId="1" pageNumber="65">
<emphasis id="01B0134BCD71383E44C15939FE8E7939" bold="true" box="[151,275,1596,1622]" pageId="1" pageNumber="65">Material:</emphasis>
<collectingCountry id="4BD38FC9CD71383E454A5938FE127938" box="[284,399,1597,1623]" name="Germany" pageId="1" pageNumber="65">Germany</collectingCountry>
, Hesse, Kellerwald-Edersee National Park, 
<quantity id="F43C62BCCD71383E47FB5938FC677938" box="[941,1018,1597,1623]" metricMagnitude="2" metricUnit="m" metricValue="4.25" pageId="1" pageNumber="65" unit="m" value="425.0">425 m</quantity>
