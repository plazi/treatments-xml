<taxonomicName id="4C56A1D5FF96D567C8C3F8EC05055775" authority="" box="[463,806,1893,1916]" class="Insecta" family="Cicadellidae" genus="Goniagnathus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="55" phylum="Arthropoda" rank="subGenus" subGenus="Tropicognathus">
