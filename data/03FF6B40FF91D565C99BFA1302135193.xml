</taxonomicName>
: 
<taxonomicName id="4C56A1D5FF96D567CA38F8EC05FC5775" authorityName="Viraktamath &amp; Gnaneswaran" authorityYear="2009" box="[820,991,1893,1916]" class="Insecta" family="Cicadellidae" genus="Goniagnathus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="55" phylum="Arthropoda" rank="species" species="anufrievi" status="sp. nov." subGenus="Tropicognathus">
<emphasis id="B9220644FF96D567CA38F8EC05FC5775" box="[820,991,1893,1916]" italics="true" pageId="4" pageNumber="55">G. (T.) anufrievi</emphasis>
</taxonomicName>
<emphasis id="B9220644FF96D567CAEBF8ED021E5775" bold="true" box="[999,1085,1892,1916]" pageId="4" pageNumber="55">
<taxonomicNameLabel id="A211BB3FFF96D567CAEBF8ED021E5775" box="[999,1085,1892,1916]" pageId="4" pageNumber="55" rank="species">sp. nov.</taxonomicNameLabel>
