<taxonomicName id="45CF4D788B2AD173FECCFCC83C8EFC91" authority="Nielsen, 1984" authorityName="Nielsen" authorityYear="1984" box="[336,644,821,847]" class="Actinopterygii" family="Aphyonidae" genus="Barathronus" kingdom="Animalia" order="Ophidiiformes" pageId="1" pageNumber="232" phylum="Chordata" rank="species" species="multidens">
<emphasis id="B0BBEAE98B2AD173FECCFCC83FD7FC91" box="[336,477,821,847]" italics="true" pageId="1" pageNumber="232">B. multidens</emphasis>
