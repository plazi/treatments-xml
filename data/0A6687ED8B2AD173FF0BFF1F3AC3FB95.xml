<taxonomicName id="45CF4D788B2AD173FB08FC803F2AFC65" authority="Nielsen &amp; Eagle, 1974" authorityName="Nielsen &amp; Eagle" authorityYear="1974" class="Actinopterygii" family="Aphyonidae" genus="Barathronus" kingdom="Animalia" order="Ophidiiformes" pageId="1" pageNumber="232" phylum="Chordata" rank="species" species="pacificus">
