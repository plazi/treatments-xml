<taxonomicName id="F6C12C64A3A665AB74E3886302981101" authority="L." class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="74" phylum="Tracheophyta" rank="species" species="aconitifolius">
Ranunculus 
<normalizedToken id="7287D73F9F12218916BC70CA6C91A99D" originalValue="aconitifólius" pageId="null" pageNumber="74">aconitifolius</normalizedToken>
