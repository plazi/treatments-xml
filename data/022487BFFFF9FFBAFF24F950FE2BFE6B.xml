<geoCoordinate id="EFB9506EFFF8FFBAFC34FEC7FC4DFEA3" box="[903,1019,294,321]" degrees="150" direction="east" minutes="0.4" orientation="longitude" pageId="37" pageNumber="238" precision="92" value="150.00667">150°0.4’E</geoCoordinate>
