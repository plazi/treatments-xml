<paragraph id="35ACA88A563AE296A1E14041A3E250F8" pageId="42" pageNumber="43">
<taxonomicName id="D9EFE48DE0331DCD52D89B78ED6FADAC" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole" order="Hymenoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">Pheidole</taxonomicName>
