<subSubSection id="46B383B8FFC96869752B6C4CFC65FE25" pageId="11" pageNumber="183" type="diagnosis">
<paragraph id="0E16D033FFC96869752B6C4CFC65FE25" blockId="11.[816,1444,303,1902]" pageId="11" pageNumber="183">
<emphasis id="3CDD0C21FFC96869752B6C4CFC01FEC5" bold="true" box="[854,980,334,355]" italics="true" pageId="11" pageNumber="183">Diagnosis</emphasis>
