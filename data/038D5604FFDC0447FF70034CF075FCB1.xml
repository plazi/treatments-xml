Class 
<taxonomicName id="4C249C91FFDC0447FF33034CF674FEAC" authority="Owen, 1846" authorityName="Owen" authorityYear="1846" box="[249,573,279,305]" class="Acanthodii" higherTaxonomySource="GBIF" kingdom="Animalia" pageId="3" pageNumber="472" phylum="Chordata" rank="class">ACANTHODII Owen, 1846</taxonomicName>
