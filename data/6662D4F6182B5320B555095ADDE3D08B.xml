<paragraph id="900DD3D8558A1192C48ABC9B742C60CB" pageId="0" pageNumber="73">
Three specimens 
<quantity id="D7D0F5C25AB54BF1EF5F6CFFA86CED19" metricMagnitude="-2" metricUnit="m" metricValue="2.85" metricValueMax="4.5" metricValueMin="1.2" unit="mm" value="28.5" valueMax="45.0" valueMin="12.0">12-45 mm</quantity>
