<taxonomicName id="4C5CE69BFF8DAB72FED9F854FDA5F830" authority="Hutchinson &amp; Allsopp, 2020" authorityName="Hutchinson &amp; Allsopp" authorityYear="2020" box="[348,552,1961,1985]" class="Insecta" family="Dynastidae" genus="Cavonus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="456" phylum="Arthropoda" rank="species" species="fovealacus" status="new species">
<emphasis id="B928410AFF8DAB72FED9F854FDA5F830" box="[348,552,1961,1985]" italics="true" pageId="7" pageNumber="456">Cavonus fovealacus</emphasis>
</taxonomicName>
<emphasis id="B928410AFF8DAB72FDB5F854FD23F830" bold="true" box="[560,686,1961,1985]" pageId="7" pageNumber="456">
