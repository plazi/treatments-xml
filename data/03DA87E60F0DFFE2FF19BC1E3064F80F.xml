<paragraph id="8BCC36F00F0DFFE2FF19BC1E3676F9E0" blockId="55.[151,602,1593,1620]" box="[151,602,1593,1620]" pageId="55" pageNumber="56">
<heading id="D084819C0F0DFFE2FF19BC1E3676F9E0" bold="true" box="[151,602,1593,1620]" fontSize="11" level="1" pageId="55" pageNumber="56" reason="1">
<emphasis id="B907EAE20F0DFFE2FF19BC1E3676F9E0" bold="true" box="[151,602,1593,1620]" pageId="55" pageNumber="56">
<taxonomicName id="4C734D730F0DFFE2FF19BC1E3584F9E7" baseAuthorityName="Bonatto in Bonatto &amp; Marinoni" baseAuthorityYear="2005" box="[151,424,1593,1620]" class="Insecta" family="Calliphoridae" genus="Mesembrinella" kingdom="Animalia" order="Diptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="bolivar">
<emphasis id="B907EAE20F0DFFE2FF19BC1E3584F9E7" bold="true" box="[151,424,1593,1620]" italics="true" pageId="55" pageNumber="56">Mesembrinella bolivar</emphasis>
