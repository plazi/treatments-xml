<taxonomicName id="4C3C69E0376322221A578BC8FDB046D4" authority="Grave &amp; Arjun &amp; Raghavan, 2018" authorityName="Grave &amp; Arjun &amp; Raghavan" authorityYear="2018" box="[297,526,222,244]" class="Malacostraca" family="Euryrhynchidae" genus="Eurindicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="369" phylum="Arthropoda" rank="species" species="bhugarbha" status="sp. nov.">
