<subSubSection id="C30529FAA809FFF24C4AF7D432121C4D" lastPageId="3" lastPageNumber="110" pageId="1" pageNumber="108" type="description">
<paragraph id="8BA07A71A809FFF04C4AF7D432D81725" blockId="1.[825,1464,2049,2125]" pageId="1" pageNumber="108">
<quantity id="4CE7D794A809FFF04C4AF7D435A11758" box="[825,910,2075,2096]" metricMagnitude="-1" metricUnit="m" metricValue="1.22" pageId="1" pageNumber="108" unit="mm" value="122.0">122 mm</quantity>
