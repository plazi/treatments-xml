<taxonomicName id="4C494D1BFF9BF461FF4DF92805D1C2DD" authorityName="Newman" authorityYear="1834" box="[227,422,1765,1790]" class="Insecta" family="Ceratopogonidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="410" phylum="Arthropoda" rank="family">Ceratopogonidae</taxonomicName>
