</taxonomicName>
(
<taxonomicName id="03C8C4D3E46FFF83FE8FF9290CACB59F" authorityName="Hull" authorityYear="1949" box="[279,405,1741,1763]" class="Insecta" family="Syrphidae" genus="Atylobaccha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
