<taxonomicName id="4C614D434972FFF9FE9C3F25DEE92055" box="[350,418,826,851]" class="Malacostraca" genus="Robustochelia" kingdom="Animalia" order="Tanaidacea" pageId="12" pageNumber="31" phylum="Arthropoda" rank="species" species="virilis">
