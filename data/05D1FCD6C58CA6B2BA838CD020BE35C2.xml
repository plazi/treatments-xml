<taxonomicName id="EB24CC073FD8C33EE1BD518852E2EBD2" class="Hexapoda" family="Carabidae" genus="Anchomenus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="761" pageNumber="762" phylum="Arthropoda" rank="species" species="thoracicum">
