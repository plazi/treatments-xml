(
<number id="3DDE5A5EFFAEFFB182F3F8D4C1EE232E" box="[681,697,1831,1856]" pageId="6" pageNumber="107" value="6.0">6</number>
), 
<number id="3DDE5A5EFFAEFFB18294F8D4C1BD232E" box="[718,746,1831,1856]" pageId="6" pageNumber="107" value="50.0">50</number>
(
<number id="3DDE5A5EFFAEFFB182A1F8D4C05D232E" box="[763,778,1831,1856]" pageId="6" pageNumber="107" value="5.0">5</number>
), 
<number id="3DDE5A5EFFAEFFB1837AF8D4C06B232E" box="[800,828,1831,1856]" pageId="6" pageNumber="107" value="51.0">51</number>
(
<number id="3DDE5A5EFFAEFFB18317F8DBC00E232E" box="[845,857,1832,1856]" pageId="6" pageNumber="107" value="1.0">1</number>
), 
<number id="3DDE5A5EFFAEFFB1832BF8D4C0DA232E" box="[881,909,1831,1856]" pageId="6" pageNumber="107" value="52.0">52</number>
(
<number id="3DDE5A5EFFAEFFB183C5F8DBC0FC232E" box="[927,939,1832,1856]" pageId="6" pageNumber="107" value="1.0">1</number>
), 
<number id="3DDE5A5EFFAEFFB18398F8D4C089232E" box="[962,990,1831,1856]" pageId="6" pageNumber="107" value="53.0">53</number>
(
<number id="3DDE5A5EFFAEFFB183B4F8DBC0AA232E" box="[1006,1021,1832,1856]" pageId="6" pageNumber="107" value="2.0">2</number>
) or 
<number id="3DDE5A5EFFAEFFB18471F8D4C710232E" box="[1067,1095,1831,1856]" pageId="6" pageNumber="107" value="55.0">55</number>
