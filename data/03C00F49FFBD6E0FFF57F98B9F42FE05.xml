<taxonomicName id="4C69C5DCFFBD6E0EFD8CF8789F94F851" authority="Jaschhof &amp; Jaschhof, 2020" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2020" box="[588,704,1987,2013]" class="Insecta" family="Cecidomyiidae" genus="Winnertzia" kingdom="Animalia" order="Diptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="bicolor" status="sp. nov.">
