<number id="EA0E248F8A63FFEB8BF1FA5175ADFA1A" box="[420,451,1487,1513]" pageId="11" pageNumber="12" value="34.0">34</number>
°00.1' E
</geoCoordinate>
, 
<date id="287810688A63FFEB8878FA5176A9FA1A" box="[557,711,1487,1513]" pageId="11" pageNumber="12" value="1998-07-18">
<number id="EA0E248F8A63FFEB8878FA517624FA1A" box="[557,586,1487,1513]" pageId="11" pageNumber="12" value="18.0">18</number>
July 
<quantity id="9B3E9B4D8A63FFEB88DFFA5176A9FA1A" box="[650,711,1487,1513]" metricMagnitude="6" metricUnit="m" metricValue="1.99812" pageId="11" pageNumber="12" unit="m" value="1998120.0">1998</quantity>
</date>
, 120 m, mud, 
<specimenCount id="4AC0FD218A63FFEB893AFA51707FFA1A" box="[879,1041,1487,1513]" pageId="11" pageNumber="12" type="generic">
