<taxonomicName id="13B121BEE91B2CD6BFFBA5A1261D8241" class="Sordariomycetes" family="Nectriaceae" genus="Thelonectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Thelonectria" order="Hypocreales" pageId="0" pageNumber="101" phylum="Ascomycota" rank="genus">
