<taxonomicName id="067799F176682137271C6014C9EAC09E" class="Sordariomycetes" family="Nectriaceae" genus="Neonectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Neonectria" order="Hypocreales" pageId="0" pageNumber="101" phylum="Ascomycota" rank="genus">
