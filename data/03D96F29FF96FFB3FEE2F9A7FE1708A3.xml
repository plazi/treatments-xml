<taxonomicName id="4C70A5BCFF96FFB3FDDDF9D7FD44089C" authorityName="Samouelle" authorityYear="1819" box="[567,663,1643,1665]" class="Malacostraca" family="Majidae" kingdom="Animalia" order="Decapoda" pageId="51" pageNumber="496" phylum="Arthropoda" rank="family">Majidae</taxonomicName>
