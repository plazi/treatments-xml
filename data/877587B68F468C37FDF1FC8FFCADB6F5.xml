<taxonomicName id="C8DC4D238F468C37FEE7FCC4FDF3B1EF" authority="O'Keefe" authorityName="O'Keefe" authorityYear="2000" box="[268,606,888,909]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="63" phylum="Arthropoda" rank="species" species="araguaensis">
<emphasis id="3DA8EAB28F468C37FEE7FCC4FE56B1EF" box="[268,507,888,909]" italics="true" pageId="3" pageNumber="63">Euconnus araguaensis</emphasis>
<materialsCitation id="BFB43CFD8F468C37FDEAFCC4FDF3B1EF" box="[513,606,888,909]" country="Venezuela" location="Etymology" municipality="The" pageId="3" pageNumber="63" specimenCount="1" stateProvince="Aragua">O’Keefe</materialsCitation>
</taxonomicName>
, 
<emphasis id="3DA8EAB28F468C37FD82FCCBFD4CB1EE" bold="true" box="[617,737,887,908]" pageId="3" pageNumber="63">new name,</emphasis>
for 
<treatmentCitation id="8E7D10B18F468C37FCE6FCC4FB8DB1EF" author="Franz" box="[781,1056,888,909]" page="49" pageId="3" pageNumber="63" year="1989">
