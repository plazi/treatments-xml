<taxonomicName id="25C64D7DFF9D8711FD6FF952FC28F8ED" box="[746,898,1787,1809]" class="Insecta" family="Miridae" genus="Kohnometopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="436" phylum="Arthropoda" rank="genus">Kohnometopus</taxonomicName>
