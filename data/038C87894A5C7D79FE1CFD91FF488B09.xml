<collectingCountry id="F332760F4A5C7D79FBC4FCB4FBC28CC9" box="[1041,1123,890,914]" name="Tunisia" pageId="8" pageNumber="11">Tunisia</collectingCountry>
; 
<collectingCountry id="F332760F4A5C7D79FF42FC54FF458CE9" box="[151,228,922,946]" name="Cyprus" pageId="8" pageNumber="11">Cyprus</collectingCountry>
