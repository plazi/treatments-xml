</taxonomicName>
(
<taxonomicName id="4C5A4D69FF2CFF292E89FBFEFD2EFBED" box="[1017,1148,1024,1047]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="248" pageNumber="249" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
