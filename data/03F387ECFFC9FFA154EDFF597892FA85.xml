<subSubSection id="C3406571FFC9FFA154BDFB387892FA85" box="[199,1173,1270,1295]" pageId="48" pageNumber="49" type="etymology">
<paragraph id="8BE536FAFFC9FFA154BDFB387892FA85" blockId="48.[151,1437,258,1295]" box="[199,1173,1270,1295]" pageId="48" pageNumber="49">
