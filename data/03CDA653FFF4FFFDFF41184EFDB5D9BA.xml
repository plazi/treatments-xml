<taxonomicName id="4C646CC6FFF4FFFDFF17184FFDCADB5E" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[193,567,1122,1149]" class="Gastropoda" family="Planorbidae" genus="Armiger" kingdom="Animalia" order="Hygrophila" pageId="29" pageNumber="30" phylum="Mollusca" rank="species" species="crista">
