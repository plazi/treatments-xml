<number id="5317F710BC4185328750B07C11057F5F" box="[807,821,1876,1902]" pageId="18" pageNumber="29" value="5.0">5</number>
<taxonomicName id="22DF9EB4BC4185328749B07C11E47F5C" box="[830,980,1876,1901]" class="Arachnida" family="Erythraeidae" genus="Callidosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="18" pageNumber="29" phylum="Arthropoda" rank="genus">
