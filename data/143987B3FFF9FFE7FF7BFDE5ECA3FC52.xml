<taxonomicName id="5B904D26FFF9FFE7FF02FDE5ECB7FD10" authority="Luter and Worheide, 2003" authorityName="Luter and Worheide" authorityYear="2003" box="[238,704,650,676]" class="Rhynchonellata" family="Thecideidae" genus="Ospreyella" kingdom="Animalia" order="Thecideida" pageId="11" pageNumber="412" phylum="Brachiopoda" rank="genus">
<emphasis id="AEE4EAB7FFF9FFE7FF02FDE5EF18FD10" bold="true" box="[238,367,650,676]" italics="true" pageId="11" pageNumber="412">Ospreyella</emphasis>
Lüter and Wörheide, 
<number id="2A582482FFF9FFE7FD6AFDE5ECB7FD10" box="[646,704,650,676]" pageId="11" pageNumber="412" value="2003.0">2003</number>
