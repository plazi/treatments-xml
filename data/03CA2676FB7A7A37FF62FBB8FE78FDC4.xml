<subSubSection id="C379C4EBFB7A7A34FF62FAA1FC81FAA9" box="[151,868,1339,1362]" pageId="1" pageNumber="2" type="materials_examined">
<paragraph id="8BDC9760FB7A7A34FF62FAA1FC81FAA9" blockId="1.[151,1436,1125,1362]" box="[151,868,1339,1362]" pageId="1" pageNumber="2">
<typeStatus id="54D829C2FB7A7A34FF62FAA1FF2DFAAA" box="[151,200,1341,1362]" pageId="1" pageNumber="2">Type</typeStatus>
species: 
<taxonomicName id="4C63ECE3FB7A7A34FED3FAA1FD8CFAA9" authority="Fabricius, 1805" authorityName="Fabricius" authorityYear="1805" box="[294,617,1339,1362]" class="Insecta" family="Sciomyzidae" genus="Dictya" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="clavipes">
