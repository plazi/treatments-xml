<quantity id="668CCA0CDBD50707F2FEEBA3E1E5784E" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.5" metricValueMin="0.5" unit="mm" value="10.0" valueMax="15.0" valueMin="5.0">5-15 mm</quantity>
