: 
<taxonomicName id="4C63EFE7FFC3FF90842D12A5C00CB3FA" authority="Burmeister, 1838" authorityName="Burmeister" authorityYear="1838" box="[314,771,1228,1253]" class="Insecta" family="Blattidae" genus="Polyzosteria" kingdom="Animalia" order="Blattodea" pageId="29" pageNumber="330" phylum="Arthropoda" rank="species" species="orientalis">
