from posterior end. Vestibule 
<number id="7B4FA847FF946356FE0EFC24FE03FC36" box="[442,472,936,962]" pageId="22" pageNumber="23" value="50.0">50</number>
long, sphincter 
<number id="7B4FA847FF946356FD2DFC24FD6DFC36" box="[665,694,936,962]" pageId="22" pageNumber="23" value="35.0">35</number>
long and 
<number id="7B4FA847FF946356FC9BFC24FC96FC36" box="[815,845,936,962]" pageId="22" pageNumber="23" value="33.0">33</number>
wide. Uterus 
<number id="7B4FA847FF946356FC42FC24FBF9FC36" box="[1014,1058,936,962]" pageId="22" pageNumber="23" value="350.0">350</number>
long, containing 
<specimenCount id="DB8171E9FF946356FB41FC24FA9EFC36" box="[1269,1349,936,962]" pageId="22" pageNumber="23" type="egg">
<number id="7B4FA847FF946356FB41FC24FADFFC36" box="[1269,1284,936,962]" pageId="22" pageNumber="23" value="5.0">5</number>
