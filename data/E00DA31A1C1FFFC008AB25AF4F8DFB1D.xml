<typeStatus id="B71FACAE1C1FFFC009BD27344CABFD24" box="[385,489,633,659]" pageId="26" pageNumber="27">holotype</typeStatus>
+ 
<number id="DE6C002B1C1FFFC00A3427344F55FD24" box="[520,535,633,659]" pageId="26" pageNumber="27" value="1.0">1</number>
