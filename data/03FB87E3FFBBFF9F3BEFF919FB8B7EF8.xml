<taxonomicName id="4C524D76FFBCFF9F3823FE66FC457D89" authority="" authorityName="" box="[859,942,476,501]" class="Insecta" family="Platypezidae" genus="Clythia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
