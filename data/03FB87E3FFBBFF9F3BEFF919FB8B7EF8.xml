<taxonomicName id="4C524D76FFBCFF9F388AFE2EFBAE7DD1" authority="" authorityName="" box="[1010,1093,404,429]" class="Insecta" family="Platypezidae" genus="Clythia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
