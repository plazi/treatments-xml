<taxonomicName id="F9DB4D277C11FF85FE84AAFB979AF85F" authority="(Bristowe, 1938)" baseAuthorityName="Bristowe" baseAuthorityYear="1938" box="[285,767,1958,1984]" class="Arachnida" family="Filistatidae" genus="Labahitha" kingdom="Animalia" order="Araneae" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="oonopiformis">
