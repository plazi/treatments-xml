: 
<specimenCount id="A68426B397699C13FEDDFC40FE7903A4" box="[332,415,938,964]" pageId="21" pageNumber="22" type="male">1 male</specimenCount>
, 
<specimenCount id="A68426B397699C13FE3BFC40FDF803A4" box="[426,542,938,964]" pageId="21" pageNumber="22" type="female">3 females</specimenCount>
