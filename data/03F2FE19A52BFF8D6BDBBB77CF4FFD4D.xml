<heading id="D0ACF863A52BFF8C6BDBBB77C8B0FA91" box="[165,738,1314,1336]" centered="true" fontSize="9" level="2" pageId="62" pageNumber="161" reason="4">
<taxonomicName id="4C5B348CA52BFF8C6BDBBB77C821FA9E" authority="THOMPSON" authorityName="THOMPSON" authorityYear="2012" box="[165,627,1314,1336]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="62" pageNumber="161" phylum="Arthropoda" rank="species" species="schlingeri" status="SP. NOV.">
<emphasis id="B92F931DA52BFF8C6BDBBB77CBB9FA9E" bold="true" box="[165,491,1314,1335]" italics="true" pageId="62" pageNumber="161">CITROGRAMMA SCHLINGERI</emphasis>
THOMPSON
</taxonomicName>
<emphasis id="B92F931DA52BFF8C6903BB73C8B0FA91" bold="true" box="[637,738,1315,1336]" pageId="62" pageNumber="161">
