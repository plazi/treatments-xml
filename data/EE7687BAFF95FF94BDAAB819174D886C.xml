<taxonomicName id="A1DF4D2FFF95FF94BE0ABB8917F4883E" authorityName="Ferrington Jr &amp; Saether" authorityYear="2011" baseAuthorityName="Ferrington Jr &amp; Saether" baseAuthorityYear="2011" box="[823,908,1771,1794]" class="Insecta" family="Chironomidae" genus="Allocladius" kingdom="Animalia" order="Diptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="nanseni">
