, 
<collectionCode id="BBDFAE000265FFA63DF2F954FCB6F924" box="[760,841,1698,1721]" country="New Zealand" httpUri="http://grbio.org/cool/n5zm-bhpt" name="Lincoln University Entomology Research Museum" pageId="58" pageNumber="57">LUNZ</collectionCode>
, 
<collectionCode id="BBDFAE000265FFA63C5FF954FC53F927" box="[853,940,1698,1722]" country="New Zealand" httpUri="http://grbio.org/cool/15kb-kde6" name="Museum of New Zealand Te Papa Tongarewa - Entomology" pageId="58" pageNumber="57" type="Museum">MONZ</collectionCode>
, 
<collectionCode id="BBDFAE000265FFA63CBDF954FBF6F927" box="[951,1033,1698,1722]" country="New Zealand" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" lsid="urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="58" pageNumber="57" type="Other institutional type">NZAC</collectionCode>
).
</paragraph>
