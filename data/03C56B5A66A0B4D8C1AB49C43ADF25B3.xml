<subSubSection id="C37689C766A0B4D8C1FB4FF13ADF25B3" pageId="245" pageNumber="246" type="biology_ecology">
<paragraph id="8BD3DA4C66A0B4D8C1FB4FF13ADF25B3" blockId="245.[151,1436,829,1250]" pageId="245" pageNumber="246">
<emphasis id="B918065E66A0B4D8C1FB4FF1393D25CB" bold="true" box="[199,295,1153,1178]" pageId="245" pageNumber="246">Biology.</emphasis>
