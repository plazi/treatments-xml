<specimenCount id="A2B4FD61703A1265B85E32B02844FB87" box="[528,615,1009,1034]" pageId="6" pageNumber="393" type="male">5 males</specimenCount>
and 
<specimenCount id="A2B4FD61703A1265B8EC32B0292CFB87" box="[674,783,1009,1034]" pageId="6" pageNumber="393" type="female">6 females</specimenCount>
