<taxonomicName id="73B24D6B703A1265BF5732B32FB0FB84" box="[1305,1427,1010,1033]" class="Insecta" family="Ichneumonidae" genus="Rhimphoctona" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="393" phylum="Arthropoda" rank="species" species="maiator">
