<subSubSection id="C31A6570FFA5FFE6BDE6EF1E5A16FE64" pageId="9" pageNumber="10" type="etymology">
<paragraph id="8BBF36FBFFA5FFE6BDE6EF1E5A16FE64" blockId="9.[151,1436,151,392]" pageId="9" pageNumber="10">
<emphasis id="B974EAE9FFA5FFE6BDE6EF1E5A28FE86" bold="true" box="[199,334,332,357]" pageId="9" pageNumber="10">Etymology.</emphasis>
