</taxonomicName>
and 
<taxonomicName id="8FF54D50FFFAD103D443F837DC9DF865" box="[815,943,1994,2018]" class="Arachnida" family="Carabodidae" genus="Diplobodes" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="179" phylum="Arthropoda" rank="genus">
