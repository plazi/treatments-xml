<taxonomicName id="4C16BABF9526FFE1FC51FBD6729CFA1A" authorityName="Osten-Sacken" authorityYear="1865" box="[291,497,1105,1129]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="dimorphus">
