<taxonomicName id="247D7FFFDEC79F4051000693F686690D" class="Pinopsida" family="Pinaceae" genus="Larix" higherTaxonomySource="CoL" hybrid-sign="x" kingdom="Plantae" lsidName="Larix eurolepis" order="Pinales" pageId="44" pageNumber="45" phylum="Tracheophyta" rank="species" species="eurolepis">
Larix 
<normalizedToken id="28E1CF20B8B93A266ED99EEDDC9732CA" originalValue="×">x</normalizedToken>
