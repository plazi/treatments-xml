<taxonomicName id="A6834D44FFD7FFF4C8BDF9556A53F936" authorityName="Boheman" authorityYear="1834" box="[421,625,1731,1757]" class="Insecta" family="Torymidae" genus="Torymus" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="416" phylum="Arthropoda" rank="species" species="caudatus">
