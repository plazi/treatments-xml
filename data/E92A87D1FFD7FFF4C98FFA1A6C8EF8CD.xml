<taxonomicName id="A6834D44FFD7FFF4CAA8F9526D04F935" authority="(Winnertz)" baseAuthorityName="Winnertz" baseAuthorityYear="1853" box="[944,1318,1731,1758]" class="Insecta" family="Cecidomyiidae" genus="Kaltenbachiola" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="416" phylum="Arthropoda" rank="species" species="strobi">
