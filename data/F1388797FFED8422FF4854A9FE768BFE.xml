<taxonomicName id="BE914D02FFEE8422FD5B51D8FCC58ADD" authorityName="Mey &amp; Gonzalez-Acuna" authorityYear="2000" box="[644,853,187,213]" class="Insecta" family="Philopteridae" genus="Caracaricola" kingdom="Animalia" order="Psocodea" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="chimangophilus">
