<specimenCount id="6F97FD08FFEE8422FE2C5061FD198B15" box="[499,649,258,285]" pageId="58" pageNumber="59" type="generic">85 specimens</specimenCount>
of 
<taxonomicName id="BE914D02FFEE8422FD745060FC6A8B15" authorityName="Mey &amp; Gonzalez-Acuna" authorityYear="2000" box="[683,1018,259,285]" class="Insecta" family="Philopteridae" genus="Caracaricola" kingdom="Animalia" order="Psocodea" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="chimangophilus">
