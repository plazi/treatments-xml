<taxonomicName id="BE914D02FFEE8422FE6551D8FDC18ADD" authorityName="Mey &amp; Gonzalez-Acuna" authorityYear="2000" box="[442,593,187,213]" class="Insecta" family="Philopteridae" genus="Caracaricola" kingdom="Animalia" order="Psocodea" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
