<taxonomicName id="4FD11B86630FFFDBFEB58EF8FE75F955" authority="Vilvens &amp; Williams, 2020" authorityName="Vilvens &amp; Williams" authorityYear="2020" box="[297,499,1755,1779]" class="Gastropoda" family="Solariellidae" genus="Ilanga" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Trochida" pageId="27" pageNumber="228" phylum="Mollusca" rank="species" species="polygramma" status="n. sp.">
<emphasis id="BAA5BC17630FFFDBFEB58EF8FE75F955" box="[297,499,1755,1779]" italics="true" pageId="27" pageNumber="228">Ilanga polygramma</emphasis>
</taxonomicName>
<emphasis id="BAA5BC17630FFFDBFE678EF8FDB0F955" bold="true" box="[507,566,1755,1779]" pageId="27" pageNumber="228">
<taxonomicNameLabel id="A196016C630FFFDBFE678EF8FDB0F955" box="[507,566,1755,1779]" pageId="27" pageNumber="228" rank="species">n. sp.</taxonomicNameLabel>
