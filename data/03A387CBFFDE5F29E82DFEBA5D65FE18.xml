<collectionCode id="ED1BAE18FFDE5F29EB84FEF15FBEFEA6" box="[445,484,328,349]" pageId="3" pageNumber="381">AB</collectionCode>
, 
<collectionCode id="ED1BAE18FFDE5F29EBD7FEF15C43FEA6" box="[494,537,328,349]" country="Portugal" name="Universidade de Lisboa, Museu Bocage" pageId="3" pageNumber="381">MB</collectionCode>
