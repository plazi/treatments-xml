<quantity id="C55FC3EB8C6A605FFC938AE8FC66FA4A" box="[839,979,1475,1498]" metricMagnitude="3" metricUnit="m" metricValue="1.661" metricValueMax="2.45" metricValueMin="0.872" pageId="60" pageNumber="193" unit="m" value="1661.0" valueMax="2450.0" valueMin="872.0">872–2450 m</quantity>
