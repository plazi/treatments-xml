<geoCoordinate id="679308C98C6A605FFB328A8DFA93FA2D" box="[1254,1318,1446,1469]" degrees="54" direction="west" orientation="longitude" pageId="60" pageNumber="193" precision="55555" value="-54.0">54°W</geoCoordinate>
