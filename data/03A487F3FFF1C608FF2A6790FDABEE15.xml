<taxonomicName id="4C0D4D66FFF1C608FD226460FD5EE965" box="[671,765,871,894]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="genus">
<emphasis id="B979EAF7FFF1C608FD226460FD5EE965" box="[671,765,871,894]" italics="true" pageId="3" pageNumber="4">Quercus</emphasis>
</taxonomicName>
