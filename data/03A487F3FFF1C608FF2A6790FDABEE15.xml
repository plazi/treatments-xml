<taxonomicName id="4C0D4D66FFF1C608FC0E648CFB9FE9B9" box="[947,1084,907,930]" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="genus">
<emphasis id="B979EAF7FFF1C608FC0E648CFB9FE9B9" box="[947,1084,907,930]" italics="true" pageId="3" pageNumber="4">Castanopsis</emphasis>
</taxonomicName>
