<taxonomicName id="6BE530C7FF9DFFEAFC09FB3AFB595C77" authority="Risso" authorityName="Risso" box="[917,1278,1266,1292]" class="Actinopterygii" family="Soleidae" genus="Synapturichthys" kingdom="Animalia" order="Pleuronectiformes" pageId="21" pageNumber="22" phylum="Chordata" rank="species" species="kleinii">
<emphasis id="9E919756FF9DFFEAFC09FB3AFB045C77" box="[917,1187,1267,1292]" italics="true" pageId="21" pageNumber="22">Synapturichthys kleinii</emphasis>
(Risso)
</taxonomicName>
(
<taxonomicName id="6BE530C7FF9DFFEAFA92FB3BFADD5C77" box="[1294,1402,1266,1292]" class="Actinopterygii" family="Soleidae" kingdom="Animalia" order="Pleuronectiformes" pageId="21" pageNumber="22" phylum="Chordata" rank="family">Soleidae</taxonomicName>
