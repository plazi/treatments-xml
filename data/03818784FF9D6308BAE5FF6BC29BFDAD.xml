<geoCoordinate id="EE1C5055FF9D6308BAE5FEAEC1E9FEC6" box="[163,241,314,335]" degrees="84" direction="east" minutes="07" orientation="longitude" pageId="10" pageNumber="1463" precision="925" value="84.11667">84°07‘E</geoCoordinate>
