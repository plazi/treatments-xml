<specimenCount id="9D2EFD1BFF9D6308B879FEBDC353FEB7" box="[575,587,297,318]" pageId="10" pageNumber="1463" type="female">♀</specimenCount>
: 
<collectingCountry id="F33F7602FF9D6308B827FEB7C381FEB0" box="[609,665,291,313]" name="Nepal" pageId="10" pageNumber="1463">Nepal</collectingCountry>
