<taxonomicName id="98DF4D12FFA50D2A1B63FA9B15E4FAC0" authorityName=": Kirkaldy &amp; Torre-Bueno" authorityYear="1909" box="[303,507,1287,1313]" class="Insecta" family="Belostomatidae" genus="Belostoma" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="475" phylum="Arthropoda" rank="species" species="elegans">
