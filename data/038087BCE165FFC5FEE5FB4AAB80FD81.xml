<taxonomicName id="4C294D29E161FFC7FE06FABEA9E3FAE6" authority="Kirkaldy. Schematic" authorityName="Kirkaldy. Schematic" authorityYear="1908" box="[491,1022,1322,1345]" class="Insecta" family="Thaumastocoridae" genus="Thaumastocoris" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="australicus">
<emphasis id="B95DEAB8E161FFC7FE06FABEA90AFAE6" box="[491,791,1322,1345]" italics="true" pageId="5" pageNumber="66">Thaumastocoris australicus</emphasis>
