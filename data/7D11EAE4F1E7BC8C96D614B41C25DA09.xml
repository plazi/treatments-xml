<paragraph id="9D97270AF1A70795F5F1E7A0FB6F9489" pageId="12" pageNumber="133">
<taxonomicName id="310281396FB2EB9C6096024550CEB2CD" authority="Motschoulsky, 1861" authorityName="Motschoulsky" authorityYear="1861" class="Insecta" family="Chrysomelidae" genus="Gallerucida" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Gallerucida gebieni" order="Coleoptera" pageId="12" pageNumber="133" phylum="Arthropoda" rank="species" species="gebieni">Gallerucida gebieni</taxonomicName>
