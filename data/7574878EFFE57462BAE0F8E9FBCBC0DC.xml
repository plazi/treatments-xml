<taxonomicName id="3ADD4D1BFFE27461BBCCFA6DFDB4C60C" authority="Ossó, 2018" authorityName="Ossó" authorityYear="2018" box="[443,639,1516,1540]" class="Malacostraca" family="Aethridae" genus="Aethra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="590" phylum="Arthropoda" rank="species" species="stalennyii" status="n. sp.">
