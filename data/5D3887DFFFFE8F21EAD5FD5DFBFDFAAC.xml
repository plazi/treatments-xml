</taxonomicName>
group, 
<taxonomicName id="12914D4AFFF18F21E903FC94FCC3FC0F" baseAuthorityName="Girault" baseAuthorityYear="1915" box="[637,797,889,913]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="120" pageNumber="2217" phylum="Arthropoda" rank="species" species="perpulchella">
