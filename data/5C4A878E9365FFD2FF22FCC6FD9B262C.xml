<taxonomicName id="13E34D1B9365FFD2FA97FA8FFD97262C" authority="Kuhlmann &amp; Proshchalykin 2011" authorityName="Kuhlmann &amp; Proshchalykin" authorityYear="2011" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="461" phylum="Arthropoda" rank="species" species="ravulus">
