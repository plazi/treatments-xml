<taxonomicName id="AB6ECE9893B152254EA4C14A24CBB8DC" class="Arachnida" family="Synaphridae" genus="Synaphris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synaphris orientalis" order="Araneae" pageId="2" pageNumber="37" phylum="Arthropoda" rank="species" species="orientalis">Synaphris orientalis</taxonomicName>
