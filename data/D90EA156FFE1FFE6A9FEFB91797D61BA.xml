<taxonomicName id="96A76BC3FFE1FFE6A9FEFBB779FD60A0" authority="EBMER 1978" authorityName="EBMER" authorityYear="1978" box="[151,508,1135,1157]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="569" phylum="Arthropoda" rank="species" species="sakagamii">
