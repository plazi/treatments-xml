<taxonomicName id="4C664D098BD2FF61FB2CFED4FE7EFE0A" authority="Herting 1984" authorityName="Herting" authorityYear="1984" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="156" pageNumber="157" phylum="Arthropoda" rank="species" species="sensu">
