<taxonomicName id="05034680EB14079512771C5F4D8E74B7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:148722" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus dromedarius pulcher Forel" order="Hymenoptera" pageNumber="234" phylum="Arthropoda" rank="variety" species="dromedarius" variety="pulcher">CAMPONOTUS DROMEDARIUS, Forel. var. pulcher</taxonomicName>
