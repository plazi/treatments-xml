<quantity id="1C0888A597ABFF7DFE40FAD2FE0ABA59" box="[418,500,1375,1399]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" pageId="252" pageNumber="253" unit="mm" value="12.0">12 mm</quantity>
