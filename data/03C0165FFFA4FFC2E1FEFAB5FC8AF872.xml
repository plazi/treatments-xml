<number id="3DA1B56EFFA4FFC1E3AEF901FD36F94A" box="[711,725,1695,1719]" pageId="5" value="2.0">2</number>
<emphasis id="B91D7B5BFFA4FFC1E3BCF901FD02F94B" box="[725,737,1695,1718]" italics="true" pageId="5">c</emphasis>
