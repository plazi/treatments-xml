<number id="3DA1B56EFFA4FFC1E1D7F979FF2FF902" box="[190,204,1767,1791]" pageId="5" value="4.0">4</number>
<emphasis id="B91D7B5BFFA4FFC1E1A5F978FF39F902" box="[204,218,1766,1791]" italics="true" pageId="5">b</emphasis>
, and 
<number id="3DA1B56EFFA4FFC1E07EF979FEC6F902" box="[279,293,1767,1791]" pageId="5" value="4.0">4</number>
<emphasis id="B91D7B5BFFA4FFC1E04DF979FED3F903" box="[292,304,1767,1790]" italics="true" pageId="5">c</emphasis>
with thickened basal part; setae 
<number id="3DA1B56EFFA4FFC1E3F0F979FD44F902" box="[665,679,1767,1791]" pageId="5" value="3.0">3</number>
<emphasis id="B91D7B5BFFA4FFC1E3CFF979FD57F903" box="[678,692,1767,1790]" italics="true" pageId="5">a</emphasis>
and 
<number id="3DA1B56EFFA4FFC1E383F979FD1BF902" box="[746,760,1767,1791]" pageId="5" value="4.0">4</number>
<emphasis id="B91D7B5BFFA4FFC1E391F979FCE5F903" box="[760,774,1767,1790]" italics="true" pageId="5">a</emphasis>
