<emphasis id="B91D7B5BFFA4FFC1E53FF979FB8DF903" box="[1110,1134,1767,1790]" italics="true" pageId="5">ps</emphasis>
<number id="3DA1B56EFFA4FFC1E506F96BFB94F8FE" box="[1135,1143,1781,1795]" pageId="5" value="2.0">2</number>
