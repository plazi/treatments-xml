<number id="3DA1B56EFFA4FFC1E410F95DFA64F926" box="[1401,1415,1731,1755]" pageId="5" value="3.0">3</number>
<emphasis id="B91D7B5BFFA4FFC1E4EEF95CFA76F926" box="[1415,1429,1730,1755]" italics="true" pageId="5">b</emphasis>
, 
<number id="3DA1B56EFFA4FFC1E1FEF979FF46F902" box="[151,165,1767,1791]" pageId="5" value="3.0">3</number>
<emphasis id="B91D7B5BFFA4FFC1E1CDF979FF53F903" box="[164,176,1767,1790]" italics="true" pageId="5">c</emphasis>
