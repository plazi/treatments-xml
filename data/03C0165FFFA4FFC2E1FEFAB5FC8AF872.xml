<number id="3DA1B56EFFA4FFC1E0F2F95DFE4AF926" box="[411,425,1731,1755]" pageId="5" value="2.0">2</number>
<emphasis id="B91D7B5BFFA4FFC1E0C1F95CFE55F926" box="[424,438,1730,1755]" italics="true" pageId="5">b</emphasis>
