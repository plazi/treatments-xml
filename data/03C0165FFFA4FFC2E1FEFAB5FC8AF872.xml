<emphasis id="B91D7B5BFFA4FFC1E544F979FBA6F903" box="[1069,1093,1767,1790]" italics="true" pageId="5">ps</emphasis>
<number id="3DA1B56EFFA4FFC1E52CF96BFBAFF8FE" box="[1093,1100,1781,1795]" pageId="5" value="1.0">1</number>
