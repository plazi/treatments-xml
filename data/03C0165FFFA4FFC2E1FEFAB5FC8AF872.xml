<emphasis id="B91D7B5BFFA4FFC1E0DAF9B5FE5CF9BF" box="[435,447,1579,1602]" italics="true" pageId="5">c</emphasis>
<number id="3DA1B56EFFA4FFC1E0D6F9A6FE24F9BB" box="[447,455,1592,1606]" pageId="5" value="2.0">2</number>
