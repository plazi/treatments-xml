<taxonomicName id="27CA4D35FEBFFED95B3CFD5AFC7C6388" authorityName="Rondani" authorityYear="1870" box="[802,975,727,753]" class="Insecta" family="Tephritidae" genus="Oxyna" kingdom="Animalia" order="Diptera" pageId="370" pageNumber="371" phylum="Arthropoda" rank="species" species="cribrina">
