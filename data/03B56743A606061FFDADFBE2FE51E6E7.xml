<taxonomicName id="4C1CADD6A601061EFEB6F8F2FD32E344" authority="(Brenske, 1894)" baseAuthorityName="Brenske" baseAuthorityYear="1894" box="[333,720,1883,1909]" class="Insecta" family="Melolonthidae" genus="Miridiba" kingdom="Animalia" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="dohrni">
