<quantity id="D01DFD921274FFE2FC10F9B8FBE2F9C6" box="[1010,1080,1556,1578]" metricMagnitude="3" metricUnit="m" metricValue="1.7449999999999999" pageId="3" pageNumber="1796" unit="m" value="1745.0">1745 m</quantity>
