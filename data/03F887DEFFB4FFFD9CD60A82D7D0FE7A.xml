<paragraph id="8BEE36C8FFB5FFFD9C86089CD5D9FF2E" blockId="67.[151,1436,1501,2031]" lastBlockId="68.[151,1436,151,428]" lastPageId="68" lastPageNumber="69" pageId="67" pageNumber="68">
<emphasis id="B925EADAFFB5FFFA9C86089CD697F838" bold="true" box="[199,430,2005,2030]" pageId="67" pageNumber="68">Records in Salento.</emphasis>
