</taxonomicName>
, = 
<taxonomicName id="80C2A1B2443ACD51FCAE69051399B140" authority="Kovalev, 1965" authorityName="Kovalev" authorityYear="1965" box="[849,1182,1353,1380]" class="Insecta" family="Cynipidae" genus="Saphonecrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="species" species="symbioticus">
