<taxonomicName id="80C2A1B2443ACD51FF3969DD1693B188" authority="Murray" authorityName="Murray" authorityYear="1784" box="[198,404,1426,1452]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="182" phylum="Tracheophyta" rank="species" species="serrata">
