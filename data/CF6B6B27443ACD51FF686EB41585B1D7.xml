<taxonomicName id="80C2A1B2443ACD51FAC8696A16DBB147" authority="(Ashmead, 1904)" baseAuthorityName="Ashmead" baseAuthorityYear="1904" class="Insecta" family="Cynipidae" genus="Andricus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="species" species="hakonensis">
