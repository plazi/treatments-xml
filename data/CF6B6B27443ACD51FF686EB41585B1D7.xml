<collectingCountry id="3FD59AA1443ACD51FDF168911549B0DC" box="[526,590,1246,1272]" name="Japan" pageId="3" pageNumber="182">Japan</collectingCountry>
and Far Eastern 
<collectingCountry id="3FD59AA1443ACD51FCEC68921459B0D3" box="[787,862,1245,1271]" name="Russia" pageId="3" pageNumber="182">Russia</collectingCountry>
(
<bibRefCitation id="2353A7C0443ACD51FC91689214E5B0D3" author="Abe, Y." box="[878,994,1245,1271]" pageId="3" pageNumber="182" pagination="253 - 255" refId="ref11285" refString="Abe, Y. (1997) Discovery of the Genus Ceroptres (Hymenoptera: Cynipidae) in Japan and Korea. Applied Entomology and Zoology, 32 (1), 253 - 255. https: // doi. org / 10.1303 / aez. 32.253" type="journal article" year="1997">Abe 1997</bibRefCitation>
; Melika 2012); from galls of 
<taxonomicName id="80C2A1B2443ACD51FAC868921692B13F" authority="Kieffer, 1903" authorityName="Kieffer" authorityYear="1903" class="Insecta" family="Cynipidae" genus="Andricus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="species" species="targionii">
<emphasis id="75B60623443ACD51FAC8689217FFB13F" italics="true" pageId="3" pageNumber="182">Andricus targionii</emphasis>
Kieffer, 1903
</taxonomicName>
and 
<taxonomicName id="80C2A1B2443ACD51FE32694D1426B138" authority="(Monzen, 1954)" baseAuthorityName="Monzen" baseAuthorityYear="1954" box="[461,801,1281,1308]" class="Insecta" family="Cynipidae" genus="Andricus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="species" species="pseudoflos">
