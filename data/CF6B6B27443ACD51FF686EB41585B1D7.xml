<taxonomicName id="80C2A1B2443ACD51FEB6696916C2B11B" authorityName="Kovalev" authorityYear="1965" box="[329,453,1318,1343]" class="Insecta" family="Cynipidae" genus="Ussuraspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="genus">
<emphasis id="75B60623443ACD51FEB6696916C2B11B" box="[329,453,1318,1343]" italics="true" pageId="3" pageNumber="182">Ussuraspis</emphasis>
