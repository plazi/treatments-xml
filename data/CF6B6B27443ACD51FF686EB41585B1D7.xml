<taxonomicName id="80C2A1B2443ACD51FDC6692115BEB1A3" authorityName="Abe" authorityYear="1997" box="[569,697,1389,1416]" class="Insecta" family="Cynipidae" genus="Ceroptres" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="species" species="masudai">
