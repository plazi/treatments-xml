<taxonomicName id="BA94A089A33D655D58CBFE4E684758DE" box="[710,1097,459,485]" class="Magnoliopsida" family="Sapotaceae" genus="Englerophytum" kingdom="Plantae" order="Ericales" pageId="12" pageNumber="507" phylum="Tracheophyta" rank="species" species="magalismontanum">
