(
<taxonomicName id="BA94A089A33D655D5E54FE4E68D458DE" box="[1113,1242,459,485]" class="Magnoliopsida" family="Sapotaceae" kingdom="Plantae" order="Ericales" pageId="12" pageNumber="507" phylum="Tracheophyta" rank="family">Sapotaceae</taxonomicName>
