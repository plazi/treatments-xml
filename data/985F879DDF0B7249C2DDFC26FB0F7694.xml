<taxonomicName id="D7F64D08DF087249C079FEF8FD98769B" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[563,632,369,392]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="agnae">
