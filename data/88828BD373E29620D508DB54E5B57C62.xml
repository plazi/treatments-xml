<taxonomicName id="2E4B5759D17BF742F2F77B39B3D6B704" LSID="http://zoobank.org/E937495C-A852-4FB6-8739-F40F5AA5C1E8" class="Arachnida" family="Linyphiidae" genus="Solenysa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Solenysa macrodonta" order="Araneae" pageId="6" pageNumber="45" phylum="Arthropoda" rank="species" species="macrodonta">Solenysa macrodonta</taxonomicName>
