<taxonomicName id="011F85CB7908A3D46D1462CCAD916577" authorityName="Bates" authorityYear="1882" class="Hexapoda" family="Carabidae" genus="Celia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="586" pageNumber="587" phylum="Arthropoda" rank="species" species="tenebrionella">
