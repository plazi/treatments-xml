<geoCoordinate id="02C24FDC0952FFD3FEFD196C76F0FCF8" box="[343,455,869,889]" degrees="32" direction="north" minutes="53.011" orientation="latitude" pageId="22" pageNumber="923" precision="1" value="32.88352">32°53.011’N</geoCoordinate>
