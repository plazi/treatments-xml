<taxonomicName class="Insecta" family="Notodontidae" genus="Hemiceras" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemiceras" order="Lepidoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="genus">
