<tr pageId="null" pageNumber="512">
<td align="left" pageId="null" pageNumber="512" valign="top">1. Stengel niederliegend oder aufsteigend, bis 3 mm dick; Krone 4-6 mm lang</td>
<td align="left" pageId="null" pageNumber="512" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="512" phylum="Tracheophyta" rank="species" species="resupinatum">
<italic pageId="null" pageNumber="512">T. resupinatum</italic>
</taxonomicName>
(Nr. 6a)
</td>
</tr>
<tr pageId="null" pageNumber="512">
<td align="left" pageId="null" pageNumber="512" valign="top">
1*. Stengel aufrecht, 
<normalizedToken originalValue="auffällig" pageId="null" pageNumber="512">auffaellig</normalizedToken>
dick und hohl (unten dicker als 4 mm); Krone 6-8 mm lang
</td>
<td align="left" pageId="null" pageNumber="512" valign="bottom">
<taxonomicName class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="512" phylum="Tracheophyta" rank="species" species="suaveolens">
<italic pageId="null" pageNumber="512">T. suaveolens</italic>
</taxonomicName>
(Nr. 6b)
</td>
</tr>
