<normalizedToken originalValue="gelbgrün" pageId="null" pageNumber="320">gelbgruen</normalizedToken>
bis 
<normalizedToken originalValue="grün" pageId="null" pageNumber="320">gruen</normalizedToken>
, bis 10 cm lange, unterirdische 
<normalizedToken originalValue="Ausläufer" pageId="null" pageNumber="320">Auslaeufer</normalizedToken>
treibend
</td>
<td align="left" pageId="null" pageNumber="320" valign="bottom">
<taxonomicName class="Liliopsida" family="Poaceae" genus="Dactylis" kingdom="Plantae" order="Poales" pageId="null" pageNumber="320" phylum="Tracheophyta" rank="species" species="aschersoniana">
