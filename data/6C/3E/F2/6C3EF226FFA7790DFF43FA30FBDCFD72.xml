<taxonomicName baseAuthorityName="Canu &amp; Bassler" baseAuthorityYear="1928" box="[438,615,404,428]" class="Gymnolaemata" family="Steginoporellidae" genus="Steginoporella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="3" pageNumber="158" phylum="Bryozoa" rank="species" species="transversalis">
