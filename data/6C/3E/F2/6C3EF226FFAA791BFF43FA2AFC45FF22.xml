<caption httpUri="https://zenodo.org/record/1437572/files/figure.png" pageId="16" pageNumber="171" startId="16.[151,250,1880,1902]" targetBox="[224,1365,193,1869]" targetPageId="16">
<paragraph blockId="16.[151,1436,1880,2025]" pageId="16" pageNumber="171">
