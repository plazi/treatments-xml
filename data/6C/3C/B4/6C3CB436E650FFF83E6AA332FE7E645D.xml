<paragraph blockId="13.[313,1269,1134,1323]" box="[313,1061,1300,1323]" pageId="13" pageNumber="1583">
<taxonomicName authority="(Mihelcic)" baseAuthorityName="Mihelcic" box="[313,661,1301,1323]" class="Arachnida" family="Eremaeidae" genus="Eueremaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="13" pageNumber="1583" phylum="Arthropoda" rank="species" species="granulatus">
