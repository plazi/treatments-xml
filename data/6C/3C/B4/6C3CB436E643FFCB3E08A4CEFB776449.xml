<paragraph blockId="30.[313,1017,946,1034]" box="[313,980,1011,1034]" pageId="30" pageNumber="1600">
<taxonomicName authority="Bayartogtokh, 2000: 150" authorityName="Bayartogtokh" authorityPageNumber="150" authorityYear="2000" box="[313,836,1011,1034]" class="Arachnida" family="Oribellidae" genus="Proteremaeus" kingdom="Animalia" order="Sarcoptiformes" pageId="30" pageNumber="1600" phylum="Arthropoda" rank="species" species="punctulatus">
