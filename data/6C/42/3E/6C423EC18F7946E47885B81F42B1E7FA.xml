<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:227007" class="Insecta" family="Formicidae" genus="Pyramica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyramica aschnakiranae Makhan" lsidName-HNS="Pyramica aschnakiranae Makhan" order="Hymenoptera" pageNumber="3" phylum="Arthropoda" rank="species" species="aschnakiranae">Pyramica aschnakiranae</taxonomicName>
