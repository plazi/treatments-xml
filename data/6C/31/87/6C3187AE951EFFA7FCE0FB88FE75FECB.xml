<subSubSection lastPageId="45" lastPageNumber="846" pageId="44" pageNumber="845" type="materials_examined">
<paragraph blockId="44.[806,1421,1109,1162]" pageId="44" pageNumber="845">
<emphasis box="[806,1304,1109,1131]" italics="true" pageId="44" pageNumber="845">
