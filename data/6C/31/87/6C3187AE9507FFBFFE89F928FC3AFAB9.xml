<collectingCountry box="[1293,1390,384,406]" name="United Kingdom" pageId="53" pageNumber="854">England</collectingCountry>
and 
<collectingCountry box="[826,909,415,436]" name="France" pageId="53" pageNumber="854">France</collectingCountry>
