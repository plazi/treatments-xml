<caption ID-DOI="http://doi.org/10.5281/zenodo.3497259" ID-Zenodo-Dep="3497259" httpUri="https://zenodo.org/record/3497259/files/figure.png" pageId="4" pageNumber="148" startId="4.[151,260,1475,1502]" subCaptionStartIDs="4.[151,224,1548,1574]" subCaptionStarts="Figure 3" targetBox="[151,1435,1143,1449]" targetPageId="4">
