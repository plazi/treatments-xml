<subSubSection pageId="9" pageNumber="116" type="distribution">
<paragraph blockId="9.[151,1437,151,464]" pageId="9" pageNumber="116">
<emphasis bold="true" box="[199,345,151,176]" pageId="9" pageNumber="116">Distribution</emphasis>
(
<figureCitation box="[360,444,151,176]" captionStart="FIGURE 29" captionStartId="20.[151,250,1985,2007]" captionTargetBox="[154,1434,206,1944]" captionTargetId="figure@20.[151,1436,193,1963]" captionTargetPageId="20" captionText="FIGURE 29. Distribution records for Centris (Hemisiella) merrillae and C. (Hemisiella) nebulosa new species in Colombia." httpUri="https://zenodo.org/record/258693/files/figure.png" pageId="9" pageNumber="116">Fig. 29</figureCitation>
). 
<collectingCountry box="[463,617,152,176]" name="Colombia" pageId="9" pageNumber="116">COLOMBIA</collectingCountry>
: 
<emphasis box="[627,780,152,175]" italics="true" pageId="9" pageNumber="116">
<collectingRegion box="[627,773,152,175]" country="Colombia" name="Amazonas" pageId="9" pageNumber="116">AMAZONAS</collectingRegion>
