<geoCoordinate box="[661,802,1806,1833]" degrees="85" direction="west" minutes="28" orientation="longitude" pageId="11" pageNumber="364" precision="15" seconds="10" value="-85.469444">85º28’10”W</geoCoordinate>
, 
<date box="[813,917,1807,1833]" pageId="11" pageNumber="364" value="1988-07">
<collectingDate box="[813,917,1807,1833]" pageId="11" pageNumber="364" value="1988-07">VII.1988</collectingDate>
</date>
, 
<collectorName box="[929,1005,1807,1833]" pageId="11" pageNumber="364">Janzen</collectorName>
&amp; 
