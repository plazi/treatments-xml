; remainder at 
<collectionCode id="7D97708AAD26FFB8FDB181B8FD80F979" box="[521,592,1730,1756]" country="Brazil" httpUri="http://grbio.org/cool/rm88-499z" name="Instituto Nacional de Pesquisas da Amazonia" pageId="4" pageNumber="57">INPA</collectionCode>
