<emphasis id="60E3EADB2C24E371B295FA04FEE72453" bold="true" box="[151,290,1522,1544]" pageId="12" pageNumber="420">FIGURE 10.</emphasis>
<taxonomicName id="95974D4A2C24E371B328FA04FD752453" authority="Ollenbach, 1928" authorityName="Ollenbach" authorityYear="1928" box="[298,688,1522,1544]" class="Insecta" family="Cicadidae" genus="Haphsa" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="420" phylum="Arthropoda" rank="species" species="karenensis">
<emphasis id="60E3EADB2C24E371B328FA04FE282453" box="[298,493,1522,1544]" italics="true" pageId="12" pageNumber="420">Haphsa karenensis</emphasis>
