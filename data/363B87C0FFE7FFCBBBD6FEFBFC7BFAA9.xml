<quantity id="796A9B33FFE0FFCAB96DFE7FFCD7FE41" box="[778,842,398,422]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="84" pageNumber="1571" unit="mm" value="8.0">8 mm</quantity>
