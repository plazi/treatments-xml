<specimenCode id="9B7A9EAFFFFA653FFE47E987D6861594" box="[466,684,471,498]" collectionCode="NHMUK" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="62" pageNumber="63" type="Museum">NHMUK 1996150</specimenCode>
