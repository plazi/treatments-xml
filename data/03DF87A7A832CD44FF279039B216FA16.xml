<taxonomicName id="4C764D32A832CD44FF279039B5B1FBED" authority="Piza, 1980" authorityName="Piza" authorityYear="1980" box="[151,703,1069,1095]" class="Insecta" family="Tettigoniidae" genus="Toledopizia" kingdom="Animalia" order="Orthoptera" pageId="13" pageNumber="44" phylum="Arthropoda" rank="species" species="salesopolensis" status="comb.">
<emphasis id="B902EAA3A832CD44FF279039B6DCFBED" bold="true" box="[151,466,1069,1095]" italics="true" pageId="13" pageNumber="54">Toledopizia salesopolensis</emphasis>
(Piza, 
<number id="3DBE2496A832CD44FD989039B56AFBED" box="[552,612,1069,1095]" pageId="13" pageNumber="54" value="1980.0">1980</number>
) 
<taxonomicNameLabel id="A23157D8A832CD44FDC49039B5B1FBED" box="[628,703,1069,1095]" pageId="13" pageNumber="54">comb.</taxonomicNameLabel>
</taxonomicName>
