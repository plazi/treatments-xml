sp. (
<taxonomicName id="DB3BE812DFEF481376D845B0162FBF63" family="Pieridae" lsidName="" pageId="44" pageNumber="45" rank="family">Pieridae</taxonomicName>
) (
<bibRefCitation id="DD5D8CA7DB19934552EE6AA426B518BF" author="Noyes, JS" journalOrPublisher="Fauna SSSR (n. s.)" pageId="148" pageNumber="149" title="Universal Chalcidoidea Database" url="http://www.nhm.ac.uk/chalcidoids" year="2011">Noyes 2011</bibRefCitation>
