<taxonomicName id="0A4DDEAAE96E4A61E6DC7486C530A72A" LSID="http://zoobank.org/18BFD8BA-1595-4EBF-9E4A-A6F75C805965" authority="Y. Yang &amp; X. Yang" authorityName="Gorham" authorityYear="1889" class="Insecta" family="Cantharidae" genus="Lycocerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lycocerus napolovi" order="Coleoptera" pageId="8" pageNumber="93" phylum="Arthropoda" rank="species" species="napolovi">Lycocerus napolovi Y. Yang &amp; X. Yang</taxonomicName>
<taxonomicNameLabel id="118EC3D6CAB9ACA5FEE6CA2343FD1322" pageId="8" pageNumber="93">sp. n.</taxonomicNameLabel>
