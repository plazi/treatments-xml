<taxonomicName id="470A866F2B2F838BC405C26E0B915934" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27277" authorityName="Forel" authorityYear="1886" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus ursus Forel" order="Hymenoptera" pageNumber="46" phylum="Arthropoda" rank="species" species="ursus">CAMPONOTUS URSUS</taxonomicName>
