<heading id="D0D6818A7221FFBBFC2EF865FAC9E746" box="[932,1295,839,860]" centered="true" fontSize="8" level="2" pageId="17" pageNumber="501" reason="8">
<taxonomicName id="4C214D657221FFBBFC2EF865FB7CE747" authorityName="Li &amp; Song" authorityYear="1992" box="[932,1210,839,860]" class="Arachnida" family="Linyphiidae" genus="Solenysa" kingdom="Animalia" order="Araneae" pageId="17" pageNumber="501" phylum="Arthropoda" rank="species" species="longqiensis">
