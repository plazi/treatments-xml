<caption id="F7CF663C04BEFF2CFF62B435FA81F9F1" box="[151,1373,1618,1642]" httpUri="https://zenodo.org/record/895214/files/figure.png" pageId="209" pageNumber="210" targetBox="[151,1436,189,1597]" targetPageId="209">
<paragraph id="A30F36B404BEFF2CFF62B435FA81F9F1" blockId="209.[151,1373,1618,1642]" box="[151,1373,1618,1642]" pageId="209" pageNumber="210">
<emphasis id="91C4EAA604BEFF2CFF62B435FEA4F9F2" bold="true" box="[151,376,1618,1642]" pageId="209" pageNumber="210">FIGURES 445–447.</emphasis>
<taxonomicName id="64B04D3704BEFF2CFE8BB435FCE8F9F1" baseAuthorityName="Ogloblin" baseAuthorityYear="1936" box="[382,820,1618,1641]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="209" pageNumber="210" phylum="Arthropoda" rank="species" species="quirogai" subGenus="Cosmocomoidea">
