<taxonomicName id="12A8EBF3334A40929660C77ADBBF95A9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145622" authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" class="Insecta" family="Cryptoceridae" genus="Cryptocerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptocerus maculatus Smith" lsidName-HNS="Cryptocerus maculatus Smith" order="Hymenoptera" pageNumber="607" phylum="Arthropoda" rank="species" species="maculatus">Cryptocerus maculatus</taxonomicName>
