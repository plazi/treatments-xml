; 
<materialsCitation id="3B793CE0FFD1FFF9FEEFFB2FFE08FA9A" collectionCode="BMM, MNHN" pageId="5" pageNumber="144" specimenCode="ML 15443, ML 15660" specimenCount="2" typeStatus="holotype">
