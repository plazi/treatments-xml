<taxonomicName id="14FF22E7FF974A5CFBAC8F72FB72EB6E" authorityName="Wilson" authorityYear="2003" box="[1033,1158,1052,1077]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="beloceps">
