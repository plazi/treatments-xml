<taxonomicName id="4C664D4ACA3FFF989220FF7799C4ECF1" box="[344,684,152,176]" class="Arachnida" family="Torrenticolidae" genus="Monatractides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="9" pageNumber="55" phylum="Arthropoda" rank="species" species="neoaustralicus" status="sp. nov.">
<emphasis id="B912EADBCA3FFF989220FF7799C4ECF1" box="[344,684,152,176]" italics="true" pageId="9" pageNumber="55">Monatractides neoaustralicus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22157A0CA3FFF9891CFFF77987DECEE" box="[695,789,152,176]" pageId="9" pageNumber="55" rank="species">
<emphasis id="B912EADBCA3FFF9891CFFF779867ECF1" bold="true" box="[695,783,152,176]" pageId="9" pageNumber="55">sp. nov</emphasis>
.
</taxonomicNameLabel>
and 
<taxonomicName id="4C664D4ACA3FFF98902DFF7698A6ECF1" box="[853,974,152,176]" class="Arachnida" family="Torrenticolidae" genus="Monatractides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="9" pageNumber="55" phylum="Arthropoda" rank="species" species="rivulus" status="sp. nov.">
