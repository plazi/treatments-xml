<taxonomicName id="4C664D4ACA3FFF989076FF0E9F25ECB6" authority="Cook, 1986" authorityName="Cook" authorityYear="1986" box="[782,1101,223,248]" class="Arachnida" family="Torrenticolidae" genus="Monatractides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="9" pageNumber="55" phylum="Arthropoda" rank="species" species="australicus">
