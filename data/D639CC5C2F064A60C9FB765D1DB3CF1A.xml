<quantity id="9968D0AF2F064A60CDEF76B51A87CF25" box="[1155,1233,1014,1038]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="13" pageNumber="238" unit="m" value="1000.0">1000m</quantity>
