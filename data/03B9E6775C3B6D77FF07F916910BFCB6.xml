<taxonomicName id="4C102CE25C386D77FE9DFEB0902BFEEA" box="[269,513,332,356]" class="Insecta" family="Phoridae" genus="Myriophora" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="luteizona">
