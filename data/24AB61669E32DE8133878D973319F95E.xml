<taxonomicName id="D8DA24108B882027D6A7B06B91504FC3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138484" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName-HNS="Myrmica laeviuscula Förster" order="Hymenoptera" pageNumber="54" phylum="Arthropoda" rank="species" species="laeviuscula">Myrm. laeviuscula</taxonomicName>
