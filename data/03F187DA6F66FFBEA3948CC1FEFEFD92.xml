<taxonomicName id="4C584D4F6F65FFBEA26B88EEFDE8FE55" box="[360,572,440,464]" class="Bivalvia" family="Lasaeidae" genus="Lasaea" kingdom="Animalia" order="Galeommatida" pageId="50" pageNumber="51" phylum="Mollusca" rank="species" species="hawaiensis">
