<taxonomicName id="70CA4D3CFF4BE2EF8686FBAED90361BF" box="[969,1037,1149,1174]" class="Hydrozoa" family="Campanulariidae" genus="Clytia" kingdom="Animalia" order="Leptothecata" pageId="166" pageNumber="167" phylum="Cnidaria" rank="genus">
