<taxonomicName id="4C332EC20936FF81FD1EB90DFC6EA666" box="[761,984,264,289]" class="Insecta" family="Crambidae" genus="Eudonia" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="puellaris">
