<taxonomicName id="4C604D0CC408DB75FC1DFE1759B4FE24" box="[1005,1157,404,429]" class="Insecta" family="Agromyzidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="206" phylum="Arthropoda" rank="family">Agromyzidae</taxonomicName>
