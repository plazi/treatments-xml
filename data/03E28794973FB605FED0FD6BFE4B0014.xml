<quantity id="4CB39B67973FB605FB48FC49FB5F0334" box="[1168,1258,1014,1040]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="4" pageNumber="5" unit="mm" value="3.8">3.8 mm</quantity>
