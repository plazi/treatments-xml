<quantity id="4CB39B67973FB605FD09FBA1FCA1031C" box="[721,788,1054,1080]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="4" pageNumber="5" unit="mm" value="4.0">4 mm</quantity>
