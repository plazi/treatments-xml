<date id="FFF51042973FB605FEB5FC49FE260334" box="[365,403,1014,1040]" pageId="4" pageNumber="5">
<number id="3D8324A5973FB605FEB5FC49FEC90334" box="[365,380,1014,1040]" pageId="4" pageNumber="5" value="4.0">4</number>
