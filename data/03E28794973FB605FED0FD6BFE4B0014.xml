<quantity id="4CB39B67973FB605FE54FC71FE5204CC" box="[396,487,974,1000]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="4" pageNumber="5" unit="mm" value="3.3">3.3 mm</quantity>
