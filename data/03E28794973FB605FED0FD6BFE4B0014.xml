<quantity id="4CB39B67973FB605FE04FC49FD8F0334" box="[476,570,1014,1040]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="4" pageNumber="5" unit="mm" value="3.5">3.5 mm</quantity>
