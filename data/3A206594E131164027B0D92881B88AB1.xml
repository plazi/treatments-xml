<paragraph id="9DEB8D5D96F3B8B3B2C949FEF178851D" pageId="0" pageNumber="25">
<table id="CA3ECF135F3F0D012D4C6E0E611CCD16" pageId="0" pageNumber="25">
<tr id="9D9695C31C2E7ADBDE4A31695B3BF5B2" pageId="0" pageNumber="25">
<th id="4BC874E3915575451CECBDE966B1632C" colspan="1" pageId="0" pageNumber="25" rowspan="1" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">Measurement</th>
