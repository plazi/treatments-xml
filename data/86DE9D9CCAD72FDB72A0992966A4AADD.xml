sp. (
<taxonomicName id="3C2A4EE72C748BAEE26E01C276CDF8D3" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="253" pageNumber="254" rank="genus">Heliconiaceae</taxonomicName>
).
</paragraph>
</subSubSection>
