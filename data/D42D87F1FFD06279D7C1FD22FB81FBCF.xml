<collectingCountry id="24937677FFD06279D5BAFC5AFCC9FC07" box="[748,834,942,967]" name="Croatia" pageId="22" pageNumber="23">Croatia</collectingCountry>
, 
<collectingCountry id="24937677FFD06279D405FC5AFC16FC07" box="[851,925,942,967]" name="Serbia" pageId="22" pageNumber="23">Serbia</collectingCountry>
, 
