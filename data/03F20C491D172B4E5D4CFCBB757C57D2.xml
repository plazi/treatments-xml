. – 
<taxonomicName id="4C5BC6DC1D172B4E5CF1FB1575AF55CA" box="[459,691,1254,1278]" class="Liliopsida" family="Pandanaceae" genus="Benstonea" kingdom="Plantae" order="Pandanales" pageId="10" pageNumber="333" phylum="Tracheophyta" rank="species" species="ellipsoidea">
