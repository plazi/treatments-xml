<quantity id="4CF09B5A297EFFB6FDB3C33A9FC4F9FE" box="[551,656,1559,1583]" metricMagnitude="-3" metricUnit="m" metricValue="1.73" pageId="71" pageNumber="72" unit="mm" value="1.73">1.73 mm</quantity>
; HL about 
<quantity id="4CF09B5A297EFFB6FC80C33A9E29F9FE" box="[788,893,1559,1583]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="71" pageNumber="72" unit="mm" value="0.3">0.30 mm</quantity>
, HW about 
<quantity id="4CF09B5A297EFFB6FB9EC33A9926F9FE" box="[1034,1138,1559,1583]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="71" pageNumber="72" unit="mm" value="0.3">0.30 mm</quantity>
, AnL about 
<quantity id="4CF09B5A297EFFB6FA91C33A983AF9FE" box="[1285,1390,1559,1583]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="71" pageNumber="72" unit="mm" value="0.34">0.34 mm</quantity>
; PL about 
<quantity id="4CF09B5A297EFFB6FF48C3119C16F982" box="[220,322,1596,1620]" metricMagnitude="-4" metricUnit="m" metricValue="2.8" pageId="71" pageNumber="72" unit="mm" value="0.28">0.28 mm</quantity>
, EL about 
<quantity id="4CF09B5A297EFFB6FE28C3169F77F982" box="[444,547,1595,1620]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="71" pageNumber="72" unit="mm" value="0.55">0.55 mm</quantity>
.
</paragraph>
