<taxonomicName id="4C709EA4A21E8E4DFE874A4EFD737B04" authority="(Chickering, 1966)" authorityName="Chickering" authorityYear="1966" baseAuthorityName="Chickering" baseAuthorityYear="1966" box="[294,648,1887,1909]" class="Arachnida" family="Dipluridae" genus="Masteria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="simla">
<emphasis id="B9043935A21E8E4DFE874A4EFE447B04" box="[294,447,1887,1909]" italics="true" pageId="46" pageNumber="47">Masteria simla</emphasis>
