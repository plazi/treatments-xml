<quantity id="4CFDB1A63D27FFC3FCF1956CDDA5FEA2" box="[785,921,310,332]" metricMagnitude="3" metricUnit="m" metricValue="1.7221200000000003" metricValueMax="1.7678400000000003" metricValueMin="1.6764000000000003" pageId="36" pageNumber="1707" unit="ft" value="5650.0" valueMax="5800.0" valueMin="5500.0">5500-5800 feet</quantity>
, litter near stream, 
<date id="FFBB3A833D27FFC3FF3F9515DF01FE8A" box="[223,317,335,356]" pageId="36" pageNumber="1707" value="1993-05-18">18.V.1993</date>
