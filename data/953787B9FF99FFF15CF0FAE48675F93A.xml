FIGURE 7. 
<emphasis id="2FEAEABDFF98FFFF5D7AF834841FF80F" bold="true" box="[285,356,1953,1975]" italics="true" pageId="23" pageNumber="124">In situ</emphasis>
images of 
<taxonomicName id="DA9E4D2CFF98FFFF5DB9F834872EF80F" box="[478,597,1953,1975]" class="Asteroidea" family="Goniasteridae" genus="Mediaster" kingdom="Animalia" order="Valvatida" pageId="23" pageNumber="124" phylum="Echinodermata" rank="species" species="tenellus">
<emphasis id="2FEAEABDFF98FFFF5DB9F834872EF80F" bold="true" box="[478,597,1953,1975]" italics="true" pageId="23" pageNumber="124">M. tenellus</emphasis>
</taxonomicName>
and 
<taxonomicName id="DA9E4D2CFF98FFFF5EE8F8348664F80F" box="[655,799,1953,1975]" class="Asteroidea" family="Goniasteridae" genus="Nymphaster" kingdom="Animalia" order="Valvatida" pageId="23" pageNumber="124" phylum="Echinodermata" rank="species" species="diomedeae">
<emphasis id="2FEAEABDFF98FFFF5EE8F8348664F80F" bold="true" box="[655,799,1953,1975]" italics="true" pageId="23" pageNumber="124">N. diomedeae</emphasis>
</taxonomicName>
. A.
</emphasis>
<taxonomicName id="DA9E4D2CFF98FFFF5F37F8378175F80F" box="[848,1038,1953,1975]" class="Asteroidea" family="Goniasteridae" genus="Mediaster" kingdom="Animalia" order="Valvatida" pageId="23" pageNumber="124" phylum="Echinodermata" rank="species" species="tenellus">
