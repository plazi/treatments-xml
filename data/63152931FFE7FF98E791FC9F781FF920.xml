<taxonomicName id="2CBCE3A4FFE6FF9BE7CBF9EF7EA0F98C" authority="Lundstrom, 1942" authorityName="Lundstrom" authorityYear="1942" box="[214,757,1558,1580]" class="Palaeacanthocephala" family="Plagiorhynchidae" genus="Plagiorhynchus" kingdom="Animalia" order="Polymorphida" pageId="5" pageNumber="80" phylum="Acanthocephala" rank="species" species="odhneri" subGenus="Plagiorhynchus">
