<collectionCode id="1D8CAE3DFFEDFFA63C0BF899FAC5BA7C" box="[1328,1387,1795,1820]" pageId="5" pageNumber="80">OOL</collectionCode>
=0.5 
<collectionCode id="1D8CAE3DFFEDFFA639ACF8BDFF72BA5F" box="[151,220,1831,1855]" country="Italy" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="5" pageNumber="80">MOD</collectionCode>
