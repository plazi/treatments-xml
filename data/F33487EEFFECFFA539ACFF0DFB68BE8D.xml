<quantity id="BC659B1DFFEDFFA63ACEF899FBFCBA7B" box="[1013,1106,1795,1819]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="5" pageNumber="80" unit="mm" value="2.9">2.9 mm</quantity>
; 
<collectionCode id="1D8CAE3DFFEDFFA63D5BF899FB0CBA7C" box="[1120,1186,1795,1820]" country="Peru" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15530" name="Universidad Nacional Agraria La Molina" pageId="5" pageNumber="80">MOL</collectionCode>
=1.5 
<collectionCode id="1D8CAE3DFFEDFFA63DE7F899FA8FBA7B" box="[1244,1313,1795,1819]" country="Italy" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="5" pageNumber="80">MOD</collectionCode>
