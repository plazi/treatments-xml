<emphasis id="49E9EAEAFFEDFFA639FCF899FEF7BA7C" bold="true" box="[199,345,1795,1820]" pageId="5" pageNumber="80">Description.</emphasis>
<typeStatus id="A426885AFFEDFFA63858F899FE61BA7C" box="[355,463,1795,1820]" pageId="5" pageNumber="80">Holotype</typeStatus>
. 
<emphasis id="49E9EAEAFFEDFFA638E7F899FDBBBA7B" box="[476,533,1795,1819]" italics="true" pageId="5" pageNumber="80">Male</emphasis>
. Body length 
<quantity id="BC659B1DFFEDFFA63B84F899FCB2BA7B" box="[703,796,1795,1819]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="5" pageNumber="80" unit="mm" value="3.4">3.4 mm</quantity>
