<materialsCitation id="EB963C9BFF8B1560EF65F7D6FDD5F7BD" ID-GBIF-Occurrence="3396412301" box="[315,552,2133,2167]" collectingDate="1914" collectionCode="AMNH" collectorName="Barnum Brown" formation="Belly River Formation" httpUri="http://research.amnh.org/paleontology/search.php?action=detail&amp;specimen_id=48124" location="Red Deer River" pageId="17" pageNumber="383" preperations="skull and lower jaws, and a few foot bones" specimenCode="AMNH 5356" specimenCount="1" stateProvince="Alberta" typeStatus="Type">A. M. No. 5356</materialsCitation>
