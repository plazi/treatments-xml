<taxonomicName id="9CFE4D45FF8B1560EF6BF795FCF2F7F7" authority="Matthew &amp; Brown, 1922" authorityName="Matthew &amp; Brown" authorityYear="1922" box="[309,783,2070,2109]" class="Reptilia" family="Dromaeosauridae" genus="Dromaeosaurus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Dinosauria" pageId="17" pageNumber="378" phylum="Chordata" rank="species" species="albertensis" status="new genus, new species">
