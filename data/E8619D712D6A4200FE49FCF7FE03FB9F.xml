). New record: 
<collectingRegion id="A20CE2852D6A4206FF67FBD3FE2EFBC2" box="[213,424,1042,1066]" country="Australia" name="Western Australia" pageId="43" pageNumber="1145">Western Australia</collectingRegion>
, 
<collectionCode id="06D9B4A22D6A4206FE07FBD3FD8CFBC2" box="[437,522,1042,1066]" country="Australia" httpUri="http://grbio.org/cool/0k2v-y6wz" name="Australian National Fish Collection" pageId="43" pageNumber="1145" type="Museum">CSIRO</collectionCode>
<specimenCode id="306E841C2D6A4206FDA1FBD3FDCFFBC2" box="[531,585,1042,1066]" collectionCode="CSIRO" country="Australia" httpUri="http://grbio.org/cool/0k2v-y6wz" name="Australian National Fish Collection" pageId="43" pageNumber="1145" type="Museum">SS10</specimenCode>
