<taxonomicName id="4C064D1FDF2D726AA7B4FD44FC27FD07" authority="(Jones 1963)" baseAuthorityName="Jones" baseAuthorityYear="1963" box="[460,876,685,709]" class="Malacostraca" family="Nannastacidae" genus="Scherocumella" kingdom="Animalia" order="Cumacea" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="pilgrimi">
