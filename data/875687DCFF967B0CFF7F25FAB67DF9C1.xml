<typeStatus id="D0448868FF967B0DFF5525D6B4A3FCD8" box="[154,210,765,789]" pageId="22" pageNumber="23">TYPE</typeStatus>
SPECIES: 
<taxonomicName id="C8FF4D49FF967B0DFE9B25D6B487FCF9" authority="Liu and Engel" authorityName="Liu and Engel" authorityYear="2007" class="Insecta" family="Figitidae" genus="Syneucoila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="magnifica" status="new species">
<emphasis id="3D8BEAD8FF967B0DFE9B25D6B621FCDE" box="[340,592,765,789]" italics="true" pageId="22" pageNumber="23">Syneucoila magnifica</emphasis>
Liu and Engel
</taxonomicName>
, 
<taxonomicNameLabel id="26B857A3FF967B0DFECB2431B5FAFCF9" box="[260,395,794,818]" pageId="22" pageNumber="23" rank="species">new species</taxonomicNameLabel>
