<taxonomicName id="4C6B1FA1B364FF96263E6324FCB2F898" authority="Nielson" authorityName="Nielson" authorityYear="1977" box="[584,822,1878,1902]" class="Insecta" family="Cicadellidae" genus="Thagria" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="vietnamensis">
