<taxonomicName id="4C6B1FA1B364FF9620E96308FAB4F864" authorityName="Nielson" authorityYear="1977" box="[1183,1328,1915,1938]" class="Insecta" family="Cicadellidae" genus="Thagria" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="vietnamensis">
