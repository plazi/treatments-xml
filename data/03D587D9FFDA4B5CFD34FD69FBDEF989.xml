<location id="8EA36014FFDA4B51FCF9FAFBFBADFA4B" LSID="urn:lsid:plazi:treatment:03D587D9FFDA4B5CFD34FD69FBDEF989:8EA36014FFDA4B51FCF9FAFBFBADFA4B" box="[842,1050,1390,1416]" country="Australia" name="Magnetic Island" pageId="71" pageNumber="72" stateProvince="Queensland">Magnetic Island</location>
