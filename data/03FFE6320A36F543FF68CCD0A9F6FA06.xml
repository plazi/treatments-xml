<typeStatus id="54EDE9860A36F543FC0DCF10AE6CFCC6" box="[1010,1112,830,855]" pageId="51" pageNumber="52">lectotype</typeStatus>
of 
<taxonomicName id="4C562CA70A36F543FB83CF11AEFCFCC7" box="[1148,1224,831,854]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="51" pageNumber="52" phylum="Mollusca" rank="species" species="wernei">
<emphasis id="B9228B360A36F543FB83CF11AEFCFCC7" box="[1148,1224,831,854]" italics="true" pageId="51" pageNumber="52">wernei</emphasis>
</taxonomicName>
, thereby rendering 
<taxonomicName id="4C562CA70A36F543FF68CF4AAB7DFCEA" authority="Philippi" authorityName="Philippi" box="[151,329,866,891]" class="Gastropoda" family="Ampullariidae" genus="Ampullaria" higherTaxonomySource="CoL" kingdom="Animalia" order="Melanosporales" pageId="51" pageNumber="52" phylum="Mollusca" rank="species" species="wernei">
