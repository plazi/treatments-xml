<taxonomicName id="ACD94D515A7DFE06F712586B777D9636" box="[284,516,1067,1089]" class="Malacostraca" family="Lysianassidae" genus="Hippomedon" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="385" phylum="Arthropoda" rank="species" species="hippolyte" status="sp. nov.">
<emphasis id="59ADEAC05A7DFE06F712586B777D9636" box="[284,516,1067,1089]" italics="true" pageId="8" pageNumber="385">Hippomedon hippolyte</emphasis>
</taxonomicName>
