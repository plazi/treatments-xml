<taxonomicName id="D4D08581FFAAFB7E597855EA40328DD2" authorityName="P. H. Lucas" authorityYear="1846" box="[338,537,1983,2007]" class="Insecta" family="Eucnemidae" genus="Anelastes" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="103" phylum="Arthropoda" rank="species" species="barbarus">
