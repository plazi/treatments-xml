<location id="1B72604E0E73FFD1FB04AC4EFAEEF883" LSID="urn:lsid:plazi:treatment:960487830E73FFD3FCEBADDEFE72FCA9:1B72604E0E73FFD1FB04AC4EFAEEF883" box="[1165,1299,1886,1902]" country="Spain" municipality="Type" name="Rio Manzanares" pageId="72" pageNumber="213" stateProvince="Madrid">Rio Manzanares</location>
