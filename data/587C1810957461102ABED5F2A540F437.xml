<taxonomicName id="2661B98C0CCD5E609E38C1B99F0B7A1E" baseAuthorityName="Blandford" baseAuthorityYear="1894" class="Insecta" family="Curculionidae" genus="Xyleborinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xyleborinus attenuatus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="attenuatus">
<emphasis id="9C82BB7E5049FE9A566CED85DB9F42AE" italics="true" pageId="0" pageNumber="1">Xyleborinus attenuatus</emphasis>
</taxonomicName>
, 
<taxonomicName id="BAD644EEC27FA89A8E5A72C89B39E4D5" genus="X." lsidName="X. saxesenii" pageId="0" pageNumber="1" rank="species" species="saxesenii">
<emphasis id="9AC83F264F4BF8F93267450CBE4727C9" italics="true" pageId="0" pageNumber="1">X. saxesenii</emphasis>
</taxonomicName>
, 
<taxonomicName id="BDD1672D9CEA545FA18378BEC0135E53" genus="X." lsidName="X. subgranulatus" pageId="0" pageNumber="1" rank="species" species="subgranulatus">
<emphasis id="73CD3D6F65E71E691DF296F8C5C754F4" italics="true" pageId="0" pageNumber="1">X. subgranulatus</emphasis>
