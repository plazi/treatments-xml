<heading id="D091F51FFFC7FFA4D3C8F8BAF2BF136C" bold="true" box="[151,447,1871,1896]" fontSize="10" level="2" pageId="16" pageNumber="455" reason="2">
<emphasis id="B9129E61FFC7FFA4D3C8F8BAF2BF136C" bold="true" box="[151,447,1871,1896]" pageId="16" pageNumber="455">
<taxonomicName id="4C6639F0FFC7FFA4D3C8F8BAF27A1363" box="[151,378,1871,1896]" class="Insecta" family="Syrphidae" genus="Merodon" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="455" phylum="Arthropoda" rank="species" species="neonanus" status="sp. n.">
<emphasis id="B9129E61FFC7FFA4D3C8F8BAF27A1363" bold="true" box="[151,378,1871,1896]" italics="true" pageId="16" pageNumber="455">Merodon neonanus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A221231AFFC7FFA4D2DFF8BAF2BF136C" box="[384,447,1871,1896]" pageId="16" pageNumber="455" rank="species">sp. n.</taxonomicNameLabel>
