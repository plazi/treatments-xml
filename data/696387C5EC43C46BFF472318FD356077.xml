<quantity id="26329B36EC43C465FA902270FA636104" box="[1344,1436,1866,1892]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="22" pageNumber="23" unit="mm" value="2.3">2.3 mm</quantity>
