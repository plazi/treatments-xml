<taxonomicName id="4C694D72455EFF92FB3FFEA1FADF490C" box="[1172,1346,260,285]" class="Insecta" family="Formicidae" genus="Metapone" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="542" phylum="Arthropoda" rank="species" species="hoelldobleri">
