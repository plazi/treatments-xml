<number id="B03A24D86D62F47341D2FA53FF53FA21" box="[215,229,1499,1523]" pageId="6" pageNumber="448" value="4.0">4</number>
</figureCitation>
<collectionCode id="60E3AE3A6D62F47341E0FA53FF4CFA21" box="[229,250,1499,1523]" pageId="6" pageNumber="448">A</collectionCode>
