<taxonomicName id="ECED2CF81D54037BC0AE17EEFD9D2998" authority="Mayer, 1903" authorityName="Mayer" authorityYear="1903" box="[249,557,1450,1472]" class="Malacostraca" family="Caprellidae" genus="Caprella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="13" pageNumber="90" phylum="Arthropoda" rank="species" species="mendax">
<emphasis id="19998B691D54037BC0AE17EEFE142998" box="[249,420,1451,1472]" italics="true" pageId="13" pageNumber="90">Caprella mendax</emphasis>
<bibRefCitation id="4F7C2A8A1D54037BC1FB17EEFD9D2998" author="Mayer P" box="[428,557,1450,1472]" pageId="13" pageNumber="90" pagination="1 - 160" refId="ref12977" refString="Mayer P. 1903. Die Caprellidae der Siboga-Expedition. Siboga-Expeditie. 34: 1 - 160." type="journal article" year="1903">Mayer, 1903</bibRefCitation>
