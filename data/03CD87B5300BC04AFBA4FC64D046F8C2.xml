<taxonomicName id="4C644D20300BC04AFBA4FC64D0C1FDC1" authority="Meigen" authorityName="Meigen" authorityYear="1803" box="[1040,1271,994,1018]" class="Insecta" family="Chironomidae" genus="Chironomus" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="237" phylum="Arthropoda" rank="genus">
