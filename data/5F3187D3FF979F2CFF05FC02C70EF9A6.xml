<treatmentCitation id="563910D4FF979F2CFF05FC02C6A6FC24" author="Garman, S" box="[254,690,961,985]" page="19" pageId="1" pageNumber="80" year="1890">
<taxonomicName id="10984D46FF979F2CFF05FC02C6A6FC24" authority="Garman, 1890: 19" authorityName="Garman" authorityYear="1890" box="[254,690,961,985]" class="Actinopterygii" family="Anostomidae" genus="Anostomus" kingdom="Animalia" order="Characiformes" pageId="1" pageNumber="80" phylum="Chordata" rank="species" species="proximus">
<emphasis id="E5ECEAD7FF979F2CFF05FC02C6A6FC24" bold="true" box="[254,690,961,985]" pageId="1" pageNumber="80">
<emphasis id="E5ECEAD7FF979F2CFF05FC02C5F3FC25" bold="true" box="[254,487,961,985]" italics="true" pageId="1" pageNumber="80">Anostomus proximus</emphasis>
<bibRefCitation id="B3094B34FF979F2CFE17FC02C6A6FC24" author="Garman, S" box="[492,690,961,984]" pageId="1" pageNumber="80" pagination="15 - 23" refId="ref1433" refString="Garman, S. 1890. On the species of the genus Anostomus. Bulletin of the Essex Institute 22: 15 - 23." type="journal article" year="1890">Garman, 1890:19</bibRefCitation>
