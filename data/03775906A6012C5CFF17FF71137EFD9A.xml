<taxonomicName id="4CDE9393A6012C5CFE93FF7111C0FF4A" box="[275,421,151,176]" class="Insecta" family="Formicidae" genus="Paltothyreus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="genus">
