<geoCoordinate id="EE4F50529C58333EFDAFF8BEFD5FF85E" box="[543,648,1914,1939]" direction="east" orientation="longitude" pageId="2" pageNumber="461" precision="925" value="55.033333">E 55° 02'</geoCoordinate>
