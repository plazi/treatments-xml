<taxonomicName id="4C66D7A9F338FFD0873AB234FDC2FC83" authorityName="Evdonin" authorityYear="1977" box="[285,527,821,846]" class="Rhabditophora" family="Polycystididae" kingdom="Animalia" order="Rhabdocoela" pageId="4" pageNumber="85" phylum="Platyhelminthes" rank="subFamily" subFamily="Typhlopolycystidinae">Typhlopolycystidinae</taxonomicName>
