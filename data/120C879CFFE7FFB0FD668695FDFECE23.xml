<quantity id="5D5D9B6FFFE4FFB0FDB082E0FD34CD85" box="[522,659,220,242]" metricMagnitude="-3" metricUnit="m" metricValue="1.415" metricValueMax="1.45" metricValueMin="1.38" pageId="35" pageNumber="43" unit="mm" value="1.415" valueMax="1.45" valueMin="1.38">1.38–1.45 mm</quantity>
