<emphasis id="A8D1EA98FFE4FFB0FF67832EFF4ECC53" box="[221,233,274,295]" italics="true" pageId="35" pageNumber="43">n</emphasis>
= 3); HW = 1.00– 
<quantity id="5D5D9B6FFFE4FFB0FE2D832EFE41CC53" box="[407,486,274,295]" metricMagnitude="-3" metricUnit="m" metricValue="1.03" pageId="35" pageNumber="43" unit="mm" value="1.03">1.03 mm</quantity>
(mean = 
<quantity id="5D5D9B6FFFE4FFB0FD84832EFD28CC53" box="[574,655,274,295]" metricMagnitude="-3" metricUnit="m" metricValue="1.02" pageId="35" pageNumber="43" unit="mm" value="1.02">1.02 mm</quantity>
