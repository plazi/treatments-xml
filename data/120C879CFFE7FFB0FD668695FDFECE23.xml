<quantity id="5D5D9B6FFFE4FFB0FDB182FEFD34CDA3" box="[523,659,193,215]" metricMagnitude="-3" metricUnit="m" metricValue="2.53" metricValueMax="2.55" metricValueMin="2.51" pageId="35" pageNumber="43" unit="mm" value="2.53" valueMax="2.55" valueMin="2.51">2.51–2.55 mm</quantity>
(mean = 
<quantity id="5D5D9B6FFFE4FFB0FEB782E0FEC2CD85" box="[269,357,220,241]" metricMagnitude="-3" metricUnit="m" metricValue="2.53" pageId="35" pageNumber="43" unit="mm" value="2.53">2.53 mm</quantity>
, 
<emphasis id="A8D1EA98FFE4FFB0FEC982E0FED8CD85" box="[371,383,220,241]" italics="true" pageId="35" pageNumber="43">n</emphasis>
