<quantity id="5D5D9B6FFFE4FFB0FEF68310FE74CC35" box="[332,467,300,322]" metricMagnitude="-4" metricUnit="m" metricValue="3.15" metricValueMax="3.5" metricValueMin="2.8" pageId="35" pageNumber="43" unit="mm" value="0.315" valueMax="0.35" valueMin="0.28">0.28–0.35 mm</quantity>
(mean = 
<quantity id="5D5D9B6FFFE4FFB0FD838310FD28CC35" box="[569,655,300,321]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="35" pageNumber="43" unit="mm" value="0.3">0.30 mm</quantity>
