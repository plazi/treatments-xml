</taxonomicName>
; simple in 
<taxonomicName id="4C44D3F1735DDC58FC639EADFB8426FB" box="[1006,1108,1177,1199]" class="Insecta" family="Staphylinidae" genus="Labomimus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="725" phylum="Arthropoda" rank="species" species="wuchaoi">
