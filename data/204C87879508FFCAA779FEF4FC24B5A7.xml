<taxonomicName id="6FE54D129508FFCAA5A4FCBFFE5EB5A7" authority="De Man, 1902" authorityName="De Man" authorityYear="1902" class="Malacostraca" family="Palaemonidae" genus="Periclimenella" kingdom="Animalia" order="Decapoda" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="spinifera">
