<taxonomicName id="E07ECE283272145573AF87C2ACE50C65" class="Insecta" family="Miridae" genus="Annona" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Annona muricata" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="muricata">Annona muricata</taxonomicName>
