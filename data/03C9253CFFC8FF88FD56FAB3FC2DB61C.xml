<geoCoordinate id="EE54F2EDFFC8FF86FD0FFA49FC9EB222" box="[678,821,1526,1550]" degrees="151,34696" direction="west" orientation="longitude" pageId="7" pageNumber="798" precision="1" value="-151.34695">151,34696°W</geoCoordinate>
