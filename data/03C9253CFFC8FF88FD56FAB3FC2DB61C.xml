<paragraph id="8BDF942AFFC8FF86FD0CFA18FB8CB205" blockId="7.[675,1211,1377,1699]" pageId="7" pageNumber="798">
LOCALITÉ 
<typeStatus id="54DB2A88FFC8FF86FCA6FA13FC95B190" box="[783,830,1452,1468]" pageId="7" pageNumber="798">TYPE</typeStatus>
