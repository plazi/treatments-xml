<geoCoordinate id="EE54F2EDFFC8FF86FBE9FA63FB1DB1D8" box="[1088,1206,1500,1524]" degrees="22,51851" direction="south" orientation="latitude" pageId="7" pageNumber="798" precision="1" value="-22.51851">22,51851°S</geoCoordinate>
