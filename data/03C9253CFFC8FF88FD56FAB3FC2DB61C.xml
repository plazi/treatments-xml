<taxonomicName id="4C60EFA9FFC8FF86FCCBFA63FC3DB1DF" box="[866,918,1500,1523]" class="Liliopsida" family="Musaceae" genus="Musa" kingdom="Plantae" order="Zingiberales" pageId="7" pageNumber="798" phylum="Tracheophyta" rank="genus">
