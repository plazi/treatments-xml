<quantity id="4C9839CFFFC8FF86FCE8FA49FCD1B222" box="[833,890,1526,1550]" metricMagnitude="1" metricUnit="m" metricValue="4.0" pageId="7" pageNumber="798" unit="m" value="40.0">40 m</quantity>
