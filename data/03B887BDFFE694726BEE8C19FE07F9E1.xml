; 
<taxonomicName id="4C114D28FFE694726F138CFCFA1FFB45" authority="Kieffer" authorityName="Kieffer" box="[1130,1428,1234,1260]" class="Insecta" family="Platygastridae" genus="Leptacis" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="bitensis">Leptacis bitensis Kieffer</taxonomicName>
