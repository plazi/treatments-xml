<taxonomicName id="2DD44D74FFAAF74EFF5DF9F8FE337B75" baseAuthorityName="Banks" baseAuthorityYear="1913" box="[180,483,1635,1661]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="costalis">
<emphasis id="D8A0EAE5FFAAF74EFF5DF9F8FE337B75" bold="true" box="[180,483,1635,1661]" italics="true" pageId="84" pageNumber="85">Cheumatopsyche costalis</emphasis>
