<taxonomicName id="4C16B9ACFF908D6CFF627154FD81FA91" box="[240,521,1302,1328]" class="Insecta" family="Phlaeothripidae" genus="Holoengythrips" kingdom="Animalia" order="Thysanoptera" pageId="13" pageNumber="138" phylum="Arthropoda" rank="species" species="undefined-1">
