</taxonomicName>
(= 
<taxonomicName id="83DE4D19FF8E8B39800ADCBBFA3E33E7" box="[1304,1419,644,668]" class="Magnoliopsida" family="Orobanchaceae" genus="Cistanche" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="17" pageNumber="218" phylum="Tracheophyta" rank="species" species="phelypaea">
<emphasis id="76AAEA88FF8E8B39800ADCBBFA3E33E7" box="[1304,1419,644,668]" italics="true" pageId="17" pageNumber="218">phelypaea</emphasis>
</taxonomicName>
), a parasite of 
<taxonomicName id="83DE4D19FF8E8B398434DC97FDF733BB" box="[294,578,680,704]" class="Magnoliopsida" family="Amaranthaceae" genus="Haloxylon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="17" pageNumber="218" phylum="Tracheophyta" rank="species" species="schweinfurthii">
<emphasis id="76AAEA88FF8E8B398434DC97FDF733BB" box="[294,578,680,704]" italics="true" pageId="17" pageNumber="218">Haloxylon schweinfurthii</emphasis>
</taxonomicName>
[= 
<taxonomicName id="83DE4D19FF8E8B398778DC97FC8F33BB" box="[618,826,680,704]" class="Magnoliopsida" family="Amaranthaceae" genus="Hammada" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="17" pageNumber="218" phylum="Tracheophyta" rank="species" species="elegans">
