<taxonomicName id="83DE4D19FF8E8B398151DCBBFB4733E7" box="[1091,1266,644,668]" class="Magnoliopsida" family="Orobanchaceae" genus="Cistanche" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="17" pageNumber="218" phylum="Tracheophyta" rank="species" species="lutea">
