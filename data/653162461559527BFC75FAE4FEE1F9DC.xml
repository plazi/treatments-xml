<typeStatus id="32236DF21559527AFCB8F8CFFC8FF827" box="[813,867,1919,1943]" pageId="6" pageNumber="327">
<collectionCode id="8B894B951559527AFCB8F8CFFC8FF827" box="[813,867,1919,1943]" pageId="6" pageNumber="327">TYPE</collectionCode>
</typeStatus>
