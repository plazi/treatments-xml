<quantity id="4CFF9B2EFFA49C4C34BAFD6CFBFEFD2D" box="[972,1066,707,731]" metricMagnitude="-3" metricUnit="m" metricValue="7.4" pageId="46" pageNumber="45" unit="mm" value="7.4">7.4 mm</quantity>
(mean= 
<quantity id="4CFF9B2EFFA49C4C33E3FD6CFB2CFD2D" box="[1173,1272,707,731]" metricMagnitude="-3" metricUnit="m" metricValue="6.9" pageId="46" pageNumber="45" unit="mm" value="6.9">6.9 mm</quantity>
, n=5). 
<emphasis id="B973EAD9FFA49C4C3221FD6CFA74FD2D" box="[1367,1440,707,731]" italics="true" pageId="46" pageNumber="45">Head:</emphasis>
