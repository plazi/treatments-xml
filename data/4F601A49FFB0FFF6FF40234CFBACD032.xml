<taxonomicName id="00C9D0DCFFBFFFF6FDF8266AFD6AD016" box="[559,674,1024,1048]" class="Insecta" family="Cicadellidae" genus="Macropsis" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="49" phylum="Arthropoda" rank="species" species="flavida">
