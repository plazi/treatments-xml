<taxonomicName id="00C9D0DCFFBFFFF6FB2B266AFA4AD016" box="[1276,1410,1025,1048]" class="Insecta" family="Cicadellidae" genus="Macropsis" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="49" phylum="Arthropoda" rank="species" species="recurvus">
