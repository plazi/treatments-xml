Project (
<collectionCode id="02C5AE672A2F052DFDCBFD0B95F1FCE7" box="[607,695,763,787]" pageId="7" pageNumber="8">ZADBI</collectionCode>
# 
<number id="D21C24852A2F052DFD44FD0B95BAFCE7" box="[720,764,763,787]" pageId="7" pageNumber="8" value="248.0">248</number>
, 
<collectionCode id="02C5AE672A2F052DFC9DFD0A9409FCE7" box="[777,847,762,787]" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="7" pageNumber="8">INBio</collectionCode>
