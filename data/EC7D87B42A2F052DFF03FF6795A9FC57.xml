<typeStatus id="BB6F88002A2F052DFF53FD269649FD1B" box="[199,271,726,751]" pageId="7" pageNumber="8">Types</typeStatus>
.
</emphasis>
<emphasis id="56A0EAB02A2F052DFE89FD2696C2FD1A" box="[285,388,726,750]" italics="true" pageId="7" pageNumber="8">
<typeStatus id="BB6F88002A2F052DFE89FD2696C2FD1A" box="[285,388,726,750]" pageId="7" pageNumber="8">Holotype</typeStatus>
</emphasis>
. Male, 
<collectingCountry id="1CC376322A2F052DFE74FD279527FD1B" box="[480,609,726,751]" name="Costa Rica" pageId="7" pageNumber="8">Costa Rica</collectingCountry>
, San José province, Moravia, 
<taxonomicName id="A3D44D212A2F052DFC55FD27934BFD1B" box="[961,1037,727,751]" class="Insecta" family="Cecidomyiidae" genus="Zadbimyia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="23" phylum="Arthropoda" rank="species" species="zurqui">Zurquí</taxonomicName>
de Moravia, 
<quantity id="A32C9B472A2F052DFB39FD269241FD1A" box="[1197,1287,726,751]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="7" pageNumber="8" unit="m" value="1600.0">1600 m</quantity>
, 
<date id="106A10622A2F052DFA82FD279794FCE7" pageId="7" pageNumber="8" value="2012-11-17" valueMax="2012-11-23" valueMin="2012-11-17">
<number id="D21C24852A2F052DFA82FD27921AFD1B" box="[1302,1372,727,751]" pageId="7" pageNumber="8" value="20.0" valueMax="23.0" valueMin="17.0">17–23</number>
Nov. 
<number id="D21C24852A2F052DFF03FD0B9794FCE7" box="[151,210,763,787]" pageId="7" pageNumber="8" value="2012.0">2012</number>
</date>
, emergence trap, 
