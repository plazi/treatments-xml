<taxonomicName id="4C6A4D18FFF5B311FAD2FA0F21FB4F9E" authorityName="Woodward" authorityYear="1901" class="Actinopterygii" family="Osteoglossiformes (awaiting allocation)" genus="Brychaetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Osteoglossiformes" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="muelleri">
