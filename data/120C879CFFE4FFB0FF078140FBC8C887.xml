<collectingRegion id="5861F868FFE4FFB0FBDA8702FB0FC827" box="[1120,1192,1342,1363]" country="Canada" name="Quebec" pageId="35" pageNumber="43">Québec</collectingRegion>
and 
<collectingRegion id="5861F868FFE4FFB0FD488765FCEAC81A" box="[754,845,1369,1390]" country="United States of America" name="Michigan" pageId="35" pageNumber="43">Michigan</collectingRegion>
to 
<collectingRegion id="5861F868FFE4FFB0FCC08765FC71C81A" box="[890,982,1369,1390]" country="United States of America" name="Kentucky" pageId="35" pageNumber="43">Kentucky</collectingRegion>
