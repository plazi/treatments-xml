<quantity id="5D5D9B6FFFE4FFB0FCFF8625FC63C95A" box="[837,964,1048,1070]" metricMagnitude="-4" metricUnit="m" metricValue="2.1" metricValueMax="2.6" metricValueMin="1.6" pageId="35" pageNumber="43" unit="mm" value="0.21" valueMax="0.26" valueMin="0.16">0.16–0.26 mm</quantity>
(mean = 
