<collectingCountry id="1CDC760B5146BF3AFB32DBAAFA9B0EFA" box="[1194,1282,390,416]" name="Mexico" pageId="2" pageNumber="171">Mexico</collectingCountry>
, and south to 
<geoCoordinate id="01FF505C5146BF3AFF0FDB86FF7E0E9E" box="[151,231,426,452]" degrees="38" direction="south" minutes="6" orientation="latitude" pageId="2" pageNumber="171" precision="925" value="-38.1">38°6’S</geoCoordinate>
, 
<geoCoordinate id="01FF505C5146BF3AFF6BDB86FEC30E9E" box="[243,346,426,452]" degrees="88" direction="west" minutes="02" orientation="longitude" pageId="2" pageNumber="171" precision="925" value="-88.03333">88°02’W</geoCoordinate>
(
<bibRefCitation id="005A4B6A5146BF3AFEF0DB86FDFF0E9E" author="Garcia Madrigal, M. S." box="[360,614,426,452]" pageId="2" pageNumber="171" pagination="63 - 195" refId="ref14782" refString="Garcia Madrigal, M. S. (2007) Annotated checklist of the amphipods (Peracarida: Amphipoda) from the tropical eastern Pacific. Contributions to the Study of East Pacific Crustaceans, 4 (2), 63 - 195." type="journal article" year="2007">García Madrigal 2007</bibRefCitation>
