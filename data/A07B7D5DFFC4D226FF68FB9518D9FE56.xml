<taxonomicName id="EFD2B7C8FFC4D227FAC8F86C184EF83A" authority="Heyden" authorityName="Heyden" authorityYear="1775" class="Insecta" family="Meloidae" genus="Mylabris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="parumpicta" subGenus="Mylabris">
