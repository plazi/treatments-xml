<typeStatus id="F76972E9FFC4D227FF36F9601B22F91A" box="[201,358,1707,1732]" pageId="34" pageNumber="35">Paralectotype</typeStatus>
: 
<specimenCount id="3ED407C2FFC4D227FE8CF9671B93F91A" box="[371,471,1707,1732]" pageId="34" pageNumber="35" type="female">
<number id="9E1ADE6CFFC4D227FE8CF9671BC5F91A" box="[371,385,1708,1732]" pageId="34" pageNumber="35" value="1.0">1</number>
