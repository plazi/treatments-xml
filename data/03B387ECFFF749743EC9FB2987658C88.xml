<emphasis id="B96EEAE8FFF849763EC9F8F487488AC1" bold="true" box="[151,347,1905,1929]" pageId="8" pageNumber="9">FIGURES 31–38</emphasis>
. 
<taxonomicName id="4C1A4D79FFF849763F34F8F484698AC0" box="[362,634,1905,1928]" class="Insecta" family="Cicadellidae" genus="Agalliopsis" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="pentaspinata" status="sp. nov.">
<emphasis id="B96EEAE8FFF849763F34F8F484698AC0" box="[362,634,1905,1928]" italics="true" pageId="8" pageNumber="9">Agalliopsis pentaspinata</emphasis>
</taxonomicName>
<emphasis id="B96EEAE8FFF849763CDFF8F484C98AC1" bold="true" box="[641,730,1905,1929]" pageId="8" pageNumber="9">
