<subSubSection id="C37B6578FFFAFF80FF12A0C6FDD2FD91" lastPageId="18" lastPageNumber="219" pageId="17" pageNumber="218" type="materials_examined">
<paragraph id="8BDE36F3FFFAFF83FF12A0C6FD48F874" blockId="17.[151,672,2009,2034]" box="[151,672,2009,2034]" pageId="17" pageNumber="218">
<emphasis id="B915EAE1FFFAFF83FF12A0C6FEDAF874" bold="true" box="[151,306,2009,2034]" pageId="17" pageNumber="218">
<typeStatus id="54DA8851FFFAFF83FF12A0C6FF3AF874" box="[151,210,2009,2034]" pageId="17" pageNumber="218">Type</typeStatus>
