<quantity id="4CEF9B25FFC7FFE39A18FE0DF8ED321C" box="[613,684,511,537]" metricMagnitude="2" metricUnit="m" metricValue="3.11" pageId="86" pageNumber="87" unit="m" value="311.0">311 m</quantity>
