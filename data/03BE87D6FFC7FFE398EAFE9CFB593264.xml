<taxonomicName id="4C174D43FFC7FFE39996FDD1F8983238" authorityName="Jordan &amp; Starks" authorityYear="1907" box="[491,729,547,573]" class="Actinopterygii" family="Antennariidae" genus="Antennarius" kingdom="Animalia" order="Lophiiformes" pageId="86" pageNumber="87" phylum="Chordata" rank="species" species="avalonis">
