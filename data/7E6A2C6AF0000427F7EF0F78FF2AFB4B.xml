<taxonomicName id="31C3E6FFF0020427F6550E8DFE2CFC56" box="[301,487,973,996]" class="Insecta" family="Cicadellidae" genus="Alapona" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="190" phylum="Arthropoda" rank="species" species="modesta" status="sp. nov.">
