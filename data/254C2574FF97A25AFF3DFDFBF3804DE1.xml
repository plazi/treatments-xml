<taxonomicName id="6AE5EFE1FF96A25AFD31FD25F59F4D3C" authority="Wang, Liu" authorityName="Wang, Liu" box="[667,926,764,789]" class="Insecta" family="Tettigoniidae" genus="Phlugiolopsis" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="168" phylum="Arthropoda" rank="species" species="punctata">
