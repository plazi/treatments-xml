<taxonomicName id="4C594D2DFFFEFFBEFDACF9DBFDB66629" authority="Ponomarenko &amp; Omelko &amp; Omelko, 2021" authorityName="Ponomarenko &amp; Omelko &amp; Omelko" authorityYear="2021" box="[530,663,1542,1565]" class="Insecta" family="Gelechiidae" genus="Tawaya" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="474" phylum="Arthropoda" rank="species" species="armeniaca" status="sp. n.">
