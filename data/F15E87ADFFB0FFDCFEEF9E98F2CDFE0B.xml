<number id="CF3F249CFFB1FFD9FE8F9C7FF06EFBE8" box="[360,374,1099,1125]" pageId="9" pageNumber="10" value="9.0">9</number>
(
<number id="CF3F249CFFB1FFD9FE6F9C7FF081FBE8" box="[392,409,1099,1125]" pageId="9" pageNumber="10" value="9.0">9</number>
