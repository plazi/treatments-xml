<taxonomicName id="4C7F4D45FF96986A181BCF40FE6FE434" box="[356,477,1305,1332]" class="Insecta" family="Ortheziidae" genus="Ortheziola" kingdom="Animalia" order="Hemiptera" pageId="48" pageNumber="349" phylum="Arthropoda" rank="genus">
