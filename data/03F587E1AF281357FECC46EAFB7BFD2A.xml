<collectionCode id="ED4DAE32AF281354FBA04065FB7AFA4F" LSID="urn:lsid:biocol.org:col:34988" box="[1124,1216,1393,1419]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="5" pageNumber="6">MNHN</collectionCode>
