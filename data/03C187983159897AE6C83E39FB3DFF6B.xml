<typeStatus id="54D3882C31598978E6C83E89FF01F8DC" box="[151,204,1493,1516]" pageId="1" pageNumber="44">Type</typeStatus>
species 
<taxonomicName id="4C684D0D31598978E7793E89FD48F8DC" authority="Karg, 1963" authorityName="Karg" authorityYear="1963" box="[294,645,1493,1516]" class="Arachnida" family="Eviphididae" genus="Alliphis" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="sculpturatus">
