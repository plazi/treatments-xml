<collectingCountry id="99FB91C8ED55FF87FF60FCDCFF43A016" box="[151,232,798,822]" name="Poland" pageId="1" pageNumber="44">Poland</collectingCountry>
, 
<collectingCountry id="99FB91C8ED55FF87FF01FCDCFEF8A016" box="[246,339,798,822]" name="Sweden" pageId="1" pageNumber="44">Sweden</collectingCountry>
