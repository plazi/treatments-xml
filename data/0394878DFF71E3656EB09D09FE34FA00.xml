, EL = 
<quantity id="4CC59B7EFF71E3686EF09CD2FEF5FA37" box="[215,303,1418,1442]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="117" pageNumber="118" unit="mm" value="1.8">1.8 mm</quantity>
, PYL = 
