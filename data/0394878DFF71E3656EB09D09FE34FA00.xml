, PYW = 
<quantity id="4CC59B7EFF71E3686C6A9CD2FD7EFA37" box="[589,676,1418,1442]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="117" pageNumber="118" unit="mm" value="0.7">0.7 mm</quantity>
, AL = 
<quantity id="4CC59B7EFF71E3686CD49CD2FC91FA37" box="[755,843,1418,1442]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="117" pageNumber="118" unit="mm" value="0.7">0.7 mm</quantity>
