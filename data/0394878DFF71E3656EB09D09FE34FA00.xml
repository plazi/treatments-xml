, PL = 
<quantity id="4CC59B7EFF71E3686AA09C3DFB37FAEB" box="[1159,1261,1381,1406]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" pageId="117" pageNumber="118" unit="mm" value="1.25">1.25 mm</quantity>
, PW = 
