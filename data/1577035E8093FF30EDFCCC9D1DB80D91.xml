<quantity id="5A261FAD8093FF30EEBCCDFD1D670D90" box="[983,1082,980,1004]" metricMagnitude="1" metricUnit="m" metricValue="5.0" metricValueMax="10.0" metricValueMin="0.0" pageId="151" pageNumber="152" unit="m" value="50.0" valueMax="100.0" valueMin="0.0">0–100 m</quantity>
