<subSubSection id="17AD6561FFC1FFE2FF10FD8CFBDC530D" box="[192,1114,611,635]" pageId="39" pageNumber="38" type="type_taxon">
<paragraph id="5F0836EAFFC1FFE2FF10FD8CFBDC530D" blockId="39.[192,1114,611,635]" box="[192,1114,611,635]" pageId="39" pageNumber="38">
<emphasis id="6DC3EAF8FFC1FFE2FF10FD8CFEEB530D" bold="true" box="[192,365,611,635]" pageId="39" pageNumber="38">
<typeStatus id="800C8848FFC1FFE2FF10FD8CFE82530D" box="[192,260,611,635]" pageId="39" pageNumber="38">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="98B74D69FFC1FFE2FEABFD8BFC0E530D" authority="Peck and Cook" authorityName="Peck and Cook" authorityYear="2014" box="[379,904,611,635]" class="Insecta" family="Leiodidae" genus="Parvocyrtusa" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="38" phylum="Arthropoda" rank="species" species="hispaniolensis">
<emphasis id="6DC3EAF8FFC1FFE2FEABFD8BFD48530D" box="[379,718,611,635]" italics="true" pageId="39" pageNumber="38">Parvocyrtusa hispaniolensis</emphasis>
Peck and Cook
</taxonomicName>
