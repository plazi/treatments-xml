<subSubSection id="BE3A09042CBA9DF3DF455B7F0B4C473D" pageId="177" pageNumber="178" type="nomenclature">
<paragraph id="EB53A8788E4B8780F03B8A23570B8FBA" pageId="177" pageNumber="178">
Subgenus 
<taxonomicName id="25E8D3F6EE5FAF8B12D86AFF0ECE4225" LSID="135A5736-08C1-5D54-85A9-E332D4AB1834" authority="Kirby, 1837" authorityName="Kirby" authorityYear="1837" class="Hexapoda" family="Carabidae" genus="Calosoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="(Chrysostigma)" order="Coleoptera" pageId="177" pageNumber="178" phylum="Arthropoda" rank="subGenus" subGenus="Chrysostigma">Chrysostigma Kirby, 1837</taxonomicName>
