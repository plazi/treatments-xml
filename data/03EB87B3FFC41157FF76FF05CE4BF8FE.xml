<taxonomicName id="4C424D26FFC51157FD97F8BDCD9EF8D2" box="[630,753,1839,1863]" class="Arachnida" family="Pygmephoridae" genus="Allopygmephorus" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="270" phylum="Arthropoda" rank="species" species="persicus">
