<paragraph id="8BF4425FFF9C7401FF5DCCCAFDC7FD56" blockId="8.[151,1437,458,1288]" pageId="8" pageNumber="9">
<emphasis id="B93F9E4DFF9C7401FF5DCCCAFED7FD06" bold="true" box="[198,369,658,684]" pageId="8" pageNumber="9">
<typeStatus id="54F0FCFDFF9C7401FF5DCCCAFEA2FD06" box="[198,260,658,684]" pageId="8" pageNumber="9">Type</typeStatus>
