in 
<collectionCode id="ED7E19884762FF853FECFADE8286FA36" box="[1177,1253,1387,1411]" pageId="1" pageNumber="65">CLBC</collectionCode>
, 
<collectionCode id="ED7E19884762FF853F84FADE8321FA36" box="[1265,1346,1387,1411]" pageId="1" pageNumber="65">FRCM</collectionCode>
, 
<collectionCode id="ED7E19884762FF853E38FADE83FBFA36" LSID="urn:lsid:biocol.org:col:33834" box="[1357,1432,1387,1411]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33834" name="Museo del Instituto de Zoologia Agricola Francisco Fernandez Yepez" pageId="1" pageNumber="65">MIZA</collectionCode>
