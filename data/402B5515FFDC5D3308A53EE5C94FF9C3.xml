<taxonomicName id="0F829F80FFDC5D330A273896C8CBFA27" authorityName="Gutu" authorityYear="1991" box="[533,732,1504,1530]" class="Malacostraca" family="Apseudidae" genus="Paradoxapseudes" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="152" phylum="Arthropoda" rank="genus">
