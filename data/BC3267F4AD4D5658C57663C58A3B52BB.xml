<taxonomicName id="C9EE8F95AF0BEEAA4E12625A71EF304B" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">Trigonopterus</taxonomicName>
