<taxonomicName id="04481E9F8903EE8AC290066224562571" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="genus">Trigonopterus</taxonomicName>
