<taxonomicName id="4C3E63FA2F70FFCA0321FCDEE2402DD4" box="[382,586,878,904]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="species" species="burkillii">
