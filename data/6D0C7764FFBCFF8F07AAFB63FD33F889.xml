, 
<location id="E07A90A9FFBCFF8F05BAF9D3FD1AF995" LSID="urn:lsid:plazi:treatment:6D0C7764FFBCFF8F07AAFB63FD33F889:E07A90A9FFBCFF8F05BAF9D3FD1AF995" box="[647,695,1591,1617]" country="Peru" municipality="Urubamba Province" name="Inca" pageId="15" pageNumber="16" stateProvince="Cusco">Inca</location>
