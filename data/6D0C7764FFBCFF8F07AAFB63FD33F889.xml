<taxonomicName id="22A5BDF1FFBCFF8F044CF90FFC4BF8C1" box="[881,998,1771,1797]" class="Arachnida" family="Theraphosidae" genus="Avicularia" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
