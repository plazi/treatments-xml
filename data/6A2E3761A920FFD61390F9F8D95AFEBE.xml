<taxonomicName id="2587FDF4A920FFD71728F942D97CF91F" authorityName="Stimpson" authorityYear="1864" box="[1071,1311,1713,1737]" class="Ascidiacea" family="Corellidae" genus="Chelyosoma" kingdom="Animalia" order="Enterogona" pageId="18" pageNumber="419" phylum="Chordata" rank="species" species="productum">
