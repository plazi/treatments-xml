<paragraph id="9375745EFCCE4F3BE935B040F75115C2" pageId="0" pageNumber="73">
Two specimens 
<quantity id="2DD99892C80A6021256476CD7112EF9A" metricMagnitude="-3" metricUnit="m" metricValue="9.5" metricValueMax="12.0" metricValueMin="7.0" unit="mm" value="9.5" valueMax="12.0" valueMin="7.0">7-12 mm</quantity>
