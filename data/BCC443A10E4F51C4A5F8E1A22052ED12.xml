<taxonomicName id="E1D9C6F05ADF5F913BEAC6BFEE0D397C" baseAuthorityName="Kusnezov" baseAuthorityYear="1916" class="Insecta" family="Cosmopterigidae" genus="Haplochrois" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haplochrois theae" order="Lepidoptera" pageId="0" pageNumber="405" phylum="Arthropoda" rank="species" species="theae">
<emphasis id="AD20F2069850B6FEDB21332AAFF5B8DE" italics="true" pageId="0" pageNumber="405">Haplochrois theae</emphasis>
</taxonomicName>
