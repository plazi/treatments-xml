<paragraph id="CB1136D6A851FF8EFF15FC99FEC20AB5" blockId="73.[128,654,788,943]" pageId="73" pageNumber="73">
<treatmentCitationGroup id="EBBE11F8A851FF8EFF15FC99FEC20AB5" pageId="73" pageNumber="73">
Now 
<taxonomicName id="0CAE4D55A851FF8EFF53FC99FF6A0A63" authority="cassinii (Lawrence, 1861)" authorityName="cassinii (Lawrence" authorityYear="1861" baseAuthorityName="Lawrence" baseAuthorityYear="1861" class="Aves" family="Thraupidae" genus="Mitrospingus" kingdom="Animalia" order="Passeriformes" pageId="73" pageNumber="73" phylum="Chordata" rank="subSpecies" species="cassinii" subSpecies="cassinii">
