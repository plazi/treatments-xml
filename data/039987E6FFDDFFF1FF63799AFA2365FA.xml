<taxonomicName id="4C304D73FFDDFFF1FEDC78EFFE8763E2" authority=": Morgan &amp; Mackay, 2017: 396" authorityName=": Morgan &amp; Mackay" authorityPageNumber="396" authorityYear="2017" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="926" phylum="Arthropoda" rank="species" species="vermiculata">
