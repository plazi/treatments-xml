<taxonomicName id="4C554D615B63856FFD676A568F8C7B50" authority="Bernhauer, 1933" authorityName="Bernhauer" authorityYear="1933" box="[688,1191,1325,1347]" class="Insecta" family="Staphylinidae" genus="Staphylinus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="459" phylum="Arthropoda" rank="species" species="beckeri" subGenus="Ascialinus">
