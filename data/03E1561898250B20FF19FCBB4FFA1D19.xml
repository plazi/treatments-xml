<taxonomicName id="4C489C8D98250B20FF19FC1A4FE719B9" authority="(Kiesenwetter)" baseAuthorityName="Kiesenwetter" baseAuthorityYear="1851" box="[192,676,994,1018]" class="Insecta" family="Heteroceridae" genus="Centuriatus" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="29" phylum="Arthropoda" rank="species" species="auromicans">
