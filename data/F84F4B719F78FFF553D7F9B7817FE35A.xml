<taxonomicName id="B7E681E49F78FFF453D7F9B78538E46E" authorityName="Hasegawa &amp; Syafruddin" authorityYear="1994" box="[151,324,1592,1619]" class="Chromadorea" family="Heligmonellidae" genus="Hasanuddinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="14" pageNumber="121" phylum="Nematoda" rank="genus">
