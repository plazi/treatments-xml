<collectionCode id="ED4705B0FFB0FFB22CEC7F08FCE37739" box="[714,853,1617,1640]" pageId="47" pageNumber="486">TYPE SPECIES</collectionCode>
. — 
<taxonomicName id="4C56E6F6FFB0FFB22DAF7F0BFCEE77DA" authority="de Saint Laurent, 1973" authorityName="de Saint Laurent" authorityYear="1973" class="Malacostraca" family="Callianassidae" genus="Calliapagurops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="47" pageNumber="486" phylum="Arthropoda" rank="species" species="charcoti">
