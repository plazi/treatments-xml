<collectorName id="26FA53628706FFC159ADC41DFE48F957" box="[509,599,1663,1689]" pageId="82" pageNumber="83">Cerrado</collectorName>
vegetation, 
<taxonomicName id="4C0F4D378706FFC15AB9C41DF92BF954" authority="(D. Burckhardt &amp; D. L. Queiroz)" baseAuthorityName="D. Burckhardt &amp; D. L. Queiroz" box="[745,1332,1663,1690]" class="Insecta" family="Psychidae" genus="Pterogyne" kingdom="Animalia" order="Lepidoptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="nitens">
<emphasis id="B97BEAA68706FFC15AB9C41DFFB5F956" box="[745,938,1663,1688]" italics="true" pageId="82" pageNumber="83">Pterogyne nitens</emphasis>
(D. Burckhardt &amp; D.L. Queiroz)
</taxonomicName>
, #267(1) (
<collectionCode id="ED1EAE718706FFC158CFC4C1FCE8F973" box="[159,247,1699,1725]" country="0" httpUri="http://grbio.org/cool/1d5a-4efs" name="Natural History Museum Bucharest" pageId="82" pageNumber="83">NHMB</collectionCode>
, slide mounted, 70% ethanol)
