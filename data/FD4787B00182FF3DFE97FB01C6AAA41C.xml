<taxonomicName id="B2EE4D250182FF3DFBEDF983C733A57C" baseAuthorityName="Valenciennes" baseAuthorityYear="1837" box="[1146,1368,1590,1609]" class="Actinopterygii" family="Prochilodontidae" genus="Prochilodus" kingdom="Animalia" order="Characiformes" pageId="135" pageNumber="520" phylum="Chordata" rank="species" species="lineatus">
