<taxonomicName id="B2EE4D250182FF3DFC58F8A3C6D1A41C" box="[975,1210,1814,1833]" class="Actinopterygii" family="Characidae" genus="Markiana" kingdom="Animalia" order="Characiformes" pageId="135" pageNumber="520" phylum="Chordata" rank="species" species="nigripinnis">
<emphasis id="479AEAB40182FF3DFC58F8A3C6D1A41C" box="[975,1210,1814,1833]" italics="true" pageId="135" pageNumber="520">Markiana nigripinnis</emphasis>
