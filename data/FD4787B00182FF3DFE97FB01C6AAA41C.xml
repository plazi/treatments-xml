<taxonomicName id="B2EE4D250182FF3DFCBEF943C187A43C" box="[809,1004,1782,1801]" class="Actinopterygii" family="Characidae" kingdom="Animalia" order="Characiformes" pageId="135" pageNumber="520" phylum="Chordata" rank="subFamily" subFamily="Tetragonopterinae">Tetragonopterinae</taxonomicName>
