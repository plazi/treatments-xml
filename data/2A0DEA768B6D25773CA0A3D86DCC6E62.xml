<taxonomicName id="1389EEDBF5CCA905B7314104372C5B33" authority="L." class="Magnoliopsida" family="Saxifragaceae" genus="Saxifraga" kingdom="Plantae" order="Saxifragales" pageId="null" pageNumber="282" phylum="Tracheophyta" rank="species" species="cotyledon">
<title id="858D387B1F5515E7B40A2C1B10402A49" pageId="null" pageNumber="282">
<pageBreakToken id="C27CFAFB230FE0D53AA4B523E91B661E" pageId="null" pageNumber="282">Saxifraga</pageBreakToken>
Cotyledon 
<authorityName id="215059D8A90DED6CFCA4479EC2A2B0EA" pageId="null" pageNumber="282">L.</authorityName>
</title>
</taxonomicName>
</paragraph>
