<taxonomicName id="A9804D339C0EFFF5FF10FDF3FEDCFD2C" box="[151,328,620,645]" class="Magnoliopsida" family="Tamaricaceae" genus="Tamarix" higherTaxonomySource="GBIF" kingdom="Plantae" order="Violales" pageId="15" pageNumber="276" phylum="Magnoliophyta" rank="species" species="gallica">
