<number id="5C1DD454FFDFFFF6B36FFAFAF31614DA" box="[292,333,1404,1426]" pageId="11" pageNumber="222" value="174.0">174</number>
; 
<bibRefCitation id="8E44BB82FFDFFFF6B31CFAFAF38314DA" author="Fisher" box="[343,472,1404,1426]" pageId="11" pageNumber="222" refString="Fisher, W. K. (1906) The starfishes of the Hawaiian islands. Bulletin of the United States Fish Commission, 23, 987 - 1130." type="journal article" year="1906">
