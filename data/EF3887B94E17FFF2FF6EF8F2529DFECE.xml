<taxonomicName id="A0914D2C4E17FFF3FBEEF84F56D2F80F" box="[1047,1158,1962,1987]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
