<collectionCode id="ED207DF91951FFBD6B2D911F1E8FFEA0" box="[337,430,273,294]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="110" pageNumber="110">AMNH</collectionCode>
