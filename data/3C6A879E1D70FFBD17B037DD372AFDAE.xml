<typeStatus id="6B78882A1D70FFBC17B0366A34AFF8B0" box="[151,209,1833,1857]" pageId="42" pageNumber="43">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="73C34D0B1D70FFBC161D366B37A7F8B1" authority="Fain and Lukoschus, 1970" authorityName="Fain and Lukoschus" authorityYear="1970" box="[314,985,1832,1856]" class="Arachnida" family="Psoroptidae" genus="Listropsoralgoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="surinamensis">
