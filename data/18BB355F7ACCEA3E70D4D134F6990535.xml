<taxonomicName id="B286756BC5C83C14DFED4D5F98B7909F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2433" class="Insecta" family="Formicidae" genus="Sphinctomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphinctomyrmex Mayr" lsidName-HNS="Sphinctomyrmex Mayr" order="Hymenoptera" pageNumber="49" phylum="Arthropoda" rank="genus">Sphinctomyrmex</taxonomicName>
