<taxonomicName id="4C694D6D1B1CC411FE8AA5A2FE55CD93" box="[264,418,1412,1437]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="8" pageNumber="53" phylum="Tracheophyta" rank="species" species="rubens">
