<taxonomicName id="5805CAEEE9F240B75500B3C056E98B8B" class="Polychaeta" family="Terebellidae" genus="Lanice" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lanice conchilega" order="Terebellida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="conchilega">Lanice conchilega</taxonomicName>
