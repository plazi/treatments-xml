<taxonomicName id="27842CB2FFF4FF88FCF06D44FAE4FEFF" authority="Watson, 2003: 168" authorityName="Watson" authorityPageNumber="168" authorityYear="2003" box="[831,1284,320,342]" class="Hydrozoa" family="Sertulariidae" genus="Calamphora" kingdom="Animalia" order="Leptothecata" pageId="42" pageNumber="297" phylum="Cnidaria" rank="species" species="quadrispinosa">
