<taxonomicName id="3DE52BC52F78FFC29FF6F8ED3DDA6407" box="[1023,1201,1879,1905]" class="Arachnida" family="Linyphiidae" genus="Dicymbium" kingdom="Animalia" order="Araneae" pageId="31" pageNumber="308" phylum="Arthropoda" rank="species" species="sinofacetum" status="sp. n.">
<emphasis id="C8918C542F78FFC29FF6F8ED3D7F6407" box="[1023,1044,1879,1904]" italics="true" pageId="31" pageNumber="308">D</emphasis>
