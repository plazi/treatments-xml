<taxonomicName id="3DE52BC52F78FFC29FCDF8953D98643E" authority="Menge 1868" authorityName="Menge" authorityYear="1868" box="[964,1267,1839,1865]" class="Arachnida" family="Linyphiidae" genus="Dicymbium" kingdom="Animalia" order="Araneae" pageId="31" pageNumber="308" phylum="Arthropoda" rank="genus">
<emphasis id="C8918C542F78FFC29FCDF8953D20643F" box="[964,1099,1839,1864]" italics="true" pageId="31" pageNumber="308">Dicymbium</emphasis>
Menge 
<number id="4C2D42612F78FFC298BFF8953D98643E" box="[1206,1267,1839,1865]" pageId="31" pageNumber="308" value="1868.0">1868</number>
</taxonomicName>
