(unknown in 
<taxonomicName id="4C404D77FFEF2A08FEF7D26FFE1FFBBC" authorityName="Olmi, Copeland &amp; Guglielmino" authorityYear="2015" box="[350,510,1057,1083]" class="Insecta" family="Dryinidae" genus="Conganteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="lymanorum">
<emphasis id="B934EAE6FFEF2A08FEF7D26FFE1FFBBC" box="[350,510,1057,1083]" italics="true" pageId="64" pageNumber="65">C. lymanorum</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C404D77FFEF2A08FD9DD26FFD10FBBC" authorityName="Olmi" authorityYear="1991" box="[564,753,1057,1083]" class="Insecta" family="Dryinidae" genus="Conganteon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="transvaalense">
<emphasis id="B934EAE6FFEF2A08FD9DD26FFD10FBBC" box="[564,753,1057,1083]" italics="true" pageId="64" pageNumber="65">C. transvaalense</emphasis>
