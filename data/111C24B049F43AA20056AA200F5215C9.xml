<paragraph id="3279EE107E0809D26586DC0D71272D16" pageId="6" pageNumber="105">
<taxonomicName id="40BE8148363FEA847D45A46276E9C582" class="Insecta" family="Carabidae" genus="Wanoblemus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Wanoblemus wui" order="Coleoptera" pageId="6" pageNumber="105" phylum="Arthropoda" rank="species" species="wui">Wanoblemus wui</taxonomicName>
