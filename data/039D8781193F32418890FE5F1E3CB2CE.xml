<collectingCountry id="F3237607193F324188C1F9F71E01B326" box="[198,363,1597,1623]" name="Kiribati" pageId="5" pageNumber="91">Gilbert Islands</collectingCountry>
and 
<collectingCountry id="F3237607193F32418999F9F71D6FB326" box="[414,517,1597,1623]" name="Australia" pageId="5" pageNumber="91">Australia</collectingCountry>
(
<bibRefCitation id="EFA54B66193F32418A14F9F71C4CB326" author="Banner &amp; Banner" box="[531,806,1597,1623]" firstAuthor="Banner" pageId="5" pageNumber="91" refId="ref7716" refString="Banner, A. H. &amp; Banner, D. M. (1966 a) The alpheid shrimp of Thailand. The Siam Society Monograph Series. Vol. 3. The Siam Society, Bangkok, 168 pp." type="book" year="1966" yearSuffix="a">Banner &amp; Banner 1966a</bibRefCitation>
, 
<bibRefCitation id="EFA54B66193F32418B36F9F61C03B326" author="Banner &amp; Banner" box="[817,873,1596,1623]" firstAuthor="Banner" pageId="5" pageNumber="91" pagination="267 - 389" refId="ref7867" refString="Banner, D. M., &amp; Banner, A. H. (1975) The alpheid shrimp of Australia. Part 2: The Genus Synalpheus. Records of the Australian Museum, 29 (12), 267 - 389, figs. 1 - 29. https: // doi. org / 10.3853 / j. 0067 - 1975.29.1975.389" type="journal article" year="1975">1975</bibRefCitation>
, 
<bibRefCitation id="EFA54B66193F32418B71F9F71B2CB326" author="Wang &amp; Sha" box="[886,1094,1596,1623]" firstAuthor="Wang" pageId="5" pageNumber="91" pagination="357 - 435" refId="ref11566" refString="Wang, Y-R. &amp; Sha, Z-L. (2015) A review of the genus Synalpheus (Crustacea: Decapoda: Caridea: Alpheidae) from China seas. Zoological Systematics, 40 (4), 357 - 435." type="journal article" year="2015">Wang &amp; Sha 2015</bibRefCitation>
). In 
<collectingCountry id="F3237607193F32418C72F9F71BDBB326" box="[1141,1201,1597,1623]" name="India" pageId="5" pageNumber="91">India</collectingCountry>
