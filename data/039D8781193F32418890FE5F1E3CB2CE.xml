<taxonomicName id="4C344D14193F32418B41F9601B6DB3B2" authority="Lovén, 1866" authorityName="Lovén" authorityYear="1866" baseAuthorityName="Carpenter" baseAuthorityYear="1888" box="[838,1031,1705,1731]" class="Crinoidea" family="Comatulidae" genus="Phanogenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="5" pageNumber="91" phylum="Echinodermata" rank="species" species="multibrachiata">
