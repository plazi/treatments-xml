<taxonomicName id="4C344D14193F32418CF2F94F1FB7B3B2" baseAuthorityName="Muller" baseAuthorityYear="1841" class="Crinoidea" family="Comasteridae" genus="Comaster" kingdom="Animalia" order="Comatulida" pageId="5" pageNumber="91" phylum="Echinodermata" rank="species" species="multifidus">
<emphasis id="B940EA85193F32418CF2F94F1FB7B3B2" italics="true" pageId="5" pageNumber="91">Comaster multifidus</emphasis>
</taxonomicName>
(Kalpeni Island), 
<taxonomicName id="4C344D14193F324189AEF9631DE7B3B2" authority="Lovén, 1866" authorityName="Lovén" authorityYear="1866" baseAuthorityName="Hartlaub" baseAuthorityYear="1893" box="[425,653,1705,1731]" class="Crinoidea" family="Comatulidae" genus="Phanogenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="5" pageNumber="91" phylum="Echinodermata" rank="species" species="gracilis">
<emphasis id="B940EA85193F324189AEF9631DE7B3B2" box="[425,653,1705,1731]" italics="true" pageId="5" pageNumber="91">Phanogenia gracilis</emphasis>
</taxonomicName>
