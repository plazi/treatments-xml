<taxonomicName id="4C294D361812F33E51B57CA0639B9BAE" authority="A. Milne-Edwards, 1880" authorityName="A. Milne-Edwards" authorityYear="1880" box="[151,712,1449,1476]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="386" phylum="Arthropoda" rank="species" species="nitidus">
