<taxonomicName id="A1EAC38009632C73FCBFF87FFCF0078D" box="[788,893,1944,1965]" class="Insecta" family="Sphaeroceridae" genus="Albistyla" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="262" phylum="Arthropoda" rank="species" species="occulta">
<emphasis id="549E641109632C73FCBFF87FFCF0078D" box="[788,893,1944,1965]" italics="true" pageId="5" pageNumber="262">
