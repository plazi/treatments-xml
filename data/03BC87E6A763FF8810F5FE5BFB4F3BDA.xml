<number id="3DDD24D7A763FF8814D8FBC0FB183AEF" box="[1210,1266,1151,1175]" pageId="2" pageNumber="430" value="1874.0">1874</number>
</taxonomicName>
, 
<taxonomicName id="4C154D73A763FF881563FBC1FED53AC0" authority="Boettger, 1888" authorityName="Boettger" authorityYear="1888" class="Reptilia" family="Gekkonidae" genus="Crossobamon" kingdom="Animalia" order="Squamata" pageId="2" pageNumber="430" phylum="Chordata" rank="genus">
