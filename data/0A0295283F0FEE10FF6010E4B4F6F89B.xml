<emphasis id="B0DFF82C3F0FEE10FF601394B603F97B" bold="true" box="[192,373,1757,1781]" pageId="25" pageNumber="24">New records.</emphasis>
<collectingRegion id="406FEADC3F0FEE10FEDB1394B571F97A" box="[379,519,1757,1781]" country="Guatemala" name="Santa Rosa" pageId="25" pageNumber="24">Santa Rosa</collectingRegion>
