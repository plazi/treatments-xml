<taxonomicName id="6ABDAD915A008643986ACA82379B4BB3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179359" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName="Monomorium medinae fridae Forel" lsidName-HNS="Monomorium medinae fridae Forel" order="Hymenoptera" pageNumber="183" phylum="Arthropoda" rank="subSpecies" species="medinae" subSpecies="fridae">Monomorium Medinae Forel, r. Fridae</taxonomicName>
<taxonomicNameLabel id="EE013A4582057CC8F85496C8B515626B" rank="subSpecies">n. st.</taxonomicNameLabel>
</paragraph>
