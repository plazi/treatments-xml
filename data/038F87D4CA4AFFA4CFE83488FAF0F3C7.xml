<taxonomicName id="4C264D41CA4AFFA5C27D3661F7FBF51A" authorityName="Schinz" authorityYear="1821" box="[2072,2173,2665,2694]" class="Mammalia" family="Mustelidae" genus="Lutra" kingdom="Animalia" order="Carnivora" pageId="26" pageNumber="645" phylum="Chordata" rank="species" species="capensis">capensis</taxonomicName>
