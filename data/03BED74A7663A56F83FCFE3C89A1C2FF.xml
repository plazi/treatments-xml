<taxonomicName id="4C171DDF7663A56F83FCFE3C8BD6C5B3" authority="Ai &amp; Wang &amp; Zhang, 2019" authorityName="Ai &amp; Wang &amp; Zhang" authorityYear="2019" box="[151,395,405,432]" class="Insecta" family="Flatidae" genus="Armorseliza" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="375" phylum="Arthropoda" rank="species" species="acontia" status="sp. nov.">
<emphasis id="B963BA4E7663A56F83FCFE3C8BD6C5B3" bold="true" box="[151,395,405,432]" italics="true" pageId="7" pageNumber="375">Armorseliza acontia</emphasis>
</taxonomicName>
