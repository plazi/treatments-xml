: 
<taxonomicName id="4C714D30FF8C8934FC45FC65CD0CFC3D" authority="Brocchi, 1879" authorityName="Brocchi" authorityYear="1879" class="Amphibia" family="Hylidae" genus="Exerodonta" kingdom="Animalia" order="Anura" pageId="99" pageNumber="100" phylum="Chordata" rank="species" species="sumichrasti">
