, 
<bibRefCitation id="EFEF33FBFF7CFF5AFD6EF9803571FCF6" author="Blackwelder, R. E." box="[742,1037,750,774]" journalOrPublisher="United States National Museum Bulletin" pageId="183" pageNumber="182" pagination="189 - 341" part="185" refId="ref193872" refString="Blackwelder, R. E. 1944. Checklist of the coleopterous insects of Mexico, Central America, the West Indies, and South America. Part 2. United States National Museum Bulletin 185 (2): 189 - 341." title="Checklist of the coleopterous insects of Mexico, Central America, the West Indies, and South America. Part 2" type="journal article" year="1944">Blackwelder (1944: 250)</bibRefCitation>
and 
<bibRefCitation id="EFEF33FBFF7CFF5AFBC1F980343FFCF6" author="Endrodi, S." box="[1097,1347,750,774]" journalOrPublisher="Entomologische Abhandlungen Staatliches Museum fur Tierkunde in Dresden" pageId="183" pageNumber="182" pagination="1 - 457" part="33" refId="ref200626" refString="Endrodi, S. 1966 a. Monographie der Dynastinae (Coleoptera, Lamellicornia). 1. Teil. Entomologische Abhandlungen Staatliches Museum fur Tierkunde in Dresden 33: 1 - 457." title="Monographie der Dynastinae (Coleoptera, Lamellicornia). 1. Teil" type="journal article" year="1966">Endrödi (1966a: 363</bibRefCitation>
, 
<bibRefCitation id="EFEF33FBFF7CFF5AFAC7F9803191FCD6" author="Endrodi, S." bookContentInfo="800 p." journalOrPublisher="Junk; The Hague, Netherlands and Akademiai Kiado; Budapest" pageId="183" pageNumber="182" refId="ref201383" refString="Endrodi, S. 1985 a. The Dynastinae of the world. Series Entomologica 28. Dr W. Junk; The Hague, Netherlands and Akademiai Kiado; Budapest. 800 p." title="The Dynastinae of the world. Series Entomologica 28. Dr W" type="book" year="1985">1985a: 156</bibRefCitation>
) record 
<taxonomicName id="4C7E3589FF7CFF5AFEDAF86033FFFCD6" authorityName="Hohne" authorityYear="1923" box="[338,643,782,806]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="183" pageNumber="182" phylum="Arthropoda" rank="species" species="signatoides">
