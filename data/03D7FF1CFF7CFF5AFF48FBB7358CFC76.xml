(named 
<taxonomicName id="4C7E3589FF7CFF5AFD67F8613285FCD6" baseAuthorityName="Hohne" baseAuthorityYear="1923" box="[751,1017,782,806]" class="Insecta" family="Dynastidae" genus="Mimeoma" kingdom="Animalia" order="Coleoptera" pageId="183" pageNumber="182" phylum="Arthropoda" rank="species" species="signatoides">
