<taxonomicName id="C0F34A3D0581C587268716370AF567C0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:191311" authority="Roger, 1863" authorityName="Roger" authorityYear="1863" class="Insecta" family="Formicidae" genus="Macromischa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Macromischa Cressoni AndrÃ©" order="Hymenoptera" pageId="16" pageNumber="296" phylum="Arthropoda" rank="species" species="Cressoni">Macromischa Cressoni</taxonomicName>
