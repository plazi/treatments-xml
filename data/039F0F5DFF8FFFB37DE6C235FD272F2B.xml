<number id="3DFEAC6CFF8FFFB37F6AC418FDE02E7E" box="[539,638,1088,1112]" pageId="21" pageNumber="22" value="110.5" valueMax="112.0" valueMin="109.0">109–112</number>
, T = 
<number id="3DFEAC6CFF8FFFB37FC8C418FC832E7E" box="[697,797,1088,1112]" pageId="21" pageNumber="22" value="117.5" valueMax="120.0" valueMin="115.0">115–120</number>
, HF = 
<number id="3DFEAC6CFF8FFFB37E18C418FC2C2E7E" box="[873,946,1088,1112]" pageId="21" pageNumber="22" value="28.5" valueMax="29.0" valueMin="28.0">28–29</number>
