<taxonomicName id="399719F0800701E9E14D5F8E754AB03E" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus walteri" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="walteri">Euplectrus walteri</taxonomicName>
