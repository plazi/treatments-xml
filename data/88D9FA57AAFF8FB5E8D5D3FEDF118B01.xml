<taxonomicName id="C12636DB81968E458BFFF8A64584E75A" authority="Nees, 1834" authorityName="Nees" authorityYear="1834" class="Insecta" family="Eulophidae" genus="Elachistus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Elachistus carinatus" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="carinatus">Elachistus carinatus</taxonomicName>
