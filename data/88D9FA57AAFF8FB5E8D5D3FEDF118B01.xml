<taxonomicName id="98CCD883CC2F1F78053CE6B4F9DFCDC1" authority="Cameron, 1904" authorityName="Cameron" authorityYear="1904" class="Insecta" family="Eulophidae" genus="Rekabia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rekabia testaceipes" order="Hymenoptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="species" species="testaceipes">Rekabia testaceipes</taxonomicName>
