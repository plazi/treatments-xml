14. 
<taxonomicName id="E6A74D49A12CFFC644090CA1702DAAED" box="[867,1119,730,765]" class="Insecta" family="Haglidae" genus="Turkestania" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="419" phylum="Arthropoda" rank="species" species="pulex">
