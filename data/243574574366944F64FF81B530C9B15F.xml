<taxonomicName id="784C43B9FE35C9EFE5EBB8195B6424CE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142026" class="Insecta" family="Formicidae" genus="Prenolepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Prenolepis sikorae Forel" order="Hymenoptera" pageNumber="238" phylum="Arthropoda" rank="species" species="sikorae">PRENOLEPIS SIKORAE</taxonomicName>
, 
<taxonomicNameLabel id="A9521B354E9143BF8B0F57058092AE46" rank="species">n. sp.</taxonomicNameLabel>
