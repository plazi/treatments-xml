<taxonomicName id="0ADD45F912C5995FDD131930007E4A77" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141974" class="Insecta" family="Formicidae" genus="Prenolepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Prenolepis braueri r. humbloti Forel" order="Hymenoptera" pageNumber="240" phylum="Arthropoda" rank="species" species="humbloti">P. Humbloti</taxonomicName>
