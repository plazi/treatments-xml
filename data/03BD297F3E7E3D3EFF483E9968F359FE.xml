<taxonomicName id="4C14E3EA3E7F3D3DFEF83D46682F5974" authority="Pereira &amp; Labarque &amp; Polotow, 2020" authorityName="Pereira &amp; Labarque &amp; Polotow" authorityYear="2020" box="[295,527,1542,1566]" class="Arachnida" family="Ctenidae" genus="Bulboctenus" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="388" phylum="Arthropoda" rank="species" species="itunaitata" status="sp. nov.">
<emphasis id="B960447B3E7F3D3DFEF83D46682F5974" box="[295,527,1542,1566]" italics="true" pageId="13" pageNumber="388">Bulboctenus itunaitata</emphasis>
