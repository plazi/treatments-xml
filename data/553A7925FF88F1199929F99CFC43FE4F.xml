<caption id="89EC98BBFF8FF1189AD1FB04FD77FB09" ID-DOI="http://doi.org/10.5281/zenodo.5447219" ID-Zenodo-Dep="5447219" httpUri="https://zenodo.org/record/5447219/files/figure.png" pageId="20" pageNumber="247" startId="20.[144,223,1220,1239]" targetBox="[368,1199,197,1189]" targetPageId="20">
