<taxonomicName id="1A93B3B0FF8FF1189B50FB04FE6DFB2A" authority="Corbera, 2008" authorityName="Corbera" authorityYear="2008" box="[273,508,1220,1239]" class="Malacostraca" family="Bodotriidae" genus="Apocuma" kingdom="Animalia" order="Cumacea" pageId="20" pageNumber="247" phylum="Arthropoda" rank="species" species="pacificum" status="sp. nov.">
<emphasis id="EFE71421FF8FF1189B50FB04FE6DFB2A" bold="true" box="[273,508,1220,1239]" italics="true" pageId="20" pageNumber="247">Apocuma pacificum</emphasis>
