<subSubSection id="C3150BC6FFA5FFC4E34F13E3DE5FFC2E" pageId="21" pageNumber="22" type="reference_group">
<paragraph id="8BB0584DFFA5FFC4E34F13E3D882FCFE" blockId="21.[264,1323,604,870]" pageId="21" pageNumber="22">
<taxonomicName id="4C0F23CEFFA5FFC4E34F13E3DE37FC8D" box="[312,528,764,789]" class="Insecta" family="Anthomyiidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="autumnalis">
<emphasis id="B97B845FFFA5FFC4E34F13E3DE37FC8D" box="[312,528,764,789]" italics="true" pageId="21" pageNumber="22">Musca autumnalis</emphasis>
</taxonomicName>
