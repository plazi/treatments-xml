<taxonomicName id="4C0F23CEFFA6FFC7E3DB10A3DED3FE4D" box="[428,756,444,469]" class="Arachnida" family="Siteroptidae" genus="Pediculaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="trombidiphilus">
