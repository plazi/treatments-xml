<quantity id="4C9E9B2709780442FEA97CAFFE74F800" box="[313,406,2001,2027]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="22" pageNumber="23" unit="mm" value="1.1">1.1 mm</quantity>
