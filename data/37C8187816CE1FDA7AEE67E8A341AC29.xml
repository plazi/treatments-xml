<taxonomicName id="A03845E5B751CFC7C36BA3254C976BC3" authority="Burm." authorityName="Burm." class="Magnoliopsida" family="Polygalaceae" genus="Polygala" kingdom="Plantae" order="Fabales" phylum="Tracheophyta" rank="species" species="stipulacea">
