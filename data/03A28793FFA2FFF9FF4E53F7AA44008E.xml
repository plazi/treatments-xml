), 
<taxonomicName id="4C0B4D06FFA2FFF9FB545127ADAE0172" box="[1165,1246,583,606]" class="Liliopsida" family="Ruppiaceae" genus="Ruppia" kingdom="Plantae" order="Alismatales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="genus">
