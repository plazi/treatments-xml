<quantity id="4CDC9B75FFC0FFEDFDC5ED7AFDBA8C53" box="[547,635,1297,1321]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="13.0" metricValueMin="5.0" pageId="63" pageNumber="64" unit="m" value="9.0" valueMax="13.0" valueMin="5.0">5–13 m</quantity>
