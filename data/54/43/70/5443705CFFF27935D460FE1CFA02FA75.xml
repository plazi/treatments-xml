), 
<taxonomicName authority="(Hong &amp; James, 2009)" baseAuthorityName="Hong &amp; James" baseAuthorityYear="2009" box="[989,1341,957,981]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="10" pageNumber="367" phylum="Annelida" rank="species" species="doriae">
<emphasis box="[989,1089,957,981]" italics="true" pageId="10" pageNumber="367">P. doriae</emphasis>
(
<bibRefCitation author="Hong Y &amp; James SW" box="[1103,1334,957,981]" pageId="10" pageNumber="367" pagination="33 - 45" refId="ref15042" refString="Hong Y &amp; James SW (2009) New earthworms of the Pheretima urceolata species group (Oligochaeta: Megascolecidae) from southern Luzon, Philippines. Zootaxa, 2059: 33 - 45." type="journal article" year="2009">Hong &amp; James, 2009</bibRefCitation>
)
</taxonomicName>
(
<quantity box="[1357,1474,957,981]" metricMagnitude="-2" metricUnit="m" metricValue="3.95" metricValueMax="4.5" metricValueMin="3.4" pageId="10" pageNumber="367" unit="mm" value="39.5" valueMax="45.0" valueMin="34.0">34–45 mm</quantity>
× 
<quantity box="[861,975,989,1013]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" metricValueMax="2.4" metricValueMin="2.0" pageId="10" pageNumber="367" unit="mm" value="2.2" valueMax="2.4" valueMin="2.0">2–2.4 mm</quantity>
), 
<taxonomicName authority="(Hong &amp; James, 2009)" baseAuthorityName="Hong &amp; James" baseAuthorityYear="2009" box="[996,1389,989,1013]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="10" pageNumber="367" phylum="Annelida" rank="species" species="batoensis">
<emphasis box="[996,1129,989,1013]" italics="true" pageId="10" pageNumber="367">P. batoensis</emphasis>
(
<bibRefCitation author="Hong Y &amp; James SW" box="[1145,1382,989,1013]" pageId="10" pageNumber="367" pagination="33 - 45" refId="ref15042" refString="Hong Y &amp; James SW (2009) New earthworms of the Pheretima urceolata species group (Oligochaeta: Megascolecidae) from southern Luzon, Philippines. Zootaxa, 2059: 33 - 45." type="journal article" year="2009">Hong &amp; James, 2009</bibRefCitation>
)
</taxonomicName>
(
