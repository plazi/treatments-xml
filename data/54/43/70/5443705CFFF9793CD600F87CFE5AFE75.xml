<subSubSection pageId="3" pageNumber="360" type="discussion">
<paragraph blockId="3.[114,753,349,469]" pageId="3" pageNumber="360">
<emphasis bold="true" box="[114,227,349,373]" pageId="3" pageNumber="360">Remarks.</emphasis>
<taxonomicName baseAuthorityName="Aspe &amp; James" baseAuthorityYear="2016" box="[237,456,349,373]" class="Clitellata" family="Megascolecidae" genus="Pheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="3" pageNumber="360" phylum="Annelida" rank="species" species="apoensis">
