<taxonomicName authority="Baker (1884: 90)" authorityName="Baker" authorityPageNumber="90" authorityYear="1884" box="[442,706,553,575]" class="Lycopodiopsida" family="Selaginellaceae" genus="Selaginella" kingdom="Plantae" order="Selaginellales" pageId="7" pageNumber="242" phylum="Tracheophyta" rank="species" species="armata">
