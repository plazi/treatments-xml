<taxonomicName class="Insecta" family="Braconidae" genus="Meteorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Meteorus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
