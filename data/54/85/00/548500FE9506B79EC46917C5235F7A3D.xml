<date value="1999-07">July 1999</date>
, January-February 2000, and 
<date value="2008-04">April 2008</date>
at 
<quantity metricMagnitude="2" metricUnit="m" metricValue="2.8" unit="m" value="280.0">280 m</quantity>
, 
<quantity metricMagnitude="2" metricUnit="m" metricValue="2.9" unit="m" value="290.0">290 m</quantity>
, and 
