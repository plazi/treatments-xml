<subSubSection pageId="5" pageNumber="4" type="etymology">
<paragraph blockId="5.[192,1441,706,762]" pageId="5" pageNumber="4">
<emphasis bold="true" box="[192,348,706,730]" pageId="5" pageNumber="4">Etymology.</emphasis>
