</tr>
<tr pageId="0" pageNumber="13">
<td colspan="1" pageId="0" pageNumber="13" rowspan="1">
<emphasis italics="true" pageId="0" pageNumber="13">
<taxonomicName authorityName="Bole" authorityYear="1961" class="Gastropoda" family="Spelaeodiscidae" genus="Spelaeodiscus" kingdom="Animalia" lsidName="Spelaeodiscus unidentatus" order="Stylommatophora" pageId="0" pageNumber="13" phylum="Mollusca" rank="species" species="unidentatus">Spelaeodiscus unidentatus</taxonomicName>
<taxonomicName authorityName="Bole" authorityYear="1961" class="Gastropoda" family="Spelaeodiscidae" genus="Spelaeodiscus" kingdom="Animalia" lsidName="Spelaeodiscus unidentatus" order="Stylommatophora" pageId="0" pageNumber="13" phylum="Mollusca" rank="species" species="unidentatus">Spelaeodiscus unidentatus</taxonomicName>
</emphasis>
</td>
<td colspan="1" pageId="0" pageNumber="13" rowspan="1">74-118</td>
<td colspan="1" pageId="0" pageNumber="13" rowspan="1">2.4-3.2</td>
<td colspan="1" pageId="0" pageNumber="13" rowspan="1">low basal tooth; palatal part of peristome with strong incision</td>
</tr>
<tr pageId="0" pageNumber="13">
<td colspan="1" pageId="0" pageNumber="13" rowspan="1">
<taxonomicName authorityName="Pall-Gergely &amp; Feher" authorityYear="2018" class="Gastropoda" family="Spelaeodiscidae" genus="Spelaeodiscus" kingdom="Animalia" lsidName="Spelaeodiscus unidentatus subsp. acutus" order="Stylommatophora" pageId="0" pageNumber="13" phylum="Mollusca" rank="subSpecies" species="unidentatus" subSpecies="acutus">
