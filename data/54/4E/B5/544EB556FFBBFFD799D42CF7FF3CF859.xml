<taxonomicName authorityName="- Eglitis" authorityYear="1956" box="[341,634,1911,1937]" class="Arachnida" family="Eriophyidae" genus="Eriophyes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="17" pageNumber="228" phylum="Arthropoda" rank="species" species="macrorhynchus">
