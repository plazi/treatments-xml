<emphasis bold="true" box="[151,905,740,767]" pageId="1" pageNumber="593">
<taxonomicName authority="Vitheepradit" authorityName="Vitheepradit" authorityYear="2017" box="[151,555,740,767]" class="Insecta" family="Naucoridae" genus="Namtokocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="593" phylum="Arthropoda" rank="species" species="sitesi" status="NEW SPECIES">
<emphasis bold="true" box="[151,387,740,766]" italics="true" pageId="1" pageNumber="593">Namtokocoris sitesi</emphasis>
Vitheepradit
</taxonomicName>
, 
<taxonomicNameLabel box="[568,768,741,767]" pageId="1" pageNumber="593" rank="species">NEW SPECIES</taxonomicNameLabel>
(
<figureCitation box="[783,866,741,767]" captionStart="FIGURE 1" captionStartId="2.[151,244,1514,1536]" captionTargetBox="[140,1425,194,1464]" captionTargetId="figure@2.[140,1425,194,1464]" captionTargetPageId="2" captionText="FIGURE 1. Diagnostic features of Namtokocoris sitesi n. sp. a) dorsal habitus of holotype, b) genital capsule of male, c) terminal abdominal sterna of female." httpUri="https://zenodo.org/record/893724/files/figure.png" pageId="1" pageNumber="593">Figs. 1</figureCitation>
, 
<figureCitation box="[881,897,741,767]" captionStart="FIGURE 2" captionStartId="3.[151,244,1394,1416]" captionTargetBox="[151,1436,517,1374]" captionTargetId="figure@3.[151,1436,517,1374]" captionTargetPageId="3" captionText="FIGURE 2. Krating Waterfall in Chanthaburi Province, Thailand, the type locality of Namtokocoris sitesi n. sp." httpUri="https://zenodo.org/record/893726/files/figure.png" pageId="1" pageNumber="593">2</figureCitation>
