<paragraph blockId="10.[151,1437,1519,1942]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[199,266,1843,1869]" pageId="10" pageNumber="11">Notes</emphasis>
. 
<taxonomicName authority="Shestakov, 1932" authorityName="Shestakov" authorityYear="1932" box="[281,567,1843,1869]" class="Insecta" family="Braconidae" genus="Myriola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
<emphasis box="[281,371,1843,1869]" italics="true" pageId="10" pageNumber="11">Myriola</emphasis>
Shestakov, 1932
</taxonomicName>
