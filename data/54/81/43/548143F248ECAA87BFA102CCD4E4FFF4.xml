<taxonomicName authority="Oudemans, 1910" authorityName="Oudemans" authorityYear="1910" class="Arachnida" family="Erythraeidae" genus="Charletonia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Charletonia banksi" order="Prostigmata" pageId="5" pageNumber="82" phylum="Arthropoda" rank="species" species="banksi">Charletonia banksi</taxonomicName>
