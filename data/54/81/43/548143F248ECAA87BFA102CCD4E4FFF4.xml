(
<normalizedToken originalValue="Trägårdh">Traegardh</normalizedToken>
, 1908); 
<taxonomicName authority="Oudemans, 1910" authorityName="Oudemans" authorityYear="1910" class="Arachnida" family="Erythraeidae" genus="Charletonia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Charletonia froggatti" order="Prostigmata" pageId="5" pageNumber="82" phylum="Arthropoda" rank="species" species="froggatti">Charletonia froggatti</taxonomicName>
Oudemans, 1910; 
<taxonomicName authority="Oudemans, 1910" authorityName="Oudemans" authorityYear="1910" class="Arachnida" family="Erythraeidae" genus="Charletonia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Charletonia feideri" order="Prostigmata" pageId="5" pageNumber="82" phylum="Arthropoda" rank="species" species="feideri">Charletonia feideri</taxonomicName>
