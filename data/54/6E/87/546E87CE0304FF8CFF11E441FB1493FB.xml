species: 
<taxonomicName authority="Trouessart, 1885" authorityName="Trouessart" authorityYear="1885" box="[294,860,610,632]" class="Arachnida" family="Proctophyllodidae" genus="Proctophyllodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="gracilis" subGenus="Pterodectes">
