species: 
<taxonomicName authority="Mironov, 2008" authorityName="Mironov" authorityYear="2008" box="[294,719,870,893]" class="Arachnida" family="Proctophyllodidae" genus="Metapterodectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="furnarius">
