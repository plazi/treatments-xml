<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181969" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole aurea Wilson" lsidName-HNS="Pheidole aurea Wilson" order="Hymenoptera" pageNumber="582" phylum="Arthropoda" rank="species" species="aurea">aurea</taxonomicName>
