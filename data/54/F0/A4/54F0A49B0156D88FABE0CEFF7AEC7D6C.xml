, and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181978" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole yucatana Wilson" lsidName-HNS="Pheidole yucatana Wilson" order="Hymenoptera" pageNumber="582" phylum="Arthropoda" rank="species" species="yucatana">yucatana</taxonomicName>
