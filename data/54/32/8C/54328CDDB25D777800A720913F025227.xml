<taxonomicName authority="" class="Insecta" family="Carabidae" genus="Mecyclothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecyclothorax (Phacothorax)" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="subGenus" subGenus="Phacothorax">Mecyclothorax (Phacothorax)</taxonomicName>
spp., dorsal view: 
<emphasis bold="true" pageId="17" pageNumber="18">A</emphasis>
, 
<taxonomicName genus="M." lsidName="M. laterobustus" pageId="17" pageNumber="18" rank="species" species="laterobustus">
<emphasis italics="true" pageId="17" pageNumber="18">M. laterobustus</emphasis>
</taxonomicName>
; 
<emphasis bold="true" pageId="17" pageNumber="18">B</emphasis>
, 
<taxonomicName genus="M." lsidName="M. laterosinuatus" pageId="17" pageNumber="18" rank="species" species="laterosinuatus">
<emphasis italics="true" pageId="17" pageNumber="18">M. laterosinuatus</emphasis>
</taxonomicName>
; 
<emphasis bold="true" pageId="17" pageNumber="18">C</emphasis>
, 
<taxonomicName genus="M." lsidName="M. laterorectus" pageId="17" pageNumber="18" rank="species" species="laterorectus">
<emphasis italics="true" pageId="17" pageNumber="18">M. laterorectus</emphasis>
</taxonomicName>
; 
<emphasis bold="true" pageId="17" pageNumber="18">D</emphasis>
, 
<taxonomicName genus="M." lsidName="M. fleutiauxi" pageId="17" pageNumber="18" rank="species" species="fleutiauxi">
<emphasis italics="true" pageId="17" pageNumber="18">M. fleutiauxi</emphasis>
</taxonomicName>
; 
<emphasis bold="true" pageId="17" pageNumber="18">E</emphasis>
, 
<taxonomicName genus="M." lsidName="M. jeanneli" pageId="17" pageNumber="18" rank="species" species="jeanneli">
