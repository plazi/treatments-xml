<quantity box="[350,488,1694,1718]" metricMagnitude="-3" metricUnit="m" metricValue="2.45" metricValueMax="2.8" metricValueMin="2.1" pageId="42" pageNumber="43" unit="mm" value="2.45" valueMax="2.8" valueMin="2.1">2.1–2.8 mm</quantity>
