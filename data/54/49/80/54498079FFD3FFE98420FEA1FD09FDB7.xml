<taxonomicName authority="(G. Forst) Tindale" authorityName="Tindale" baseAuthorityName="G. Forst" class="Polypodiopsida" family="Dryopteridaceae" genus="Arachniodes" kingdom="Plantae" order="Polypodiales" pageId="3" pageNumber="142" phylum="Tracheophyta" rank="species" species="aristata">
<emphasis italics="true" pageId="3" pageNumber="142">Arachniodes aristata</emphasis>
