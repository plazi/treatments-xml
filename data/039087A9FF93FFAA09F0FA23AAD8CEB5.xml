<taxonomicName id="4C394D3CFF93FFA90B43F956AD35CCC0" box="[548,624,1681,1705]" class="Reptilia" kingdom="Animalia" order="Squamata" pageId="60" pageNumber="75" phylum="Chordata" rank="subOrder" subOrder="Sauria">Sauria</taxonomicName>
: 
<taxonomicName id="4C394D3CFF93FFA90B1DF956ADA9CCC0" authorityName="Gray" authorityYear="1825" box="[634,748,1681,1705]" class="Reptilia" family="Anguidae" kingdom="Animalia" order="Squamata" pageId="60" pageNumber="61" phylum="Chordata" rank="family">Anguidae</taxonomicName>
