<paragraph id="8B8636BFFF93FFA909F0F955ADBDCCC0" blockId="60.[151,1436,1681,2030]" box="[151,760,1681,1705]" pageId="60" pageNumber="61">
Host: 
<taxonomicName id="4C394D3CFF93FFA909BCF956AEE9CCC0" baseAuthorityName="Pallas" baseAuthorityYear="1775" box="[219,428,1681,1705]" class="Reptilia" family="Anguidae" genus="Pseudopus" kingdom="Animalia" order="Squamata" pageId="60" pageNumber="61" phylum="Chordata" rank="species" species="apodus">
<emphasis id="B94DEAADFF93FFA909BCF956AEE9CCC0" box="[219,428,1681,1705]" italics="true" pageId="60" pageNumber="61">Pseudopus apodus</emphasis>
</taxonomicName>
(
<taxonomicName id="4C394D3CFF93FFA908DDF956AD5CCCC0" box="[442,537,1681,1705]" class="Reptilia" kingdom="Animalia" pageId="60" pageNumber="61" phylum="Chordata" rank="class">Reptilia</taxonomicName>
