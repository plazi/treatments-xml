<paragraph id="8C6773334059FFBCFEB2ABD0FBE2969E" blockId="54.[264,1051,1441,1464]" box="[264,1051,1441,1464]" pageId="54" pageNumber="55">
<taxonomicName id="4BD808B04059FFBCFEB2ABD0FBE2969E" authority="Ueckermann &amp; Loots, 1988: 107" authorityName="Ueckermann &amp; Loots" authorityPageNumber="107" authorityYear="1988" box="[264,1051,1441,1464]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="spermahyphus" subGenus="Amblyseius">
