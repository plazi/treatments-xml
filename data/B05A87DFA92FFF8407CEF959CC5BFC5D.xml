<taxonomicName id="FFF34D4AA92FFF8306CEF877CA02F807" authority="Michx." authorityName="Michx." box="[407,635,2019,2045]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="212" phylum="Arthropoda" rank="species" species="alata">
<emphasis id="0A87EADBA92FFF8306CEF877CA58F807" box="[407,545,2019,2045]" italics="true" pageId="11" pageNumber="212">Ulmus alata</emphasis>
Michx.
</taxonomicName>
, 
<taxonomicName id="FFF34D4AA92FFF8305DFF870CB59F807" box="[646,800,2020,2045]" class="Magnoliopsida" family="Fabaceae" genus="Apios" kingdom="Plantae" order="Fabales" pageId="11" pageNumber="212" phylum="Tracheophyta" rank="species" species="americana">
<emphasis id="0A87EADBA92FFF8305DFF870CB59F807" box="[646,800,2020,2045]" italics="true" pageId="11" pageNumber="212">U. americana</emphasis>
</taxonomicName>
L., 
<taxonomicName id="FFF34D4AA92FFF830414F870CB83F807" authority="Muhl." authorityName="Muhl." box="[845,1018,2019,2045]" class="Insecta" family="Tingidae" genus="Ulmus" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="212" phylum="Arthropoda" rank="species" species="rubra">
