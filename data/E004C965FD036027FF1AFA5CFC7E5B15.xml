<collectingDate id="0C57A75BFD026027FDA2FC43FD3D5ED5" box="[559,754,980,1004]" pageId="53" pageNumber="54" value="2006-08-25" valueMax="2006-09-02" valueMin="2006-08-25">25.viii–2.ix.2006</collectingDate>
</date>
, 
<collectorName id="C5581DA5FD026027FD70FC43FC585ED5" box="[765,919,980,1004]" pageId="53" pageNumber="54">Bunlu Subsiri</collectorName>
leg., 
<specimenCode id="380BD008FD026027FC5BFC43FBDB5ED5" box="[982,1044,980,1004]" collectionCode="T" name="Tavera, Department of Geology and Geophysics" pageId="53" pageNumber="54">T471</specimenCode>
