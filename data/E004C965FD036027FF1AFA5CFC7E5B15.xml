<collectingCountry id="10BA38E3FD026027FEEAF983FE055B15" box="[359,458,1556,1580]" name="Thailand" pageId="53" pageNumber="54">Thailand</collectingCountry>
(
<collectingRegion id="AA69B691FD026027FE54F983FDDF5B15" box="[473,528,1556,1580]" country="Thailand" name="Loei" pageId="53" pageNumber="54">Loei</collectingRegion>
, 
<collectingRegion id="AA69B691FD026027FD91F983FD515B15" box="[540,670,1556,1580]" country="Thailand" name="Phetchabun" pageId="53" pageNumber="54">Phetchabun</collectingRegion>
and 
<collectingRegion id="AA69B691FD026027FD58F983FC695B15" box="[725,934,1556,1580]" country="Thailand" name="Ubon Ratchathani" pageId="53" pageNumber="54">Ubon Ratchathani</collectingRegion>
).
</paragraph>
</subSubSection>
