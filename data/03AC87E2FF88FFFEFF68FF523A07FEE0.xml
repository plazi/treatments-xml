<location id="8EDA602FFF88FFFEFED9FE693B61FDA0" LSID="urn:lsid:plazi:treatment:03AC87E2FF88FFFEFF68FF523A07FEE0:8EDA602FFF88FFFEFED9FE693B61FDA0" box="[294,518,494,516]" country="Australia" name="Crawford Springs" pageId="51" pageNumber="51" stateProvince="Northern Territory">Crawford Springs</location>
, 
<collectingRegion id="49C1F816FF88FFFEFDDFFE693989FD86" country="Australia" name="Northern Territory" pageId="51" pageNumber="51">Northern Territory</collectingRegion>
