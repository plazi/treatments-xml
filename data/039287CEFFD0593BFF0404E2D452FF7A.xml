<taxonomicName id="4C3B4D5BFFD0593DFF0404E2D5C8FB34" authority="Spungis, 1987" authorityName="Spungis" authorityYear="1987" box="[151,643,1152,1178]" class="Insecta" family="Cecidomyiidae" genus="Tetraneuromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="256" phylum="Arthropoda" rank="species" species="lamellata">
