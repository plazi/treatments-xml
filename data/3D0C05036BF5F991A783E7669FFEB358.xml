<geoCoordinate id="4AB5B59171D60978FA184D0ABFA9B610" direction="north" orientation="latitude" precision="925" value="10.433333">10°26'N</geoCoordinate>
, 
<geoCoordinate id="EB2557E34F02D93545E3913F52E03D8E" direction="west" orientation="longitude" precision="925" value="-84.01667">84°01'W</geoCoordinate>
