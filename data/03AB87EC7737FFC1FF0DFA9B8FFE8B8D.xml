<collectingCountry id="F315766A7737FFC1FC9EFAF3882D88AD" box="[772,831,1385,1409]" name="United States of America" pageId="9" pageNumber="50">USA</collectingCountry>
. In 
<collectingCountry id="F315766A7737FFC1FCF4FAF388A088AD" box="[878,946,1385,1409]" name="Brazil" pageId="9" pageNumber="50">Brazil</collectingCountry>
it occurs in the states of 
<collectingRegion id="49C6F8187737FFC1FB40FAF38E0188AD" box="[1242,1299,1385,1409]" country="Brazil" name="Acre" pageId="9" pageNumber="50">Acre</collectingRegion>
, Amazonas, 
<collectingRegion id="49C6F8187737FFC1FF0DFA178BF98889" box="[151,235,1421,1445]" country="Brazil" name="Amapa" pageId="9" pageNumber="50">Amapá</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FF62FA178A2E8889" box="[248,316,1421,1445]" country="Brazil" name="Bahia" pageId="9" pageNumber="50">Bahia</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FED3FA178A9F8889" box="[329,397,1421,1445]" country="Brazil" name="Ceara" pageId="9" pageNumber="50">Ceará</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FE01FA17892D8889" box="[411,575,1421,1445]" country="Brazil" name="Espirito Santo" pageId="9" pageNumber="50">Espírito Santo</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FDD6FA1789828889" box="[588,656,1421,1445]" country="Brazil" name="Goias" pageId="9" pageNumber="50">Goiás</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FD04FA1788008889" box="[670,786,1421,1445]" country="Brazil" name="Maranhao" pageId="9" pageNumber="50">Maranhão</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FCBAFA1488A48889" box="[800,950,1421,1445]" country="Brazil" name="Mato Grosso" pageId="9" pageNumber="50">Mato Grosso</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FC59FA148FBF8889" box="[963,1197,1421,1445]" country="Brazil" name="Mato Grosso do Sul" pageId="9" pageNumber="50">Mato Grosso do Sul</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FB21FA178E448889" box="[1211,1366,1421,1445]" country="Brazil" name="Minas Gerais" pageId="9" pageNumber="50">Minas Gerais</collectingRegion>
, Pará, 
<collectingRegion id="49C6F8187737FFC1FF0DFA2B8A3488E5" box="[151,294,1457,1481]" country="Brazil" name="Pernambuco" pageId="9" pageNumber="50">Pernambuco</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FEAAFA2B8A6D88E5" box="[304,383,1457,1481]" country="Brazil" name="Parana" pageId="9" pageNumber="50">Paraná</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FE10FA2B893C88E5" box="[394,558,1457,1481]" country="Brazil" name="Rio de Janeiro" pageId="9" pageNumber="50">Rio de Janeiro</collectingRegion>
, 
<collectingRegion id="49C6F8187737FFC1FDA3FA2B89BB88E5" box="[569,681,1457,1481]" country="Brazil" name="Rondonia" pageId="9" pageNumber="50">Rondônia</collectingRegion>
