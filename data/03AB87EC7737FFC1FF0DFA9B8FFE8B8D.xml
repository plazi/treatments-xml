<subSubSection id="C31865717737FFC1FF0DFADF8FFE8B8D" pageId="9" pageNumber="50" type="distribution">
<paragraph id="8BBD36FA7737FFC1FF0DFADF885388C1" blockId="9.[151,1438,1349,1698]" pageId="9" pageNumber="50">
<emphasis id="B976EAE87737FFC1FF0DFADF8A228872" bold="true" box="[151,304,1349,1374]" inLineHeading="true" pageId="9" pageNumber="50" reason="1">Distribution.</emphasis>
From Mexico to 
<collectingCountry id="F315766A7737FFC1FD93FADC892D8871" box="[521,575,1350,1373]" name="Peru" pageId="9" pageNumber="50">Peru</collectingCountry>
, northern 
<collectingCountry id="F315766A7737FFC1FD20FADF883F8871" box="[698,813,1349,1373]" name="Argentina" pageId="9" pageNumber="50">Argentina</collectingCountry>
, 
<collectingCountry id="F315766A7737FFC1FCA7FADC88B48872" box="[829,934,1350,1374]" name="Paraguay" pageId="9" pageNumber="50">Paraguay</collectingCountry>
, 
<collectingCountry id="F315766A7737FFC1FC2DFADC8F088872" box="[951,1050,1350,1374]" name="Uruguay" pageId="9" pageNumber="50">Uruguay</collectingCountry>
, 
<collectingCountry id="F315766A7737FFC1FBB0FADF8F638871" box="[1066,1137,1349,1373]" name="Brazil" pageId="9" pageNumber="50">Brazil</collectingCountry>
, 
<collectingCountry id="F315766A7737FFC1FB18FADF8FEA8871" box="[1154,1272,1349,1373]" name="Venezuela" pageId="9" pageNumber="50">Venezuela</collectingCountry>
, 
<collectingCountry id="F315766A7737FFC1FA92FADF8BFF88AD" name="Trinidad and Tobago" pageId="9" pageNumber="50">Trinidad and Tobago</collectingCountry>
, Guianas, 
<collectingCountry id="F315766A7737FFC1FEFDFAF38AD688AD" box="[359,452,1385,1409]" name="Curaçao" pageId="9" pageNumber="50">Curaçao</collectingCountry>
