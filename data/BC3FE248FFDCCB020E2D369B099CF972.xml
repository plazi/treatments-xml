<taxonomicName id="F39628DDFFDCCB020E2D376E0F93FABE" authority="Gauld, 1988: 276" authorityName="Gauld" authorityPageNumber="276" authorityYear="1988" box="[264,671,1320,1344]" class="Insecta" family="Ichneumonidae" genus="Enicospilus" kingdom="Animalia" order="Hymenoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="liesneri">
