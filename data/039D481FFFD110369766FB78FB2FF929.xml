<quantity id="4CCC54ECFFDF10359745FD93FD11FDE5" box="[535,679,540,566]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" metricValueMax="2.2" metricValueMin="2.1" pageId="25" pageNumber="254" unit="mm" value="2.15" valueMax="2.2" valueMin="2.1">2.1–2.2 mm</quantity>
