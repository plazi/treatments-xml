and west to 
<collectingRegion id="3B3B93E41A3AF46238444F921D44FF2D" isEnumeration="true" pageId="42" pageNumber="43">Texas, Missouri</collectingRegion>
, and 
<collectingRegion id="3B3B93E41A3AF4623C2A4FB71DD1FF2D" box="[319,407,189,215]" country="United States of America" name="Kansas" pageId="42" pageNumber="43">Kansas</collectingRegion>
