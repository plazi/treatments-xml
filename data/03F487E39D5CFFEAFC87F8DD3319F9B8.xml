<collectingCountry id="F34A76659D5DFFEAFE64FE963301FE41" box="[462,570,376,400]" name="Colombia" pageId="33" pageNumber="684">Colombia</collectingCountry>
, 
<collectingCountry id="F34A76659D5DFFEAFDEEFE963386FE41" box="[580,701,376,400]" name="Costa Rica" pageId="33" pageNumber="684">Costa Rica</collectingCountry>
, 
