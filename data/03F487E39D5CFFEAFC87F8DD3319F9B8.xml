<geoCoordinate id="EE6950329D5CFFEBFBC6F86B35FCF84F" box="[1132,1223,1925,1950]" degrees="07" direction="north" minutes="56" orientation="latitude" pageId="32" pageNumber="683" precision="925" value="7.9333334">07°56′N</geoCoordinate>
, 
<geoCoordinate id="EE6950329D5CFFEBFB7BF86B3406F84F" box="[1233,1341,1925,1950]" degrees="077" direction="west" minutes="42" orientation="longitude" pageId="32" pageNumber="683" precision="925" value="-77.7">077°42′W</geoCoordinate>
; 
<quantity id="4CA59B109D5CFFEBFAEDF8693245F86E" metricMagnitude="3" metricUnit="m" metricValue="1.05" metricValueMax="1.1" metricValueMin="1.0" pageId="32" pageNumber="683" unit="m" value="1050.0" valueMax="1100.0" valueMin="1000.0">1000– 1100 m</quantity>
