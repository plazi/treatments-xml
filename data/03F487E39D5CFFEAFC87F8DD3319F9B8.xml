<taxonomicName id="4C5D4D769D5DFFEAFF02FDF63026FD83" authority="Croat &amp; Grayum" authorityName="Croat &amp; Grayum" class="Liliopsida" family="Araceae" genus="Philodendron" kingdom="Plantae" order="Alismatales" pageId="33" pageNumber="684" phylum="Tracheophyta" rank="variety" species="wilburii" variety="longipedunculatum">
