<heading id="D0B281E1CF5A8313C1922C868C89FB3F" bold="true" box="[151,713,1231,1258]" fontSize="11" level="1" pageId="9" pageNumber="482" reason="1">
<taxonomicName id="4C454D0ECF5A8313C1922C868C89FB3F" authority="Ameghino, 1887 b" authorityName="Ameghino" authorityYear="1887" box="[151,713,1231,1258]" class="Mammalia" family="Interatheriidae" genus="Protypotherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Notoungulata" pageId="9" pageNumber="482" phylum="Chordata" rank="species" species="attenuatum">
