</emphasis>
<taxonomicName id="4C734D10FFC7FFCEFE930E6F87D3FEB2" authority="(Diesing, 1839) Travassos, 1934" authorityName="(Diesing, 1839) Travassos" authorityYear="1934" box="[315,926,258,283]" class="Trematoda" family="Paramphistomidae" genus="Nematophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plagiorchiida" pageId="6" pageNumber="55" phylum="Platyhelminthes" rank="species" species="grandis">
