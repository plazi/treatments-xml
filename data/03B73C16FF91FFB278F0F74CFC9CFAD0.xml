<taxonomicName id="4C1EF683FF90FFB27AAEF658FCB2FBE5" authorityName="Tatarnic &amp; Cassis" authorityYear="2008" box="[746,811,1088,1108]" class="Insecta" family="Miridae" genus="Coridromius" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="lestoni">
