<taxonomicName id="2DDC4D7BFFEC2A15DE93FF438A62B0C1" box="[226,341,188,213]" class="Insecta" family="Formicidae" genus="Heteroponera" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="species" species="wilsoni">
