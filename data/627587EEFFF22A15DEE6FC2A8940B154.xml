<taxonomicName id="2DDC4D7BFFEC2A15DD74FF67889BB0A4" box="[773,940,151,176]" class="Insecta" family="Formicidae" genus="Heteroponera" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="167" phylum="Arthropoda" rank="species" species="rhodopygea">
