<taxonomicName id="48628A2C0729C0C53030F29A7F14B258" authorityName="Cameron" authorityYear="1904" class="Insecta" family="Eumenidae" genus="Zethus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zethus trimaculatus" order="Hymenoptera" pageId="0" pageNumber="253" phylum="Arthropoda" rank="species" species="trimaculatus">Zethus trimaculatus</taxonomicName>
