<taxonomicName id="71D5052F363B57DC46BF48B895159ACC" authorityName="Giordani Soika" authorityYear="1941" class="Insecta" family="Eumenidae" genus="Zethus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zethus luzonensis" order="Hymenoptera" pageId="0" pageNumber="253" phylum="Arthropoda" rank="species" species="luzonensis">
