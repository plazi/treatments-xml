<taxonomicName id="C2E34D2AFFB7FF82FF79FD909ED4FDD9" authority="Kossmann, 1877" authorityName="Kossmann" authorityYear="1877" box="[247,645,582,609]" class="Hexanauplia" family="Lichomolgidae" kingdom="Animalia" order="Cyclopoida" pageId="12" pageNumber="13" phylum="Arthropoda" rank="family">Lichomolgidae Kossmann, 1877</taxonomicName>
