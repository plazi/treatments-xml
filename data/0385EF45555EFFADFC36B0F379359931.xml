<taxonomicName id="4C2C25D0555DFFABFDDFB0EA796E9A0F" authority="Tumpeesuwan &amp; Tumpeesuwan, 2015" authorityName="Tumpeesuwan &amp; Tumpeesuwan" authorityYear="2015" box="[624,860,1270,1292]" class="Magnoliopsida" family="Fabaceae" genus="Pearsonia" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="289" phylum="Tracheophyta" rank="species" species="lamphunensis" status="new species">
<emphasis id="B9588241555DFFABFDDFB0EA796E9A0F" box="[624,860,1270,1292]" italics="true" pageId="2" pageNumber="289">Pearsonia lamphunensis</emphasis>
