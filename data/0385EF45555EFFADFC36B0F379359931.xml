<taxonomicName id="4C2C25D0555DFFABFF6EB10E7B169A2C" authorityName="Kobelt" authorityYear="1902" box="[193,292,1299,1320]" class="Magnoliopsida" family="Fabaceae" genus="Pearsonia" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="289" phylum="Tracheophyta" rank="genus">
