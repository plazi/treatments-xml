<paragraph id="81769D7852CECB409FF61D4C0207FCB6" pageId="12" pageNumber="129">
<taxonomicName id="C90D812937E68045D36A62D77B69930C" class="Actinopterygii" family="Mormyridae" genus="Cryptomyrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cryptomyrus ogoouensis" order="Osteoglossiformes" pageId="12" pageNumber="129" phylum="Chordata" rank="species" species="ogoouensis">Cryptomyrus ogoouensis</taxonomicName>
