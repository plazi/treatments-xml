<taxonomicName id="7FFC2B8AFFB7FFB67017C821FE536F06" box="[293,493,1093,1115]" class="Ascidiacea" family="Pyuridae" genus="Herdmania" kingdom="Animalia" order="Pleurogona" pageId="39" pageNumber="40" phylum="Chordata" rank="species" species="coutieri">
