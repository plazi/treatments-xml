<taxonomicName id="4C504D42FF8F0A02BE91F86A8B65A09B" box="[991,1075,1933,1959]" class="Insecta" family="Haglotettigoniidae" genus="Gryllus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="230" phylum="Arthropoda" rank="genus">
