SEM, 
<taxonomicName id="4C764D08186EFFEFFE62F890BBF6F8B5" box="[468,685,1801,1824]" class="Arachnida" family="Salticidae" genus="Marengo" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="33" phylum="Arthropoda" rank="species" species="rattotensis" status="sp. n.">
<emphasis id="B902EA99186EFFEFFE62F890BBF6F8B5" box="[468,685,1801,1824]" italics="true" pageId="8" pageNumber="33">
<taxonomicName id="4C764D08186EFFEFFE62F890BB6EF8B5" box="[468,565,1801,1824]" class="Arachnida" family="Salticidae" genus="Marengo" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="33" phylum="Arthropoda" rank="genus">Marengo</taxonomicName>
<taxonomicName id="4C764D08186EFFEFFD8AF890BBF6F8B5" box="[572,685,1801,1824]" pageId="8" pageNumber="33" rank="species" species="rattotensis" status="sp. n.">rattotensis</taxonomicName>
</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23157E2186EFFEFFD05F891BBB5F8B5" box="[691,750,1800,1824]" pageId="8" pageNumber="33" rank="species">
