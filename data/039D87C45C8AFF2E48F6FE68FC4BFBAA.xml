<emphasis id="B940EAC05C8AFF2E48F6FE68FD22FE2F" bold="true" box="[151,748,439,466]" pageId="149" pageNumber="150">
<taxonomicName id="4C344D515C8AFF2E48F6FE68FE34FE2F" authority="Barbosa &amp; Azevedo" authorityName="Barbosa &amp; Azevedo" box="[151,506,439,466]" class="Insecta" family="Bethylidae" genus="Anisepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="149" pageNumber="150" phylum="Arthropoda" rank="species" species="guianae">
<emphasis id="B940EAC05C8AFF2E48F6FE68FF37FE2C" bold="true" box="[151,249,439,465]" italics="true" pageId="149" pageNumber="150">guianae</emphasis>
