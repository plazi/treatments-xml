<geoCoordinate id="EE235007FFF2FFD69A37FED5F8EF3144" box="[586,686,295,321]" degrees="44" direction="north" minutes="20" orientation="latitude" pageId="99" pageNumber="100" precision="925" value="44.333332">44°20’N</geoCoordinate>
