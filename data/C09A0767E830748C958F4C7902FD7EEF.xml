<taxonomicName id="088113F4EA63A909406CE0479AED2C94" LSID="urn:lsid:zoobank.org:act:7781FCA5-2F6F-49CF-B629-1CAB9799A914" authority="Winterton, 2007" authorityName="Winterton" authorityYear="2007" class="Insecta" family="Therevidae" genus="Vomerina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Vomerina" order="Diptera" pageId="1" pageNumber="66" phylum="Arthropoda" rank="genus">Vomerina Winterton, 2007</taxonomicName>
