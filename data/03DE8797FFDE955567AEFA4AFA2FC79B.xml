<taxonomicName id="4C774D02FFDD955566DEFB46FC4EC6C3" authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[487,803,1199,1221]" class="Diplopoda" family="Pachybolidae" genus="Zehntnerobolus" kingdom="Animalia" order="Spirobolida" pageId="2" pageNumber="23" phylum="Arthropoda" rank="genus">
