<emphasis id="B92CEAF2FFECD432FDCFF99BFD63F9C6" bold="true" box="[618,661,1568,1587]" pageId="2" pageNumber="201">8–9)</emphasis>
<taxonomicName id="4C584D63FFECD432FD38F99AFCE0F9C1" authorityName="Howden" authorityYear="1971" box="[669,790,1569,1588]" class="Insecta" family="Hybosoridae" genus="Callosides" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="201" phylum="Arthropoda" rank="species" species="campbelli">
<emphasis id="B92CEAF2FFECD432FD38F99AFCE0F9C1" box="[669,790,1569,1588]" italics="true" pageId="2" pageNumber="201">C. campbelli</emphasis>
