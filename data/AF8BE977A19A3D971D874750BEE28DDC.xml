<paragraph id="EC7BD5F1D50EC002B6E120D65FE75AD2" pageId="32" pageNumber="83">
<taxonomicName id="88F43DEA3F4169910A0725A01D5048E8" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pholcus singalang" order="Araneae" pageId="32" pageNumber="83" phylum="Arthropoda" rank="species" species="singalang">Pholcus singalang</taxonomicName>
