<geoCoordinate id="D5D4500BCD0FFF90FC03FB94FBB8FC4C" box="[984,1085,1039,1059]" degrees="17" direction="north" minutes="17" orientation="latitude" pageId="13" pageNumber="350" precision="15" seconds="58" value="17.299444">17°17’58N</geoCoordinate>
