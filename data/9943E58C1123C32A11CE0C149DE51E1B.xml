<taxonomicName id="B6256C97DFB4EB44082654B84D6491DA" LSID-ZBK="urn:lsid:zoobank.org:act:EC392115-9EE9-4E08-8527-D03FF0B839E2" class="Actinopterygii" family="Rivulidae" genus="Trigonectes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Trigonectes rubromarginatus Costa 1990:145" order="Cyprinodontiformes" pageNumber="7" phylum="Chordata" rank="species" species="rubromarginatus">T. rubromarginatus</taxonomicName>
+ 
