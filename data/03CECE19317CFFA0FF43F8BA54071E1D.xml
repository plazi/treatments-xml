<taxonomicName id="4C67048C3170FFA0FE91FF7A50BF1FED" authority="Yuan &amp; Xue, 2019" authorityName="Yuan &amp; Xue" authorityYear="2019" box="[325,450,154,179]" class="Magnoliopsida" family="Stachyuraceae" genus="Latitudo" kingdom="Plantae" order="Crossosomatales" pageId="8" pageNumber="160" phylum="Tracheophyta" rank="species" species="asiaticis" status="sp. nov.">
<emphasis id="B913A31D3170FFA0FE91FF7A50BF1FED" box="[325,450,154,179]" italics="true" pageId="8" pageNumber="160">L. asiaticis</emphasis>
</taxonomicName>
