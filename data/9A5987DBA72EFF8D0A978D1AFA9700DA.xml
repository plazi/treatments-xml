FEMALE. Length mm 
<number id="A43824EAA72FFF8E085D8BABFD1B05AD" box="[605,715,151,175]" pageId="57" pageNumber="58" value="5.005" valueMax="5.13" valueMin="4.88">4.88–5.13</number>
; head length 
<quantity id="D5089B28A72FFF8E095F8BABFC2F05AD" box="[863,1023,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="1.4249999999999998" metricValueMax="1.45" metricValueMin="1.4" pageId="57" pageNumber="58" unit="mm" value="1.4249999999999998" valueMax="1.45" valueMin="1.4">1.40–1.45 mm</quantity>
; head width 
<quantity id="D5089B28A72FFF8E0E8E8BABFAFE05AD" box="[1166,1326,151,175]" metricMagnitude="-3" metricUnit="m" metricValue="1.79" metricValueMax="1.8" metricValueMin="1.78" pageId="57" pageNumber="58" unit="mm" value="1.79" valueMax="1.8" valueMin="1.78">1.78–1.80 mm</quantity>
; forewing length 
<quantity id="D5089B28A72FFF8E0AE08B80FEAF05D6" box="[224,383,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="3.9699999999999998" metricValueMax="4.13" metricValueMin="3.81" pageId="57" pageNumber="58" unit="mm" value="3.9699999999999998" valueMax="4.13" valueMin="3.81">3.81–4.13 mm</quantity>
