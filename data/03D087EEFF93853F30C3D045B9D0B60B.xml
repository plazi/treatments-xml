<taxonomicName id="4C794D7BFF9D853C33DED0C3BEF2B3D9" box="[906,1011,1967,1989]" class="Insecta" family="Formicidae" genus="Anochetus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="renatae">
