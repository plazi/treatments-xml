<taxonomicName id="09E843524E6D240C8ADC946754CD73E0" class="Insecta" family="Formicidae" genus="Gnamptogenys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gnamptogenys costata" order="Hymenoptera" pageId="13" pageNumber="30" phylum="Arthropoda" rank="species" species="costata">Gnamptogenys costata</taxonomicName>
