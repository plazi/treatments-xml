<taxonomicName id="AEDA59214433F82117087A03EE5DB15F" LSID="http://zoobank.org/8538420B-979B-4158-8D99-7376538DD583" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus hedqvisti" order="Hymenoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="hedqvisti">Heterospilus hedqvisti Marsh</taxonomicName>
