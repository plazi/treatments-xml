<quantity id="FC6F9B691553FF9B55E71F62FD3FF9A4" box="[644,733,1599,1624]" metricMagnitude="-3" metricUnit="m" metricValue="8.9" pageId="9" pageNumber="708" unit="mm" value="8.9">8.9 mm</quantity>
