<taxonomicName id="FC974D0F1551FF9956761AF7FE7EFC3F" authorityName="Li, Mo, Dong, Yang &amp; Muranyi" authorityYear="2018" box="[277,412,938,963]" class="Insecta" family="Nemouridae" genus="Amphinemura" kingdom="Animalia" order="Plecoptera" pageId="11" pageNumber="710" phylum="Arthropoda" rank="species" species="albicauda">
