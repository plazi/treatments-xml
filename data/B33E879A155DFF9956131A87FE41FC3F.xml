<taxonomicName id="FC974D0F1551FF9956AE1A1FFD89FCA7" authorityName="Du &amp; Zhao" authorityYear="2021" box="[461,619,834,859]" class="Insecta" family="Nemouridae" genus="Amphinemura" kingdom="Animalia" order="Plecoptera" pageId="11" pageNumber="710" phylum="Arthropoda" rank="species" species="oxyacantha">
