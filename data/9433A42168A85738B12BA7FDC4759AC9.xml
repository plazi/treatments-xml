<taxonomicName id="DFF83BB6364CD0FDD1A009F56F24B36F" authorityName="Whitehead" authorityYear="1972" class="Hexapoda" family="Carabidae" genus="Schizogenius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="289" pageNumber="290" phylum="Arthropoda" rank="subSpecies" species="seticollis" subSpecies="vandykei">
