<taxonomicName id="328F6DECEB634076FE0BD143FD556D42" authorityName="Hughes" authorityYear="2011" box="[504,648,1878,1902]" class="Malacostraca" family="Maeridae" genus="Mallacoota" kingdom="Animalia" order="Amphipoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="bulowara" status="sp. nov.">
<emphasis id="C7FBCA7DEB634076FE0BD143FD556D42" box="[504,648,1878,1902]" italics="true" pageId="58" pageNumber="59">M. bulowara</emphasis>
