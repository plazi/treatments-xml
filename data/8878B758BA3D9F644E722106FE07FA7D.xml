<taxonomicName id="C7D17DCDBA3D9F634E7C239DFD75FC69" box="[559,652,912,938]" class="Bivalvia" family="Veneridae" genus="Callista" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="75" pageNumber="76" phylum="Mollusca" rank="genus">
