<quantity id="4C9B0A9BF278474D1619FDEDFCE9F8E0" metricMagnitude="-3" metricUnit="m" metricValue="1.8199999999999998" metricValueMax="1.88" metricValueMin="1.76" pageId="4" pageNumber="297" unit="mm" value="1.8199999999999998" valueMax="1.88" valueMin="1.76">1.76–1.88 mm</quantity>
