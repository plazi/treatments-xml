<typeStatus id="35435EEE1D37DC70E40C65E4F0DEFD06" box="[316,425,741,767]" pageId="3" pageNumber="50">Holotype</typeStatus>
worker. CI 
<number id="5C30F26B1D37DC70E70C65E4F32BFD06" box="[572,604,741,767]" pageId="3" pageNumber="50" value="88.0">88</number>
; HL 
<number id="5C30F26B1D37DC70E7AD65E4F3A3FD06" box="[669,724,741,767]" pageId="3" pageNumber="50" value="0.5">0.50</number>
