<paragraph id="070736C9682BFFA6FF38F927FB79F925" blockId="106.[151,1436,1661,1744]" box="[151,1274,1722,1744]" pageId="106" pageNumber="107">
<taxonomicName id="C0B84D4A682BFFA6FF38F927FE4FF925" authority="(Schiner)" baseAuthorityName="Schiner" box="[151,460,1722,1744]" class="Insecta" family="Curtonotidae" genus="Diplocentra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="simplex">
