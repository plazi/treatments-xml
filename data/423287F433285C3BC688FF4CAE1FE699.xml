<emphasis id="F8EFEAF033285C3BC688FF4CAF54E7CD" bold="true" box="[475,787,215,239]" pageId="18" pageNumber="995">
<taxonomicName id="0D9B4D6133285C3BC688FF4CAE8FE7CD" baseAuthorityName="Bernhauer" baseAuthorityYear="1910" box="[475,712,215,239]" class="Insecta" family="Staphylinidae" genus="Micrillus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="995" phylum="Arthropoda" rank="species" species="aegyptiacus">
<emphasis id="F8EFEAF033285C3BC688FF4CAE8FE7CD" bold="true" box="[475,712,215,239]" italics="true" pageId="18" pageNumber="995">Micrillus aegyptiacus</emphasis>
