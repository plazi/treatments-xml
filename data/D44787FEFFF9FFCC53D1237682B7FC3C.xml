<taxonomicName id="9BEE4D6BFFF9FFCF53D123768070FE59" authority="Zherikhin, 1987" box="[151,337,367,393]" class="Insecta" family="Curculionidae" genus="Niphadomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="168" phylum="Arthropoda" rank="genus">
