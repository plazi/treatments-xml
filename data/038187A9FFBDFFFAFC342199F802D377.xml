<typeStatus id="5493881DFFBDFFF9FC83254FF9C7D629" box="[864,921,1562,1586]" pageId="44" pageNumber="1000">Type</typeStatus>
species: 
<taxonomicName id="4C284D3CFFBDFFF9FBE7254CFFC8D629" authority="Perty, 1841" authorityName="Perty" authorityYear="1841" box="[1028,1430,1561,1586]" class="Arachnida" family="Phthiracaridae" genus="Phthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageId="44" pageNumber="1000" phylum="Arthropoda" rank="species" species="contractilis">
