<taxonomicName id="8AA6DF199ECA94DA73CC897EA6F97971" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="species" species="gerrardii">
<emphasis id="313F779FE1E19BEBF54068607EB5EEEF" italics="true" pageId="49" pageNumber="50">Acacia gerrardii</emphasis>
