<collectingCountry id="D2EE7677FFE0FFB3AA7BF8857AD9E03F" box="[361,425,1891,1914]" name="Japan" pageId="6" pageNumber="551">Japan</collectingCountry>
and 
<collectingCountry id="D2EE7677FFE0FFB3AAF6F8847949E03E" box="[484,569,1890,1915]" name="Taiwan" pageId="6" pageNumber="551">Taiwan</collectingCountry>
