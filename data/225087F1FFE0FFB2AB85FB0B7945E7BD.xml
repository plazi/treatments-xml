<quantity id="6D019B02FFE0FFB3A954F88479CCE03F" box="[582,700,1890,1915]" metricMagnitude="2" metricUnit="m" metricValue="2.25" metricValueMax="4.0" metricValueMin="0.5" pageId="6" pageNumber="551" unit="m" value="225.0" valueMax="400.0" valueMin="50.0">50–400 m</quantity>
