<taxonomicName id="27842CB2FFEAFF96FEF26AD9FE35F95F" authorityName="Billard" authorityYear="1925" box="[317,469,1757,1781]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="52" pageNumber="307" phylum="Cnidaria" rank="subSpecies" species="gayi" subSpecies="parva">
<emphasis id="D2F08B23FFEAFF96FEF26AD9FE35F95F" box="[317,469,1757,1781]" italics="true" pageId="52" pageNumber="307">S. gayi parva</emphasis>
</taxonomicName>
