<taxonomicName id="27842CB2FFEAFF96FDA96B79FD05F83F" authorityName="El Beshbeeshy" authorityYear="2011" box="[614,741,1917,1941]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="52" pageNumber="307" phylum="Cnidaria" rank="species" species="jorgensis">
