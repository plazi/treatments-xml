<taxonomicName id="D7F64D08DF0E724FC39CFC70FD7D7303" authority="Walsingham, 1907" authorityName="Walsingham" authorityYear="1907" box="[470,669,1016,1040]" class="Insecta" family="Blastobasidae" genus="Hypatopa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="semela">
