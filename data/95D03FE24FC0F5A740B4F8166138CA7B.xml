<geoCoordinate id="2EA2D1DA8DEC269D3DEF4C9E02D81FD4" degrees="03" direction="south" minutes="52" orientation="latitude" precision="15" seconds="30" value="-3.8750002">03°52'30&quot;S</geoCoordinate>
