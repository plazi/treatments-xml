<taxonomicName id="E8C84D57BE6BE675FD86FC78EB16FC2F" authority="Pallas, 1773" authorityName="Pallas" authorityYear="1773" box="[727,1078,975,1000]" class="Mammalia" family="Muridae" genus="Lagurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="11" pageNumber="234" phylum="Chordata" rank="species" species="lagurus">
