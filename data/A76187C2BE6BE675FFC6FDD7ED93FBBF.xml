<taxonomicName id="E8C84D57BE6BE675FA66FC05EAB4FC02" box="[1335,1428,940,965]" class="Mammalia" family="Muridae" genus="Ellobius" kingdom="Animalia" order="Rodentia" pageId="11" pageNumber="234" phylum="Chordata" rank="genus">
