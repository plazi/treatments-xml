<taxonomicName id="4C6B6DB9323D5C78FA58D777ADCAF9E1" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="37" pageNumber="36" phylum="Tracheophyta" rank="species" species="sisymbriifolium">
