<taxonomicName id="4C454D3A317AFFC6525B059EA9E230CE" authority="Cockerell" authorityName="Cockerell" authorityYear="1922" box="[518,765,1744,1769]" class="Insecta" family="Apidae" genus="Nannotrigona" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="354" phylum="Arthropoda" rank="genus">
