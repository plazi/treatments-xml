<typeStatus id="1113C681FFABF9575F76FC9FF0DAFCF5" box="[1109,1157,811,835]" pageId="59" pageNumber="60">type</typeStatus>
species of 
<taxonomicName id="09A803A0FFABF9575E22FC9FF4A9FCD0" authorityName="Forster" authorityYear="1968" class="Malacostraca" family="Necrocarcinidae" genus="Pseudonecrocarcinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="59" pageNumber="60" phylum="Arthropoda" rank="genus">
<emphasis id="FCDCA431FFABF9575E22FC9FF4A9FCD0" italics="true" pageId="59" pageNumber="60">Pseudonecrocarcinus</emphasis>
</taxonomicName>
