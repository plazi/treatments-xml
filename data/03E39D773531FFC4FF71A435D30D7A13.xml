: 
<taxonomicName id="4C4A57E23533FFC2FED3A489D0897E4A" baseAuthorityName="LECONTE" baseAuthorityYear="1875" box="[272,467,1275,1296]" class="Insecta" family="Scirtidae" genus="Sarabandus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="1442" phylum="Arthropoda" rank="species" species="robustus">
