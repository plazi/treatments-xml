<taxonomicName id="9AF815C7FF9AFF8BF2CE38F4FE0BFEC6" baseAuthorityName="Bates" baseAuthorityYear="1887" box="[160,481,244,269]" class="Insecta" family="Cerambycidae" genus="Pseudoperma" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="625" phylum="Arthropoda" rank="species" species="chalcogramma">
