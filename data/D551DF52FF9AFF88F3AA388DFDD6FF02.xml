<taxonomicName id="9AF815C7FF9AFF8BF1493939FC02FE99" baseAuthorityName="Bates" baseAuthorityYear="1887" box="[807,1000,313,338]" class="Insecta" family="Cerambycidae" genus="Pseudoperma" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="625" phylum="Arthropoda" rank="species" species="chalcogramma">
