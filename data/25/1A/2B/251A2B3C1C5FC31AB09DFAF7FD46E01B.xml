<subSubSection box="[199,1000,672,695]" pageId="2" pageNumber="118" type="etymology">
<paragraph blockId="2.[151,1436,152,1041]" box="[199,1000,672,695]" pageId="2" pageNumber="118">
<emphasis bold="true" box="[199,327,672,695]" pageId="2" pageNumber="118">Etymology.</emphasis>
