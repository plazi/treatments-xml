<paragraph blockId="1.[264,1324,358,1864]" pageId="1" pageNumber="2">
<emphasis bold="true" box="[264,450,358,384]" pageId="1" pageNumber="2">DIAGNOSIS ­</emphasis>
With 
<number box="[532,546,359,385]" pageId="1" pageNumber="2" value="4.0">4</number>
