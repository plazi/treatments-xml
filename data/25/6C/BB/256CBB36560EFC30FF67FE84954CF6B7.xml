, 
<date box="[1308,1391,450,474]" pageId="3" pageNumber="2" value="1940-10">x.1940</date>
, sin datos de hospedero ni colector, 1(
<geoCoordinate box="[596,690,482,506]" degrees="8, 34" direction="north" orientation="latitude" pageId="3" pageNumber="2" precision="555" value="8.34">8, 34 N</geoCoordinate>
