<collectorName box="[231,254,295,319]" pageId="57" pageNumber="58">St</collectorName>
<collectionCode box="[260,289,296,319]" pageId="57" pageNumber="58">IV</collectionCode>
</materialsCitation>
<materialsCitation collectionCode="IV" pageId="57" pageNumber="58" specimenCode="IV 37" specimenCount="1">
