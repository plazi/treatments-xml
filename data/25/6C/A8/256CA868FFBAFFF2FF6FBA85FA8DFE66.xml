<taxonomicName authority="Moraes" authorityName="Moraes" box="[610,1056,440,464]" class="Arachnida" family="Phytoseiidae" genus="Ueckermannseius" kingdom="Animalia" order="Mesostigmata" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="danhomeenis">
