<subSubSection pageId="37" pageNumber="38" type="discussion">
<paragraph blockId="37.[151,1437,813,1485]" pageId="37" pageNumber="38">
<emphasis bold="true" box="[199,315,1389,1413]" inLineHeading="true" pageId="37" pageNumber="38" reason="1">Remarks.</emphasis>
