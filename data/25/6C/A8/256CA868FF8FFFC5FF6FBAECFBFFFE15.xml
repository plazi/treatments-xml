<taxonomicName authority="Zannou" authorityName="Zannou" box="[619,903,332,356]" class="Arachnida" family="Phytoseiidae" genus="Transeius" kingdom="Animalia" order="Mesostigmata" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="katumaniensis">
