<paragraph blockId="10.[151,1437,758,1290]" pageId="10" pageNumber="71">
<emphasis bold="true" box="[151,219,758,785]" pageId="10" pageNumber="71">Adult</emphasis>
. Wingspan 
<quantity box="[355,438,759,786]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="10" pageNumber="71" unit="mm" value="19.0">19 mm</quantity>
