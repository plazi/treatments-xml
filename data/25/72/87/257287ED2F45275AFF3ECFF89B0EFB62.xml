<quantity box="[576,676,549,570]" metricMagnitude="-4" metricUnit="m" metricValue="4.3" pageId="18" pageNumber="373" unit="mm" value="0.43">0.43 mm</quantity>
± 0.04, CL 
<quantity box="[209,308,579,601]" metricMagnitude="-4" metricUnit="m" metricValue="5.8" pageId="18" pageNumber="373" unit="mm" value="0.58">0.58 mm</quantity>
