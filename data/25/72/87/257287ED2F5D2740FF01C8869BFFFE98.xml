<taxonomicName authority="(Forskal)" baseAuthorityName="Forskal" baseAuthorityYear="1775" box="[258,596,1827,1846]" class="Malacostraca" family="Grapsidae" genus="Metopograpsus" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="367" phylum="Arthropoda" rank="species" species="messor">
<emphasis box="[258,493,1828,1846]" italics="true" pageId="12" pageNumber="367">Metopograpsus messor</emphasis>
