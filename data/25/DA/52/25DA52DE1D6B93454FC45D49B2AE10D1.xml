[[ 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:2B512DF5-BEDE-4C4B-8E41-9AEB8DDA4B35" class="Actinopterygii" family="Nandidae" genus="Nandus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Nandus Valenciennes 1831:481" order="Perciformes" pageNumber="51" phylum="Chordata" rank="genus">Nandus Valenciennes</taxonomicName>
]]
