<taxonomicName baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="1999" box="[877,1072,224,248]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="windarraensis">
