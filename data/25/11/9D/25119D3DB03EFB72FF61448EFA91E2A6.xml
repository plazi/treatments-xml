<taxonomicName box="[1337,1451,1901,1925]" class="Diplopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Glomerida" pageId="8" pageNumber="268" phylum="Arthropoda" rank="order">Glomerida</taxonomicName>
