<collectingCountry box="[383,450,1658,1684]" name="China" pageId="6" pageNumber="527">China</collectingCountry>
(
<collectingRegion box="[465,543,1658,1684]" country="China" name="Hunan" pageId="6" pageNumber="527">Hunan</collectingRegion>
