<paragraph pageId="null" pageNumber="787">
<taxonomicName authority="L." class="Magnoliopsida" family="Caryophyllaceae" genus="Cucubalus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="787" phylum="Tracheophyta" rank="species" species="baccifer">
Cucubalus 
<normalizedToken originalValue="báccifer" pageId="null" pageNumber="787">baccifer</normalizedToken>
<authorityName pageId="null" pageNumber="787">L.</authorityName>
</taxonomicName>
</paragraph>
