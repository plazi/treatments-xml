</geoCoordinate>
, 
<quantity box="[1336,1431,901,927]" metricMagnitude="3" metricUnit="m" metricValue="2.019" pageId="4" pageNumber="5" unit="m" value="2019.0">2019 m</quantity>
, 0 
<number box="[165,219,941,967]" pageId="4" pageNumber="5" value="6.09">6.09</number>
. 
<number box="[223,285,941,967]" pageId="4" pageNumber="5" value="2005.0">2005</number>
, leg. MF, JK, DM, TSz: 
