<quantity box="[289,365,732,758]" metricMagnitude="0" metricUnit="m" metricValue="3.0" metricValueMax="4.0" metricValueMin="2.0" pageId="16" pageNumber="49" unit="m" value="3.0" valueMax="4.0" valueMin="2.0">2–4 m</quantity>
