</taxonomicName>
, 
<taxonomicName authority="Yan, Jin, Wu, Guo &amp; Guo, 2012" authorityName="Yan, Jin, Wu, Guo &amp; Guo" authorityYear="2012" class="Arachnida" family="Podocinidae" genus="Podocinum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="456" phylum="Arthropoda" rank="species" species="tibetensis">
