, 
<taxonomicName box="[1252,1348,512,536]" class="Arachnida" family="Podocinidae" genus="Podocinum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="456" phylum="Arthropoda" rank="species" species="sagax">
<emphasis box="[1252,1266,513,536]" italics="true" pageId="12" pageNumber="456">P</emphasis>
. 
<emphasis box="[1283,1348,513,536]" italics="true" pageId="12" pageNumber="456">sagax</emphasis>
</taxonomicName>
and an unidentified species of 
<taxonomicName box="[418,553,548,572]" class="Arachnida" family="Podocinidae" genus="Podocinella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="456" phylum="Arthropoda" rank="genus">
