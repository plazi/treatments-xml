<emphasis box="[943,1075,512,536]" italics="true" pageId="12" pageNumber="456">bengalensis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1092,1234,512,536]" class="Arachnida" family="Podocinidae" genus="Podocinum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="456" phylum="Arthropoda" rank="species" species="pacificum">
