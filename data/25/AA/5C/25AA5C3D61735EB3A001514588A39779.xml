<taxonomicName authorityName="Yao, Pham &amp; Li" authorityYear="2012" class="Arachnida" family="Pholcidae" genus="Pholcus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pholcus lexuancanhi" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="lexuancanhi">
