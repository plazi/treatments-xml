<subSubSection box="[151,451,1261,1288]" pageId="3" pageNumber="286" type="nomenclature">
<paragraph blockId="3.[151,451,1261,1323]" box="[151,451,1261,1288]" pageId="3" pageNumber="286">
<heading bold="true" box="[151,451,1261,1288]" fontSize="11" level="1" pageId="3" pageNumber="286" reason="1">
<emphasis bold="true" box="[151,451,1261,1288]" pageId="3" pageNumber="286">
<taxonomicName authorityName="Neto &amp; Ribeiro &amp; Passos" authorityYear="2020" box="[151,357,1261,1288]" class="Insecta" family="Hydroptilidae" genus="Flintiella" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="286" phylum="Arthropoda" rank="species" species="triaena">
