, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="145.25711">E145°15.427'</geoCoordinate>
to 
<geoCoordinate direction="south" orientation="latitude" precision="1" value="-6.3820834">S06°22.925'</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="145.27742">E145°16.645'</geoCoordinate>
