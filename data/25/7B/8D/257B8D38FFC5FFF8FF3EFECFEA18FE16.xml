<paragraph blockId="20.[151,1437,259,501]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[151,267,367,393]" pageId="20" pageNumber="21">Remarks.</emphasis>
<taxonomicName authorityName="Ruckes" authorityYear="1966" box="[276,537,367,393]" class="Insecta" family="Pentatomidae" genus="Dryptocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="crenata">
