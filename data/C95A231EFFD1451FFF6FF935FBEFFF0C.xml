<taxonomicName id="86F3E98BFFDE451FFC93FF47FC28FF21" box="[875,1020,188,213]" class="Insecta" family="Megachilidae" genus="Anthidiellum" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="337" phylum="Arthropoda" rank="genus">
