<quantity id="4CA49B71E118FF8FD9BEFA24FC061094" box="[949,1019,1484,1510]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="38" pageNumber="39" unit="m" value="500.0">500m</quantity>
