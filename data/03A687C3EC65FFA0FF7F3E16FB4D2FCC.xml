<subSubSection id="C315655EEC65FFA1FF7F3E50FCB62B76" box="[151,863,1382,1405]" pageId="0" pageNumber="65" type="materials_examined">
<paragraph id="8BB036D5EC65FFA1FF7F3E50FCB62B76" blockId="0.[151,863,1382,1405]" box="[151,863,1382,1405]" pageId="0" pageNumber="65">
<typeStatus id="54B48877EC65FFA1FF7F3E50FF252B76" box="[151,204,1382,1405]" pageId="0" pageNumber="65">Type</typeStatus>
species: 
<taxonomicName id="4C0F4D56EC65FFA1FEC53E50FCEB2B76" authority="Yin &amp; Nomura" authorityName="Yin &amp; Nomura" box="[301,770,1382,1405]" class="Insecta" family="Staphylinidae" genus="Buobellenden" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="65" phylum="Arthropoda" rank="species" species="jingyuanensis" status="sp. nov.">
<emphasis id="B97BEAC7EC65FFA1FEC53E50FDB02B76" box="[301,601,1382,1405]" italics="true" pageId="0" pageNumber="65">Buobellenden jingyuanensis</emphasis>
Yin &amp; Nomura
</taxonomicName>
