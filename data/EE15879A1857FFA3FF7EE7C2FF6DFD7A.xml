<subSubSection id="2EA665071855FFA3FF2EE5F2FF6DFD7A" pageId="11" pageNumber="12" type="distribution">
<paragraph id="6603368C1855FFA3FF2EE5F2FD03FE76" blockId="11.[151,1436,251,709]" box="[199,670,431,458]" pageId="11" pageNumber="12">
<emphasis id="54C8EA9E1855FFA3FF2EE5F2FEFCFE76" bold="true" box="[199,353,431,457]" pageId="11" pageNumber="12">Distribution.</emphasis>
<collectingCountry id="1EAB761C1855FFA3FE8EE5F2FE37FE76" box="[359,426,431,457]" name="China" pageId="11" pageNumber="12">China</collectingCountry>
(
<collectingRegion id="A478F86E1855FFA3FE50E5EDFD88FE75" box="[441,533,432,458]" country="China" name="Yunnan" pageId="11" pageNumber="12">Yunnan</collectingRegion>
