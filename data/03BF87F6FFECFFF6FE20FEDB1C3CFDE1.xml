; F3 l/ 
<geoCoordinate id="EE225027FFECFFF1FDDCFC561A79FC0E" box="[562,639,899,923]" degrees="1.40" direction="west" orientation="longitude" pageId="11" pageNumber="631" precision="555" value="-1.4">w 1.40</geoCoordinate>
; clava l/ 
<geoCoordinate id="EE225027FFECFFF1FD3CFC561B17FC0E" box="[722,785,899,923]" degrees="3.0" direction="west" orientation="longitude" pageId="11" pageNumber="631" precision="5555" value="-3.0">w 3.0</geoCoordinate>
; ped l/F1 l 0.79; F1 l/F2 l 1.56; F1 l/ F3 l 2.0; F1 l/clava l 0.93; F2 l/clava l 0.60; F3 l/clava l 0.47; F1 w/ped 
<geoCoordinate id="EE225027FFECFFF1FC28FC731C12FC2B" box="[966,1044,934,958]" degrees="1.29" direction="west" orientation="longitude" pageId="11" pageNumber="631" precision="555" value="-1.29">w 1.29</geoCoordinate>
; antennal spicule l/C3 l 0.40. Mesosoma: l/ 
<geoCoordinate id="EE225027FFECFFF1FE13FC1D1A4DFC75" box="[509,587,968,992]" degrees="1.68" direction="west" orientation="longitude" pageId="11" pageNumber="631" precision="555" value="-1.68">w 1.68</geoCoordinate>
