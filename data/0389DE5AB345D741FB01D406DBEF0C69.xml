<quantity id="4CD8C2A9B345D741FB2CD064DBAD0ECC" box="[601,694,1385,1412]" metricMagnitude="-3" metricUnit="m" metricValue="5.8" pageId="16" pageNumber="17" unit="mm" value="5.8">5.8 mm</quantity>
