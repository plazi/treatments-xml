genus: 
<taxonomicName id="4C4884F5B40AFFB1447FFB30DCE099E0" authority="Illiger, 1811" authorityName="Illiger" authorityYear="1811" box="[280,499,1204,1226]" class="Aves" family="Icteridae" genus="Cassicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="5" pageNumber="290" phylum="Chordata" rank="genus">
<emphasis id="B93C2364B40AFFB1447FFB30DC6199E3" box="[280,370,1204,1225]" italics="true" pageId="5" pageNumber="290">Cassicus</emphasis>
Illiger, 
<number id="3D80ED51B40AFFB144A6FB30DCE099E0" box="[449,499,1204,1226]" pageId="5" pageNumber="290" value="1811.0">1811</number>
</taxonomicName>
