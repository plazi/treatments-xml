<taxonomicName id="6ACB4D39FF862714FAF9F813E05C848B" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2000" box="[390,621,1921,1943]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="morgani">
<emphasis id="9FBFEAA8FF862714FAF9F813E05C848B" box="[390,621,1921,1943]" italics="true" pageId="67" pageNumber="68">Limbodessus morgani</emphasis>
