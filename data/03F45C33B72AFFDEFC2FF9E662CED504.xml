<quantity id="4CA540C0B729FFDEFD61FD0A645FD0DA" box="[707,790,1142,1166]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" pageId="14" pageNumber="85" unit="mm" value="4.6">4.6 mm</quantity>
; length of thorax, males 2.0– 
<quantity id="4CA540C0B729FFDEFE72FDE86568D0FF" box="[464,545,1172,1196]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="14" pageNumber="85" unit="mm" value="2.1">2.1 mm</quantity>
