<quantity id="4CA540C0B729FFDEFDDBFDE86584D0FF" box="[633,717,1172,1196]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="14" pageNumber="85" unit="mm" value="2.2">2.2 mm</quantity>
