<quantity id="B96E50E8FFD5FFB0FB8AF9F7FB89FDAB" box="[1055,1144,510,535]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="8" pageNumber="414" unit="mm" value="3.5">3.5 mm</quantity>
