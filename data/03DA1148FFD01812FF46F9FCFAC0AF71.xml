<key id="FFE2F76FFFD01813FF46F9FCFDD7A816" box="[151,560,1630,1656]" pageId="18" pageNumber="19">
<emphasis id="B9077C4CFFD01813FF46F9FCFDD7A816" bold="true" box="[151,560,1630,1656]" pageId="18" pageNumber="19">
<keyLead id="3082036BFFD01813FF46F9FCFDD7A816" box="[151,560,1630,1656]" pageId="18" pageNumber="19">
<number id="3DBBB279FFD01813FF46F9FCFF51A816" box="[151,182,1630,1656]" pageId="18" pageNumber="19" value="26.0">26</number>
) 
<taxonomicName id="4C73DBDDFFD01813FF14F9FCFDD7A816" authority="Bei-Bienko, 1951" authorityName="Bei-Bienko" authorityYear="1951" box="[197,560,1630,1656]" class="Insecta" family="Tettigoniidae" genus="Hypsopedes" kingdom="Animalia" order="Orthoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
<emphasis id="B9077C4CFFD01813FF14F9FCFEB3A816" bold="true" box="[197,340,1630,1656]" italics="true" pageId="18" pageNumber="19">Hypsopedes</emphasis>
<bibRefCitation id="EFE2DDAFFFD01813FE8DF9FCFDD7A816" author="Bei-Bienko" box="[348,560,1630,1656]" pageId="18" pageNumber="19" refString="Bei-Bienko, G. Y. (1951) Studies on long-horned grasshoppers (Orthoptera: Tettigoniidae) of the USSR and adjacent countries. Trudy Vsesoyeiznogo Entomologicheskojo Obschchestva [Horae Societatis Entomologicae Unionis Sovieticae], 43, 129 - 170 (in Russian)." type="journal article" year="1951">
Bei-Bienko, 
<number id="3DBBB279FFD01813FE24F9FCFDD7A816" box="[501,560,1630,1656]" pageId="18" pageNumber="19" value="1951.0">1951</number>
</bibRefCitation>
</taxonomicName>
</keyLead>
</emphasis>
