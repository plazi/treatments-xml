<geoCoordinate id="9DF7699EA3A5A3A4A7ABCF7EC7C3D3AE" direction="north" orientation="latitude" precision="1" value="22.162933">22°09.776'N</geoCoordinate>
, 
<geoCoordinate id="0FE5633EA06DE77D93DBB505441F4614" direction="east" orientation="longitude" precision="1" value="100.87608">100°52.565'E</geoCoordinate>
, elevation ca 921 m, 26.06.2013, tropical seasonal rain forest, sieving; 4♂1♀, Guanping Town: Xishuangbanna nature reserve, 
<geoCoordinate id="DC1B80984266378F65AA96930489C91B" direction="north" orientation="latitude" precision="1" value="22.227434">22°13.646'N</geoCoordinate>
, 
<geoCoordinate id="E1F6562356A789E9AFCF2B9656584118" direction="east" orientation="longitude" precision="1" value="100.88913">100°53.348'E</geoCoordinate>
, elevation ca 939 m, 27.06.2013, tropical seasonal rain forest, sieving; 1♀, 
<geoCoordinate id="4F03F957F6D701E79760AA3C663AB082" direction="north" orientation="latitude" precision="1" value="22.32435">22°19.461'N</geoCoordinate>
, 
<geoCoordinate id="0B2A6D3AE2541F47D44343CFF731696E" direction="east" orientation="longitude" precision="1" value="100.89048">100°53.429'E</geoCoordinate>
, elevation ca 851 m, 28.06.2013, tropical seasonal rain forest, sieving; 1♂, 
<geoCoordinate id="DAA19DD1B4501549E9CDB669210FD5A4" direction="north" orientation="latitude" precision="1" value="22.324383">22°19.463'N</geoCoordinate>
