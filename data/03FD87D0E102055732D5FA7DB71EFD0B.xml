<taxonomicName id="4C544D45E10105573602FE32B1D9FE17" box="[1088,1223,441,464]" class="Adenophorea" family="Tripylidae" genus="Trischistoma" kingdom="Animalia" order="Enoplida" pageId="18" pageNumber="19" phylum="Nematoda" rank="species" species="waiotama" status="sp. nov">
