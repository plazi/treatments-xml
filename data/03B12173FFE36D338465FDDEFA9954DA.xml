<taxonomicName id="4C18EBE6FFE06D3284F1F943FDF05687" box="[412,614,1764,1787]" class="Pycnogonida" family="Nymphonidae" genus="Nymphon" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="polyglia" status="sp. nov.">
<emphasis id="B96C4C77FFE06D3284F1F943FDF05687" box="[412,614,1764,1787]" italics="true" pageId="2" pageNumber="3">Nymphon polyglia</emphasis>
