<taxonomicName id="4C084D6DFF85FFA2FC48FDC7FC71A266" box="[902,1004,562,587]" class="Insecta" family="Bombyliidae" genus="Systropus" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="21" phylum="Arthropoda" rank="species" species="hoppo">
