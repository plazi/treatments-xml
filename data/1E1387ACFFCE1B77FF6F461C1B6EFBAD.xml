</taxonomicName>
(= 
<taxonomicName id="51BA4D39FFCE1B77FB0047F81FC2FBAC" class="Diplopoda" family="Arthrosphaeridae" genus="Arthrosphaera" kingdom="Animalia" order="Sphaerotheriida" pageId="5" pageNumber="456" phylum="Arthropoda" rank="species" species="brandti">
