<taxonomicName id="4C6B4D2FFFD4FFECFB6F1E6022FBFDDC" box="[1277,1429,583,608]" class="Rhynchonellata" family="Cancellothyrididae" genus="Terebratulina" kingdom="Animalia" order="Terebratulida" pageId="4" pageNumber="59" phylum="Brachiopoda" rank="genus">
