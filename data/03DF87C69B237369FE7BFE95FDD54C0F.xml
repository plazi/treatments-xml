<taxonomicName id="4C764D539B207369FE60FDD9FD544D2B" box="[415,558,602,621]" class="Insecta" family="Curculionidae" genus="Curculio" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="139" phylum="Arthropoda" rank="species" species="fausti">
