<taxonomicName id="3FBDC8FB081C15F1DD5C166372ECA2CF" class="Magnoliopsida" family="Asteraceae" genus="Oocephala" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Oocephala" order="Asterales" pageId="33" pageNumber="82" phylum="Tracheophyta" rank="genus">Oocephala</taxonomicName>
