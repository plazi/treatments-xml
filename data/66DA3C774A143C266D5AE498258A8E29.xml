<paragraph id="5AEE54459794EA5DA43A5B93F4E61C82" pageId="33" pageNumber="82">
<taxonomicName id="42036027031E025B73EC2492D6894D9D" authority="subsect. Oocephalae" class="Magnoliopsida" family="Asteraceae" genus="Vernonia" higherTaxonomySource="CoL" infraspecific-rank="subsect." kingdom="Plantae" lsidName="Vernonia" order="Asterales" pageId="33" pageNumber="82" phylum="Tracheophyta" rank="genus" subsection="Oocephalae">Vernonia subsect. Oocephalae</taxonomicName>
