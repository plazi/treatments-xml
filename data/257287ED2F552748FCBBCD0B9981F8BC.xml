<caption id="F9A466732F54274EFF14C93A9D27F8AA" ID-DOI="http://doi.org/10.5281/zenodo.5438729" ID-Zenodo-Dep="5438729" httpUri="https://zenodo.org/record/5438729/files/figure.png" pageId="3" pageNumber="358" startId="3.[155,233,1846,1865]" targetBox="[155,1435,199,1814]" targetPageId="3">
<paragraph id="AD6436FB2F54274EFF14C93A9D27F8AA" blockId="3.[155,1434,1846,1895]" pageId="3" pageNumber="358">
<emphasis id="9FAFEAE92F54274EFF14C93A987FF884" bold="true" box="[155,259,1846,1865]" pageId="3" pageNumber="358">Figure 1.</emphasis>
<taxonomicName id="6ADB4D782F54274EFE81C93B9BF0F887" authority="Crosnier. First" authorityName="Crosnier. First" authorityYear="1965" box="[270,652,1847,1866]" class="Malacostraca" family="Grapsidae" genus="Grapsus" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="358" phylum="Arthropoda" rank="species" species="fourmanoiri">
