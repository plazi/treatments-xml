<taxonomicName id="4C394D61FFFA6A19172CF998FCAAD6E6" authority="Glover &amp; Taylor, 2007" authorityName="Glover &amp; Taylor" authorityYear="2007" class="Magnoliopsida" family="Amaranthaceae" genus="Leucosphaera" kingdom="Plantae" order="Caryophyllales" pageId="59" pageNumber="168" phylum="Tracheophyta" rank="species" species="diaphana" status="n. sp.">
