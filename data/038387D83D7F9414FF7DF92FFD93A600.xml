<taxonomicName id="4C2A4D4D3D789415FC52F879FB14A5E5" authorityName="Chemsak &amp; Linsley" authorityYear="1970" box="[952,1194,1978,2000]" class="Insecta" family="Cerambycidae" genus="Eburodacrys" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="505" phylum="Arthropoda" rank="species" species="hesperidis">
<emphasis id="B95EEADC3D789415FC52F879FB14A5E5" box="[952,1194,1978,2000]" italics="true" pageId="12" pageNumber="505">Eburodacrys hesperidis</emphasis>
