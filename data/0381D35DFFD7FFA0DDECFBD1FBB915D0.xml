</taxonomicName>
(
<taxonomicName id="4C2819C8FFD7FFA1DCB1F907FED412B5" box="[147,299,1690,1714]" class="Magnoliopsida" family="Connaraceae" kingdom="Plantae" order="Oxalidales" pageId="88" pageNumber="1974" phylum="Tracheophyta" rank="family">Connaraceae</taxonomicName>
