, 
<date id="FF8AB2B5BA689D01FF0DFC6CFE34FC1B" box="[226,395,956,979]" pageId="29" pageNumber="30" value="1905-03-04">
<collectingDate id="EFCE4B5DBA689D01FF0DFC6CFE34FC1B" box="[226,395,956,979]" pageId="29" pageNumber="30" value="1905-03-04">4 March 1905</collectingDate>
