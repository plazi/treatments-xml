<paragraph id="9C0F65DB2CE3D5E03C27F2E1FCC8E56C" pageId="0" pageNumber="19">
<taxonomicName id="A2F33E67D9794EEF9C7A9AB87AB43D43" authorityName="Blume" authorityYear="1851" class="Dicotyledoneae" family="Betulaceae" genus="Corylus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Corylus sieboldiana" order="Fagales" pageId="0" pageNumber="19" phylum="Angiospermae" rank="species" species="sieboldiana">
<emphasis id="8CBC66217864139064778CD34841C1E9" italics="true" pageId="0" pageNumber="19">Corylus sieboldiana</emphasis>
</taxonomicName>
and 
<taxonomicName id="DAA7EFFB683C5AE1AF03F9BEA57B91E8" genus="C." lsidName="C. heterophylla" pageId="0" pageNumber="19" rank="species" species="heterophylla">
