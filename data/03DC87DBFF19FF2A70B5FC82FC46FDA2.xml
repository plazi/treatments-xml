</taxonomicName>
(
<taxonomicName id="4C754D4EFF19FF2A7294FA16FCF8FFC1" box="[694,857,1423,1447]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="149" pageNumber="150" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
