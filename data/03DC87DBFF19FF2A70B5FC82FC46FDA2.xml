<taxonomicName id="4C754D4EFF19FF2A7150F9F1FE51FC19" baseAuthorityName="Pallas" baseAuthorityYear="1811" box="[370,496,1640,1664]" class="Aves" family="Motacillidae" genus="Anthus" kingdom="Animalia" order="Passeriformes" pageId="149" pageNumber="150" phylum="Chordata" rank="species" species="cervinus">
