<collectionCode id="42B61DCEFF2283D2FB935AD5FBF6FD36" box="[1111,1126,693,717]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="159" pageNumber="159" type="Herbarium">P</collectionCode>
