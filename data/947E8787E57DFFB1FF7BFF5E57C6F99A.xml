(
<figureCitation id="84EC2A14E57DFFB0FF73FF7354E6FF12" box="[159,231,186,210]" captionStart="FIGURE 8" captionStartId="4.[151,250,1624,1647]" captionText="FIGURE 8. Euhybus nigritarsis sp. nov., adult (holotype). Scale bar = 1 mm." pageId="4" pageNumber="377">Figs 8</figureCitation>
, 
<figureCitation id="84EC2A14E57DFFB0FF1FFF735502FF12" box="[243,259,186,210]" captionStart="FIGURE 9" captionStartId="5.[151,250,1000,1023]" captionTargetBox="[268,1322,523,953]" captionTargetId="figure@5.[242,1345,484,979]" captionTargetPageId="5" captionText="FIGURE 9. Euhybus nigritarsis sp. nov., wing. Scale bar = 0.5 mm." httpUri="https://zenodo.org/record/217310/files/figure.png" pageId="4" pageNumber="377">9</figureCitation>
