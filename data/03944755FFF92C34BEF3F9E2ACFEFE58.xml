<taxonomicName id="4C3D8DC0FFFB2C34BF37FABEAD05FAEB" authorityName="Assemblage Zone (Rubidge" authorityYear="1995" box="[382,526,1295,1314]" class="Reptilia" family="Oudenodontidae" genus="Tropidostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Therapsida" pageId="3" pageNumber="480" phylum="Chordata" rank="genus">
<emphasis id="B9492A51FFFB2C34BF37FABEAD05FAEB" box="[382,526,1295,1314]" italics="true" pageId="3" pageNumber="480">Tropidostoma</emphasis>
