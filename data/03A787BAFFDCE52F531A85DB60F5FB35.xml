<location id="8ED16077FFDCE52F556984A1667DFC75" LSID="urn:lsid:plazi:treatment:03A787BAFFDCE52F531A85DB60F5FB35:8ED16077FFDCE52F556984A1667DFC75" box="[1356,1398,937,961]" country="China" municipality="The" name="The" pageId="29" pageNumber="146" stateProvince="Xinjiang Uygur">The</location>
