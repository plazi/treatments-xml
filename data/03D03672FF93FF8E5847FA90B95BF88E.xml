<number id="3DB19543FF96FF8E5D77FC41B836FBAE" box="[1080,1095,1015,1041]" pageId="6" pageNumber="7" value="2.0">2</number>
/ 
<number id="3DB19543FF96FF8E5D1EFC41B82FFBAE" box="[1105,1118,1015,1041]" pageId="6" pageNumber="7" value="3.0">3</number>
