<taxonomicName id="C6E44D458672FFD4FE5BFC7CFDFFFC45" authorityName="Reuter" authorityYear="1871" box="[464,593,980,1005]" class="Insecta" family="Anthocoridae" genus="Lasiochilus" kingdom="Animalia" order="Hemiptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="genus">
