<quantity id="4CEF9B25FF30FF149A0DFF65F88530B4" box="[624,708,151,177]" metricMagnitude="1" metricUnit="m" metricValue="4.55" metricValueMax="9.0" metricValueMin="0.1" pageId="161" pageNumber="162" unit="m" value="45.5" valueMax="90.0" valueMin="1.0">1–90 m</quantity>
(
<quantity id="4CEF9B25FF30FF149AAEFF64F97530B7" box="[723,820,150,178]" metricMagnitude="1" metricUnit="m" metricValue="4.54152" metricValueMax="8.9916" metricValueMin="0.09144" pageId="161" pageNumber="162" unit="ft" value="149.0" valueMax="295.0" valueMin="3.0">3–295 ft</quantity>
