<taxonomicName id="8BE84D58274D1206AB4731F6FD8AF927" authority="Zhang &amp; Yang &amp; Grootaert, 2008" authorityName="Zhang &amp; Yang &amp; Grootaert" authorityYear="2008" box="[276,570,1710,1733]" class="Insecta" family="Dolichopodidae" genus="Hercostomus" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="21" phylum="Arthropoda" rank="species" species="singaporensis" status="new species">
