<number id="DB0E3973FFFDBF46A4A9FC311B2AFC00" box="[996,1036,938,961]" pageId="2" pageNumber="176" value="3.5" valueMax="5.0" valueMin="2.0">2–5</number>
November 
<number id="DB0E3973FFFDBF46A3C2FC311BE1FC00" box="[1167,1223,938,961]" pageId="2" pageNumber="176" value="2006.0">2006</number>
