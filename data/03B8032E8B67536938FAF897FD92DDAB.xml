<taxonomicName id="4C11C9BB8B6A5369391CFC76FDCED89C" authorityName="Leschen &amp; Wegrzynowicz" authorityYear="2008" box="[369,514,978,1001]" class="Insecta" family="Erotylidae" genus="Arrowcryptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="56" phylum="Arthropoda" rank="genus">
