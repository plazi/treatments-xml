<taxonomicName id="4C11C9BB8B6A53693BBAFC76FB89D89C" authorityName="Leschen &amp; Wegrzynowicz" authorityYear="2008" box="[983,1093,978,1001]" class="Insecta" family="Erotylidae" genus="Arrowcryptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="56" phylum="Arthropoda" rank="species" species="skelleyi">
<emphasis id="B9656E2A8B6A53693BBAFC76FB89D89C" box="[983,1093,978,1001]" italics="true" pageId="11" pageNumber="56">A. skelleyi</emphasis>
