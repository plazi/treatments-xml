<elevation id="3A88D19357647108EB78287FE3C3FEC2" box="[381,487,271,295]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="5" pageNumber="69" unit="m" value="1400.0">1400 m</elevation>
</quantity>
.a.s.l., 
