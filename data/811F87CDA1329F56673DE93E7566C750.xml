</taxonomicName>
<taxonomicNameLabel id="20F157B2A1339F5067DCEB83765EC0A9" box="[502,584,1716,1739]" pageId="11" pageNumber="234" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="CEB64D58A1339F506452EB8376FDC0A9" authorityName="Haswell" authorityYear="1879" box="[632,747,1715,1739]" class="Malacostraca" family="Aoridae" genus="Xenocheira" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="234" phylum="Arthropoda" rank="species" species="fasciata">
