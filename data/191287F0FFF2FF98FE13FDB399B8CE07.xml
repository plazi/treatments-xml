(
<figureCitation id="09802A63FFF2FF99FD88FD9399DEC904" box="[550,681,573,597]" captionStart="Figure 5" captionStartId="33.[138,198,1030,1049]" captionTargetBox="[264,1057,135,992]" captionTargetId="figure-191@33.[262,1062,133,994]" captionTargetPageId="33" captionText="Figure 5. (A, B) Trididemnum nebula (QM G328001 syntype): (A) thorax; (B) larva. (C) Trididemnum titanium (QM G323351 holotype): colony. (D, E) Lissoclinum capsulatum (QM G308804 holotype): (D) thorax; (E) abdomen. Scale bars: 0.1 mm (A, B, D, E), 1 cm (C)." figureDoi="http://doi.org/10.5281/zenodo.4669592" httpUri="https://zenodo.org/record/4669592/files/figure.png" pageId="42" pageNumber="1204">Figures 5C</figureCitation>
, 
<figureCitation id="09802A63FFF2FF99FD19FD93999DC904" box="[695,746,573,597]" captionStart="Figure 11" captionStartId="40.[125,185,1584,1603]" captionTargetBox="[280,1018,129,1548]" captionTargetId="figure-1@40.[280,1019,129,1548]" captionTargetPageId="40" captionText="Figure 11. Scanning electron micrographs. (A) Trididemnum pigmentatum (QMG308842); (B) Trididemnum sibogae (QM G308893); (C) Trididemnum titanium (QM G308816); (D) Lissoclinum capsulatum (QM G308804); (E) Lissoclinum reginum (QM G308871); (F) Didemnidae?genus?species (QM G308889); (G, H) Didemnidae?genus?species [(G) QM G308888; (H) QM G308900]." figureDoi="http://doi.org/10.5281/zenodo.4669610" httpUri="https://zenodo.org/record/4669610/files/figure.png" pageId="42" pageNumber="1204">11C</figureCitation>
) 
<taxonomicName id="56BB4D65FFF2FF99FFD3FDF09948C927" authority=": Kott 2001, p 285" authorityName=": Kott" authorityPageNumber="285" authorityYear="2001" box="[125,575,606,630]" class="Ascidiacea" family="Didemnidae" genus="Trididemnum" kingdom="Animalia" order="Enterogona" pageId="42" pageNumber="1204" phylum="Chordata" rank="species" species="sibogae">
<emphasis id="A3CFEAF4FFF2FF99FFD3FDF09A10C927" box="[125,359,606,630]" italics="true" pageId="42" pageNumber="1204">Trididemnum sibogae</emphasis>
: 
<bibRefCitation id="F52A4B17FFF2FF99FED8FDF09A84C927" author="Kott P" box="[374,499,606,630]" pageId="42" pageNumber="1204" pagination="1 - 410" refId="ref21062" refString="Kott P. 2001. The Australian Ascidiacea Pt 4, Didemnidae. Memoirs of the Queensland Museum 47: 1 - 410." type="journal article" year="2001">Kott 2001</bibRefCitation>
