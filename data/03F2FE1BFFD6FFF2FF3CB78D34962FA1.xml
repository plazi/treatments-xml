spp. 5, 
<taxonomicName id="4C5B348EFFD7FFF4FD75B11A341F2857" authority="Kozlov, 2020" authorityName="Kozlov" authorityYear="2020" box="[734,809,1549,1573]" class="Insecta" family="Adelidae" genus="Nemophora" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="480" phylum="Arthropoda" rank="species" species="alba" status="sp. nov.">
<emphasis id="B92F931FFFD7FFF4FD75B11A341F2857" box="[734,809,1549,1573]" italics="true" pageId="3" pageNumber="480">N. alba</emphasis>
</taxonomicName>
