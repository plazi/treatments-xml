; 13–17, 
<taxonomicName id="4C5B348EFFD0FFF3FB98B08E338E29C0" authority="Kozlov, 2020" authorityName="Kozlov" authorityYear="2020" box="[1075,1208,1946,1970]" class="Insecta" family="Adelidae" genus="Nemophora" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="481" phylum="Arthropoda" rank="species" species="bruneiella" status="sp. nov.">
