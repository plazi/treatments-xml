<emphasis id="B963EAD2FFFEFFDA9A77FA55F88635C5" bold="true" box="[522,711,1446,1473]" pageId="111" pageNumber="112">Dusky Rockfish</emphasis>
. To 
<quantity id="4CEF9B25FFFEFFDA9A84FA54F97F35C4" box="[761,830,1446,1473]" metricMagnitude="-1" metricUnit="m" metricValue="5.9" pageId="111" pageNumber="112" unit="cm" value="59.0">59 cm</quantity>
(
<quantity id="4CEF9B25FFFEFFDA9B31FA55F9DC35C7" box="[844,925,1447,1474]" metricMagnitude="-1" metricUnit="m" metricValue="5.892799999999999" pageId="111" pageNumber="112" unit="in" value="23.2">23.2 in</quantity>
) FL; 
<quantity id="4CEF9B25FFFEFFDA9BA1FA55FE7735C4" box="[988,1078,1447,1473]" metricMagnitude="-1" metricUnit="m" metricValue="6.09" pageId="111" pageNumber="112" unit="cm" value="60.9">60.9 cm</quantity>
(
<quantity id="4CEF9B25FFFEFFDA9C38FA55FEC035C7" box="[1093,1153,1447,1474]" metricMagnitude="-1" metricUnit="m" metricValue="6.096" pageId="111" pageNumber="112" unit="in" value="24.0">24 in</quantity>
) TL based on conversion factor provided by James Orr. 
<collectingRegion id="49D3F822FFFEFFDA9A72FA39F8C135E0" box="[527,640,1483,1509]" country="Japan" name="Hokkaido" pageId="111" pageNumber="112">Hokkaido</collectingRegion>
, 
<collectingCountry id="F3007650FFFEFFDA9AF4FA39F88835E0" box="[649,713,1483,1509]" name="Japan" pageId="111" pageNumber="112">Japan</collectingCountry>
and eastern Kamchatka to about 
<geoCoordinate id="EE235007FFFEFFDA9C4CFA39FE2D35E0" box="[1073,1132,1483,1509]" degrees="60" direction="north" orientation="latitude" pageId="111" pageNumber="112" precision="55555" value="60.0">60°N</geoCoordinate>
