species: 
<taxonomicName id="748A2483CE3DFF84D32E9C29B32AFE92" authority="Takagi, 1965" authorityName="Takagi" authorityYear="1965" box="[302,624,358,381]" class="Insecta" family="Dolichopodidae" genus="Conchopus" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="rectus">
