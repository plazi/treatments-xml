: Sichuan, 
<specimenCount id="9D68D882B226FFCDFD2E4A33494BFCAD" box="[651,776,794,818]" pageId="6" pageNumber="43" type="female">
<number id="3DA6012CB226FFCDFD2E4A3348E4FCAD" box="[651,679,794,818]" pageId="6" pageNumber="43" value="58.0">58</number>
