<number id="3DA6012CB226FFCDFCB64A334962FCAD" box="[787,801,794,818]" pageId="6" pageNumber="43" value="6.0">6</number>
males
</specimenCount>
, on 
<taxonomicName id="4C6E6888B226FFCDFC3F4A324ED0FCAC" box="[922,1171,795,819]" class="Magnoliopsida" family="Ericaceae" genus="Rhododendron" kingdom="Plantae" order="Ericales" pageId="6" pageNumber="43" phylum="Tracheophyta" rank="species" species="simsii">
