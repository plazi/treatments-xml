<caption id="DF460795FFC9FFE1FF09E7E0F7D3F84A" ID-DOI="http://doi.org/10.5281/zenodo.4636542" ID-Zenodo-Dep="4636542" httpUri="https://zenodo.org/record/4636542/files/figure.png" pageId="5" pageNumber="368" startId="5.[151,264,1758,1782]" targetBox="[158,1433,185,1728]" targetPageId="5">
<paragraph id="8B86571DFFC9FFE1FF09E7E0F7D3F84A" blockId="5.[151,1436,1758,1932]" pageId="5" pageNumber="368">
<emphasis id="B94D8B0FFFC9FFE1FF09E7E0F721F93A" bold="true" box="[151,322,1758,1782]" pageId="5" pageNumber="368">FIGURES 8–24</emphasis>
. 
<taxonomicName id="4C392C9EFFC9FFE1FECEE7E0F453F93A" authority="Tian &amp; Zheng &amp; Xing, 2021" authorityName="Tian &amp; Zheng &amp; Xing" authorityYear="2021" box="[336,560,1758,1782]" class="Insecta" family="Diaspididae" genus="Nikkoaspis" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="368" phylum="Arthropoda" rank="species" species="brevispina" status="sp. n.">
<emphasis id="B94D8B0FFFC9FFE1FECEE7E0F453F93A" box="[336,560,1758,1782]" italics="true" pageId="5" pageNumber="368">Nikkoaspis brevispina</emphasis>
</taxonomicName>
<emphasis id="B94D8B0FFFC9FFE1FDA6E7E0F411F93A" bold="true" box="[568,626,1758,1782]" pageId="5" pageNumber="368">
