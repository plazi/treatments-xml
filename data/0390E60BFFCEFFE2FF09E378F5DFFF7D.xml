. 
<taxonomicName id="4C392C9EFFC8FFE0FE87E6A6F79AF87C" authority="Tian &amp; Zheng &amp; Xing, 2021" authorityName="Tian &amp; Zheng &amp; Xing" authorityYear="2021" box="[281,505,1944,1968]" class="Insecta" family="Diaspididae" genus="Nikkoaspis" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="367" phylum="Arthropoda" rank="species" species="brevispina" status="sp. n.">
<emphasis id="B94D8B0FFFC8FFE0FE87E6A6F79AF87C" box="[281,505,1944,1968]" italics="true" pageId="4" pageNumber="367">Nikkoaspis brevispina</emphasis>
</taxonomicName>
<emphasis id="B94D8B0FFFC8FFE0FE61E6A6F45BF87C" bold="true" box="[511,568,1944,1968]" pageId="4" pageNumber="367">
