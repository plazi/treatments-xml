<taxonomicName id="AF984D02FF9B155FFD3EFE5CFCDBD508" authorityName="Hernando &amp; Ribera" authorityYear="2014" box="[696,823,492,519]" class="Insecta" family="Limnichidae" genus="Caccothryptus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="sinensis">
