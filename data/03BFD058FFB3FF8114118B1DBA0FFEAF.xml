<paragraph id="8BA9614EFFB2FF8215EA8CF9BF61F98F" blockId="41.[160,1156,1528,1692]" box="[160,300,1528,1553]" pageId="41" pageNumber="811">
<emphasis id="B962BD5CFFB2FF8215EA8CF9BF61F98F" bold="true" box="[160,300,1528,1553]" italics="true" pageId="41" pageNumber="811">Distribution</emphasis>
</paragraph>
<paragraph id="8BA9614EFFB2FF8215EA8F1DBF6DF902" blockId="41.[160,1156,1528,1692]" pageId="41" pageNumber="811">
<taxonomicName id="4C161ACDFFB2FF8215EA8F1DBF9EF9AA" baseAuthorityName="Klausewitz" baseAuthorityYear="1957" box="[160,467,1564,1588]" class="Reptilia" family="Agamidae" genus="Acanthocercus" kingdom="Animalia" order="Squamata" pageId="41" pageNumber="811" phylum="Chordata" rank="species" species="ugandaensis">
<emphasis id="B962BD5CFFB2FF8215EA8F1DBF9EF9AA" box="[160,467,1564,1588]" italics="true" pageId="41" pageNumber="811">Acanthocercus ugandaensis</emphasis>
