<taxonomicName id="5DB3FA9722B4C28C1AB4434A9A79E0E6" authority="Burmeister, 1847" authorityName="Burmeister" authorityYear="1847" class="Insecta" family="Scarabaeidae" genus="Augoderia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Augoderia" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="genus">Augoderia Burmeister, 1847</taxonomicName>
