(
<collectionCode id="DEEDC8CCFFB0FFB1706AC90FFE116EDE" LSID="urn:lsid:biocol.org:col:34988" box="[344,431,1387,1411]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="32" pageNumber="33">MNHN</collectionCode>
