<taxonomicName id="AA4F1BC52DB7541B5F195C1AAC047786" class="Liliopsida" family="Marantaceae" genus="Calathea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Calathea" order="Zingiberales" pageId="94" pageNumber="95" phylum="Tracheophyta" rank="genus">Calathea</taxonomicName>
