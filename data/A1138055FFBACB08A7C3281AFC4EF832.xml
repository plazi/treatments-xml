; R 
<number id="9F722364FFBACB08A79B2DCFFF0FFB7D" box="[207,215,1179,1193]" pageId="5" pageNumber="202" value="4.0">4</number>
+ 
<number id="9F722364FFBACB08A7B42DCFFF3FFB7D" box="[224,231,1179,1193]" pageId="5" pageNumber="202" value="5.0">5</number>
