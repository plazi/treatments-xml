<taxonomicName id="A0D6EBFAC550CE8B2C10A3B69CB16305" class="Lecanoromycetes" family="Parmeliaceae" genus="Neoprotoparmelia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Neoprotoparmelia plurisporibadia" order="Lecanorales" pageId="0" pageNumber="19" phylum="Ascomycota" rank="species" species="plurisporibadia">Neoprotoparmelia plurisporibadia</taxonomicName>
