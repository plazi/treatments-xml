<quantity id="2F629B2B9052FF9AE427A60911ABFD8F" box="[1041,1186,547,571]" metricMagnitude="2" metricUnit="m" metricValue="7.54" metricValueMax="13.08" metricValueMin="2.0" pageId="5" pageNumber="580" unit="m" value="754.0" valueMax="1308.0" valueMin="200.0">200â€“1308 m</quantity>
(
