<taxonomicName id="4C7DF5CC3663FFFAFCF3F9C931FF3D17" authority="(Skuse)" authorityName="Skuse" baseAuthorityName="Skuse" baseAuthorityYear="1888" box="[854,1211,1611,1636]" class="Insecta" family="Sciaridae" genus="Pseudolycoriella" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cavatica">
<emphasis id="B909525D3663FFFAFCF3F9C931263D10" box="[854,1122,1612,1636]" italics="true" pageId="14" pageNumber="15">Pseudolycoriella cavatica</emphasis>
