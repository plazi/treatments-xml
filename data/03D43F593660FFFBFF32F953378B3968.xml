<taxonomicName id="4C7DF5CC3663FFFAFEC5F9C937193D17" baseAuthorityName="Winnertz" baseAuthorityYear="1867" box="[352,605,1612,1636]" class="Insecta" family="Sciaridae" genus="Pseudolycoriella" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="bruckii">
