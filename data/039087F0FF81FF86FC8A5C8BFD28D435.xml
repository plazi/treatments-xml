<taxonomicName id="4C394D65FF8FFF85FF35553AFE04DD08" authority="Gao &amp; Yang, 2004" authorityName="Gao &amp; Yang" authorityYear="2004" box="[236,462,2064,2085]" class="Insecta" family="Lauxaniidae" genus="Homoneura" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="360" phylum="Arthropoda" rank="species" species="longispina" status="new species">
<emphasis id="B94DEAF4FF8FFF85FF35553AFE04DD08" box="[236,462,2064,2085]" italics="true" pageId="9" pageNumber="360">Homoneura longispina</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A27E578FFF8FFF85FE035525FD9BDD09" box="[474,593,2063,2084]" pageId="9" pageNumber="360" rank="species">new species</taxonomicNameLabel>
