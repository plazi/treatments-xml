<paragraph id="8B8636E6FF8CFF86FF515DEBFD28D435" blockId="10.[136,775,193,280]" pageId="10" pageNumber="361">
<collectingRegion id="49FDF804FF8CFF86FF515DEBFF2DD5F5" box="[136,231,193,216]" country="China" name="Guangxi" pageId="10" pageNumber="361">Guangxi</collectingRegion>
