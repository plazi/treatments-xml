<taxonomicName id="2ADF37C2724FFFC7FCA0F8B39BC2F8AB" authority="Bian &amp; Shi" authorityName="Bian &amp; Shi" box="[776,1159,1842,1867]" class="Insecta" family="Haglidae" genus="Eugryllacris" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="443" phylum="Arthropoda" rank="species" species="elongata" status="sp. nov.">
