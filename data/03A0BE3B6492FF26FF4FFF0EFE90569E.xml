<taxonomicName id="4C0974AE6492FF27FF2DFEC8FE13561C" authorityName="Gray" authorityYear="1828" box="[245,466,350,375]" class="Mammalia" family="Delphinidae" genus="Delphinus" kingdom="Animalia" order="Cetacea" pageId="152" pageNumber="153" phylum="Chordata" rank="species" species="capensis">
