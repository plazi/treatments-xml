<emphasis id="C9A473DBCCDFB9C5893F38FDA78C89C2" italics="true" pageId="null" pageNumber="845">
<taxonomicName id="4391C5B1A3336112566E530670FDADD9" class="Magnoliopsida" family="Apiaceae" genus="Pleurospermum" kingdom="Plantae" order="Apiales" pageId="null" pageNumber="845" phylum="Tracheophyta" rank="genus">Pleurospermum</taxonomicName>
