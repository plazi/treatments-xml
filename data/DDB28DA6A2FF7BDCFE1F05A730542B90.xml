<paragraph id="48F591790A5B2535BCBA604373390053" pageId="null" pageNumber="845">
Grafia 
<normalizedToken id="8BB29E782FA9485031906A06C9DA8E2D" originalValue="gólaka" pageId="null" pageNumber="845">golaka</normalizedToken>
(Hacquet) Rchb.
</paragraph>
</subSubSection>
<subSubSection id="C2BF5C5CA6E6A4227DDF760741C3A57F" pageId="null" pageNumber="845" type="reference_group">
<paragraph id="B10AC28468D5C76093624A31B1DA70B7" pageId="null" pageNumber="845">
(
<emphasis id="22EF80ECE862564851219AAE90FDE3F9" italics="true" pageId="null" pageNumber="845">
<taxonomicName id="08C1C623852EFC368E335E8B8A36F0BD" class="Magnoliopsida" family="Apiaceae" genus="Hladnikia" kingdom="Plantae" order="Apiales" pageId="null" pageNumber="845" phylum="Tracheophyta" rank="genus">Hladnikia</taxonomicName>
