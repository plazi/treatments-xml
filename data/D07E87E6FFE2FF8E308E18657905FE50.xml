<collectingCountry id="20C07660FFE7FF8E30091D817A59FE50" box="[304,350,444,467]" name="Iran" pageId="4" pageNumber="33">Iran</collectingCountry>
, 
<collectingRegion id="9A13F812FFE7FF8E30511D817AF9FE50" box="[360,510,444,467]" country="Iran" name="Fars" pageId="4" pageNumber="33">Fars province</collectingRegion>
