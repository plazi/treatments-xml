<subSubSection id="CE97657EFF89FFA5FF0E30A6FD13FF2D" box="[151,646,225,249]" pageId="21" pageNumber="22" type="type_taxon">
<paragraph id="863236F5FF89FFA5FF0E30A6FD13FF2D" blockId="21.[151,646,225,249]" box="[151,646,225,249]" pageId="21" pageNumber="22">
<emphasis id="B4F9EAE7FF89FFA5FF0E30A6FE88FF2C" bold="true" box="[151,285,225,249]" pageId="21" pageNumber="22">
<typeStatus id="59368857FF89FFA5FF0E30A6FF44FF2C" box="[151,209,225,249]" pageId="21" pageNumber="22">Type</typeStatus>
genus.
</emphasis>
<taxonomicName id="418D4D76FF89FFA5FEBD30A6FD17FF2D" authority="Jan, 1861: 191" authorityName="Jan" authorityPageNumber="191" authorityYear="1861" box="[292,642,225,248]" class="Reptilia" family="Leptotyphlopidae" genus="Tetracheilostoma" kingdom="Animalia" order="Squamata" pageId="21" pageNumber="22" phylum="Chordata" rank="genus">
<emphasis id="B4F9EAE7FF89FFA5FEBD30A6FE49FF2D" box="[292,476,225,248]" italics="true" pageId="21" pageNumber="22">Tetracheilostoma</emphasis>
<bibRefCitation id="E21C4B04FF89FFA5FE7A30A6FD17FF2D" author="Jan" box="[483,642,225,248]" firstAuthor="Jan" page="191" pageId="21" pageNumber="22" pagination="178 - 199" refId="ref25178" refString="Jan, G. (1861) Note sulla famiglia dei tiflopidi sui loro generi e sulle specie del genere Stenostoma. Archivio Per La Zoologia, L'Anatomia E La Fisiologia, 1, 178 - 199." type="journal article" year="1861">Jan, 1861: 191</bibRefCitation>
