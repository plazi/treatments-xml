<collectingCountry id="7DA476364C388601FEAAFA4EFE76FCE3" box="[357,427,1500,1524]" name="China" pageId="1" pageNumber="58">China</collectingCountry>
, 
<collectingCountry id="7DA476364C388601FE79FA4FFE24FCE3" box="[438,505,1501,1524]" name="Japan" pageId="1" pageNumber="58">Japan</collectingCountry>
, 
<collectingCountry id="7DA476364C388601FDCCFA4EFDB4FCE3" box="[515,617,1500,1524]" name="Thailand" pageId="1" pageNumber="58">Thailand</collectingCountry>
, 
<collectingCountry id="7DA476364C388601FDBAFA4EFD05FCE3" box="[629,728,1500,1524]" name="Vietnam" pageId="1" pageNumber="58">Vietnam</collectingCountry>
.
</paragraph>
</subSubSection>
