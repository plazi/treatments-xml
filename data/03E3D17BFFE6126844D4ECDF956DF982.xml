<quantity id="4CB2CD88FFE612684122E80594DBFB4B" metricMagnitude="2" metricUnit="m" metricValue="6.325" metricValueMax="10.65" metricValueMin="2.0" pageId="24" pageNumber="25" unit="m" value="632.5" valueMax="1065.0" valueMin="200.0">200– 1065 m</quantity>
