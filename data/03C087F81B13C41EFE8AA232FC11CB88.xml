<taxonomicName id="4C694D6D1B13C41EFE52A34AFCC7CB88" authority="(Fr.) Kar." authorityName="(Fr.) Kar." box="[464,816,876,902]" class="Agaricomycetes" family="Fomitopsidaceae" genus="Fomitopsis" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="52" phylum="Basidiomycota" rank="species" species="pinicola">
