<taxonomicName id="4C694D6D1B13C41EFDDEA362FD0CCB53" box="[604,763,836,861]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="52" phylum="Tracheophyta" rank="species" species="strobus">
<emphasis id="B91DEAFC1B13C41EFDDEA362FD0CCB53" box="[604,763,836,861]" italics="true" pageId="7" pageNumber="52">Pinus strobus</emphasis>
</taxonomicName>
