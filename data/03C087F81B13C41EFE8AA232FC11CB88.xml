</taxonomicName>
, 
<taxonomicName id="4C694D6D1B13C41EFE8AA362FE21CB53" box="[264,470,836,861]" class="Pinopsida" family="Pinaceae" genus="Tsuga" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="52" phylum="Tracheophyta" rank="species" species="canadensis">
