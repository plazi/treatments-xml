), Jinja - Rossel Is[land], M.B.P. [Milne Bay Province], 
<collectingCountry id="F3248CB7FFEDFFBCFAD1725CBAA3FEF8" box="[1377,1437,261,287]" name="Papua New Guinea" pageId="7" pageNumber="62">PNG</collectingCountry>
[
<collectingCountry id="F3248CB7FFEDFFBCFF2D7273BEB7FEA3" box="[157,393,298,324]" name="Papua New Guinea" pageId="7" pageNumber="62">Papua New Guinea</collectingCountry>
], Oct[ober] 
<number id="3DFBDE00FFEDFFBCFDA47273BD71FEA3" box="[532,591,298,324]" pageId="7" pageNumber="62" value="1981.0">1981</number>
(labels 
<figureCitation id="1308D0A2FFEDFFBCFD1C7273BDC5FEA3" box="[684,763,298,324]" captionStart="FIGURES 28 – 31" captionStartId="7.[151,269,1906,1930]" captionTargetBox="[165,1421,392,1859]" captionTargetId="figure@7.[151,1436,381,1885]" captionTargetPageId="7" captionText="FIGURES 28 – 31. Metataenia (Parachysodema) rothschildi, UN 1207 specimen. Fig. 28, labels; Fig. 29, lateral aspect; Fig. 30, dorsal aspect; Fig. 31, ventral aspect." httpUri="https://zenodo.org/record/293327/files/figure.png" pageId="7" pageNumber="62">
