<typeStatus id="D76D39AAFFB37E756F29EAC7FB99FE14" box="[1082,1132,472,498]" pageId="7" pageNumber="8">type</typeStatus>
host plant, 
<taxonomicName id="CFD6FC8BFFB37E756FE5EAC7FACBFE17" box="[1270,1342,472,497]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="argyri">
<emphasis id="3AA25B1AFFB37E756FE5EAC7FACBFE17" box="[1270,1342,472,497]" italics="true" pageId="7" pageNumber="8">argyri</emphasis>
