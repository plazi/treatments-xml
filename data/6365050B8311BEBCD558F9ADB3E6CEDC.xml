<pageBreakToken id="51D210FCC4561226A43F16EA4CE9075C" pageId="60" pageNumber="61" start="start">Genus</pageBreakToken>
<taxonomicName id="C3AAE9DAECA1C9BC1950E821EAC64D96" LSID="urn:lsid:zoobank.org:act:173E8950-B09A-45E4-8463-5688C6C26438" class="Arachnida" family="Selenopidae" genus="Makdiops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Makdiops" order="Araneae" pageId="60" pageNumber="61" phylum="Arthropoda" rank="genus">Makdiops</taxonomicName>
<taxonomicNameLabel id="741A7FA53125854584CF47F7C2371E5C" pageId="60" pageNumber="61">gen. n.</taxonomicNameLabel>
