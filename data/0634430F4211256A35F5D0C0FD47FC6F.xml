<emphasis id="BCE92E0B4211256B35F5D0C0FEA9FF39" bold="true" box="[151,270,151,177]" italics="true" pageId="8" pageNumber="41">Patellapis</emphasis>
(
<emphasis id="BCE92E0B4211256B347DD0C0FE11FF39" bold="true" box="[287,438,151,177]" italics="true" pageId="8" pageNumber="41">Chaetalictus</emphasis>
) 
<emphasis id="BCE92E0B4211256B34A5D0C0FDF2FF39" bold="true" box="[455,597,151,177]" italics="true" pageId="8" pageNumber="41">kahuziensis</emphasis>
