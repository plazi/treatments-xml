<quantity id="FA0348EAD02BFFFCFD52FD9EFC8CFD8C" box="[702,791,577,601]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="11" pageNumber="362" unit="mm" value="2.1">2.1 mm</quantity>
