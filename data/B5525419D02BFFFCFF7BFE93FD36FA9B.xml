# 
<quantity id="FA0348EAD02BFFFCFDCCFDB9FDD1FDAB" box="[544,586,613,638]" metricMagnitude="-1" metricUnit="m" metricValue="2.286" pageId="11" pageNumber="362" unit="in" value="9.0">9 in</quantity>
