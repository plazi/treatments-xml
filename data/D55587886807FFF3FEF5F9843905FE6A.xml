<taxonomicName id="9AFC4D1D6807FFF5FD1EF97C3F73F89F" box="[892,1049,1769,1793]" class="Insecta" family="Figitidae" genus="Dilyta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="species" species="subclavata">
