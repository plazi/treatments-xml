<taxonomicName id="4C6D4D5EEEF53B91FC44F98FFB1FF99A" authority="Stock, 1967" authorityName=", Stock" authorityYear="1967" box="[897,1242,1601,1628]" class="Maxillopoda" family="Notodelphyidae" genus="Thoracodelphys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="420" pageNumber="421" phylum="Arthropoda" rank="genus">
