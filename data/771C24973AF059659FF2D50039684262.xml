<taxonomicName id="88EF66A2D8755C67ACD06C365AE73623" class="Dicotyledoneae" family="Salicaceae" genus="Populus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Salicales" pageId="4" pageNumber="131" phylum="Angiospermae" rank="genus">
