<quantity id="1F4E14F0ED012E35FDB0FA5AFCA027E3" box="[628,800,1468,1493]" metricMagnitude="-4" metricUnit="m" metricValue="6.799999999999999" metricValueMax="7.1" metricValueMin="6.5" pageId="11" pageNumber="412" unit="mm" value="0.6799999999999999" valueMax="0.71" valueMin="0.65">0.65–0.71 mm</quantity>
