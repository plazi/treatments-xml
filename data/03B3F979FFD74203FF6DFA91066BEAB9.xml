<collectingCountry id="F30D08FFFFD64203FD81FE6007ABEAB9" box="[635,702,440,463]" name="Japan" pageId="37" pageNumber="38">Japan</collectingCountry>
, Tosa Bay, 
<specimenCount id="9D1C83E6FFD64203FCC4FE60066BEAB9" box="[830,894,440,464]" pageId="37" pageNumber="38" type="generic">
