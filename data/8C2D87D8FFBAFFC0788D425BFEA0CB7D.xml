<taxonomicName id="C3844D4DFFBAFFC27CC746A9FAEDCE49" authorityName="Yasunaga &amp; Yamada &amp; Ohno" authorityYear="2020" box="[1245,1350,1779,1803]" class="Insecta" family="Microphysidae" genus="Loricula" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="99" phylum="Arthropoda" rank="species" species="mikawa">
