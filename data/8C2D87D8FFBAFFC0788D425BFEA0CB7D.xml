<taxonomicName id="C3844D4DFFBAFFC27B494695FC54CFA5" authority="Yasunaga &amp; Yamada &amp; Ohno, 2020" authorityName="Yasunaga &amp; Yamada &amp; Ohno" authorityYear="2020" box="[851,1023,1743,1767]" class="Insecta" family="Microphysidae" genus="Loricula" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="99" phylum="Arthropoda" rank="species" species="mikawa" status="sp. nov.">
<emphasis id="36F0EADCFFBAFFC27B494695FC54CFA5" box="[851,1023,1743,1767]" italics="true" pageId="1" pageNumber="99">Loricula mikawa</emphasis>
</taxonomicName>
<emphasis id="36F0EADCFFBAFFC27C1F4695FBFECFA5" bold="true" box="[1029,1109,1743,1767]" pageId="1" pageNumber="99">
