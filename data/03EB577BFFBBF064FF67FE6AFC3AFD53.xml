<specimenCount id="9D442DE4FFBBF067FF54FAACFEDEFAC2" box="[243,319,1285,1312]" pageId="13" pageNumber="12" type="generic">
<quantity id="4CBA4B88FFBBF067FF54FAACFEDAFAC2" box="[243,315,1285,1312]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="13" pageNumber="12" unit="m" value="200.0">200 m</quantity>
