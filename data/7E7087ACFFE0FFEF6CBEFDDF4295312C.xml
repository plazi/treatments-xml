<taxonomicName id="31D94D39FFE0FFEF6FCFFC1B42D736C0" authorityName="Blanchard" authorityYear="1845" box="[998,1157,988,1014]" class="Insecta" family="Cerambycidae" kingdom="Animalia" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="tribe" tribe="Acanthocinini">Acanthocinini</taxonomicName>
