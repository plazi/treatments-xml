<taxonomicName id="4C6513EED7470B04FF6FFD86FDBB6C32" authority="(Scopoli, 1763)" baseAuthorityName="Scopoli" baseAuthorityYear="1763" box="[151,594,536,563]" class="Insecta" family="Chrysididae" genus="Pseudomalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="200" phylum="Arthropoda" rank="species" species="violaceus">
