<number id="622F24EB2454F66FFC51FE97FBCF9E3F" box="[1019,1091,284,310]" pageId="3" pageNumber="56" value="13.5" valueMax="15.0" valueMin="12.0">12–15</number>
setae, R 
<number id="622F24EB2454F66FFB0DFEACFB3F9E35" box="[1191,1203,295,316]" pageId="3" pageNumber="56" value="4.0">4</number>
