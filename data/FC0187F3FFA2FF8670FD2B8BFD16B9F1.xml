<taxonomicName id="B3A84D66FFA2FF8670A42A1FFD8ABA22" authority="Prandi &amp; Grossi" authorityName="Prandi &amp; Grossi" authorityYear="2021" box="[206,621,259,285]" class="Insecta" family="Dynastidae" genus="Eupatorus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="species" species="pyros" status="new species">
