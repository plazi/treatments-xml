<paragraph id="D15B046CEE63BA7357F2805EFE9AF9E6" blockId="12.[192,339,1601,1658]" box="[192,282,1601,1625]" pageId="12" pageNumber="11">
<heading id="8A13B300EE63BA7357F2805EFE9AF9E6" bold="true" box="[192,282,1601,1625]" fontSize="10" level="2" pageId="12" pageNumber="11" reason="3">
<emphasis id="E390D87EEE63BA7357F2805EFE9AF9E6" bold="true" box="[192,282,1601,1625]" pageId="12" pageNumber="11">Adults</emphasis>
</heading>
</paragraph>
