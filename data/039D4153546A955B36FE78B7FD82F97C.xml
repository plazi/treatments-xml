Honshu: ‘Oze,’ 
<superScript id="7C415D0D546A955B37127D5AFE55FA20" attach="left" box="[379,398,1402,1416]" fontSize="6" pageId="25" pageNumber="26">[1]</superScript>
(
<typeStatus id="548F4EE7546A955B37F37D5CFE15FA3E" box="[410,462,1404,1430]" pageId="25" pageNumber="26">type</typeStatus>
locality); ‘Titibu, Hakone’ 
<superScript id="7C415D0D546A955B35637D5AFCC6FA20" attach="left" box="[778,797,1402,1416]" fontSize="6" pageId="25" pageNumber="26">[1]</superScript>
