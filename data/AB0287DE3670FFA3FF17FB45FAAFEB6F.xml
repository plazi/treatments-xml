<taxonomicName id="E4AB4D4B3670FFA3FCFAFAC5FBA0EB0F" box="[890,1031,1306,1329]" class="Diplopoda" family="Callipodidae" genus="Lysiopetalum" kingdom="Animalia" order="Callipodida" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="carinatum">
