<taxonomicName id="54DF68A6D77EFF90A412A430659F9F34" box="[578,692,1591,1616]" class="Trilobita" family="Proetidae" genus="Doublatia" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Proetida" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">
