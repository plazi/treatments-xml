<taxonomicName id="4C3673E6FFDF911DA52CFF74C5B7FF24" authority="Tippelt &amp; Schwentner, 2018" authorityName="Tippelt &amp; Schwentner" authorityYear="2018" box="[1083,1205,188,213]" class="Branchiopoda" family="Cyzicidae" genus="Eocyzicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="40" pageNumber="441" phylum="Arthropoda" rank="species" species="ubiquus" status="sp. nov.">
<emphasis id="B942D477FFDF911DA52CFF74C54EFF25" box="[1083,1100,189,212]" italics="true" pageId="40" pageNumber="441">E</emphasis>
. 
<emphasis id="B942D477FFDF911DA54BFF75C5B7FF24" box="[1116,1205,188,213]" italics="true" pageId="40" pageNumber="441">ubiquus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A271690CFFDF911DA5AAFF75C415FF25" box="[1213,1303,188,213]" pageId="40" pageNumber="441" rank="species">
<emphasis id="B942D477FFDF911DA5AAFF75C413FF24" bold="true" box="[1213,1297,188,213]" pageId="40" pageNumber="441">sp. nov</emphasis>
.
</taxonomicNameLabel>
, 
<taxonomicName id="4C3673E6FFDF911DA430FF74C49EFF24" authority="Tippelt &amp; Schwentner, 2018" authorityName="Tippelt &amp; Schwentner" authorityYear="2018" box="[1319,1436,188,213]" class="Branchiopoda" family="Cyzicidae" genus="Eocyzicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="40" pageNumber="441" phylum="Arthropoda" rank="species" species="richteri" status="sp. nov.">
<emphasis id="B942D477FFDF911DA430FF74C43AFF25" box="[1319,1336,189,212]" italics="true" pageId="40" pageNumber="441">E</emphasis>
. 
<emphasis id="B942D477FFDF911DA45FFF75C49EFF24" box="[1352,1436,188,213]" italics="true" pageId="40" pageNumber="441">richteri</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A271690CFFDF911DA180FF16C1EDFF06" box="[151,239,223,248]" pageId="40" pageNumber="441" rank="species">
<emphasis id="B942D477FFDF911DA180FF16C1EBFF09" bold="true" box="[151,233,223,248]" pageId="40" pageNumber="441">sp. nov</emphasis>
.
</taxonomicNameLabel>
, 
<taxonomicName id="4C3673E6FFDF911DA1E9FF28C0A2FF09" authority="Tippelt &amp; Schwentner, 2018" authorityName="Tippelt &amp; Schwentner" authorityYear="2018" box="[254,416,223,248]" class="Branchiopoda" family="Cyzicidae" genus="Eocyzicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="40" pageNumber="441" phylum="Arthropoda" rank="species" species="phytophilus" status="sp. nov.">
<emphasis id="B942D477FFDF911DA1E9FF28C00DFF09" box="[254,271,225,248]" italics="true" pageId="40" pageNumber="441">E</emphasis>
. 
<emphasis id="B942D477FFDF911DA00AFF16C0A2FF09" box="[285,416,223,248]" italics="true" pageId="40" pageNumber="441">phytophilus</emphasis>
</taxonomicName>
