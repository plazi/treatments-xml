(
<taxonomicName id="4C2CAEF79572FFDDFE40FD8CFE2F7F12" authorityName="Gray" authorityYear="1850" box="[416,501,548,573]" class="Gastropoda" family="Athoracophoridae" genus="Janella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="22" pageNumber="223" phylum="Mollusca" rank="genus">
