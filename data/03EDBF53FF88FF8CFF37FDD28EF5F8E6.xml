<subSubSection id="C35E5DCEFF88FF8CFF67F91F8C9AF946" pageId="112" pageNumber="113" type="etymology">
<paragraph id="8BFB0E45FF88FF8CFF67F91F8C9AF946" blockId="112.[151,1436,706,1892]" pageId="112" pageNumber="113">
<emphasis id="B930D257FF88FF8CFF67F91F8D01F91E" bold="true" box="[199,339,1666,1692]" pageId="112" pageNumber="113">Etymology.</emphasis>
The name 
<taxonomicName id="4C4475C6FF88FF8CFE7CF91E8EC7F91E" authorityName="Zannou, Moraes &amp; Oliveira" authorityYear="2008" box="[476,661,1667,1692]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="neomagdalenae">
