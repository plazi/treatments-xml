<collectingCountry id="F37D0BE92016FFFA215753E2FD4F76A3" box="[566,652,957,981]" name="Finland" pageId="14" pageNumber="1214">Finland</collectingCountry>
, 
<collectingCountry id="F37D0BE92016FFFA21F853E2FD2576A3" box="[665,742,957,981]" name="France" pageId="14" pageNumber="1214">France</collectingCountry>
, 
