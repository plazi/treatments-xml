<taxonomicName id="BCA68CC8FFE2FFA7BF72724CFE8CFD86" authorityName="Uzel" authorityYear="1895" box="[275,378,608,634]" class="Insecta" family="Phlaeothripidae" genus="Liothrips" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="351" phylum="Arthropoda" rank="genus">
