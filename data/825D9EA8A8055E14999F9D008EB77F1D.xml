<taxonomicName id="0E44E6732C45081E176F0FAF2E3C532F" genus="Pet." lsidName="Pet. rubiginosus" pageId="0" pageNumber="175" rank="species" species="rubiginosus">
<emphasis id="362A94A84CD71716747231047FBE23CF" italics="true" pageId="0" pageNumber="175">Pet. rubiginosus</emphasis>
</taxonomicName>
</th>
<th id="4A242D799E60B7CF6D9E3FF7C183105C" colspan="1" pageId="0" pageNumber="175" rowspan="1">
<taxonomicName id="EAC1962A461A2E4A1A8F7E2BFFC6C315" genus="Pet." lsidName="Pet. hechiensis" pageId="0" pageNumber="175" rank="species" species="hechiensis">
