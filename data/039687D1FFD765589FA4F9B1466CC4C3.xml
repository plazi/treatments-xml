<paragraph id="8B8036C7FFD7655B9FA4F9B1462EC24F" blockId="85.[151,619,1629,1656]" box="[151,619,1629,1656]" pageId="85" pageNumber="86">
<heading id="D0C881ABFFD7655B9FA4F9B1462EC24F" bold="true" box="[151,619,1629,1656]" fontSize="11" level="1" pageId="85" pageNumber="86" reason="1">
<taxonomicName id="4C3F4D44FFD7655B9FA4F9B1462EC24F" authority="Kamal, 1926" authorityName="Kamal" authorityYear="1926" box="[151,619,1629,1656]" class="Insecta" family="Megaspilidae" genus="Conostigmus" kingdom="Animalia" order="Hymenoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="timberlakei">
