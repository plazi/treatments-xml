<taxonomicName id="EE8C4D70FFFF3D33FEDCF92EFBD569F3" authority="Robillard &amp; Su, 2018" authorityName="Robillard &amp; Su" authorityYear="2018" box="[294,500,1753,1775]" class="Insecta" family="Gryllidae" genus="Macrobinthus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="20" pageNumber="261" phylum="Arthropoda" rank="species" species="kutini" status="n. sp.">
