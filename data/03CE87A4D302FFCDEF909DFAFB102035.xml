<taxonomicName id="4C674D31D302FFCDEB7D9C92FB4222CD" box="[1146,1263,437,462]" class="Mammalia" family="Cricetidae" genus="Sigmodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="8" pageNumber="503" phylum="Chordata" rank="species" species="toltecus">
