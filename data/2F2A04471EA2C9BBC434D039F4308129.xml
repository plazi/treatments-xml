<geoCoordinate id="6FB0BE52DAF3ADCE971A0FAB00854C49" direction="north" orientation="latitude" precision="1" value="27.1052">27.10520°N</geoCoordinate>
, 
<geoCoordinate id="D96A804C68BEC0AD3C2D9E15E043E9C7" direction="east" orientation="longitude" precision="1" value="98.7798">98.77980°E</geoCoordinate>
