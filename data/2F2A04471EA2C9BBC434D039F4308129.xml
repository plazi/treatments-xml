<geoCoordinate id="C6BB0514739D1DCB05048E76013B551B" direction="north" orientation="latitude" precision="1" value="26.56784">26.56784°N</geoCoordinate>
, 
<geoCoordinate id="B192E64EF831018CB25D0EB52991AFF8" direction="east" orientation="longitude" precision="1" value="98.90884">98.90884°E</geoCoordinate>
