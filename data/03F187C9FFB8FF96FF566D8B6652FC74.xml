<collectingCountry id="F34F764FFFA6FF96FEA66B8265D8FC3C" box="[359,426,907,933]" name="China" pageId="16" pageNumber="387">China</collectingCountry>
(
<collectingRegion id="499CF83DFFA6FF96FE786B826667FC3C" box="[441,533,907,933]" country="China" name="Yunnan" pageId="16" pageNumber="387">Yunnan</collectingRegion>
