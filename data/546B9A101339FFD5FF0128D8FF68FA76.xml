<subSubSection id="94D8788D1339FFD4FF0128D8FE33FB6C" box="[151,385,1253,1279]" pageId="2" pageNumber="329" type="nomenclature">
<paragraph id="DC7D2B061339FFD4FF0128D8FE33FB6C" blockId="2.[151,385,1253,1279]" box="[151,385,1253,1279]" pageId="2" pageNumber="329">
<heading id="87359C6A1339FFD4FF0128D8FE33FB6C" bold="true" box="[151,385,1253,1279]" fontSize="11" level="1" pageId="2" pageNumber="329" reason="1">
<emphasis id="EEB6F7141339FFD4FF0128D8FE33FB6C" bold="true" box="[151,385,1253,1279]" pageId="2" pageNumber="329">
<taxonomicName id="1BC250851339FFD4FF0128D8FE9AFB6C" authority="Frolová &amp; Ďuriš, 2018" authorityName="Frolová &amp; Ďuriš" authorityYear="2018" box="[151,296,1253,1279]" class="Malacostraca" family="Palaemonidae" genus="Madangella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="329" phylum="Arthropoda" rank="genus" status="gen. n.">
