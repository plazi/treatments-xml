<taxonomicName id="4C4F12F32149FFF3E258471CFE7AFB9A" box="[333,504,1048,1066]" class="Insecta" family="Rutelidae" genus="Anomala" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="89" phylum="Arthropoda" rank="species" species="manserimanseri">
