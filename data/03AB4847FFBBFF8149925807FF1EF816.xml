<taxonomicName id="4C0282D2FFBBFF81499258C8FE56F90C" authority="Beck, 1999" authorityName="Beck" authorityYear="1999" box="[151,425,1722,1744]" class="Insecta" family="Noctuidae" genus="Protarchanara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="168" phylum="Arthropoda" rank="genus">
