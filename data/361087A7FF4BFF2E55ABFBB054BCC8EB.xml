<emphasis id="8CCDEAA3FF4BFF2E55ABFB055113C9AF" bold="true" box="[151,377,1184,1210]" pageId="142" pageNumber="143">Material examined</emphasis>
. 
<materialsCitation id="0ED13CECFF4BFF2E54BBFB045499C9AF" ID-GBIF-Occurrence="3026466751" box="[391,1267,1184,1211]" collectionCode="MNHN" collectorName="Banc de la Torche" country="New Caledonia" elevation="300" location="New Caledonia" pageId="142" pageNumber="143" specimenCount="1" typeStatus="holotype">
<collectionCode id="D8A8AE74FF4BFF2E54BBFB0451B7C9AE" box="[391,477,1185,1211]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="142" pageNumber="143">MNHN</collectionCode>
DCl 2966, 
<typeStatus id="61028813FF4BFF2E575CFB0552AFC9AF" box="[608,709,1184,1210]" pageId="142" pageNumber="143">holotype</typeStatus>
, 
<collectingCountry id="C6AE7621FF4BFF2E57ECFB0453EAC9AF" box="[720,896,1184,1211]" name="New Caledonia" pageId="142" pageNumber="143">New Caledonia</collectingCountry>
