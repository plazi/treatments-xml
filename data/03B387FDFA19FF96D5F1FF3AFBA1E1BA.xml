<collectingCountry id="F30D767BFA19FF96D5F1FDA9FF77E185" box="[151,219,626,652]" name="Israel" pageId="17" pageNumber="18">Israel</collectingCountry>
, 
<collectingCountry id="F30D767BFA19FF96D581FDA9FEB5E185" box="[231,281,626,652]" name="Iran" pageId="17" pageNumber="18">Iran</collectingCountry>
