<collectingCountry id="F30D767BFA19FF96D673FDA9FCF0E185" box="[789,860,626,652]" name="Malta" pageId="17" pageNumber="18">Malta</collectingCountry>
, 
<collectingCountry id="F30D767BFA19FF96D60FFDA9FC7AE185" box="[873,982,626,652]" name="Morocco" pageId="17" pageNumber="18">Morocco</collectingCountry>
