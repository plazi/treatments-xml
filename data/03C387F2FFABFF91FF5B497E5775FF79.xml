<quantity id="4C929B01FFABFF9EFE0149E55443F94C" box="[461,571,1735,1760]" metricMagnitude="5" metricUnit="m" metricValue="1.609344" pageId="39" pageNumber="40" unit="mi" value="100.0">100 miles</quantity>
east of Shiraz, S. 
<collectingCountry id="F37D7674FFABFF9EFCC949E55537F94C" box="[773,847,1735,1760]" name="Iran" pageId="39" pageNumber="40">Persia</collectingCountry>
”.
