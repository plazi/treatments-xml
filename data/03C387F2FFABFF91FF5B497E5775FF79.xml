<taxonomicName id="4C6A4D67FFABFF9EFC37487A522FF8C3" authorityName="Linnaeus" authorityYear="1753" box="[1019,1111,1880,1903]" class="Magnoliopsida" family="Anacardiaceae" genus="Pistacia" kingdom="Plantae" order="Sapindales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="genus">Pistacia</taxonomicName>
