<typeStatus id="548C8854D37CD1491D06F99DCDB43192" box="[314,412,1662,1686]" pageId="5" pageNumber="65">holotype</typeStatus>
from Canal Zone, 
<taxonomicName id="4C374D75D37CD1491E4EF99DCEEF3192" box="[626,711,1662,1686]" class="Arachnida" family="Trechaleidae" genus="Enna" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="frijoles" status="sp. nov.">Frijoles</taxonomicName>
