<subSubSection id="C32D657DD37CD1491CABF99CCDB231BE" pageId="5" pageNumber="65" type="materials_examined">
<paragraph id="8B8836F6D37CD1491CABF99CCDB231BE" blockId="5.[151,1436,1662,2011]" pageId="5" pageNumber="65">
<emphasis id="B943EAE4D37CD1491CABF99CCCFE3193" bold="true" box="[151,214,1663,1687]" pageId="5" pageNumber="65">
<typeStatus id="548C8854D37CD1491CABF99CCCF83193" box="[151,208,1663,1687]" pageId="5" pageNumber="65">Type</typeStatus>
