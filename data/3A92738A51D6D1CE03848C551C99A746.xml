<taxonomicName id="B1601348252642C899FD48DE3DDBDB96" class="Dicotyledoneae" family="Clusiaceae" genus="Garcinia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Guttiferales" pageId="9" pageNumber="50" phylum="Angiospermae" rank="species" species="danckelmanniana">Garcinia danckelmanniana</taxonomicName>
