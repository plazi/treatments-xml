in 
<collectingCountry id="9BE26BFA4740FFC6C623E66CFE85FE54" box="[291,336,368,391]" name="Iran" pageId="17" pageNumber="418">Iran</collectingCountry>
(
<bibRefCitation id="8764569B4740FFC6C65EE673FDB3FE5B" author="Abd-Rabou" box="[350,614,367,392]" pageId="17" pageNumber="418" refString="Abd-Rabou, S., Ghahari, H., Huang, J. &amp; Boucek, Z. (2005) New records of aphelinid and pteromalid wasps (Hymenoptera: Chalcidoidea: Aphelinidae, Pteromalidae) from Iran. Egyptian Journal of Agricultural Research, 83, 1619 - 1623." type="journal article" year="2005">
Abd-Rabou 
<emphasis id="D181F7784740FFC6C6EAE66DFDCFFE5B" box="[490,538,367,392]" italics="true" pageId="17" pageNumber="418">et al</emphasis>
. 
<number id="553D394D4740FFC6C529E673FDB3FE5B" box="[553,614,367,392]" pageId="17" pageNumber="418" value="2005.0">2005</number>
