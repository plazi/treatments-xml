<heading id="92661056414AFFAABD81FA9E662CC12D" bold="true" box="[936,1318,1280,1304]" centered="true" fontSize="10" level="2" pageId="4" pageNumber="250" reason="2">
<taxonomicName id="0E91DCB9414AFFAABD81FA9E662CC12D" authority="Balogh, 1962" authorityName="Balogh" authorityYear="1962" box="[936,1318,1280,1304]" class="Arachnida" family="Lohmanniidae" genus="Lohmannia" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="250" phylum="Arthropoda" rank="species" species="similis">
