<taxonomicName id="6E2DBD3CCAED369098958781B2F09512" class="Insecta" family="Staphylinidae" genus="Quedius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Quedius" order="Coleoptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="genus">Quedius</taxonomicName>
