(
<typeStatus id="104E06894A17FFA0FDA76A4ADFBAF9FE" box="[578,668,1639,1661]" pageId="0" pageNumber="289">holotype</typeStatus>
<collectionCode id="A9E420EE4A17FFA0FD476A4ADFF3F9FE" box="[674,725,1639,1661]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13150" lsid="urn:lsid:biocol.org:col:13150" name="Guangxi Institute of Botany" pageId="0" pageNumber="289" type="Herbarium">IBK</collectionCode>
!, isotype 
<collectionCode id="A9E420EE4A17FFA0FCD76A4ADE40F9FE" box="[818,870,1639,1661]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13150" lsid="urn:lsid:biocol.org:col:13150" name="Guangxi Institute of Botany" pageId="0" pageNumber="289" type="Herbarium">IBK</collectionCode>
