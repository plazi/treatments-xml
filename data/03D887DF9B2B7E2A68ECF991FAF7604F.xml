. 
<taxonomicName id="4C714D4A9B2A7E2A6961FA2EFDE46149" authorityName="Fairchild" authorityYear="1951" box="[282,522,1524,1548]" class="Insecta" family="Tabanidae" genus="Leucotabanus" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="375" phylum="Arthropoda" rank="species" species="pauculus">
<emphasis id="B905EADB9B2A7E2A6961FA2EFDE46149" box="[282,522,1524,1548]" italics="true" pageId="9" pageNumber="375">Leucotabanus pauculus</emphasis>
