<taxonomicName id="B0EB7B63820C2DCDFC114E2013A99B95" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1853" class="Gastropoda" family="Bulimulidae" genus="Stenostylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="96" pageNumber="97" phylum="Mollusca" rank="species" species="meleagris">
