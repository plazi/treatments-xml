; 35, 
<taxonomicName id="F366CC51741E1E5B380731155195A68E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28542" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex peltatus Kempf" order="Hymenoptera" pageNumber="488" phylum="Arthropoda" rank="species" species="peltatus">C. peltatus</taxonomicName>
; 36, 
<taxonomicName id="3D1E0BB11331DD516EB4248BA78ACE5A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28551" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex wheeleri Forel" order="Hymenoptera" pageNumber="488" phylum="Arthropoda" rank="species" species="wheeleri">C. wheeleri</taxonomicName>
