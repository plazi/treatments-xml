<subSubSection id="C37265704047FFA407F45ADCFDCD5D9A" pageId="5" pageNumber="32" type="materials_examined">
<paragraph id="8BD736FB4047FFA407F45ADCFE825DBF" blockId="5.[151,1437,151,1581]" pageId="5" pageNumber="32">
