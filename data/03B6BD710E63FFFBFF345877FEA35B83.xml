<collectionCode id="ED0E94A20E63FFFAFED25E1DFE7E5E55" LSID="urn:lsid:biocol.org:col:34871" box="[369,450,1035,1059]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="32" pageNumber="233">USNM</collectionCode>
