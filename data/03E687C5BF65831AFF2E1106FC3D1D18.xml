(
<number id="3D8724F4BF63831AFAA01656FAEA182B" box="[1305,1358,332,356]" pageId="20" pageNumber="21" value="1.19">1.19</number>
), body whorl height 
<quantity id="4CB79B36BF63831AFE941675FE7418C8" box="[301,464,367,391]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" metricValueMax="1.66" metricValueMin="1.34" pageId="20" pageNumber="21" unit="mm" value="1.5" valueMax="1.66" valueMin="1.34">1.34–1.66 mm</quantity>
(
<number id="3D8724F4BF63831AFE591675FDB018C8" box="[480,532,367,391]" pageId="20" pageNumber="21" value="1.51">1.51</number>
), body whorl width 
<quantity id="4CB79B36BF63831AFD401675FC3918C8" box="[761,925,367,391]" metricMagnitude="-3" metricUnit="m" metricValue="1.13" metricValueMax="1.27" metricValueMin="0.99" pageId="20" pageNumber="21" unit="mm" value="1.13" valueMax="1.27" valueMin="0.99">0.99–1.27 mm</quantity>
(
<number id="3D8724F4BF63831AFC141675FC4618C8" box="[941,994,367,391]" pageId="20" pageNumber="21" value="1.12">1.12</number>
), aperture height 
<quantity id="4CB79B36BF63831AFB1E1675FAEE18C8" box="[1191,1354,367,391]" metricMagnitude="-4" metricUnit="m" metricValue="8.7" metricValueMax="9.7" metricValueMin="7.7" pageId="20" pageNumber="21" unit="mm" value="0.87" valueMax="0.97" valueMin="0.77">0.77–0.97 mm</quantity>
(
<number id="3D8724F4BF63831AFAE31675FA3418C8" box="[1370,1424,367,391]" pageId="20" pageNumber="21" value="0.87">0.87</number>
), aperture width 
<quantity id="4CB79B36BF63831AFEF9168EFE4718E3" box="[320,483,404,428]" metricMagnitude="-4" metricUnit="m" metricValue="7.35" metricValueMax="8.0" metricValueMin="6.7" pageId="20" pageNumber="21" unit="mm" value="0.735" valueMax="0.8" valueMin="0.67">0.67–0.80 mm</quantity>
(
<number id="3D8724F4BF63831AFE4B168EFD8C18E3" box="[498,552,404,428]" pageId="20" pageNumber="21" value="0.73">0.73</number>
), total number of whorls 
<number id="3D8724F4BF63831AFCFB168EFCD718E3" box="[834,883,404,428]" pageId="20" pageNumber="21" value="4.5">4.50</number>
–5.00 (
<number id="3D8724F4BF63831AFC78168EFC5118E3" box="[961,1013,404,428]" pageId="20" pageNumber="21" value="4.78">4.78</number>
) (
<collectionCode id="ED5EAE16BF63831AFBB2168EFBFA18E3" LSID="urn:lsid:biocol.org:col:34871" box="[1035,1118,404,428]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="20" pageNumber="21">USNM</collectionCode>
<number id="3D8724F4BF63831AFBDC168EFB6D18E3" box="[1125,1225,404,428]" pageId="20" pageNumber="21" value="1153672.0">1153672</number>
, 
<emphasis id="B93BEAC1BF63831AFB6D168FFB4618E3" box="[1236,1250,405,428]" italics="true" pageId="20" pageNumber="21">n</emphasis>
= 
<number id="3D8724F4BF63831AFAB9168EFABB18E3" box="[1280,1311,404,428]" pageId="20" pageNumber="21" value="10.0">10</number>
).
</paragraph>
<paragraph id="8BF036D3BF63831AFF7E16A2FC7518BB" blockId="20.[151,1436,151,1111]" pageId="20" pageNumber="21">
<emphasis id="B93BEAC1BF63831AFF7E16A2FDA7189F" bold="true" box="[199,515,439,464]" pageId="20" pageNumber="21">
