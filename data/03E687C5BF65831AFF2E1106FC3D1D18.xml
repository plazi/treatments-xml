, body whorl height 
<quantity id="4CB79B36BF63831AFBD816ADFB6D1880" box="[1121,1225,439,463]" metricMagnitude="-3" metricUnit="m" metricValue="1.52" pageId="20" pageNumber="21" unit="mm" value="1.52">1.52 mm</quantity>
, body whorl width 
<quantity id="4CB79B36BF63831AFF2E16C6FF5918BB" box="[151,253,476,500]" metricMagnitude="-3" metricUnit="m" metricValue="1.12" pageId="20" pageNumber="21" unit="mm" value="1.12">1.12 mm</quantity>
, aperture height 
<quantity id="4CB79B36BF63831AFE0116C6FDBA18BB" box="[440,542,476,500]" metricMagnitude="-4" metricUnit="m" metricValue="8.9" pageId="20" pageNumber="21" unit="mm" value="0.89">0.89 mm</quantity>
, aperture width 
