<number id="3DD0684EFFFF1802F18BFC1B12128FE2" box="[1262,1287,1002,1023]" pageId="21" pageNumber="22" value="20.0">20</number>
Post. 
<number id="3DD0684EFFFF1802F432FBE216658835" box="[343,368,1043,1064]" pageId="21" pageNumber="22" value="13.0">13</number>
<number id="3DD0684EFFFF1802F76AFBE2153D8835" box="[527,552,1043,1064]" pageId="21" pageNumber="22" value="14.0">14</number>
<number id="3DD0684EFFFF1802F7A2FBE215F58835" box="[711,736,1043,1064]" pageId="21" pageNumber="22" value="19.0">19</number>
<number id="3DD0684EFFFF1802F61AFBE2148D8835" box="[895,920,1043,1064]" pageId="21" pageNumber="22" value="19.0">19</number>
<number id="3DD0684EFFFF1802F152FBE213458835" box="[1079,1104,1043,1064]" pageId="21" pageNumber="22" value="18.0">18</number>
<number id="3DD0684EFFFF1802F18BFBE212128835" box="[1262,1287,1043,1064]" pageId="21" pageNumber="22" value="20.0">20</number>
</paragraph>
<paragraph id="8BA77A69FFFF1802F5FAFB97146B893A" blockId="21.[159,1287,1126,1188]" lastBlockId="21.[151,1436,1260,1608]" pageId="21" pageNumber="22">
Ant. 
<date id="FFA65CA9FFFF1802F432FB9715F58866" box="[343,736,1126,1147]" pageId="21" pageNumber="22" value="1918-10-14">
<number id="3DD0684EFFFF1802F432FB9716658866" box="[343,368,1126,1147]" pageId="21" pageNumber="22" value="10.0">10</number>
<number id="3DD0684EFFFF1802F76AFB97153D8866" box="[527,552,1126,1147]" pageId="21" pageNumber="22" value="14.0">14</number>
