<subSubSection id="C315653F871AFFDD5897C0E9FFE8FD6B" box="[199,1015,651,677]" pageId="78" pageNumber="79" type="etymology">
<paragraph id="8BB036B4871AFFDD5897C0E9FFE8FD6B" blockId="78.[151,1437,151,821]" box="[199,1015,651,677]" pageId="78" pageNumber="79">
<emphasis id="B97BEAA6871AFFDD5897C0E9FD51FD6B" bold="true" box="[199,334,651,677]" pageId="78" pageNumber="79">Etymology.</emphasis>
