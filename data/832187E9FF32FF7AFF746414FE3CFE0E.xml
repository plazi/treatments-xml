<taxonomicName id="CC884D7CFF32FF7BFECA6458FD7BF9CD" authority="Bush, 2017" authorityName="Bush" authorityYear="2017" box="[297,690,1624,1646]" class="Insecta" family="Philopteridae" genus="Priceiella" kingdom="Animalia" order="Phthiraptera" pageId="184" pageNumber="185" phylum="Arthropoda" rank="species" species="alliocephala" status="new species" subGenus="Thescelovora">
