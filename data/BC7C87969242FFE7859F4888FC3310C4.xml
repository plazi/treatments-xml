<taxonomicName id="F3D54D039243FFE786CF4889FBF910A7" box="[967,1099,404,428]" class="Insecta" family="Cicadellidae" genus="Balclutha" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="524" phylum="Arthropoda" rank="species" species="saltuella">
