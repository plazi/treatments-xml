<taxonomicName id="F3D54D039243FFE786114889FC3A10A7" box="[793,904,404,428]" class="Insecta" family="Cicadellidae" genus="Balclutha" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="524" phylum="Arthropoda" rank="species" species="alstoni">
