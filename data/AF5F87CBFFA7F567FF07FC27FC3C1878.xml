(
<collectionCode id="41E7AE18FFA6F567FAABFE4EFA771AF4" box="[1339,1414,440,464]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="13" pageNumber="14">ANIC</collectionCode>
