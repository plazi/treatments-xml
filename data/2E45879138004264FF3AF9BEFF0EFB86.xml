<taxonomicName id="61EC4D0438004267FF20F9CAFE70F938" authority="Harrison and Walker (1979)" authorityName="Harrison and Walker" authorityYear="1979" class="Aves" family="Anseriformes (awaiting allocation)" genus="Palaeopapia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anseriformes" pageId="9" pageNumber="10" phylum="Chordata" rank="species" species="eous">
