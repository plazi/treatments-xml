<taxonomicName id="4C0D32BCFFCDFFD1FBEE2C8222B0FC93" box="[1071,1194,764,788]" class="Insecta" family="Cicadellidae" genus="Aguriahana" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="50" phylum="Arthropoda" rank="species" species="recticornis">recticornis</taxonomicName>
