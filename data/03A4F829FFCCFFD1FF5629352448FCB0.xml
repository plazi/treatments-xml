<typeStatus id="54B6F79DFFCDFFD1FF042CEE271AFD2F" box="[197,256,656,680]" pageId="11" pageNumber="50">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54B6F79DFFCDFFD1FEBB2CF127FFFD20" box="[378,485,655,679]" pageId="11" pageNumber="50">Holotype</typeStatus>
, 
<number id="3DC55B18FFCDFFD1FE312CEE241FFD2F" box="[496,517,656,680]" pageId="11" pageNumber="50" value="3.0">3</number>
, 
<collectingCountry id="F31A09AFFFCDFFD1FDD22CF12475FD20" box="[531,623,655,679]" name="China" pageId="11" pageNumber="50">CHINA</collectingCountry>
. 
<emphasis id="B979952DFFCDFFD1FDBB2CEE255CFD2F" bold="true" box="[634,838,655,680]" pageId="11" pageNumber="50">Yunnan Province</emphasis>
: Xinzhu Botanical Garden, Lijiang, 
<taxonomicName id="4C0D32BCFFCDFFD1FB222CEE2320FD2F" box="[1251,1338,656,680]" class="Magnoliopsida" family="Juglandaceae" genus="Juglans" kingdom="Plantae" order="Fagales" pageId="11" pageNumber="50" phylum="Tracheophyta" rank="genus">
<emphasis id="B979952DFFCDFFD1FB222CEE2320FD2F" box="[1251,1338,656,680]" italics="true" pageId="11" pageNumber="50">Juglans</emphasis>
</taxonomicName>
, altitude 
<quantity id="4CF5E4DAFFCDFFD1FF562CCA26F5FD4B" box="[151,239,692,716]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="11" pageNumber="50" unit="m" value="2300.0">2300 m</quantity>
