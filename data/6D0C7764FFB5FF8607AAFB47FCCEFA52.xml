<taxonomicName id="22A5BDF1FFB5FF86039DFAD7FAA3FA89" authorityName="Gabriel" authorityYear="2014" box="[1184,1294,1331,1357]" class="Arachnida" family="Theraphosidae" genus="Longilyra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
<emphasis id="D7D11A60FFB5FF86039DFAD7FAA3FA89" box="[1184,1294,1331,1357]" italics="true" pageId="6" pageNumber="7">Longilyra</emphasis>
