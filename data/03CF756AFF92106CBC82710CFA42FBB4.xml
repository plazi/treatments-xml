<quantity id="4C9E6999FF90106CBF4476B7FC53FE2B" box="[849,906,404,428]" metricMagnitude="1" metricUnit="m" metricValue="7.0" pageId="3" pageNumber="194" unit="m" value="70.0">70 m</quantity>
a.s.l.
</elevation>
, 
<geoCoordinate id="EE52A2BBFF90106CBFC676B7FBBFFE2B" box="[979,1126,404,429]" direction="south" orientation="latitude" pageId="3" pageNumber="194" precision="15" value="-22.958055">22Â° 57' 29'' S</geoCoordinate>
, 
<geoCoordinate id="EE52A2BBFF90106CB87F76B7FAD5FE2B" box="[1130,1292,404,429]" direction="west" orientation="longitude" pageId="3" pageNumber="194" precision="15" value="-42.01472">42Â° 00' 53'' W</geoCoordinate>
