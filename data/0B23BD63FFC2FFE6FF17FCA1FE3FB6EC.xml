<taxonomicName id="448A77F6FFC2FFE6FD36FC4DFD50B1B7" box="[694,756,942,965]" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="17" pageNumber="168" phylum="Tracheophyta" rank="genus">
