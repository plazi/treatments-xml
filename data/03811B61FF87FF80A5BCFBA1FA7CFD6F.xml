<quantity id="4CD00792FF87FF80A6AFF926FC2EFDA6" box="[900,986,606,631]" metricMagnitude="3" metricUnit="m" metricValue="2.761" pageId="14" pageNumber="355" unit="m" value="2761.0">2761 m</quantity>
