<taxonomicName id="4C28D1F4FF87FF80A17AF926FB0DFDA6" box="[1105,1273,606,631]" class="Gastropoda" family="Limapontiidae" genus="Olea" kingdom="Animalia" order="Sacoglossa" pageId="14" pageNumber="355" phylum="Mollusca" rank="species" species="europaea">
<emphasis id="B95C7665FF87FF80A17AF926FB0DFDA6" box="[1105,1273,606,631]" italics="true" pageId="14" pageNumber="355">Olea europaea</emphasis>
</taxonomicName>
community at 
<quantity id="4CD00792FF87FF80A5BCF9FAFF00FD4A" box="[151,244,642,667]" metricMagnitude="3" metricUnit="m" metricValue="2.285" pageId="14" pageNumber="355" unit="m" value="2285.0">2285 m</quantity>
