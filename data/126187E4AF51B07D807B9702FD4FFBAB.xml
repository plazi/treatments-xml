<taxonomicName id="5DC84D71AF52B07D80EC91C7FDCCFBF8" box="[415,524,1004,1029]" class="Magnoliopsida" family="Malvaceae" genus="Byttneria" kingdom="Plantae" order="Malvales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="genus">
