<taxonomicName id="5F5592F2074172DB588BD1F07A0DCDB2" class="Dicotyledoneae" family="Campanulaceae" genus="Campanula" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Campanula stricta" order="Campanulales" pageId="4" pageNumber="87" phylum="Angiospermae" rank="species" species="stricta">
