<paragraph id="6379E347FFA1FF88EF99F8D932ECF81F" blockId="19.[189,1400,1855,2022]" pageId="19" pageNumber="68">
<taxonomicName id="A4C698C4FFA1FF88EF99F8D932E6F8A0" authorityName="Nutting" authorityYear="1904" box="[189,394,1855,1881]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="19" pageNumber="68" phylum="Cnidaria" rank="species" species="areyi">
