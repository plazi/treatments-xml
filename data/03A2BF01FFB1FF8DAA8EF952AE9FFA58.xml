species.
</emphasis>
<taxonomicName id="4C0B7594FFB1FF8CAB20F8C6ACCEF8A0" authority="(Eggers, 1927)" authorityName="Cognato &amp; Smith &amp; Beaver" authorityYear="2020" baseAuthorityName="Eggers" baseAuthorityYear="1927" box="[313,682,1824,1851]" class="Insecta" family="Curculionidae" genus="Fraudatrix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="544" phylum="Arthropoda" rank="species" species="melas">
