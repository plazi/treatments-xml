<taxonomicName id="B2F878FE154A2540961CE687A49366BD" class="Insecta" family="Staphylinidae" genus="Sinotrisus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinotrisus nomurai" order="Coleoptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="species" species="nomurai">Sinotrisus nomurai</taxonomicName>
