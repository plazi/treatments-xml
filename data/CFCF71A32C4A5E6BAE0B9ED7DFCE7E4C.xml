</paragraph>
<paragraph id="B806797D1874461AC358FA1AEF59D8AB" pageId="754" pageNumber="755">
<taxonomicName id="1EC4DDE2D873DE2A119EC324D1ABA7E0" authorityName="Casey" authorityYear="1920" class="Insecta" family="Carabidae" genus="Taphranchus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="754" pageNumber="755" phylum="Arthropoda" rank="genus">Taphranchus</taxonomicName>
