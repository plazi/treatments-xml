<taxonomicName id="2A28426D97A3878223A9478FA09B8B78" authorityName="Bonelli" authorityYear="1810" class="Hexapoda" family="Carabidae" genus="Agonum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="754" pageNumber="755" phylum="Arthropoda" rank="genus">
