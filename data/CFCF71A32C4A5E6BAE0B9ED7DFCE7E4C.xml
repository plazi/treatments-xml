<taxonomicName id="9F890D3DF73EDAF4508FD27B674320B0" authorityName="Bonelli" authorityYear="1810" class="Hexapoda" family="Carabidae" genus="Agonum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="754" pageNumber="755" phylum="Arthropoda" rank="genus">
