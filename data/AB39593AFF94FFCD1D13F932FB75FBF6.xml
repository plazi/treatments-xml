<collectingCountry id="5B87A8BCFF94FFCD18BFFBD5FAD3FC7E" box="[1188,1318,897,923]" name="Guatemala" pageId="20" pageNumber="21">Guatemala</collectingCountry>
, 
<collectingCountry id="5B87A8BCFF94FFCD1D13FBFDFE88FC26" box="[264,381,937,963]" name="Honduras" pageId="20" pageNumber="21">Honduras</collectingCountry>
, 
<collectingCountry id="5B87A8BCFF94FFCD1D8AFBFDFE07FC26" box="[401,498,937,963]" name="Jamaica" pageId="20" pageNumber="21">Jamaica</collectingCountry>
