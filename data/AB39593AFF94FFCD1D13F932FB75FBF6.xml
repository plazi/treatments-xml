<collectingCountry id="5B87A8BCFF94FFCD1E1CFBFDFD79FC26" box="[519,652,937,963]" name="Martinique" pageId="20" pageNumber="21">Martinique</collectingCountry>
, 
<collectingCountry id="5B87A8BCFF94FFCD1EBBFBFDFD09FC26" box="[672,764,937,963]" name="Mexico" pageId="20" pageNumber="21">Mexico</collectingCountry>
, 
<collectingCountry id="5B87A8BCFF94FFCD1F0AFBFDFC78FC26" box="[785,909,937,963]" name="Nicaragua" pageId="20" pageNumber="21">Nicaragua</collectingCountry>
