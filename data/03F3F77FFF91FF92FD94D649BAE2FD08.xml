<paragraph id="8BE54669FF8EFF92FDB4D202BAE2FD08" blockId="16.[533,1536,208,682]" pageId="16" pageNumber="17">
<taxonomicName id="4C5A3DEAFF8EFF92FDB4D202BDD5FD25" baseAuthorityName="K. Andersen" baseAuthorityYear="1912" box="[565,670,624,648]" class="Mammalia" family="Nycteridae" genus="Nycteris" kingdom="Animalia" order="Chiroptera" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="tragata">
<emphasis id="B92E9A7BFF8EFF92FDB4D202BD03FD25" box="[565,584,624,647]" italics="true" pageId="16" pageNumber="17">N</emphasis>
