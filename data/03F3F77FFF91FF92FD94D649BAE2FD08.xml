<taxonomicName id="4C5A3DEAFF91FF8DFBE0D75DBB9DF8E4" authorityName="E. Geoffroy" authorityYear="1813" box="[1121,1238,1838,1862]" class="Mammalia" family="Nycteridae" genus="Nycteris" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="javanica">
