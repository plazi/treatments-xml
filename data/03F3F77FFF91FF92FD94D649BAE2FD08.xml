<taxonomicName id="4C5A3DEAFF91FF8DFB80D75DBB1EF8E4" authorityName="E. Geoffroy &amp; G. Cuvier" authorityYear="1795" box="[1025,1109,1839,1862]" class="Mammalia" family="Nycteridae" genus="Nycteris" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="16" phylum="Chordata" rank="genus">
