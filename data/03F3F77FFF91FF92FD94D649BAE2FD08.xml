<taxonomicName id="4C5A3DEAFF91FF8DFC61D723BB1EF8CA" authority="in Peninsular" authorityName="E. Geoffroy" authorityYear="1813" box="[992,1109,1873,1897]" class="Mammalia" family="Nycteridae" genus="Nycteris" kingdom="Animalia" order="Chiroptera" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="javanica">
