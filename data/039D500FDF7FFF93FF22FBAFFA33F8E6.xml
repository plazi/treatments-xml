<taxonomicName id="4C349A9ADF7FFF93FB2DF9ACFAB0F9B6" box="[1176,1300,1637,1658]" class="Insecta" family="Leptophlebiidae" genus="Choroterpes" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="383" phylum="Arthropoda" rank="genus">
