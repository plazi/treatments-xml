<taxonomicName id="4C349A9ADF7FFF93FAF9F913FA3EF923" box="[1356,1434,1754,1775]" class="Insecta" family="Leptophlebiidae" genus="Indialis" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="383" phylum="Arthropoda" rank="genus">
