<taxonomicName id="4C349A9ADF7FFF93FAD9FA89FA30FA99" box="[1388,1428,1344,1365]" class="Insecta" family="Leptophlebiidae" genus="Isca" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="383" phylum="Arthropoda" rank="genus">
