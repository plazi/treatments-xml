(
<taxonomicName id="4C349A9ADF7FFF93FA96F9ACFA2AF9B6" box="[1315,1422,1637,1658]" class="Insecta" family="Leptophlebiidae" genus="Euthraulus" kingdom="Animalia" order="Ephemeroptera" pageId="2" pageNumber="383" phylum="Arthropoda" rank="genus">
