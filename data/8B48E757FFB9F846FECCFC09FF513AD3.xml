species: 
<taxonomicName id="C4E12DC2FFB9F846FECAFBBFFF36381F" authorityName="Criscione &amp; Hallan &amp; Puillandre &amp; Fedosov" authorityYear="2021" box="[301,543,1070,1092]" class="Gastropoda" family="Raphitomidae" genus="Globodaphne" kingdom="Animalia" order="Neogastropoda" pageId="28" pageNumber="989" phylum="Mollusca" rank="species" species="pomum">Globodaphne pomum</taxonomicName>
