<taxonomicName id="4C714D7A3C6FB76BFF4CFF6975BEFF2C" authority="Puthz, 2012" authorityName="Puthz" authorityYear="2012" box="[178,497,216,238]" class="Insecta" family="Staphylinidae" genus="Megalopinus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="649" phylum="Arthropoda" rank="species" species="nevermannianus" status="nov.sp.">
<emphasis id="B905EAEB3C6FB76BFF4CFF6975BEFF2C" bold="true" box="[178,497,216,238]" italics="true" pageId="36" pageNumber="649">Megalopinus nevermannianus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23657903C6FB76BFE08FF69760CFF2C" box="[502,579,216,238]" pageId="36" pageNumber="649" rank="species">nov.sp.</taxonomicNameLabel>
