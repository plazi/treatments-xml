<paragraph id="8D1F0FD2F84BAB046F5327497BAC27D8" pageId="79" pageNumber="80">
<taxonomicName id="BE61A5FC4949D19063F9FC3214BFBBC8" genus="Trigonodactyliens" lsidName="Trigonodactyliens" pageId="79" pageNumber="80" rank="genus">Trigonodactyliens</taxonomicName>
<normalizedToken id="DFE916431BA524BFA21E318E5CB83693" originalValue="Brullé">Brulle</normalizedToken>
, 1834: 127 [stem: Trigonodactyl-]. Type genus: 
<taxonomicName id="C5E092B09457CDF01A7358EB2D6603F8" authority="Dejean, 1831" authorityName="Dejean" authorityYear="1831" class="Insecta" family="Carabidae" genus="Trigonodactyla" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trigonodactyla" order="Coleoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="genus">Trigonodactyla</taxonomicName>
