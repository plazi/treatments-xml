<taxonomicName id="CD78FB219783BC933AE9CDA88D5A8994" authority="Narendran, 2003" authorityName="Narendran" authorityYear="2003" class="Insecta" family="Chalcididae" genus="Heydoniella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heydoniella" order="Hymenoptera" pageId="97" pageNumber="98" phylum="Arthropoda" rank="genus">Heydoniella Narendran, 2003</taxonomicName>
