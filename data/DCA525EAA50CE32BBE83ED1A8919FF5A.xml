<taxonomicName id="BEC5D1F7D907CA40BBB7BB54F43F350F" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" class="Insecta" family="Ypsolophidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ypsolopha straminella" order="Lepidoptera" pageId="3" pageNumber="28" phylum="Arthropoda" rank="species" species="straminella">Ypsolopha straminella</taxonomicName>
