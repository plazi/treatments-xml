<taxonomicName id="05C971B28C9696496BAC7DFE7AFB0874" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" class="Insecta" family="Ypsolophidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ypsolopha" order="Lepidoptera" pageId="3" pageNumber="28" phylum="Arthropoda" rank="genus">Ypsolopha</taxonomicName>
