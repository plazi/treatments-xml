<normalizedToken id="C14EE981439C7CD01BD060F6C7C84EE6" originalValue="a–f">a-f</normalizedToken>
<taxonomicName id="1E4E67B3AF7EBAA55EAD0D6BA6795185" authority="Latreille, 1796" authorityName="Latreille" authorityYear="1796" class="Insecta" family="Ypsolophidae" genus="Ypsolopha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ypsolopha asperella" order="Lepidoptera" pageId="3" pageNumber="28" phylum="Arthropoda" rank="species" species="asperella">Ypsolopha asperella</taxonomicName>
