L. and 
<taxonomicName id="9DC850D6FFC32A584BB3A0CAFC7CFB1A" box="[767,897,1195,1220]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="species" species="pratensis">
