<typeStatus id="54A0E5D00A72460F2E46FC65FD94D344" box="[557,607,1003,1027]" pageId="93" pageNumber="94">Type</typeStatus>
species: 
<taxonomicName id="4C1B20F10A72460F2ED4FC64FBE0D344" authority="Hempel" authorityName="Hempel" authorityYear="1934" box="[703,1067,1002,1027]" class="Insecta" family="Eriococcidae" genus="Pseudotectococcus" kingdom="Animalia" order="Hemiptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="anonae">
