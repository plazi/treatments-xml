<location id="8EA9FFB9FF8EFFCBFB5B87C1FAB2F9D3" LSID="urn:lsid:plazi:treatment:03DF1874FF8EFFCAFF508793FB29FE98:8EA9FFB9FF8EFFCBFB5B87C1FAB2F9D3" box="[1180,1371,1520,1543]" country="Papua New Guinea" municipality="King" name="Goodenough Bay" pageId="4" pageNumber="56">Goodenough Bay</location>
