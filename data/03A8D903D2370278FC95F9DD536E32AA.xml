Basonym: 
<taxonomicName id="4C011396D2370279FC06F8F654B53404" authority="Vancanneyt et al. 2006" authorityName="Vancanneyt" authorityYear="2006" box="[943,1463,1794,1819]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="56" pageNumber="2838" phylum="Firmicutes" rank="species" species="parabrevis">
