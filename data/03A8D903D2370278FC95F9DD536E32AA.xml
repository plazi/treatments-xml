<taxonomicName id="4C011396D2370279FC95F94252AB35D0" box="[828,937,1718,1742]" class="Bacilli" family="Lactobacillaceae" genus="Levilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="56" pageNumber="2838" phylum="Firmicutes" rank="species" species="parabrevis">
