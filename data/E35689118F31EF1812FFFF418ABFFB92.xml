<number id="DD372A208F32EF18131DFC9D8BE7FCDE" box="[373,446,836,860]" pageId="26" pageNumber="27" value="24.5" valueMax="28.0" valueMin="21.0">21â€“28</number>
.x. 
<number id="DD372A208F32EF1813BCFC9D884BFCDE" box="[468,530,836,860]" pageId="26" pageNumber="27" value="2007.0">2007</number>
</date>
, T 
<number id="DD372A208F32EF181046FC9D883FFCDF" box="[558,614,836,861]" pageId="26" pageNumber="27" value="3176.0">3176</number>
[
<collectionCode id="0DEEA0C28F32EF18101CFC9D88E7FCDE" LSID="urn:lsid:biocol.org:col:34797" box="[628,702,836,860]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="26" pageNumber="27">INHS</collectionCode>
