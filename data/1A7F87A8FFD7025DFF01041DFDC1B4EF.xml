<emphasis id="A0A2EAACFFD60258FF01000BFE86B176" bold="true" box="[151,268,1800,1822]" pageId="11" pageNumber="12">FIGURE 4</emphasis>
. 
<taxonomicName id="55D64D3DFFD60258FE8F000AFE73B175" authority="Mironov &amp; Galloway, 2019" authorityName="Mironov &amp; Galloway" authorityYear="2019" box="[281,505,1800,1822]" class="Arachnida" family="Trouessartiidae" genus="Trouessartia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="bochkovi" status="sp. n.">
<emphasis id="A0A2EAACFFD60258FE8F000AFE73B175" box="[281,505,1800,1822]" italics="true" pageId="11" pageNumber="12">Trouessartia bochkovi</emphasis>
</taxonomicName>
<emphasis id="A0A2EAACFFD60258FD96000BFDB3B176" bold="true" box="[512,569,1800,1821]" pageId="11" pageNumber="12">
