<taxonomicName id="7CE6EDFCC14EFFAB1C8BF06BFC9AFED5" authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" box="[732,826,348,374]" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="tintin" status="sp. nov.">
