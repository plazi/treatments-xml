<quantity id="4C8F9B27E626FFB7FD6C114AFCBBFABB" box="[736,812,1348,1374]" metricMagnitude="2" metricUnit="m" metricValue="7.1" pageId="11" pageNumber="12" unit="m" value="710.0">710 m</quantity>
