<quantity id="4C8F9B27E626FFB7FC6E114AFBBAFABB" box="[994,1069,1348,1374]" metricMagnitude="2" metricUnit="m" metricValue="2.5" pageId="11" pageNumber="12" unit="m" value="250.0">250 m</quantity>
