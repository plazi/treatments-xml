<taxonomicName id="4C4ACD8EFB18FFE0FEC0A1DBFDE6F853" authority="Namayandeh &amp; Hudson, 2022" authorityName="Namayandeh &amp; Hudson" authorityYear="2022" box="[281,554,1975,1999]" class="Insecta" family="Chironomidae" genus="Parakiefferiella" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="126" phylum="Arthropoda" rank="species" species="mishigami" status="sp. nov.">
<emphasis id="B93E6A1FFB18FFE0FEC0A1DBFDE6F853" box="[281,554,1975,1999]" italics="true" pageId="6" pageNumber="126">Parakiefferiella mishigami</emphasis>
</taxonomicName>
<emphasis id="B93E6A1FFB18FFE0FDE9A1D4FD4DF84C" bold="true" box="[560,641,1976,2000]" pageId="6" pageNumber="126">
