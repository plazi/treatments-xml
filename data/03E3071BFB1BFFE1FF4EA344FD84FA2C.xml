<quantity id="4CB21BE8FB1BFFE3FE8DA091FE61F884" box="[340,429,1789,1816]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="5" pageNumber="125" unit="mm" value="2.9">2.9 mm</quantity>
. Wing 
<quantity id="4CB21BE8FB1BFFE3FE22A091FD9DF884" box="[507,593,1789,1816]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="5" pageNumber="125" unit="mm" value="2.1">2.1 mm</quantity>
long and 
<quantity id="4CB21BE8FB1BFFE3FD19A091FCE8F884" box="[704,804,1789,1816]" metricMagnitude="-4" metricUnit="m" metricValue="5.1" pageId="5" pageNumber="125" unit="mm" value="0.51">0.51 mm</quantity>
