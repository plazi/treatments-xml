<taxonomicName id="6ABC4D2C5347FFDCFEB00FCA29CEFC3D" authorityName="Meyrick" authorityYear="1894" box="[362,466,970,996]" class="Insecta" family="Lecithoceridae" genus="Torodora" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="87" phylum="Arthropoda" rank="genus">
