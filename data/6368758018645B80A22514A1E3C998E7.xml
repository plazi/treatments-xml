<paragraph id="4395E9EA7178E922882A35215AFB0355" pageId="0" pageNumber="107">Female unknown.</paragraph>
</subSubSection>
<subSubSection id="1C42D61A5749AB2E69FBF062EF545345" pageId="0" pageNumber="107" type="type material">
<paragraph id="93A3BA0DA349D993DB0C401CE6C01223" pageId="0" pageNumber="107">Type material.</paragraph>
