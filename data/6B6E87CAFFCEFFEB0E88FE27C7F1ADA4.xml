<taxonomicName id="24C74D5FFFCEFFEB0DB9FA94C23EAE1D" authority="Turner" authorityName="Turner" authorityYear="1918" box="[752,1095,1376,1401]" class="Insecta" family="Crabronidae" genus="Tachytes" kingdom="Animalia" order="Hymenoptera" pageId="28" pageNumber="1725" phylum="Arthropoda" rank="species" species="diversicornis">
