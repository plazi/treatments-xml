<taxonomicName id="D74720DEA32403407C2EE746829F276B" baseAuthorityName="Broderip" baseAuthorityYear="1832" class="Gastropoda" family="Bulimulidae" genus="Scutalus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="129" pageNumber="130" phylum="Mollusca" rank="species" species="proteus">
