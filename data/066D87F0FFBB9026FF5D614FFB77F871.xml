<taxonomicName id="49C44D65FFBB9026FB4161CFFA2DF828" box="[1163,1436,1940,1964]" class="Insecta" family="Phoridae" genus="Coniceromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="angularis">
