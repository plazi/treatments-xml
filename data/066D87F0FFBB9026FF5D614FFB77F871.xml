<collectionCode id="E8D5AE23FFBB9026FE0361E3FDA3F855" box="[457,530,1977,2001]" country="Brazil" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="6" pageNumber="7">MZSP</collectionCode>
