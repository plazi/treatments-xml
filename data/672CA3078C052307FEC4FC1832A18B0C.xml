<subSubSection id="A79F419A8C052307FEF4FA2832A18B0C" lastPageId="2" lastPageNumber="41" pageId="1" pageNumber="40" type="description">
<paragraph id="EF3A12118C052304FEF4FA2833888AEC" blockId="1.[264,1323,1012,1438]" box="[312,681,1412,1438]" pageId="1" pageNumber="40">
<emphasis id="DDF1CE038C052304FEF4FA2830DA8AEC" bold="true" box="[312,507,1412,1438]" pageId="1" pageNumber="40">Site of infection</emphasis>
