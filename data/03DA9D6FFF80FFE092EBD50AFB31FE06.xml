<emphasis id="B907F06BFF86FFE192EBD793FEDFF870" bold="true" box="[151,286,1956,1978]" pageId="20" pageNumber="307">FIGURE 11.</emphasis>
<taxonomicName id="4C7357FAFF86FFE19358D793FD52F873" authority="(Lefebvre, 1842)" baseAuthorityName="Lefebvre" baseAuthorityYear="1842" box="[292,659,1956,1978]" class="Insecta" family="Ascalaphidae" genus="Libelloides" kingdom="Animalia" order="Neuroptera" pageId="20" pageNumber="307" phylum="Arthropoda" rank="species" species="latinus">
