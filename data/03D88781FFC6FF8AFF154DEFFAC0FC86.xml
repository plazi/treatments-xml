<collectingCountry id="F3667607FFC0FF8AFD7B4CD0FCCDFD6D" box="[761,835,654,680]" name="China" pageId="12" pageNumber="13">China</collectingCountry>
: from 
<collectingRegion id="49B5F875FFC0FF8AFC0D4CD0FBCBFD6D" box="[911,1093,654,680]" country="China" name="Hebei" pageId="12" pageNumber="13">Hebei Province</collectingRegion>
