<taxonomicName id="4C714D14FFC0FF8AFB8D4FF9FB2FFE05" authorityName="Gao, Gao &amp; Zhu" authorityYear="2002" box="[1039,1185,423,448]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="lingulatus">
<emphasis id="B905EA85FFC0FF8AFB8D4FF9FB2FFE05" box="[1039,1185,423,448]" italics="true" pageId="12" pageNumber="13">P. lingulatus</emphasis>
