<collectingRegion id="3F3C40B57B5EFFF9FF02E9B491D3FE4F" box="[140,254,413,439]" country="United States of America" name="California" pageId="47" pageNumber="48">California</collectingRegion>
and in the mountains of 
<collectingRegion id="3F3C40B57B5EFFF9FDACE9B4935AFE4F" box="[546,631,413,439]" country="United States of America" name="Nevada" pageId="47" pageNumber="48">Nevada</collectingRegion>
and 
<collectingRegion id="3F3C40B57B5EFFF9FD3FE9B49232FE4F" box="[689,799,413,439]" country="United States of America" name="Colorado" pageId="47" pageNumber="48">Colorado</collectingRegion>
