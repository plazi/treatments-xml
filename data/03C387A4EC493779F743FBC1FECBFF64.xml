<quantity id="4C929B57EC473778F67AFB8FFC1FFB95" box="[823,897,1047,1071]" metricMagnitude="2" metricUnit="m" metricValue="3.41" pageId="24" pageNumber="789" unit="m" value="341.0">341 m</quantity>
