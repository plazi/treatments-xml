, 
<taxonomicName id="249640F8B019C1F628D8548BF0F3BB87" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1855" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="38" pageNumber="39" phylum="Mollusca" rank="species" species="columbiensis" subGenus="Mesembrinus">
