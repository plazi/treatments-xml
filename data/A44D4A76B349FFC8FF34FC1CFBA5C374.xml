<number id="9A2CE947B348FFC8FE8CFD8FFE84C5CE" box="[303,373,622,647]" pageId="21" pageNumber="22" value="82.5" valueMax="85.0" valueMin="80.0">80–85</number>
µm, dark brown; 
<number id="9A2CE947B348FFC8FDEBFD8FFD83C5CE" box="[584,626,622,647]" pageId="21" pageNumber="22" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
