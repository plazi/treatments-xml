<taxonomicName id="5AC5A0A51F30A469FF0B8D8AFD30578E" authority="(ALEXANDER, 1913)" baseAuthorityName="Alexander" baseAuthorityYear="1913" box="[248,693,1506,1531]" class="Insecta" family="Cylindrotomidae" genus="Triogma" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="1184" phylum="Arthropoda" rank="species" species="kuwanai">
<emphasis id="AFB107341F30A469FF0B8D8AFE4A578F" box="[248,463,1507,1530]" italics="true" pageId="28" pageNumber="1184">TRIOGMA KUWANAI</emphasis>
