<collectionCode id="C4C264AA352AFFA815FCFB0FFE1EFB23" box="[430,469,1197,1223]" country="Sweden" lsid="urn:lsid:biocol.org:col:13421" name="Katedralskolan" pageId="7" pageNumber="8" type="Herbarium">SK</collectionCode>
