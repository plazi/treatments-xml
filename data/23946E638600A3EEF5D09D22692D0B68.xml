<paragraph id="A65252B277B23A3A34E35BC03FA6E859" pageId="0" pageNumber="85">
<taxonomicName id="2E2E4D06E0DC92678EE6B013E665170C" class="Arachnida" family="Salticidae" genus="Agorioides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Agorioides cherubino" order="Araneae" pageId="0" pageNumber="85" phylum="Arthropoda" rank="species" species="cherubino">Agorioides cherubino</taxonomicName>
