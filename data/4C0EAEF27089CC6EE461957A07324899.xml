<taxonomicName id="8ACCD32645FBEE600BCCE4093A6AE03F" class="Insecta" family="Torymidae" genus="Torymus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Torymus" order="Hymenoptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="genus">Torymus</taxonomicName>
