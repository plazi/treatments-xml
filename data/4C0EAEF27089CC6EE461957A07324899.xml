<paragraph id="3159364355DD548552FD087C271225D6" pageId="0" pageNumber="8013">
<taxonomicName id="00CC1C62A6254FF6DE9475967A6EFE04" authority="" authorityName="" class="Insecta" family="Aphididae" genus="P." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="P. medicaginis" order="Hemiptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="species" species="medicaginis">P. medicaginis</taxonomicName>
