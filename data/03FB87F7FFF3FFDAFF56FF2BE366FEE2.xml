<taxonomicName id="4C524D62FFF3FFDAFD75FE98E3CAFEBF" authority="Juss." authorityName="Juss." box="[692,930,292,317]" class="Magnoliopsida" family="Polemoniaceae" kingdom="Plantae" order="Ericales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="family">Polemoniaceae Juss.</taxonomicName>
) (UCDavis Nemabase 
<number id="3D9A24C6FFF3FFDAFB72FE99E485FEBF" box="[1203,1261,293,317]" pageId="6" pageNumber="213" value="2010.0">2010</number>
) and 
<taxonomicName id="4C524D62FFF3FFDAFAF5FE98E090FEE2" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="species" species="pratense">
<emphasis id="B926EAF3FFF3FFDAFAF5FE98E090FEE2" italics="true" pageId="6" pageNumber="213">Trifolium pratense</emphasis>
