<paragraph id="8BED36E1FFF3FFDAFF56FF00E366FEE2" blockId="6.[151,1436,151,352]" pageId="6" pageNumber="213">
<taxonomicName id="4C524D62FFF3FFDAFF56FF00E14BFF56" box="[151,291,187,212]" class="Secernentea" family="Aphelenchoididae" genus="Aphelenchoides" kingdom="Animalia" order="Aphelenchida" pageId="6" pageNumber="213" phylum="Nematoda" rank="species" species="subtenuis">
<emphasis id="B926EAF3FFF3FFDAFF56FF00E14BFF56" box="[151,291,187,212]" italics="true" pageId="6" pageNumber="213">A. subtenuis</emphasis>
</taxonomicName>
was first described on 
<taxonomicName id="4C524D62FFF3FFDAFDF5FF00E2C9FF51" box="[564,673,188,211]" class="Liliopsida" family="Amaryllidaceae" genus="Narcissus" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFDF5FF00E2C9FF51" box="[564,673,188,211]" italics="true" pageId="6" pageNumber="213">Narcissus</emphasis>
</taxonomicName>
bulbs (
<taxonomicName id="4C524D62FFF3FFDAFD38FF07E3C6FF56" box="[761,942,187,212]" class="Liliopsida" family="Amaryllidaceae" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="family">Amaryllidaceae</taxonomicName>
) (
<bibRefCitation id="EFC34B10FFF3FFDAFC06FF07E40CFF51" author="Goodey" box="[967,1124,187,212]" pageId="6" pageNumber="213" refString="Goodey, T. (1933) Plant Parasitic Nematodes. Methuen and Co., London, 306 pp." type="book" year="1933">
Goodey 
<number id="3D9A24C6FFF3FFDAFBEAFF07E40CFF51" box="[1067,1124,187,211]" pageId="6" pageNumber="213" value="1933.0">1933</number>
</bibRefCitation>
) and it is mostly found on monocots hosts of the genera 
<taxonomicName id="4C524D62FFF3FFDAFE2DFF62E25FFF75" box="[492,567,222,247]" class="Liliopsida" family="Amaryllidaceae" genus="Allium" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFE2DFF62E25FFF75" box="[492,567,222,247]" italics="true" pageId="6" pageNumber="213">Allium</emphasis>
</taxonomicName>
L. (
<taxonomicName id="4C524D62FFF3FFDAFDA6FF62E374FF75" box="[615,796,222,247]" class="Liliopsida" family="Amaryllidaceae" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="family">Amaryllidaceae</taxonomicName>
) and 
<taxonomicName id="4C524D62FFF3FFDAFC9CFF63E3A2FF74" box="[861,970,223,246]" class="Liliopsida" family="Amaryllidaceae" genus="Narcissus" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFC9CFF63E3A2FF74" box="[861,970,223,246]" italics="true" pageId="6" pageNumber="213">Narcissus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C524D62FFF3FFDAFC1BFF63E442FF74" box="[986,1066,223,246]" class="Liliopsida" family="Iridaceae" genus="Crocus" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFC1BFF63E442FF74" box="[986,1066,223,246]" italics="true" pageId="6" pageNumber="213">Crocus</emphasis>
</taxonomicName>
L. and 
<taxonomicName id="4C524D62FFF3FFDAFB45FF63E4C3FF74" box="[1156,1195,223,246]" class="Insecta" family="Tarachodidae" genus="Iris" kingdom="Animalia" order="Mantodea" pageId="6" pageNumber="213" phylum="Arthropoda" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFB45FF63E4C3FF74" box="[1156,1195,223,246]" italics="true" pageId="6" pageNumber="213">Iris</emphasis>
</taxonomicName>
(
<taxonomicName id="4C524D62FFF3FFDAFB7AFF62E540FF75" box="[1211,1320,222,247]" class="Liliopsida" family="Iridaceae" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="family">Iridaceae</taxonomicName>
), 
<taxonomicName id="4C524D62FFF3FFDAFAFDFF62E514FF75" box="[1340,1404,222,247]" class="Liliopsida" family="Asparagaceae" genus="Scilla" kingdom="Plantae" order="Asparagales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFAFDFF62E514FF75" box="[1340,1404,222,247]" italics="true" pageId="6" pageNumber="213">Scilla</emphasis>
</taxonomicName>
L. and 
<taxonomicName id="4C524D62FFF3FFDAFF04FEBDE164FE98" box="[197,268,257,282]" class="Liliopsida" family="Liliaceae" genus="Tulipa" kingdom="Plantae" order="Liliales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFF04FEBDE164FE98" box="[197,268,257,282]" italics="true" pageId="6" pageNumber="213">Tulipa</emphasis>
</taxonomicName>
L. (
<taxonomicName id="4C524D62FFF3FFDAFEFBFEBDE1B7FE98" authority="Juss." authorityName="Juss." box="[314,479,257,282]" class="Liliopsida" family="Liliaceae" kingdom="Plantae" order="Liliales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="family">Liliaceae Juss.</taxonomicName>
