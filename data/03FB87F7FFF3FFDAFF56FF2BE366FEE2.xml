<taxonomicName id="4C524D62FFF3FFDAFDDDFEBDE2FDFE98" box="[540,661,257,282]" class="Liliopsida" family="Colchicaceae" genus="Colchicum" kingdom="Plantae" order="Liliales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="genus">
<emphasis id="B926EAF3FFF3FFDAFDDDFEBDE2FDFE98" box="[540,661,257,282]" italics="true" pageId="6" pageNumber="213">Colchicum</emphasis>
</taxonomicName>
