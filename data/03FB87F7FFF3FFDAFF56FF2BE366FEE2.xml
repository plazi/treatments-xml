L. (
<taxonomicName id="4C524D62FFF3FFDAFEE4FEFBE1F9FEE2" box="[293,401,327,352]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="6" pageNumber="213" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
