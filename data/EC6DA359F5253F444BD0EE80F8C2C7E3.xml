<location id="611B4494F5253F444EFDECE5FADBC2D0" LSID="urn:lsid:plazi:treatment:EC6DA359F5253F444BD0EE80F8C2C7E3:611B4494F5253F444EFDECE5FADBC2D0" box="[1181,1343,909,952]" county="Swezey" municipality="Nov." name="Talofofo" pageId="41" pageNumber="52">Talofofo</location>
, 
