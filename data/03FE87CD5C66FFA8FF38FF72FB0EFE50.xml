</subSubSection>
<subSubSection id="C34D65505C66FFB5FF38FEE0FDA4FEB0" pageId="47" pageNumber="48" type="reference_group">
<paragraph id="8BE836DB5C66FFB5FF38FEE0FDA4FEB0" blockId="47.[151,1437,261,316]" pageId="47" pageNumber="48">
<treatmentCitationGroup id="AB4711F55C66FFB5FF38FEE0FDA4FEB0" pageId="47" pageNumber="48">
<treatmentCitation id="0AF610CA5C66FFB5FF38FEE0FD85FE90" author="Hoffman, K. M." box="[151,623,261,284]" page="268" pageId="47" pageNumber="48" year="2002">
