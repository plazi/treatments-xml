and 
<collectingCountry id="F340764B5C66FFB5FB6FF8A5FAF4F8D6" box="[1216,1310,1856,1882]" name="Panama" pageId="47" pageNumber="48">Panama</collectingCountry>
(
<bibRefCitation id="EFC64B2A5C66FFB5FA80F8A5FF3DF8F3" author="Hoffman" firstAuthor="Hoffman" pageId="47" pageNumber="48" pagination="251 - 275" refId="ref30681" refString="Hoffman, K. M. (2002) Family Mantispidae. In Penny, N. D. (ed.), A guide to the lacewings (Neuroptera) of Costa Rica. Proceedings of the California Academy of Sciences, 53 (12), 251 - 275." type="journal article" year="2002">Hoffman 2002</bibRefCitation>
