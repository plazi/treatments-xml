<taxonomicName id="4C5C4D4BFFD1FFC6FF5E5FCEFDF46782" authority="Linnaeus" authorityName="Linnaeus" box="[151,516,655,680]" class="Insecta" family="Geometridae" genus="Cosmorhoe" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="404" phylum="Arthropoda" rank="species" species="ocellata">
