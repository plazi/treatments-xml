<taxonomicName authorityName="Peters" authorityYear="1867" class="Mammalia" family="Emballonuridae" genus="Peropteryx" kingdom="Animalia" order="Chiroptera" pageId="11" pageNumber="11" phylum="Chordata" rank="species" species="kappleri">
