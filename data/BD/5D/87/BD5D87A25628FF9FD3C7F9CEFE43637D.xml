</taxonomicName>
(
<collectionCode box="[389,472,1051,1075]" pageId="54" pageNumber="54">MUSM</collectionCode>
5931) or as 
<taxonomicName baseAuthorityName="Allen" baseAuthorityYear="1904" class="Mammalia" family="Phyllostomidae" genus="Hsunycteris" kingdom="Animalia" order="Chiroptera" pageId="54" pageNumber="54" phylum="Chordata" rank="species" species="thomasi">
