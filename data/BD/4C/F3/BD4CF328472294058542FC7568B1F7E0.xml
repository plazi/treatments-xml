<subSubSection lastPageId="58" lastPageNumber="59" pageId="57" pageNumber="58" type="type specimens">
<paragraph pageId="57" pageNumber="58">Type specimens.</paragraph>
<paragraph pageId="57" pageNumber="58">
<taxonomicName class="Insecta" family="Scarabaeidae" genus="Canthon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Canthon (Goniocanthon) fulgidus" order="Coleoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="subSpecies" species="fulgidus" subGenus="Goniocanthon" subSpecies="martinezi">Canthon (Goniocanthon) fulgidus martinezi</taxonomicName>
