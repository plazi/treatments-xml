<date box="[1110,1242,1000,1026]" pageId="3" pageNumber="38" value="1910" valueMax="1913">
<number box="[1110,1242,1000,1026]" pageId="3" pageNumber="38" value="1911.5" valueMax="1913.0" valueMin="1910.0">1910-1913</number>
