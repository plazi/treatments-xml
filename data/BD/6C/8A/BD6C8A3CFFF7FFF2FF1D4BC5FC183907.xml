of 
<taxonomicName box="[612,810,925,950]" class="Insecta" family="Issidae" genus="Orinda" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="38" phylum="Arthropoda" rank="species" species="bimaculifrons">
