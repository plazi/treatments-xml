<subSubSection pageId="37" pageNumber="38" type="etymology">
<paragraph blockId="37.[151,1436,263,1527]" pageId="37" pageNumber="38">
<emphasis bold="true" box="[198,331,1462,1488]" pageId="37" pageNumber="38">Etymology</emphasis>
