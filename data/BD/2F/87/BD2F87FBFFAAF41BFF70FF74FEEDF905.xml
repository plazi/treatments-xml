. 
<taxonomicName authorityName="Kruse and Powell" authorityYear="2009" box="[365,681,1617,1642]" class="Insecta" family="Tortricidae" genus="Sparganothoides" kingdom="Animalia" order="Lepidoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="silaceana">
