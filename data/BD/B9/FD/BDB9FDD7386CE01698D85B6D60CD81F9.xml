<subSubSection pageId="null" pageNumber="130" type="nomenclature">
<paragraph pageId="null" pageNumber="130">
<taxonomicName authority="L." class="Magnoliopsida" family="Lamiaceae" genus="Stachys" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="130" phylum="Tracheophyta" rank="species" species="palustris">
<pageBreakToken pageId="null" pageNumber="130" start="start">Stachys</pageBreakToken>
