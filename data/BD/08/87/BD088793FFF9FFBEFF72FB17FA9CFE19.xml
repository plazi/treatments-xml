<paragraph blockId="1.[151,471,1263,1322]" box="[151,471,1263,1289]" pageId="1" pageNumber="216">
<heading bold="true" box="[151,471,1263,1289]" fontSize="11" level="1" pageId="1" pageNumber="216" reason="1">
<emphasis bold="true" box="[151,471,1263,1289]" pageId="1" pageNumber="216">
<taxonomicName box="[151,327,1263,1289]" class="Insecta" family="Leptoceridae" genus="Magadacerina" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="216" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[151,327,1263,1289]" italics="true" pageId="1" pageNumber="216">Magadacerina</emphasis>
</taxonomicName>
