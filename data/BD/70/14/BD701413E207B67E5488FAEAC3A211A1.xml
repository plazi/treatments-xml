<emphasis bold="true" pageId="59" pageNumber="782">
<taxonomicName authority="Bolzern &amp; Burckhardt &amp; Hänggi, 2013" authorityName="Bolzern &amp; Burckhardt &amp; Hänggi" authorityYear="2013" box="[1196,1398,1765,1785]" class="Arachnida" family="Agelenidae" genus="Tegenaria" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="782" phylum="Arthropoda" rank="species" species="annae" status="sp. nov.">
<emphasis bold="true" box="[1196,1398,1765,1785]" italics="true" pageId="59" pageNumber="782">Tegenaria annae</emphasis>
</taxonomicName>
<taxonomicNameLabel pageId="59" pageNumber="782" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; O- R, 
<taxonomicName authorityName="BARRIENTOS" authorityYear="1981" box="[286,484,1795,1813]" class="Arachnida" family="Agelenidae" genus="Tegenaria" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="782" phylum="Arthropoda" rank="species" species="carensis">
<emphasis box="[286,484,1795,1813]" italics="true" pageId="59" pageNumber="782">Tegenaria carensis</emphasis>
</taxonomicName>
; S- V, 
<taxonomicName authorityName="Barrientos" authorityYear="1978" box="[553,752,1795,1814]" class="Arachnida" family="Agelenidae" genus="Tegenaria" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="782" phylum="Arthropoda" rank="species" species="ramblae">
<emphasis box="[553,752,1795,1814]" italics="true" pageId="59" pageNumber="782">Tegenaria ramblae</emphasis>
</taxonomicName>
; and W, X, 
<taxonomicName baseAuthorityName="Clerck" baseAuthorityYear="1757" box="[879,1097,1795,1814]" class="Arachnida" family="Agelenidae" genus="Tegenaria" kingdom="Animalia" order="Araneae" pageId="59" pageNumber="782" phylum="Arthropoda" rank="species" species="domestica">
