<taxonomicName authorityName="BRIGNOLI" authorityYear="1976" box="[1256,1434,1789,1811]" class="Arachnida" family="Agelenidae" genus="Tegenaria" kingdom="Animalia" order="Araneae" pageId="91" pageNumber="814" phylum="Arthropoda" rank="species" species="regispyrrhi">
