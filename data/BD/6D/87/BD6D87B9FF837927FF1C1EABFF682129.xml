<taxonomicName authority="Badonnel" authorityName="Badonnel" authorityYear="1948" class="Insecta" family="Peripsocidae" genus="Peripsocus" kingdom="Animalia" order="Psocodea" pageId="15" pageNumber="438" phylum="Arthropoda" rank="species" species="ghesquierei">
