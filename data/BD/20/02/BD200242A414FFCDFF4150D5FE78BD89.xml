<materialsCitation ID-GBIF-Occurrence="3007911309" collectorName="Natural History Museum &amp; Zoo" country="Croatia" location="Croatia" pageId="3" pageNumber="278" specimenCount="3" specimenCount-female="1" specimenCount-male="2" typeStatus="holotype">
<typeStatus box="[199,303,727,753]" pageId="3" pageNumber="278">Holotype</typeStatus>
and 
<typeStatus box="[363,470,728,754]" pageId="3" pageNumber="278">paratypes</typeStatus>
and the mentioned 
<typeStatus box="[701,878,727,753]" isOtherTypeStatus="true" pageId="3" pageNumber="278">further material</typeStatus>
are deposited in the 
<collectorName box="[1120,1405,727,754]" pageId="3" pageNumber="278">Natural History Museum</collectorName>
&amp; 
<collectorName box="[151,199,764,790]" pageId="3" pageNumber="278">Zoo</collectorName>
, Split, 
<collectingCountry box="[277,363,763,789]" name="Croatia" pageId="3" pageNumber="278">Croatia</collectingCountry>
