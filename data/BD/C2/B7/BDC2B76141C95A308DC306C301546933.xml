</taxonomicName>
, 
<taxonomicName class="Sordariomycetes" family="Diaporthaceae" genus="Ophiodiaporthe" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Ophiodiaporthe" order="Diaporthales" pageId="0" pageNumber="67" phylum="Ascomycota" rank="genus">
