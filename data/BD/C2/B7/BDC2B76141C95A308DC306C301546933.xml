<taxonomicName class="Sordariomycetes" family="Diaporthaceae" genus="Mazzantia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Mazzantia" order="Diaporthales" pageId="0" pageNumber="67" phylum="Ascomycota" rank="genus">
