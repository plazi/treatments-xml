, 
<taxonomicName baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1846" class="Gastropoda" family="Bulimulidae" genus="Protoglyptus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="96" pageNumber="97" phylum="Mollusca" rank="species" species="martinicensis">
