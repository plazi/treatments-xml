<subSubSection pageId="null" pageNumber="557" type="reference_group">
<paragraph pageId="null" pageNumber="557">
(
<taxonomicName authority="Ten." authorityName="Ten." class="Liliopsida" family="Asparagaceae" genus="Ornithogalum" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="557" phylum="Tracheophyta" rank="species" species="gussonei">
