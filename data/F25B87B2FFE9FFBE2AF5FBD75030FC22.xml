<bibRefCitation id="1E634B55FFEEFFBE2B89FCC95028FCE2" author="Berlese A" box="[144,306,835,859]" pageId="4" pageNumber="1622" pagination="7 - 16" refId="ref6966" refString="Berlese A. 1917. Intorno agli Uropodidae. Redia. 13: 7 - 16." type="journal article" year="1917">Berlese, 1917</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="BDF24D27FFEEFFBE2A52FCCE5254FCE2" authority="Hirschmann, 1979" authorityName="Hirschmann" authorityYear="1979" box="[331,846,835,860]" class="Arachnida" family="Macrodinychidae" genus="Macrodinychus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="1632" phylum="Arthropoda" rank="subGenus" subGenus="Bregetovamacrodinychus">
<emphasis id="4886EAB6FFEEFFBE2A52FCCE5343FCE5" box="[331,601,836,860]" italics="true" pageId="4" pageNumber="1622">Bregetovamacrodinychus</emphasis>
<bibRefCitation id="1E634B55FFEEFFBE2977FCC95254FCE2" author="Hirschmann W" box="[622,846,835,859]" pageId="4" pageNumber="1622" pagination="57 - 70" refId="ref7363" refString="Hirschmann W. 1979. Stadiensystematik der Parasitiformes Teil 1. Stadienfamilien und Stadiengattungen der Atrichopygidiina, erstellt im Vergleich zum Gangsystem Hirschmann, 1979. Acarol Schrift Vergl Milbenkunde. 26: 57 - 70." type="journal article" year="1979">Hirschmann, 1979</bibRefCitation>
</taxonomicName>
