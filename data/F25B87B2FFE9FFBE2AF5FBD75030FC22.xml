<taxonomicName id="BDF24D27FFEEFFBE28BCFCAE5028FCE2" authority="Berlese, 1917" authorityName="Berlese" authorityYear="1917" class="Arachnida" family="Macrodinychidae" genus="Macrodinychus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="1622" phylum="Arthropoda" rank="genus">
