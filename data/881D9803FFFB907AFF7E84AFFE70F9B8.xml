<emphasis id="32C0F507FFFB907AFF7E84F0FE11FE63" bold="true" box="[151,395,498,524]" pageId="5" pageNumber="6">Material examined.</emphasis>
<materialsCitation id="B0DC2348FFFB907AFE7B84F0FB52FE63" box="[402,1224,498,524]" collectorName="Locality No." location="Mt. Meron" pageId="5" pageNumber="6" specimenCount="1" stateProvince="Faro">
<collectorName id="AD414CC3FFFB907AFE7B84F0FDBDFE63" box="[402,551,498,524]" pageId="5" pageNumber="6">Locality No.</collectorName>
19: 
<location id="056B7FCEFFFB907AFDB284F0FD44FE63" LSID="urn:lsid:plazi:treatment:881D9803FFFB907AFF7E84AFFE70F9B8:056B7FCEFFFB907AFDB284F0FD44FE63" box="[603,734,498,524]" name="Mt. Meron" pageId="5" pageNumber="6" stateProvince="Faro">Mt. Meron</location>
: near 
<collectingRegion id="C270E7F7FFFB907AFCCA84F0FCD5FE63" box="[803,847,498,524]" country="Portugal" name="Faro" pageId="5" pageNumber="6">Har</collectingRegion>
Bar-Yohai (only 
<specimenCount id="16B2E29CFFFB907AFBF384F0FB5AFE63" box="[1050,1216,498,524]" pageId="5" pageNumber="6" type="generic">one specimen</specimenCount>
)
</materialsCitation>
