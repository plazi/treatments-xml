<taxonomicName id="F64B2E16995050A4228613778786AEA9" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:140796" class="Insecta" family="Formicidae" genus="Eciton" kingdom="Animalia" lsidName-HNS="Eciton (Acamatus) raptor Forel" order="Hymenoptera" pageNumber="289" phylum="Arthropoda" rank="species" species="raptor" subGenus="Acamatus">Eciton (Acamatus) raptor</taxonomicName>
