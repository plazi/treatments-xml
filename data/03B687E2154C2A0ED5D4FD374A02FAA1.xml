<typeStatus id="54A48856154C2A0ED4C9FD064B35FC80" box="[431,512,766,786]" pageId="41" pageNumber="768">Holotype</typeStatus>
, 
<collectingRegion id="49DBF816154C2A0ED74AFD064B51FC80" box="[556,612,766,786]" country="Malaysia" name="Sabah" pageId="41" pageNumber="768">Sabah</collectingRegion>
