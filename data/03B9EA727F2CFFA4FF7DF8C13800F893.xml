<taxonomicName id="4C1020E77F2BFFA4FBB3F92D3C15F893" authority="(Aurivillius, 1898)" baseAuthorityName="Aurivillius" baseAuthorityYear="1898" class="Maxillopoda" family="Scalpellidae" genus="Arcoscalpellum" kingdom="Animalia" order="Pedunculata" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="mamillatum">
