<taxonomicName id="1F215F2A1044C837D591B1BCE74B83F8" authority="Jacobaea L." class="Magnoliopsida" family="Asteraceae" genus="Senecio" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="505" phylum="Tracheophyta" rank="genus">
