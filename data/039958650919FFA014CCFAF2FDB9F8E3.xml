(
<bibRefCitation id="EFA194820919FFA015EAF9C1FDEEF99F" author="Righi, G." box="[433,563,1611,1637]" pageId="56" pageNumber="57" pagination="99 - 116" refId="ref104002" refString="Righi, G. (1967) Sobre alguns Decapoda do Brasil (Crustacea, Brachyura, Pinnotheridae e Partheipidae). Papeis Avulsos de Zoologia, 20, 99 - 116." type="journal article" year="1967">Righi 1967</bibRefCitation>
; 
<bibRefCitation id="EFA194820919FFA01664F9C1FD61F99F" author="Melo, G. A. S." box="[575,700,1611,1637]" pageId="56" pageNumber="57" refId="ref99429" refString="Melo, G. A. S. (1996) Manual de identificacao dos Brachyura (caranguejos e siris) do Litoral Brasileiro. Editora, Pleiade, Sao Paulo, 604 pp." type="book" year="1996">Melo 1996</bibRefCitation>
