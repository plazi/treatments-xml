<emphasis id="1A9EEAC5FF8AFFBAFF1BFD7FFCD6FD26" bold="true" box="[151,869,653,679]" pageId="14" pageNumber="465">
Genus 
<taxonomicName id="EFEA4D54FF8AFFBAFF62FD7FFE46FD26" authority="Mosely 1953" authorityName="Mosely" authorityYear="1953" box="[238,501,653,679]" class="Insecta" family="Calocidae" genus="Caenota" kingdom="Animalia" order="Trichoptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="genus">
<emphasis id="1A9EEAC5FF8AFFBAFF62FD7FFEE0FD26" bold="true" box="[238,339,653,679]" italics="true" pageId="14" pageNumber="465">Caenota</emphasis>
<bibRefCitation id="4C7B4B26FF8AFFBAFED5FD7FFE46FD26" author="Mosely" box="[345,501,653,679]" pageId="14" pageNumber="465" refString="Mosely, M. E. &amp; Kimmins, D. E. (1953) The Trichoptera (caddis-flies) of Australia and New Zealand. Adlard and Son, Limited, Dorking, Surrey, United Kingdom, 550 pp." type="book" year="1953">
Mosely 
<number id="9E2224F0FF8AFFBAFE36FD7FFE46FD26" box="[442,501,653,679]" pageId="14" pageNumber="465" value="1953.0">1953</number>
</bibRefCitation>
</taxonomicName>
(in 
<bibRefCitation id="4C7B4B26FF8AFFBAFDAAFD7FFCEEFD26" author="Mosely" box="[550,861,653,679]" pageId="14" pageNumber="465" refString="Mosely, M. E. &amp; Kimmins, D. E. (1953) The Trichoptera (caddis-flies) of Australia and New Zealand. Adlard and Son, Limited, Dorking, Surrey, United Kingdom, 550 pp." type="book" year="1953">
Mosely &amp; Kimmins 
<number id="9E2224F0FF8AFFBAFCADFD7FFCEEFD26" box="[801,861,653,679]" pageId="14" pageNumber="465" value="1953.0">1953</number>
