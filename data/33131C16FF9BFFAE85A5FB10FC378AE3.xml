<taxonomicName id="7CBAD683FF9BFFAE8419FAEAFDCF8836" authority="Chevreux, 1906" authorityName="Chevreux" authorityYear="1906" box="[299,604,1317,1342]" class="Malacostraca" family="Lysianassidae" genus="Waldeckia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
