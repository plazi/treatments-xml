<specimenCount id="10C7FD235428FFAC6360FEC9FD2DFE98" box="[588,647,296,320]" pageId="19" pageNumber="20" type="female">320♀</specimenCount>
(
<collectionCode id="60D0AE6F5428FFAC63BAFEC9FD47FE98" box="[662,749,296,320]" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="19" pageNumber="20">ZMUC</collectionCode>
