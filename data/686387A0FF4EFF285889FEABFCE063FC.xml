<emphasis id="D2BEEAA4FF4EFF285A75FE72FCE36360" box="[619,848,511,537]" italics="true" pageId="131" pageNumber="132">
<taxonomicName id="27CA4D35FF4EFF285A75FE72FCFF6360" authorityName="Rondani" authorityYear="1863" box="[619,844,511,537]" class="Insecta" family="Lauxaniidae" genus="Sapromyza" kingdom="Animalia" order="Diptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="lateritia">Sapromyza lateritia</taxonomicName>
