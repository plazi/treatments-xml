<taxonomicName id="6269C0B3BD3A5143709B577CC5E95355" class="Insecta" family="Chrysomelidae" genus="Coscinoptera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coscinoptera" order="Coleoptera" pageId="23" pageNumber="34" phylum="Arthropoda" rank="genus">Coscinoptera</taxonomicName>
