<number id="5F1F24C8FFFBC46BFD351127FD116415" box="[655,668,512,536]" pageId="7" pageNumber="556" value="3.0">3</number>
+ 
<number id="5F1F24C8FFFBC46BFD141127FD366415" box="[686,699,512,536]" pageId="7" pageNumber="556" value="3.0">3</number>
