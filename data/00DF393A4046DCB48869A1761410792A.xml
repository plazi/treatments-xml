<subSubSection id="8396E6F96739DB09D2057C0CED25C0C9" pageId="6" pageNumber="77" type="materials_examined">
<paragraph id="B9368F7BBB2AD5BA575ED2F9A0CE32CA" pageId="6" pageNumber="77">Material examined.</paragraph>
<paragraph id="F14BF26821CE6844B76795920CE0FE82" pageId="6" pageNumber="77">
