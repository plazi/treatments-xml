<taxonomicName id="4C6F4D4AA170FFC27FAE99BCA22A52E2" authority="Martens, Halse &amp; Schon, 2012" authorityName="Martens, Halse &amp; Schon" authorityYear="2012" box="[455,1133,916,943]" class="Ostracoda" family="Cyprididae" genus="Bennelongia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="gwelupensis">
