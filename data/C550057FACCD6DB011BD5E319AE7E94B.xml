<taxonomicName id="07FED236775876ADDDB7914ABF335859" LSID="http://zoobank.org/92CD2FEB-8875-497D-9248-12A3BD5074BA" class="Insecta" family="Cicadellidae" genus="Thaioneura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thaioneura suphanburia" order="Hemiptera" pageId="6" pageNumber="13" phylum="Arthropoda" rank="species" species="suphanburia">Thaioneura suphanburia</taxonomicName>
