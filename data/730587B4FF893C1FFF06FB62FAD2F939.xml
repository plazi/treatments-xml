<quantity id="3C549B47FF893C1FFDBFF9EDFDCCF946" box="[558,601,1557,1582]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="4" pageNumber="483" unit="m" value="5.0">5 m</quantity>
