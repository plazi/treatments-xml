<taxonomicName id="4C294D45FFEDFFAEEEDA602EFD317973" box="[641,755,792,817]" class="Arachnida" family="Eriophyidae" genus="Tetra" kingdom="Animalia" order="Prostigmata" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="hupeanae">
