<emphasis id="B95DEAD4FFEDFFAEE8B4602EFA9E7973" box="[1263,1372,792,817]" italics="true" pageId="10" pageNumber="11">
<taxonomicName id="4C294D45FFEDFFAEE8B4602EFA9A7973" box="[1263,1368,792,817]" class="Magnoliopsida" family="Fabaceae" genus="Dalbergia" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="hupeana">hupeana</taxonomicName>
