<taxonomicName id="DD844D45FFF4003DFA8FF4A8CC933902" authority="Attems, 1936" authorityName="Attems" authorityYear="1936" class="Diplopoda" family="Zephroniidae" genus="Zephronia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sphaerotheriida" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="viridescens">
