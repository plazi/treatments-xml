<taxonomicName id="B7D5B5A419471684E7FB02BC1F12A869" class="Insecta" family="Carabidae" genus="Amphimenes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amphimenes medius" order="Coleoptera" pageId="14" pageNumber="31" phylum="Arthropoda" rank="species" species="medius">Amphimenes medius</taxonomicName>
