</taxonomicName>
males. A – 
<taxonomicName id="4C78F1F8E80951357C6AF7D93DCEF7C7" authority="Yin &amp; Li, 2021" authorityName="Yin &amp; Li" authorityYear="2021" box="[572,655,2061,2080]" class="Insecta" family="Staphylinidae" genus="Pselaphodes" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="44" phylum="Arthropoda" rank="species" species="jendeki" status="sp. nov.">
<emphasis id="B90C5669E80951357C6AF7D93DCEF7C7" box="[572,655,2061,2080]" italics="true" pageId="9" pageNumber="44">P. jendeki</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23FEB12E80951357CC2F7D93D98F7C7" box="[660,729,2061,2080]" pageId="9" pageNumber="44" rank="species">sp. nov.</taxonomicNameLabel>
; B – 
<taxonomicName id="4C78F1F8E80951357D52F7D93C15F7C7" authority="Yin &amp; Li, 2021" authorityName="Yin &amp; Li" authorityYear="2021" box="[772,852,2061,2080]" class="Insecta" family="Staphylinidae" genus="Pselaphodes" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="44" phylum="Arthropoda" rank="species" species="nongfa" status="sp. nov.">
