<number id="FD0424FCFFB1C01EFD9FFDAA2B3BFDC6" box="[519,571,548,572]" pageId="7" pageNumber="204" value="0.43">0.43</number>
, 
<number id="FD0424FCFFB1C01EFDD0FDAA2B7CFDC6" box="[584,636,548,572]" pageId="7" pageNumber="204" value="1.8">1.80</number>
, 
