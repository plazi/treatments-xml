, 
<number id="FD0424FCFFB1C01EFC69FDAA2D24FDC6" box="[1009,1060,548,572]" pageId="7" pageNumber="204" value="1.33">1.33</number>
, 
<number id="FD0424FCFFB1C01EFBAAFDAA2D66FDC7" box="[1074,1126,548,573]" pageId="7" pageNumber="204" value="1.16">1.16</number>
, 
<number id="FD0424FCFFB1C01EFBEAFDAA2DA6FDC6" box="[1138,1190,548,572]" pageId="7" pageNumber="204" value="0.71">0.71</number>
), III 
<number id="FD0424FCFFB1C01EFB79FDAA2C12FDC7" box="[1249,1298,548,573]" pageId="7" pageNumber="204" value="4.36">4.36</number>
(
<number id="FD0424FCFFB1C01EFABBFDAA2C57FDC7" box="[1315,1367,548,573]" pageId="7" pageNumber="204" value="1.15">1.15</number>
