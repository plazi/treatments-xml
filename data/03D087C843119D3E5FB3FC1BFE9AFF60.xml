<taxonomicName id="4C794D5D43119D385DF0FBF1FC9CFBFA" box="[724,870,1080,1102]" class="Insecta" family="Tephritidae" genus="Gonioglossum" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="57" phylum="Arthropoda" rank="genus">
