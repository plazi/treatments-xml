</taxonomicName>
and 
<taxonomicName id="4C1C4D3E4F201569FD827CB2FCCBFB63" authorityName="Knight" authorityYear="1933" box="[580,768,1228,1254]" class="Gastropoda" family="Araeonematidae" genus="Rhabdotocochlis" higherTaxonomySource="CoL" kingdom="Animalia" order="Trochida" pageId="23" pageNumber="24" phylum="Mollusca" rank="genus">
