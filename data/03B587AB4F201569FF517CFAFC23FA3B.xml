<taxonomicName id="4C1C4D3E4F201569FED57CB3FEB7FB63" authorityName="Mansuy" authorityYear="1912" box="[275,380,1229,1254]" class="Gastropoda" family="Araeonematidae" genus="Yunnania" higherTaxonomySource="CoL" kingdom="Fungi" order="Trochida" pageId="23" pageNumber="24" phylum="Ascomycota" rank="genus">
