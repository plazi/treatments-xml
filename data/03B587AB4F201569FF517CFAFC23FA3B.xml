</taxonomicName>
, 
<taxonomicName id="4C1C4D3E4F201569FE4D7CB2FDC7FB63" authorityName="Knight" authorityYear="1933" box="[395,524,1228,1254]" class="Gastropoda" family="Euomphalidae" genus="Araeonema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Euomphalina" pageId="23" pageNumber="24" phylum="Mollusca" rank="genus">
