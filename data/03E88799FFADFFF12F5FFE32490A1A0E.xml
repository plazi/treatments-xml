<collectingCountry id="F356761FFFADFFF12F43FD7A4E4E1CFE" box="[538,627,674,700]" name="Mexico" pageId="17" pageNumber="18">México</collectingCountry>
(
<collectingRegion id="4985F86DFFADFFF12FDAFD7A4F2C1CFE" box="[643,785,674,700]" country="Mexico" name="Tamaulipas" pageId="17" pageNumber="18">Tamaulipas</collectingRegion>
