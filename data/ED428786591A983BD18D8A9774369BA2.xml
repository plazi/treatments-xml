<taxonomicName id="A2EB4D13591A983BD21E8E8271859BFF" authority="(Wood, 1867)" baseAuthorityName="Wood" baseAuthorityYear="1867" box="[772,1191,1442,1469]" class="Chilopoda" family="Scutigeridae" genus="Dendrothereua" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scutigeromorpha" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="linceci">
