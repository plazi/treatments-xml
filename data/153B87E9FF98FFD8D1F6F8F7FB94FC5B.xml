<taxonomicName id="5A924D7CFF9BFFD8D1A6FEBFFEF7FEE3" box="[199,325,260,284]" class="Insecta" family="Elateridae" genus="Lacon" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="379" phylum="Arthropoda" rank="species" species="salvazai">
