<taxonomicName id="05F34D0EFF1968E0FF4AFDA1CD8AFDEA" authority="(Coleman 2007)" baseAuthorityName="Coleman" baseAuthorityYear="2007" box="[189,669,585,611]" class="Malacostraca" family="Acanthonotozomellidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="159" pageNumber="160" phylum="Arthropoda" rank="family">
