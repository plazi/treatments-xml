<taxonomicName id="5B894D1E137D5407FF33FC7D7401E56E" authority="Wang &amp; Zhang 2014: 587" authorityName="Wang &amp; Zhang" authorityPageNumber="587" authorityYear="2014" box="[151,615,1022,1044]" class="Insecta" family="Cicadellidae" genus="Concavocorona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
