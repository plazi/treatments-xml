<quantity id="4C9C6EB8FFC3FFF6FB07FDC9FB4AFD7D" box="[1155,1244,619,646]" metricMagnitude="-3" metricUnit="m" metricValue="7.1" pageId="19" pageNumber="380" unit="mm" value="7.1">7.1 mm</quantity>
