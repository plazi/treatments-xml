<collectingCountry id="F37383CDFFC4FFF1FEE3FBC1FE6CFB86" box="[359,506,1123,1149]" name="South Africa" pageId="20" pageNumber="381">South Africa</collectingCountry>
: 
<collectingRegion id="49A00DBFFFC4FFF1FD80FBC1FD35FB86" box="[516,675,1123,1149]" country="South Africa" name="Western Cape" pageId="20" pageNumber="381">Western Cape</collectingRegion>
