<quantity id="4C9C6EB8FFC3FFF6FCDBFDC9FC2EFD7D" box="[863,952,619,646]" metricMagnitude="-3" metricUnit="m" metricValue="8.4" pageId="19" pageNumber="380" unit="mm" value="8.4">8.4 mm</quantity>
