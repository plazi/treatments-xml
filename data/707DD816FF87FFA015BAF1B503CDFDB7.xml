<taxonomicName id="3FD41283FF87FFA315BAF1B502D9FC79" authority="Troubridge" authorityName="Troubridge" authorityYear="2008" box="[151,478,906,932]" class="Insecta" family="Noctuidae" genus="Sympistis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="apep" status="sp. n.">
