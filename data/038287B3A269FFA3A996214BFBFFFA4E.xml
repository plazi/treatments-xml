, 
<collectingRegion id="49EFF847A268FFA3AEF0216EFA3BFA6B" box="[1252,1417,1391,1414]" country="Australia" name="South Australia" pageId="105" pageNumber="362">South Australia</collectingRegion>
, 
<collectingRegion id="49EFF847A268FFA3A90E218DFCC6FA4E" box="[794,884,1420,1443]" country="Australia" name="Victoria" pageId="105" pageNumber="362">Victoria</collectingRegion>
, 
<collectingRegion id="49EFF847A268FFA3A96A218DFBF0FA4E" box="[894,1090,1420,1443]" country="Australia" name="New South Wales" pageId="105" pageNumber="362">New South Wales</collectingRegion>
).
</paragraph>
