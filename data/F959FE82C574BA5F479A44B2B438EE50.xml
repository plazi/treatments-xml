<taxonomicName id="A5A50CAD483C19EB26BA353CE7EDB683" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33077" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Oligomyrmex" kingdom="Animalia" lsidName-HNS="Oligomyrmex grandidieri Forel" order="Hymenoptera" pageNumber="201" phylum="Arthropoda" rank="species" species="grandidieri" status="nov. sp.">OLIGOMYRMEX GRANDIDIERI</taxonomicName>
, 
