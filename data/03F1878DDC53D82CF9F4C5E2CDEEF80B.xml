<number id="3D9024BCDC51D82CFD1BC416C8B6F87B" box="[1144,1188,1900,1924]" pageId="6" pageNumber="45" value="608.0">608</number>
/ 
<number id="3D9024BCDC51D82CFDCEC416C8C5F87B" box="[1197,1239,1900,1924]" pageId="6" pageNumber="45" value="391.0">391</number>
, ventral shield L/ W 
<number id="3D9024BCDC51D82CF9DAC4EACCF6F857" box="[185,228,1936,1960]" pageId="6" pageNumber="45" value="714.0">714</number>
/ 
<number id="3D9024BCDC51D82CF98EC4EACD0AF857" box="[237,280,1936,1960]" pageId="6" pageNumber="45" value="425.0">425</number>
, genital field L/W 
<number id="3D9024BCDC51D82CF88CC4EACE08F857" box="[495,538,1936,1960]" pageId="6" pageNumber="45" value="140.0">140</number>
/ 
<number id="3D9024BCDC51D82CFB40C4EACE5FF857" box="[547,589,1936,1960]" pageId="6" pageNumber="45" value="129.0">129</number>
, palp total L 
<number id="3D9024BCDC51D82CFB86C4EACF00F857" box="[741,786,1936,1960]" pageId="6" pageNumber="45" value="189.0">189</number>
; data taken from 
<bibRefCitation id="EFC94B6ADC51D82CFABAC4EAC845F857" author="Cook" box="[985,1111,1936,1960]" pageId="6" pageNumber="45" refString="Cook, D. R. (1986) Water mites from Australia. Memoirs of the American Entomological Institute, 40, 1 - 568." type="journal article" year="1986">
Cook 
<number id="3D9024BCDC51D82CFD7DC4EAC845F857" box="[1054,1111,1936,1960]" pageId="6" pageNumber="45" value="1986.0">1986</number>
</bibRefCitation>
) and considerably elongated genital field (L/W ratio 
<number id="3D9024BCDC51D82CF8C1C4CECDD7F833" box="[418,453,1972,1996]" pageId="6" pageNumber="45" value="1.6">1.6</number>
<emphasis id="B92CEA89DC51D82CF8AFC4CFCDF1F833" box="[460,483,1973,1996]" italics="true" pageId="6" pageNumber="45">vs</emphasis>
. 
<number id="3D9024BCDC51D82CF891C4CECE0AF833" box="[498,536,1972,1996]" pageId="6" pageNumber="45" value="1.1">1.1</number>
