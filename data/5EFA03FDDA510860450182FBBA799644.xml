<paragraph id="FE5E8861C4A0462EAF79BF14D1F29401" pageId="null" pageNumber="208">
<taxonomicName id="7D76774CB44442C2FAFDE84A03B992E4" authority="All." class="Liliopsida" family="Hydrocharitaceae" genus="Najas" kingdom="Plantae" order="Alismatales" pageId="null" pageNumber="208" phylum="Tracheophyta" rank="species" species="minor">
