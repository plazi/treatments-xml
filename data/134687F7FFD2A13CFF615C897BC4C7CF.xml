<paragraph id="9B5036E1FFD2A13CFF615D397E62C512" blockId="3.[151,336,295,317]" box="[151,336,295,317]" pageId="3" pageNumber="4">
<taxonomicName id="5CEF4D62FFD2A13CFF615D397E62C512" box="[151,336,295,317]" class="Actinopterygii" family="Tetraodontidae" genus="Tetraodon" kingdom="Animalia" order="Tetraodontiformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="inermis">
