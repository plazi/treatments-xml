<quantity id="4CE8F6817F07FF88FBFEF86239D5F821" box="[1044,1196,1937,1962]" metricMagnitude="2" metricUnit="m" metricValue="2.32" metricValueMax="3.31" metricValueMin="1.33" pageId="48" pageNumber="49" unit="m" value="232.0" valueMax="331.0" valueMin="133.0">
