<taxonomicName id="4C051AAAFFF30C0BFC66FA7CE471F9C4" authorityName="Assing" authorityYear="2010" box="[900,1030,1525,1543]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="319" phylum="Arthropoda" rank="species" species="involutum">
