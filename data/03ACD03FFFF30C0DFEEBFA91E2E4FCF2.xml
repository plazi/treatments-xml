<taxonomicName id="4C051AAAFFF20C0AFDBAFA22E2E1FA78" authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[600,662,1451,1467]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="320" phylum="Arthropoda" rank="species" species="alesialesi" status="sp. n.">
