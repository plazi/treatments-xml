; PL 
<quantity id="2644459AFFD4D3195AB2C8E2614490F9" box="[948,1053,692,716]" metricMagnitude="-4" metricUnit="m" metricValue="2.7" pageId="8" pageNumber="9" unit="mm" value="0.27">0.27 mm</quantity>
; PW 
<quantity id="2644459AFFD4D3195D5AC8E2619D90F9" box="[1116,1220,692,716]" metricMagnitude="-4" metricUnit="m" metricValue="3.3" pageId="8" pageNumber="9" unit="mm" value="0.33">0.33 mm</quantity>
; EL 
