<collectionCode id="42B61DCEFF3F83CFFE5F5A64FE38FDE7" box="[411,424,516,540]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="130" pageNumber="130" type="Herbarium">P</collectionCode>
