<collectingCountry id="F3647649011CDA7CFC0DF8D53F812DE5" box="[925,1015,1913,1938]" name="Mexico" pageId="23" pageNumber="24">
<collectingRegion id="49B7F83B011CDA7CFC0DF8D53F812DE5" box="[925,1015,1913,1938]" country="Mexico" name="Mexico" pageId="23" pageNumber="24">Mexico</collectingRegion>
