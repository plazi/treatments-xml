<number id="3D891352FFA0CD12FEF31465FE852D2E" box="[313,358,1080,1104]" pageId="3" pageNumber="66" value="126.0">126</number>
; maximum width 
<number id="3D891352FFA0CD12FDFB1465FDBD2D2E" box="[561,606,1080,1104]" pageId="3" pageNumber="66" value="156.0">156</number>
