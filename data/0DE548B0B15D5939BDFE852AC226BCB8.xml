<taxonomicName id="3DC3814D3A8C40F34EDCD81CBF166BF6" class="Thecofilosea" family="Castanellidae" genus="Castanea" higherTaxonomySource="GBIF,CoL" kingdom="Chromista" lsidName="Castanea mollissima" order="Phaeocalpida" pageId="0" pageNumber="65" phylum="Cercozoa" rank="species" species="mollissima">
<emphasis id="863D124EC03EAC5A591DCCD0D5211F6B" italics="true" pageId="0" pageNumber="65">Castanea mollissima</emphasis>
</taxonomicName>
