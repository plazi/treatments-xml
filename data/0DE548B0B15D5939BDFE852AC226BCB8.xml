<taxonomicName id="B0D5048451D7AFDFE8395434461A7E77" authorityName="W.C.Cheng ex Yen C.Yang" authorityYear="1945" class="Dicotyledoneae" family="Lauraceae" genus="Machilus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Machilus pingii" order="Magnoliales" pageId="0" pageNumber="65" phylum="Angiospermae" rank="species" species="pingii">
