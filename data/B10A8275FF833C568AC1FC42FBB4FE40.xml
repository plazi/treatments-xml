<taxonomicName id="FEA348E0FF833C578AC1FBDAFDAFFBCE" authority="Sharp, 1886: 448" authorityName="Sharp" authorityPageNumber="448" authorityYear="1886" box="[151,567,1120,1145]" class="Insecta" family="Chrysomelidae" genus="Caccomolpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="plagiatus">
