<taxonomicName id="21E64D3DFF997865A4B9FA4238C6BEA3" authority="Senna, 1892" authorityName="Senna" authorityYear="1892" box="[151,674,1434,1460]" class="Insecta" family="Brentidae" genus="Hoplopisthius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="subGenus" subGenus="Hoplopisthius">
