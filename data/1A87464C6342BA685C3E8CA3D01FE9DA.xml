<taxonomicName id="0D2F5C46C48C4EE6C184D8848C7715E5" class="Arachnida" family="Linyphiidae" genus="Erigone" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="194" phylum="Arthropoda" rank="species" species="subitanea">subitanea</taxonomicName>
