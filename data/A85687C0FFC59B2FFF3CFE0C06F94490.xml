<quantity id="E7079B33FFC59B2FFC51FD7B011E4298" box="[1018,1102,705,732]" metricMagnitude="5" metricUnit="m" metricValue="1.35" pageId="3" pageNumber="584" unit="km" value="135.0">135 km</quantity>
south of 
<location id="2520600DFFC59B2FFB1FFD7B001C4298" LSID="urn:lsid:plazi:treatment:A85687C0FFC59B2FFF3CFE0C06F94490:2520600DFFC59B2FFB1FFD7B001C4298" box="[1204,1356,705,731]" country="Tanzania" latitude="-4.15" longLatPrecision="1307" longitude="32.716667" name="Lake Victoria" pageId="3" pageNumber="584">Lake Victoria</location>
and 
<locationDeviation id="31F22FF4FFC59B2FFA2BFD7B04D042BC" location="Kahama" pageId="3" pageNumber="584">
<quantity id="E7079B33FFC59B2FFA2BFD7B05EB42BC" metricMagnitude="4" metricUnit="m" metricValue="3.0" pageId="3" pageNumber="584" unit="km" value="30.0">30 km</quantity>
SSW of Kahama
</locationDeviation>
, with coordinates 
<geoCoordinate id="45CB5011FFC59B2FFDE6FD5F07FA42BC" box="[589,682,741,767]" degrees="04" direction="south" minutes="09" orientation="latitude" pageId="3" pageNumber="584" precision="925" value="-4.15">04°09’S</geoCoordinate>
, 
<geoCoordinate id="45CB5011FFC59B2FFD1CFD5F064842BC" box="[695,792,741,767]" degrees="32" direction="east" minutes="43" orientation="longitude" pageId="3" pageNumber="584" precision="925" value="32.716667">32°43’E</geoCoordinate>
</materialsCitation>
