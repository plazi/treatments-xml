<taxonomicName id="CDA04D5FAF46FFF58235FB44FED8FB75" authority="Cope" authorityName="Cope ED" authorityYear="1876" baseAuthorityName="Cope" class="Reptilia" family="Plesiosauria (awaiting allocation)" genus="Uronautes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plesiosauria" pageId="12" pageNumber="352" phylum="Chordata" rank="species" species="cetiformis">Uronautes cetiformis</taxonomicName>
