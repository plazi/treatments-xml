, 
<geoCoordinate id="96DDD0C5FFEA171DFF39FD85FE8412F1" box="[151,257,1493,1519]" direction="east" orientation="longitude" pageId="3" pageNumber="216" precision="925" value="120.86667">
<number id="4521A425FFEA171DFF39FD85FF4412F1" box="[151,193,1495,1519]" pageId="3" pageNumber="216" value="120.0">120</number>
o 
<number id="4521A425FFEA171DFF66FD85FF6D12F1" box="[200,232,1495,1519]" pageId="3" pageNumber="216" value="52.0">52</number>
’E
</geoCoordinate>
, 
<quantity id="34111BE7FFEA171DFEA3FD85FED112F1" box="[269,340,1495,1519]" metricMagnitude="2" metricUnit="m" metricValue="3.9" pageId="3" pageNumber="216" unit="m" value="390.0">390 m</quantity>
elevation, by light trapping, 
<date id="875790C2FFEA171DFD39FD85FCF212F1" box="[663,887,1495,1520]" pageId="3" pageNumber="216" value="1996-08-13" valueMax="1996-08-14" valueMin="1996-08-13">
