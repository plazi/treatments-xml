<taxonomicName id="0EE118870475FFF6FF33F9AD7713164D" authorityName="J.V.F.Lamouroux" authorityYear="1809" box="[151,257,1632,1658]" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" order="Bryopsidales" pageId="37" pageNumber="38" phylum="Chlorophyta" rank="genus">
