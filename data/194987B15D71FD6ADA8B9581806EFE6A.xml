<taxonomicName id="56E04D245D71FD69D84D965D80AAF9EC" authority="Rafinesque, 1810" authorityName="Rafinesque" authorityYear="1810" box="[593,930,1552,1577]" class="Actinopterygii" family="Bothidae" genus="Bothus" kingdom="Animalia" order="Pleuronectiformes" pageId="13" pageNumber="168" phylum="Chordata" rank="species" species="rumulo">
