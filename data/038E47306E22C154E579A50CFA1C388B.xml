<taxonomicName id="4C278DA56E23C154E027A07BFB7C3F07" authorityName="Gray &amp; Smith" authorityYear="2008" box="[1059,1248,851,872]" class="Arachnida" family="Stiphidiidae" genus="Karriella" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="36" phylum="Arthropoda" rank="species" species="treenensis">
<emphasis id="B9532A346E23C154E027A07BFB7C3F07" box="[1059,1248,851,872]" italics="true" pageId="23" pageNumber="36">Karriella treenensis</emphasis>
</taxonomicName>
(KS34750); (
<emphasis id="B9532A346E23C154E166A07BFAE33F07" box="[1378,1407,851,872]" italics="true" pageId="23" pageNumber="36">c,d</emphasis>
), 
<taxonomicName id="4C278DA56E23C154E681A046FC8D3FEC" authorityName="Gray &amp; Smith" authorityYear="2008" box="[645,785,878,899]" class="Arachnida" family="Stiphidiidae" genus="Karriella" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="36" phylum="Arthropoda" rank="species" species="walpolensis">
<emphasis id="B9532A346E23C154E681A046FC8D3FEC" box="[645,785,878,899]" italics="true" pageId="23" pageNumber="36">K. walpolensis</emphasis>
