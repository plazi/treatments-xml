Basin, 
<number id="3DFDD073FF94757F4ACFFDE41CBBFDE4" box="[687,746,533,559]" pageId="2" pageNumber="63" value="3570.0">3570</number>
metres; 
<taxonomicName id="4C35B9D7FF94757F4B2DFDE41DFCFDE4" box="[845,941,533,559]" class="Malacostraca" family="Leptognathiidae" genus="Leptognathia" kingdom="Animalia" order="Tanaidacea" pageId="2" pageNumber="63" phylum="Arthropoda" rank="species" species="langi">
<emphasis id="B9411E46FF94757F4B2DFDE41DFCFDE4" bold="true" box="[845,941,533,559]" italics="true" pageId="2" pageNumber="63">A. langi</emphasis>
</taxonomicName>
; 
<taxonomicName id="4C35B9D7FF94757F4BDCFDE41A73FDE4" box="[956,1058,533,559]" class="Malacostraca" family="Anarthruridae" genus="Anarthruropsis" kingdom="Animalia" order="Tanaidacea" pageId="2" pageNumber="63" phylum="Arthropoda" rank="species" species="longa">
<emphasis id="B9411E46FF94757F4BDCFDE41A73FDE4" bold="true" box="[956,1058,533,559]" italics="true" pageId="2" pageNumber="63">A. longa</emphasis>
</taxonomicName>
; 
<taxonomicName id="4C35B9D7FF94757F4C52FDE41E85FD9C" authority="Kudinova-Pasternak, 1990" authorityName="Kudinova-Pasternak" authorityYear="1990" class="Malacostraca" family="Anarthruridae" genus="Anarthruropsis" kingdom="Animalia" order="Tanaidacea" pageId="2" pageNumber="63" phylum="Arthropoda" rank="species" species="similis">
<emphasis id="B9411E46FF94757F4C52FDE41ACFFDE5" box="[1074,1182,533,558]" italics="true" pageId="2" pageNumber="63">A. similis</emphasis>
