<quantity id="4C9B4DBF6B3CFFACF0C0622BFD12F954" box="[678,766,1648,1672]" metricMagnitude="2" metricUnit="m" metricValue="2.4699999999999998" metricValueMax="4.89" metricValueMin="0.05" pageId="6" pageNumber="765" unit="m" value="247.0" valueMax="489.0" valueMin="5.0">5-489 m</quantity>
(
<quantity id="4C9B4DBF6B3CFFACF168622BFCB6F954" box="[782,858,1648,1672]" metricMagnitude="1" metricUnit="m" metricValue="2.9" metricValueMax="5.3" metricValueMin="0.5" pageId="6" pageNumber="765" unit="m" value="29.0" valueMax="53.0" valueMin="5.0">5-53 m</quantity>
ATELIER 
<taxonomicName id="4C639BD96B3CFFACF1A9622BFBE1F954" box="[975,1037,1648,1672]" class="Gastropoda" family="Muricidae" genus="Rapa" kingdom="Animalia" order="Neogastropoda" pageId="6" pageNumber="765" phylum="Mollusca" rank="genus">RAPA</taxonomicName>
