and 
<taxonomicName id="4C6B4D6DFB5DB3096A59993CFC8A8E65" baseAuthorityName="Hubner" baseAuthorityYear="1808" box="[592,785,1618,1643]" class="Insecta" family="Noctuidae" genus="Eublemma" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="56" phylum="Arthropoda" rank="species" species="parva">
