<taxonomicName id="4C6B4D6DFB5DB3096BF69940FB678E49" box="[1023,1276,1582,1607]" class="Magnoliopsida" family="Asteraceae" genus="Limbarda" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="56" phylum="Tracheophyta" rank="species" species="crithmoides">
