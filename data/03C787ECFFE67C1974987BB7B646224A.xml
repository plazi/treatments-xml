</taxonomicName>
. 
<taxonomicName id="4C6E4D79FFE67C19755C7DFCB04C26AA" authorityName="Borradaile" authorityYear="1907" box="[375,638,1483,1509]" class="Malacostraca" family="Odontodactylidae" genus="Odontodactylus" kingdom="Animalia" order="Stomatopoda" pageId="3" pageNumber="376" phylum="Arthropoda" rank="species" species="latirostris">
