<paragraph id="D75236BAFFE9FFEBFF6B7AF9FCF17C2A" blockId="69.[189,1399,1517,1614]" pageId="69" pageNumber="70">
<taxonomicName id="10ED4D39FFE9FFEBFF6B7AF9FEA87C63" authorityName="Hyatt" authorityYear="1894" box="[189,374,1517,1543]" class="Cephalopoda" family="Trocholitidae" genus="Trocholitoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tarphycerida" pageId="69" pageNumber="70" phylum="Mollusca" rank="genus">
