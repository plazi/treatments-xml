<taxonomicName id="EBD035AA961D2210FD57FC98FCFB3E21" authorityName=", Swirski &amp; Golan" authorityYear="1967" box="[752,891,811,835]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="532" phylum="Arthropoda" rank="species" species="largoensis">
