<taxonomicName id="EBD035AA961D2210FB4EFCBAFAE03E43" authorityName=", Swirski &amp; Golan" authorityYear="1967" box="[1257,1376,777,801]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="532" phylum="Arthropoda" rank="species" species="largoensis">
