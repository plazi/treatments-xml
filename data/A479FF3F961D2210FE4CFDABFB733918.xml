<taxonomicName id="EBD035AA961D2210FCDDFCBAFC713E43" authorityName=", Swirski &amp; Golan" authorityYear="1967" box="[890,1009,777,801]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="532" phylum="Arthropoda" rank="species" species="largoensis">
