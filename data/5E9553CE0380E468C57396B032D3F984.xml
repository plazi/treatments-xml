species from China. 1 
<taxonomicName id="A18EC3B3CD43AD72B3DA75F47A539884" class="Arachnida" family="Agelenidae" genus="Papiliocoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papiliocoelotes guanyinensis" order="Araneae" pageId="10" pageNumber="43" phylum="Arthropoda" rank="species" species="guanyinensis">Papiliocoelotes guanyinensis</taxonomicName>
sp. n. 2 
<taxonomicName id="52AEB17B51BB99013A3DC95EFB6F7251" class="Arachnida" family="Agelenidae" genus="Papiliocoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papiliocoelotes guitangensis" order="Araneae" pageId="10" pageNumber="43" phylum="Arthropoda" rank="species" species="guitangensis">Papiliocoelotes guitangensis</taxonomicName>
sp. n. 3 
<taxonomicName id="449B3EC4D96BBFC73AA883F07367559C" class="Arachnida" family="Agelenidae" genus="Papiliocoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papiliocoelotes jiepingensis" order="Araneae" pageId="10" pageNumber="43" phylum="Arthropoda" rank="species" species="jiepingensis">Papiliocoelotes jiepingensis</taxonomicName>
sp. n. 4 
<taxonomicName id="329AC57404FEB0537C3170CA433B9D22" class="Arachnida" family="Agelenidae" genus="Papiliocoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papiliocoelotes meiyuensis" order="Araneae" pageId="10" pageNumber="43" phylum="Arthropoda" rank="species" species="meiyuensis">Papiliocoelotes meiyuensis</taxonomicName>
sp. n. 5 
<taxonomicName id="7D8C9A720DA0FA93412F49D215256AD7" class="Arachnida" family="Agelenidae" genus="Papiliocoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Papiliocoelotes yezhouensis" order="Araneae" pageId="10" pageNumber="43" phylum="Arthropoda" rank="species" species="yezhouensis">Papiliocoelotes yezhouensis</taxonomicName>
sp. n.
</paragraph>
</caption>
