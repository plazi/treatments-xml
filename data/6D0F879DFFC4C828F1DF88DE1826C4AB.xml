<geoCoordinate id="8092504CFFC4C828F050894F18AAC18C" box="[295,422,888,913]" degrees="23" direction="north" minutes="58" orientation="latitude" pageId="9" pageNumber="370" precision="15" seconds="32" value="23.975555">23°58′32″N</geoCoordinate>
