<taxonomicName id="4C4D4D06A149047ABBC163DEFDC86C8A" authority="Matschie, 1907" authorityName="Matschie" authorityYear="1907" box="[151,606,975,1001]" class="Mammalia" family="Erinaceidae" genus="Erinaceus" kingdom="Animalia" order="Erinaceomorpha" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="tschifuensis">
