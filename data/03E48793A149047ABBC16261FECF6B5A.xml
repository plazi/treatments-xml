<quantity id="4CB59B60A149047AB97563C1FDC86C8A" box="[547,606,975,1001]" metricMagnitude="1" metricUnit="m" metricValue="4.84378" pageId="8" pageNumber="9" unit="in" value="1907.0">1907</quantity>
</taxonomicName>
