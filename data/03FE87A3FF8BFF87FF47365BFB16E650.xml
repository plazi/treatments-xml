<taxonomicName id="4C574D36FF8BFF84FEFD36F1FD46E48D" authority="Zeller, 1839" authorityName="Zeller" authorityYear="1839" box="[301,761,1688,1711]" class="Insecta" family="Gelechiidae" genus="Gelechia" kingdom="Animalia" order="Lepidoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="albiceps" subGenus="Brachmia">
