<subSubSection pageId="3" pageNumber="376" type="discussion">
<paragraph blockId="3.[151,1436,151,351]" pageId="3" pageNumber="376">
<emphasis bold="true" box="[199,315,254,279]" pageId="3" pageNumber="376">Remarks.</emphasis>
<taxonomicName box="[321,520,254,279]" class="Insecta" family="Arctiidae" genus="Paraheliosia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="376" phylum="Arthropoda" rank="species" species="rufa">
<emphasis box="[321,520,254,279]" italics="true" pageId="3" pageNumber="376">Paraheliosia rufa</emphasis>
</taxonomicName>
differs from 
<taxonomicName box="[666,778,254,278]" class="Insecta" family="Arctiidae" genus="Paraheliosia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="376" phylum="Arthropoda" rank="species" species="elegans">
<emphasis box="[666,778,254,278]" italics="true" pageId="3" pageNumber="376">P. elegans</emphasis>
</taxonomicName>
