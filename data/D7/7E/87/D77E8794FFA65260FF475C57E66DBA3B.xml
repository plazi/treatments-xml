<taxonomicName authority="Wang &amp; Liu, 2019" authorityName="Wang &amp; Liu" authorityYear="2019" box="[609,744,1758,1781]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="291" phylum="Arthropoda" rank="species" species="pectinacea" status="sp. nov.">
<emphasis box="[609,744,1758,1781]" italics="true" pageId="5" pageNumber="291">P. pectinacea</emphasis>
</taxonomicName>
<emphasis bold="true" box="[750,830,1757,1781]" pageId="5" pageNumber="291">
<taxonomicNameLabel box="[750,830,1757,1781]" pageId="5" pageNumber="291" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype, slide No. ZMUC-NK019; 11, 
<taxonomicName authority="Wang &amp; Liu, 2019" authorityName="Wang &amp; Liu" authorityYear="2019" box="[1248,1343,1757,1781]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="291" phylum="Arthropoda" rank="species" species="pileata" status="sp. nov.">
