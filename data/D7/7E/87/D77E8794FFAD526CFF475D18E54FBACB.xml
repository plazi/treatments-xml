<taxonomicName authorityName="Meyrick" authorityYear="1908" box="[543,666,1946,1970]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="295" phylum="Arthropoda" rank="genus">
<emphasis box="[543,666,1946,1970]" italics="true" pageId="9" pageNumber="295">Promalactis</emphasis>
</taxonomicName>
spp. 15, 
<taxonomicName authority="Wang &amp; Liu, 2019" authorityName="Wang &amp; Liu" authorityYear="2019" box="[755,892,1946,1970]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="295" phylum="Arthropoda" rank="species" species="bigeminata" status="sp. nov.">
<emphasis box="[755,892,1946,1970]" italics="true" pageId="9" pageNumber="295">P. bigeminata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[898,977,1946,1970]" pageId="9" pageNumber="295">
<taxonomicNameLabel box="[898,977,1946,1970]" pageId="9" pageNumber="295" rank="species">sp. nov.</taxonomicNameLabel>
