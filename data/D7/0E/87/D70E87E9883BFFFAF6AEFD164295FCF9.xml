<typeStatus box="[180,281,946,972]" pageId="25" pageNumber="248">paratype</typeStatus>
<number box="[288,303,946,972]" pageId="25" pageNumber="248" value="1.0">1</number>
on six slides, labelled 
<date box="[570,685,946,972]" pageId="25" pageNumber="248" value="1979-02-06">
<number box="[570,584,946,972]" pageId="25" pageNumber="248" value="6.0">6</number>
­II­ 
<number box="[623,685,946,972]" pageId="25" pageNumber="248" value="1979.0">1979</number>
</date>
, KH­ 
<number box="[755,787,946,972]" pageId="25" pageNumber="248" value="79.0">79</number>
­ 
<number box="[795,805,946,972]" pageId="25" pageNumber="248" value="1.0">1</number>
, St. 
<number box="[856,872,946,972]" pageId="25" pageNumber="248" value="5.0">5</number>
, A­ 
<number box="[920,936,946,972]" pageId="25" pageNumber="248" value="2.0">2</number>
/ 
<number box="[946,980,946,972]" pageId="25" pageNumber="248" value="3.5" valueMax="6.0" valueMin="1.0">1­6</number>
, 
<typeStatus box="[992,1093,946,972]" pageId="25" pageNumber="248">paratype</typeStatus>
<number box="[1100,1115,946,972]" pageId="25" pageNumber="248" value="3.0">3</number>
on six slides, labelled 
<date pageId="25" pageNumber="248" value="1979-02-06">
<number box="[1382,1397,946,972]" pageId="25" pageNumber="248" value="6.0">6</number>
­II­ 
<number box="[151,212,986,1012]" pageId="25" pageNumber="248" value="1979.0">1979</number>
</date>
, KH­ 
<number box="[283,315,986,1012]" pageId="25" pageNumber="248" value="79.0">79</number>
­ 
<number box="[323,333,986,1012]" pageId="25" pageNumber="248" value="1.0">1</number>
