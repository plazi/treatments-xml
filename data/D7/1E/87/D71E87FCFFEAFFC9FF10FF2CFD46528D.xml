<collectingCountry box="[379,482,899,923]" name="Jamaica" pageId="12" pageNumber="11">Jamaica</collectingCountry>
, 
<collectingRegion box="[494,611,899,923]" country="Jamaica" name="Trelawny" pageId="12" pageNumber="11">Trelawny</collectingRegion>
