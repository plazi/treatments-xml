<subSubSection pageId="47" pageNumber="46" type="etymology">
<paragraph blockId="47.[192,1440,1795,1851]" pageId="47" pageNumber="46">
<emphasis bold="true" box="[192,339,1795,1819]" pageId="47" pageNumber="46">Etymology</emphasis>
