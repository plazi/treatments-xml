<collectingCountry box="[1259,1375,680,706]" name="Argentina" pageId="13" pageNumber="464">Argentina</collectingCountry>
: 
<collectingRegion country="Argentina" name="Corrientes" pageId="13" pageNumber="464">Corrientes</collectingRegion>
