<materialsCitation ID-GBIF-Occurrence="1416805658" collectingDate="1914-02-10" collectingDateMax="1914-02-11" collectingDateMin="1914-02-10" collectionCode="NMNH" location="New York" pageId="17" pageNumber="318" specimenCount="1" stateProvince="New York" typeStatus="syntype">
One male (
<date pageId="17" pageNumber="318" value="1914-02-10">
<collectingDate pageId="17" pageNumber="318" value="1914-02-10">10- ii-1914</collectingDate>
</date>
), three females (10 &amp; 
<date box="[485,605,1332,1357]" pageId="17" pageNumber="318" value="1914-02-11">
<collectingDate box="[485,605,1332,1357]" pageId="17" pageNumber="318" value="1914-02-11">11-ii-1914</collectingDate>
</date>
