BENTHAUS, stn DW 2013, 
<geoCoordinate box="[1120,1205,1225,1249]" degrees="22" direction="south" minutes="39" orientation="latitude" pageId="15" pageNumber="348" precision="925" value="-22.65">22°39’S</geoCoordinate>
, 
<geoCoordinate box="[677,778,1252,1276]" degrees="152" direction="west" minutes="50" orientation="longitude" pageId="15" pageNumber="348" precision="925" value="-152.83333">152°50’W</geoCoordinate>
, 
<quantity box="[787,875,1252,1276]" metricMagnitude="1" metricUnit="m" metricValue="8.65" metricValueMax="9.3" metricValueMin="8.0" pageId="15" pageNumber="348" unit="m" value="86.5" valueMax="93.0" valueMin="80.0">80-93 m</quantity>
