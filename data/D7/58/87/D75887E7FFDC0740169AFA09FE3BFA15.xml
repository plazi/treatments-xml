<collectingCountry box="[115,171,1455,1479]" name="Japan" pageId="2" pageNumber="335">Japan</collectingCountry>
, 
<collectingCountry box="[179,363,1455,1479]" name="Papua New Guinea" pageId="2" pageNumber="335">Papua New Guinea</collectingCountry>
, 
<collectingCountry box="[371,520,1455,1479]" name="New Caledonia" pageId="2" pageNumber="335">New Caledonia</collectingCountry>
