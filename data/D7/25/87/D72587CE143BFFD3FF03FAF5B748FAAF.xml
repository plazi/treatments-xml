<emphasis box="[875,1131,756,783]" italics="true" pageId="2" pageNumber="179">Cornigerius maeoticus</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Spandl" authorityYear="1923" box="[1184,1312,756,783]" class="Branchiopoda" family="Podonidae" genus="Cornigerius" kingdom="Animalia" order="Diplostraca" pageId="2" pageNumber="179" phylum="Arthropoda" rank="species" species="lacustris">
<emphasis box="[1184,1312,756,783]" italics="true" pageId="2" pageNumber="179">C. lacustris</emphasis>
</taxonomicName>
from the Black Sea (BS) and Caspian Lake (CL), and from Lake Hazar (LH), 
<collectingCountry box="[294,376,792,819]" name="Turkey" pageId="2" pageNumber="179">Turkey</collectingCountry>
