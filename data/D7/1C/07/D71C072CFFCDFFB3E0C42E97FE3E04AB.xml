<paragraph blockId="4.[151,1436,1744,2023]" box="[151,1253,1744,1771]" pageId="4" pageNumber="445">
<emphasis bold="true" box="[151,308,1744,1770]" pageId="4" pageNumber="445">
<typeStatus box="[151,210,1744,1770]" pageId="4" pageNumber="445">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Paulian, 1968" authorityName="Paulian" authorityYear="1968" box="[316,976,1744,1771]" class="Insecta" family="Hybosoridae" genus="Philharmostes" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="445" phylum="Arthropoda" rank="species" species="zicsii" subGenus="Melanophilharmostes">
<emphasis box="[316,478,1744,1770]" italics="true" pageId="4" pageNumber="445">Philharmostes</emphasis>
(
<emphasis box="[494,738,1744,1770]" italics="true" pageId="4" pageNumber="445">Melanophilharmostes</emphasis>
