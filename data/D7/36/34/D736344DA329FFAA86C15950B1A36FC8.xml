</taxonomicName>
(
<taxonomicName box="[390,536,330,357]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="6" pageNumber="323" phylum="Basidiomycota" rank="species" species="versicolor">
<emphasis box="[390,406,331,356]" italics="true" pageId="6" pageNumber="323">T</emphasis>
. 
<emphasis box="[422,536,330,356]" italics="true" pageId="6" pageNumber="323">versicolor</emphasis>
</taxonomicName>
, 
<taxonomicName box="[551,698,330,357]" class="Agaricomycetes" family="Polyporaceae" genus="Trametes" kingdom="Fungi" order="Polyporales" pageId="6" pageNumber="323" phylum="Basidiomycota" rank="species" species="pubescens">
<emphasis box="[551,567,331,356]" italics="true" pageId="6" pageNumber="323">T</emphasis>
. 
<emphasis box="[583,698,330,356]" italics="true" pageId="6" pageNumber="323">pubescens</emphasis>
</taxonomicName>
