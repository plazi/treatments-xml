, 
<collectingCountry box="[613,706,655,681]" name="Georgia" pageId="7" pageNumber="324">Georgia</collectingCountry>
, 
<collectingCountry box="[722,770,656,682]" name="Iran" pageId="7" pageNumber="324">Iran</collectingCountry>
, 
<collectingCountry box="[787,862,655,681]" name="Russia" pageId="7" pageNumber="324">Russia</collectingCountry>
