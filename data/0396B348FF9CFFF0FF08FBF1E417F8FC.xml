<number id="3DF71079FF9DFFF3FEAAFF51E708FF03" box="[309,347,188,212]" pageId="41" pageNumber="42" value="7.5">7.5</number>
; length of mid femur 10.0; length of hind femur 
<number id="3DF71079FF9DFFF3FC1CFF51E5EBFF03" box="[899,952,188,212]" pageId="41" pageNumber="42" value="23.5">23.5</number>
