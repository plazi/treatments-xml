<number id="3DF71079FF9DFFF3FB0BFF51E2E9FF03" box="[1172,1210,188,212]" pageId="41" pageNumber="42" value="1.9">1.9</number>
; length of cerci 
<number id="3DF71079FF9DFFF3FAEEFF51E3C4FF03" box="[1393,1431,188,212]" pageId="41" pageNumber="42" value="3.7">3.7</number>
