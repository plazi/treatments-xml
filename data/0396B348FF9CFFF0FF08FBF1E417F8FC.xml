<number id="3DF71079FF9CFFF2FD2AF897E4BAF845" box="[693,745,1914,1938]" pageId="40" pageNumber="41" value="16.5">16.5</number>
; length of pronotum 4.0; length of tegmen 
<number id="3DF71079FF9CFFF2FB54F897E2ACF845" box="[1227,1279,1914,1938]" pageId="40" pageNumber="41" value="31.2">31.2</number>
