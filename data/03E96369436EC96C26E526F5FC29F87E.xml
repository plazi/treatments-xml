<quantity id="4CB87F9A436EC96C27AB27DDFDAFF87E" box="[473,563,1991,2015]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="9" pageNumber="10" unit="m" value="1000.0">1000 m</quantity>
