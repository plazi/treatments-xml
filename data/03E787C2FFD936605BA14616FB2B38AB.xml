</taxonomicName>
, 
<taxonomicName id="4C4E4D57FFD836605E2C46D7FF16388B" authority="Thunb." authorityName="Thunb." class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="62" phylum="Tracheophyta" rank="species" species="rugosa">
<emphasis id="B93AEAC6FFD836605E2C46D7FA6838E9" box="[1306,1436,1946,1967]" italics="true" pageId="1" pageNumber="62">Rosa rugosa</emphasis>
Thunb.
</taxonomicName>
, and 
<taxonomicName id="4C4E4D57FFD836605A1646F4FE19388B" authority="Pall" authorityName="Pall" box="[288,493,1975,1998]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="62" phylum="Tracheophyta" rank="species" species="davurica">
