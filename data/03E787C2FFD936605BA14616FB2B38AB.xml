<taxonomicName id="4C4E4D57FFD836605F2C46D7FAFB38E9" authority="Thunb." authorityName="Thunb." box="[1050,1295,1945,1967]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="62" phylum="Tracheophyta" rank="species" species="multiflora">
