<taxonomicName id="4C4E4D57FFD836605835469BFBA238AB" authority="Matsmura 1917" authorityName="Matsmura" authorityYear="1917" box="[771,1110,2006,2029]" class="Insecta" family="Aphididae" genus="Sitobion" kingdom="Animalia" order="Aphidomorpha" pageId="1" pageNumber="62" phylum="Arthropoda" rank="species" species="ibarae">
