<quantity id="4CC69B3EFF9FCD26FC0B46CDFBB54ECC" box="[977,1070,1201,1227]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="17" pageNumber="18" unit="mm" value="0.8">0.8 mm</quantity>
