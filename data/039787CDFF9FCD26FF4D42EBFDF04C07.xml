<quantity id="4CC69B3EFF9FCD26FACE46CDFAE94ECC" box="[1300,1394,1201,1227]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="17" pageNumber="18" unit="mm" value="1.8">1.8 mm</quantity>
