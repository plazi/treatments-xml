<number id="1E330D6855566E1EFA99F923FAC7F926" box="[1305,1329,1676,1697]" pageId="1" pageNumber="52" value="21.0">21</number>
</figureCitation>
) 
<taxonomicName id="6FFB64CC55566E1EFAC9F923FA6AF926" box="[1353,1436,1676,1697]" class="Insecta" family="Thripidae" genus="Scirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="52" phylum="Arthropoda" rank="species" species="spinosus">
