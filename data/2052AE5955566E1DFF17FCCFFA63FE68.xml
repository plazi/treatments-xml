<keyLead id="130ABC7A55566E1EFF17F893FA60F8E9" pageId="1" pageNumber="52">
<number id="1E330D6855566E1EFF17F893FF53F8D6" box="[151,165,1852,1873]" pageId="1" pageNumber="52" value="9.0">9</number>
