<taxonomicName id="D3DBE2EC8F73FFE1FEACF8F4FE4FF8F5" box="[283,457,1861,1883]" class="Insecta" family="Hydroptilidae" genus="Microptila" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="108" phylum="Arthropoda" rank="species" species="genka" status="sp. nov.">
<emphasis id="26AF457D8F73FFE1FEACF8F4FE4FF8F5" box="[283,457,1861,1883]" italics="true" pageId="4" pageNumber="108">Microptila genka</emphasis>
</taxonomicName>
