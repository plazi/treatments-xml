<taxonomicName id="5D9869D4540E2E621C2CCD79FE02CA0E" baseAuthorityName="Hartmann" baseAuthorityYear="1965" box="[256,445,539,560]" class="Ostracoda" family="Cyprididae" genus="Tanycypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="20" pageNumber="421" phylum="Arthropoda" rank="species" species="marina">
