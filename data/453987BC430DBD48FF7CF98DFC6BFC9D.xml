<bibRefCitation id="A9014B5B4302BD48FC61FE64FB8CFE6A" author="Provenzano" box="[906,1123,439,463]" pageId="8" pageNumber="34" refString="Provenzano, A. J. (1961) Pagurid crabs (Decapoda: Anomura) from St. John, Virgin Islands, with descriptions of three new species. Crustaceana, 3, 151 - 166." type="journal article" year="1961">
Provenzano (
<number id="7B58248D4302BD48FBCAFE6BFBB4FE6A" box="[1057,1115,439,463]" pageId="8" pageNumber="34" value="1961.0">1961</number>
)
</bibRefCitation>
and 
<bibRefCitation id="A9014B5B4302BD48FB4AFE6BFAB4FE6A" author="Williams" box="[1185,1371,439,463]" pageId="8" pageNumber="34" refString="Williams, A. B. (1965) Marine decapod crustaceans of the Carolinas. Fishery Bulletin of the Fish and Wildlife Service, 65 (1), 1 - 298." type="journal article" year="1965">
Williams (
<number id="7B58248D4302BD48FAF2FE6BFABCFE6A" box="[1305,1363,439,463]" pageId="8" pageNumber="34" value="1965.0">1965</number>
)
</bibRefCitation>
cited 
<taxonomicName id="0A904D294302BD48FF7CFE01FDD4FE51" authority="Stimpson, 1859" authorityName="Stimpson" authorityYear="1859" box="[151,571,476,500]" class="Malacostraca" family="Paguridae" genus="Spiropagurus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="34" phylum="Arthropoda" rank="species" species="dispar">
