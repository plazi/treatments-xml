<taxonomicName id="0A904D294302BD48FD2CFD69FC8DFD69" box="[711,866,692,716]" class="Malacostraca" family="Paguridae" genus="Iridopagurus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="34" phylum="Arthropoda" rank="species" species="caribbensis">
