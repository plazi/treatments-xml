<taxonomicName id="0A904D294302BD48FDFEFD05FD4AFD55" box="[533,677,728,752]" class="Malacostraca" family="Paguridae" genus="Iridopagurus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="34" phylum="Arthropoda" rank="species" species="reticulatus">
<emphasis id="FFE4EAB84302BD48FDFEFD05FD4AFD55" box="[533,677,728,752]" italics="true" pageId="8" pageNumber="34">I. reticulatus</emphasis>
</taxonomicName>
