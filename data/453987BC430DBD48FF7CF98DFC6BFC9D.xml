<taxonomicName id="0A904D294302BD48FC0AFD94FBAAFDC5" box="[993,1093,584,608]" class="Malacostraca" family="Paguridae" genus="Iridopagurus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="34" phylum="Arthropoda" rank="species" species="dispar">
<emphasis id="FFE4EAB84302BD48FC0AFD94FBAAFDC5" box="[993,1093,584,608]" italics="true" pageId="8" pageNumber="34">S. dispar</emphasis>
</taxonomicName>
