<taxonomicName id="DB974D55FFD2FF8AFBD76D39FBEDF04E" authorityName="Dumitrica &amp; Zügel" authorityYear="2003" box="[1053,1119,1040,1063]" class="Polycystina" family="Ultranaporidae" genus="Napora" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="52" pageNumber="57" phylum="Radiozoa" rank="species" species="aranea">
