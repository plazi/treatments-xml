8593, 8590, 8771; 
<collectionCode id="49FA36BDDA21E877FE26FE9D6E77FECC" box="[480,555,332,356]" country="Venezuela" httpUri="http://grbio.org/cool/ar2q-hqc7" name="Museo de la Estacion Biologia de Rancho Grande" pageId="14" pageNumber="89">EBRG</collectionCode>
14454) are: skull length, 23.5, 23.4, 23.3, 23.7; condyloincisive length, 22.1, 22.1, 22.3, 22.2; condylocanine length, 20.9, 20.8, 20.8, 21.0; postorbital breadth, 5.9, 5.9, 5.9, 6.2; braincase breadth, 10.8, 10.9, 10.4, 10.4; mastoid breadth, 12.0, 12.2, 12.1, 12.1; zygomatic breadth, 13.6, 13.8, 13.8, 13.1; maxillary toothrow length, 6.7, 6.8, 6.9, 6.8; M2-M2 breadth, 7.8, 8.0, 8.2, 8.2; dentary length, 15.0, 15.1, 15.4, 15.4; mandibular toothrow length, 7.4, 7.7, 7.7, 7.7; forearm length, 44.3, 44.6, 45.1, 44.6; third metacarpal length, 41.4, 41.3, 42.1, 42.5; fourth metacarpal length, 41.3, 40.4, 41.9, 41.2; fifth metacarpal length, 41.6, 42.0, 43.9, 41.8; total length, 71.0, 69.0, 75.0, 67.0; hind-foot length, 14.0, 15.0, 15.0, 14.0; ear length, 17.5, 16.0, 18.0, 18.0; body mass, 22.5, 24.0, 23.0, 26.3.
</paragraph>
