<taxonomicName id="4C37D68BFFA6FF93FF33B1C3FEBE3289" authorityName="Linnaeus" authorityYear="1758" box="[198,273,151,177]" class="Bivalvia" family="Ostreidae" genus="Ostrea" kingdom="Animalia" order="Ostreida" pageId="7" pageNumber="108" phylum="Mollusca" rank="genus">
