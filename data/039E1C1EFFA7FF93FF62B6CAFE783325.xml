<taxonomicName id="4C37D68BFFA6FF93FD4DB1C3FC833289" box="[696,812,151,177]" class="Demospongiae" family="Clionaidae" genus="Cliona" kingdom="Animalia" order="Clionaida" pageId="7" pageNumber="108" phylum="Porifera" rank="species" species="undetermined">
