<taxonomicName id="4C37D68BFFA7FF92FC32B696FA8D35E4" baseAuthorityName="Halbert" baseAuthorityYear="1915" box="[967,1314,1986,2012]" class="Arachnida" family="Halacaridae" genus="Copidognathus" kingdom="Animalia" order="Trombidiformes" pageId="6" pageNumber="107" phylum="Arthropoda" rank="species" species="septentrionalis">
