<taxonomicName id="7EED4D008F7CB328FEF8FDFCFE62FDD9" box="[274,422,549,572]" class="Gastropoda" family="Pupinidae" genus="Pseudopomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="siyomensis">
<emphasis id="8B99EA918F7CB328FEF8FDFCFE62FDD9" box="[274,422,549,572]" italics="true" pageId="21" pageNumber="22">P. siyomensis</emphasis>
</taxonomicName>
and 
<tableCitation id="F46F03388F7CB328FE37FDFDFDF7FDD8" box="[477,563,548,573]" captionStart="TABLE 5" captionStartId="11.[151,239,728,751]" captionTargetBox="[159,1411,778,986]" captionTargetPageId="11" captionText="TABLE 5. Shell characters found to be different between specimens within the same museum samples (India)." httpUri="http://table.plazi.org/id/ED92660B8F62B336FF7DFD01FAF0FD0A" pageId="21" pageNumber="22" tableUuid="ED92660B8F62B336FF7DFD01FAF0FD0A">
