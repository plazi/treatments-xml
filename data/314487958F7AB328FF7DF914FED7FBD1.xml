<taxonomicName id="7EED4D008F7CB328FC58FE60FB85FE35" box="[946,1089,439,464]" class="Gastropoda" family="Pupinidae" genus="Pseudopomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="himalayae">
<emphasis id="8B99EA918F7CB328FC58FE60FB85FE35" box="[946,1089,439,464]" italics="true" pageId="21" pageNumber="22">P. himalayae</emphasis>
</taxonomicName>
, 
<taxonomicName id="7EED4D008F7CB328FBBAFE60FB06FE35" box="[1104,1218,439,464]" class="Gastropoda" family="Pupinidae" genus="Pseudopomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="sophiae" status="n. sp.">
<emphasis id="8B99EA918F7CB328FBBAFE60FB06FE35" box="[1104,1218,439,464]" italics="true" pageId="21" pageNumber="22">P. sophiae</emphasis>
</taxonomicName>
<emphasis id="8B99EA918F7CB328FB22FE6EFAC3FE35" bold="true" box="[1224,1287,439,464]" pageId="21" pageNumber="22">
<taxonomicNameLabel id="90AA57EA8F7CB328FB22FE6EFAC3FE35" box="[1224,1287,439,464]" pageId="21" pageNumber="22" rank="species">n. sp.</taxonomicNameLabel>
