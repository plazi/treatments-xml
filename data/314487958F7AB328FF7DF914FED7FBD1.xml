<taxonomicName id="7EED4D008F7CB328FC0EFDD8FB8CFDFD" box="[996,1096,511,536]" class="Gastropoda" family="Pupinidae" genus="Pseudopomatias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="abletti" status="n. sp.">
<emphasis id="8B99EA918F7CB328FC0EFDD8FB8CFDFD" box="[996,1096,511,536]" italics="true" pageId="21" pageNumber="22">P. abletti</emphasis>
</taxonomicName>
<emphasis id="8B99EA918F7CB328FBA5FE26FB4AFDFD" bold="true" box="[1103,1166,511,536]" pageId="21" pageNumber="22">
<taxonomicNameLabel id="90AA57EA8F7CB328FBA5FE26FB4AFDFD" box="[1103,1166,511,536]" pageId="21" pageNumber="22" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
