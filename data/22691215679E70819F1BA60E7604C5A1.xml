<taxonomicName id="D74F3C70F49E6185BDAA20AB7F338FC1" class="Magnoliopsida" family="Fabaceae" genus="Prosopis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Prosopis juliflora" order="Fabales" pageId="12" pageNumber="51" phylum="Tracheophyta" rank="species" species="juliflora">Prosopis juliflora</taxonomicName>
