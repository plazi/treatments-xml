<emphasis id="B918FD3EFFE3051FDB15583FBB10FCB3" bold="true" box="[1053,1146,872,893]" pageId="6" pageNumber="249">syn. nov.</emphasis>
<taxonomicName id="4C6C5AAFFFE3051FDF9F58DFBD45FC52" authority="Sohi &amp; Mann 1992" authorityName="Sohi &amp; Mann" authorityYear="1992" box="[151,559,902,925]" class="Insecta" family="Cicadellidae" genus="Kapsa" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="249" phylum="Arthropoda" rank="species" species="thankotensis">
