<taxonomicName id="6D7F3568F639E90ED1FA9D8C2D8E9239" baseAuthorityName="Poiret" baseAuthorityYear="1801" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis antidiluviana" order="Pteropoda" pageId="218" pageNumber="219" phylum="Mollusca" rank="species" species="antidiluviana">
