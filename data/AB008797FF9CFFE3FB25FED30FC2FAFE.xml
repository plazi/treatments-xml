<taxonomicName id="E4A94D02FF9DFFE3F837FCB20FD9FB3E" authority="Martins &amp; Galileo, 1996" authorityName="Martins &amp; Galileo" authorityYear="1996" box="[94,465,1248,1272]" class="Insecta" family="Cerambycidae" genus="Fredlanea" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="106" phylum="Arthropoda" rank="species" species="maculata">
