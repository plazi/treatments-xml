<taxonomicName id="E4A94D02FF9DFFE3F91AFC920C76FB1E" authority="(Bates, 1881)" baseAuthorityName="Bates" baseAuthorityYear="1881" box="[371,638,1216,1240]" class="Insecta" family="Cerambycidae" genus="Fredlanea" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="106" phylum="Arthropoda" rank="species" species="aequatoria">
<emphasis id="11DDEA93FF9DFFE3F91AFC920FE2FB1E" box="[371,490,1217,1240]" italics="true" pageId="3" pageNumber="106">F. aequatoria</emphasis>
