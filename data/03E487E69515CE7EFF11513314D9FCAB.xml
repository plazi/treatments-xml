</emphasis>
, Esfahan, 
<date id="FFF310309515CE7CFDD251E415D3FE5C" box="[596,712,458,482]" pageId="1" value="1962-12-07">
<number id="3D8524D79515CE7CFDD251E41579FE5C" box="[596,610,458,482]" pageId="1" value="7.0">7</number>
.xii.1962
</date>
, 4 oviparous females, 
<specimenCount id="9D4BFD799515CE7CFC5451E4133FFE5C" box="[978,1060,458,482]" pageId="1" type="male">
<number id="3D8524D79515CE7CFC5451E414FBFE5C" box="[978,992,458,482]" pageId="1" value="1.0">1</number>
male
</specimenCount>
, from 
<taxonomicName id="4C4D4D739515CE7CFBF251E213DEFE5D" box="[1140,1221,460,483]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="1" phylum="Tracheophyta" rank="genus">
