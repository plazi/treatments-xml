<taxonomicName id="9CB64DB3FFC8EF78C4F5FF31FDE9FF12" authority="Moulton, 1932" authorityName="Moulton" authorityYear="1932" box="[151,586,151,176]" class="Insecta" family="Heterothripidae" genus="Heterothrips" kingdom="Animalia" order="Thysanoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="flavitibia">
<emphasis id="69C2EA22FFC8EF78C4F5FF31FDE9FF12" bold="true" box="[151,586,151,176]" pageId="11" pageNumber="12">
<emphasis id="69C2EA22FFC8EF78C4F5FF31FE34FF12" bold="true" box="[151,407,151,176]" italics="true" pageId="11" pageNumber="12">Heterothrips flavitibia</emphasis>
<bibRefCitation id="3F274BC1FFC8EF78C5FCFF3EFDE9FF12" author="Moulton" box="[414,586,151,176]" pageId="11" pageNumber="12" refString="Moulton, D. (1932) The Thysanoptera of South America I. Revista de Entomologia, 2, 451 - 484." type="journal article" year="1932">
Moulton, 
<number id="ED7E2417FFC8EF78C670FF31FDE9FF12" box="[530,586,151,176]" pageId="11" pageNumber="12" value="1932.0">1932</number>
</bibRefCitation>
</emphasis>
