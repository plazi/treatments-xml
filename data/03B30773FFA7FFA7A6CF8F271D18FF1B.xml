Basionym: 
<taxonomicName id="4C1ACDE6FFA7FFA0A74B8F4E1B3FF8B0" authority="Schlechter (1914: 442)" authorityName="Schlechter" authorityPageNumber="442" authorityYear="1914" box="[275,782,1884,1907]" class="Magnoliopsida" family="Apocynaceae" genus="Schistogyne" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="3" pageNumber="12" phylum="Tracheophyta" rank="species" species="boliviensis">
