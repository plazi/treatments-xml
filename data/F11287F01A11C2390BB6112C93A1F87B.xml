<subSubSection id="31A1656D1A11C2390BB611D594B7FC08" box="[151,904,931,954]" pageId="14" pageNumber="46" type="materials_examined">
<paragraph id="790436E61A11C2390BB611D594B7FC08" blockId="14.[151,904,931,954]" box="[151,904,931,954]" pageId="14" pageNumber="46">
<emphasis id="4BCFEAF41A11C2390BB611D59624FC0B" bold="true" box="[151,283,931,954]" pageId="14" pageNumber="46">
<typeStatus id="A60088441A11C2390BB611D597F5FC08" box="[151,202,932,953]" pageId="14" pageNumber="46">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="BEBB4D651A11C2390A0B11D59525FC0B" box="[298,538,932,954]" class="Demospongiae" family="Podospongiidae" genus="Diplopodospongia" kingdom="Animalia" order="Poecilosclerida" pageId="14" pageNumber="46" phylum="Porifera" rank="species" species="rara" status="gen. nov.">
<emphasis id="4BCFEAF41A11C2390A0B11D59525FC0B" box="[298,538,932,954]" italics="true" pageId="14" pageNumber="46">Diplopodospongia rara</emphasis>
</taxonomicName>
<emphasis id="4BCFEAF41A11C239090111D595EAFC08" bold="true" box="[544,725,932,953]" pageId="14" pageNumber="46">
<taxonomicNameLabel id="50FC578F1A11C239090111D59540FC08" box="[544,639,932,953]" pageId="14" pageNumber="46" rank="genus">gen. nov.</taxonomicNameLabel>
