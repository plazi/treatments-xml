<quantity id="4CC3779D4505571E2721FA6EFC8FFA03" box="[800,858,1497,1519]" metricMagnitude="4" metricUnit="m" metricValue="4.0" pageId="27" pageNumber="732" unit="km" value="40.0">40 km</quantity>
