<quantity id="4CC3779D4505571E2616FA46FD58F9EA" box="[535,653,1521,1542]" metricMagnitude="3" metricUnit="m" metricValue="2.6" metricValueMax="3.2" metricValueMin="2.0" pageId="27" pageNumber="732" unit="m" value="2600.0" valueMax="3200.0" valueMin="2000.0">2000-3200m</quantity>
