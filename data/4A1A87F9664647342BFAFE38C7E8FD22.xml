<taxonomicName id="05B34D6C664647342BFAFE76C2FEFE70" authority="Priesner, 1952: 200" authorityName="Priesner" authorityPageNumber="200" authorityYear="1952" box="[151,507,474,496]" class="Insecta" family="Phlaeothripidae" genus="Chaetokarnyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
