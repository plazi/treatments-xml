a.s.l.
</elevation>
; 
<location id="8E8C603CFFFBFE01FDDEF8B8C9956B13" LSID="urn:lsid:plazi:treatment:03FA87F1FFFBFE00FF0FF992C81A6DC8:8E8C603CFFFBFE01FDDEF8B8C9956B13" box="[582,737,1829,1852]" country="Morocco" county="Weygoldt" municipality="Weygoldt" name="Tassili-n-Ajjer" pageId="2" pageNumber="588">Tassili-n-Ajjer</location>
= 
<location id="8E8C603CFFFBFE01FD65F8B8C8EE6B13" LSID="urn:lsid:plazi:treatment:03FA87F1FFFBFE00FF0FF992C81A6DC8:8E8C603CFFFBFE01FD65F8B8C8EE6B13" box="[765,922,1829,1852]" country="Morocco" county="Weygoldt" municipality="Weygoldt" name="Tassili-n-Ajjer" pageId="2" pageNumber="588">Tassili-n-Ajjer</location>
, about 
