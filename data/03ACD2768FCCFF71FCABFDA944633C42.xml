<taxonomicName id="4C0518E38FCCFF71FD13FDE244893D01" authority="Gertsch, 1971: 57" authorityName="Gertsch" authorityPageNumber="57" authorityYear="1971" box="[701,1183,602,623]" class="Arachnida" family="Pholcidae" genus="Coryssocnemis" kingdom="Animalia" order="Araneae" pageId="152" pageNumber="153" phylum="Arthropoda" rank="species" species="placidus">
<emphasis id="B971BF728FCCFF71FD13FDE243D23D01" box="[701,964,602,623]" italics="true" pageId="152" pageNumber="153">Coryssocnemis placidus</emphasis>
