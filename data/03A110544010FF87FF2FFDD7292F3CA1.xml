<subSubSection id="C312F2C94010FF87FF7FFCFC2F443CE9" pageId="1" pageNumber="192" type="distribution">
<paragraph id="8BB7A1424010FF87FF7FFCFC2F443CE9" blockId="1.[151,1436,714,991]" pageId="1" pageNumber="192">
<emphasis id="B97C7D504010FF87FF7FFCFC2D463C30" bold="true" box="[199,345,822,847]" pageId="1" pageNumber="192">Distribution</emphasis>
. Previously known from 
<collectingCountry id="F31FE1D24010FF87FD33FCFD2ED13C31" box="[651,718,823,846]" name="Japan" pageId="1" pageNumber="192">Japan</collectingCountry>
, 
<collectingCountry id="F31FE1D24010FF87FD67FCFC2F393C31" box="[735,806,822,846]" name="Guam" pageId="1" pageNumber="192">Guam</collectingCountry>
, 
<collectingCountry id="F31FE1D24010FF87FC8EFCFC2FA83C31" box="[822,951,822,846]" name="Philippines" pageId="1" pageNumber="192">Philippines</collectingCountry>
, 
<collectingCountry id="F31FE1D24010FF87FC7FFCFC28283C31" box="[967,1079,822,846]" name="Indonesia" pageId="1" pageNumber="192">Indonesia</collectingCountry>
, New 
<collectingCountry id="F31FE1D24010FF87FB31FCFC28C23C31" box="[1161,1245,822,846]" name="Guinea-Bissau" pageId="1" pageNumber="192">Guinea</collectingCountry>
, 
<collectingCountry id="F31FE1D24010FF87FB55FCFC29493C31" box="[1261,1366,822,846]" name="Malaysia" pageId="1" pageNumber="192">Malaysia</collectingCountry>
, 
<collectingCountry id="F31FE1D24010FF87FADFFCFD2D133C0D" name="New Caledonia" pageId="1" pageNumber="192">New Caledonia</collectingCountry>
