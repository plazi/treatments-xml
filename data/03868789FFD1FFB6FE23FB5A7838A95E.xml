<taxonomicName id="4C2F4D1CFFD1FFB9FF41FB107923A872" authority="Pasoce 1871: 199" authorityName="Pasoce" authorityPageNumber="199" authorityYear="1871" box="[175,552,1261,1283]" class="Insecta" family="Curculionidae" genus="Panopides" kingdom="Animalia" order="Coleoptera" pageId="5" phylum="Arthropoda" rank="species" species="anticus">
