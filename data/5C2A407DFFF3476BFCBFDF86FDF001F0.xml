<quantity id="137B5C8EFFF3476CFB41DE6EFB08041E" box="[1198,1261,1854,1878]" metricMagnitude="1" metricUnit="m" metricValue="1.3" pageId="3" pageNumber="115" unit="m" value="13.0">13 m</quantity>
