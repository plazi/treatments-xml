<geoCoordinate id="B1B797ACFFF3476CFC6DDE6EFC08041E" box="[898,1005,1854,1878]" degrees="25" direction="north" minutes="07" orientation="latitude" pageId="3" pageNumber="115" precision="925" value="25.116667">25°07’ N</geoCoordinate>
