<quantity id="8D6B9B35FF8CFFE7FBE5FB761894FB00" box="[1123,1170,1254,1279]" metricMagnitude="0" metricUnit="m" metricValue="6.0" pageId="63" pageNumber="64" unit="m" value="6.0">6 m</quantity>
