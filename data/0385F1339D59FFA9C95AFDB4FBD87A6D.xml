10. 
<taxonomicName id="4C2C3BA69D59FFA9C90EFDB3FC247D43" authority="Fisch." authorityName="Fisch." box="[584,916,579,606]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="heterophylla">
