<taxonomicName id="4C34B2ADFFCAFFDD3FCED97DFD89F980" authorityName="Daugeron &amp; Grootaert" authorityYear="2005" box="[230,589,1603,1627]" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="361" phylum="Arthropoda" rank="species" species="aspina" subGenus="Coptophlebia">
