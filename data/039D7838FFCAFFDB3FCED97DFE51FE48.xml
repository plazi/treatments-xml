<taxonomicName id="4C34B2ADFFCBFFDC3DA0DFEBFC2BFF32" authorityName="Daugeron &amp; Grootaert" authorityYear="2005" box="[648,1007,213,237]" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="361" phylum="Arthropoda" rank="species" species="aspina" subGenus="Coptophlebia">
