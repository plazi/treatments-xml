</taxonomicName>
[
<collectionCode id="ED2BAE2E5B775003FD03FD2D34975C8D" box="[640,679,686,700]" country="Australia" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="6" pageNumber="151">NEW</collectionCode>
<collectionCode id="ED2BAE2E5B775003FD2DFD2E35215C8D" box="[686,785,685,700]" pageId="6" pageNumber="151">SYNONYMY</collectionCode>
]. See comment under 
