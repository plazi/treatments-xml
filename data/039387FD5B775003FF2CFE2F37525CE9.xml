<taxonomicName id="4C3A4D685B775003FEBAFD2A345F5C8F" authority="(Chaudoir, 1843)" baseAuthorityName="Chaudoir" baseAuthorityYear="1843" box="[313,623,681,702]" class="Insecta" family="Carabidae" genus="Loxandrus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="151" phylum="Arthropoda" rank="species" species="saphyrinus">
