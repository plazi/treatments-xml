<taxonomicName id="4C3673E6FFD39111A27BFA3CC576F9FC" authority="Tippelt &amp; Schwentner, 2018" authorityName="Tippelt &amp; Schwentner" authorityYear="2018" box="[876,1140,1524,1549]" class="Branchiopoda" family="Cyzicidae" genus="Eocyzicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="36" pageNumber="437" phylum="Arthropoda" rank="species" species="occidentalis" status="sp. nov.">
<emphasis id="B942D477FFD39111A27BFA3CC576F9FC" box="[876,1140,1524,1549]" italics="true" pageId="36" pageNumber="437">Eocyzicus occidentalis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A271690CFFD39111A591FA3DC5E8F9FD" box="[1158,1258,1524,1549]" pageId="36" pageNumber="437" rank="species">
<emphasis id="B942D477FFD39111A591FA3DC5E6F9FC" bold="true" box="[1158,1252,1524,1549]" pageId="36" pageNumber="437">sp. nov</emphasis>
