<taxonomicName id="9E8AE6F0FF9D6046FE1FFBADFD64C960" box="[461,683,1071,1094]" class="Insecta" family="Geometridae" genus="Acasis" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="66" phylum="Arthropoda" rank="subSpecies" species="appensata" status="ssp. nov." subSpecies="callaina">
