<taxonomicName id="4C7CF6F0FF96CA44FE06A5BEFCAFFAC4" authority="Wiedemann" authorityName="Wiedemann" box="[393,793,1276,1302]" class="Insecta" family="Conopidae" genus="Thecophora" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="distincta">
