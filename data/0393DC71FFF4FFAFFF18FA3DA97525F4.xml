<quantity id="4CC2C082FFF7FFAFFE6DFE7FAF6326B7" box="[482,617,404,429]" metricMagnitude="-3" metricUnit="m" metricValue="5.15" metricValueMax="5.4" metricValueMin="4.9" pageId="3" pageNumber="518" unit="mm" value="5.15" valueMax="5.4" valueMin="4.9">4.9–5.4 mm</quantity>
(
<quantity id="4CC2C082FFF7FFAFFDF7FE7FAFD926B7" box="[632,723,404,429]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" pageId="3" pageNumber="518" unit="mm" value="5.1">5.1 mm</quantity>
); ♀, 
<quantity id="4CC2C082FFF7FFAFFC85FE7FAE9B26B7" box="[778,913,404,429]" metricMagnitude="-3" metricUnit="m" metricValue="5.85" metricValueMax="6.1" metricValueMin="5.6" pageId="3" pageNumber="518" unit="mm" value="5.85" valueMax="6.1" valueMin="5.6">5.6–6.1 mm</quantity>
(
<quantity id="4CC2C082FFF7FFAFFC10FE7FAEF626B7" box="[927,1020,404,429]" metricMagnitude="-3" metricUnit="m" metricValue="5.7" pageId="3" pageNumber="518" unit="mm" value="5.7">5.7 mm</quantity>
).
</paragraph>
