</taxonomicName>
and 
<taxonomicName id="4C47D5DC9C55FFD32201FAF4D79B0C1D" authority="Ingrisch" authorityName="Ingrisch" authorityYear="2001" box="[873,1170,1312,1336]" class="Insecta" family="Tetrigidae" genus="Xistra" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="1903" phylum="Arthropoda" rank="species" species="longicornis">
