<number id="0B6F24ABB426FFC830E92A1FFD7B9B02" box="[712,752,1069,1092]" pageId="3" pageNumber="4" value="134.0">134</number>
</treatmentCitation>
(catalogue); 
<treatmentCitation id="3C06109DB426FFC8315D2A1FFBB09B02" author="Lamas" box="[892,1083,1069,1092]" page="77" pageId="3" pageNumber="4" year="1995">
<bibRefCitation id="D9364B7DB426FFC8315D2A1FFB9C9B02" author="Lamas" box="[892,1047,1069,1092]" pageId="3" pageNumber="26" refString="Lamas, G. (1995 a) A critical review of J. Y. Miller's Checklist of the Neotropical Castniidae (Lepidoptera). Revista Peruana de Entomologia, 37, 73 - 87." type="journal article" year="1995" yearSuffix="a">
Lamas, 
<number id="0B6F24ABB426FFC831F02A1FFB819B02" box="[977,1034,1069,1092]" pageId="3" pageNumber="4" value="1995.0">1995</number>
a
</bibRefCitation>
: 
<number id="0B6F24ABB426FFC836012A1FFBB09B02" box="[1056,1083,1069,1092]" pageId="3" pageNumber="4" value="77.0">77</number>
