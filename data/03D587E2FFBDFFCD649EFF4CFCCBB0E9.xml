<taxonomicName id="4C7C4D77FFBEFFCB6559F840FD23B3B0" authority="Li, Dai &amp; Li" authorityName="Li, Dai &amp; Li" box="[336,718,1946,1969]" class="Insecta" family="Cicadellidae" genus="Sangeeta" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="131" phylum="Arthropoda" rank="species" species="sinuomacula" status="sp. nov.">
