<taxonomicName id="4C0FD1D0A249FF87DECB62A9908EFE08" authority="Meigen, 1818" authorityName="Meigen" authorityYear="1818" box="[151,703,439,464]" class="Insecta" family="Tipulidae" genus="Ctenophora" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="154" phylum="Arthropoda" rank="species" species="guttata" subGenus="Ctenophora">
<emphasis id="B97B7641A249FF87DECB62A9908EFE08" bold="true" box="[151,703,439,464]" pageId="2" pageNumber="154">
<emphasis id="B97B7641A249FF87DECB62A99311FE08" bold="true" box="[151,288,439,464]" italics="true" pageId="2" pageNumber="154">Ctenophora</emphasis>
(
<emphasis id="B97B7641A249FF87DF6C62A99388FE08" bold="true" box="[304,441,439,464]" italics="true" pageId="2" pageNumber="154">Ctenophora</emphasis>
) 
<emphasis id="B97B7641A249FF87DF9562A69028FE17" bold="true" box="[457,537,440,463]" italics="true" pageId="2" pageNumber="154">guttata</emphasis>
<bibRefCitation id="EF9ED7A2A249FF87DC7C62A9908EFE08" author="Meigen" box="[544,703,439,464]" pageId="2" pageNumber="154" refString="Meigen, J. W. (1818) Systematische Beschreibung der bekannten europaischen zweiflugeligen Insekten. Aachen, 1, i - xxxvi, 1 - 333. http: // dx. doi. org / 10.5962 / bhl. title. 13731" type="journal article" year="1818">
Meigen, 
<number id="3DC7B874A249FF87DCDB62A9908EFE08" box="[647,703,439,464]" pageId="2" pageNumber="154" value="1818.0">1818</number>
</bibRefCitation>
