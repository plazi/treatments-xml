<subSubSection id="3EE2A9D99E47C009FF0DFAAD6E51EAE7" pageId="3" pageNumber="342" type="biology_ecology">
<paragraph id="7647FA529E47C009FF0DFAAD6E69EA4D" blockId="3.[249,1389,1191,1695]" pageId="3" pageNumber="342">
<emphasis id="448C26409E47C009FF0DFAAD6D5AEA6E" box="[249,344,1284,1307]" italics="true" pageId="3" pageNumber="342">Standort.</emphasis>
