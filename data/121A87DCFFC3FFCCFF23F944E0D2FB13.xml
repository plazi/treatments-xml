<emphasis id="A8C7EAD8FFC2FFCFFCDAF8F2E4ACF894" box="[878,1169,1806,1828]" italics="true" pageId="13" pageNumber="164">
Notofairchildia 
<taxonomicName id="5DB34D49FFC2FFCFFBA5F8F2E4ACF894" authority="Wagner" authorityName="Wagner" box="[1041,1169,1806,1828]" pageId="13" pageNumber="164" rank="species" species="stuckenbergi">stuckenbergi</taxonomicName>
