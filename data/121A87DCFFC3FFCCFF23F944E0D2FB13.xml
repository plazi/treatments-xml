Nemopalpus 
<taxonomicName id="5DB34D49FFC2FFCFFC66F90CE433F8B6" authority="Macquart" authorityName="Macquart" box="[978,1038,1776,1798]" pageId="13" pageNumber="164" rank="species" species="flavus">flavus</taxonomicName>
</emphasis>
(Macquart)
</taxonomicName>
; c— 
<taxonomicName id="5DB34D49FFC2FFCFFB03F90DE1A4F894" authority="Wagner &amp; Stuckenberg" authorityName="Wagner &amp; Stuckenberg" class="Insecta" family="Psychodidae" genus="Laurenceomyia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="164" phylum="Arthropoda" rank="species" species="similis">
<emphasis id="A8C7EAD8FFC2FFCFFB03F90DE5A1F8B6" box="[1207,1436,1776,1798]" italics="true" pageId="13" pageNumber="164">
Laurenceomyia 
<taxonomicName id="5DB34D49FFC2FFCFFAEEF90CE5A1F8B6" authority="Wagner &amp; Stuckenberg" authorityName="Wagner &amp; Stuckenberg" box="[1370,1436,1776,1798]" pageId="13" pageNumber="164" rank="species" species="similis">similis</taxonomicName>
</emphasis>
(Wagner &amp; Stuckenberg)
</taxonomicName>
; d— 
<taxonomicName id="5DB34D49FFC2FFCFFE7EF8F2E303F894" authority="Young" authorityName="Young" box="[458,830,1806,1828]" class="Insecta" family="Psychodidae" genus="Boreofairchildia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="164" phylum="Arthropoda" rank="species" species="nearcticus">
<emphasis id="A8C7EAD8FFC2FFCFFE7EF8F2E2DCF894" box="[458,737,1806,1828]" italics="true" pageId="13" pageNumber="164">
Boreofairchildia 
<taxonomicName id="5DB34D49FFC2FFCFFDCCF8F3E2DCF894" authority="Young" authorityName="Young" box="[632,737,1807,1828]" pageId="13" pageNumber="164" rank="species" species="nearcticus">nearcticus</taxonomicName>
</emphasis>
(Young)
</taxonomicName>
; e— 
