<taxonomicName id="98B74D69FFD3FFF0FE2DFA4CFD0B54CD" authorityName="Peck and Cook" authorityYear="2014" box="[509,653,1443,1467]" class="Insecta" family="Leiodidae" genus="Aglyptinus" kingdom="Animalia" order="Coleoptera" pageId="53" pageNumber="52" phylum="Arthropoda" rank="species" species="longipalpus">
