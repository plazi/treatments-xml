<location id="052E606FFFDD8A3BF79CF61DF686D080" LSID="urn:lsid:plazi:treatment:885887A2FFDD8A3AF896FB47F94CD195:052E606FFFDD8A3BF79CF61DF686D080" box="[2128,2307,2492,2531]" country="India" name="Hodgson" pageId="18" pageNumber="297" stateProvince="Himachal Pradesh, Uttarakhand, West Bengal, Sikkim, Arunachal Pradesh">Hodgson</location>
