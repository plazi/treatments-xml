, BC 
<collectionCode id="ED50A3E9FFA10F5A1A56FF67A6CCFF22" box="[375,432,151,175]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="4" pageNumber="41" type="Museum">ZSM</collectionCode>
</materialsCitation>
<materialsCitation id="3B293171FFA10F5A1A96FF68A5D0FF22" box="[439,684,151,175]" country="Indonesia" location="Lep" pageId="4" pageNumber="41" specimenCount="1" typeStatus="paratype">
<location id="8E9E6DF7FFA10F5A1A96FF68A69EFF22" LSID="urn:lsid:plazi:treatment:03E88A3AFFA60F5A1BB6F952A34CFAA2:8E9E6DF7FFA10F5A1A96FF68A69EFF22" box="[439,482,152,175]" country="Indonesia" name="Lep" pageId="4" pageNumber="41">Lep</location>
