<quantity id="4C8B66A4FFA9EF06FDFD81F199F2FB10" box="[632,707,1209,1233]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="8" pageNumber="7" unit="km" value="15.0">15 km</quantity>
N El Llano (
<collectionCode id="ED625384FFA9EF06FCE581F198ACFB10" box="[864,925,1209,1233]" pageId="8" pageNumber="7">FTH</collectionCode>
