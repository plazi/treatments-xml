<taxonomicName id="4C73B0C2FFA9EF06FC0581D098E7FB71" authorityName="Dillon and Dillon" authorityYear="1946" box="[896,982,1176,1200]" class="Insecta" family="Cerambycidae" genus="Cylicasta" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="nysa">
