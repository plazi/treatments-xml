<materialsCitation id="96CE3CFEFF81FFC65984C5F4CE16FDF4" collectingDate="2017-11-23" collectorName="No. X &amp; Jian Huang &amp; Zhu Hong Wang &amp; Agriculture &amp; Forestry University" country="China" county="Mengla County" location="Xishuangbanna Tropical Plants Garden of Chinese Academy" municipality="Menglun Town" pageId="16" pageNumber="1894" specimenCount="1" specimenCount-female="1" stateProvince="Yunnan" typeStatus="holotype">
