<geoCoordinate id="EE6BEDADFC66FFEFF164FE2BFC24FE14" box="[886,961,477,497]" degrees="99" direction="east" minutes="20" orientation="longitude" pageId="39" pageNumber="1048" precision="925" value="99.333336">99°20‘E</geoCoordinate>
, 
<quantity id="4CA7268FFC66FFEFF1DFFE2BFBE7FE14" box="[973,1026,477,497]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="39" pageNumber="1048" unit="m" value="500.0">500m</quantity>
