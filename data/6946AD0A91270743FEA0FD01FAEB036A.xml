of 
<taxonomicName id="26EF679F91270743FE03FD5AFD190311" box="[427,734,764,790]" class="Insecta" family="Aphididae" genus="Aphis" kingdom="Animalia" order="Aphidomorpha" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="kochiae" subGenus="Zyxaphis">
<emphasis id="D39BC00E91270743FE03FD5AFE290311" box="[427,494,765,790]" italics="true" pageId="4" pageNumber="5">Aphis</emphasis>
(
<emphasis id="D39BC00E91270743FDABFD5AFDAA0311" box="[515,621,765,790]" italics="true" pageId="4" pageNumber="5">Zyxaphis</emphasis>
) 
<emphasis id="D39BC00E91270743FD2BFD5AFD190311" box="[643,734,765,790]" italics="true" pageId="4" pageNumber="5">kochiae</emphasis>
</taxonomicName>
