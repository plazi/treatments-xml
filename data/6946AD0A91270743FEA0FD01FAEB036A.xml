<taxonomicName id="26EF679F91270743FC1BFD5AFBB80311" box="[947,1151,765,790]" class="Magnoliopsida" family="Amaranthaceae" genus="Kochia" kingdom="Plantae" order="Caryophyllales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="prostrata">
