<emphasis id="13975759FFE5FF82B3D6FCDE83AEFC4A" bold="true" box="[151,686,871,897]" pageId="32" pageNumber="33">
<taxonomicName id="E6E3F0C8FFE5FF82B3D6FCDE835FFC4A" authority="Arias-Penna" authorityName="Arias-Penna" box="[151,607,871,897]" class="Insecta" family="Braconidae" genus="Urosigalphus" kingdom="Animalia" order="Hymenoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="monticola" status="n. sp.">
<emphasis id="13975759FFE5FF82B3D6FCDE80BAFC4A" bold="true" box="[151,442,871,897]" italics="true" pageId="32" pageNumber="33">Urosigalphus monticola</emphasis>
Arias-Penna
</taxonomicName>
, 
<taxonomicNameLabel id="08A4EA22FFE5FF82B12AFCDE83AEFC4A" box="[619,686,871,897]" pageId="32" pageNumber="33" rank="species">n. sp.</taxonomicNameLabel>
