<quantity id="4C939B53064A2C53FC29FCC04B68FCF1" box="[952,1045,836,862]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="2" pageNumber="45" unit="mm" value="4.2">4.2 mm</quantity>
. 
<collectingCountry id="F37C7626064A2C53FB8EFCC74E6CFC2E" name="China" pageId="2" pageNumber="45">China</collectingCountry>
(
<collectingRegion id="49AFF854064A2C53FEB1FCE34EFEFC2D" box="[288,387,871,898]" country="China" name="Guangxi" pageId="2" pageNumber="45">Guangxi</collectingRegion>
)............................ 
