</emphasis>
(
<collectionCode id="ED2FAE32676F1304FB895E06E42B2D7C" box="[1120,1138,590,616]" country="USA" lsid="urn:lsid:biocol.org:col:15707" name="Field Museum of Natural History, Botany Department" pageId="5" pageNumber="163" type="Herbarium">F</collectionCode>
