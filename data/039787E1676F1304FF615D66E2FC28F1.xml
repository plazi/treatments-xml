<emphasis id="B94AEAE5676F1304FEB35E4FE2792D34" box="[346,544,518,545]" italics="true" pageId="5" pageNumber="163">
<taxonomicName id="4C3E4D74676F1304FEB35E4FE1F92D35" authorityName="Linnaeus" authorityYear="1753" box="[346,416,519,545]" class="Magnoliopsida" family="Cistaceae" genus="Cistus" kingdom="Plantae" order="Malvales" pageId="5" pageNumber="163" phylum="Tracheophyta" rank="genus">Cistus</taxonomicName>
× 
<taxonomicName id="4C3E4D74676F1304FE5F5E4EE2792D34" authorityName="Font Quer &amp; Maire" authorityYear="1930" box="[438,544,518,544]" class="Magnoliopsida" family="Cistaceae" genus="Cistus" kingdom="Plantae" order="Malvales" pageId="5" pageNumber="163" phylum="Tracheophyta" rank="species" species="clausonii">clausonii</taxonomicName>
</emphasis>
