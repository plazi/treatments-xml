<taxonomicName id="FF924D02691FFFCCFEA1FF415AD6FF0B" authorityName="K. Viets" authorityYear="1914" box="[279,497,151,177]" class="Arachnida" family="Anisitsiellidae" genus="Platymamersopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="88" pageNumber="89" phylum="Arthropoda" rank="genus">
<emphasis id="0AE6EA93691FFFCCFEA1FF415AD6FF0B" bold="true" box="[279,497,151,177]" italics="true" pageId="88" pageNumber="89">Platymamersopsis</emphasis>
</taxonomicName>
</emphasis>
