<taxonomicName id="77CDFEE4FF908A2487D11974FCAFFEB7" box="[718,792,360,383]" class="Arachnida" family="Ctenidae" genus="Ctenus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="69" phylum="Arthropoda" rank="genus">
