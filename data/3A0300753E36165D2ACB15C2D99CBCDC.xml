<taxonomicName id="75AACAE03E36165D29D114EBDCC0BC81" authority="angustior" box="[909,1163,1411,1437]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="subSpecies" species="angustior" subSpecies="angustior">
