<taxonomicName id="1B0C9417556234AC08A85149280BE389" authorityName="T. Deng, D. G. Zhang, Qun Liu &amp; Z. M. Li" authorityYear="2021" class="Trilobita" family="Asteraceae" genus="Youngia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Youngia hangii" order="Phacopida" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="hangii">
