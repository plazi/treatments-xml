<taxonomicName id="DC5E4E4FA77E23E80F6138253C0576F6" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia ehrenbergiana" order="Fabales" pageId="48" pageNumber="49" phylum="Tracheophyta" rank="species" species="ehrenbergiana">
