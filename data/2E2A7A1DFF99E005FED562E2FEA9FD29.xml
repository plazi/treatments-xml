<taxonomicName id="6183B088FF9AE005FD85637EFC95FDB2" box="[600,860,604,629]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="plumae">
