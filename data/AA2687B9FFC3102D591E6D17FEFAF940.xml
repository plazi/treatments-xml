<collectorName id="8F7A5379FFC3102D5E1B6813FAC6FBB7" box="[1329,1394,1039,1065]" pageId="12" pageNumber="13">Chen</collectorName>
, L. 
<collectorName id="8F7A5379FFC3102D5BF46829FEABFBD1" box="[222,287,1077,1103]" pageId="12" pageNumber="13">Gong</collectorName>
