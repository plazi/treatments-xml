<taxonomicName id="0BA44D4BFFD60C7B5C20550C09F83D46" authority="Gremli" box="[881,1169,1346,1370]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="12" pageNumber="305" phylum="Tracheophyta" rank="subSpecies" species="jacea" subSpecies="angustifolia">
subsp. 
<emphasis id="FED0EADAFFD60C7B5CEA550C09563D46" bold="true" box="[955,1087,1346,1370]" italics="true" pageId="12" pageNumber="305">angustifolia</emphasis>
Gremli
