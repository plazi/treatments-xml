sa, d 
<number id="AD26AE6BFF81FFE8BDCDF9F0DFCDF9DC" box="[1235,1250,1621,1646]" pageId="10" pageNumber="56" value="2.0">2</number>
a, 
<date id="6F509A8CFF81FFE8BDE2F9F3DE34F9DC" box="[1276,1307,1622,1646]" pageId="10" pageNumber="56">
v 
<number id="AD26AE6BFF81FFE8BC12F9F3DE34F9DC" box="[1292,1307,1622,1646]" pageId="10" pageNumber="56" value="4.0">4</number>
