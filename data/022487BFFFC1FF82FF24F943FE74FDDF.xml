<taxonomicName id="4D8D4D2AFFC1FF83FF24F943FDD4F95E" authority="Trouessart, 1894" authorityName="Trouessart" authorityYear="1894" box="[151,610,1698,1725]" class="Arachnida" family="Halacaridae" genus="Scaptognathus" kingdom="Animalia" order="Trombidiformes" pageId="28" pageNumber="229" phylum="Arthropoda" rank="species" species="hallezi">
