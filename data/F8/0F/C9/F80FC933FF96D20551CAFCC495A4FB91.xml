<taxonomicName baseAuthorityName="Girault" baseAuthorityYear="1913" box="[579,854,992,1017]" class="Insecta" family="Torymidae" genus="Podagrion" kingdom="Animalia" order="Hymenoptera" pageId="31" pageNumber="785" phylum="Arthropoda" rank="species" species="magniclavum">
