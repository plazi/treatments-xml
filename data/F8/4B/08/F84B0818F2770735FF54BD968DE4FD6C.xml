<paragraph blockId="12.[809,1425,197,648]" box="[809,1401,197,219]" pageId="12" pageNumber="625">
<treatmentCitationGroup box="[809,1401,197,219]" pageId="12" pageNumber="625">
<taxonomicName baseAuthorityName="Poll" baseAuthorityYear="1967" box="[809,1091,197,219]" class="Actinopterygii" family="Mormyridae" genus="Paramormyrops" kingdom="Animalia" order="Osteoglossiformes" pageId="12" pageNumber="625" phylum="Chordata" rank="species" species="jacksoni">
