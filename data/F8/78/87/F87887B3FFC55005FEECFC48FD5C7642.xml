<taxonomicName authorityName="Krauss" authorityYear="1879" box="[472,585,1046,1065]" class="Insecta" family="Rhaphidophoridae" genus="Troglophilus" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="429" phylum="Arthropoda" rank="genus">Troglophilus</taxonomicName>
