<taxonomicName authorityName="Curtis" authorityYear="1829" class="Insecta" family="Megaspilidae" genus="Ceraphron" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ceraphron halidayi" order="Hymenoptera" pageId="0" pageNumber="123" phylum="Arthropoda" rank="species" species="halidayi">Ceraphron halidayi</taxonomicName>
