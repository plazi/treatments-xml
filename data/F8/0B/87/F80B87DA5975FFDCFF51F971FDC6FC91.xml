<collectingCountry box="[1044,1149,793,817]" name="Morocco" pageId="27" pageNumber="28">Morocco</collectingCountry>
, 
<collectingCountry box="[1166,1253,793,817]" name="Algeria" pageId="27" pageNumber="28">Algeria</collectingCountry>
