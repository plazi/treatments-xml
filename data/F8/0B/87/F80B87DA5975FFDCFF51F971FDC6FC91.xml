<collectingCountry box="[709,790,829,853]" name="Turkey" pageId="27" pageNumber="28">Turkey</collectingCountry>
, 
<collectingCountry box="[803,904,829,853]" name="Armenia" pageId="27" pageNumber="28">Armenia</collectingCountry>
