<paragraph blockId="8.[151,1437,755,859]" pageId="8" pageNumber="40">
<emphasis bold="true" box="[151,422,755,781]" pageId="8" pageNumber="40">Habitats and status:</emphasis>
<taxonomicName box="[431,693,756,781]" class="Clitellata" family="Glossiphoniidae" genus="Helobdella" kingdom="Animalia" order="Rhynchobdellida" pageId="8" pageNumber="40" phylum="Annelida" rank="species" species="stagnalis">
<emphasis box="[431,693,756,781]" italics="true" pageId="8" pageNumber="40">Helobdella stagnalis</emphasis>
