<emphasis box="[211,338,755,778]" italics="true" pageId="119" pageNumber="857">Description</emphasis>
based on 
<typeStatus box="[452,551,755,779]" pageId="119" pageNumber="857">paratype</typeStatus>
female, 
<quantity box="[645,712,754,778]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="119" pageNumber="857" unit="mm" value="6.0">6 mm</quantity>
