<taxonomicName class="Insecta" family="Elcanidae" genus="Parelcana" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parelcana pulchmacula" order="Orthoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="species" species="pulchmacula">
