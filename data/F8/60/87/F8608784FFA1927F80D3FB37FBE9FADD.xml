<taxonomicName authority="Yabe &amp; Hanzawa, 1928" authorityName="Yabe &amp; Hanzawa" authorityYear="1928" class="Globothalamea" family="Miogypsinidae" genus="Miogypsinoides" kingdom="Chromista" order="Rotaliida" pageId="2" pageNumber="239" phylum="Foraminifera" rank="genus">
