<taxonomicName authority="Blanford, 1867" authorityName="Blanford" authorityYear="1867" box="[893,1211,1212,1237]" class="Gastropoda" family="Iravadiidae" genus="Iravadia" kingdom="Animalia" order="Littorinimorpha" pageId="1" pageNumber="238" phylum="Mollusca" rank="species" species="ornata">
