<paragraph blockId="9.[151,1436,1069,1957]" pageId="9" pageNumber="10">
<emphasis box="[199,394,1537,1561]" italics="true" pageId="9" pageNumber="10">
<collectingCountry box="[199,257,1537,1561]" name="Malta" pageId="9" pageNumber="10">Male</collectingCountry>
macroptera
