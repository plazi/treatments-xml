<paragraph blockId="15.[151,1437,786,1999]" pageId="15" pageNumber="16">
<emphasis box="[199,391,1651,1675]" italics="true" pageId="15" pageNumber="16">
<collectingCountry box="[199,257,1651,1675]" name="Malta" pageId="15" pageNumber="16">Male</collectingCountry>
microptera
</emphasis>
