<specimenCount box="[725,825,1938,1962]" pageId="15" pageNumber="16" type="female">1 female</specimenCount>
in water trap; Ravenshoe, 
<date box="[1141,1257,1938,1962]" pageId="15" pageNumber="16" value="1995-07-03">
<number box="[1141,1154,1938,1962]" pageId="15" pageNumber="16" value="3.0">3</number>
.vii. 
<number box="[1195,1257,1938,1962]" pageId="15" pageNumber="16" value="1995.1">1995</number>
</date>
, 
<specimenCount box="[1271,1371,1938,1962]" pageId="15" pageNumber="16" type="female">1 female</specimenCount>
