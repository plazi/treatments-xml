<collectionCode box="[1046,1095,1146,1167]" pageId="16" pageNumber="139">SVL</collectionCode>
<geoCoordinate box="[1104,1166,1146,1168]" degrees="0.523" direction="north" orientation="latitude" pageId="16" pageNumber="139" precision="55" value="0.523">0.523</geoCoordinate>
<geoCoordinate box="[1167,1244,1146,1168]" degrees="0.608" direction="west" orientation="longitude" pageId="16" pageNumber="139" precision="55" value="-0.608">–0.608</geoCoordinate>
, median 0.574; HW/ 
<collectionCode box="[859,908,1177,1198]" pageId="16" pageNumber="139">SVL</collectionCode>
of females 
<geoCoordinate box="[1065,1127,1177,1199]" degrees="0.268" direction="north" orientation="latitude" pageId="16" pageNumber="139" precision="55" value="0.268">0.268</geoCoordinate>
<geoCoordinate box="[1127,1205,1177,1199]" degrees="0.309" direction="west" orientation="longitude" pageId="16" pageNumber="139" precision="55" value="-0.309">–0.309</geoCoordinate>
, of males 
