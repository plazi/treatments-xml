<emphasis id="B963EA81F25DFF99FFB8FCA602B17EF0" bold="true" box="[113,253,449,473]" italics="true" pageId="7" pageNumber="8">Distribution.</emphasis>
â€“
</emphasis>
<collectingRegion id="49D3F871F25DFF99FED3FCA603267EF1" box="[282,362,449,472]" country="India" name="Sikkim" pageId="7" pageNumber="8">Sikkim</collectingRegion>
, 
<collectingCountry id="F3007603F25DFF99FEBFFCA603E37EF1" box="[374,431,449,472]" name="India" pageId="7" pageNumber="8">India</collectingCountry>
, 
<collectingCountry id="F3007603F25DFF99FE72FCA600477EF1" box="[443,523,449,472]" name="Bhutan" pageId="7" pageNumber="8">Bhutan</collectingCountry>
