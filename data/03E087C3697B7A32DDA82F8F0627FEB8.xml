<geoCoordinate id="EE7D5012697B7A2CDCBC2E410749FA8A" box="[439,513,1306,1326]" degrees="15" direction="north" minutes="14" orientation="latitude" pageId="15" pageNumber="186" precision="925" value="15.233334">15째14'N</geoCoordinate>
