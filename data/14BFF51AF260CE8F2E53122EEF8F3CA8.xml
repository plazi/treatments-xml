<taxonomicName id="5817DD51B23B0DB5C6B5A0E7864DED20" authority="Paykull, 1800" authorityName="Paykull" authorityYear="1800" class="Insecta" family="Erotylidae" genus="Engis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Engis" order="Coleoptera" pageId="166" pageNumber="167" phylum="Arthropoda" rank="genus">Engis</taxonomicName>
