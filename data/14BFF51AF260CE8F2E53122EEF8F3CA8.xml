<taxonomicName id="4DF91491A8C961BB77E59D0F290FEB89" authority="Lacordaire, 1842" authorityName="Lacordaire" authorityYear="1842" class="Insecta" family="Erotylidae" genus="Encaustes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Encaustes" order="Coleoptera" pageId="166" pageNumber="167" phylum="Arthropoda" rank="genus">Encaustes</taxonomicName>
