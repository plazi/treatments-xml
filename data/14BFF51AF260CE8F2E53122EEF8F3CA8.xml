<taxonomicName id="217B10D0DBCE5091ECA5BF3FAF298267" authority="Paykull, 1800" authorityName="Paykull" authorityYear="1800" class="Insecta" family="Erotylidae" genus="Engis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Engis" order="Coleoptera" pageId="166" pageNumber="167" phylum="Arthropoda" rank="genus">Engis</taxonomicName>
