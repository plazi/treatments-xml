<taxonomicName id="4BA625E9CDF3ED627AA6BE273E5528F5" class="Insecta" family="Carabidae" genus="Macrocheilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Macrocheilus" order="Coleoptera" pageId="9" pageNumber="86" phylum="Arthropoda" rank="genus">Macrocheilus</taxonomicName>
