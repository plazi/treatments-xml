<taxonomicName id="4C394D48F9048432FBB237A3B6BFFB46" authorityName="Harris" authorityYear="1841" box="[1030,1211,1193,1219]" class="Insecta" family="Lasiocampidae" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="family">Lasiocampidae</taxonomicName>
