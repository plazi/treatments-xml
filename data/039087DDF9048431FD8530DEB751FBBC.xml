<taxonomicName id="4C394D48F9048432FD3537A0B114FB46" authorityName="Schrank" authorityYear="1802" box="[641,784,1194,1219]" class="Insecta" family="Lasiocampidae" genus="Lasiocampa" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">
