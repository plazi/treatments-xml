<collectingCountry id="FFE77653FFCABE54C5682663B4A6E56E" box="[1358,1436,1028,1053]" name="Poland" pageId="3" pageNumber="304">Poland</collectingCountry>
(
<collectionCode id="E1E1AE06FFCABE54C0B8264EB1D6E532" box="[158,236,1065,1089]" pageId="3" pageNumber="304">DATE</collectionCode>
), 
<number id="313824E4FFCABE54C126264EB034E532" box="[256,270,1065,1089]" pageId="3" pageNumber="304" value="4.0">4</number>
<typeStatus id="584B8861FFCABE54C133264EB0BAE532" box="[277,384,1065,1089]" pageId="3" pageNumber="304">paratypes</typeStatus>
at the Institute of Soil Biology BC 
<collectionCode id="E1E1AE06FFCABE54C332264EB25AE532" box="[788,864,1065,1089]" pageId="3" pageNumber="304">ASCR</collectionCode>
, České Budějovice in the 
<collectingCountry id="FFE77653FFCABE54C4A1264FB403E532" box="[1159,1337,1064,1089]" name="Czech Republic" pageId="3" pageNumber="304">Czech Republic</collectingCountry>
(
<collectionCode id="E1E1AE06FFCABE54C56C264EB443E532" box="[1354,1401,1065,1089]" httpUri="http://grbio.org/cool/831b-8swc" name="Institute of Spelology Emile Racovita" pageId="3" pageNumber="304">ISB</collectionCode>
