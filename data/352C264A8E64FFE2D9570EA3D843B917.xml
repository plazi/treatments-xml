<taxonomicName id="7A85ECDF8E64FFE2DAE309A7DA0FB8F5" authorityName="Oliverio" authorityYear="2008" box="[676,824,1758,1781]" class="Gastropoda" family="Muricidae" genus="Babelomurex" kingdom="Animalia" order="Neogastropoda" pageId="61" pageNumber="542" phylum="Mollusca" rank="species" species="macrocephalus">
