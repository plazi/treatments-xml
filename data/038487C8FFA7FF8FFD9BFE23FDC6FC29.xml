<taxonomicName id="4C2D4D5DFFA1FF8CFEE2F8C3FD04F8FE" authority="Yang &amp; Pan &amp; Liu &amp; Yang, 2021" authorityName="Yang &amp; Pan &amp; Liu &amp; Yang" authorityYear="2021" box="[344,738,1890,1917]" class="Insecta" family="Tipulidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="165" phylum="Arthropoda" rank="species" species="nayogabuensis" status="sp. nov." subGenus="Vestiplex">
<emphasis id="B959EACCFFA1FF8CFEE2F8C3FE42F8FE" box="[344,420,1890,1916]" italics="true" pageId="13" pageNumber="165">Tipula</emphasis>
