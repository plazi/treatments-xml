<taxonomicName id="4C2D4D5DFFA0FF8DFEECF825FD06F81C" authority="Yang &amp; Pan &amp; Liu &amp; Yang, 2021" authorityName="Yang &amp; Pan &amp; Liu &amp; Yang" authorityYear="2021" box="[342,736,1924,1951]" class="Insecta" family="Tipulidae" genus="Tipula" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="164" phylum="Arthropoda" rank="species" species="nayogabuensis" status="sp. nov." subGenus="Vestiplex">
