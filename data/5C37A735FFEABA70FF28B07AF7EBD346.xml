<collectingCountry id="AC8956B3FFEBBA70FF28B661F61AD026" box="[151,268,285,309]" name="Colombia" pageId="17" pageNumber="18">Colombia</collectingCountry>
:” 
<collectionCode id="B28F8EE6FFEBBA70FE9FB661F67ED026" LSID="urn:lsid:biocol.org:col:34784" box="[288,360,285,309]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34784" name="Academy of Natural Sciences of Philadelphia" pageId="17" pageNumber="18">ANSP</collectionCode>
<number id="62560404FFEBBA70FED0B661F6D0D026" box="[367,454,285,309]" pageId="17" pageNumber="18" value="104845.0">104845</number>
, 
<collectionCode id="B28F8EE6FFEBBA70FE6EB661F534D026" LSID="urn:lsid:biocol.org:col:34871" box="[465,546,285,309]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="17" pageNumber="18">USNM</collectionCode>
<number id="62560404FFEBBA70FD96B661F596D026" box="[553,640,285,309]" pageId="17" pageNumber="18" value="532640.0">532640</number>
