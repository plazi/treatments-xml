<geoCoordinate id="EE235007FF05FF219A38FD41F8FE32C8" box="[581,703,691,717]" degrees="23" direction="north" minutes="24.6" orientation="latitude" pageId="148" pageNumber="149" precision="92" value="23.41">23°24.6’N</geoCoordinate>
