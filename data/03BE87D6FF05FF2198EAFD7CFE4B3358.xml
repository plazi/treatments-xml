<quantity id="4CEF9B25FF05FF21989FFCB1FB683358" box="[226,297,835,861]" metricMagnitude="2" metricUnit="m" metricValue="1.83" pageId="148" pageNumber="149" unit="m" value="183.0">183 m</quantity>
(
