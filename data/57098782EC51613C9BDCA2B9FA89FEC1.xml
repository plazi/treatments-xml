<taxonomicName id="18A04D17EC50613F9B85A572FECDF8EB" baseAuthorityName="Herbst" baseAuthorityYear="1806" box="[206,351,1873,1897]" class="Insecta" family="Elateridae" genus="Hemicrepidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="326" phylum="Arthropoda" rank="species" species="memnonius">
