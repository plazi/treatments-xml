<taxonomicName id="18A04D17EC50613F9BDCA556FEB7F80F" baseAuthorityName="DeGeer" baseAuthorityYear="1774" box="[151,293,1909,1933]" class="Insecta" family="Elateridae" genus="Pheletes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="326" phylum="Arthropoda" rank="species" species="aeneoniger">
