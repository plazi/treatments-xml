<taxonomicName id="18A04D17EC50613F9970A50DFD55F8C7" authority="Etzler, 2019" authorityName="Etzler" authorityYear="2019" box="[571,711,1837,1861]" class="Insecta" family="Elateridae" genus="Tetralimonius" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="327" phylum="Arthropoda" rank="genus" status="new genus">
