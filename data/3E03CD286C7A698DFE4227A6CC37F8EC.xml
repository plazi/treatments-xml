<taxonomicName id="AC469E483D10E6C47023A36FF44EC93D" authorityName="Hagedorn" authorityYear="1908" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus sumatranus" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="sumatranus">
