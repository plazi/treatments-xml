<taxonomicName id="0DF21508B3DC4908DD36A103AED3ECC6" authorityName="Hopkins" authorityYear="1915" class="Insecta" family="Curculionidae" genus="Ambrosiodmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ambrosiodmus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="104C7D5E464364AD13F62EE9116C8FA7" italics="true" pageId="0" pageNumber="1">Ambrosiodmus</emphasis>
</taxonomicName>
, 
<taxonomicName id="CFC201A975567422BB28924E00405FED" authorityName="Hopkins" authorityYear="1915" class="Insecta" family="Curculionidae" genus="Coptodryas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coptodryas" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="063B90504F958B338299148EE57499C8" italics="true" pageId="0" pageNumber="1">Coptodryas</emphasis>
