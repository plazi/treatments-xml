<quantity id="4CF6C9F7FFD4FFE36401FAC9A6C2FAE0" box="[635,722,1294,1320]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" pageId="46" pageNumber="47" unit="mm" value="18.0">18 mm</quantity>
