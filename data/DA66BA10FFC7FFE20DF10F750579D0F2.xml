<taxonomicName id="95CF7085FFC4FFE30C930FE304CCD0B4" baseAuthorityName="Cope" baseAuthorityYear="1868" box="[501,664,1972,1996]" class="Reptilia" family="Diploglossidae" genus="Comptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="26" pageNumber="227" phylum="Chordata" rank="species" species="badius">
