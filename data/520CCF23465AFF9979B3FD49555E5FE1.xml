<emphasis id="E8D1A227465BFF9979B3FC1D563859A3" bold="true" box="[151,268,929,951]" pageId="5" pageNumber="442">FIGURE 3.</emphasis>
<taxonomicName id="1DA505B6465BFF997836FC1D55D359A2" authority="Liu &amp; Ding. A. Right" authorityName="Liu &amp; Ding. A. Right" box="[274,743,929,951]" class="Insecta" family="Aphelocheiridae" genus="Aphelocheirus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="442" phylum="Arthropoda" rank="species" species="ellipsoideus">
