<typeStatus id="844A55F7E16B0C7916C188FDC86D0E56">Holotype</typeStatus>
worker plus 4 worker 
<typeStatus id="79CEB3CCBD23B9770CDEBC2630BB50F7">paratypes</typeStatus>
labelled &quot;KIR: 
<geoCoordinate id="F260AAF33ACC2ADE737A6CBC6FC6C5D4" direction="north" orientation="latitude" value="41.8327">41.8327° N</geoCoordinate>
, 
<geoCoordinate id="F5938B7DBB37A496E4BF3846801509BB" direction="east" orientation="longitude" value="71.1948">71.1948° E</geoCoordinate>
Tshat-kal valley, 1830 m R. Schultz 1998.07.28-115&quot; and &quot;Holo-type 
<taxonomicName id="865CA534957ABC84FF8062B1528795BB" authority="Seifert &amp; Schultz" authorityName="Seifert &amp; Schultz" authorityYear="2009" class="Insecta" evidence="document-1-1" evidenceDetail="combination" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageNumber="266" phylum="Arthropoda" rank="species" source="9F720053E8E751B46EE93B1DC4051628,2EC9779F7393F010A3B5B9BD37DC6607" species="orangea">Formica orangea Seifert &amp; Schultz</taxonomicName>
&quot; / &quot; 
<typeStatus id="FAF42D27C06BB1DE42E15A6935A9CC27">Paratype</typeStatus>
<taxonomicName id="527CE09D1223741A5C3AC2610F9885BB" authority="Seifert &amp; Schultz" authorityName="Seifert &amp; Schultz" authorityYear="2009" class="Insecta" evidence="document-1-1" evidenceDetail="combination" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageNumber="266" phylum="Arthropoda" rank="species" source="D618D668202D28A979A88A5E2882FF49,F067E551A8300D4FB264D41185793E45" species="orangea">Formica orangea Seifert &amp; Schultz</taxonomicName>
&quot;, 5 
<typeStatus id="816D866B9E7D8A88004A42F7136D6FE2">paratype</typeStatus>
workers in etha-nol, 
<collectionCode id="EDC1AFA8FDA14D84A8A1A2C1DB1ECD22" collectionName="SMN">SMN</collectionCode>
<normalizedToken id="93EA58D1838E2260CE5AE83D0EF268B0" originalValue="Görlitz">Goerlitz</normalizedToken>
; from the same nest series: 3 mounted 
<typeStatus id="8D854C44B1AAAAD97B1185600370CE1D">paratype</typeStatus>
workers and 19 
<typeStatus id="F2D967286B5519484920CE12F6D31633">paratype</typeStatus>
