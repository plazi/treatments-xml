on a specimen of 
<taxonomicName id="5D98F5D6FF876A16F468F946FC56F95C" authorityName="de Man" authorityYear="1917" box="[826,955,1736,1759]" class="Malacostraca" family="Pandalidae" genus="Heterocarpus" kingdom="Animalia" order="Decapoda" pageId="46" pageNumber="46" phylum="Arthropoda" rank="species" species="sibogae">
