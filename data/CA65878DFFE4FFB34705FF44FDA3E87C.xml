<taxonomicName id="85CC4D18FFE4FFB14712FF4BFC56EA9E" box="[881,968,215,242]" class="Reptilia" family="Colubridae" genus="Coluber" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="428" phylum="Chordata" rank="genus">
