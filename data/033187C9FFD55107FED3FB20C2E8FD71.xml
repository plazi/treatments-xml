</taxonomicName>
GROUP Of GENUS 
<taxonomicName id="4C984D5CFFD55106FC04FB20C235FB7B" authorityName="Pocock" authorityYear="1893" box="[980,1133,1169,1195]" class="Arachnida" family="Bothriuridae" genus="Urophonius" kingdom="Animalia" order="Scorpiones" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
