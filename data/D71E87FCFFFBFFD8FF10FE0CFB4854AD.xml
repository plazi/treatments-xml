<taxonomicName id="98B74D69FFFBFFD8FE27FA2CFDDD54AD" authorityName="Peck &amp; Cook" authorityYear="2014" box="[503,603,1475,1499]" class="Insecta" family="Leiodidae" genus="Zeadolopus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="28" phylum="Arthropoda" rank="species" species="flavidus">
