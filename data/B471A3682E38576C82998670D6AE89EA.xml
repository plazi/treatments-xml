<taxonomicName id="C971E49A91A7ED2425BA35DF9416A8A3" class="Insecta" genus="E." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="E. waaf" order="Coleoptera" pageId="0" pageNumber="45" phylum="Arthropoda" rank="species" species="waaf">
