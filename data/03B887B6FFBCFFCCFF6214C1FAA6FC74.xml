<collectingCountry id="F3067630FFBCFFCCFD2216BDFC70FC0B" box="[727,911,935,959]" name="United Kingdom" pageId="16" pageNumber="17">Shetland Islands</collectingCountry>
and 
<collectingCountry id="F3067630FFBCFFCCFC3316BDFBD1FC0B" box="[966,1070,935,959]" name="Antarctica" pageId="16" pageNumber="17">Antarctic</collectingCountry>
Peninsula down to 
<specimenCount id="9D17FD29FFBCFFCCFAFE16BDFAA6FC74" box="[1291,1369,935,960]" pageId="16" pageNumber="17" type="generic">
<quantity id="4CE99B45FFBCFFCCFAFE16BDFAABFC74" box="[1291,1364,935,960]" metricMagnitude="2" metricUnit="m" metricValue="2.5" pageId="16" pageNumber="17" unit="m" value="250.0">250 m</quantity>
.
