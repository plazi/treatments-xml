<number id="3DD92487FFBCFFCCFDED169EFDDBFC28" box="[536,548,900,924]" pageId="16" pageNumber="17" value="3.0">3</number>
CYS 
<number id="3DD92487FFBCFFCCFD9C169EFD6CFC28" box="[617,659,900,924]" pageId="16" pageNumber="17" value="136.0">136</number>
(
<collectionCode id="ED00AE65FFBCFFCCFD57169EFD0BFC28" box="[674,756,900,924]" pageId="16" pageNumber="17">BOLD</collectionCode>
