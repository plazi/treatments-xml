<paragraph id="8BCE368D6C02FF91FECBFB38FD59FAD0" blockId="50.[265,683,1269,1294]" box="[265,683,1269,1294]" pageId="50" pageNumber="51">
<heading id="D08681E16C02FF91FECBFB38FD59FAD0" box="[265,683,1269,1294]" centered="true" fontSize="8" level="2" pageId="50" pageNumber="51" reason="2">
<taxonomicName id="4C714D0E6C02FF91FECBFB38FD59FAD0" authority="BENSON, 1947" authorityName="BENSON" authorityYear="1947" box="[265,683,1269,1294]" class="Insecta" family="Tenthredinidae" genus="Rhogogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="chambersi">
<emphasis id="B905EA9F6C02FF91FECBFB38FD59FAD0" bold="true" box="[265,683,1269,1294]" pageId="50" pageNumber="51">
<emphasis id="B905EA9F6C02FF91FECBFB38FDFBFAD3" bold="true" box="[265,521,1269,1293]" italics="true" pageId="50" pageNumber="51">Rhogogaster chambersi</emphasis>
<bibRefCitation id="EFE04B7C6C02FF91FDD2FB3BFD91FAD2" author="Benson, R. B." box="[528,611,1270,1294]" pageId="50" pageNumber="51" pagination="96 - 99" refId="ref29591" refString="Benson, R. B. 1947: A new British sawfly (Hymenoptera, Tenthredinidae) related to Rhogogaster picta (Klug). - Entomologist's Monthly Magazine 83: 96 - 99." type="journal article" year="1947">BENSON</bibRefCitation>
