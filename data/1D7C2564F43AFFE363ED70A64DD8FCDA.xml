<specimenCount id="83D35FFBF43AFFE2678070244B06F9C7" box="[1206,1373,1539,1561]" pageId="22" pageNumber="808" type="generic">ten specimens</specimenCount>
from the 
<typeStatus id="4A6E2AD0F43AFFE2604470054DF8F9E9" box="[882,931,1570,1591]" pageId="22" pageNumber="808">type</typeStatus>
