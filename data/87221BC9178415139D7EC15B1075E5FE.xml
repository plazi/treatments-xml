(Kuntze) Baker, 
<taxonomicName id="9FD5684489F16B23D3E3E53C93DA08EA" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia latispatha" order="Zingiberales" pageId="248" pageNumber="249" phylum="Tracheophyta" rank="species" species="latispatha">Heliconia latispatha</taxonomicName>
Benth. (
<bibRefCitation id="FA79CC26E9ADB6A70DC84252B8299BFF" pageId="248" pageNumber="249">Staines 1996</bibRefCitation>
); 
<taxonomicName id="3E6FC74C777C51060FD9C140047A1E58" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia trichocarpa" order="Zingiberales" pageId="248" pageNumber="249" phylum="Tracheophyta" rank="species" species="trichocarpa">Heliconia trichocarpa</taxonomicName>
G. S. Daniels and F. G. Stiles, 
<taxonomicName id="3CF0DDBF4F8E3BD63ACFDAB1C9DCC57B" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia wilsonii" order="Zingiberales" pageId="248" pageNumber="249" phylum="Tracheophyta" rank="species" species="wilsonii">Heliconia wilsonii</taxonomicName>
G. S. Daniels and F. G. Stiles (
<taxonomicName id="5E972C9A9C37D30482A6C083BED43C5F" genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="248" pageNumber="249" rank="genus">Heliconiaceae</taxonomicName>
), 
<taxonomicName id="86B2C9213BEF1A292DA8202042BBFEA2" class="Liliopsida" family="Marantaceae" genus="Calathea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Calathea crotalifera" order="Zingiberales" pageId="248" pageNumber="249" phylum="Tracheophyta" rank="species" species="crotalifera">Calathea crotalifera</taxonomicName>
S. Watson (
<taxonomicName id="AB41459E335238E6AAA9713277943153" genus="Marantaceae" lsidName="Marantaceae" pageId="248" pageNumber="249" rank="genus">Marantaceae</taxonomicName>
), 
<taxonomicName id="B6B57922D00A135EB37D8D9CB7548655" class="Liliopsida" family="Musaceae" genus="Musa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Musa velutina" order="Zingiberales" pageId="248" pageNumber="249" phylum="Tracheophyta" rank="species" species="velutina">Musa velutina</taxonomicName>
