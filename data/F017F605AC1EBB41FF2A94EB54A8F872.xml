<taxonomicName id="BFBE3C90AC1CBB41FE9C974C553FFAC5" authority="Kobjakova, 1971" authorityName="Kobjakova" authorityYear="1971" box="[289,790,1328,1350]" class="Malacostraca" family="Diogenidae" genus="Areopaguristes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="174" phylum="Arthropoda" rank="species" species="hirsutimanus" status="comb. nov.">
