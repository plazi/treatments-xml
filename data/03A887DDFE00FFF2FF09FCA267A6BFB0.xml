<taxonomicName id="4C014D48FE00FFF2FD18FB5F64FFBFDC" authority="St Hil" authorityName="St Hil" box="[646,944,1181,1206]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="10" pageNumber="579" phylum="Tracheophyta" rank="species" species="paraguariensis">
