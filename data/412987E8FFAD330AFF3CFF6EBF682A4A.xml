<caption id="9DFF6676FFAB330BFF3CF85BB8AD2C81" httpUri="https://zenodo.org/record/285226/files/figure.png" pageId="20" pageNumber="53" targetBox="[199,1387,193,1932]" targetPageId="20">
<paragraph id="C93F36FEFFAB330BFF3CF85BB8AD2C81" blockId="20.[151,1436,1954,2006]" pageId="20" pageNumber="53">
<emphasis id="FBF4EAECFFAB330BFF3CF85BBD0A2CEF" bold="true" box="[151,291,1954,1976]" pageId="20" pageNumber="53">FIGURE 11.</emphasis>
<taxonomicName id="0E804D7DFFAB330BFE85F85BBE912CEF" authority="Nunomura, 2004" authorityName="Nunomura" authorityYear="2004" box="[302,696,1954,1976]" class="Malacostraca" family="Gnathiidae" genus="Gnathia" kingdom="Animalia" order="Isopoda" pageId="20" pageNumber="53" phylum="Arthropoda" rank="species" species="mutsuensis">
<taxonomicName id="0E804D7DFFAB330BFE85F85BBE5F2CEF" authority="Nunomura" authorityName="Nunomura" box="[302,630,1954,1976]" class="Malacostraca" family="Gnathiidae" genus="Gnathia" higherTaxonomySource="CoL" kingdom="Animalia" order="Isopoda" pageId="20" pageNumber="53" phylum="Arthropoda" rank="species" species="mutsuensis">
<taxonomicName id="0E804D7DFFAB330BFE85F85BBDD22CEF" box="[302,507,1954,1976]" class="Malacostraca" family="Gnathiidae" genus="Gnathia" higherTaxonomySource="CoL" kingdom="Animalia" order="Isopoda" pageId="20" pageNumber="53" phylum="Arthropoda" rank="species" species="mutsuensis">
