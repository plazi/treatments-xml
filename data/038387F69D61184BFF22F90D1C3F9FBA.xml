<taxonomicName id="4C2A4D639D60184BFE06FECE1E4E9F2E" authority="Jaloszynski, 2007" authorityName="Jaloszynski" authorityYear="2007" box="[435,802,260,285]" class="Insecta" family="Scydmaenidae" genus="Cephennodes" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="400" phylum="Arthropoda" rank="species" species="spatulipes">
