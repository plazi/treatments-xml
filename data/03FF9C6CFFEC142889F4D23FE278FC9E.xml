<emphasis id="B922F168FFEC142889F4D23FE34CFD19" bold="true" box="[151,820,614,640]" pageId="11" pageNumber="284">
<taxonomicName id="4C5656F9FFEC142889F4D23FE13FFD19" box="[151,327,614,640]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="11" pageNumber="284" phylum="Arthropoda" rank="genus">
