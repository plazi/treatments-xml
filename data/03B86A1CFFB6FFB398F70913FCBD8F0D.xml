<taxonomicName id="4C11A089FFB4FFB4993B09A7FD918C8F" authority="Gonçalves &amp; Mejdalani, 2017" authorityName="Gonçalves &amp; Mejdalani" authorityYear="2017" box="[347,532,1952,1974]" class="Insecta" family="Cicadellidae" genus="Nelidina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="88" phylum="Arthropoda" rank="species" species="coronata" status="sp. nov.">
