<taxonomicName id="4C44F6F0AC136141FF28F163B418FDF9" authority="Tragardh, 1915" authorityName="Tragardh" authorityYear="1915" box="[151,572,538,564]" class="Arachnida" family="Tetranychidae" genus="Schizotetranychus" kingdom="Animalia" order="Prostigmata" pageId="10" pageNumber="43" phylum="Arthropoda" rank="genus">
