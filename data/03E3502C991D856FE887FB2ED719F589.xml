<taxonomicName id="4C4A9AB9991D856FE8D1FB2ED637F482" authority="(Zetterstedt, 1843) (Sympycninae)" authorityName="Sympycninae" baseAuthorityName="Zetterstedt" baseAuthorityYear="1843" box="[193,870,1232,1260]" class="Insecta" family="Dolichopodidae" genus="Campsicnemus" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="pumilio">
