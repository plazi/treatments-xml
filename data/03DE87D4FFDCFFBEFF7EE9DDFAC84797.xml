<taxonomicName id="4C774D41FFDDFFBDFA6CEB8BFEEE41AF" class="Insecta" family="Eulichadidae" genus="Eulichas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="tenuicornis">
