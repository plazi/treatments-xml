<quantity id="4CDB9B6722577B46FF03FE87FEF2FECA" box="[255,333,345,371]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="16" pageNumber="17" unit="m" value="800.0">800 m</quantity>
