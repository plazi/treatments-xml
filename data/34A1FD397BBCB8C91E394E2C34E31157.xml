<taxonomicName id="133B0A2F3DAD392C44BC3CB778F7C972" class="Magnoliopsida" family="Asteraceae" genus="Doronicum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="490" phylum="Tracheophyta" rank="species" species="pardalianches">
<italic id="CF7983D55436FB594B5569F450235F12" pageId="null" pageNumber="490">D. Pardalianches</italic>
</taxonomicName>
(Nr. 1)
