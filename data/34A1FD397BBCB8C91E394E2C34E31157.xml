<td id="135238DCD1E61F6355C8EB4632D78A03" align="left" pageId="null" pageNumber="490" valign="top">
3*. Pflanze ohne 
<normalizedToken id="F21856BB151F842D7281414D012A3D07" originalValue="Ausläufer" pageId="null" pageNumber="490">Auslaeufer</normalizedToken>
; 
<normalizedToken id="EF1B3A44C54D4807FD20C2CAADEF1ABF" originalValue="Blätter" pageId="null" pageNumber="490">Blaetter</normalizedToken>
