<specimenCode id="DBD29EE149190D26FDD5BAD9FD74FB07" box="[609,746,1218,1242]" collectionCode="G" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="5" pageNumber="46" type="Herbarium">G00113038</specimenCode>
