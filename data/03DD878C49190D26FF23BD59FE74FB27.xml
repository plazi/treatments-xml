<collectionCode id="ED65AE5F49190D26FD88BAD9FDCCFB07" box="[572,594,1218,1242]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="5" pageNumber="46" type="Herbarium">G</collectionCode>
