<heading id="303E2E5DFFFCFB739709FBDEFB938F4A" bold="true" box="[598,1034,1088,1118]" centered="true" fontSize="12" level="1" pageId="10" pageNumber="11" reason="0">
<taxonomicName id="ACC9E2B2FFFCFB739709FBDEFB938F4A" authority="Waldock 2013" authorityName="Waldock" authorityYear="2013" box="[598,1034,1088,1118]" class="Arachnida" family="Salticidae" genus="Maratus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="karrie">
