<taxonomicName id="56C48AB28B13FFA1195FFAFDFA97FADA" box="[1142,1320,1332,1357]" class="Malacostraca" family="Palicidae" genus="Rectopalicus" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="143" phylum="Arthropoda" rank="species" species="woodmasoni">
