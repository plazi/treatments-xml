<heading id="5E590C14FFADFF8B26208EA9D3FFF9A3" bold="true" box="[264,505,1518,1544]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<emphasis id="37DA676AFFADFF8B26208EA9D3FFF9A3" bold="true" box="[264,505,1518,1544]" pageId="1" pageNumber="2">
<taxonomicName id="C2AEC0FBFFADFF8B26208EA9D3AEF9A3" box="[264,424,1518,1544]" class="Insecta" family="Phlaeothripidae" genus="Apostlethrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus" status="gen.n.">
<emphasis id="37DA676AFFADFF8B26208EA9D3AEF9A3" bold="true" box="[264,424,1518,1544]" italics="true" pageId="1" pageNumber="2">Apostlethrips</emphasis>
</taxonomicName>
<taxonomicNameLabel id="2CE9DA11FFADFF8B26878EA9D3FFF9A3" box="[431,505,1518,1544]" pageId="1" pageNumber="2" rank="genus">gen.n.</taxonomicNameLabel>
</emphasis>
