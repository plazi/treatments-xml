species 
<taxonomicName id="C2AEC0FBFFAEFF8826FD89C3D0D5FD36" box="[469,723,644,669]" class="Insecta" family="Phlaeothripidae" genus="Apostlethrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="apostus" status="sp.n.">
<emphasis id="37DA676AFFAEFF8826FD89C3D0D5FD36" box="[469,723,644,669]" italics="true" pageId="2" pageNumber="3">Apostlethrips apostus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="2CE9DA11FFAEFF8825F289C3D117FD35" box="[730,785,644,670]" pageId="2" pageNumber="3" rank="species">sp.n.</taxonomicNameLabel>
</paragraph>
