<number id="3DAC1517CC6F4E38FD3E7FF88AE08288" box="[734,766,713,739]" pageId="24" pageNumber="25" value="35.0">35</number>
), and 
<taxonomicName id="4C647CB3CC6F4E38FCA77FF88BA78289" box="[839,953,713,738]" class="Polychaeta" family="Syllidae" genus="Epigamia" kingdom="Animalia" order="Phyllodocida" pageId="24" pageNumber="25" phylum="Annelida" rank="genus">
<emphasis id="B910DB22CC6F4E38FCA77FF88BA78289" box="[839,953,713,738]" italics="true" pageId="24" pageNumber="25">Epigamia</emphasis>
