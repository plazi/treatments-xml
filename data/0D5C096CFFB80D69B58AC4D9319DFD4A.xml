), PW 
<quantity id="420D159FFFBB0D69B031C59235E7FF6E" metricMagnitude="-4" metricUnit="m" metricValue="5.05" metricValueMax="5.3" metricValueMin="4.8" pageId="14" pageNumber="519" unit="mm" value="0.505" valueMax="0.53" valueMin="0.48">0.48–0.53 mm</quantity>
