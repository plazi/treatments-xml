<emphasis id="B9357D72FF90FFDEFE0AF9195FD7F92D" box="[444,709,1692,1715]" italics="true" pageId="5" pageNumber="32">
Anisodiplosis 
<taxonomicName id="4C41DAE3FF90FFDEFDE5F9195FD7F92D" authority="Maia" authorityName="Maia" box="[595,709,1692,1715]" pageId="5" pageNumber="32" rank="species" species="waltheriae" status="sp. nov.">waltheriae</taxonomicName>
