(
<figureCitation id="137ABDE5FF97FFD9FD42F9E95E66F918" box="[756,884,1644,1670]" captionStart-0="FIGURES 1 – 4" captionStart-1="FIGURES 5 8" captionStart-2="FIGURES 9 – 10" captionStart-3="FIGURES 11 – 13" captionStart-4="FIGURES 14 – 17" captionStart-5="FIGURES 18 – 19" captionStartId-0="4.[264,362,1834,1858]" captionStartId-1="5.[264,382,1692,1716]" captionStartId-2="6.[264,382,1449,1473]" captionStartId-3="7.[264,382,1497,1521]" captionStartId-4="8.[264,383,1705,1729]" captionStartId-5="9.[264,382,988,1012]" captionTargetBox-0="[321,1281,335,1781]" captionTargetBox-1="[266,1289,307,1692]" captionTargetBox-2="[421,1180,356,1436]" captionTargetBox-3="[385,1176,313,1455]" captionTargetBox-4="[355,1228,338,1650]" captionTargetBox-5="[264,1322,283,981]" captionTargetId-0="figure@4.[271,1315,318,1808]" captionTargetId-1="figure@5.[211,1435,284,1692]" captionTargetId-2="figure@6.[308,1209,329,1439]" captionTargetId-3="figure@7.[357,1272,302,1491]" captionTargetId-4="figure@8.[294,1282,302,1693]" captionTargetId-5="figure@9.[793,1323,280,986]" captionTargetPageId-0="4" captionTargetPageId-1="5" captionTargetPageId-2="6" captionTargetPageId-3="7" captionTargetPageId-4="8" captionTargetPageId-5="9" captionText-0="FIGURES 1 – 4. Anisodiplosis waltheriae Maia, sp. nov. 1, Male head, frontal view. 2, Female head, frontal view. 3, Male antennal flagellomere 5. 4, Female antennal flagellomere 5. Scale bars in mm." captionText-1="FIGURES 5 8. Anisodiplosis waltheriae Maia, sp. nov. 5, Female wing. 6, Female midtarsal claw and empodium, lateral view. 7, Male abdominal segment 6 to end, dorsolateral view. 8, Female abdominal segment 6 to end, lateral view. Scale bars in mm." captionText-2="FIGURES 9 – 10. Anisodiplosis waltheriae Maia, sp. nov. 9, Male terminalia, dorsal view. 10, Female terminalia, lateral view. Scale bars in mm." captionText-3="FIGURES 11 – 13. Anisodiplosis waltheriae Maia, sp. nov. 11, Female terminalia, lateral view. 12, Pupa, cephalic region, frontal view. 13, Prothoracic spiracle. Scale bars in mm." captionText-4="FIGURES 14 – 17. Anisodiplosis waltheriae Maia, sp. nov. 14, Terminal segments, dorsal view. 15, Larva, general aspect, dorsal view. 16, Spatula, lateral and sternal papillae. 17, Larva, posterior segments, dorsal view. Scale bars in mm." captionText-5="FIGURES 18 – 19. Galls of Anisodiplosis waltheriae Maia, sp. nov. on Waltheria indica. 18, Galls on adaxial leaf surface. 19, Galls on inflorescence buds and leaf axis." httpUri-0="https://zenodo.org/record/170168/files/figure.png" httpUri-1="https://zenodo.org/record/170169/files/figure.png" httpUri-2="https://zenodo.org/record/170170/files/figure.png" httpUri-3="https://zenodo.org/record/170171/files/figure.png" httpUri-4="https://zenodo.org/record/170172/files/figure.png" httpUri-5="https://zenodo.org/record/170173/files/figure.png" pageId="2" pageNumber="29">
Figs. 
<number id="3D89B347FF97FFD9FC8EF9E95E66F918" box="[824,884,1644,1670]" pageId="2" pageNumber="29" value="10.0" valueMax="19.0" valueMin="1.0">1–19</number>
</figureCitation>
)
