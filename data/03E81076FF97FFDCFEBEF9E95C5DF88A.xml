<caption id="DF3EF1E8FF93FFDDFEBEFA2C5E0DFA7D" httpUri="https://zenodo.org/record/170170/files/figure.png" pageId="6" pageNumber="33" targetBox="[421,1180,356,1436]" targetPageId="6">
<paragraph id="8BFEA160FF93FFDDFEBEFA2C5E0DFA7D" blockId="6.[264,1323,1447,1507]" pageId="6" pageNumber="33">
<emphasis id="B9357D72FF93FFDDFEBEFA2C5CD5FA5F" bold="true" box="[264,455,1447,1473]" pageId="6" pageNumber="33">FIGURES 9–10.</emphasis>
<taxonomicName id="4C41DAE3FF93FFDDFE65FA2C5E34FA5E" authority="Maia" authorityName="Maia" box="[467,806,1449,1472]" class="Insecta" family="Cecidomyiidae" genus="Anisodiplosis" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="33" phylum="Arthropoda" rank="species" species="waltheriae" status="sp. nov.">
<taxonomicName id="4C41DAE3FF93FFDDFE65FA2C5FF2FA5E" box="[467,736,1449,1472]" class="Insecta" family="Cecidomyiidae" genus="Anisodiplosis" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="33" phylum="Arthropoda" rank="species" species="waltheriae">
<emphasis id="B9357D72FF93FFDDFE65FA2C5FF2FA5E" box="[467,736,1449,1472]" italics="true" pageId="6" pageNumber="33">
<taxonomicName id="4C41DAE3FF93FFDDFE65FA2C5F71FA5E" box="[467,611,1449,1472]" class="Insecta" family="Cecidomyiidae" genus="Anisodiplosis" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="33" phylum="Arthropoda" rank="genus">Anisodiplosis</taxonomicName>
<taxonomicName id="4C41DAE3FF93FFDDFDD8FA2C5FF2FA5E" authority="Maia" authorityName="Maia" box="[622,736,1449,1472]" pageId="6" pageNumber="33" rank="species" species="waltheriae" status="sp. nov.">waltheriae</taxonomicName>
