<taxonomicName id="4C734D51FF8F1760FECB68B8ABA0FD7D" box="[272,444,729,752]" class="Arachnida" family="Pionidae" genus="Pionacercopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="118" phylum="Arthropoda" rank="genus">
<emphasis id="B907EAC0FF8F1760FECB68B8ABA0FD7D" box="[272,444,729,752]" italics="true" pageId="7" pageNumber="118">Pionacercopsis</emphasis>
