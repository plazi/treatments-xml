<taxonomicName id="74A94D488C7A043ED145B9DFEDCAD434" authority="Urban, 1993" authorityName="Urban" authorityYear="1993" box="[616,980,1770,1793]" class="Insecta" family="Megachilidae" genus="Ctenanthidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="313" phylum="Arthropoda" rank="species" species="gracile">
