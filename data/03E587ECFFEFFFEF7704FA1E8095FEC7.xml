<taxonomicName id="4C4C4D79FFEFFFE87704FA1E83E6F9C8" authorityName="Illiger" authorityYear="1811" box="[829,905,1532,1556]" class="Mammalia" kingdom="Animalia" order="Sirenia" pageId="91" phylum="Chordata" rank="order">Sirenia</taxonomicName>
(
<taxonomicName id="4C4C4D79FFEFFFE877AEFA1E8447F9C8" authorityName="Gill" authorityYear="1872" box="[919,1064,1532,1556]" class="Mammalia" family="Trichechidae" kingdom="Animalia" order="Sirenia" pageId="91" phylum="Chordata" rank="family">Trichechidae</taxonomicName>
