<taxonomicName id="4C254D7CFFD5FFA05BDFF9D9FCD1F952" box="[663,775,1648,1671]" class="Branchiopoda" family="Cyzicidae" genus="Eocyzicus" kingdom="Animalia" order="Diplostraca" pageId="6" pageNumber="214" phylum="Arthropoda" rank="genus">
