Six specimens: near to Cueva El Infierno (
<geoCoordinate id="EE1F95801100FFB83E1CFF38FBDCFF3B" box="[871,1044,188,215]" degrees="25" direction="north" minutes="18" orientation="latitude" pageId="15" pageNumber="516" precision="1" seconds="30.95" value="25.308596">25°18′30.95′′N</geoCoordinate>
, 
<geoCoordinate id="EE1F95801100FFB83964FF38FB14FF3B" box="[1055,1244,188,215]" degrees="100" direction="west" minutes="12" orientation="longitude" pageId="15" pageNumber="516" precision="1" seconds="37.85" value="-100.21051">100°12′37.85′′W</geoCoordinate>
, 
<emphasis id="B95F2F551100FFB83993FF3AFAC2FF3B" box="[1256,1290,190,215]" italics="true" pageId="15" pageNumber="516">ca.</emphasis>
<quantity id="4CD35EA21100FFB8386AFF38FAAEFF3B" box="[1297,1382,188,215]" metricMagnitude="3" metricUnit="m" metricValue="1.5219999999999998" pageId="15" pageNumber="516" unit="m" value="1522.0">1522 m</quantity>
