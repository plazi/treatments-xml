<taxonomicName id="4C7982BEFF80C509FD87FC2BFD5CFD4A" box="[593,756,629,654]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="rubicundus">
