<taxonomicName id="4C7982BEFF80C509FB2BFC0EFA35FDAD" box="[1277,1437,592,617]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="flavescens">
