<emphasis id="C1BD9D34FF9EFFCD01C8F996FD2CD9D4" box="[433,668,1652,1674]" italics="true" pageId="31" pageNumber="1074">
Namibiobolbu s 
<taxonomicName id="34C93AA5FF9EFFCD022BF996FD2CD9D4" authorityName="Kolbe" authorityYear="1907" box="[594,668,1652,1674]" class="Insecta" family="Bolboceratidae" genus="Bolboceras" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="1074" phylum="Arthropoda" rank="species" species="iphicles">iphicles</taxonomicName>
</emphasis>
<emphasis id="C1BD9D34FF9EFFCD02D3F991FD0DD9D6" bold="true" box="[682,701,1651,1672]" pageId="31" pageNumber="1074">E</emphasis>
; 
<taxonomicName id="34C93AA5FF9EFFCD02A8F997FCF5D9D4" baseAuthorityName="Krikken" baseAuthorityYear="1977" box="[721,837,1652,1674]" class="Insecta" family="Bolboceratidae" genus="Namibiobolbus" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="1074" phylum="Arthropoda" rank="species" species="heracles">
<emphasis id="C1BD9D34FF9EFFCD02A8F997FCF5D9D4" box="[721,837,1652,1674]" italics="true" pageId="31" pageNumber="1074">N. heracles</emphasis>
</taxonomicName>
