<taxonomicName id="4C348BC654789549372E78B8FE31FF1A" box="[327,490,152,178]" class="Chilopoda" family="Mecistocephalidae" genus="Arrup" kingdom="Animalia" order="Geophilomorpha" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="undetermined">
