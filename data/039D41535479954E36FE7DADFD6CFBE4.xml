<quantity id="4CCC5DA05478954935EC78B8FC19FF1A" box="[901,962,152,178]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" pageId="11" pageNumber="12" unit="cm" value="5.0">5 cm</quantity>
