<taxonomicName id="4C754D221257FFD6FED45785FD8EF93D" authority="Liu, 2000" authorityName="Liu" authorityYear="2000" box="[281,620,1695,1717]" class="Insecta" family="Tettigoniidae" genus="Sinocyrtaspis" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="131" phylum="Arthropoda" rank="species" species="truncata">
