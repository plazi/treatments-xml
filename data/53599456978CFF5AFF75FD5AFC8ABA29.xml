<typeStatus id="044B9BE2978CFF5AFF75FC92FF37BC1B" box="[151,201,799,820]" pageId="219" pageNumber="220">Type</typeStatus>
species: 
<taxonomicName id="1CF05EC3978CFF5AFEC4FC93FD86BC1B" authority="Karny, 1928 a" authorityName="Karny" authorityYear="1928" box="[294,632,798,820]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="219" pageNumber="220" phylum="Arthropoda" rank="species" species="xiphiura">
<emphasis id="E984F952978CFF5AFEC4FC93FE14BC1B" box="[294,490,798,820]" italics="true" pageId="219" pageNumber="220">Gryllacris xiphiura</emphasis>
<bibRefCitation id="BF6158B1978CFF5AFE12FC92FD86BC1B" author="Karny, H. H." box="[496,632,798,820]" pageId="219" pageNumber="220" pagination="215 - 260" refId="ref187555" refString="Karny, H. H. (1928 a) Revision der Gryllacriden des ungarischen National-Museums. Annales Historico-Naturales Musei Nationalis Hungarici, Pars Zoologica, 25, 215 - 260." type="journal article" year="1928">Karny, 1928a</bibRefCitation>
</taxonomicName>
