<collectingCountry id="F33B45A99B77FFD4FE0DFCDD95CFFCA7" box="[433,491,829,854]" name="India" pageId="3" pageNumber="176">India</collectingCountry>
(
<collectingRegion id="49E8CBDB9B77FFD4FE42FCDD96ABFCA7" box="[510,655,829,854]" country="India" name="Maharashtra" pageId="3" pageNumber="176">Maharashtra</collectingRegion>
; 
<collectingRegion id="49E8CBDB9B77FFD4FD22FCDE9710FCA7" box="[670,820,829,854]" country="India" name="West Bengal" pageId="3" pageNumber="176">West Bengal</collectingRegion>
), 
<collectingCountry id="F33B45A99B77FFD4FCF0FCDE9758FCA4" box="[844,892,830,853]" name="Iran" pageId="3" pageNumber="176">Iran</collectingCountry>
, 
<collectingCountry id="F33B45A99B77FFD4FC37FCDD97D9FCA7" box="[907,1021,829,854]" name="Sri Lanka" pageId="3" pageNumber="176">Sri Lanka</collectingCountry>
and 
<collectingCountry id="F33B45A99B77FFD4FB80FCDD9084FCA7" box="[1084,1184,829,854]" name="Thailand" pageId="3" pageNumber="176">Thailand</collectingCountry>
(
