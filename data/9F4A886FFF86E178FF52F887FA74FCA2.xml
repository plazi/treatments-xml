<number id="A12B2B5EFF87E178FD3DFD77FCC2FD4E" box="[760,810,679,700]" pageId="1" pageNumber="188" value="2002.0">2002</number>
, fig. 
<number id="A12B2B5EFF87E178FC9EFD77FC8DFD4E" box="[859,869,679,700]" pageId="1" pageNumber="188" value="1.0">1</number>
