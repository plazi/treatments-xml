<keyStep id="AC1721DCFF87E178FF52FD15FA74FCA2" pageId="1" pageNumber="188">
<paragraph id="175C3979FF87E178FF52FD15FA74FCE6" blockId="1.[151,1436,151,848]" pageId="1" pageNumber="188">
<keyLead id="AC129A4CFF87E178FF52FD15FA74FCE6" pageId="1" pageNumber="188">
<number id="A12B2B5EFF87E178FF52FD15FF4BFD28" box="[151,163,709,730]" pageId="1" pageNumber="188" value="8.0">8</number>
