<taxonomicName id="4E945BB5E81A19E3AC64ED97B90F9379" class="Insecta" family="Pyralidae" genus="Lista" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lista insulsalis" order="Lepidoptera" pageId="9" pageNumber="106" phylum="Arthropoda" rank="species" species="insulsalis">Lista insulsalis</taxonomicName>
