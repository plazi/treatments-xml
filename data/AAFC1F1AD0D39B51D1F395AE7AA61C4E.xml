<taxonomicName id="B69710032FD554076FD2C8B50D65942D" class="Insecta" family="Arctiidae" genus="Paracles" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracles severa" order="Lepidoptera" pageId="9" pageNumber="74" phylum="Arthropoda" rank="species" species="severa">Paracles severa</taxonomicName>
