<subSubSection id="C531E9ED3C5D44E21A3242B7F31FD1E4" pageId="2" pageNumber="13" type="adelphotaxon">
<paragraph id="2D27702B1B270DBD3CE81EC9B746975C" pageId="2" pageNumber="13">Adelphotaxon.</paragraph>
<paragraph id="DE48A82972A745EE8688217193CA1B6A" pageId="2" pageNumber="13">
Probably 
<taxonomicName id="86FC40D81E218436B13CE52DA6FACFE3" authorityName="Baehr" authorityYear="1997" class="Insecta" family="Carabidae" genus="Notopseudomorpha" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="2" pageNumber="13" phylum="Arthropoda" rank="genus">
