<date id="43078780FFABFFD4FCFBF8F4FC040C14" box="[822,911,1824,1856]" pageId="3" pageNumber="4" value="1932-02-25">
<number id="8171B367FFABFFD4FCFBF8F4FCC90C6E" box="[822,834,1835,1856]" pageId="3" pageNumber="4" value="2.0">2</number>
<number id="8171B367FFABFFD4FC85F8FFFC040C14" box="[840,911,1824,1850]" pageId="3" pageNumber="4" value="28.5" valueMax="32.0" valueMin="25.0">25­32</number>
</date>
, 
<emphasis id="05CD7D52FFABFFD4FC51F8FFFC210C17" box="[924,938,1824,1849]" italics="true" pageId="3" pageNumber="4">h</emphasis>
<date id="43078780FFABFFD4FC67F8F4FB8F0C14" box="[938,1028,1824,1856]" pageId="3" pageNumber="4" value="1922-01-28">
