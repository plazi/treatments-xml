<taxonomicName id="306C41E88CC06B268226A8703A438523" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vaccinium myrtillus" order="Ericales" pageId="0" pageNumber="1068" phylum="Tracheophyta" rank="species" species="myrtillus">Vaccinium myrtillus</taxonomicName>
