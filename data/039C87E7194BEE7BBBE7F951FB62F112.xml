<taxonomicName id="4C354D72194BEE7BBA94F882FDF7F7FE" authorityName="Ricker" authorityYear="1952" box="[459,554,1913,1937]" class="Insecta" family="Nemouridae" genus="Soyedina" kingdom="Animalia" order="Plecoptera" pageId="8" pageNumber="107" phylum="Arthropoda" rank="genus">
