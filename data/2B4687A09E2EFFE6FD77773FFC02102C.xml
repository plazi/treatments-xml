[
<taxonomicName id="64EF4D359E2EFFE6FD78773EFB8C1576" authority="Dalmas, 1900" authorityName="Dalmas. This" authorityYear="1900" box="[715,1146,804,827]" class="Aves" family="Vireonidae" genus="Hylophilus" kingdom="Animalia" order="Passeriformes" pageId="42" pageNumber="42" phylum="Chordata" rank="species" species="pallidifrons">
<emphasis id="919BEAA49E2EFFE6FD78773EFC391576" box="[715,975,804,827]" italics="true" pageId="42" pageNumber="42">Hylophilus pallidifrons</emphasis>
<bibRefCitation id="C77E4B479E2EFFE6FC6B773FFB8C1576" author="Dalmas, R." box="[984,1146,804,827]" pageId="42" pageNumber="42" pagination="132 - 144" refId="ref115320" refString="Dalmas, R. 1900. Note sur une collection d'oiseaux de l'ile de Tobago (mer des Antilles). Memoires de la Societe Zoologique de France 13: 132 - 144." type="journal article" year="1900">Dalmas, 1900</bibRefCitation>
