<taxonomicName id="24F550E9475EFFD8C202E14BFA41F9A3" authority="Wimmer" authorityName="Wimmer" box="[1282,1428,1623,1648]" class="Magnoliopsida" family="Fabaceae" genus="Sarothamnus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="416" phylum="Magnoliophyta" rank="genus">
<emphasis id="D181F778475EFFD8C202E14BFA41F9A3" box="[1282,1428,1623,1648]" italics="true" pageId="15" pageNumber="416">Sarothamnus</emphasis>
</taxonomicName>
, 
<taxonomicName id="24F550E9475EFFD8C797E160FF19F946" box="[151,204,1660,1685]" class="Magnoliopsida" family="Fabaceae" genus="Ulex" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="416" phylum="Tracheophyta" rank="genus">
