<taxonomicName id="24F550E9475EFFD8C336E144FB5CF9BC" box="[1078,1161,1624,1647]" class="Magnoliopsida" family="Fabaceae" genus="Cytisus" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="416" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F778475EFFD8C336E144FB5CF9BC" box="[1078,1161,1624,1647]" italics="true" pageId="15" pageNumber="416">Cytisus</emphasis>
</taxonomicName>
, 
<taxonomicName id="24F550E9475EFFD8C39AE144FB24F9BC" box="[1178,1265,1624,1647]" class="Magnoliopsida" family="Fabaceae" genus="Genista" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="416" phylum="Tracheophyta" rank="genus">
<emphasis id="D181F778475EFFD8C39AE144FB24F9BC" box="[1178,1265,1624,1647]" italics="true" pageId="15" pageNumber="416">Genista</emphasis>
</taxonomicName>
