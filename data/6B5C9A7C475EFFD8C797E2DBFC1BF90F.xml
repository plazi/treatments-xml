<taxonomicName id="24F550E9475EFFD8C482E14BFC25F9A3" box="[898,1008,1623,1648]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="416" phylum="Tracheophyta" rank="tribe" tribe="Genisteae">Genisteae</taxonomicName>
