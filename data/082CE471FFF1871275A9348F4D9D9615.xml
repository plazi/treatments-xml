<taxonomicName id="47852EE4FFF1871274CC309D4D1E9798" authorityName="Zettel" authorityYear="2009" box="[498,625,1157,1182]" class="Insecta" family="Gerridae" genus="Archaeoptilomera" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="40" phylum="Arthropoda" rank="species" species="kodadai" status="sp. n.">
