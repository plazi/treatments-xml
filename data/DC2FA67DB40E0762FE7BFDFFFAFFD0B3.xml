<taxonomicName id="93866CE8B40D0763FF07FA9EFB75D413" authority="Lourenco, 2000" authorityName="Lourenco" authorityYear="2000" box="[198,345,1364,1385]" class="Arachnida" family="Buthidae" genus="Tityus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scorpiones" pageId="6" pageNumber="453" phylum="Arthropoda" rank="subSpecies" species="ityus" status="n. sp." subSpecies="prancei">
<emphasis id="66F2CB79B40D0763FF07FA9EFB75D413" box="[198,345,1364,1385]" italics="true" pageId="6" pageNumber="453">T ityus prancei</emphasis>
</taxonomicName>
<taxonomicNameLabel id="7DC17602B40D0763FEA2FA99FBB3D412" box="[355,415,1363,1384]" pageId="6" pageNumber="453" rank="species">n. sp.</taxonomicNameLabel>
