<collectionCode id="ED492DD2FF81FF90FB0120FF5F4CFF30" box="[1267,1376,198,219]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="10" pageNumber="1333" type="Museum">NHMUK</collectionCode>
