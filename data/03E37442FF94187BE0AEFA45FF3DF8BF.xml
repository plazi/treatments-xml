<typeStatus id="54F17BF6FF94187BE18EF98AFE3DF984" box="[439,492,1581,1604]" pageId="16" pageNumber="57">Type</typeStatus>
species 
<taxonomicName id="4C4ABED7FF94187BE27EF98AFC5BF984" authority="Reuter." authorityName="Reuter." box="[583,906,1581,1604]" class="Insecta" family="Phlaeothripidae" genus="Phloeothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="16" pageNumber="57" phylum="Arthropoda" rank="species" species="setinodis">
<emphasis id="B93E1946FF94187BE27EF98AFCE7F984" box="[583,822,1581,1604]" italics="true" pageId="16" pageNumber="57">Phloeothrips setinodis</emphasis>
