<taxonomicName id="6740B3E9FA5B881F7371243FA756D22E" authority="Bezzi, 1912" authorityName="Bezzi" authorityYear="1912" class="Insecta" family="Syrphidae" genus="Protylocera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Protylocera melanthysana" order="Diptera" pageId="0" pageNumber="83" phylum="Arthropoda" rank="species" species="melanthysana">Protylocera melanthysana</taxonomicName>
