<paragraph id="8BC93725FFB7FFD740BFFBFEFC76E010" blockId="18.[151,1430,1102,1525]" box="[671,916,1102,1127]" pageId="18" pageNumber="19">
<taxonomicName id="4C764CA6FFB7FFD740BFFBFEFC76E010" authority="Bliven" authorityName="Bliven" box="[671,916,1102,1127]" class="Insecta" family="Psyllidae" genus="Nyctiphalerus" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
