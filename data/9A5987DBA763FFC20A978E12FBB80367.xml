<taxonomicName id="D5F04D4EA763FFC209718E6EFC0D006B" box="[881,989,1362,1385]" class="Insecta" family="Halictidae" genus="Halictus" kingdom="Animalia" order="Hymenoptera" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="versans">
