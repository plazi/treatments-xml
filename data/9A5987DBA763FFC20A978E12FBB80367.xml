</taxonomicName>
, 
<taxonomicName id="D5F04D4EA763FFC208A88E6EFCE6006B" box="[680,822,1361,1385]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="117" pageNumber="118" phylum="Arthropoda" rank="species" species="ruidosense">
