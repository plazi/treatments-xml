<taxonomicName id="E479B3982CD097A40B943A7D1A9108B2" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="416" phylum="Tracheophyta" rank="species" species="phyllostachys">
<italic id="3AA5E32E92265818FC74E0E97FB0E248" pageId="null" pageNumber="416">R. phyllostachys</italic>
</taxonomicName>
