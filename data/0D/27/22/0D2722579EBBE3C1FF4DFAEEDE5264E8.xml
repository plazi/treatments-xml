<paragraph blockId="222.[151,1437,1409,1976]" box="[199,1018,1518,1542]" pageId="222" pageNumber="223">
Body length 
<quantity box="[343,431,1518,1542]" metricMagnitude="-3" metricUnit="m" metricValue="4.9" pageId="222" pageNumber="223" unit="mm" value="4.9">4.9 mm</quantity>
