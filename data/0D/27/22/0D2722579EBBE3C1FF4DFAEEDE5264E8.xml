<quantity box="[635,720,1518,1542]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="222" pageNumber="223" unit="mm" value="3.9">3.9 mm</quantity>
and antenna length 
<quantity box="[946,1014,1518,1542]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="222" pageNumber="223" unit="mm" value="6.0">6 mm</quantity>
