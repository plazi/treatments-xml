<quantity box="[343,431,325,349]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="204" pageNumber="205" unit="mm" value="4.3">4.3 mm</quantity>
, fore wing length 
<quantity box="[635,720,325,349]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" pageId="204" pageNumber="205" unit="mm" value="3.4">3.4 mm</quantity>
and antenna length more than 
<quantity box="[1066,1155,325,349]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="204" pageNumber="205" unit="mm" value="4.5">4.5 mm</quantity>
