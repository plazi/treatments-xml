, 
<taxonomicName authority="Sharaf &amp; Aldawood, 2012" authorityName="Sharaf &amp; Aldawood" authorityYear="2012" box="[753,1258,583,608]" class="Insecta" family="Formicidae" genus="Tapinoma" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="species" species="wilsoni">
