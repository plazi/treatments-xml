Hlaváč, 
<number box="[605,661,476,500]" pageId="4" pageNumber="391" value="2013.0">2013</number>
</taxonomicName>
(
<taxonomicName box="[678,807,476,501]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
: 
<taxonomicName box="[820,959,476,501]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="subFamily" subFamily="Pselaphinae">Pselaphinae</taxonomicName>
); 
<taxonomicName authority="Assing, 2013" authorityName="Assing" authorityYear="2013" box="[981,1395,476,501]" class="Insecta" family="Staphylinidae" genus="Pinophilinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="species" species="spinosus">
