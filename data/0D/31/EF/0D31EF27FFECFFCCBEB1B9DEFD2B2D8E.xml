Assing, 
<number box="[1336,1395,476,500]" pageId="4" pageNumber="391" value="2013.0">2013</number>
</taxonomicName>
; an undescribed 
<taxonomicName box="[303,426,511,536]" class="Insecta" family="Staphylinidae" genus="Peltodonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="genus">
<emphasis box="[303,426,511,536]" italics="true" pageId="4" pageNumber="391">Peltodonia</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[498,627,511,536]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
