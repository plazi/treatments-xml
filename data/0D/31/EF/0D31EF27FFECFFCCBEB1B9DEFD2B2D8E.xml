<taxonomicName box="[1313,1436,511,536]" class="Insecta" family="Anthicidae" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="family">Anthicidae</taxonomicName>
(
<taxonomicName box="[158,288,548,573]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
