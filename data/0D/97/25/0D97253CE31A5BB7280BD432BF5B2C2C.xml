<subSubSection pageId="21" pageNumber="38" type="taxonomic note">
<paragraph pageId="21" pageNumber="38">Taxonomic note.</paragraph>
<paragraph pageId="21" pageNumber="38">
<taxonomicName class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole plagiaria" order="Hymenoptera" pageId="21" pageNumber="38" phylum="Arthropoda" rank="species" species="plagiaria">Pheidole plagiaria</taxonomicName>
