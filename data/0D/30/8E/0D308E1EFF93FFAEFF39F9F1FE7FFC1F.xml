<collectionCode LSID="urn:lsid:biocol.org:col:34840" box="[530,614,472,498]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34840" name="Staatliches Museum fuer Naturkund Stuttgart" pageId="28" pageNumber="29">SMNS</collectionCode>
, cAss); 
<number box="[717,732,472,498]" pageId="28" pageNumber="29" value="1.0">1</number>
ex., NW Baku, Altyagach, 
<quantity box="[1077,1174,472,498]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="28" pageNumber="29" unit="m" value="1200.0">1200 m</quantity>
, 
<date box="[1189,1377,472,498]" pageId="28" pageNumber="29" value="1996-06-21" valueMax="1996-06-23" valueMin="1996-06-21">
<number box="[1189,1219,472,498]" pageId="28" pageNumber="29" value="21.0">21</number>
.â€“ 
<number box="[1240,1271,472,498]" pageId="28" pageNumber="29" value="23.0">23</number>
.VI. 
<number box="[1313,1377,472,498]" pageId="28" pageNumber="29" value="1996.0">1996</number>
</date>
, leg. Schawaller (
<collectionCode LSID="urn:lsid:biocol.org:col:34840" box="[297,383,512,538]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34840" name="Staatliches Museum fuer Naturkund Stuttgart" pageId="28" pageNumber="29">SMNS</collectionCode>
