<taxonomicName class="Aves" family="Vireonidae" genus="Hylophilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hylophilus" order="Passeriformes" pageId="193" pageNumber="194" phylum="Chordata" rank="genus">Hylophilus</taxonomicName>
