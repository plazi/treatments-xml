<subSubSection box="[199,1365,330,355]" pageId="11" pageNumber="27" type="diagnosis">
<paragraph blockId="11.[151,1436,258,355]" box="[199,1365,330,355]" pageId="11" pageNumber="27">
<emphasis bold="true" box="[199,314,330,355]" pageId="11" pageNumber="27">Diagnosis</emphasis>
