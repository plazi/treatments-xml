<collectionCode box="[745,808,1840,1866]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="6" pageNumber="389" type="Museum">MCZ</collectionCode>
