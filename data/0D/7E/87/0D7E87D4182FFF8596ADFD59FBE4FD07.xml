. Comparison of 
<taxonomicName authority="sp." authorityName="Sp." box="[438,752,259,283]" class="Insecta" family="Tetrigidae" genus="Criotettix" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="147" phylum="Arthropoda" rank="species" species="gariyabandicus">
<emphasis box="[438,713,259,283]" italics="true" pageId="5" pageNumber="148">Criotettix gariyabandicus</emphasis>
<taxonomicNameLabel box="[720,805,260,283]" pageId="5" pageNumber="148" rank="species">
<emphasis bold="true" box="[720,752,260,283]" pageId="5" pageNumber="148">sp.</emphasis>
<emphasis bold="true" box="[758,805,260,283]" pageId="5" pageNumber="148">nov.</emphasis>
</taxonomicNameLabel>
</taxonomicName>
and 
<taxonomicName authority="Hancock, 1913" authorityName="Hancock" authorityYear="1913" box="[857,1239,259,283]" class="Insecta" family="Tetrigidae" genus="Criotettix" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="148" phylum="Arthropoda" rank="species" species="orientalis">
