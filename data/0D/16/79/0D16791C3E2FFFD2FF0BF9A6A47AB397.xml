<typeStatus box="[151,254,1866,1891]" pageId="14" pageNumber="15">lectotype</typeStatus>
and 
<specimenCount box="[308,524,1866,1891]" pageId="14" pageNumber="15" type="generic" typeStatus="paralectotypes">four paralectotypes</specimenCount>
are 
<emphasis bold="true" box="[571,757,1866,1891]" pageId="14" pageNumber="15">here designated</emphasis>
from the material of 
<taxonomicName authorityName="Sharp" authorityYear="1876" box="[997,1163,1866,1891]" class="Insecta" family="Zopheridae" genus="Bitoma" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="distans">
