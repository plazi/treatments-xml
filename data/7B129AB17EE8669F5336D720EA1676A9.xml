<paragraph id="E0631E7BC62AF02E677060C822EE679D" pageId="15" pageNumber="364">
<taxonomicName id="91FE56F958E7AA5B4D19D105603EE787" class="Insecta" family="Curculionidae" genus="Magdalis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Magdalis hispoides" order="Coleoptera" pageId="15" pageNumber="364" phylum="Arthropoda" rank="species" species="hispoides">Magdalis hispoides</taxonomicName>
