<pageStartToken id="A15CE79D1D8F17F37B997ECA8A9E88C6" pageNumber="129">50</pageStartToken>
. 
<taxonomicName id="D81370E94D320D01E4CA15161FE45831" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29074" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" lsidName-HNS="Formica exsecta Nylander" order="Hymenoptera" pageNumber="129" phylum="Arthropoda" rank="species" species="exsecta">Formica exsecta Nylander</taxonomicName>
