<taxonomicName id="9DEB4D3EFF638E907C4F75866EF3FF65" authority="(Umbgrove, 1925)" baseAuthorityName="Umbgrove" baseAuthorityYear="1925" box="[295,716,157,181]" class="Anthozoa" family="Faviidae" genus="Montastrea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="187" pageNumber="188" phylum="Cnidaria" rank="species" species="maxima">
