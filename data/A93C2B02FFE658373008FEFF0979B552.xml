<emphasis id="13E14606FFE65837330AFE070CF4B26E" box="[271,387,499,524]" italics="true" pageId="22" pageNumber="23">Juniperus</emphasis>
</taxonomicName>
and 
<taxonomicName id="E695E197FFE6583733B9FE060F72B26E" authorityName="Linnaeus" authorityYear="1753" box="[444,517,498,524]" class="Magnoliopsida" family="Cistaceae" genus="Cistus" kingdom="Plantae" order="Malvales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
<emphasis id="13E14606FFE6583733B9FE060F72B26E" box="[444,517,498,524]" italics="true" pageId="22" pageNumber="23">Cistus</emphasis>
