</taxonomicName>
, 
<taxonomicName id="E695E197FFE6583731E1FB05095AB569" authorityName="Linnaeus" authorityYear="1753" box="[996,1069,1265,1291]" class="Magnoliopsida" family="Cistaceae" genus="Cistus" kingdom="Plantae" order="Malvales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
