</taxonomicName>
, 
<taxonomicName id="E695E197FFE6583732B8FE070C77B26E" box="[189,256,499,524]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
<emphasis id="13E14606FFE6583732B8FE070C77B26E" box="[189,256,499,524]" italics="true" pageId="22" pageNumber="23">Pinus</emphasis>
