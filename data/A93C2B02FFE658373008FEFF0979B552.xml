<taxonomicName id="E695E197FFE65837370AFE390818B184" box="[1295,1391,461,486]" class="Magnoliopsida" family="Anacardiaceae" genus="Pistacia" kingdom="Plantae" order="Sapindales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
