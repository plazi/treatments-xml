<quantity id="4CF69B413C62FFB0E90BFDFDFD2A1418" box="[625,689,583,607]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="6" pageNumber="547" unit="m" value="200.0">200m</quantity>
