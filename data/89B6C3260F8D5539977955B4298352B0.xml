<taxonomicName id="161AC7B99B895014016091F6B2208AA9" authorityName="Schmiedeknecht" authorityYear="1913" class="Insecta" family="Ichneumonidae" genus="Anisotacrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anisotacrus" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="genus">Anisotacrus</taxonomicName>
