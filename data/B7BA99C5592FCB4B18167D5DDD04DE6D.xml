<taxonomicName id="4AA493DA097E0FB9E13EFBB030F420D7" genus="Ethelurgina" lsidName="Ethelurgina scutatorius" pageId="0" pageNumber="9042" rank="species" species="scutatorius">scutatorius</taxonomicName>
