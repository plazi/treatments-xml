<collectingCountry id="9E917650FF13FF40FDB9FE5FFDBDFAB2" box="[512,588,395,421]" name="Samoa" pageId="145" pageNumber="146">Samoa</collectingCountry>
and 
<collectingCountry id="9E917650FF13FF40FD39FE5FFD39FAB2" box="[640,712,395,421]" name="Tonga" pageId="145" pageNumber="146">Tonga</collectingCountry>
