<subSubSection id="AE9C654BFF13FF40FF7EFEB2FB8AF922" pageId="145" pageNumber="146" type="distribution">
<paragraph id="E63936C0FF13FF40FF7EFEB2FE7BFAF9" blockId="145.[151,1437,286,565]" pageId="145" pageNumber="146">
<emphasis id="D4F2EAD2FF13FF40FF7EFEB2FE90FA96" bold="true" box="[199,353,358,385]" pageId="145" pageNumber="146">Distribution.</emphasis>
Red Sea, Indo-West Pacific: East Africa, 
<collectingCountry id="9E917650FF13FF40FCF9FEB3FC22FA96" box="[832,979,359,385]" name="South Africa" pageId="145" pageNumber="146">South Africa</collectingCountry>
, 
<collectingCountry id="9E917650FF13FF40FC59FEB3FBC9FA96" box="[992,1080,359,385]" name="Yemen" pageId="145" pageNumber="146">Socotra</collectingCountry>
, 
<collectingCountry id="9E917650FF13FF40FBFCFEB3FB31FA96" box="[1093,1216,359,385]" name="Seychelles" pageId="145" pageNumber="146">Seychelles</collectingCountry>
, 
<collectingCountry id="9E917650FF13FF40FB74FEB3FAA7FA96" box="[1229,1366,359,385]" name="Madagascar" pageId="145" pageNumber="146">Madagascar</collectingCountry>
