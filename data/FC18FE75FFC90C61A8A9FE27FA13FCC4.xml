<taxonomicName id="B3B134E0FFC90C61A8FEFE27FD8EFE5B" authority="Schuchert, 1996" authorityName="Schuchert" authorityYear="1996" box="[223,567,431,455]" class="Hydrozoa" family="Eucodoniidae" kingdom="Animalia" order="Anthoathecata" pageId="1" pageNumber="49" phylum="Cnidaria" rank="family">Eucodoniidae Schuchert, 1996</taxonomicName>
