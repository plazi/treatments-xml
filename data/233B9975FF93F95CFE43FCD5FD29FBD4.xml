<emphasis id="99E6F471FF93F95CFDE7FC7AFD7CFC1E" box="[609,730,970,995]" italics="true" pageId="4" pageNumber="153">H. geminus</emphasis>
(Fabricius)
</taxonomicName>
; 
<taxonomicName id="6C9253E0FF93F95CFCE7FC7AFB26FC1E" authority="(Guignot)" baseAuthorityName="Guignot" baseAuthorityYear="1954" box="[865,1152,970,995]" class="Insecta" family="Dytiscidae" genus="Hydroglyphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="153" phylum="Arthropoda" rank="species" species="pendjabensis">
<emphasis id="99E6F471FF93F95CFCE7FC7AFBAAFC1E" box="[865,1036,970,995]" italics="true" pageId="4" pageNumber="153">H. pendjabensis</emphasis>
(Guignot)
</taxonomicName>
; 
<taxonomicName id="6C9253E0FF93F95CFF26FC5DFE22FBFB" authority="(Klug)" baseAuthorityName="Klug" baseAuthorityYear="1834" box="[160,388,1005,1030]" class="Insecta" family="Dytiscidae" genus="Hydroglyphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="153" phylum="Arthropoda" rank="species" species="signatellus">
<emphasis id="99E6F471FF93F95CFF26FC5DFE93FBFB" box="[160,309,1005,1030]" italics="true" pageId="4" pageNumber="153">H. signatellus</emphasis>
(Klug)
</taxonomicName>
; 
<taxonomicName id="6C9253E0FF93F95CFE14FC5DFC68FBFB" authority="(Sharp) (Ghosh and Nilsson 2012)" authorityName="Ghosh and Nilsson" authorityYear="2012" baseAuthorityName="Sharp" baseAuthorityYear="1882" box="[402,974,1005,1030]" class="Insecta" family="Dytiscidae" genus="Hydroglyphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="153" phylum="Arthropoda" rank="species" species="flammulatus">
<emphasis id="99E6F471FF93F95CFE14FC5DFD9DFBFB" box="[402,571,1005,1030]" italics="true" pageId="4" pageNumber="153">H. flammulatus</emphasis>
(Sharp) (
<bibRefCitation id="CF035592FF93F95CFD2EFC5EFC63FBFB" author="Ghosh SK &amp; Nilsson AN" box="[680,965,1006,1030]" pageId="4" pageNumber="153" refId="ref4795" refString="Ghosh SK, Nilsson AN. 2012. Catalogue of the diving beetles of India and adjacent countries (Coleoptera: Dytiscidae). Umea: Skorvnopparn." type="book" year="2012">Ghosh and Nilsson 2012</bibRefCitation>
