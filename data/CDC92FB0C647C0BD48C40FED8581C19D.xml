<taxonomicName id="516B87890743009A2C3A01E981759078" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis distinguenda" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="distinguenda">Chrysis distinguenda</taxonomicName>
