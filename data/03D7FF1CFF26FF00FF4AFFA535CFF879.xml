<taxonomicName id="4C7E3589FF26FF00FD27FE563599FAA0" authority="Boos and Ratcliffe, 1985" authorityName="Boos and Ratcliffe" authorityYear="1985" box="[687,1253,1335,1360]" class="Insecta" family="Cetoniidae" genus="Inca" kingdom="Animalia" order="Coleoptera" pageId="237" pageNumber="236" phylum="Arthropoda" rank="subSpecies" species="clathratus" subSpecies="quesneli">
