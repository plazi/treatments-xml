<taxonomicName id="4C1D7587FFC8FFA1FDAFF81541C797FC" authority="Walker, 1852" authorityName="Walker" authorityYear="1852" box="[600,704,1982,2005]" class="Insecta" family="Syrphidae" genus="Ubristes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="goettei">
<emphasis id="B969D216FFC8FFA1FDAFF81541C797FC" box="[600,704,1982,2005]" italics="true" pageId="53" pageNumber="54">S. goettei</emphasis>
</taxonomicName>
see table 
<number id="3DD51C23FFC8FFA1FCD8F815403897FF" box="[815,831,1982,2006]" pageId="53" pageNumber="54" value="3.0">3</number>
.
</paragraph>
