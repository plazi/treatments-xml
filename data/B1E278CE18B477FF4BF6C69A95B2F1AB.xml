<taxonomicName id="8AF331190FA6F3694CE924BD06418D06" authority="Sheng &amp; Sun, 2010" authorityName="Sheng &amp; Sun" authorityYear="2010" class="Insecta" family="Ichneumonidae" genus="Carinityla" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Carinityla" order="Hymenoptera" pageId="2" pageNumber="63" phylum="Arthropoda" rank="genus">Carinityla</taxonomicName>
</td>
</tr>
</table>
</paragraph>
