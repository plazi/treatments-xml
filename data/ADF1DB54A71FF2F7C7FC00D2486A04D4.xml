<paragraph id="E42379B79410D0E53F457F48E9A63FB6" pageId="4" pageNumber="17">
<taxonomicName id="348E1ADFC04329847C85C2D88555F36C" authority="C.L.Koch, 1836" authorityName="C.L.Koch" authorityYear="1836" class="Arachnida" family="Laelapidae" genus="Laelaps" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Laelaps (Hypoaspis) canestrinii" order="Mesostigmata" pageId="4" pageNumber="17" phylum="Arthropoda" rank="species" species="canestrinii" subGenus="Hypoaspis">Laelaps (Hypoaspis) canestrinii</taxonomicName>
<bibRefCitation id="9FA2136E5EBDD482DED4CAF822140845" author="Berlese, A" journalOrPublisher="Redia" pageId="6" pageNumber="19" pagination="299 - 474" title="Illustrazione iconografica degli Acari mirmecofili." volume="1" year="1904">Berlese 1904</bibRefCitation>
