<taxonomicName id="E6854D0EFFB8FF8E350B90D012F6F82E" authority="Durante, 2009" authorityName="Durante" authorityYear="2009" class="Insecta" family="Arctiidae" genus="Afrasura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="384" phylum="Arthropoda" rank="subSpecies" species="indecisa" subSpecies="orientalis">
