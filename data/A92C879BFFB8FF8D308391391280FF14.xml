<taxonomicName id="E6854D0EFFB8FF8E33AD90D017C5F80B" box="[953,1177,1971,1995]" class="Insecta" family="Arctiidae" genus="Afrasura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="384" phylum="Arthropoda" rank="subSpecies" species="indecisa" subSpecies="indecisa">
