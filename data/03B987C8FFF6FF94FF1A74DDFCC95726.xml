<subSubSection id="C30A6555FFF7FF94FF4A762CFEC951AA" pageId="5" pageNumber="6" type="etymology">
<paragraph id="8BAF36DEFFF7FF94FF4A762CFEC951AA" blockId="5.[151,1437,151,1293]" pageId="5" pageNumber="6">
<emphasis id="B964EACCFFF7FF94FF4A762CFEB75177" bold="true" box="[199,334,836,861]" pageId="5" pageNumber="6">Etymology.</emphasis>
