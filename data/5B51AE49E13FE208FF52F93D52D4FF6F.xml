<paragraph id="D3471F5FE13FE209FF03F87A5384F842" blockId="18.[151,1436,1810,2036]" pageId="18" pageNumber="19">
<emphasis id="E18CC34DE13FE209FF03F87A5671F812" bold="true" box="[198,319,1930,1956]" pageId="18" pageNumber="19">Remarks.</emphasis>
<taxonomicName id="14F864DCE13FE209FE82F87B5485F812" authority="Kudinova-Pasternak, 1970" authorityName="Kudinova-Pasternak" authorityYear="1970" box="[327,971,1930,1956]" class="Malacostraca" family="Leptognathiidae" genus="Leptognathia" kingdom="Animalia" order="Tanaidacea" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="rotundicauda">
<emphasis id="E18CC34DE13FE209FE82F87B55C6F812" box="[327,648,1931,1956]" italics="true" pageId="18" pageNumber="19">Leptognathia rotundicauda</emphasis>
<bibRefCitation id="B76962AEE13FE209FD55F87A5485F812" author="Kudinova-Pasternak" box="[656,971,1930,1956]" pageId="18" pageNumber="42" refString="Kudinova-Pasternak, R. K. (1970) Tanaidacea of the Kurile-Kamchatka Trench. Trudy Instituta Okeanologii. Akademiya Nauk SSSR, 86, 341 - 380." type="journal article" year="1970">
Kudinova-Pasternak, 
<number id="65300D78E13FE209FC4BF87A5485F812" box="[910,971,1930,1956]" pageId="18" pageNumber="19" value="1970.0">1970</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="14F864DCE13FE209FC1DF87B526BF812" authority="Larsen, 2005" authorityName="Larsen" authorityYear="2005" box="[984,1317,1930,1956]" class="Malacostraca" family="Leptognathiidae" genus="Leptognathia" kingdom="Animalia" order="Tanaidacea" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="longimanus">
<emphasis id="E18CC34DE13FE209FC1DF87B53CEF812" box="[984,1152,1931,1956]" italics="true" pageId="18" pageNumber="19">L. longimanus</emphasis>
<bibRefCitation id="B76962AEE13FE209FB4DF87A526BF812" author="Larsen" box="[1160,1317,1930,1956]" pageId="18" pageNumber="43" refString="Larsen, K. (2005). Deep-Sea Tanaidacea (Crustacea; Peracarida) from the Gulf of Mexico. Crustacean Monographs, 5, Brill, Leiden, 381 pp." type="book" year="2005">
