<taxonomicName id="4C4C95A0FFABED6CFFCEFE1C9F1BFE52" authority="Saaristo and Marusik, 2004: 52" authorityName="Saaristo and Marusik" authorityPageNumber="52" authorityYear="2004" box="[120,554,434,458]" class="Arachnida" family="Oonopidae" genus="Ferchestina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="23" phylum="Arthropoda" rank="genus">
