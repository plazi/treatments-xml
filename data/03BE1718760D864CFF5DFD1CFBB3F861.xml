<taxonomicName id="4C17DD8D760D864DFF5DFD1CFD9FFCB0" authority="Giglio-Tos, 1914" authorityName="Giglio-Tos" authorityYear="1914" box="[151,519,746,773]" class="Insecta" family="Mantidae" genus="Chopardiella" kingdom="Animalia" order="Mantodea" pageId="12" pageNumber="347" phylum="Arthropoda" rank="genus">
<emphasis id="B9637A1C760D864DFF5DFD1CFD9FFCB0" bold="true" box="[151,519,746,773]" pageId="12" pageNumber="347">
<emphasis id="B9637A1C760D864DFF5DFD1CFEADFCB1" bold="true" box="[151,309,746,772]" italics="true" pageId="12" pageNumber="347">Chopardiella</emphasis>
<bibRefCitation id="EF86DBFF760D864DFEF6FD1DFD9FFCB0" author="Giglio-Tos" box="[316,519,747,773]" pageId="12" pageNumber="347" refString="Giglio-Tos, E. (1914) Mantidi esotici. VI. Vatinae. Bullettino della Societa Entomologica Italiana, 29 (684), 1 - 87." type="journal article" year="1914">
Giglio-Tos, 
<number id="3DDFB429760D864DFE06FD1DFD9FFCB0" box="[460,519,747,773]" pageId="12" pageNumber="347" value="1914.0">1914</number>
</bibRefCitation>
