<quantity id="3B129B26B11FA13FE9A2F937E285F97A" box="[664,751,1766,1791]" metricMagnitude="-2" metricUnit="m" metricValue="6.5" pageId="19" pageNumber="470" unit="mm" value="65.0">65 mm</quantity>
