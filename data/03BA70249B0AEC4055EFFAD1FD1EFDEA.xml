[
<collectingCountry id="F30481A29B0AEC4F5078F9AFFACAF980" box="[1280,1346,1622,1649]" name="India" pageId="22" pageNumber="223">India</collectingCountry>
].
</paragraph>
<paragraph id="8BACC1329B0AEC4F55BFF983FB80F964" blockId="22.[151,1437,1587,2045]" box="[199,1032,1658,1685]" pageId="22" pageNumber="223">
