<geoCoordinate id="7151F3B2BFF594DFEC2460FCAAAD6356" degrees="27" direction="north" minutes="20" orientation="latitude" precision="15" seconds="32" value="27.342222">27°20'32&quot;N</geoCoordinate>
, 
<geoCoordinate id="05D7DE3664DA35586F2B399286803869" degrees="92" direction="east" minutes="16" orientation="longitude" precision="15" seconds="17" value="92.27139">92°16'17&quot;E</geoCoordinate>
, 1550 m, 27.iv-1.v.2008, H. 
<normalizedToken id="2D8DF806C371DA594DCCA42DDED40AFB" originalValue="Podskalská">Podskalska</normalizedToken>
&amp; P. 
<normalizedToken id="972FB5982F24D07AC702E04FC0136564" originalValue="Šipek">Sipek</normalizedToken>
(NHMP, 1); as previous except: Etalin vicinity, 
<geoCoordinate id="ABD3787FBB87196C92DF2F7C70C2EB3F" degrees="28" direction="north" minutes="36" orientation="latitude" precision="15" seconds="56" value="28.615557">28°36'56&quot;N</geoCoordinate>
