<taxonomicName id="4C2F4D2E9627FFCDFEC6BA266466F9D0" box="[284,537,1588,1610]" class="Insecta" family="Haglidae" genus="Homogryllacris" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="62" phylum="Arthropoda" rank="species" species="anelytra" status="sp. nov.">
<emphasis id="B95BEABF9627FFCDFEC6BA266466F9D0" box="[284,537,1588,1610]" italics="true" pageId="4" pageNumber="62">Homogryllacris anelytra</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26857C49627FFCDFDF8BA26640BF9D3" box="[546,628,1588,1609]" pageId="4" pageNumber="62" rank="species">
