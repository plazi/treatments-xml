<taxonomicName id="FC9235B0FFF7CD32FE052ED4FDE2EA1B" box="[411,557,764,790]" class="Entognatha" family="Campodeidae" genus="Cestocampa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplura" pageId="10" pageNumber="53" phylum="Arthropoda" rank="genus">
