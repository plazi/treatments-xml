5. 
<taxonomicName id="4C414D75FFC3FFA9FB54FB471022FB3C" authorityName="Exell" authorityYear="1932" box="[1224,1398,1205,1227]" class="Magnoliopsida" family="Annonaceae" genus="Mischogyne" kingdom="Plantae" order="Magnoliales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="michelioides">M. michelioides</taxonomicName>
