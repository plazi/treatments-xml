<taxonomicName id="4C414D75FFC3FFA9FE60FBCF173AFBA4" authorityName="Exell" authorityYear="1932" box="[508,622,1086,1107]" class="Magnoliopsida" family="Annonaceae" genus="Mischogyne" kingdom="Plantae" order="Magnoliales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="genus">
<emphasis id="B935EAE4FFC3FFA9FE60FBCF173AFBA4" box="[508,622,1086,1107]" italics="true" pageId="7" pageNumber="8">Mischogyne</emphasis>
