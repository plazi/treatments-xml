<taxonomicName id="79E84D6FFFF0D80A257C9180B48372AF" box="[1111,1232,654,677]" class="Insecta" family="Rutelidae" genus="Platycoelia" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="68" phylum="Arthropoda" rank="genus">
