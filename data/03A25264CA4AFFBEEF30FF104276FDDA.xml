<taxonomicName id="4C0B98F1CA4AFFB9EC10FE96425CFE27" authority="Pinhey, 1967" authorityName="Pinhey" authorityYear="1967" box="[562,857,378,405]" class="Insecta" family="Coenagrionidae" genus="Pseudagrion" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Odonata" pageId="83" pageNumber="528" phylum="Arthropoda" rank="species" species="symoensii">
