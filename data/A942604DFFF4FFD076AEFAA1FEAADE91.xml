<taxonomicName id="E6EBAAD8FFF7FFD075C5FF04FB93DC89" box="[1020,1122,153,177]" class="Magnoliopsida" family="Malvaceae" genus="Dombeya" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="446" phylum="Tracheophyta" rank="genus">
