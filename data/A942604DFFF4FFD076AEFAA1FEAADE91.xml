<taxonomicName id="E6EBAAD8FFF7FFD072A5FF04FAE4DC89" box="[1180,1301,153,177]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="446" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
