<subSubSection id="C360E29DFD55FFB9FF45FA6923BAFB2F" box="[199,1153,1150,1177]" pageId="1" pageNumber="361" type="etymology">
<paragraph id="8BC5B116FD55FFB9FF45FA6923BAFB2F" blockId="1.[151,1437,1041,2040]" box="[199,1153,1150,1177]" pageId="1" pageNumber="361">
<emphasis id="B90E6D04FD55FFB9FF45FA692675FB2F" bold="true" box="[199,334,1150,1176]" pageId="1" pageNumber="361">Etymology.</emphasis>
