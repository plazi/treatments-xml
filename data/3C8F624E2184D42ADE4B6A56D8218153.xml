<geoCoordinate id="9C54D0AD9AAF368636FB43719C140C67" direction="north" orientation="latitude" precision="15" value="19.211111">19°12'40&quot;N</geoCoordinate>
, 
<geoCoordinate id="746EC746225F865E76593435894AA524" direction="east" orientation="longitude" precision="15" value="113.21083">113°12'39&quot;E</geoCoordinate>
