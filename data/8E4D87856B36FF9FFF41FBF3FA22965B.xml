<subSubSection id="4EFE65186B36FF9CFF41FB61FCBE9010" pageId="5" pageNumber="58" type="etymology">
<paragraph id="065B36936B36FF9CFF41FB61FCBE9010" blockId="5.[151,1437,1253,2026]" pageId="5" pageNumber="58">
<emphasis id="3490EA816B36FF9CFF41FB61FE9591CB" bold="true" box="[151,299,1253,1279]" pageId="5" pageNumber="58">Etymology:</emphasis>
Named 
<taxonomicName id="C1E44D106B36FF9CFE44FB61FDBD91CA" box="[402,515,1253,1278]" class="Insecta" family="Coenagrionidae" genus="Telebasis" kingdom="Animalia" order="Odonata" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="myrianae">
