<materialsCitation id="3B77712CFFF3FFE3FF6FFACF7C28F9E9" ID-GBIF-Occurrence="2610671311" collectingDate="1999-10-08" collectionCode="USNM" pageId="4" pageNumber="115" specimenCode="PAL 641969" specimenCount="1">
