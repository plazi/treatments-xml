<taxonomicName id="57B6EEDC32D79083C8A583C1DDB385D1" LSID="urn:lsid:zoobank.org:act:90586ED4-0AAD-4173-A958-D16655A5B3CA" class="Diplopoda" family="Paradoxosomatidae" genus="Boreohesperus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Boreohesperus undulatus" order="Polydesmida" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="undulatus">Boreohesperus undulatus</taxonomicName>
