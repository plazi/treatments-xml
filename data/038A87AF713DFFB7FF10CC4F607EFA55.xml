<quantity id="4CDB9B5C713DFFB7FC1ACC876565FA1C" box="[925,1050,1435,1459]" metricMagnitude="-2" metricUnit="m" metricValue="4.0" metricValueMax="4.3" metricValueMin="3.7" pageId="9" pageNumber="90" unit="mm" value="40.0" valueMax="43.0" valueMin="37.0">37–43 mm</quantity>
