<taxonomicName id="4C4F819DE32BFFF780345B7AFAE4EA40" box="[1318,1375,571,592]" class="Insecta" family="Ptiliidae" genus="Micridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="236" phylum="Arthropoda" rank="species" species="oweni" status="sp. n.">
