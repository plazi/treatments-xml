<taxonomicName id="4C1B20F10A71460C2CFCF8E8FDC6D038" authority="Miller &amp; Gonzalez 1975" authorityName="Miller &amp; Gonzalez" authorityYear="1975" box="[151,525,1894,1919]" class="Insecta" family="Eriococcidae" genus="Stibococcus" kingdom="Animalia" order="Hemiptera" pageId="94" pageNumber="95" phylum="Arthropoda" rank="genus">
<emphasis id="B96F87600A71460C2CFCF8E8FEDBD039" box="[151,272,1894,1918]" italics="true" pageId="94" pageNumber="95">Stibococcus</emphasis>
<bibRefCitation id="EF8A26830A71460C2D7DF8E8FDC6D038" author="Miller &amp; Gonzalez" box="[278,525,1894,1919]" firstAuthor="Miller" pageId="94" pageNumber="95" pagination="131 - 163" refId="ref208628" refString="Miller, D. R. &amp; Gonzalez, R. H. (1975) A taxonomic analysis of the Eriococcidae of Chile. Revista Chilena de Entomologia, 9, 131 - 163." type="journal article" year="1975">Miller &amp; González 1975</bibRefCitation>
</taxonomicName>
, 154–156. 
<typeStatus id="54A0E5D00A71460C2E11F8E9FD67D038" box="[634,684,1895,1919]" pageId="94" pageNumber="95">Type</typeStatus>
species: 
<taxonomicName id="4C1B20F10A71460C2F63F8E8FB5FD038" authority="Miller &amp; Gonzalez" authorityName="Miller &amp; Gonzalez" box="[776,1172,1894,1919]" class="Insecta" family="Eriococcidae" genus="Stibococcus" kingdom="Animalia" order="Hemiptera" pageId="94" pageNumber="95" phylum="Arthropoda" rank="species" species="cerinus">
