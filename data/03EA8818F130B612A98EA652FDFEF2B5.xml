<taxonomicName id="4C43428DF13EB612AB00A77AFD61F301" authority="Chatzaki &amp; Keer, 2019" authorityName="Chatzaki &amp; Keer" authorityYear="2019" box="[537,643,1604,1628]" class="Arachnida" family="Gnaphosidae" genus="Echemus" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="442" phylum="Arthropoda" rank="species" species="kaltsasi" status="sp.n.">
