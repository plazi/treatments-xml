</taxonomicName>
y 
<taxonomicName id="E4824D3EF4097402A4FC3CD9FE04FDB4" authority="Mez" authorityName="Mez" box="[136,396,503,527]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="2" pageNumber="107" phylum="Tracheophyta" rank="species" species="catharinensis">
