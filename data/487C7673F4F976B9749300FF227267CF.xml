<taxonomicName id="D10342947556087A8E039DF67E3EACEB" authority="Balf.fil." authorityName="Balf.fil." class="Magnoliopsida" family="Apocynaceae" genus="Mitolepis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Mitolepis intricata" order="Gentianales" pageId="2" pageNumber="47" phylum="Tracheophyta" rank="species" species="intricata">Mitolepis intricata</taxonomicName>
, 
<taxonomicName id="B8CEE57F61B5A7BED685FA7F48F7EB5F" class="Magnoliopsida" family="Acanthaceae" genus="Ballochia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ballochia" order="Lamiales" pageId="2" pageNumber="47" phylum="Tracheophyta" rank="genus">Ballochia</taxonomicName>
spp., 
