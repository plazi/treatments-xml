<taxonomicName id="0536C3FDD01BE6759B1A30C5DD6682E7" authority="Schoorl, 1990" authorityName="Schoorl" authorityYear="1990" class="Insecta" family="Cossidae" genus="Mormogystia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Mormogystia proleuca" order="Lepidoptera" pageId="1" pageNumber="46" phylum="Arthropoda" rank="species" species="proleuca">Mormogystia proleuca</taxonomicName>
