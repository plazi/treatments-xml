<taxonomicName id="4C624D1CFF9AFFD9FF46FAED9888E46A" authorityName="Macquart" authorityYear="1835" box="[205,385,1309,1335]" class="Insecta" family="Sphaeroceridae" kingdom="Animalia" order="Diptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="family">Sphaeroceridae</taxonomicName>
