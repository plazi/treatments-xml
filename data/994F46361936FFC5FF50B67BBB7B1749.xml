<caption id="4599A7A81937FFC2FF50B7A5BEC014C1" httpUri="https://zenodo.org/record/210799/files/figure.png" pageId="11" pageNumber="54" targetBox="[196,1434,176,1906]" targetPageId="11">
<paragraph id="1159F7201937FFC2FF50B7A5BEC014C1" blockId="11.[151,1436,1953,2036]" pageId="11" pageNumber="54">
