<taxonomicName id="1EB3E5BEFFF4FFABFD91711EFC96B3B7" authority="Mosely" authorityName="Mosely" box="[589,837,1897,1923]" class="Insecta" family="Hydroptilidae" genus="Ochrotrichia" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="47" phylum="Arthropoda" rank="species" species="arranca">
<emphasis id="EBC7422FFFF4FFABFD91711EFDB1B3B6" box="[589,610,1897,1922]" italics="true" pageId="6" pageNumber="47">O</emphasis>
. 
<emphasis id="EBC7422FFFF4FFABFDAD711EFD1CB3B6" box="[625,719,1897,1922]" italics="true" pageId="6" pageNumber="47">arranca</emphasis>
