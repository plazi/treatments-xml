<taxonomicName id="F2CBEA281E4E78491B03E6731EA4EDA9" class="Insecta" family="Staphylinidae" genus="Boreophilia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Boreophilia" order="Coleoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="genus">Boreophilia</taxonomicName>
