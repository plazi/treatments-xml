<quantity id="05629B2FFFB3FFF6FD39FA6E9C0891F2" box="[707,793,1484,1510]" metricMagnitude="-3" metricUnit="m" metricValue="9.2" pageId="13" pageNumber="414" unit="mm" value="9.2">9.2 mm</quantity>
