<taxonomicName id="96B5F4642C9FE53E14A3AECC03927241" class="Dicotyledoneae" family="Betulaceae" genus="Corylus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Corylus heterophylla" order="Fagales" pageId="0" pageNumber="19" phylum="Angiospermae" rank="species" species="heterophylla">
