<taxonomicName id="3F88B2F5740CAF7AB2511DE6C875EE1B" class="Dicotyledoneae" family="Betulaceae" genus="Corylus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Corylus heterophylla" order="Fagales" pageId="0" pageNumber="19" phylum="Angiospermae" rank="species" species="heterophylla">
