<date id="7E50105C2B33FFB1FB66F86C6F15F081" box="[1152,1302,1936,1962]" pageId="2" pageNumber="3" value="2002-03">
March 
<number id="BC2624BB2B33FFB1FB3FF86C6F15F081" box="[1241,1302,1936,1962]" pageId="2" pageNumber="3" value="2002.0">2002</number>
</date>
