<paragraph id="95D9E96A1A21B82E796694299A165AB6" pageId="16" pageNumber="107">
<taxonomicName id="2D817D557EE241A340A5B2FF1C178BF9" class="Insecta" family="Cetoniidae" genus="Atrichelaphinis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atrichelaphinis deplanata" order="Coleoptera" pageId="16" pageNumber="107" phylum="Arthropoda" rank="species" species="deplanata">Atrichelaphinis deplanata</taxonomicName>
