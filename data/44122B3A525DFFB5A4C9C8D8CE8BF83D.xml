<taxonomicName id="0BBBE1AF525BFFB5A7C5CC03CA47FD09" box="[923,1061,623,645]" class="Insecta" family="Asilidae" genus="Neodiogmites" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="species" species="melanogaster">
