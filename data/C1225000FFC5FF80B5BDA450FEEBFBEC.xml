<taxonomicName id="8E8B9A95FFC5FF81B5BDA40DFE55F838" authority="Ashmead 1902" authorityName="Ashmead" authorityYear="1902" box="[151,509,1905,1927]" class="Insecta" family="Ichneumonidae" genus="Bioblapsis" kingdom="Animalia" order="Hymenoptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="tricincta">
<emphasis id="7BFF3D04FFC5FF81B5BDA40DFEF6F838" box="[151,350,1905,1927]" italics="true" pageId="100" pageNumber="101">Bioblapsis tricincta</emphasis>
