<taxonomicName id="51D04D2B5C6F690A7194FD3CFC916EB6" authorityName="Pelsue &amp; Zhang" authorityYear="2000" box="[642,813,649,670]" class="Insecta" family="Curculionidae" genus="Curculio" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="5" phylum="Arthropoda" rank="species" species="marioi" status="n. sp.">
