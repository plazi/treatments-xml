<taxonomicName id="8EE05DB2E216AC68E438FB0DFB766383" box="[1113,1172,1191,1215]" class="Magnoliopsida" family="Moraceae" genus="Ficus" kingdom="Plantae" order="Rosales" pageId="31" pageNumber="2455" phylum="Tracheophyta" rank="genus">
