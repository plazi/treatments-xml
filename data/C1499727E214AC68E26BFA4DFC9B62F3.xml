<taxonomicName id="8EE05DB2E216AC68E37DFB6DFC4863E3" authorityName=": Bezzi" authorityYear="1920" box="[796,938,1223,1247]" class="Insecta" family="Tephritidae" genus="Perilampsis" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="2455" phylum="Arthropoda" rank="species" species="formosula">
