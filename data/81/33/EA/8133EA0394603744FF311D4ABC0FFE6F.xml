<taxonomicName authority="(Nguyen 2015 c)" authorityName="Nguyen" authorityYear="2015" baseAuthorityName="Nguyen" baseAuthorityYear="2015" class="Insecta" family="Eumenidae" genus="Anterhynchium" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="subSpecies" species="flavomarginatum" subSpecies="flavomarginatum">
