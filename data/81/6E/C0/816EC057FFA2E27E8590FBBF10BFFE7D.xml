<taxonomicName authority="Noodt, 1962" authorityName="Noodt" authorityYear="1962" box="[285,682,1948,1971]" class="Maxillopoda" family="Parastenocarididae" genus="Remaneicaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="10" pageNumber="509" phylum="Arthropoda" rank="species" species="palaciosi">
