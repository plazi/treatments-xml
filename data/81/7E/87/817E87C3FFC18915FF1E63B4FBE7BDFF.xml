<typeStatus box="[671,815,1639,1663]" pageId="17" pageNumber="18">allolectotype</typeStatus>
specimens from 
<collectionCode box="[1007,1092,1640,1663]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="17" pageNumber="18">MNHN</collectionCode>
(
<specimenCode box="[1107,1204,1639,1663]" collectionCode="MNHN" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="17" pageNumber="18">AR1317</specimenCode>
