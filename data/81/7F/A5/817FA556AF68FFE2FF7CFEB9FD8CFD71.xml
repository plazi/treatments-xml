<taxonomicName authority="Oudemans and Voigts, 1904" authorityName="Oudemans and Voigts" authorityYear="1904" class="Arachnida" family="Parasitidae" genus="Parasitus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="266" phylum="Arthropoda" rank="species" species="eta">
<emphasis box="[436,491,623,646]" italics="true" pageId="4" pageNumber="266">P. eta</emphasis>
Oudemans and Voigts, 1904
</taxonomicName>
, while 
<taxonomicName authorityName="Oudemans and Voigts" authorityYear="1904" box="[306,367,659,682]" class="Arachnida" family="Parasitidae" genus="Parasitus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="266" phylum="Arthropoda" rank="species" species="eta">
<emphasis box="[306,367,659,682]" italics="true" pageId="4" pageNumber="266">P. eta</emphasis>
</taxonomicName>
