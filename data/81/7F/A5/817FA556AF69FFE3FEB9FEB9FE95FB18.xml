<collectingRegion box="[192,273,555,579]" country="Iran" name="Tehran" pageId="5" pageNumber="267">Tehran</collectingRegion>
(
<geoCoordinate box="[292,411,555,579]" degrees="35" direction="north" minutes="42" orientation="latitude" pageId="5" pageNumber="267" precision="15" seconds="2" value="35.700558">35°42’2&quot;N</geoCoordinate>
