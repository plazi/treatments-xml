, 
<collectingCountry box="[1160,1252,884,908]" name="Georgia" pageId="91" pageNumber="92">Georgia</collectingCountry>
, 
<collectingCountry box="[1268,1367,885,909]" name="Hungary" pageId="91" pageNumber="92">Hungary</collectingCountry>
, 
<collectingCountry box="[1383,1431,885,908]" name="Iran" pageId="91" pageNumber="92">Iran</collectingCountry>
, 
<collectingCountry box="[151,204,920,944]" name="Italy" pageId="91" pageNumber="92">Italy</collectingCountry>
, 
<collectingCountry box="[215,348,920,944]" name="Kazakhstan" pageId="91" pageNumber="92">Kazakhstan</collectingCountry>
, 
<collectingCountry box="[361,442,920,944]" name="Poland" pageId="91" pageNumber="92">Poland</collectingCountry>
, 
<collectingCountry box="[454,532,920,944]" name="Russia" pageId="91" pageNumber="92">Russia</collectingCountry>
, 
<collectingCountry box="[544,657,920,944]" name="Tajikistan" pageId="91" pageNumber="92">Tajikistan</collectingCountry>
, 
<collectingCountry box="[669,750,920,944]" name="Turkey" pageId="91" pageNumber="92">Turkey</collectingCountry>
, 
<collectingCountry box="[762,917,920,944]" name="Turkmenistan" pageId="91" pageNumber="92">Turkmenistan</collectingCountry>
and 
<collectingCountry box="[973,1103,920,944]" name="Uzbekistan" pageId="91" pageNumber="92">Uzbekistan</collectingCountry>
. In 
<collectingCountry box="[1146,1194,921,944]" name="Iran" pageId="91" pageNumber="92">Iran</collectingCountry>
, 
<taxonomicName box="[1206,1355,920,944]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="multivorus">
<emphasis box="[1206,1355,920,944]" italics="true" pageId="91" pageNumber="92">T. multivorus</emphasis>
</taxonomicName>
occurs in Ardebil, Esfahan, 
<collectingCountry box="[389,440,957,980]" name="Iran" pageId="91" pageNumber="92">Fars</collectingCountry>
, Hamadan, Kermanshah, Lorestan and Markazi. This mealybug is known from 
<number box="[1352,1380,956,980]" pageId="91" pageNumber="92" value="19.0">19</number>
host plant familities worldwide (Ben-Dov 
<emphasis box="[572,628,992,1016]" italics="true" pageId="91" pageNumber="92">et al.</emphasis>
, 
<number box="[642,703,992,1016]" pageId="91" pageNumber="92" value="2012.0">2012</number>
).
</paragraph>
