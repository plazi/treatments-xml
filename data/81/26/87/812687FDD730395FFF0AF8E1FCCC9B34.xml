<collectingCountry box="[1185,1295,511,535]" name="Tajikistan" pageId="88" pageNumber="89">Tajikistan</collectingCountry>
and 
<collectingCountry box="[1351,1433,511,535]" name="Turkey" pageId="88" pageNumber="89">Turkey</collectingCountry>
