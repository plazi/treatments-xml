(
<taxonomicName box="[158,268,764,788]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
), 
<date box="[288,395,764,788]" pageId="21" pageNumber="22" value="2001-03-01">
