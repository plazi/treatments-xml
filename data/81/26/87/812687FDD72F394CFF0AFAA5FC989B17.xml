, 
<collectingCountry box="[466,535,440,464]" name="Egypt" pageId="75" pageNumber="76">Egypt</collectingCountry>
, 
<collectingCountry box="[548,627,440,463]" name="France" pageId="75" pageNumber="76">France</collectingCountry>
, 
<collectingCountry box="[640,733,439,463]" name="Georgia" pageId="75" pageNumber="76">Georgia</collectingCountry>
, 
<collectingCountry box="[745,827,439,463]" name="Greece" pageId="75" pageNumber="76">Greece</collectingCountry>
, 
<collectingCountry box="[840,939,440,464]" name="Hungary" pageId="75" pageNumber="76">Hungary</collectingCountry>
, 
<collectingCountry box="[951,999,440,463]" name="Iran" pageId="75" pageNumber="76">Iran</collectingCountry>
, 
<collectingCountry box="[1012,1077,439,463]" name="Israel" pageId="75" pageNumber="76">Israel</collectingCountry>
, 
<collectingCountry box="[1090,1157,440,463]" name="Japan" pageId="75" pageNumber="76">Japan</collectingCountry>
, 
<collectingCountry box="[1169,1247,439,463]" name="Jordan" pageId="75" pageNumber="76">Jordan</collectingCountry>
, 
<collectingCountry box="[1259,1360,439,463]" name="Lebanon" pageId="75" pageNumber="76">Lebanon</collectingCountry>
, 
<collectingCountry box="[1372,1436,439,463]" name="Spain" pageId="75" pageNumber="76">Spain</collectingCountry>
and 
<collectingCountry box="[200,282,476,500]" name="Turkey" pageId="75" pageNumber="76">Turkey</collectingCountry>
