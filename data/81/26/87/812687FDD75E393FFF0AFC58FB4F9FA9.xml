<subSubSection pageId="56" pageNumber="57" type="discussion">
<paragraph blockId="56.[151,1437,1110,1962]" pageId="56" pageNumber="57">
<emphasis bold="true" box="[199,374,1902,1927]" pageId="56" pageNumber="57">COMMENTS.</emphasis>
<taxonomicName box="[381,497,1902,1927]" class="Insecta" family="Pseudococcidae" genus="Phenacoccus" kingdom="Animalia" order="Hemiptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="pumilus">
<emphasis box="[381,497,1902,1927]" italics="true" pageId="56" pageNumber="57">P. pumilus</emphasis>
