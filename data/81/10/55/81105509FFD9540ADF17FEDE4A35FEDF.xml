</locationDeviation>
, 
<date box="[113,202,784,803]" pageId="9" pageNumber="116" value="1947-07-09">
<collectingDate box="[113,202,784,803]" pageId="9" pageNumber="116" value="1947-07-09">9.vii.1947</collectingDate>
</date>
, 
<collectorName box="[211,279,784,803]" pageId="9" pageNumber="116">Exp. N.</collectorName>
<collectingRegion box="[284,325,784,803]" country="Turkey" name="Mus" pageId="9" pageNumber="116">Mus</collectingRegion>
. ČSR, 
<specimenCount box="[385,433,780,804]" pageId="9" pageNumber="116" type="female">2 ♀♀</specimenCount>
(all 
<collectionCode box="[473,537,784,803]" country="Czech Republic" httpUri="http://grbio.org/cool/hj12-2xm7" name="National Museum Prague" pageId="9" pageNumber="116">NMPC</collectionCode>
