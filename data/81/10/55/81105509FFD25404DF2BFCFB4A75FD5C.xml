material of 
<taxonomicName authorityName="Motschulsky" authorityYear="1849" box="[504,689,574,598]" class="Insecta" family="Oedemeridae" genus="Probosca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="111" phylum="Arthropoda" rank="species" species="cinerea">
