<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[510,580,368,391]" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="79" phylum="Arthropoda" rank="genus">
