<paragraph pageId="null" pageNumber="304">
<pageBreakToken pageId="null" pageNumber="304" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="304" phylum="Tracheophyta" rank="genus">Galium</taxonomicName>
