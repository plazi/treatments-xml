<taxonomicName authority="Braun, 2011" authorityName="Braun" authorityYear="2011" box="[1135,1440,1189,1213]" class="Insecta" family="Phaneropteridae" genus="Ottotettix" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus">
