<taxonomicName authorityName="Latreille" authorityYear="1802" class="Insecta" family="Sphecidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="family">Sphecidae</taxonomicName>
