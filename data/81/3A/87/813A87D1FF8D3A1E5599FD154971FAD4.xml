<subSubSection pageId="11" pageNumber="572" type="materials_examined">
<paragraph blockId="11.[143,1103,797,988]" box="[143,353,797,821]" pageId="11" pageNumber="572">
<heading box="[143,353,797,821]" fontSize="10" level="3" pageId="11" pageNumber="572" reason="5">
