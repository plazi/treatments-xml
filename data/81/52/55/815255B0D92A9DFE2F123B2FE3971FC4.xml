<taxonomicName authorityName="Casey" authorityYear="1920" class="Hexapoda" family="Carabidae" genus="Elaphrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="265" pageNumber="266" phylum="Arthropoda" rank="species" species="hesperius">Elaphrus hesperius</taxonomicName>
