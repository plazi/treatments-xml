<geoCoordinate direction="north" orientation="latitude" precision="1" value="35.272415">35°16'20.7&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="23.53775">23°32'15.9&quot;E</geoCoordinate>
, 1 m depth, fine sand; 3 specimens, Pachia Ammos, Crete, 
<geoCoordinate direction="north" orientation="latitude" precision="1" value="35.111">35°06'39.6&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="25.809">25°48'32.4&quot;E</geoCoordinate>
