<taxonomicName id="EAAB4D66FFDFFFD1FEF2FC29FDF7FC6E" box="[363,530,943,968]" class="Magnoliopsida" family="Passifloraceae" kingdom="Plantae" order="Malpighiales" pageId="50" pageNumber="51" phylum="Tracheophyta" rank="family">Passifloraceae</taxonomicName>
