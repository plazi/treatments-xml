<taxonomicName id="4C0FFCFBFFEAFFD4FE4AAFBDFC8136A1" authority="Ladd, 1966" authorityName="Ladd" authorityYear="1966" box="[415,791,703,726]" class="Gastropoda" family="Barleeiidae" genus="Amphithalamus" kingdom="Animalia" order="Littorinimorpha" pageId="4" pageNumber="5" phylum="Mollusca" rank="species" species="myersi">
<emphasis id="B97B5B6AFFEAFFD4FE4AAFBDFD0236A1" box="[415,660,703,726]" italics="true" pageId="4" pageNumber="5">Amphithalamus myersi</emphasis>
