<taxonomicName id="04AA2EA1FFB8FFD7FEBF7D18FE524D78" authorityName="Ross" authorityYear="2001" box="[345,468,566,590]" class="Insecta" family="Scelembiidae" genus="Gibocercus" kingdom="Animalia" order="Embioptera" pageId="7" pageNumber="345" phylum="Arthropoda" rank="species" species="sandrae">
