<collectionCode id="ED4840A9FFADFF9D989AC7778C4CFDE2" box="[1229,1319,512,538]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34975" lsid="urn:lsid:biocol.org:col:34975" name="Instituut voor Taxonomische Zoologie, Zoologisch Museum" pageId="126" pageNumber="127">ZMAN</collectionCode>
