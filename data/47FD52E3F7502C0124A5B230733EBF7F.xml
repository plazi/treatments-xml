<taxonomicName id="7053EA6BD0A4E66B47E6C083B6927EAF" family="Pucciniastraceae" genus="M." higherTaxonomySource="treatment-meta" kingdom="Fungi" lsidName="M. neoexigua" order="Pucciniales" pageId="0" pageNumber="1" rank="species" species="neoexigua">M. neoexigua</taxonomicName>
