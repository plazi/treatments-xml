<subSubSection id="C318DCA54B57FFECD2B18E96FDD550C8" box="[151,558,719,744]" pageId="2" pageNumber="163" type="materials_examined">
<paragraph id="8BBD8F2E4B57FFECD2B18E96FDD550C8" blockId="2.[151,1437,719,1789]" box="[151,558,719,744]" pageId="2" pageNumber="163">
