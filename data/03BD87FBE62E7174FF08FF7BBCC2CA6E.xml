<emphasis id="B960EAFFE62E7174FEE6FE36BDAACFAE" box="[377,405,474,499]" italics="true" pageId="3" pageNumber="196">
d 
<number id="3DDC24CAE62E7174FE16FE36BDAACFAE" box="[393,405,474,499]" pageId="3" pageNumber="196" value="1.0">1</number>
