<taxonomicName id="4C304D40FFC82F09FF2BFB808DC66406" baseAuthorityName="Champion" baseAuthorityYear="1909" box="[234,498,1066,1087]" class="Insecta" family="Curculionidae" genus="Linogeraeus" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="164" phylum="Arthropoda" rank="species" species="squamirostris">
