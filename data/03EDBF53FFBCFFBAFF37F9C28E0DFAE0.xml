<specimenCode id="DBE2A63EFFBEFFBAFAEEFCDD8922FCD8" box="[1358,1392,832,858]" collectionCode="R" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="70" pageNumber="71">R1</specimenCode>
