<quantity id="4CF3FAAFF01BFFCD84337186FDA4B3DD" box="[583,638,1414,1441]" metricMagnitude="-2" metricUnit="m" metricValue="8.0" pageId="7" pageNumber="88" unit="cm" value="8.0">8 cm</quantity>
