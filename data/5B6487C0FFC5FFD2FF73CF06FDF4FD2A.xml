<caption id="87B2665EFFC2FFD1FF73C9FEFD90F823" ID-DOI="http://doi.org/10.5281/zenodo.4386307" ID-Zenodo-Dep="4386307" httpUri="https://zenodo.org/record/4386307/files/figure.png" pageId="13" pageNumber="193" startId="13.[151,250,1943,1967]" targetBox="[211,1378,187,1911]" targetPageId="13">
