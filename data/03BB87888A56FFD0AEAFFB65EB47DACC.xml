<paragraph id="8BAD369E8A56FFD0AEAFFB65EA19DB76" blockId="22.[246,531,1266,1290]" box="[246,531,1266,1290]" pageId="22" pageNumber="23">
<heading id="D0E581F28A56FFD0AEAFFB65EA19DB76" box="[246,531,1266,1290]" centered="true" fontSize="36" level="2" pageId="22" pageNumber="23" reason="8">
<taxonomicName id="4C124D1D8A56FFD0AEAFFB65EA19DB76" authority="(Cresson)" baseAuthorityName="Cresson" baseAuthorityYear="1879" box="[246,531,1266,1290]" class="Insecta" family="Apidae" genus="Ericrocis" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="lata">
