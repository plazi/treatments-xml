<taxonomicName id="20F00789FFC55334FAEC6E76FF502D80" authorityName="Waterhouse" authorityYear="1888" class="Insecta" family="Bostrichidae" genus="Stephanopachys" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="407" phylum="Arthropoda" rank="genus">
