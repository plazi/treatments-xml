<quantity id="761DC2BB3E32FF87FC44F9F0A6838E71" box="[906,994,1557,1579]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="9" pageNumber="1018" unit="mm" value="1.8">1,8 mm</quantity>
), 
<quantity id="761DC2BB3E32FF84FC34F9F0A58588B2" lastPageId="10" lastPageNumber="1019" metricMagnitude="-3" metricUnit="m" metricValue="1.85" metricValueMax="1.9" metricValueMin="1.8" pageId="9" pageNumber="1018" unit="mm" value="1.85" valueMax="1.9" valueMin="1.8">1,8- 1,9 mm</quantity>
(
<specimenCount id="A7E3A4D73E31FF84FF3DFF3DA47C88B7" box="[243,285,216,237]" pageId="10" pageNumber="1019" type="female">♀♀</specimenCount>
); Pronotumbreite 
<quantity id="761DC2BB3E31FF84FE16FF37A70788B2" box="[472,614,210,232]" metricMagnitude="-4" metricUnit="m" metricValue="7.75" metricValueMax="8.1" metricValueMin="7.4" pageId="10" pageNumber="1019" unit="mm" value="0.775" valueMax="0.81" valueMin="0.74">0,74-0,81 mm</quantity>
(33; 
