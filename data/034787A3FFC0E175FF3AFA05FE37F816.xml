<taxonomicName id="4CEE4D36FFCEE175FDD3F8B1FC5DF89E" authority="Kuang, 1989" authorityName="Kuang" authorityYear="1989" box="[638,1017,1806,1832]" class="Arachnida" family="Eriophyidae" genus="Aculops" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="66" phylum="Arthropoda" rank="species" species="sophorae">
<emphasis id="B99AEAA7FFCEE175FDD3F8B1FCF7F891" box="[638,851,1806,1831]" italics="true" pageId="9" pageNumber="66">Aculops sophorae</emphasis>
