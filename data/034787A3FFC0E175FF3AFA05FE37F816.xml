<taxonomicName id="4CEE4D36FFCEE175FB84F8E1FB1CF8C1" box="[1065,1208,1886,1911]" class="Arachnida" family="Eriophyidae" genus="Aculops" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="66" phylum="Arthropoda" rank="species" species="sophorae">
