<taxonomicName id="4C324D16877FFFD5FECDF7A9113BF83F" authorityName="Dekkers, Maxwell, &amp; Congdon" authorityYear="2019" box="[324,533,1939,1965]" class="Gastropoda" family="Seraphsidae" genus="Terebellum" kingdom="Animalia" order="Littorinimorpha" pageId="37" pageNumber="438" phylum="Mollusca" rank="species" species="simoni">
