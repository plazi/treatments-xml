<heading id="D0C98184C11C685BFE32F4EBFD20FB3D" box="[476,703,1173,1195]" centered="true" fontSize="9" level="2" pageId="0" pageNumber="509" reason="2">
<taxonomicName id="4C3E4D6BC11C685BFE32F4EBFDD9FB3C" authorityName="Monne &amp; Napp" authorityYear="2004" box="[476,582,1174,1195]" class="Insecta" family="Cerambycidae" genus="Timbaraba" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="509" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B94AEAFAC11C685BFE32F4EBFDD9FB3C" box="[476,582,1174,1195]" italics="true" pageId="0" pageNumber="509">Timbaraba</emphasis>
</taxonomicName>
, 
<emphasis id="B94AEAFAC11C685BFDBAF4E8FD20FB3D" bold="true" box="[596,703,1173,1194]" pageId="0" pageNumber="509">
<taxonomicNameLabel id="A2795781C11C685BFDBAF4E8FD20FB3D" box="[596,703,1173,1194]" pageId="0" pageNumber="509" rank="genus">new genus</taxonomicNameLabel>
