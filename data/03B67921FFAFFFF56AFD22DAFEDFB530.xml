<taxonomicName id="4C1FB3B4FFA1FFF5686426C7FE23B548" authorityName="Leach" authorityYear="1814" box="[348,462,240,264]" class="Malacostraca" family="Podoceridae" genus="Podocerus" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="9" phylum="Arthropoda" rank="genus">
