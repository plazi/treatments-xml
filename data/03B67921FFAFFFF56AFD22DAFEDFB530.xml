<taxonomicName id="4C1FB3B4FFA1FFF56B8326C7FCF4B547" authority="Hughes, 2013" authorityName="Hughes" authorityYear="2013" box="[699,793,240,263]" class="Malacostraca" family="Podoceridae" genus="Podocerus" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="9" phylum="Arthropoda" rank="species" species="miscix" status="sp. nov.">
