<taxonomicName id="E9265D79C3E7A08774EA7915782C8502" authority="Konow, 1902" authorityName="Konow" authorityYear="1902" class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristiphora (Pristiphora) denudata" order="Hymenoptera" pageId="0" pageNumber="1168" phylum="Arthropoda" rank="species" species="denudata" subGenus="Pristiphora">Pristiphora (Pristiphora) denudata Konow, 1902</taxonomicName>
