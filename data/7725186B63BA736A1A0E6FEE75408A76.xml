0.15, 
<normalizedToken id="2B8E828B5297FB0FC2D3102039910F0E" originalValue="PME–PLE">PME-PLE</normalizedToken>
0.35, 
<normalizedToken id="C816A4FA7A3D701855F9822BB202D6FF" originalValue="AME–PME">AME-PME</normalizedToken>
