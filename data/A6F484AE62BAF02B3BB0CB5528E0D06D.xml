<taxonomicName id="F42FCDEBAA030CEE08718980DFB1F86D" authority="Scherer, 1961" authorityName="Scherer" authorityYear="1961" class="Insecta" family="Chrysomelidae" genus="Afroalytus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Afroalytus" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">Afroalytus</taxonomicName>
