<taxonomicName id="4C424D4F3B3FE352FFB7FC19FEFD9EA0" authority="De Haan, 1833: 5" authorityName="De Haan" authorityPageNumber="5" authorityYear="1833" box="[113,382,976,998]" class="Malacostraca" family="Hexapodidae" genus="Hexapus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="400" phylum="Arthropoda" rank="genus">
