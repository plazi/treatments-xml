<number id="3D9524E49428FF8D99F828A75049FF14" box="[744,803,188,212]" pageId="3" pageNumber="166" value="36.1">36.1</number>
; tegmen: 
<number id="3D9524E49428FF8D988028A750CFFF14" box="[912,933,188,212]" pageId="3" pageNumber="166" value="3.0">3</number>
