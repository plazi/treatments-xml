<taxonomicName id="44E4C841B3F29158FAC7E1FB9D78D0D6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2414" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus Mayr" order="Hymenoptera" pageNumber="232" phylum="Arthropoda" rank="genus">Camponotus</taxonomicName>
Mayr
