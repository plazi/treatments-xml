<collectingCountry id="F301C1C4C871FFEB8DA69CB6FCFE20D6" box="[721,789,1455,1479]" name="Egypt" pageId="6" pageNumber="2287">Egypt</collectingCountry>
at 
<quantity id="4CEE2CB1C871FFEB8C489CB7FC7520D7" box="[831,926,1454,1478]" metricMagnitude="1" metricUnit="m" metricValue="2.25" metricValueMax="2.7" metricValueMin="1.8" pageId="6" pageNumber="2287" unit="m" value="22.5" valueMax="27.0" valueMin="18.0">18–27 m</quantity>
