<collectionCode id="F4F1AE412773780AFC76FCE638C86708" box="[907,990,818,841]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="3" pageNumber="813" type="Museum">FMNH</collectionCode>
