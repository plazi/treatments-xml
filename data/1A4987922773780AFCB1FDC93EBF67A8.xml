<collectionCode id="F4F1AE412773780AFC54FC8538E86728" box="[937,1022,849,873]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="3" pageNumber="813" type="Museum">FMNH</collectionCode>
