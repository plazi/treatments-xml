<subSubSection id="C30C8FA7FFFCDF7268C41456FE46FE91" pageId="9" pageNumber="10" type="diagnosis">
<paragraph id="8BA9DC2CFFFCDF7268C41456FE46FE91" blockId="9.[151,1436,279,343]" pageId="9" pageNumber="10">
<emphasis id="B962003EFFFCDF7268C41456FEDDFEF7" bold="true" box="[151,283,279,305]" pageId="9" pageNumber="10">Diagnosis:</emphasis>
