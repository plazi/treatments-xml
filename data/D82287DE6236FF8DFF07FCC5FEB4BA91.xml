</emphasis>
. 
<taxonomicName id="978B4D4B6236FF8DFEF9FC35FCB1BEBD" authority="Giesbert, 1991" authorityName="Giesbert" authorityYear="1991" box="[361,871,966,990]" class="Insecta" family="Cerambycidae" genus="Ommata" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="69" phylum="Arthropoda" rank="species" species="costipennis" subGenus="Eclipta">
