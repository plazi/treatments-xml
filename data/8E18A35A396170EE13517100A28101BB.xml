<emphasis id="51E19020F5CEBAD9E047ED1272277A27" bold="true" pageId="11" pageNumber="14">Figures 35-36.</emphasis>
<taxonomicName id="07B20C15BB0A994F16C609CBC6537B1A" authority="" authorityName="Talamas" authorityYear="2017" class="Insecta" family="Platygastroidea" genus="Trissolcus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trissolcus corai" order="Hemiptera" pageId="11" pageNumber="14" phylum="Arthropoda" rank="species" species="corai">
