<taxonomicName id="4C2C94F7FFCEFFA47CBE37D6FCDA949A" box="[683,837,1679,1704]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="13" pageNumber="14" phylum="Tracheophyta" rank="species" species="armeniaca">P. armeniaca</taxonomicName>
