<keyStep id="30D8F7D1FFCEFFA47E82374EFA0A9513" pageId="13" pageNumber="14">
<subSubSection id="C336BCFFFFCEFFA47E82374EFA0B949B" pageId="13" pageNumber="14" type="key">
<paragraph id="8B93EF74FFCEFFA47E82374EFA0B949B" blockId="13.[151,1436,1559,2028]" pageId="13" pageNumber="14">
<keyLead id="30DD4C41FFCEFFA47E82374EFA0B949B" pageId="13" pageNumber="14">
<number id="3DE4FD53FFCEFFA47E82374EFF389403" box="[151,167,1559,1585]" pageId="13" pageNumber="14" value="1.0">1</number>
