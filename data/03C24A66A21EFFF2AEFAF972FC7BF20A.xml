<taxonomicName id="4C6B80F3A21CFFF2AF71F959FE1FFD94" authorityName="Peck" authorityYear="1873" box="[259,463,1905,1927]" class="Myxomycetes" family="Physaraceae" genus="Craterium" kingdom="Protozoa" order="Physarales" pageId="7" pageNumber="112" phylum="Mycetozoa" rank="species" species="obovatum">Craterium obovatum</taxonomicName>
