<collectingCountry id="F37CBBE0A21DFFF3ACF9F8CEFD30FCED" box="[651,736,1765,1791]" name="Austria" pageId="6" pageNumber="111">Austria</collectingCountry>
, 
<collectingCountry id="F37CBBE0A21DFFF3AC99F8CEFCE1FCED" box="[747,817,1765,1791]" name="China" pageId="6" pageNumber="111">China</collectingCountry>
, 
<collectingCountry id="F37CBBE0A21DFFF3AD4EF8CDFCAFFD12" box="[828,895,1766,1792]" name="Japan" pageId="6" pageNumber="111">Japan</collectingCountry>
