<paragraph id="8BD4FB70A21DFFF3AEFAF97AFDC1FD7E" blockId="6.[136,529,1837,1900]" box="[136,529,1873,1900]" pageId="6" pageNumber="111">
<taxonomicName id="4C6B80F3A21DFFF3AEFAF97AFE65FD79" authority="Peck" authorityName="Peck" authorityYear="1873" box="[136,437,1873,1900]" class="Myxomycetes" family="Physaraceae" genus="Craterium" kingdom="Protozoa" order="Physarales" pageId="6" pageNumber="111" phylum="Mycetozoa" rank="species" species="obovatum">
<emphasis id="B91F2762A21DFFF3AEFAF97AFEA9FD7E" bold="true" box="[136,377,1873,1900]" italics="true" pageId="6" pageNumber="111">Craterium obovatum</emphasis>
