<taxonomicName id="4C6B80F3A21DFFF3AE94F906FEA0FD55" authorityName="Chevallier" authorityYear="1826" box="[230,368,1837,1863]" class="Myxomycetes" family="Physaraceae" kingdom="Protozoa" order="Physarales" pageId="6" pageNumber="111" phylum="Mycetozoa" rank="family">Physaraceae</taxonomicName>
