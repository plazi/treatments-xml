<taxonomicName id="08C464D17005E0B0289C3E71B59E6C07" class="Insecta" family="Tenthredinidae" genus="Pristiphora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristiphora leucopus" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="leucopus">Pristiphora leucopus</taxonomicName>
