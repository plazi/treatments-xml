<heading id="D0F881A0FFCFFFE3FF40FF3CFE4901D2" bold="true" box="[151,508,151,177]" fontSize="11" level="1" pageId="16" pageNumber="17" reason="1">
<emphasis id="B97BEADEFFCFFFE3FF40FF3CFE4901D2" bold="true" box="[151,508,151,177]" pageId="16" pageNumber="17">
Genus 
<taxonomicName id="4C0F4D4FFFCFFFE3FF39FF3CFE3201D2" box="[238,391,151,177]" class="Arachnida" family="Zerconidae" genus="Boreozercon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B97BEADEFFCFFFE3FF39FF3CFE3201D2" bold="true" box="[238,391,151,177]" italics="true" pageId="16" pageNumber="17">Boreozercon</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24857A5FFCFFFE3FE59FF3CFE4901D2" box="[398,508,151,177]" pageId="16" pageNumber="17" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
