<taxonomicName id="85B7D18C36682267FE17FBA8877CFB1C" authority="(Butler, 1886)" baseAuthorityName="Butler" baseAuthorityYear="1886" box="[458,791,1138,1162]" class="Insecta" family="Pieridae" genus="Pontia" kingdom="Animalia" order="Lepidoptera" pageId="22" pageNumber="1564" phylum="Arthropoda" rank="species" species="distorta">
