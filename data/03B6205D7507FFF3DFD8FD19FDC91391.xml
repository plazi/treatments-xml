<quantity id="4CE73CAE7507FFF2DC96F8E7FBCB10BE" box="[985,1075,1905,1927]" metricMagnitude="-2" metricUnit="m" metricValue="1.56" pageId="42" pageNumber="43" unit="mm" value="15.6">15.6 mm</quantity>
