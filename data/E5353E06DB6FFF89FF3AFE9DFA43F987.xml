<taxonomicName id="AA9CF493DB6FFF89FE31FE9DFDEFFECC" authorityName="Murrill" authorityYear="1905" box="[412,560,342,368]" class="Agaricomycetes" family="Meripilaceae" genus="Rigidoporus" kingdom="Fungi" order="Polyporales" pageId="7" pageNumber="198" phylum="Basidiomycota" rank="genus">
