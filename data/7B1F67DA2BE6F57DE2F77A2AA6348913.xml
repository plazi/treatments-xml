<taxonomicName id="1BD9B843AC78BB48F56A312BAF6F1CF6" authority="Lu, Zhang &amp; Liu, 2017" authorityName="Lu, Zhang &amp; Liu" authorityYear="2017" class="Insecta" family="Babinskaiidae" genus="Electrobabinskaia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Electrobabinskaia" order="Neuroptera" pageId="4" pageNumber="35" phylum="Arthropoda" rank="genus">Electrobabinskaia Lu, Zhang &amp; Liu, 2017</taxonomicName>
