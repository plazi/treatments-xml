<collectionCode id="ED5CAE5FFFB1330B5BFCFAC8B90F0C4E" box="[765,859,1379,1403]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="27" pageNumber="26">AMNH</collectionCode>
