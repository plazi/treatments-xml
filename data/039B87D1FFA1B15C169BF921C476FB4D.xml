<taxonomicName id="4C324D44FFA1B15F169BF88EC553F8C6" authority="Beatty, 1970: 464" authorityName="Beatty" authorityPageNumber="464" authorityYear="1970" box="[151,522,1869,1891]" class="Arachnida" family="Segestriidae" genus="Ariadna" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="weaveri">
