<collectingCountry id="E5C9F2D8807CFFDFE831CF2D19BF0F3C" name="South Africa" pageId="120" pageNumber="121">South Africa</collectingCountry>
, 
<collectingCountry id="E5C9F2D8807CFFDFED86CF0E18290F3C" box="[237,372,295,320]" name="Madagascar" pageId="120" pageNumber="121">Madagascar</collectingCountry>
