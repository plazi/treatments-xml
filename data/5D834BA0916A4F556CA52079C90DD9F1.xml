<taxonomicName id="72D3DFF9839BEB1605BC55D7901C130C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:8018" class="Insecta" family="Proctotrupidae" genus="Exallonyx" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Exallonyx chiuae Townes" order="Hymenoptera" pageNumber="40" phylum="Arthropoda" rank="species" species="chiuae">E. chiuae Townes and Townes</taxonomicName>
