<subSubSection id="C354F5A64164FFFBBBDF471EFA99FBA2" box="[199,1398,1050,1075]" pageId="6" pageNumber="337" type="etymology">
<paragraph id="8BF1A62D4164FFFBBBDF471EFA99FBA2" blockId="6.[151,1437,258,1075]" box="[199,1398,1050,1075]" pageId="6" pageNumber="337">
<emphasis id="B93A7A3F4164FFFBBBDF471EFEBEFBA2" bold="true" box="[199,337,1050,1075]" pageId="6" pageNumber="337">Etymology:</emphasis>
