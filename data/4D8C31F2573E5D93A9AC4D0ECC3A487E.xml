<taxonomicName id="E88FBE9435960E9094671152AC08BD0C" class="Equisetopsida" family="Chrysobalanaceae" genus="Hirtella" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Hirtella triandra" order="Malpighiales" pageId="0" pageNumber="609" phylum="Magnoliophyta" rank="species" species="triandra">
<emphasis id="E8F2C615A46F02FB470FA8341D0A4F2B" italics="true" pageId="0" pageNumber="609">Hirtella triandra</emphasis>
</taxonomicName>
(
<taxonomicName id="B709130F5E3B0D0B49D3A1C245DCCCC4" class="Equisetopsida" family="Chrysobalanaceae" genus="Hirtella" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Malpighiales" pageId="0" pageNumber="609" phylum="Magnoliophyta" rank="family">Chrysobalanaceae</taxonomicName>
) (
<bibRefCitation id="31AD5AF2954346E19A99C850B76360EF" author="Wood, SL" journalOrPublisher="Brigham Young University, M. L. Bean Life Science Museum; Provo" pageId="0" pageNumber="609" refId="B91" refString="Wood, SL, 2007. Bark and ambrosia beetles of South America (Coleoptera: Scolytidae). Brigham Young University, M.L. Bean Life Science Museum; Provo" title="Bark and ambrosia beetles of South America (Coleoptera: Scolytidae)." year="2007">Wood 2007</bibRefCitation>
