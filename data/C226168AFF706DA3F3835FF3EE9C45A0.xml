<taxonomicName id="8FFEEE461AC494405F6AC4F73F020377" authorityName="Balat" authorityYear="1955" class="insecta" family="Philopteridae" genus="Brueelia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brueelia rosickyi" order="Phthiraptera" pageId="12" pageNumber="29" phylum="Arthropoda" rank="species" species="rosickyi">
