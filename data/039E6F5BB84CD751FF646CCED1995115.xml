<collectingRegion id="49F310AFB84CD751FAF26C4ED7B65115" country="China" name="Sichuan" pageId="6" pageNumber="299">Sichuan Province</collectingRegion>
and 
<collectingRegion id="49F310AFB84CD751FC426C6ED0805115" box="[986,1173,290,313]" country="China" name="Yunnan" pageId="6" pageNumber="299">Yunnan Province</collectingRegion>
