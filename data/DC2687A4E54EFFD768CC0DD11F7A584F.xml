<taxonomicName id="938F4D31E54EFFD669280A151FDF5D69" box="[371,712,1875,1897]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="114" pageNumber="115" phylum="Arthropoda" rank="species" species="vidanoi" subGenus="Lymaenon">
<emphasis id="66FBEAA0E54EFFD669280A151CE15D68" box="[371,502,1875,1896]" italics="true" pageId="114" pageNumber="115">Gonatocerus</emphasis>
