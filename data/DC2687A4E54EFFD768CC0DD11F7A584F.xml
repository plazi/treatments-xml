, 
<quantity id="93779B57E54FFFD76A070C641FBD5B3A" box="[604,682,290,314]" metricMagnitude="2" metricUnit="m" metricValue="1.25" pageId="115" pageNumber="116" unit="m" value="125.0">125 m</quantity>
, 
