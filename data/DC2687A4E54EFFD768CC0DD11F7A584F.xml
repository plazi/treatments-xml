(
<taxonomicName id="938F4D31E54FFFD76AC80DD11E265AAF" box="[659,817,151,175]" class="Insecta" family="Membracidae" kingdom="Animalia" order="Hemiptera" pageId="115" pageNumber="116" phylum="Arthropoda" rank="family">Membracidae</taxonomicName>
) (
<bibRefCitation id="301E4B43E54FFFD76B1C0DD119575AAF" author="Viggiani &amp; Jesu" box="[839,1088,151,175]" firstAuthor="Viggiani" pageId="115" pageNumber="116" refString="Viggiani, G. &amp; Jesu, R. (1986) Due nuovi Mimaridi parassitoidi oofagi di Gargara genistae F. (Homoptera: Membracidae). Bollettino del Laboratorio di Entomologia Agraria &quot; Filippo Silvestri &quot; di Portici, 43, 29 - 34." type="journal article" year="1986">Viggiani &amp; Jesu 1986</bibRefCitation>
, 
<bibRefCitation id="301E4B43E54FFFD76C100DD119935AAF" author="Viggiani, G." box="[1099,1156,151,175]" journalOrPublisher="Proceedings of the 6 th Auchenorrhyncha Meeting, Turin, Italy, 7 - 11 Sept. 1987" pageId="115" pageNumber="116" pagination="555 - 565" part="1987" refId="ref122378" refString="Viggiani, G. (1988 [1987]). Biosystematics of mymarid egg-parasitoids of Auchenorrhyncha. Proceedings of the 6 th Auchenorrhyncha Meeting, Turin, Italy, 7 - 11 Sept. 1987, 555 - 565." title="Biosystematics of mymarid egg-parasitoids of Auchenorrhyncha" type="journal article" year="1988">1988</bibRefCitation>
) [as 
