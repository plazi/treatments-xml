<location id="CE71600DA802FFDDFFC8FB42FF6A0DB7" LSID="urn:lsid:plazi:treatment:430787C0A802FFDDFF5DFD87FC19083C:CE71600DA802FFDDFFC8FB42FF6A0DB7" box="[93,215,1175,1197]" county="Collection" municipality="Miller" name="Specimens" pageId="26" pageNumber="26">Specimens</location>
