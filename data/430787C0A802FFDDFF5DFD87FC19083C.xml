<collectionCode id="ADBFAE13A802FFDDFE5CFC33FD9A0AE1" box="[457,551,998,1019]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="26" pageNumber="26">AMNH</collectionCode>
