<taxonomicName id="4C394D07FFAB347DFEC0F93EFDF281DD" baseAuthorityName="Koch" baseAuthorityYear="1837" box="[341,542,1714,1738]" class="Arachnida" family="Lebertiidae" genus="Lebertia" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="122" phylum="Arthropoda" rank="species" species="inaequalis">
