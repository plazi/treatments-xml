<taxonomicName id="77DB06C0FB4B2440239C8520FE4CFBE9" authorityName="Reshchikov &amp; Alvarado" authorityYear="2022" box="[436,490,1110,1136]" class="Insecta" family="Ichneumonidae" genus="Exetastes" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="58" phylum="Arthropoda" rank="species" species="tullu">
