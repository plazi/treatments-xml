, 
<taxonomicName id="7CE6EDFCC142FFA71ABCF3D5FAD7FD5F" authority="Enghoff, 2014" authorityName="Enghoff" authorityYear="2014" box="[1259,1399,738,764]" class="Diplopoda" family="Odontopygidae" genus="Chaleponcus" kingdom="Animalia" order="Spirostreptida" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="mwabvui" status="sp. nov.">
