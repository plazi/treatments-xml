<taxonomicName id="4C48D489FFA4FFA929F5FCCBFDC5FC91" authority="Hsiao, 1964" authorityName="Hsiao" authorityYear="1964" box="[151,534,834,860]" class="Insecta" family="Aradidae" genus="Yangiella" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="30" phylum="Arthropoda" rank="species" species="mimetica">
<emphasis id="B93C7318FFA4FFA929F5FCCBFDC5FC91" bold="true" box="[151,534,834,860]" pageId="1" pageNumber="30">
<emphasis id="B93C7318FFA4FFA929F5FCCBFEADFC91" bold="true" box="[151,382,834,860]" italics="true" pageId="1" pageNumber="30">Yangiella mimetica</emphasis>
<bibRefCitation id="EFD9D2FBFFA4FFA928E7FCCBFDC5FC91" author="Hsiao" box="[389,534,834,860]" pageId="1" pageNumber="38" refString="Hsiao, T. Y. (1964) Results of the zoologico-botanical expedition to Southwest China 1955 - 1957. Acta Entomologica Sinica, 13, 587 - 605." type="journal article" year="1964">
Hsiao, 
<number id="3D80BD2DFFA4FFA928B9FCCBFDC5FC91" box="[475,534,834,860]" pageId="1" pageNumber="30" value="1964.0">1964</number>
</bibRefCitation>
