<quantity id="2E0562ABFF8CF64DFEC5FB0A32B055A2" box="[323,407,1194,1218]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="5" pageNumber="320" unit="mm" value="4.4">4.4 mm</quantity>
