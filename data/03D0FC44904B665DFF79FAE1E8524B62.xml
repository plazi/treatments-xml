<taxonomicName id="4C7936D1904B665DFD59FA02E93B48A5" authorityName="Wahlgren" authorityYear="1917" box="[695,800,1456,1474]" class="Insecta" family="Scathophagidae" genus="Norellisoma" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="131" phylum="Arthropoda" rank="genus">
