<taxonomicName id="4C7936D1904B665DFE7DFA02E82448A5" box="[403,575,1456,1474]" class="Insecta" family="Scathophagidae" genus="Norellia" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="131" phylum="Arthropoda" rank="species" species="triangulum">
