</emphasis>
<taxonomicName id="70BC4CB0E708FFCDFEE2B355FCC1510C" authority="Perty 1832: 109" authorityName="Perty" authorityPageNumber="109" authorityYear="1832" box="[382,846,1891,1916]" class="Insecta" family="Cleridae" genus="Chariessa" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="ramicornis">
