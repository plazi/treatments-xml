<subSubSection id="C3536579FFFAFF9BFF2498CBFE46F946" box="[192,503,1727,1751]" pageId="9" pageNumber="8" type="nomenclature">
<paragraph id="8BF636F2FFFAFF9BFF2498CBFE46F946" blockId="9.[192,503,1727,1751]" box="[192,503,1727,1751]" pageId="9" pageNumber="8">
<heading id="D0BE819EFFFAFF9BFF2498CBFE46F946" bold="true" box="[192,503,1727,1751]" fontSize="10" level="2" pageId="9" pageNumber="8" reason="3">
<taxonomicName id="4C494D71FFFAFF9BFF2498CBFE46F946" authority="Latreille" authorityName="Latreille" authorityYear="1802" box="[192,503,1727,1751]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="genus">
