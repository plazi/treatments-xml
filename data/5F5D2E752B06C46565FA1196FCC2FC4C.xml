<number id="613C8D442B05C46564ED114CFE4CFE05" box="[384,413,367,391]" pageId="2" pageNumber="66" value="24.0">24</number>
° 
<number id="613C8D442B05C46564C5114CFE16FE05" box="[424,455,367,391]" pageId="2" pageNumber="66" value="44.0">44</number>
’ 
<number id="613C8D442B05C46564A1114CFE3BFE05" box="[460,490,367,391]" pageId="2" pageNumber="66" value="57.0">57</number>
N, 
<number id="613C8D442B05C4656761114CFDE7FE05" box="[524,566,367,391]" pageId="2" pageNumber="66" value="121.0">121</number>
° 
<number id="613C8D442B05C465672C114CFD8EFE05" box="[577,607,367,391]" pageId="2" pageNumber="66" value="37.0">37</number>
’ 
<number id="613C8D442B05C4656709114CFD52FE05" box="[612,643,367,391]" pageId="2" pageNumber="66" value="30.0">30</number>
E), Yuanshan, Yilan County, 
<collectingCountry id="AFE3DFF32B05C46566A5114CFBCFFE05" box="[968,1054,367,391]" name="Taiwan" pageId="2" pageNumber="66">Taiwan</collectingCountry>
.
