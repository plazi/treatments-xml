</taxonomicName>
(
<taxonomicName id="A58972FC023EF248B5E800395987D004" class="Magnoliopsida" family="Capparaceae" kingdom="Plantae" order="Capparales" phylum="Tracheophyta" rank="family">Capparaceae</taxonomicName>
