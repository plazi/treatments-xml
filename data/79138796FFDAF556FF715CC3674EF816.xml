<taxonomicName id="36BA4D03FFDAF556FA655FEB652EF816" authorityName="Smith" authorityYear="1854" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="maculifrons">
