<taxonomicName id="36BA4D03FFDAF556FAC85F3761AEF85E" authorityName="Smith" authorityYear="1854" box="[1326,1436,1917,1943]" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="sponsa">
