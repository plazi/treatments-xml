<collectionCode id="97ABAE45FFDAF556FC6B5D2B67E1FAB2" box="[909,979,1377,1403]" pageId="5" pageNumber="156">TYPE</collectionCode>
<collectionCode id="97ABAE45FFDAF556FC3F5D2B602CFAB2" box="[985,1054,1377,1403]" pageId="5" pageNumber="156">HYM</collectionCode>
