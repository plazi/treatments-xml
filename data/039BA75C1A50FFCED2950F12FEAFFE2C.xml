<taxonomicName id="4C326DC91A51FFCED37F0826FE5BFEE0" box="[294,456,335,358]" class="Insecta" family="Acrididae" genus="Leptacris" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="326" phylum="Arthropoda" rank="species" species="kraussi">
