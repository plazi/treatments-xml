<quantity id="4CCABBAF1A51FFCED0AF0818FCA8FE0E" box="[758,827,369,392]" metricMagnitude="2" metricUnit="m" metricValue="9.0" pageId="15" pageNumber="326" unit="m" value="900.0">900 m</quantity>
