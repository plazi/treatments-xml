</paragraph>
<paragraph id="8B8D164A1A51FFCED2AB0818FEAFFE2C" blockId="15.[211,1444,301,426]" pageId="15" pageNumber="326">
<collectingRegion id="49F6D8A81A51FFCED2AB0818FE87FE0E" box="[242,276,369,392]" country="Cameroon" name="East" pageId="15" pageNumber="326">Les</collectingRegion>
