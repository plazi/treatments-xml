<taxonomicName id="3AF24D73FFD3FF956EC6F322FD499466" authority="LeConte, 1858" authorityName="LeConte" authorityYear="1858" box="[389,732,1316,1338]" class="Insecta" family="Tenebrionidae" genus="Eleodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="418" phylum="Arthropoda" rank="species" species="pedinoides">
