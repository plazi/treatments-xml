<taxonomicName id="4C70A5BCFFBAFF9FFDD6FA01FD170BCE" box="[572,708,1469,1491]" class="Malacostraca" family="Galatheidae" genus="Alainius" kingdom="Animalia" order="Decapoda" pageId="31" pageNumber="476" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis id="B904022DFFBAFF9FFDD6FA01FD4E0BCF" box="[572,669,1469,1490]" italics="true" pageId="31" pageNumber="476">Alainius</emphasis>
