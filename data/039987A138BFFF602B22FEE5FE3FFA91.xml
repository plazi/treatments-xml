<quantity id="4CC89B5238BFFF632B01FC58FD28FC49" box="[584,692,950,977]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="173" pageNumber="174" unit="mm" value="0.6">0.60 mm</quantity>
