<taxonomicName id="398C4D62FFF8C60DFEE611C077D19315" authorityName="Kensley &amp; Schotte" authorityYear="2000" box="[264,503,1535,1559]" class="Malacostraca" family="Anthuridae" genus="Xenanthura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="45" pageNumber="2100" phylum="Arthropoda" rank="species" species="victoriae">
