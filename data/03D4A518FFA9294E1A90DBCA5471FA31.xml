<emphasis id="B909C81CFFAF294C1A90DA125721F8D5" bold="true" box="[151,274,1808,1830]" pageId="5" pageNumber="336">FIGURE 3.</emphasis>
<taxonomicName id="4C7D6F8DFFAF294C1B1EDA125420F8D6" authority="Pešić &amp; Smit &amp; Mary, 2018" authorityName="Pešić &amp; Smit &amp; Mary" authorityYear="2018" box="[281,531,1808,1830]" class="Arachnida" family="Torrenticolidae" genus="Torrenticola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="336" phylum="Arthropoda" rank="species" species="mayottensis" status="sp. nov.">
<emphasis id="B909C81CFFAF294C1B1EDA125420F8D6" box="[281,531,1808,1830]" italics="true" pageId="5" pageNumber="336">Torrenticola mayottensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23A7567FFAF294C181DDA135459F8D6" box="[538,618,1809,1830]" pageId="5" pageNumber="336" rank="species">
