<collectingCountry id="F31312BEFF92FFBCCB47F931FDC6B368" box="[527,632,1778,1804]" name="Brazil" pageId="7" pageNumber="200">BRAZIL</collectingCountry>
. 
<emphasis id="B9708E3CFF92FFBCCBCAF932FCBDB368" bold="true" box="[642,771,1777,1804]" pageId="7" pageNumber="200">
<collectingRegion id="49C09CCCFF92FFBCCBCAF932FD40B368" box="[642,766,1777,1804]" country="Brazil" name="Sao Paulo" pageId="7" pageNumber="200">São Paulo</collectingRegion>
:
</emphasis>
<location id="8EDB04F5FF92FFBCCA42F931FCC4B368" LSID="urn:lsid:plazi:treatment:03ADE338FF92FFBCC9C0F9FDFE57BC48:8EDB04F5FF92FFBCCA42F931FCC4B368" box="[778,890,1778,1804]" country="Brazil" name="Angatuba" pageId="7" pageNumber="200" stateProvince="Sao Paulo">Angatuba</location>
, 
<date id="FFBA74EEFF92FFBCCACCF931FBE3B368" box="[900,1117,1778,1804]" pageId="7" pageNumber="200" value="2013-12-17">
<collectingDate id="EFFE8D06FF92FFBCCACCF931FBE3B368" box="[900,1117,1778,1804]" pageId="7" pageNumber="200" value="2013-12-17">17 December 2013</collectingDate>
</date>
, 
