<taxonomicName authority="Naskrecki, 2008 a" authorityName="Naskrecki" authorityYear="2008" box="[411,717,332,357]" class="Insecta" family="Phaneropteridae" genus="Adapantus" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="50" phylum="Arthropoda" rank="species" species="affluens">
