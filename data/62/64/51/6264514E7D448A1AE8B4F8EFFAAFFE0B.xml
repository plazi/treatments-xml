, 
<taxonomicName authority="Chopard, 1954" authorityName="Chopard" authorityYear="1954" box="[151,461,367,392]" class="Insecta" family="Phaneropteridae" genus="Adapantus" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="50" phylum="Arthropoda" rank="species" species="marmoratus">
<emphasis box="[151,288,369,392]" italics="true" pageId="9" pageNumber="50">marmoratus</emphasis>
<bibRefCitation author="Chopard" box="[294,461,367,392]" pageId="9" pageNumber="50" pagination="25 - 98" refId="ref11754" refString="Chopard L. (1954) La reserve naturelle integrale du Mont Nimba, fascicule 2. III. Orthopteroides Ensiferes. Memoires de l'Institut Francais d'Afrique Noire, 40, 25 - 98." type="journal article" year="1954">Chopard, 1954</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="(Chopard 1954)" authorityName="Chopard" authorityYear="1954" baseAuthorityName="Chopard" baseAuthorityYear="1954" box="[475,727,367,392]" class="Insecta" family="Phaneropteridae" genus="Adapantus" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="50" phylum="Arthropoda" rank="species" species="nitens">
<emphasis box="[475,542,369,392]" italics="true" pageId="9" pageNumber="50">nitens</emphasis>
(
<bibRefCitation author="Chopard" box="[556,718,367,392]" pageId="9" pageNumber="50" pagination="25 - 98" refId="ref11754" refString="Chopard L. (1954) La reserve naturelle integrale du Mont Nimba, fascicule 2. III. Orthopteroides Ensiferes. Memoires de l'Institut Francais d'Afrique Noire, 40, 25 - 98." type="journal article" year="1954">Chopard 1954</bibRefCitation>
