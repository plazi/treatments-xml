<subSubSection pageId="20" pageNumber="788" type="materials_examined">
<paragraph blockId="20.[491,1524,229,702]" pageId="20" pageNumber="788">
<emphasis bold="true" box="[531,747,230,254]" pageId="20" pageNumber="788">Material examined</emphasis>
— 
<specimenCount box="[790,939,229,253]" pageId="20" pageNumber="788" type="female">three females</specimenCount>
, 
<collectingCountry box="[951,1025,229,253]" name="Russia" pageId="20" pageNumber="788">Russia</collectingCountry>
, 
<collectingRegion box="[1037,1229,229,254]" country="Russia" name="Tyumen'" pageId="20" pageNumber="788">Tyumen Province</collectingRegion>
, vicinity of Malinovka, 
<geoCoordinate box="[491,593,259,288]" degrees="55" direction="north" minutes="06" orientation="latitude" pageId="20" pageNumber="788" precision="925" value="55.1">
55°06 
<superScript attach="left" box="[555,563,259,277]" fontSize="7" pageId="20" pageNumber="788">′</superScript>
N
</geoCoordinate>
, 
<geoCoordinate box="[603,693,264,288]" degrees="65" direction="east" minutes="04" orientation="longitude" pageId="20" pageNumber="788" precision="925" value="65.066666">65°04 E</geoCoordinate>
, 
<date box="[705,834,264,289]" pageId="20" pageNumber="788" value="2019-05-08">08 May 2019</date>
