5 (5-6), 
<emphasis box="[1455,1476,1980,2004]" italics="true" pageId="21" pageNumber="789">j6</emphasis>
5 (5-6), 
<emphasis box="[557,582,2015,2039]" italics="true" pageId="21" pageNumber="789">J2</emphasis>
5 (5-6), 
<emphasis box="[673,698,2015,2038]" italics="true" pageId="21" pageNumber="789">J5</emphasis>
9 (8-10), 
<emphasis box="[803,827,2015,2039]" italics="true" pageId="21" pageNumber="789">z2</emphasis>
15 (15-16), 
<emphasis box="[958,981,2015,2039]" italics="true" pageId="21" pageNumber="789">z4</emphasis>
15 (13-16), 
