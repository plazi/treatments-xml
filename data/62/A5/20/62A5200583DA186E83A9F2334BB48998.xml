= 
<taxonomicName class="Actinopterygii" family="Rondeletiidae" genus="Rondeletia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rondeletia orthoneura" order="Beryciformes" pageId="0" pageNumber="45" phylum="Chordata" rank="species" species="orthoneura">Rondeletia orthoneura</taxonomicName>
