, height 
<number box="[1079,1119,1836,1862]" pageId="9" pageNumber="26" value="1.3">1.3</number>
; abdomen length 
<number box="[264,304,1876,1902]" pageId="9" pageNumber="26" value="2.2">2.2</number>
. Approximate leg lengths 
<number box="[612,627,1876,1902]" pageId="9" pageNumber="26" value="4.0">4</number>
&gt; 
<number box="[657,672,1876,1902]" pageId="9" pageNumber="26" value="1.0">1</number>
&gt; 
<number box="[702,717,1876,1902]" pageId="9" pageNumber="26" value="3.0">3</number>
