<taxonomicName box="[834,963,1793,1816]" class="Insecta" family="Staphylinidae" genus="Seeversiella" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="genus">Seeversiella</taxonomicName>
<taxonomicName authority="Gusarov" authorityName="Gusarov" box="[979,1126,1793,1816]" pageId="76" pageNumber="77" rank="species" species="impressicollis" status="sp. n.">impressicollis</taxonomicName>
