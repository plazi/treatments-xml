<taxonomicName box="[629,758,1694,1717]" class="Insecta" family="Staphylinidae" genus="Seeversiella" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="genus">Seeversiella</taxonomicName>
<taxonomicName authority="Gusarov" authorityName="Gusarov" box="[764,884,1694,1717]" pageId="64" pageNumber="65" rank="species" species="curtipennis" status="sp. n.">curtipennis</taxonomicName>
