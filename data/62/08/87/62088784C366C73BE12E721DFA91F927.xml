. Abdominal segment 8 of 
<taxonomicName authority="Gusarov" authorityName="Gusarov" box="[768,1134,1508,1531]" class="Insecta" family="Staphylinidae" genus="Seeversiella" kingdom="Animalia" order="Coleoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="micralymma" status="sp. n.">
<taxonomicName box="[768,1037,1508,1531]" class="Insecta" family="Staphylinidae" genus="Seeversiella" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="micralymma">
<emphasis box="[768,1037,1508,1531]" italics="true" pageId="73" pageNumber="74">
<taxonomicName box="[768,897,1508,1531]" class="Insecta" family="Staphylinidae" genus="Seeversiella" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="genus">Seeversiella</taxonomicName>
<taxonomicName authority="Gusarov" authorityName="Gusarov" box="[904,1037,1508,1531]" pageId="73" pageNumber="74" rank="species" species="micralymma" status="sp. n.">micralymma</taxonomicName>
</emphasis>
</taxonomicName>
Gusarov
</taxonomicName>
, 
