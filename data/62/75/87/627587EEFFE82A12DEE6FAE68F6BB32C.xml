<number box="[151,204,583,608]" pageId="23" pageNumber="174" value="0.58">0.58</number>
; SI: 
<number box="[254,285,583,608]" pageId="23" pageNumber="174" value="64.0">64</number>
; PW: 
<number box="[353,406,583,608]" pageId="23" pageNumber="174" value="0.68">0.68</number>
; WL: 
<number box="[475,527,583,608]" pageId="23" pageNumber="174" value="1.25">1.25</number>
