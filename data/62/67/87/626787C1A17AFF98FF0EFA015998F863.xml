H. Wildpret
</taxonomicName>
, 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[151,217,1739,1764]" class="Liliopsida" family="Asparagaceae" genus="Yucca" kingdom="Plantae" order="Asparagales" pageId="3" pageNumber="104" phylum="Tracheophyta" rank="genus">
<emphasis box="[151,217,1739,1764]" italics="true" pageId="3" pageNumber="104">Yucca</emphasis>
</taxonomicName>
sp., 
<taxonomicName box="[269,464,1739,1764]" class="Magnoliopsida" family="Lythraceae" genus="Punica" kingdom="Plantae" order="Myrtales" pageId="3" pageNumber="104" phylum="Tracheophyta" rank="species" species="granatum">
<emphasis box="[269,464,1739,1764]" italics="true" pageId="3" pageNumber="104">Punica granatum</emphasis>
</taxonomicName>
L., 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[508,606,1738,1764]" class="Magnoliopsida" family="Malvaceae" genus="Hibiscus" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="104" phylum="Tracheophyta" rank="genus">
<emphasis box="[508,606,1738,1764]" italics="true" pageId="3" pageNumber="104">Hibiscus</emphasis>
</taxonomicName>
sp., 
<taxonomicName authorityName="Linneo" authorityYear="1753" box="[658,849,1738,1764]" class="Magnoliopsida" family="Apocynaceae" genus="Nerium" kingdom="Plantae" order="Gentianales" pageId="3" pageNumber="104" phylum="Tracheophyta" rank="species" species="oleander">
<emphasis box="[658,849,1738,1764]" italics="true" pageId="3" pageNumber="104">Nerium oleander</emphasis>
</taxonomicName>
L. and 
<taxonomicName authorityName="C.Linnaeus" authorityYear="1753" box="[933,1053,1738,1764]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="104" phylum="Tracheophyta" rank="genus">
<emphasis box="[933,1053,1738,1764]" italics="true" pageId="3" pageNumber="104">Euphorbia</emphasis>
</taxonomicName>
sp. at Caleta de Fuste (
<date box="[1312,1431,1738,1764]" pageId="3" pageNumber="104" value="2017-02-13">13.II.2017</date>
, 
<date box="[151,281,1774,1800]" pageId="3" pageNumber="104" value="2017-03-30">30.III.2017</date>
), Costa Calma (08–09.II.2017, 
<date box="[628,747,1774,1800]" pageId="3" pageNumber="104" value="2917-02-11">11.II.2917</date>
, 
<date box="[757,876,1774,1800]" pageId="3" pageNumber="104" value="2017-02-14">14.II.2017</date>
, 
<date box="[886,1058,1774,1800]" pageId="3" pageNumber="104" value="2017-03-21" valueMax="2017-03-22" valueMin="2017-03-21">21–22.III.2017</date>
), La Lajita (
<date box="[1191,1319,1774,1800]" pageId="3" pageNumber="104" value="2017-03-23">23.III.2017</date>
) and Morro Jable (
<date box="[254,385,1810,1836]" pageId="3" pageNumber="104" value="2017-03-24">24.III.2017</date>
