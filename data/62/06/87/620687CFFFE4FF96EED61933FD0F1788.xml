) 
<number box="[763,808,1764,1788]" pageId="5" pageNumber="206" value="7.5" valueMax="8.0" valueMin="7.0">7–8</number>
(
<number box="[817,830,1764,1788]" pageId="5" pageNumber="206" value="3.0">3</number>
) marginal setae. Chaetotaxy of sternites IV–XI: (
<number box="[1384,1398,1763,1788]" pageId="5" pageNumber="206" value="2.0">2</number>
) 
<number pageId="5" pageNumber="206" value="7.0" valueMax="8.0" valueMin="6.0">6– 8</number>
(
<number box="[175,192,1799,1824]" pageId="5" pageNumber="206" value="2.0">2</number>
