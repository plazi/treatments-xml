<emphasis box="[1083,1110,1868,1893]" italics="true" pageId="12" pageNumber="213">vb</emphasis>
very short, 
<quantity box="[1252,1369,1868,1893]" metricMagnitude="-5" metricUnit="m" metricValue="2.5" pageId="12" pageNumber="213" unit="mm" value="0.025">0.025 mm</quantity>
