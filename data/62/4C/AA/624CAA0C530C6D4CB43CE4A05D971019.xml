<subSubSection pageId="null" pageNumber="83" type="vernacular_name">
<paragraph pageId="null" pageNumber="83">
Stacheitruchtiger 
<normalizedToken originalValue="Hahnentuß" pageId="null" pageNumber="83">Hahnentuss</normalizedToken>
</paragraph>
