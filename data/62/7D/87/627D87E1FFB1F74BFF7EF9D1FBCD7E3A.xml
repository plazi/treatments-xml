<paragraph blockId="79.[151,846,1610,1676]" box="[151,846,1610,1636]" pageId="79" pageNumber="80">
<heading bold="true" box="[151,846,1610,1636]" fontSize="11" level="1" pageId="79" pageNumber="80" reason="1">
<emphasis bold="true" box="[151,846,1610,1636]" pageId="79" pageNumber="80">
<taxonomicName authority="Olah &amp; Barnard" authorityName="Olah &amp; Barnard" authorityYear="2008" box="[151,690,1610,1636]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="abhugna" status="new species">
<emphasis bold="true" box="[151,472,1610,1636]" italics="true" pageId="79" pageNumber="80">Cheumatopsyche abhugna</emphasis>
Oláh &amp; Barnard
</taxonomicName>
, 
<taxonomicNameLabel box="[703,846,1610,1636]" pageId="79" pageNumber="80" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
