<subSubSection box="[151,827,807,833]" pageId="29" pageNumber="30" type="nomenclature">
<paragraph blockId="29.[151,827,807,874]" box="[151,827,807,833]" pageId="29" pageNumber="30">
<heading bold="true" box="[151,827,807,833]" fontSize="11" level="1" pageId="29" pageNumber="30" reason="1">
<emphasis bold="true" box="[151,827,807,833]" pageId="29" pageNumber="30">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,671,807,833]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="malmi" status="new species">
<emphasis bold="true" box="[151,442,807,833]" italics="true" pageId="29" pageNumber="30">Cheumatopsyche malmi</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[683,827,807,833]" pageId="29" pageNumber="30" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
