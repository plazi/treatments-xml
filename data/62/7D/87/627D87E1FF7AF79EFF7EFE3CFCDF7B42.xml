<subSubSection box="[151,832,423,449]" pageId="132" pageNumber="133" type="nomenclature">
<paragraph blockId="132.[151,832,423,490]" box="[151,832,423,449]" pageId="132" pageNumber="133">
<heading bold="true" box="[151,832,423,449]" fontSize="11" level="1" pageId="132" pageNumber="133" reason="1">
<emphasis bold="true" box="[151,832,423,449]" pageId="132" pageNumber="133">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,675,423,449]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="132" pageNumber="133" phylum="Arthropoda" rank="species" species="ecsedii" status="new species">
<emphasis bold="true" box="[151,446,423,449]" italics="true" pageId="132" pageNumber="133">Cheumatopsyche ecsedii</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[688,832,423,449]" pageId="132" pageNumber="133" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
