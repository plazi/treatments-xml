<paragraph blockId="76.[151,856,1791,1858]" box="[151,856,1791,1817]" pageId="76" pageNumber="77">
<heading bold="true" box="[151,856,1791,1817]" fontSize="11" level="1" pageId="76" pageNumber="77" reason="1">
<emphasis bold="true" box="[151,856,1791,1817]" pageId="76" pageNumber="77">
