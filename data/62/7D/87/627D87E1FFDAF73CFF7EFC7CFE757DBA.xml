<paragraph blockId="36.[151,841,999,1066]" box="[151,841,999,1025]" pageId="36" pageNumber="37">
<heading bold="true" box="[151,841,999,1025]" fontSize="11" level="1" pageId="36" pageNumber="37" reason="1">
<emphasis bold="true" box="[151,841,999,1025]" pageId="36" pageNumber="37">
