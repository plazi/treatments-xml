<subSubSection box="[151,879,1716,1742]" pageId="14" pageNumber="15" type="nomenclature">
<paragraph blockId="14.[151,879,1716,1782]" box="[151,879,1716,1742]" pageId="14" pageNumber="15">
<heading bold="true" box="[151,879,1716,1742]" fontSize="11" level="1" pageId="14" pageNumber="15" reason="1">
<emphasis bold="true" box="[151,879,1716,1742]" pageId="14" pageNumber="15">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,721,1716,1742]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="acuminata" status="new species">
<emphasis bold="true" box="[151,493,1716,1742]" italics="true" pageId="14" pageNumber="15">Cheumatopsyche acuminata</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[735,879,1716,1742]" pageId="14" pageNumber="15" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
