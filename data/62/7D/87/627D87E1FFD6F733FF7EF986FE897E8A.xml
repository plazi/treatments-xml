<taxonomicName authorityName="Olah &amp; Johanson" authorityYear="2008" box="[365,472,832,857]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="bohakayi">
