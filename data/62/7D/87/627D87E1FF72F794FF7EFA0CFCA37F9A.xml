<paragraph blockId="140.[151,837,1431,1498]" box="[151,837,1431,1457]" pageId="140" pageNumber="141">
<heading bold="true" box="[151,837,1431,1457]" fontSize="11" level="1" pageId="140" pageNumber="141" reason="1">
<emphasis bold="true" box="[151,837,1431,1457]" pageId="140" pageNumber="141">
