<paragraph blockId="59.[151,889,1015,1082]" box="[151,889,1015,1041]" pageId="59" pageNumber="60">
<heading bold="true" box="[151,889,1015,1041]" fontSize="11" level="1" pageId="59" pageNumber="60" reason="1">
<emphasis bold="true" box="[151,889,1015,1041]" pageId="59" pageNumber="60">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,731,1015,1041]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="nohondoan" status="new species">
<emphasis bold="true" box="[151,503,1015,1041]" italics="true" pageId="59" pageNumber="60">Cheumatopsyche nohondoan</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[745,889,1015,1041]" pageId="59" pageNumber="60" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
