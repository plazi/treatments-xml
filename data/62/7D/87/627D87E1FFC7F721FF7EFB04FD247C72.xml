<paragraph blockId="57.[151,845,1183,1250]" box="[151,845,1183,1209]" pageId="57" pageNumber="58">
<heading bold="true" box="[151,845,1183,1209]" fontSize="11" level="1" pageId="57" pageNumber="58" reason="1">
<emphasis bold="true" box="[151,845,1183,1209]" pageId="57" pageNumber="58">
<taxonomicName authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,688,1183,1209]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="kumasa" status="new species">
<emphasis bold="true" box="[151,459,1183,1209]" italics="true" pageId="57" pageNumber="58">Cheumatopsyche kumasa</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel box="[701,845,1183,1209]" pageId="57" pageNumber="58" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
