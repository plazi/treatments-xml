<taxonomicName authorityName="Cockerell" authorityYear="1897" class="Arachnida" family="Salticidae" genus="Icius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Icius peckhamae" order="Araneae" pageId="349" pageNumber="350" phylum="Arthropoda" rank="species" species="peckhamae">
