</paragraph>
<paragraph blockId="7.[151,1178,1620,1644]" box="[151,1178,1620,1644]" pageId="7" pageNumber="56">
<emphasis bold="true" box="[151,264,1620,1644]" pageId="7" pageNumber="56">
<collectionCode box="[151,238,1620,1644]" pageId="7" pageNumber="56">PLATE</collectionCode>
