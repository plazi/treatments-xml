<collectionCode id="ED5BAE1FFFCCAB417A72FE92FB63EA0D" box="[1101,1165,349,375]" country="Myanmar" httpUri="http://biocol.org/urn:lsid:biocol.org:col:12615" name="Arts and Science University" pageId="37" pageNumber="38" type="Herbarium">ASM</collectionCode>
