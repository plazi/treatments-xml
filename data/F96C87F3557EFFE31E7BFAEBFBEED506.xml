<taxonomicName id="B6C54D66557DFFE31C17F955FBEDD48E" authorityName="Niitsuma. Lengths" authorityYear="1992" box="[868,1052,1679,1705]" class="Insecta" family="Chironomidae" genus="Polypedilum" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="paraviceps" subGenus="Uresipedilum">
