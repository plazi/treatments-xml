<taxonomicName id="4B25919B5C3690A7F19975C6F6DE8527" class="Insecta" family="Curculionidae" genus="Haversia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haversia defensa" order="Coleoptera" pageId="22" pageNumber="47" phylum="Arthropoda" rank="species" species="defensa">Haversia defensa</taxonomicName>
