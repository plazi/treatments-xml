<paragraph id="E560E537BC4B8538854FB1FC10447F57" blockId="24.[264,1324,1708,1894]" pageId="24" pageNumber="35">
<taxonomicName id="22DF9EB4BC4B8538854FB1FC10287EDC" box="[312,536,1748,1773]" class="Arachnida" family="Laelapidae" genus="Laelaspis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="24" pageNumber="35" phylum="Arthropoda" rank="species" species="secedens">
