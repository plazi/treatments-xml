<paragraph id="51A987E4194FFDEF455425D437EFEEF9" pageId="8" pageNumber="145">
<taxonomicName id="F2DFB8372EDD3D06E75E1248A817B232" family="Sphingidae" lsidName="" pageId="8" pageNumber="145" rank="family">Sphingidae</taxonomicName>
: 
<taxonomicName id="F4DCDE078D105F69E2BBB19FFAC2790D" class="Insecta" family="Sphingidae" genus="Erinnyis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erinnyis crameri" order="Lepidoptera" pageId="8" pageNumber="145" phylum="Arthropoda" rank="species" species="crameri">Erinnyis crameri</taxonomicName>
, 
<taxonomicName id="FB04A7423648E7B1541E64849AAFE9CD" class="Insecta" family="Sphingidae" genus="Erinnyis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erinnyis ello" order="Lepidoptera" pageId="8" pageNumber="145" phylum="Arthropoda" rank="species" species="ello">Erinnyis ello</taxonomicName>
