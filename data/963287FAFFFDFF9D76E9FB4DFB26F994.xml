<taxonomicName id="D99B4D6FFFFDFF9D754CF9D7FBD1F994" authority="Oudemans" authorityName="Oudemans" box="[818,1030,1575,1600]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="20" pageNumber="121" phylum="Arthropoda" rank="species" species="tiliae">
