<paragraph id="2023A33E150CDB530B85FE3AEDD1FE11" blockId="1.[151,1260,473,495]" box="[151,1260,473,495]" pageId="1" pageNumber="508">
<taxonomicName id="E79CD8BD150CDB530B85FE3AE9C6FE10" authorityName="Ale-Rocha &amp; Carvalho" authorityYear="2017" box="[151,251,473,494]" class="Insecta" family="Hybotidae" genus="Neohybos" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="508" phylum="Arthropoda" rank="genus">
