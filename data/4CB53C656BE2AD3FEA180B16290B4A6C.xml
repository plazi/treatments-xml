<taxonomicName id="17C514C9814C9FF96C622B2E9F78EC5C" class="Insecta" family="Cerambycidae" genus="Monochamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Monochamus galloprovincialis" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="galloprovincialis">Monochamus galloprovincialis</taxonomicName>
