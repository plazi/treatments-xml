<taxonomicName id="27ED50B0374CDF9EAC6980DCD85E7801" class="Insecta" family="Buprestidae" genus="Anthaxia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthaxia aurulenta" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="aurulenta">Anthaxia aurulenta</taxonomicName>
(Fabricius), 
<taxonomicName id="34285FBCF9651FCB8AA6B0920E8FC793" class="Insecta" family="Buprestidae" genus="Anthaxia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthaxia deaurata" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="deaurata">Anthaxia deaurata</taxonomicName>
(Gmelin), 
<taxonomicName id="064BD483BC06B32CE008CBC5765E0737" class="Insecta" family="Buprestidae" genus="Chrysobothris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysobothris solieri" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="solieri">Chrysobothris solieri</taxonomicName>
(Laporte &amp; Gory), 
<taxonomicName id="B2AD90139F1D395575B50A42A3F5A84B" authority="Kerremans, 1900" authorityName="Kerremans" authorityYear="1900" class="Insecta" family="Buprestidae" genus="Ovalisia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ovalisia mirifica" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="mirifica">Ovalisia mirifica</taxonomicName>
(Mulsant), 
<taxonomicName id="36A2861D4A3217006B2810CD1B63A221" class="Insecta" family="Buprestidae" genus="Melanophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Melanophila picta subsp. decastigma" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="subSpecies" species="picta" subSpecies="decastigma">Melanophila picta decastigma</taxonomicName>
(Fabricius); 
<taxonomicName id="F5B99F7ADB580DCCDD2E34F20AE8F5A4" class="Insecta" family="Buprestidae" genus="Sphenoptera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphenoptera tappesi" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="tappesi">Sphenoptera tappesi</taxonomicName>
Marseul (
<taxonomicName id="47B07E7ABE1C7F0E800D891E77BDAA98" family="Buprestidae" lsidName="" pageId="4" pageNumber="33" rank="family">Buprestidae</taxonomicName>
); 
<taxonomicName id="AC1D42134F5C46B761ED6E176248D365" class="Insecta" family="Cerambycidae" genus="Arhopalus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arhopalus syriacus" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="syriacus">Arhopalus syriacus</taxonomicName>
(Reitter), 
<taxonomicName id="78CAA4D6731C5B4C4A6AA6BEA6C9647F" class="Insecta" family="Cerambycidae" genus="Stictoleptura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stictoleptura rubra" order="Coleoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="rubra">Stictoleptura rubra</taxonomicName>
