(
<bibRefCitation id="4E54E1A68B6EFFE635E5FF17C1212598" author="Berlese" box="[1234,1374,210,232]" pageId="9" refString="Berlese, A. (1905) Acari nuovi. Materiali pel &quot; Manipulus V &quot;. Redia, 2, 231 - 238." type="journal article" year="1905">Berlese, 1905</bibRefCitation>
)
</taxonomicName>
;
</emphasis>
Italy
</keyLead>
</paragraph>
<paragraph id="2A7A9C578B6EFFE631A0FF2AC1E42452" blockId="9.[151,1436,151,1640]" pageId="9">
<keyLead id="91343F628B6EFFE631A0FF2AC1E42452" pageId="9">
