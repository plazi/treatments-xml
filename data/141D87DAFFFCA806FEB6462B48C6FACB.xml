<taxonomicName id="5BB44D4FFFFFA807FEEB41194990FBC2" authority="(Schedl)" authorityName="Beaver &amp; Hulcr" authorityYear="2008" baseAuthorityName="Beaver &amp; Hulcr" baseAuthorityYear="2008" box="[360,698,1086,1105]" class="Insecta" family="Curculionidae" genus="Cryptoxyleborus" kingdom="Animalia" order="Coleoptera" pageId="14" phylum="Arthropoda" rank="species" species="percuneolus">
<emphasis id="AEC0EADEFFFFA807FEEB41194940FBC2" box="[360,618,1086,1105]" italics="true" pageId="14">Cryptoxyleborus percuneolus</emphasis>
