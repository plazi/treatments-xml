<quantity id="4CB09B23BA7E512BFCC966FDF0F2FDCB" box="[862,997,491,519]" metricMagnitude="-3" metricUnit="m" metricValue="4.15" metricValueMax="4.5" metricValueMin="3.8" pageId="10" pageNumber="535" unit="mm" value="4.15" valueMax="4.5" valueMin="3.8">3.8–4.5 mm</quantity>
