</taxonomicName>
sp. (
<taxonomicName id="8DEE4D50BF5FFFFBFC01F6396D21640D" box="[994,1119,1627,1654]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="12" pageNumber="163" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
