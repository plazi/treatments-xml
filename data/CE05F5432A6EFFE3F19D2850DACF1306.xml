<collectionCode id="20BDDC902A6EFFE0F37428C4D8981195" box="[638,692,1931,1955]" httpUri="http://grbio.org/cool/xsgz-nxkh" name="Instituto de Ciencias Naturales, Museo de Historia Natural" pageId="5" pageNumber="76">ICN</collectionCode>
