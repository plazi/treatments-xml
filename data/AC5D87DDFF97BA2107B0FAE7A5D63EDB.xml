<taxonomicName id="E3F44D48FF95BA2107AFFCA5A3D33843" authorityName="Tippmann" authorityYear="1960" box="[483,657,871,895]" class="Insecta" family="Cerambycidae" genus="Cylindrommata" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2643" phylum="Arthropoda" rank="genus">
