<taxonomicName id="EEDE4D53FFCE1333FC99F980AD82F926" authority="Zhang, Bai, Heiss &amp; Cai" authorityName="Zhang, Bai, Heiss &amp; Cai" box="[151,749,1638,1664]" class="Insecta" family="Aradidae" genus="Vietnamaptera" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="62" phylum="Arthropoda" rank="species" species="bogiessa" status="sp. n.">
<emphasis id="1BAAEAC2FFCE1333FC99F980AEDAF926" bold="true" box="[151,437,1638,1664]" italics="true" pageId="2" pageNumber="62">Vietnamaptera bogiessa</emphasis>
Zhang, Bai, Heiss &amp; Cai
</taxonomicName>
, 
<taxonomicNameLabel id="009957B9FFCE1333FEF5F980AC52F926" box="[763,829,1638,1664]" pageId="2" pageNumber="62" rank="species">sp. n.</taxonomicNameLabel>
(
<figureCitation id="B1E52A55FFCE1333FF42F980ACA3F926" box="[844,972,1638,1664]" captionStart-0="FIGURE 1" captionStart-1="FIGURES 2 – 4" captionStart-2="FIGURES 5 – 13" captionStartId-0="1.[151,255,1893,1917]" captionStartId-1="2.[151,272,1467,1491]" captionStartId-2="3.[151,271,1665,1689]" captionTargetBox-0="[157,1426,659,1862]" captionTargetBox-1="[154,1433,449,1446]" captionTargetBox-2="[218,1369,202,1635]" captionTargetId-0="figure@1.[151,1436,645,1874]" captionTargetId-1="figure@2.[151,1436,444,1454]" captionTargetId-2="figure@3.[211,1376,197,1644]" captionTargetPageId-0="1" captionTargetPageId-1="2" captionTargetPageId-2="3" captionText-0="FIGURE 1. Vietnamaptera bogiessa sp. n. 3, Holotype. Habitus. Scale bar = 1 mm." captionText-1="FIGURES 2 – 4. Vietnamaptera bogiessa sp. n. 2, Habitus, antennae and legs partly removed; 3, head; 4, abdominal segments V – VIII; 2, lateral view; 3, 4, ventral view. Abbreviations. pr = pronotum; ms = mesonotum; mt = metanotum; mtg = mediotergites III – VI (tergal plate). Scale bar for figs. 2, 4 = 1.66 mm; for fig. 3 = 1 mm." captionText-2="FIGURES 5 – 13. Vietnamaptera bogiessa sp. n., Holotype. 5 – 7, Pygophore; 8 - 10, left paramere; 11 – 13, phallus and basal structure. 5, 9, 13, dorsal view; 6, 8, 11, ventral view; 7, 10, 12, lateral view. Scale bar for figs. 5 – 7 = 0.5 mm; for figs. 8 – 13 = 0.25 mm." httpUri-0="https://zenodo.org/record/196515/files/figure.png" httpUri-1="https://zenodo.org/record/196516/files/figure.png" httpUri-2="https://zenodo.org/record/196517/files/figure.png" pageId="2" pageNumber="62">
Figs. 
<number id="9F1624F7FFCE1333FF9EF980ACA3F926" box="[912,972,1638,1664]" pageId="2" pageNumber="62" value="7.0" valueMax="13.0" valueMin="1.0">1–13</number>
</figureCitation>
