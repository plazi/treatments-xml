<paragraph id="DB4F25409796FF40FF75FE38FCEDBEE4" blockId="193.[151,787,437,459]" box="[151,787,437,459]" pageId="193" pageNumber="194">
<taxonomicName id="1CF05EC39796FF40FF75FE38FF01BEE4" authorityName="Ingrisch" authorityYear="2018" box="[151,255,437,459]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="193" pageNumber="194" phylum="Arthropoda" rank="genus">
<emphasis id="E984F9529796FF40FF75FE38FF01BEE4" box="[151,255,437,459]" italics="true" pageId="193" pageNumber="194">Gryllacris</emphasis>
</taxonomicName>
(
<taxonomicName id="1CF05EC39796FF40FEEFFE38FE99BEE4" baseAuthorityName="Stoll" baseAuthorityYear="1813" box="[269,359,437,459]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="193" pageNumber="194" phylum="Arthropoda" rank="species" species="signifera">
<emphasis id="E984F9529796FF40FEEFFE38FE99BEE4" box="[269,359,437,459]" italics="true" pageId="193" pageNumber="194">signifera</emphasis>
</taxonomicName>
