<taxonomicName id="7BB778F79C5EFFD6FAB8FDBB2916FD6B" authority="Chazeau 1991" authorityName="Chazeau" authorityYear="1991" class="Insecta" family="Aleyrodidae" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="317" phylum="Arthropoda" rank="family">
