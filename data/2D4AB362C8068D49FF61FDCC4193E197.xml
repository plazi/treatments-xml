<taxonomicName id="62E379F7C8068D49FC0CFC5A452BE107" box="[1018,1104,1014,1039]" class="Insecta" family="Staphylinidae" genus="Byraxis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="262" phylum="Arthropoda" rank="genus">
