<taxonomicName id="6A8416A9352A68077EF4FD63FBF55AF1" box="[1000,1118,657,680]" class="Mollusca" family="Mollusca" genus="Laminaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca" pageId="14" pageNumber="515" phylum="Mollusca" rank="genus">
