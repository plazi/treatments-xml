<taxonomicName id="6A8416A9352A68077CC9FD46FD0C5A94" box="[469,679,692,717]" class="Polychaeta" family="Serpulidae" genus="Filograna" kingdom="Animalia" order="Sabellida" pageId="14" pageNumber="515" phylum="Annelida" rank="species" species="implexa">
<emphasis id="9FF0B138352A68077CC9FD46FD0C5A94" box="[469,679,692,717]" italics="true" pageId="14" pageNumber="515">Filograna implexa</emphasis>
