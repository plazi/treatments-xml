<taxonomicName id="BEC14D1CFFA8E057D604FC47FCDFFED3" authority="Ludwig &amp; Heding, 1935" authorityName="Ludwig &amp; Heding" authorityYear="1935" box="[283,874,961,987]" class="Holothuroidea" family="Cucumariidae" genus="Cucumaria" kingdom="Animalia" order="Dendrochirotida" pageId="9" pageNumber="10" phylum="Echinodermata" rank="species" species="bouvetensis">
