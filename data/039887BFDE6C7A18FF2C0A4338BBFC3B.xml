<taxonomicName id="4C314D2ADE6C7A19FDAC0F483932F9AB" authorityName="Mugrabi &amp; Azevedo" authorityYear="2016" box="[588,638,1623,1646]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="76" pageNumber="527" phylum="Arthropoda" rank="species" species="pepa">
