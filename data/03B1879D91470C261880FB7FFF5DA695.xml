<paragraph id="8BA7368B91470C2718D0F88AFD06A1B3" blockId="32.[151,1436,1845,2014]" pageId="32" pageNumber="33">
<materialsCitation id="3B703CD691470C2718D0F88AFD06A1B3" ID-GBIF-Occurrence="1585189557" country="Vietnam" location="West Africa" pageId="32" pageNumber="33" specimenCount="1" stateProvince="Cote">
