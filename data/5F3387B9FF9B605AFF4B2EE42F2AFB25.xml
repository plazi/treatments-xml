<taxonomicName id="109A4D2CFF99605AFD942FDB2883FB26" box="[584,698,1152,1177]" class="Insecta" family="Eriococcidae" genus="Alpinococcus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="elongatus">
