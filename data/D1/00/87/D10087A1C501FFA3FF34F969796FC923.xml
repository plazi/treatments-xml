<collectingCountry box="[1142,1244,301,327]" name="Pakistan" pageId="12" pageNumber="39">Pakistan</collectingCountry>
, 
<collectingCountry box="[1257,1330,301,327]" name="China" pageId="12" pageNumber="39">China</collectingCountry>
