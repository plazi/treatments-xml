<taxonomicName authority="Roxb." authorityName="Roxb." box="[1156,1436,906,932]" class="Magnoliopsida" family="Fabaceae" genus="Dalbergia" kingdom="Plantae" order="Fabales" pageId="11" pageNumber="38" phylum="Tracheophyta" rank="species" species="sissoo">
