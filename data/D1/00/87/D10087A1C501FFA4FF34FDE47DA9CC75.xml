<date box="[482,625,944,970]" pageId="11" pageNumber="38" value="2009-06-18">
<number box="[482,512,944,970]" pageId="11" pageNumber="38" value="18.0">18</number>
.VI. 
<number box="[556,625,944,970]" pageId="11" pageNumber="38" value="2009.0">2009</number>
</date>
