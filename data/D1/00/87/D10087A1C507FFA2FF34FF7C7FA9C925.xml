<collectingCountry box="[597,668,298,324]" name="Japan" pageId="13" pageNumber="40">Japan</collectingCountry>
, 
<collectingCountry box="[680,756,298,324]" name="South Korea" pageId="13" pageNumber="40">Korea</collectingCountry>
, 
<collectingCountry box="[769,843,298,324]" name="China" pageId="13" pageNumber="40">China</collectingCountry>
, 
<collectingCountry box="[856,930,298,324]" name="Nepal" pageId="13" pageNumber="40">Nepal</collectingCountry>
, 
<collectingCountry box="[943,1032,298,324]" name="Bhutan" pageId="13" pageNumber="40">Bhutan</collectingCountry>
, 
<collectingCountry box="[1044,1105,298,324]" name="India" pageId="13" pageNumber="40">India</collectingCountry>
and now 
<collectingCountry box="[1224,1325,298,324]" name="Pakistan" pageId="13" pageNumber="40">Pakistan</collectingCountry>
(
<bibRefCitation author="Metcalf" pageId="13" pageNumber="47" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i - vii, 1 - 585." type="book chapter" year="1963" yearSuffix="a">
