<number box="[568,605,189,215]" pageId="6" pageNumber="33" value="1.4">1.4</number>
(1.0– 
<number box="[679,717,189,215]" pageId="6" pageNumber="33" value="1.5">1.5</number>
); width of head including eyes: male 
<number box="[1172,1224,189,215]" pageId="6" pageNumber="33" value="6.56">6.56</number>
(6.0– 
<number box="[1296,1334,189,215]" pageId="6" pageNumber="33" value="7.3">7.3</number>
), female 
<number box="[151,206,226,252]" pageId="6" pageNumber="33" value="5.8">5.80</number>
(
<number box="[224,265,226,252]" pageId="6" pageNumber="33" value="5.5">5.5</number>
–6.0); width of pronotum including suprahumeral plates: male 
<number box="[1074,1129,226,252]" pageId="6" pageNumber="33" value="9.1">9.10</number>
(7.0– 
<number box="[1208,1265,226,252]" pageId="6" pageNumber="33" value="10.2">10.2</number>
), female 
<number box="[1382,1437,226,252]" pageId="6" pageNumber="33" value="9.2">9.20</number>
(8.0– 
<number box="[215,268,264,290]" pageId="6" pageNumber="33" value="10.5">10.5</number>
); width of mesonotum: male 
<number box="[618,669,264,290]" pageId="6" pageNumber="33" value="8.47">8.47</number>
