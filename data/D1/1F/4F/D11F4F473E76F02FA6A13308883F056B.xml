<taxonomicName authorityName="Solier" authorityYear="1851" class="Insecta" family="Scarabaeidae" genus="Catoclastus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Catoclastus chevrolatii" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="chevrolatii">
