<location LSID="urn:lsid:plazi:treatment:D176A054FFFDBD47FF1E2F04FC37FC86:5C004799FFFDBD46FDB62EE0FD78FA2B" box="[575,642,1502,1526]" municipality="Museum" name="These" pageId="22" pageNumber="23" stateProvince="Budapest">These</location>
