<typeStatus box="[348,435,1575,1599]" pageId="22" pageNumber="23">syntype</typeStatus>
<materialsCitation box="[442,766,1574,1598]" collectionCode="MNHN" pageId="22" pageNumber="23" specimenCount="1" stateProvince="Budapest" typeStatus="paralectotype">
in 
<collectionCode box="[471,559,1575,1598]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="22" pageNumber="23">MNHN</collectionCode>
, as 
