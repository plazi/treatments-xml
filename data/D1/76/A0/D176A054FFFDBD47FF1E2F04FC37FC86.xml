<typeStatus box="[1107,1194,1503,1527]" pageId="22" pageNumber="23">syntype</typeStatus>
(
<collectionCode box="[1210,1302,1502,1526]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="22" pageNumber="23">MNHN</collectionCode>
)
</materialsCitation>
