</materialsCitation>
<materialsCitation collectionCode="HNHM" pageId="22" pageNumber="23" specimenCount="1" specimenCount-male="1" stateProvince="Budapest" typeStatus="holotype">
of 
<taxonomicName authorityName=", Kaszab" authorityYear="1951" box="[1140,1317,1395,1419]" class="Insecta" family="Meloidae" genus="Cerocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="prochaskana">
