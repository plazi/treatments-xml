<taxonomicName authority="Pardo" authorityName="Pardo Alcaide" authorityYear="1977" box="[408,563,1928,1953]" class="Insecta" family="Meloidae" genus="Cerocoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="rapillyi">
