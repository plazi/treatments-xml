<taxonomicName authority="Soares &amp; Soares, 1947" authorityName="Soares &amp; Soares" authorityYear="1947" box="[151,770,1540,1566]" class="Arachnida" family="Arachnida" genus="Thrasychiroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Arachnida" pageId="8" pageNumber="477" phylum="Arthropoda" rank="species" species="brasilicus">
