<subSubSection pageId="58" pageNumber="83" type="diagnostic characters">
<paragraph pageId="58" pageNumber="83">Diagnostic characters.</paragraph>
<paragraph pageId="58" pageNumber="83">
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Okia" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="58" pageNumber="83" phylum="Tracheophyta" rank="species" species="birmanica">
