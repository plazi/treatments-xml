<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136290" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus meinerti Forel" lsidName-HNS="Camponotus meinerti Forel" order="Hymenoptera" pageNumber="18" phylum="Arthropoda" rank="species" species="meinerti">meinerti</taxonomicName>
