), 
<taxonomicName authority="Hong &amp; Inkhavilay &amp; James, 2018" authorityName="Hong &amp; Inkhavilay &amp; James" authorityYear="2018" box="[671,966,1970,1992]" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplotaxida" pageId="2" pageNumber="240" phylum="Annelida" rank="species" species="hoauykanangensis" status="sp. nov.">
