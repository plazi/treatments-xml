<caption id="2BEE6642E902964343F7F870FF23AF9E" ID-DOI="http://doi.org/10.5281/zenodo.4471524" ID-Zenodo-Dep="4471524" httpUri="https://zenodo.org/record/4471524/files/figure.png" pageId="33" pageNumber="34" startId="33.[151,250,1943,1967]" targetBox="[216,1370,193,1907]" targetPageId="33">
