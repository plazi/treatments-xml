<taxonomicName id="4C3647ABFF9FFFC49212464EFB1FFEA9" box="[1043,1177,330,355]" class="Insecta" family="Scirtidae" genus="Nothocyphon" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="335" phylum="Arthropoda" rank="species" species="amphora">
