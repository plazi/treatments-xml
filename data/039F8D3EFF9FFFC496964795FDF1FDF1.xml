<quantity id="4CCE91CDFF9FFFC4975F4648FE6FFEA8" box="[350,489,330,355]" metricMagnitude="-3" metricUnit="m" metricValue="2.5" metricValueMax="2.6" metricValueMin="2.4" pageId="34" pageNumber="335" unit="mm" value="2.5" valueMax="2.6" valueMin="2.4">2.4−2.6 mm</quantity>
