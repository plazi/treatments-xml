<caption id="DF2AFB97FFC7F77FFF02482A0D95F888" ID-DOI="http://doi.org/10.5281/zenodo.5352468" ID-Zenodo-Dep="5352468" httpUri="https://zenodo.org/record/5352468/files/figure.png" pageId="24" pageNumber="23" startId="24.[192,275,1864,1886]" targetBox="[324,1302,243,1759]" targetPageId="24">
