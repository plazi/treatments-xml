<caption id="DF1DD593FFB7CC41FFE9387CB4A4FA43" ID-DOI="http://doi.org/10.5281/zenodo.3652042" ID-Zenodo-Dep="3652042" httpUri="https://zenodo.org/record/3652042/files/figure.png" pageId="7" pageNumber="60" startId="7.[83,122,1392,1411]" targetBox="[132,982,158,1351]" targetPageId="7">
<paragraph id="8BDD851BFFB7CC41FFE9387CB4A4FA43" blockId="7.[83,1014,1392,1508]" pageId="7" pageNumber="60">
