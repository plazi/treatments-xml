<taxonomicName id="4C74E8EAFC130619FCF9F8ACA3E36233" authority="(Chamberlain &amp; Lamond) Thulin (2003: 217)" authorityName="Thulin" authorityPageNumber="217" authorityYear="2003" baseAuthorityName="Chamberlain &amp; Lamond" box="[877,1451,1825,1847]" class="Magnoliopsida" family="Cleomaceae" genus="Cleome" kingdom="Plantae" order="Brassicales" pageId="2" pageNumber="56" phylum="Tracheophyta" rank="species" species="omanensis">
