<quantity id="4C8C3E8CFC130619FB23F889A34E621E" box="[1207,1286,1796,1818]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="2" pageNumber="56" unit="mm" value="3.5" valueMax="4.0" valueMin="3.0">3–4 mm</quantity>
