<specimenCount id="9D5FFD692A1EFF80B3BBFB81FEDE2412" box="[212,295,1091,1118]" pageId="15" pageNumber="16" type="generic">
<quantity id="4CA19B052A1EFF80B3BBFB81FEDA2412" box="[212,291,1091,1118]" metricMagnitude="2" metricUnit="m" metricValue="5.5" pageId="15" pageNumber="16" unit="m" value="550.0">550 m</quantity>
