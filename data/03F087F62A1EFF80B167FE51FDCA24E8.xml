<quantity id="4CA19B052A1EFF80B3D2FB4BFEEE24E8" box="[189,279,1161,1188]" metricMagnitude="3" metricUnit="m" metricValue="5.492999999999999" pageId="15" pageNumber="16" unit="m" value="5493.0">5493 m</quantity>
