<taxonomicName id="167D76313D433661088A209019E1D120" class="Insecta" family="Ichneumonidae" genus="Campoletis" kingdom="Animalia" lsidName="Campoletis" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="genus">Campoletis</taxonomicName>
