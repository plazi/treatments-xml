<taxonomicName id="7EE386B39C482A02EFC15D20FE185E1A" authority="Navas, 1919" authorityName="Navas" authorityYear="1919" box="[151,414,799,826]" class="Insecta" family="Myrmeleontidae" genus="Synclisis" kingdom="Animalia" order="Neuroptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="genus">
