<paragraph id="911A36BB1548FFB7FF1CF8041179C7DE" blockId="5.[151,800,1885,1945]" box="[151,800,1920,1945]" pageId="5" pageNumber="550">
Syn. 
<taxonomicName id="56A54D381548FFB7FF5BF8041179C7DE" authority="Cobb, 1913 (Heyns, 1968)" authorityName="Cobb, 1913 (Heyns" authorityYear="1968" box="[208,800,1920,1945]" class="Adenophorea" family="Nygolaimidae" genus="Nygolaimus" kingdom="Animalia" order="Dorylaimida" pageId="5" pageNumber="550" phylum="Nematoda" rank="subGenus" subGenus="Paravulvus">
