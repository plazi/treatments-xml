<number id="3DEAB20FFF85FFC487094D1BFDD84005" box="[521,552,1622,1646]" pageId="12" pageNumber="191" value="17.0">17</number>
; nape scaled, median predorsal scales 
<number id="3DEAB20FFF85FFC486D64D1BFC0D4005" box="[982,1021,1622,1646]" pageId="12" pageNumber="191" value="8.5" valueMax="9.0" valueMin="8.0">8-9</number>
