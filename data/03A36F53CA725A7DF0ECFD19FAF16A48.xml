<taxonomicName id="4C0AA5C6CA725A7DF2D4FCDCFC8F6A1A" box="[687,867,789,814]" class="Anthozoa" family="Plexauridae" genus="Echinomuricea" kingdom="Animalia" order="Alcyonacea" pageId="19" pageNumber="20" phylum="Cnidaria" rank="genus">
