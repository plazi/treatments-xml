<taxonomicName id="1CD883F5FFE5FFBDFD8DB026FC5E816C" authority="Thunberg, 1784" authorityName="Thunberg" authorityYear="1784" box="[611,1005,1853,1877]" class="Insecta" family="Tortricidae" genus="Tortrix" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="osbeckiana">
