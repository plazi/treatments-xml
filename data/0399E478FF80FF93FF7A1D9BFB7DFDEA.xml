<taxonomicName id="4C302EEDFF8EFF92FE681CA1FD2AFA43" authority="(Flint)" authorityName="Blahnik &amp; Holzenthal" authorityYear="2008" baseAuthorityName="Flint" baseAuthorityYear="1974" box="[389,696,1492,1516]" class="Insecta" family="Glossosomatidae" genus="Mortoniella" kingdom="Animalia" order="Trichoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="pacuara">
