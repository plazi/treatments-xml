<taxonomicName id="88DB24C3284F9265D444B640FC3CF9C1" authority="Tu, Lelej et Chen" authorityName="Tu, Lelej et Chen" box="[563,1014,1600,1627]" class="Insecta" family="Mutillidae" genus="Hemutilla" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="76" phylum="Arthropoda" rank="species" species="granulata" status="sp. nov.">
