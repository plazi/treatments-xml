<materialsCitation id="85EE3CB0FFABF41AFE69F86EFE3DF817" collectingDate="1986-08-12" collectionCode="EME" collectorName="J. Brown &amp; J. Powell" country="Mexico" location="Male" pageId="55" pageNumber="56" specimenCount="1" stateProvince="Sinaloa" typeStatus="holotype">
<typeStatus id="EA3D884FFFABF41AFE69F86EFE1AF87D" box="[398,511,1933,1959]" pageId="55" pageNumber="56">Holotype</typeStatus>
: 
