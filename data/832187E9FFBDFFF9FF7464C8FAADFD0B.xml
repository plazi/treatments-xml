<taxonomicName id="CC884D7CFFB0FFF9FE5A63DCFD18FE56" authorityName="Bonaparte" authorityYear="1853" box="[441,721,476,501]" class="Aves" family="Laniidae" genus="Eurocephalus" kingdom="Animalia" order="Passeriformes" pageId="58" pageNumber="59" phylum="Chordata" rank="species" species="rueppelli">
