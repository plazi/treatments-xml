<emphasis id="39FCEAEDFFB3FFFAFF7465A2FE9FF81B" bold="true" box="[151,342,1954,1976]" pageId="57" pageNumber="58">FIGURES 77–82.</emphasis>
<taxonomicName id="CC884D7CFFB3FFFAFEBD65A2FDACF81B" authority="Bush, 2017" authorityName="Bush" authorityYear="2017" box="[350,613,1954,1976]" class="Insecta" family="Philopteridae" genus="Teinomordeus" kingdom="Animalia" order="Phthiraptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="entelosetus" status="n. gen.">
<emphasis id="39FCEAEDFFB3FFFAFEBD65A2FDACF81B" box="[350,613,1954,1976]" italics="true" pageId="57" pageNumber="58">Teinomordeus entelosetus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="22CF5796FFB3FFFAFD8D65A2FD71F81B" box="[622,696,1954,1976]" pageId="57" pageNumber="58" rank="genus">
