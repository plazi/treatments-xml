</taxonomicName>
; 
<taxonomicName id="F9C84D43FFC57C0FFDEDD6EFADD7DDAB" box="[567,710,1977,2001]" class="Malacostraca" family="Callianassidae" genus="Callianassa" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="232" phylum="Arthropoda" rank="species" species="wennerae">
<emphasis id="0CBCEAD2FFC57C0FFDEDD6EFADD7DDAB" box="[567,710,1977,2001]" italics="true" pageId="3" pageNumber="232">C. wennerae</emphasis>
