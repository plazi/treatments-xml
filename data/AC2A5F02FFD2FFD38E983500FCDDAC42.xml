<paragraph id="243CEE14FFD2FFD28E9835ABFE8FADBD" blockId="2.[151,1435,1950,2003]" pageId="2" pageNumber="255">
<taxonomicName id="E3839597FFD2FFD28E9835ABFDEFADDA" authority="Blanchard, 1846: 209" authorityName="Blanchard" authorityPageNumber="209" authorityYear="1846" box="[151,596,1950,1973]" class="Insecta" family="Cerambycidae" genus="Trypanidius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="255" phylum="Arthropoda" rank="species" species="andicola">
