<taxonomicName id="4C6CA1CF6640B438C56F4E313CDA240B" box="[1107,1216,1345,1370]" class="Insecta" family="Coccidae" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="family">Coccidae</taxonomicName>
