<taxonomicName id="4C6CA1CF6640B438C1DD4E1539DC242C" baseAuthorityName="Schrank" baseAuthorityYear="1801" box="[225,454,1381,1406]" class="Insecta" family="Coccidae" genus="Physokermes" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="piceae">
