<taxonomicName id="3C9AEBB6FFB9FF9DFED26E1EFCD58675" box="[343,495,333,356]" class="Actinopterygii" family="Cyprinidae" genus="Metzia" kingdom="Animalia" order="Cypriniformes" pageId="5" pageNumber="231" phylum="Chordata" rank="species" species="parva">
