<taxonomicName id="4C454D7E1D32FFFCFD81FE2AFCA5FE52" authorityName="Madl" authorityYear="2018" box="[583,774,489,511]" class="Insecta" family="Chrysididae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="914" phylum="Arthropoda" rank="species" species="communis">
