<collectingRegion id="5B28F83DFFACFFD0FBF8A3B2FB59F98F" box="[1043,1207,1518,1543]" country="Brazil" name="Espirito Santo" pageId="51" pageNumber="52">Espírito Santo</collectingRegion>
