<taxonomicName id="4C744D5DFF90731C1B0FFC60FABF9499" authority="Troughton, 1932 b" authorityName="Troughton" authorityYear="1932" box="[829,1386,906,933]" class="Mammalia" family="Thylacomyidae" genus="Macrotis" kingdom="Animalia" order="Peramelemorphia" pageId="41" pageNumber="318" phylum="Chordata" rank="subSpecies" species="lagotis" subSpecies="interjecta">
<emphasis id="B900EACCFF90731C1B0FFC60FABF9499" bold="true" box="[829,1386,906,933]" pageId="41" pageNumber="318">
<emphasis id="B900EACCFF90731C1B0FFC60FB549499" bold="true" box="[829,1153,906,933]" italics="true" pageId="41" pageNumber="318">Macrotis lagotis interjecta</emphasis>
<bibRefCitation id="EFE54B2FFF90731C1CBAFC60FABF9499" author="Troughton, E" box="[1160,1386,907,933]" pageId="41" pageNumber="318" pagination="219 - 235" refId="ref147202" refString="Troughton, E. L. G. 1932 b. A revision of the rabbit-bandicoots. Australian Zoologist 7 (3): 219 - 235." type="journal article" year="1932">Troughton, 1932b</bibRefCitation>
</emphasis>
