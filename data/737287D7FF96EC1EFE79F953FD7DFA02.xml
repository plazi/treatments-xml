<emphasis id="C9AFEAD3FF89EC1EFBD6FBC4FB8AFB49" box="[1104,1119,1148,1173]" italics="true" pageId="48" pageNumber="49">a</emphasis>
<emphasis id="C9AFEAD3FF89EC1EFBD8FB35FBB2FB47" box="[1118,1127,1165,1179]" italics="true" pageId="48" pageNumber="49">
<subScript id="675F3484FF89EC1EFBD8FB35FBB2FB47" attach="left" box="[1118,1127,1165,1179]" fontSize="6" pageId="48" pageNumber="49">1</subScript>
</emphasis>
setae.
