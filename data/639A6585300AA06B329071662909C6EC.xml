<normalizedToken id="7BD2CB3F80DA17C1AFD364F256D96246" originalValue="Blätter" pageId="null" pageNumber="332">Blaetter</normalizedToken>
nicht 
<normalizedToken id="691FB43366FB4D69CB22B174C61B2D57" originalValue="überragend" pageId="null" pageNumber="332">ueberragend</normalizedToken>
; 
<normalizedToken id="138FCC02E85E02BEAD2BFB50C23E846D" originalValue="Blütenstand" pageId="null" pageNumber="332">Bluetenstand</normalizedToken>
