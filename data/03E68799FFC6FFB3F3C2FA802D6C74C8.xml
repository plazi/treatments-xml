<taxonomicName id="4C4F4D0CFFC5FFB3F21DFF402FE274EB" authorityName="Zelazny &amp; Webb" authorityYear="2011" baseAuthorityName="Fennah" baseAuthorityYear="1956" box="[328,490,188,212]" class="Insecta" family="Derbidae" genus="Levu" kingdom="Animalia" order="Hemiptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="lactineus">
