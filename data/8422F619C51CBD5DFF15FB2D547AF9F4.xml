<taxonomicName id="CB8B3C8CC51CBD5DFCD4FA8752BDFAF0" authorityName="Lepeletier" authorityYear="1841" box="[854,981,1323,1349]" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="243" phylum="Arthropoda" rank="species" species="obsoleta">
