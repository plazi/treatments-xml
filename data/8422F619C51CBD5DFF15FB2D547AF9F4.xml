<taxonomicName id="CB8B3C8CC51CBD5DFBA5FA1751A7FA4C" authority="Friese, 1901" authorityName="Friese" authorityYear="1901" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="243" phylum="Arthropoda" rank="subSpecies" species="obsoleta" subSpecies="pleuralis">
