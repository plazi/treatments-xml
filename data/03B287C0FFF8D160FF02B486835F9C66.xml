<taxonomicName id="4C1B4D55FFF8D160FC76B2F9865E9FC6" authorityName="Zhu &amp; Ono" authorityYear="2007" box="[995,1141,1413,1438]" class="Arachnida" family="Thomisidae" genus="Philodamia" kingdom="Animalia" order="Araneae" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="pingxiang">
<emphasis id="B96FEAC4FFF8D160FC76B2F9865E9FC6" box="[995,1141,1413,1438]" italics="true" pageId="53" pageNumber="54">P. pingxiang</emphasis>
