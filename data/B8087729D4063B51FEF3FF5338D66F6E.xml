<typeStatus id="EF1A789DD4063B52FFB1FEC239E26979" box="[121,177,329,353]" pageId="1" pageNumber="268">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="F7A1BDBCD4063B52FEC5FEC13B566979" authority="Plant, 2011" authorityName="Plant" authorityYear="2011" box="[269,517,330,353]" class="Insecta" family="Empididae" genus="Sororsenexa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="268" phylum="Arthropoda" rank="species" species="macalpinei" status="n.sp.">
<emphasis id="02D51A2DD4063B52FEC5FEC13B566979" box="[269,517,330,353]" italics="true" pageId="1" pageNumber="268">Sororsenexa macalpinei</emphasis>
