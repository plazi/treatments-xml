with 
<taxonomicName id="90EA72D0FFBEF03BFB9CFF3602D1FF49" authority="Trautmann, 1926" authorityName="* Trautmann" authorityYear="1926" box="[1078,1432,151,177]" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="caucasium">
<emphasis id="659ED541FFBEF03BFB9CFF360399FF48" box="[1078,1232,151,176]" italics="true" pageId="34" pageNumber="35">H. caucasium</emphasis>
<bibRefCitation id="337B74A2FFBEF03BFB7CFF3602D1FF49" author="Trautmann, W." box="[1238,1432,151,177]" pageId="34" pageNumber="35" pagination="4 - 12" refId="ref42391" refString="Trautmann, W. (1926) Untersuchungen an einigen Goldwespenformen. Entomologische Zeitschrift, 40, 4 - 12." type="journal article" year="1926">Trautmann, 1926</bibRefCitation>
