only species of 
<taxonomicName id="20899DA634213A08CF55FE7545E4FAB2" authorityName="Schott" authorityYear="1925" box="[840,982,438,463]" class="Entognatha" family="Paronellidae" genus="Paronellides" kingdom="Animalia" order="Collembola" pageId="10" pageNumber="161" phylum="Arthropoda" rank="genus">
