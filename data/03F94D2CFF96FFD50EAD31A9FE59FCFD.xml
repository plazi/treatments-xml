<quantity id="4CA851DFFF96FFD60DF43303FBCAF97C" box="[974,1126,1659,1686]" metricMagnitude="-2" metricUnit="m" metricValue="1.1099999999999999" metricValueMax="1.25" metricValueMin="0.97" pageId="33" pageNumber="234" unit="mm" value="11.1" valueMax="12.5" valueMin="9.7">9.7–12.5 mm</quantity>
