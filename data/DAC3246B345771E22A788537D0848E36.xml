<taxonomicName id="480C48976D0BD5B07DC8B8D45927EDFF" class="Arachnida" family="Anapidae" genus="Anapisona" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="kethleyi">Anapisona kethleyi</taxonomicName>
