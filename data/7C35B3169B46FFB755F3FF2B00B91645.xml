<taxonomicName id="339C79839B46FFB65449FEF400B01620" authority="Curtis, 1833" authorityName="Curtis" authorityYear="1833" box="[301,725,312,335]" class="Insecta" family="Endomychidae" genus="Holoparamecus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="depressus">
