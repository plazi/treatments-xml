<collectingRegion id="49C6F83BFF8DE306FBBE502BFB24FC84" box="[1114,1220,1905,1931]" country="Argentina" name="Misiones" pageId="67" pageNumber="68">Misiones</collectingRegion>
, 
<collectingRegion id="49C6F83BFF8DE306FB2B502BFAD0FC83" box="[1231,1328,1905,1932]" country="Argentina" name="Santa Fe" pageId="67" pageNumber="68">Santa Fe</collectingRegion>
and 
<collectingRegion id="49C6F83BFF8DE306FA825028FEE1FCA0" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="67" pageNumber="68">Buenos Aires</collectingRegion>
