° 
<number id="3DF16204C51B966BA708FBB4FDE17467" box="[571,601,1061,1087]" pageId="3" pageNumber="4" value="17.0">17</number>
’ 
<number id="3DF16204C51B966BA76CFBB4FD217467" box="[607,665,1061,1087]" pageId="3" pageNumber="4" value="17.9">17.9</number>
” N
</geoCoordinate>
, 
<geoCoordinate id="EE0D16E4C51B966BA7E0FBB4FC337467" box="[723,907,1061,1087]" direction="west" orientation="longitude" pageId="3" pageNumber="4" precision="1" value="-82.32775">
<number id="3DF16204C51B966BA7E0FBB4FD4B7467" box="[723,755,1061,1087]" pageId="3" pageNumber="4" value="82.0">82</number>
