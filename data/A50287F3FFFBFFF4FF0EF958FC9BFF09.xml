, 
<collectingRegion id="EF6FF807FFFBFFF5FD3DF830FCE4F869" box="[676,769,1974,1999]" country="Guinea-Bissau" name="Quinara" pageId="22" pageNumber="23">Quinara</collectingRegion>
, 
<collectingRegion id="EF6FF807FFFBFFF5FC8DF830FC90F869" box="[788,885,1974,1999]" country="Guinea-Bissau" name="Tombali" pageId="22" pageNumber="23">Tombali</collectingRegion>
</collectingRegion>
