, prope 
<location id="8EA02BC4FF80FFE067E3FC36FAE5FC37" LSID="urn:lsid:plazi:treatment:03D6CC09FF80FFE06384FF07FE38FBCE:8EA02BC4FF80FFE067E3FC36FAE5FC37" box="[1263,1351,965,991]" country="Turkmenistan" municipality="Regio" name="Koosen" pageId="1" pageNumber="290" stateProvince="Ashgabat">Koosen</location>
, 
<date id="FFC15BDFFF80FFE06658FC37FF61FBEA" pageId="1" pageNumber="290" value="1900-05-19">
<collectingDate id="EF85A237FF80FFE06658FC37FF61FBEA" pageId="1" pageNumber="290" value="1900-05-19">19 May 1900</collectingDate>
</date>
, leg. 
<emphasis id="B90BA10DFF80FFE063F1FC1BFE24FBEA" box="[253,390,1000,1026]" italics="true" pageId="1" pageNumber="290">
<collectorName id="268A18C9FF80FFE063F1FC1BFEF4FBEA" box="[253,342,1000,1026]" pageId="1" pageNumber="290">Sintenis</collectorName>
295
</emphasis>
, det. 
<emphasis id="B90BA10DFF80FFE062CFFC1AFDA7FBEA" box="[451,517,1001,1026]" italics="true" pageId="1" pageNumber="290">
<determinerName id="7E1C91DBFF80FFE062CFFC1AFDA7FBEA" box="[451,517,1001,1026]" pageId="1" pageNumber="290">Freyn</determinerName>
