), L of IV-L- 
<number id="3DFBFA367C7B560FADE85C74FE381F4A" box="[436,472,1629,1655]" pageId="1" pageNumber="2" value="4.0" valueMax="6.0" valueMin="2.0">2-6</number>
: 
<number id="3DFBFA367C7B560FADBF5C74FDE01F4A" box="[483,512,1629,1655]" pageId="1" pageNumber="2" value="38.0">38</number>
(
<number id="3DFBFA367C7B560FAE4D5C74FDD11F4A" box="[529,561,1629,1655]" pageId="1" pageNumber="2" value="35.0">35</number>
), 
<number id="3DFBFA367C7B560FAE1B5C74FD841F4A" box="[583,612,1629,1655]" pageId="1" pageNumber="2" value="61.0">61</number>
(
<number id="3DFBFA367C7B560FAE295C74FD751F4A" box="[629,661,1629,1655]" pageId="1" pageNumber="2" value="63.0">63</number>
), 
<number id="3DFBFA367C7B560FAEF75C74FD281F4A" box="[683,712,1629,1655]" pageId="1" pageNumber="2" value="87.0">87</number>
(
<number id="3DFBFA367C7B560FAE855C74FD1A1F4A" box="[729,762,1629,1655]" pageId="1" pageNumber="2" value="80.0">80</number>
), 
<number id="3DFBFA367C7B560FAF535C74FCDB1F4A" box="[783,827,1629,1655]" pageId="1" pageNumber="2" value="104.0">104</number>
(
<number id="3DFBFA367C7B560FAF105C74FC8D1F4A" box="[844,877,1629,1655]" pageId="1" pageNumber="2" value="96.0">96</number>
), 
<number id="3DFBFA367C7B560FAFDE5C74FC7F1F4A" box="[898,927,1629,1655]" pageId="1" pageNumber="2" value="99.0">99</number>
(
<number id="3DFBFA367C7B560FAFEC5C74FC321F4A" box="[944,978,1629,1655]" pageId="1" pageNumber="2" value="99.0">99</number>
