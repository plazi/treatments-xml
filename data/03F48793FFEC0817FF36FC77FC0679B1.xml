<taxonomicName id="4C5D4D06FFEB0817FEB8F883FE1C7928" authority="Ardila-Camacho &amp; Cancino-López &amp; Acevedo &amp; Contreras-Ramos, 2019" authorityName="Ardila-Camacho &amp; Cancino-López &amp; Acevedo &amp; Contreras-Ramos" authorityYear="2019" box="[281,425,1872,1896]" class="Insecta" family="Mantispidae" genus="Plega" kingdom="Animalia" order="Neuroptera" pageId="5" pageNumber="356" phylum="Arthropoda" rank="species" species="mixteca" status="sp. n.">
