(FMNH-INS 
<number id="DF77B805FF83083122988FA22558B6D5" box="[709,783,1160,1184]" pageId="19" pageNumber="47" value="28926.0">28926</number>
), ex 
<taxonomicName id="AEBFD1A1FF830831231A8FA22243B6D5" box="[839,1044,1160,1184]" class="Aves" family="Icteridae" genus="Cacicus" kingdom="Animalia" order="Passeriformes" pageId="19" pageNumber="47" phylum="Chordata" rank="species" species="solitarius">
