<taxonomicName id="BDEE9CB717CA5405626295BDDABF4B70" class="Liliopsida" family="Iridaceae" genus="Crocus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Crocus chrysanthus" order="Asparagales" pageId="0" pageNumber="1014" phylum="Tracheophyta" rank="species" species="chrysanthus">Crocus chrysanthus</taxonomicName>
