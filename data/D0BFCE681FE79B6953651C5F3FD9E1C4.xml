<taxonomicName id="90FB41547E2B78FA58B938DA8B5CA28A" class="Insecta" family="Carabidae" genus="Bembidium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bembidium ephippiatum" order="Coleoptera" pageId="27" pageNumber="114" phylum="Arthropoda" rank="species" species="ephippiatum">Bembidium ephippiatum</taxonomicName>
