<taxonomicName id="7E9E9BE9FFF1FC34FD20FEBAFCAA6124" authorityName="Leach" authorityYear="1815" box="[727,828,333,359]" class="Insecta" family="Cossidae" kingdom="Animalia" order="Lepidoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="family">Cossidae</taxonomicName>
