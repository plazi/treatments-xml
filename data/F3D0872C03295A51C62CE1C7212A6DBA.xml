<geoCoordinate id="8FB9CB4D1D7FABE6FE4227CAD8A302DE" direction="south" orientation="latitude" precision="1" value="-3.0021334">
3°0
<normalizedToken id="858BB01805014B5524C67CD3E67B856C" originalValue="’7.68”">'7.68&quot;</normalizedToken>
S
</geoCoordinate>
, 
<geoCoordinate id="C8452D652ED3818A9BFE66BAC8100035" direction="west" orientation="longitude" precision="1" value="-69.04709">
69°2
