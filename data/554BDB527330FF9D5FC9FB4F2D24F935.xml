<taxonomicName id="1AE211C77333FF9D5AD9FB722846FB66" baseAuthorityName="Yamaguti &amp; Yamasu" baseAuthorityYear="1960" class="Maxillopoda" family="Lernanthropidae" genus="Sagum" kingdom="Animalia" order="Siphonostomatoida" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="epinepheli">
