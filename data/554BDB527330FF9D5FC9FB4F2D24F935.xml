<taxonomicName id="1AE211C77333FF9D5D47FB722D6DFB5A" authority="(Temminck, 1842) (Ho et al., 2011)" baseAuthorityName="Temminck" baseAuthorityYear="1842" box="[537,1063,1203,1230]" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="82" pageNumber="83" phylum="Chordata" rank="species" species="awoara">
