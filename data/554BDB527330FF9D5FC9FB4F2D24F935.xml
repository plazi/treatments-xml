<taxonomicName id="1AE211C77333FF9D5EBDFB3A2B0DFA82" authorityName="Bloch" authorityYear="1793" box="[483,583,1276,1301]" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="82" pageNumber="83" phylum="Chordata" rank="species" species="merra">
<emphasis id="EF96B6567333FF9D5EBDFB3A2B0DFA82" box="[483,583,1276,1301]" italics="true" pageId="82" pageNumber="83">E. merra</emphasis>
</taxonomicName>
