<taxonomicName id="1AE211C77333FF9D5B5EFB1E2CD2FB66" authority="(Bloch, 1790)" baseAuthorityName="Bloch" baseAuthorityYear="1790" box="[1024,1432,1239,1265]" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="82" pageNumber="83" phylum="Chordata" rank="species" species="coeruleopunctatus">
<emphasis id="EF96B6567333FF9D5B5EFB1E2DBAFB66" box="[1024,1264,1239,1265]" italics="true" pageId="82" pageNumber="83">E. coeruleopunctatus</emphasis>
