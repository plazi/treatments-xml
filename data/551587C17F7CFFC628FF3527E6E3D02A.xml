<taxonomicName id="1ABC4D547F7EFFC629BF30CBE60CD192" authorityName="Ito &amp; Nozaki" authorityYear="2018" box="[471,524,224,248]" class="Insecta" family="Hydropsychidae" genus="Diplectrona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="551" phylum="Arthropoda" rank="species" species="uken">
