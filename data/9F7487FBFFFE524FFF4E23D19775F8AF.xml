<taxonomicName id="D0DD4D6EFFF1524FFC3B26F59037F8F7" box="[994,1148,404,428]" class="Insecta" family="Ceratopogonidae" genus="Parabezzia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="399" phylum="Arthropoda" rank="species" species="caribbeana">
<emphasis id="25A9EAFFFFF1524FFC3B26F59037F8F7" box="[994,1148,404,428]" italics="true" pageId="9" pageNumber="399">P. caribbeana</emphasis>
</taxonomicName>
, on mid tarsomere 
<quantity id="D0259B08FFF1524FFA8F26F491CBF8F6" box="[1366,1408,404,429]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="9" pageNumber="399" unit="in" value="5.0">5 in</quantity>
<taxonomicName id="D0DD4D6EFFF1524FFA5E26F5949EF894" class="Insecta" family="Ceratopogonidae" genus="Parabezzia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="399" phylum="Arthropoda" rank="species" species="unica" status="sp. nov.">
<emphasis id="25A9EAFFFFF1524FFA5E26F5949EF894" italics="true" pageId="9" pageNumber="399">P. unica</emphasis>
</taxonomicName>
<taxonomicNameLabel id="3E9A5784FFF1524FFF0726D79572F88B" box="[222,313,439,464]" pageId="9" pageNumber="399" rank="species">
<emphasis id="25A9EAFFFFF1524FFF0726D79572F88B" bold="true" box="[222,313,439,464]" pageId="9" pageNumber="399">sp. nov.</emphasis>
</taxonomicNameLabel>
, on all tarsomeres in 
<taxonomicName id="D0DD4D6EFFF1524FFDED26D996E5F88B" box="[564,686,440,464]" class="Insecta" family="Ceratopogonidae" genus="Parabezzia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="399" phylum="Arthropoda" rank="species" species="brunnea">
