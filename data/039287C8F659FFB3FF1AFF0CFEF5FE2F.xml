<taxonomicName id="4C3B4D5DF659FFB2FE96F9E9FE16F90B" box="[283,506,1650,1672]" class="Insecta" family="Rhagionidae" genus="Litoleptis" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="49" phylum="Arthropoda" rank="species" species="niyodoensis" status="sp. n.">
<emphasis id="B94FEACCF659FFB2FE96F9E9FE16F90B" box="[283,506,1650,1672]" italics="true" pageId="8" pageNumber="49">Litoleptis niyodoensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27C57B7F659FFB2FD8CF9E9FDD7F904" box="[513,571,1650,1671]" pageId="8" pageNumber="49" rank="species">
