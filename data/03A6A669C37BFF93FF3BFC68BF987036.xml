<collectionCode id="ED1E8FBAC379FF93FD74FF39BEEA707E" box="[711,748,231,257]" country="Australia" lsid="urn:lsid:biocol.org:col:14575" name="University of New England" pageId="14" pageNumber="21" type="Herbarium">NE</collectionCode>
