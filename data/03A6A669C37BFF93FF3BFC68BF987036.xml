<emphasis id="B97BCB6DC379FF93FF7DFED5BD39705B" box="[206,319,266,292]" italics="true" pageId="14" pageNumber="21">
<collectionCode id="ED1E8FBAC379FF93FF7DFED5BCE4705B" box="[206,226,267,292]" country="Finland" lsid="urn:lsid:biocol.org:col:15618" name="University of Helsinki" pageId="14" pageNumber="21" type="Herbarium">H</collectionCode>
