<paragraph id="172BB46609C0DAD07858FA66217DE90E" pageId="61" pageNumber="62">
<taxonomicName id="CDF365851F17A1068ADBC5D3C91B7D84" authority="Berlese, 1885" authorityName="Berlese" authorityYear="1885" class="Arachnida" family="Lohmanniidae" genus="Angelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" phylum="Arthropoda" rank="species" species="palustris">Angelia palustris</taxonomicName>
