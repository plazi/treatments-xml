<subSubSection id="F5E13566CCA2032D81F8900BADF61772" type="reference_group">
<paragraph id="1632C5D5E13894BB96FACDAA605B29F3" pageId="61" pageNumber="62">
<taxonomicName id="D64FF1E91077E45CB6932BE6B52CDAD7" class="Arachnida" family="Nothridae" genus="Nothrus" kingdom="Animalia" order="Sarcoptiformes" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="palustris">Nothrus palustris C. L. Koch</taxonomicName>
