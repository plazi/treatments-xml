'' S
</geoCoordinate>
, 
<geoCoordinate id="94E964BFFFA70943B03EB0BCC17BEABD" box="[339,514,1192,1218]" direction="west" orientation="longitude" pageId="13" pageNumber="14" precision="1" value="-72.58192">
<number id="4715105FFFA70943B03EB0BCC20BEABD" box="[339,370,1192,1218]" pageId="13" pageNumber="14" value="72.0">72</number>
° 
