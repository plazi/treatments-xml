(
<taxonomicName id="09A803A0FF82F97E5961FAC4F6ACFA30" box="[578,755,1392,1414]" class="Malacostraca" family="Palaeocorystidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="18" pageNumber="19" phylum="Arthropoda" rank="family">Palaeocorystidae</taxonomicName>
