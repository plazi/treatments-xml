<quantity id="4C839B41FFB2FFDDFB4CF9D3FB5BF99D" box="[1047,1179,1591,1615]" metricMagnitude="3" metricUnit="m" metricValue="1.1" metricValueMax="1.3" metricValueMin="0.9" pageId="40" pageNumber="129" unit="m" value="1100.0" valueMax="1300.0" valueMin="900.0">
<elevation id="0056D197FFB2FFDDFB4CF9D3FB5BF99D" box="[1047,1179,1591,1615]" metricMagnitude="3" metricUnit="m" metricValue="1.1" metricValueMax="1.3" metricValueMin="0.9" pageId="40" pageNumber="129" unit="m" value="1100.0" valueMax="1300.0" valueMin="900.0">900-1300 m</elevation>
</quantity>
