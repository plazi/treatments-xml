</taxonomicName>
<materialsCitation id="3B133CF9FFB2FFDCFE29FA1EFEABFACD" country="China" county="Fujian" elevation="1100" lastPageId="41" lastPageNumber="130" location="Oriental" municipality="The" pageId="40" pageNumber="129" specimenCount="570" stateProvince="Fujian and Yunnan">
