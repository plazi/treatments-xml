<collectingCountry id="F36C7634FFB2FFDDFC92FA1EFBCEF9C0" box="[969,1038,1530,1554]" name="China" pageId="40" pageNumber="129">China</collectingCountry>
), 
<collectingCountry id="F36C7634FFB2FFDDFB78FA1EFB58F9C0" box="[1059,1176,1530,1554]" name="Hong Kong" pageId="40" pageNumber="129">Hongkong</collectingCountry>
