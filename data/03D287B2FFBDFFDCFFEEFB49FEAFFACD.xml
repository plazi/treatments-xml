<elevation id="0056D197FFB3FFDCFB52FB54FF00FB37" metricMagnitude="3" metricUnit="m" metricValue="1.93" metricValueMax="2.16" metricValueMin="1.7" pageId="41" pageNumber="130" unit="m" value="1930.0" valueMax="2160.0" valueMin="1700.0">1700- 2160 m</elevation>
