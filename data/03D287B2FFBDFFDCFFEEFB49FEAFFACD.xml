<quantity id="4C839B41FFB3FFDCFE59FB08FE8AFAD6" box="[258,330,1260,1284]" metricMagnitude="2" metricUnit="m" metricValue="5.7" pageId="41" pageNumber="130" unit="m" value="570.0">
<elevation id="0056D197FFB3FFDCFE59FB08FE8AFAD6" box="[258,330,1260,1284]" metricMagnitude="2" metricUnit="m" metricValue="5.7" pageId="41" pageNumber="130" unit="m" value="570.0">570 m</elevation>
