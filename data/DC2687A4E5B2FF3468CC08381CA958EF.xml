(
<taxonomicName id="938F4D31E5B3FF2B6A560ACD1FA15DA1" authority="Howard, 1908" authorityName="Howard" authorityYear="1908" box="[525,694,1931,1953]" class="Insecta" family="Mymaridae" genus="Cosmocomoidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="genus">
