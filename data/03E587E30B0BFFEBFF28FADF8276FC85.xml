<taxonomicName id="4C4C4D760B05FFEBFCC4FCE88271FC8A" box="[891,1024,799,824]" class="Insecta" family="Scirtidae" genus="Ora" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="77" phylum="Arthropoda" rank="species" species="depressa">
