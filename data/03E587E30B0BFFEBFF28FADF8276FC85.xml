<taxonomicName id="4C4C4D760B05FFEBFEB2FD3587E5FCA7" box="[269,404,764,789]" class="Insecta" family="Scirtidae" genus="Ora" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="77" phylum="Arthropoda" rank="species" species="depressa">
