<subSubSection id="C333B3C6F253D77CE7A8FE3BFD63FE3B" box="[151,655,472,499]" pageId="1" pageNumber="428" type="nomenclature">
<paragraph id="8B96E04DF253D77CE7A8FE3BFD63FE3B" blockId="1.[151,655,472,499]" box="[151,655,472,499]" pageId="1" pageNumber="428">
<heading id="D0DE5721F253D77CE7A8FE3BFD63FE3B" bold="true" box="[151,655,472,499]" fontSize="11" level="1" pageId="1" pageNumber="428" reason="1">
<emphasis id="B95D3C5FF253D77CE7A8FE3BFD63FE3B" bold="true" box="[151,655,472,499]" pageId="1" pageNumber="428">
<taxonomicName id="4C299BCEF253D77CE7A8FE3BFDD3FE3B" authority="Harbach" authorityName="Harbach &amp; Taai" authorityYear="2014" box="[151,575,472,499]" class="Insecta" family="Culicidae" genus="Nyctomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="428" phylum="Arthropoda" rank="species" species="biunguiculata" status="sp. n.">
<emphasis id="B95D3C5FF253D77CE7A8FE3BFE24FE3A" bold="true" box="[151,456,472,498]" italics="true" pageId="1" pageNumber="428">Nyctomyia biunguiculata</emphasis>
Harbach
</taxonomicName>
, 
<taxonomicNameLabel id="A26E8124F253D77CE573FE3AFD63FE3B" box="[588,655,473,499]" pageId="1" pageNumber="428" rank="species">sp. n.</taxonomicNameLabel>
