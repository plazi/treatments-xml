<taxonomicName id="4C784D597A45FFC3FEDDF922FDC9F950" authorityName="Otto &amp; Hill" authorityYear="2016" box="[374,523,1761,1786]" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="fimbriatus">
