<taxonomicName id="4C774D69FFE33531FE82FD2CFD29BBB1" authority="Blanchard, 1845" authorityName="Blanchard" authorityYear="1845" box="[306,682,661,688]" class="Insecta" family="Phalangopsidae" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="743" phylum="Arthropoda" rank="family">PHALANGOPSIDAE Blanchard, 1845</taxonomicName>
