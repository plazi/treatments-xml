<taxonomicName id="4C724D79FF97A577FEC5FE29B375E5B7" authority="Urban 1972" authorityName="Urban" authorityYear="1972" box="[278,570,422,448]" class="Magnoliopsida" family="Rosaceae" genus="Argentina" kingdom="Plantae" order="Rosales" pageId="10" pageNumber="26" phylum="Tracheophyta" rank="genus">
<collectingCountry id="F365766AFF97A577FEC5FE29B0C3E5B7" box="[278,396,422,448]" name="Argentina" pageId="10" pageNumber="26">Argentina</collectingCountry>
(
<bibRefCitation id="EFE34B0BFF97A577FE4FFE29B37EE5B7" author="Urban" box="[412,561,422,448]" pageId="10" pageNumber="30" refString="Urban, D. (1972) As especies de Svastrides Michener, LaBerge e Moure, 1955 (Hymenoptera, Apoidea). Revista Brasileira de Biologia, 32, 485 - 498." type="journal article" year="1972">
Urban 
<number id="3DBA24DDFF97A577FE22FE29B37EE5B7" box="[497,561,422,448]" pageId="10" pageNumber="26" value="1972.0">1972</number>
</bibRefCitation>
)
