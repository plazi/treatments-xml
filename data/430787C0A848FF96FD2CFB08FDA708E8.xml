<paragraph id="CB1136D6A848FF97FD0EFA97FC400CBF" blockId="80.[667,1193,1291,1445]" pageId="80" pageNumber="80">
<treatmentCitationGroup id="EBBE11F8A848FF97FD0EFA97FC400CBF" pageId="80" pageNumber="80">
Now 
<taxonomicName id="0CAE4D55A848FF97FD41FA94FC770C4F" authorityName="Lafresnaye" authorityYear="1837" box="[724,970,1345,1365]" class="Aves" family="Thraupidae" genus="Ramphocelus" kingdom="Animalia" order="Passeriformes" pageId="80" pageNumber="80" phylum="Chordata" rank="species" species="dimidiatus">
<emphasis id="F9DAEAC4A848FF97FD41FA94FC770C4F" box="[724,970,1345,1365]" italics="true" pageId="80" pageNumber="80">Ramphocelus dimidiatus</emphasis>
</taxonomicName>
× 
<taxonomicName id="0CAE4D55A848FF97FC7EFA97FBC40C4E" authorityName="Bonaparte" authorityYear="1838" box="[1003,1145,1346,1364]" class="Aves" family="Thraupidae" genus="Ramphocelus" kingdom="Animalia" order="Passeriformes" pageId="80" pageNumber="80" phylum="Chordata" rank="species" species="icteronotus">
