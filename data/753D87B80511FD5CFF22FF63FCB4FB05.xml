<taxonomicName id="3A944D2D0511FD53FEA8F93CFD03F979" authority="Montgomery, 1931" authorityName="Montgomery" authorityYear="1931" box="[285,717,1734,1757]" class="Malacostraca" family="Hymenosomatidae" genus="Lucascinus" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="503" phylum="Arthropoda" rank="species" species="bedfordi">
