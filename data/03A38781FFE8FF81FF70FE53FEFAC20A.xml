<taxonomicName id="4C0A4D14FFE8FF81FF70FE53FF07C7EB" box="[151,210,440,466]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="21" phylum="Arthropoda" rank="species" species="lata">Lata</taxonomicName>
