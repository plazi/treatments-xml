<taxonomicName id="28D605B5FFE7FFCCFE53FD8AFD76FDCF" authority="Fabricius, 1787" authorityName="Fabricius" authorityYear="1787" box="[431,712,620,639]" class="Insecta" family="Chrysomelidae" genus="Cassida" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="336" phylum="Arthropoda" rank="species" species="palliata">
<emphasis id="DDA2A224FFE7FFCCFE53FD8AFD85FDCF" box="[431,571,620,639]" italics="true" pageId="61" pageNumber="336">Cassida palliata</emphasis>
Fabricius, 1787
</taxonomicName>
, by monotypy.
</paragraph>
<paragraph id="EF697E36FFE7FFCCFF8DFD60FE82FD04" blockId="61.[113,1089,593,692]" pageId="61" pageNumber="336">
<treatmentCitationGroup id="CFC65918FFE7FFCCFF8DFD60FE82FD04" pageId="61" pageNumber="336">
<treatmentCitation id="6E775827FFE7FFCCFF8DFD60FEEBFD29" author="SPAETH F." box="[113,341,646,665]" page="129" pageId="61" pageNumber="336" year="1913">
