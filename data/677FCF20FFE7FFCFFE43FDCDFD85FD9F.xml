Chevrolat, 
<quantity id="282ED3D3FFE7FFCCFEC9FDB7FEDEFDD4" box="[309,352,593,612]" metricMagnitude="1" metricUnit="m" metricValue="4.65582" pageId="61" pageNumber="336" unit="in" value="1833.0">1833</quantity>
