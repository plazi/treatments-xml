species: 
<taxonomicName id="28D605B5FFE7FFCCFD62FD60FC00FD29" authority="Fabricius, 1787" authorityName="Fabricius" authorityYear="1787" box="[670,958,646,665]" class="Insecta" family="Chrysomelidae" genus="Cassida" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="336" phylum="Arthropoda" rank="species" species="palliata">
<emphasis id="DDA2A224FFE7FFCCFD62FD60FC92FD29" box="[670,812,646,665]" italics="true" pageId="61" pageNumber="336">Cassida palliata</emphasis>
