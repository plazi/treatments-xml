<taxonomicName id="28D605B5FFE7FFCCFCADFDB7FE27FDCF" authority="Guerin-Meneville, 1833" authorityName="Guerin-Meneville" authorityYear="1833" class="Insecta" family="Chrysomelidae" genus="Cassida" kingdom="Animalia" order="Coleoptera" pageId="61" pageNumber="336" phylum="Arthropoda" rank="species" species="longicornis" subGenus="Cyclosoma">
