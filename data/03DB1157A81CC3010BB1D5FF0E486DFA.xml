, 
<number id="3DBAB266A81DC3040B92D1C00FD569DF" box="[180,226,954,980]" pageId="3" pageNumber="293" value="2.2">2, 2</number>
, 
<number id="3DBAB266A81DC3040BC9D1C00E2A69DF" box="[239,285,954,980]" pageId="3" pageNumber="293" value="8.2">8, 2</number>
, 
<number id="3DBAB266A81DC3040A0FD1C00E6F69DF" box="[297,344,954,980]" pageId="3" pageNumber="293" value="2.2">2, 2</number>
, 
<number id="3DBAB266A81DC3040A42D1C00EA569DF" box="[356,402,954,980]" pageId="3" pageNumber="293" value="2.2">2, 2</number>
, 
