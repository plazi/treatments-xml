<paragraph id="8BCDA041A81DC3040BE0D2920E436B21" blockId="3.[151,1439,152,1620]" pageId="3" pageNumber="293">
<typeStatus id="54C91EE3A81DC3040BE0D2920E376B09" box="[198,256,232,258]" pageId="3" pageNumber="293">Type</typeStatus>
