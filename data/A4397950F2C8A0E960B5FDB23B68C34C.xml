<taxonomicName id="29C54F9F09EDBCD6CFAC63D87C346434" LSID="http://species-id.net/wiki/Calocosmus_chevrolati" authority="Fisher" class="Insecta" family="Cerambycidae" genus="Calocosmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calocosmus chevrolati" order="Coleoptera" pageId="19" pageNumber="72" phylum="Arthropoda" rank="species" species="chevrolati">Calocosmus chevrolati Fisher</taxonomicName>
