<taxonomicName id="79FA6A16DDC639B5F19943734C0B1996" class="Sordariomycetes" family="Annulatascaceae" genus="Conlarium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Conlarium sacchari" order="Annulatascales" pageId="5" pageNumber="6" phylum="Ascomycota" rank="species" species="sacchari">Conlarium sacchari</taxonomicName>
