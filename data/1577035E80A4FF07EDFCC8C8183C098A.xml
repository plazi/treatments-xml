<quantity id="5A261FAD80A4FF07EE84C9901D1E09AD" box="[1007,1091,1977,2001]" metricMagnitude="1" metricUnit="m" metricValue="1.0" metricValueMax="2.0" metricValueMin="0.0" pageId="160" pageNumber="161" unit="m" value="10.0" valueMax="20.0" valueMin="0.0">0–20 m</quantity>
