<taxonomicName id="5ADEC9CB80A4FF07E87FC9031C0A093D" baseAuthorityName="Forsskal in Niebuhr" baseAuthorityYear="1775" box="[1300,1367,1834,1857]" class="Actinopterygii" family="Gerreidae" genus="Gerres" kingdom="Animalia" order="Perciformes" pageId="160" pageNumber="161" phylum="Chordata" rank="species" species="oyena">
