, mont 
<location id="66374F9A1F68FF93FD89FB9DFDFA3793" LSID="urn:lsid:plazi:treatment:EB41A8571F68FF95FFE6FD32FBC437A8:66374F9A1F68FF93FD89FB9DFDFA3793" box="[531,592,1148,1172]" country="New Caledonia" name="Panie" pageId="35" pageNumber="314">Panié</location>
, vers 
<quantity id="A410B4A41F68FF93FFEBFB77FF1837A9" box="[113,178,1174,1198]" metricMagnitude="2" metricUnit="m" metricValue="9.5" pageId="35" pageNumber="314" unit="m" value="950.0">
