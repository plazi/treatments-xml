<emphasis id="4FA495EEC1E67037A68A77DB6B186CAA" bold="true" pageId="0" pageNumber="61">Figure 11.</emphasis>
<taxonomicName id="821CB5D79503C318B4A64C5190BEB2DC" authority="Froelich, 1799" authorityName="Froelich" authorityYear="1799" class="Insecta" family="Coccinellidae" genus="Lithophilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lithophilus naviauxi" order="Coleoptera" pageId="0" pageNumber="61" phylum="Arthropoda" rank="species" species="naviauxi">
