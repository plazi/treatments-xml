<taxonomicName id="27CA4D35FEC7FEA15C74FDAEFACC6344" authorityName="Rondani" authorityYear="1857" box="[1130,1407,547,573]" class="Insecta" family="Syrphidae" genus="Platycheirus" kingdom="Animalia" order="Diptera" pageId="266" pageNumber="267" phylum="Arthropoda" rank="species" species="spathulatus">
