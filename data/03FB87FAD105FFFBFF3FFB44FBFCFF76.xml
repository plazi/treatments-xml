<taxonomicName id="4C524D6FD105FFFAFF3FFB44FF06FABA" box="[151,239,1278,1304]" class="Insecta" family="Tenebrionidae" genus="Stenomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="29" phylum="Arthropoda" rank="species" species="furcata">
