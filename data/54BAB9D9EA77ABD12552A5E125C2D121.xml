<taxonomicName id="EEFF1CE0672BB807A017C1B24EEDCCF1" LSID="http://zoobank.org/4655DB46-7474-430E-BC17-AC3E7CD9AA1E" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus fahringeri" order="Hymenoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="fahringeri">Heterospilus fahringeri Marsh</taxonomicName>
