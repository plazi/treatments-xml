<collectionCode id="ED2B88D1FFA7FF80A2D5FC95FE2AE423" box="[434,442,834,855]" country="Romania" lsid="urn:lsid:biocol.org:col:14415" name="&amp;quot;Alexandru Ioan Cuza&amp;quot; University" pageId="1" pageNumber="638" type="Herbarium">I</collectionCode>
