<location id="4F336068FF424FF5FBF0FE174269FCB5" LSID="urn:lsid:plazi:treatment:C24587A5FF454FF5FF50FEA5467BFC68:4F336068FF424FF5FBF0FE174269FCB5" box="[1079,1296,836,861]" country="Turkey" municipality="Remarks. Ikiyaka Mountain" name="Yuksekova district" pageId="164" pageNumber="165" stateProvince="Hakkari Province">Yüksekova district</location>
