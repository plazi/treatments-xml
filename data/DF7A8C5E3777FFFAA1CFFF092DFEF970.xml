; head mediodorsal length 
<number id="E11B2F6F3777FFFAA511FB1C2974FB68" box="[1097,1151,1154,1178]" pageId="9" pageNumber="238" value="1.85">1.85</number>
; head max. width 
<number id="E11B2F6F3777FFFAA43AFB1C2893FB68" box="[1378,1432,1154,1178]" pageId="9" pageNumber="238" value="4.35">4.35</number>
; antenna total length 
<number id="E11B2F6F3777FFFAA0D2FB392CCBFB4D" box="[394,448,1191,1215]" pageId="9" pageNumber="238" value="1.4">1.40</number>
; length of antennomeres 
<number id="E11B2F6F3777FFFAA3B4FB392E2AFB4D" box="[748,801,1191,1215]" pageId="9" pageNumber="238" value="0.15">0.15</number>
, 
<number id="E11B2F6F3777FFFAA268FB392E6EFB4D" box="[816,869,1191,1215]" pageId="9" pageNumber="238" value="0.22">0.22</number>
, 
<number id="E11B2F6F3777FFFAA22CFB392EA2FB4D" box="[884,937,1191,1215]" pageId="9" pageNumber="238" value="0.25">0.25</number>
, 
<number id="E11B2F6F3777FFFAA2E0FB392EE6FB4D" box="[952,1005,1191,1215]" pageId="9" pageNumber="238" value="0.16">0.16</number>
, 
<number id="E11B2F6F3777FFFAA2A4FB39293AFB4D" box="[1020,1073,1191,1215]" pageId="9" pageNumber="238" value="0.23">0.23</number>
, 
<number id="E11B2F6F3777FFFAA567FB39297FFB4D" box="[1087,1140,1191,1215]" pageId="9" pageNumber="238" value="0.22">0.22</number>
, 
<number id="E11B2F6F3777FFFAA5DBFB3929B2FB4D" box="[1155,1209,1191,1215]" pageId="9" pageNumber="238" value="0.17">0.17</number>
; prementum length 
<number id="E11B2F6F3777FFFAA1CFFB532DC6FB17" box="[151,205,1229,1253]" pageId="9" pageNumber="238" value="3.37">3.37</number>
; prementum maximum width 
<number id="E11B2F6F3777FFFAA370FB532F55FB17" box="[552,606,1229,1253]" pageId="9" pageNumber="238" value="3.05">3.05</number>
