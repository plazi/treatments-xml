<emphasis id="B93BEA9DFFC3FFB5F392F9C82F497271" bold="true" box="[199,321,1589,1614]" inLineHeading="true" pageId="32" pageNumber="33" reason="1">Diagnosis.</emphasis>
<taxonomicName id="4C4F4D0CFFC3FFB5F21CF9C82C057272" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[329,525,1589,1613]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="maai">
