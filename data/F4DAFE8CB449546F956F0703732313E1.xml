<taxonomicName id="0652CE31B12841D6B411CDA3636F4392" LSID="F4DAFE8C-B449-546F-956F-0703732313E1" authority="group" authorityName="group" class="Arachnida" family="Clubionidae" genus="Clubiona" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Clubiona japonicola" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="japonicola">Clubiona japonicola group</taxonomicName>
