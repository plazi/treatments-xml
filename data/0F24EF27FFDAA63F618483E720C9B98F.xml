. Asia: 
<collectingCountry id="FF9A1EA1FFD8A63F60A1874C223BB953" box="[434,526,583,607]" name="Bahrain" pageId="3">Bahrain</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F630F874C2297B953" box="[540,674,583,607]" name="Bangladesh" pageId="3">Bangladesh</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F63A2874C2330B953" box="[689,773,583,607]" name="Bhutan" pageId="3">Bhutan</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F6207874C236FB953" box="[788,858,583,607]" name="China" pageId="3">China</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F627A874C2390B953" box="[873,933,583,607]" name="India" pageId="3">India</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F62A6874C23CEB953" box="[949,1019,583,607]" name="Nepal" pageId="3">Nepal</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F651A874C245EB953" box="[1033,1131,583,607]" name="Pakistan" pageId="3">Pakistan</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F6568874C24DAB953" box="[1147,1263,583,607]" name="Sri Lanka" pageId="3">Sri Lanka</collectingCountry>
, 
<collectingCountry id="FF9A1EA1FFD8A63F65ED874C2551B953" box="[1278,1380,583,607]" name="Thailand" pageId="3">Thailand</collectingCountry>
, and 
<collectingCountry id="FF9A1EA1FFD8A63F6184876020CDB98F" box="[151,248,619,643]" name="Vietnam" pageId="3">Vietnam</collectingCountry>
