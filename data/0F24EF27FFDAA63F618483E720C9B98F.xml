<paragraph id="87325E31FFDAA63D6184823824C3BC44" blockId="1.[151,1270,1842,1865]" box="[151,1270,1842,1865]" pageId="1">
<taxonomicName id="408D25B2FFDAA63D61848238218EBC44" authority="Kapur, 1958: 324" authorityName="Kapur" authorityPageNumber="324" authorityYear="1958" box="[151,443,1842,1865]" class="Insecta" family="Coccinellidae" genus="Afidentula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" phylum="Arthropoda" rank="genus">
<emphasis id="B5F98223FFDAA63D618482382135BC45" box="[151,256,1843,1865]" italics="true" pageId="1">Afidentula</emphasis>
Kapur, 
<number id="31454C16FFDAA63D6041823921BDBC44" box="[338,392,1842,1864]" pageId="1" value="1958.0">1958</number>
: 
<number id="31454C16FFDAA63D60818238218EBC44" box="[402,443,1843,1864]" pageId="1" value="324.0">324</number>
</taxonomicName>
. 
<typeStatus id="5836E093FFDAA63D60D7823F21C3BC45" box="[452,502,1844,1865]" pageId="1">Type</typeStatus>
