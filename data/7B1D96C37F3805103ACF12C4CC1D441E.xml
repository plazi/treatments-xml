<taxonomicName id="2494DEA4C9519B1A1FFB9EA7634702B9" class="Insecta" family="Histeridae" genus="Paramyrmetes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paramyrmetes" order="Coleoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="genus">Paramyrmetes</taxonomicName>
