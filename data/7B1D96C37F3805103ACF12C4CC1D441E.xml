<taxonomicName id="EF7DF0BCC60067B3FF9B8D7A73DC1EF3" class="Insecta" family="Histeridae" genus="Paramyrmetes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paramyrmetes foveipennis" order="Coleoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="species" species="foveipennis">Paramyrmetes foveipennis</taxonomicName>
