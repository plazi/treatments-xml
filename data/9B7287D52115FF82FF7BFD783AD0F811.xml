<emphasis id="21AFEAD12115FF82FD25FC113BABFBFF" box="[713,742,999,1024]" italics="true" pageId="7" pageNumber="60">
<number id="A51324E42115FF82FD25FC113B9BFBFF" box="[713,726,999,1024]" pageId="7" pageNumber="60" value="1.0">1</number>
