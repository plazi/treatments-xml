<subSubSection box="[151,321,1199,1225]" pageId="1" pageNumber="56" type="nomenclature">
<paragraph blockId="1.[151,321,1199,1225]" box="[151,321,1199,1225]" pageId="1" pageNumber="56">
<heading bold="true" box="[151,321,1199,1225]" fontSize="11" level="1" pageId="1" pageNumber="56" reason="1">
<emphasis bold="true" box="[151,321,1199,1225]" pageId="1" pageNumber="56">
<taxonomicName box="[151,239,1199,1225]" class="Insecta" family="Phyloblattidae" genus="Sobytie" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="1" pageNumber="56" phylum="Arthropoda" rank="genus" status="gen.n.">
<emphasis bold="true" box="[151,239,1199,1225]" italics="true" pageId="1" pageNumber="56">Sobytie</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[246,321,1199,1225]" pageId="1" pageNumber="56" rank="genus">gen.n.</taxonomicNameLabel>
</emphasis>
