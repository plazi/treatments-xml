<geoCoordinate direction="north" orientation="latitude" precision="5" value="45.7575">45.7575°N</geoCoordinate>
, 
<geoCoordinate direction="west" orientation="longitude" precision="5" value="-66.5726">66.5726°W</geoCoordinate>
