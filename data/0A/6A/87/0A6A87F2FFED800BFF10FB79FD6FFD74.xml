<paragraph blockId="47.[151,1437,151,717]" pageId="47" pageNumber="448">
<emphasis bold="true" box="[199,338,620,645]" pageId="47" pageNumber="448">Comments:</emphasis>
<taxonomicName box="[344,609,620,645]" class="Insecta" family="Micropezidae" genus="Cardiacephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="448" phylum="Arthropoda" rank="species" species="secunda">
<emphasis box="[344,609,620,645]" italics="true" pageId="47" pageNumber="448">Cardiacephala secunda</emphasis>
</taxonomicName>
