</paragraph>
<paragraph blockId="26.[151,1437,151,717]" box="[199,1431,620,645]" pageId="26" pageNumber="227">
<materialsCitation ID-GBIF-Occurrence="1913888622" box="[199,1431,620,645]" location="Zhejiang" pageId="26" pageNumber="227" specimenCount="1" stateProvince="Zhejiang" typeStatus="holotype">
