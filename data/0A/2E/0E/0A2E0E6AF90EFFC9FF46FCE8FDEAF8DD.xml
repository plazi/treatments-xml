<taxonomicName authority="Fairmaire, 1891" authorityName="Fairmaire" authorityYear="1891" box="[798,1234,1823,1848]" class="Insecta" family="Staphylinidae" genus="Staphylinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="232" phylum="Arthropoda" rank="species" species="fraternus">
