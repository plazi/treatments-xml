<typeStatus box="[1010,1172,1752,1776]" pageId="31" pageNumber="232">LECTOTYPE</typeStatus>
<taxonomicName authority="Fairmaire, 1891" authorityName="Fairmaire" authorityYear="1891" class="Insecta" family="Staphylinidae" genus="Staphylinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="232" phylum="Arthropoda" rank="species" species="fraternus">
<emphasis box="[1188,1436,1751,1776]" italics="true" pageId="31" pageNumber="232">Staphylinus fraternus</emphasis>
