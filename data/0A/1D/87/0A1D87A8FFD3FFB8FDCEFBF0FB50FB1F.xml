<taxonomicName authorityName="Verhoeff" authorityYear="1899" box="[996,1322,1130,1156]" class="Diplopoda" family="Haplobainosomatidae" genus="Haplobainosoma" kingdom="Animalia" order="Chordeumatida" pageId="111" pageNumber="112" phylum="Arthropoda" rank="species" species="lusitanum">
