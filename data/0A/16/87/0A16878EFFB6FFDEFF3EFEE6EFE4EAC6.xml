<emphasis bold="true" box="[151,819,285,311]" pageId="24" pageNumber="25">
<taxonomicName authority="Salles, Massariol &amp; Angeli" authorityName="Salles, Massariol &amp; Angeli" box="[151,712,285,311]" class="Insecta" family="Baetidae" genus="Cloeodes" kingdom="Animalia" order="Ephemeroptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="ioachimi" status="sp. nov.">
<emphasis bold="true" box="[151,373,285,311]" italics="true" pageId="24" pageNumber="25">Cloeodes ioachimi</emphasis>
Salles, Massariol &amp; Angeli
</taxonomicName>
, 
<taxonomicNameLabel box="[725,819,285,311]" pageId="24" pageNumber="25" rank="species">sp. nov.</taxonomicNameLabel>
