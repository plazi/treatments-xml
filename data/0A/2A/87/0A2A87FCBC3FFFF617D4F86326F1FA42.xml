<subSubSection pageId="3" pageNumber="437" type="etymology">
<paragraph blockId="3.[151,1436,1129,1442]" pageId="3" pageNumber="437">
<emphasis bold="true" box="[199,334,1345,1370]" pageId="3" pageNumber="437">Etymology.</emphasis>
<emphasis box="[347,436,1345,1370]" italics="true" pageId="3" pageNumber="437">Tianzhu</emphasis>
+ 
<taxonomicName box="[451,575,1345,1370]" class="Insecta" family="Carabidae" genus="Aphaenops" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="437" phylum="Arthropoda" rank="genus">
