<quantity box="[1052,1175,1189,1212]" metricMagnitude="2" metricUnit="m" metricValue="8.084999999999999" metricValueMax="8.22" metricValueMin="7.95" pageId="2" pageNumber="289" unit="m" value="808.5" valueMax="822.0" valueMin="795.0">795–822 m</quantity>
