<paragraph blockId="6.[151,1436,1688,1741]" pageId="6" pageNumber="67">
<emphasis bold="true" box="[151,276,1688,1711]" pageId="6" pageNumber="67">FIGURE 5.</emphasis>
<taxonomicName box="[285,449,1688,1710]" class="Entognatha" family="Entomobryidae" genus="Coecobrya" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="67" phylum="Arthropoda" rank="species" species="pani" status="sp. nov.">
<emphasis box="[285,449,1688,1710]" italics="true" pageId="6" pageNumber="67">Coecobrya pani</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[457,540,1689,1710]" pageId="6" pageNumber="67" rank="species">
