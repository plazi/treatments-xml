<paragraph blockId="6.[189,1398,1431,1537]" pageId="6" pageNumber="91">
<materialsCitation ID-GBIF-Occurrence="3357497313" collectionCode="MNHN" country="Mexico" location="Oaxaca" pageId="6" pageNumber="91" specimenCode="MNHN 1891.0260" specimenCount="1" stateProvince="Oaxaca" typeStatus="type">
