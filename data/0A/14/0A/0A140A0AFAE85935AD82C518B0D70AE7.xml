<taxonomicName authorityName="Sloane" authorityYear="1907" class="Insecta" family="Carabidae" genus="Cuneipectus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="486" pageNumber="487" phylum="Arthropoda" rank="genus">
