<taxonomicName authorityName="Brulle" authorityYear="1834" class="Insecta" family="Carabidae" genus="Microcheila" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="486" pageNumber="487" phylum="Arthropoda" rank="genus">
