<taxonomicName class="Hexapoda" family="Carabidae" genus="Dercylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="486" pageNumber="487" phylum="Arthropoda" rank="genus">
