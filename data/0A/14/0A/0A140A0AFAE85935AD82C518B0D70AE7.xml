<taxonomicName authorityName="Chaudoir" authorityYear="1880" lsidName="" pageId="486" pageNumber="487" rank="tribe" tribe="Peleciini">Peleciini</taxonomicName>
(about 75 species), 
<taxonomicName authorityName="Bonelli" authorityYear="1810" lsidName="" pageId="486" pageNumber="487" rank="tribe" tribe="Pterostichini">Pterostichini</taxonomicName>
