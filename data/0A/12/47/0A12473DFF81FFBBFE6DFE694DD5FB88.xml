/ 
<number box="[773,862,861,888]" pageId="1" pageNumber="478" value="1.9" valueMax="2.1" valueMin="1.7">1.7–2.1</number>
, width 
<number box="[953,1039,861,888]" pageId="1" pageNumber="478" value="1.2000000000000002" valueMax="1.3" valueMin="1.1">1.1–1.3</number>
