<number box="[575,661,893,920]" pageId="1" pageNumber="478" value="1.4" valueMax="1.5" valueMin="1.3">1.3–1.5</number>
/ 
<number box="[670,758,893,920]" pageId="1" pageNumber="478" value="1.9500000000000002" valueMax="2.1" valueMin="1.8">1.8–2.1</number>
, width 
<number box="[840,931,893,920]" pageId="1" pageNumber="478" value="0.8500000000000001" valueMax="0.9" valueMin="0.8">0.8–0.9</number>
/ 
<number box="[940,1024,893,920]" pageId="1" pageNumber="478" value="1.4" valueMax="1.5" valueMin="1.3">1.3–1.5</number>
