<collectingCountry box="[938,1005,608,630]" name="Russia" pageId="10" pageNumber="407">Russia</collectingCountry>
(North Caucasus), and 
<collectingCountry box="[337,397,637,659]" name="China" pageId="10" pageNumber="407">China</collectingCountry>
(
<collectingRegion box="[411,503,637,659]" country="China" name="Xinjiang Uygur" pageId="10" pageNumber="407">Xinjiang</collectingRegion>
