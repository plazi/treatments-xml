. 
<taxonomicName authorityName=": Dow and Wirth" authorityYear="1972" box="[977,1163,1206,1231]" class="Insecta" family="Ceratopogonidae" genus="Forcipomyia" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="20" phylum="Arthropoda" rank="species" species="johannseni" subGenus="Thyridomyia">
