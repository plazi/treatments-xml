<collectingCountry box="[342,421,1356,1382]" name="Russia" pageId="9" pageNumber="224">Russia</collectingCountry>
: 
<emphasis box="[432,615,1356,1383]" italics="true" pageId="9" pageNumber="224">
<taxonomicName box="[432,526,1357,1383]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="9" pageNumber="224" phylum="Tracheophyta" rank="genus">Quercus</taxonomicName>
