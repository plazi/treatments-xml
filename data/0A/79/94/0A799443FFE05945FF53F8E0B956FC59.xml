<subSubSection pageId="3" pageNumber="389" type="etymology">
<paragraph blockId="3.[151,1437,151,1005]" pageId="3" pageNumber="389">
<emphasis bold="true" box="[199,334,692,717]" pageId="3" pageNumber="389">Etymology.</emphasis>
