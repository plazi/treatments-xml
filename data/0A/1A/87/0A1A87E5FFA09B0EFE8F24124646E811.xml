<collectingCountry box="[743,814,755,779]" name="China" pageId="23" pageNumber="678">China</collectingCountry>
Sea off 
<collectingRegion box="[919,1056,755,779]" country="China" name="Guangdong" pageId="23" pageNumber="678">Guangdong</collectingRegion>
(
<bibRefCitation author="Liu R &amp; Wang S" pageId="23" pageNumber="678" pagination="320 - 334" refId="ref25028" refString="Liu R, Wang S. 1980. Five new species of the genus Acanthomysis (Crustacea Mysidacea) from the South China Sea. Oceanologia et Limnologia Sinica 11: 320 - 334. (Chi with Eng abstract and description)." type="journal article" year="1980">Liu and Wang 1980</bibRefCitation>
) and the East 
<collectingCountry box="[436,507,788,812]" name="China" pageId="23" pageNumber="678">China</collectingCountry>
Sea off 
<collectingRegion box="[606,707,788,812]" country="China" name="Zhejiang" pageId="23" pageNumber="678">Zhejiang</collectingRegion>
(
<bibRefCitation author="Wang S &amp; Liu R" box="[724,968,788,812]" pageId="23" pageNumber="678" pagination="191 - 222" refId="ref25774" refString="Wang S, Liu R. 1997. Mysidacea fauna of the East China Sea. Studia Marina Sinica 38: 191 - 222. (Chi with Eng abstract)." type="journal article" year="1997">Wang and Liu 1997</bibRefCitation>
