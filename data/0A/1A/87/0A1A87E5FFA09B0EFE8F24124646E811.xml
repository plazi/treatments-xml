<paragraph blockId="22.[138,1185,1597,1685]" pageId="22" pageNumber="677">
<taxonomicName authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Liu and Wang" baseAuthorityYear="1980" box="[138,360,1597,1621]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="22" pageNumber="677" phylum="Arthropoda" rank="species" species="leptura">
<emphasis box="[138,360,1597,1621]" italics="true" pageId="22" pageNumber="677">Orientomysis leptura</emphasis>
</taxonomicName>
