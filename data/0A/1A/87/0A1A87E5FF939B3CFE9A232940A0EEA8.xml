<taxonomicName authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Ii" baseAuthorityYear="1964" box="[731,922,1243,1267]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="37" pageNumber="692" phylum="Arthropoda" rank="species" species="okayamaensis">
