<paragraph blockId="37.[125,1172,1243,1331]" pageId="37" pageNumber="692">
<taxonomicName authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Liu and Wang" baseAuthorityYear="1980" box="[125,351,1243,1267]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="37" pageNumber="692" phylum="Arthropoda" rank="species" species="serrata">
