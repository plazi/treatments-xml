<taxonomicName baseAuthorityName="ERICHSON" baseAuthorityYear="1839" box="[766,894,1370,1392]" class="Insecta" family="Staphylinidae" genus="Medon" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="492" phylum="Arthropoda" rank="species" species="brunneus">
<emphasis box="[766,894,1370,1392]" italics="true" pageId="3" pageNumber="492">M. brunneus</emphasis>
</taxonomicName>
