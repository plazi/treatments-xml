<collectingCountry box="[466,539,1399,1421]" name="Turkey" pageId="3" pageNumber="492">Turkey</collectingCountry>
to 
<collectingCountry box="[571,627,1399,1421]" name="Israel" pageId="3" pageNumber="492">Israel</collectingCountry>
