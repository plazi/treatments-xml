<number box="[1225,1240,1159,1185]" pageId="7" pageNumber="38" value="6.0">6</number>
(
<number box="[1260,1308,1159,1185]" pageId="7" pageNumber="38" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
