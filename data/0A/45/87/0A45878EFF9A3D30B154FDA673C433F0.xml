<number box="[807,822,1159,1185]" pageId="7" pageNumber="38" value="4.0">4</number>
(
<number box="[841,889,1159,1185]" pageId="7" pageNumber="38" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
