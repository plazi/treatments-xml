<number box="[1261,1308,1119,1145]" pageId="7" pageNumber="38" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
), basiventral femoral setae (
<emphasis box="[584,612,1159,1184]" italics="true" pageId="7" pageNumber="38">bv</emphasis>
) 
<number box="[632,647,1159,1185]" pageId="7" pageNumber="38" value="6.0">6</number>
(
<number box="[666,714,1159,1185]" pageId="7" pageNumber="38" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
