<number box="[1227,1242,1239,1265]" pageId="5" pageNumber="36" value="3.0">3</number>
(
<number box="[1260,1308,1239,1265]" pageId="5" pageNumber="36" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
