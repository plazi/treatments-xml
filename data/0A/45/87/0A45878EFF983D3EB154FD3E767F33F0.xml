<number box="[1063,1077,1239,1265]" pageId="5" pageNumber="36" value="4.0">4</number>
(
<number box="[1095,1144,1239,1265]" pageId="5" pageNumber="36" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
