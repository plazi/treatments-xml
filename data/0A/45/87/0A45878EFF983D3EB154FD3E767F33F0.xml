<number box="[347,362,1279,1305]" pageId="5" pageNumber="36" value="3.0">3</number>
(
<number box="[386,434,1279,1305]" pageId="5" pageNumber="36" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
