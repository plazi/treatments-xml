<number box="[1191,1220,1119,1145]" pageId="3" pageNumber="34" value="53.0">53</number>
(
<number box="[1238,1314,1119,1145]" pageId="3" pageNumber="34" value="52.5" valueMax="54.0" valueMin="51.0">51–54</number>
