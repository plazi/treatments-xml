<emphasis bold="true" box="[1118,1323,1158,1184]" pageId="3" pageNumber="34">Prodorsal shield</emphasis>
<number box="[264,293,1199,1225]" pageId="3" pageNumber="34" value="42.0">42</number>
(
<number box="[311,390,1199,1225]" pageId="3" pageNumber="34" value="41.5" valueMax="44.0" valueMin="39.0">39–44</number>
