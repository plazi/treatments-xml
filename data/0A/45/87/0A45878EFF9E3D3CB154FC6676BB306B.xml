<number box="[648,677,1159,1185]" pageId="3" pageNumber="34" value="16.0">16</number>
(
<number box="[694,771,1159,1185]" pageId="3" pageNumber="34" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
