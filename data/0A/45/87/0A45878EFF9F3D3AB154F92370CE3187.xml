<subSubSection lastPageId="3" lastPageNumber="34" pageId="2" pageNumber="33" type="materials_examined">
<paragraph blockId="2.[264,702,1777,1801]" box="[264,702,1777,1801]" pageId="2" pageNumber="33">
<typeStatus box="[264,317,1777,1800]" pageId="2" pageNumber="33">Type</typeStatus>
species: 
<taxonomicName box="[415,610,1778,1801]" class="Arachnida" family="Eriophyidae" genus="Calvittacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="regiae" status="sp. nov.">
<emphasis box="[415,610,1778,1801]" italics="true" pageId="2" pageNumber="33">Calvittacus regiae</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[617,702,1777,1801]" pageId="2" pageNumber="33" rank="species">
