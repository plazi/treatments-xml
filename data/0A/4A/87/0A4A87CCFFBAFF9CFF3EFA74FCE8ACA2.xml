<paragraph blockId="12.[151,1437,1575,2036]" pageId="12" pageNumber="227">
<emphasis bold="true" box="[151,379,1575,1600]" pageId="12" pageNumber="227">Material examined</emphasis>
. 
<number box="[394,408,1576,1600]" pageId="12" pageNumber="227" value="1.0">1</number>
