<taxonomicName authority="" authorityName="" class="Insecta" family="Aphididae" genus="P." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="P. salicis" order="Hemiptera" pageId="0" pageNumber="8013" phylum="Arthropoda" rank="species" species="salicis">P. salicis</taxonomicName>
