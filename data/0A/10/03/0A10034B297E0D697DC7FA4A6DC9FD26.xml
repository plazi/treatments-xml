(
<emphasis box="[1350,1427,547,573]" italics="true" pageId="35" pageNumber="236">Halicl.</emphasis>
) 
<taxonomicName authorityName="Tubular" baseAuthorityName="Thiele" baseAuthorityYear="1905" box="[151,265,583,609]" class="Demospongiae" family="Chalinidae" genus="Haliclona" kingdom="Animalia" order="Haplosclerida" pageId="35" pageNumber="236" phylum="Porifera" rank="species" species="spinosella">
