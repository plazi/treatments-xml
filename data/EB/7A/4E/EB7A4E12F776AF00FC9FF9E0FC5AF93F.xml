<subSubSection pageId="5" pageNumber="302" type="discussion">
<paragraph blockId="5.[793,1399,1545,1729]" pageId="5" pageNumber="302">
<emphasis bold="true" box="[794,881,1545,1569]" pageId="5" pageNumber="302">Remark</emphasis>
- Redescriptions of five 
<taxonomicName box="[1136,1343,1545,1569]" class="Insecta" family="Braconidae" genus="Mesobracon" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="302" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis box="[1136,1262,1545,1569]" italics="true" pageId="5" pageNumber="302">Mesobracon</emphasis>
