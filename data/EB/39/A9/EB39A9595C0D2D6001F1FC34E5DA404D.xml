, PL 
<quantity box="[1048,1187,1979,2006]" metricMagnitude="-3" metricUnit="m" metricValue="4.25" metricValueMax="4.3" metricValueMin="4.2" pageId="2" pageNumber="589" unit="mm" value="4.25" valueMax="4.3" valueMin="4.2">4.2–4.3 mm</quantity>
, PW 
<quantity box="[1251,1390,1979,2006]" metricMagnitude="-3" metricUnit="m" metricValue="4.45" metricValueMax="4.5" metricValueMin="4.4" pageId="2" pageNumber="589" unit="mm" value="4.45" valueMax="4.5" valueMin="4.4">4.4–4.5 mm</quantity>
, EL 9.0– 
<quantity box="[200,288,2015,2042]" metricMagnitude="-3" metricUnit="m" metricValue="9.2" pageId="2" pageNumber="589" unit="mm" value="9.2">9.2 mm</quantity>
, EW 
<quantity box="[350,488,2015,2042]" metricMagnitude="-3" metricUnit="m" metricValue="7.05" metricValueMax="7.2" metricValueMin="6.9" pageId="2" pageNumber="589" unit="mm" value="7.05" valueMax="7.2" valueMin="6.9">6.9–7.2 mm</quantity>
