<taxonomicName lsidName="" pageId="40" pageNumber="41" rank="tribe" tribe="Aresconini">Aresconini</taxonomicName>
and 
<taxonomicName lsidName="" pageId="40" pageNumber="41" rank="tribe" tribe="Triadomerini">Triadomerini</taxonomicName>
