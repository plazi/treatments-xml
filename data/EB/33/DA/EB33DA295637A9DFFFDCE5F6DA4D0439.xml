<paragraph pageId="297" pageNumber="298">
<taxonomicName authorityName="LeConte" authorityYear="1850" class="Hexapoda" family="Carabidae" genus="Dyschirius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="297" pageNumber="298" phylum="Arthropoda" rank="species" species="parvus">Dyschirius parvus</taxonomicName>
