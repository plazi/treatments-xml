<emphasis bold="true" box="[680,693,1263,1280]" pageId="20" pageNumber="291">A</emphasis>
, habit; 
<emphasis bold="true" box="[755,768,1263,1280]" pageId="20" pageNumber="291">B</emphasis>
, flower, anterior view; 
<emphasis bold="true" box="[958,972,1263,1280]" pageId="20" pageNumber="291">C</emphasis>
