<paragraph pageId="null" pageNumber="510">
<taxonomicName authority="Schreber" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="510" phylum="Tracheophyta" rank="species" species="patens">
