</subSubSection>
<subSubSection pageId="2" pageNumber="145" type="nomenclature">
<paragraph pageId="2" pageNumber="145">
<taxonomicName LSID="http://zoobank.org/0C1D768E-779F-42A9-BC6A-520BA2447E06" authority="Martinez, Lázaro, Pedraza-Lara &amp; Zaldívar-Riverón, 2016" authorityName="Martinez, Lázaro, Pedraza-Lara &amp; Zaldívar-Riverón" authorityYear="2016" class="Insecta" family="Braconidae" genus="Sergey" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sergey" order="Hymenoptera" pageId="2" pageNumber="145" phylum="Arthropoda" rank="genus">Sergey</taxonomicName>
