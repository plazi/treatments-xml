<taxonomicName box="[801,946,1187,1209]" class="Insecta" family="Haglotettigoniidae" genus="Brasilodontus" kingdom="Animalia" order="Orthoptera" pageId="25" pageNumber="176" phylum="Arthropoda" rank="species" species="riodocensis">
<emphasis box="[801,946,1187,1209]" italics="true" pageId="25" pageNumber="176">B. riodocensis</emphasis>
</taxonomicName>
; 
<emphasis bold="true" box="[959,979,1187,1209]" pageId="25" pageNumber="176">G</emphasis>
— 
<taxonomicName box="[1004,1224,1187,1209]" class="Insecta" family="Haglotettigoniidae" genus="Xulavuna" kingdom="Animalia" order="Orthoptera" pageId="25" pageNumber="176" phylum="Arthropoda" rank="species" species="adenoptera" status="sp. n.">
