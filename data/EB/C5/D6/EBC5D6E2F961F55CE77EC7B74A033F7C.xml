<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina crocisae" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="crocisae">crocisae</taxonomicName>
