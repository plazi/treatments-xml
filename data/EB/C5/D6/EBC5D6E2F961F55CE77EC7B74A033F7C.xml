<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina leopoldi" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="leopoldi">leopoldi</taxonomicName>
, 
<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina novus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="novus">novus</taxonomicName>
, 
<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina varipilosus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="varipilosus">varipilosus</taxonomicName>
and 
<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina xylocopae" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="xylocopae">xylocopae</taxonomicName>
), and 
<bibRefCitation author="Baker, EW" journalOrPublisher="International Journal of Acarology" pageId="0" pageNumber="1" pagination="227 - 232" title="An unusual new species of Neocypholaelaps (Acari: Ameroseiidae) from the nests of stingless bees (Apidae: Meliponinae)." url="https://doi.org/10.1080/01647958508683421" volume="11" year="1985">Baker and Delfinado-Baker (1985)</bibRefCitation>
, based on nine world species (
<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina ampullula" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="ampullula">ampullula</taxonomicName>
, 
<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina apicola" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="apicola">apicola</taxonomicName>
, 
<taxonomicName authority="" authorityName="" class="Insecta" family="Melolonthidae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina cocos" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="cocos">cocos</taxonomicName>
