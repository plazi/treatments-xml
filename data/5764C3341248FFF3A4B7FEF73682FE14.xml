(
<collectingCountry id="A7DA32B2124EFFF3A66DFE9C36EBFE86" box="[589,706,332,357]" name="Taiwan" pageId="13" pageNumber="204">TAIWAN</collectingCountry>
)
</emphasis>
: Hualien, 
<collectingCountry id="A7DA32B2124EFFF3A76DFE9C37AAFE86" box="[845,899,332,357]" name="Peru" pageId="13" pageNumber="204">Pi-lu</collectingCountry>
Sacred Tree (
<quantity id="1835DFC7124EFFF3A014FE9C30BCFE87" box="[1076,1173,332,357]" metricMagnitude="3" metricUnit="m" metricValue="2.15" pageId="13" pageNumber="204" unit="m" value="2150.0">2150 m</quantity>
), 
<specimenCount id="C9CBB9AB124EFFF3A090FE9C3139FE86" box="[1200,1296,332,357]" pageId="13" pageNumber="204" type="male">
<number id="69056005124EFFF3A090FE9C3097FE86" box="[1200,1214,332,357]" pageId="13" pageNumber="204" value="5.0">5</number>
males
</specimenCount>
, 
