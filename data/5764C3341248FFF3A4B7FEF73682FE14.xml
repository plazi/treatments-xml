<quantity id="1835DFC7124FFFF2A04EFB1D30FCFB07" box="[1134,1237,1229,1253]" metricMagnitude="-3" metricUnit="m" metricValue="1.39" pageId="12" pageNumber="203" unit="mm" value="1.39">1.39 mm</quantity>
