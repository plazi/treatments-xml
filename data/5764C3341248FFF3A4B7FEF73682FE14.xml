<quantity id="1835DFC7124FFFF2A133FB783153FB23" box="[1299,1402,1192,1216]" metricMagnitude="-4" metricUnit="m" metricValue="3.7" pageId="12" pageNumber="203" unit="mm" value="0.37">0.37 mm</quantity>
