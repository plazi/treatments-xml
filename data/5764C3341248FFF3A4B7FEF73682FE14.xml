<quantity id="1835DFC7124FFFF2A63AFB2036A8FAEB" box="[538,641,1264,1288]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="12" pageNumber="203" unit="mm" value="0.13">0.13 mm</quantity>
