<quantity id="1835DFC7124FFFF2A644FB7836E2FB23" box="[612,715,1192,1216]" metricMagnitude="-3" metricUnit="m" metricValue="2.41" pageId="12" pageNumber="203" unit="mm" value="2.41">2.41 mm</quantity>
