<collectingCountry id="32807602FFE5FF955DAA9BDC29184A71" box="[717,834,1268,1294]" name="Nicaragua" pageId="52" pageNumber="51">Nicaragua</collectingCountry>
, 
<collectingCountry id="32807602FFE5FF955C2C9BDC29E54A70" box="[843,959,1268,1295]" name="Venezuela" pageId="52" pageNumber="51">Venezuela</collectingCountry>
