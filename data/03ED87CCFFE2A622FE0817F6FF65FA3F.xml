<taxonomicName id="4C444D59FFE2A622FE6814C5FD2BFC01" box="[449,670,952,976]" class="Magnoliopsida" family="Anacardiaceae" genus="Schinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="27" pageNumber="82" phylum="Tracheophyta" rank="species" species="fasciculatus">
