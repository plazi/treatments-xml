<taxonomicName id="50FD4D65FF845E54FF62F7EEFD8DB42B" authority="Xu &amp; Gao &amp; Chen, 2007" authorityName="Xu &amp; Gao &amp; Chen" authorityYear="2007" box="[236,559,2065,2086]" class="Insecta" family="Drosophilidae" genus="Stegana" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="45" phylum="Arthropoda" rank="species" species="acutipenis" status="new species" subGenus="Oxyphortica">
