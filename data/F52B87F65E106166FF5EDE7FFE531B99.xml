<collectionCode id="1B93AE255E106167FE82DD70FEFB1E78" box="[331,362,1841,1862]" country="Romania" lsid="urn:lsid:biocol.org:col:15579" name="Babes-Bolyai University" pageId="38" pageNumber="171" type="Herbarium">CL</collectionCode>
/ CW 
