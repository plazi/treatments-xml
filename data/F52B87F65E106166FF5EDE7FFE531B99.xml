<taxonomicName id="BA824D635E106167FF1CDD10FE271E58" authorityName="FABRICIUS" authorityYear="1804" box="[213,438,1873,1894]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="171" phylum="Arthropoda" rank="species" species="truncorum">
