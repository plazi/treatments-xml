<quantity id="BA7A9B055E106167FDCDDD50FDF71E18" box="[516,614,1809,1831]" metricMagnitude="0" metricUnit="m" metricValue="2.045" pageId="38" pageNumber="171" unit="mm" value="2045.0">2045 µm</quantity>
