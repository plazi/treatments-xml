<collectionCode id="1B93AE255E106167FD1ADD10FD7E1E58" box="[723,751,1873,1894]" country="Sierra Leone" lsid="urn:lsid:biocol.org:col:14608" name="University of Sierra Leone, Njala University College" pageId="38" pageNumber="171" type="Herbarium">SL</collectionCode>
