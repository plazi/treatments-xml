<taxonomicName id="4C25CEE828060823114FFAB1FB1CFAF2" authorityName="Westwood" authorityYear="1874" box="[1047,1210,1368,1392]" class="Insecta" family="Cetoniidae" genus="Allorhina" kingdom="Animalia" order="Coleoptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="hypoglauca">
