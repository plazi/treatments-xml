<location id="8EFAE3B0280608231746FA95FD14FA16" LSID="urn:lsid:plazi:treatment:038C047D2806082315CFFCE6FDB2FA7D:8EFAE3B0280608231746FA95FD14FA16" box="[542,690,1404,1428]" country="Nicaragua" name="Nevertheless" pageId="76" pageNumber="77">Nevertheless</location>
