<taxonomicName id="4C34828AFFCE10249150F915FE3EF91F" authority="Papp, 2013" authorityName="Papp" authorityYear="2013" class="Insecta" family="Braconidae" genus="Chorebus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="237" phylum="Arthropoda" rank="species" species="deminens" status="sp. n." subGenus="Stiphrocera">
