106. 
<taxonomicName id="E1C81386FF93FFF4FDFD4C4AFB92F8EC" authority="Verhoeff, 1928" authorityName="Verhoeff" authorityYear="1928" box="[592,1055,1816,1843]" class="Diplopoda" family="Julidae" genus="Cylindroiulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Julida" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="arborum">
