<taxonomicName id="B7F110A7C707FFCAACC4FA818E170F0A" authority="(Duchassaing &amp; Michelotti, 1860)" baseAuthorityName="Duchassaing &amp; Michelotti" baseAuthorityYear="1860" box="[443,1108,1305,1332]" class="Anthozoa" family="Plexauridae" genus="Thesea" higherTaxonomySource="CoL" kingdom="Animalia" order="Alcyonacea" pageId="7" pageNumber="96" phylum="Cnidaria" rank="species" species="guadalupensis">
