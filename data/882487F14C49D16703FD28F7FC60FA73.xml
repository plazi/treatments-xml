<number id="B64524C04C45D16703D52CF2FF72FB62" box="[191,244,1170,1196]" pageId="9" pageNumber="10" value="1.77">1.77</number>
± 
<number id="B64524C04C45D167027F2CF2FEC9FB62" box="[277,335,1170,1196]" pageId="9" pageNumber="10" value="0.02">0.02</number>
