<taxonomicName id="FD73BC5197341F463D64FB8CACF3D6D3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:140430" class="Insecta" family="Formicidae" genus="Monomorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Monomorium minutum r. hildebrandti Forel" order="Hymenoptera" pageNumber="256" phylum="Arthropoda" rank="subSpecies" species="minutum" subSpecies="hildebrandti">race: m. hildebrandti</taxonomicName>
