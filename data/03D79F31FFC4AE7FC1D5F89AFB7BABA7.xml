<taxonomicName id="4C7E55A4FFC4AE70C38AF8B3FB9EADAC" authority="Rondani, 1856" authorityName="Rondani" authorityYear="1856" box="[712,1027,1722,1748]" class="Insecta" family="Stratiomyidae" genus="Clorisoma" kingdom="Animalia" order="Diptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="genus">
