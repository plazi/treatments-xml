<taxonomicName id="43BBCF882308FF9EFD77FA0BCF5EB627" authorityName="Ponder" authorityYear="2003" box="[691,876,1525,1548]" class="Gastropoda" family="Bithyniidae" genus="Gabbia" kingdom="Animalia" order="Littorinimorpha" pageId="101" pageNumber="102" phylum="Mollusca" rank="species" species="clathrata">
<emphasis id="B6CF68192308FF9EFD77FA0BCF5EB627" box="[691,876,1525,1548]" italics="true" pageId="101" pageNumber="102">Gabbia clathrata</emphasis>
