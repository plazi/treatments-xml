<taxonomicName id="4C7F4D417C0D4132FEDFF18DFE23D1B2" box="[281,500,1940,1963]" class="Pycnogonida" family="Pycnogonidae" genus="Pycnogonum" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="6" pageNumber="128" phylum="Arthropoda" rank="species" species="spatium">
