<taxonomicName id="4C4F4D0CFF94FFE2F3C2FE212F8175C9" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[151,393,476,502]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="119" pageNumber="120" phylum="Arthropoda" rank="species" species="bakeri">
