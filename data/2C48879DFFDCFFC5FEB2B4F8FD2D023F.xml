<taxonomicName id="63E14D08FFDCFFC2FE12B4B3FDE003C1" box="[424,635,719,744]" class="Insecta" family="Sciaridae" genus="Xenosciara" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="invisa" status="sp. n.">
<emphasis id="9695EA99FFDCFFC2FE12B4B3FDE003C1" box="[424,635,719,744]" italics="true" pageId="9" pageNumber="10">Xenosciara invisa</emphasis>
</taxonomicName>
