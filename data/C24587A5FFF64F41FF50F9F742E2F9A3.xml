<materialsCitation id="FA843CEEFFF64F41FF50F995430CFB37" ID-GBIF-Occurrence="1416807046" box="[151,1141,1222,1247]" location="Harz" pageId="16" pageNumber="17" specimenCount="1" stateProvince="Harz">
