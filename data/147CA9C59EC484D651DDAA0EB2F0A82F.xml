<taxonomicName id="F3B1FF9D983D86ADA5811C5ACD561AE7" authority="Thunb." class="Magnoliopsida" family="Penaeaceae" genus="Olinia" kingdom="Plantae" order="Myrtales" phylum="Tracheophyta" rank="genus">
