<emphasis id="0CF1821DD90CFFBBFF1E105FFF56FAA2" bold="true" box="[184,198,1281,1299]" pageId="10" pageNumber="339">G</emphasis>
, subgenital plate; 
<emphasis id="0CF1821DD90CFFBBFEF9105FFEFDFAA2" bold="true" box="[351,365,1281,1299]" pageId="10" pageNumber="339">H</emphasis>
