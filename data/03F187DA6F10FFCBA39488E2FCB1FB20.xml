<taxonomicName id="4C584D4F6F10FFCBA2658D3EFD5CFB04" baseAuthorityName="Pilsbry" baseAuthorityYear="1921" box="[358,648,1129,1153]" class="Bivalvia" family="Cardiidae" genus="Frigidocardium" kingdom="Animalia" order="Cardiida" pageId="71" pageNumber="72" phylum="Mollusca" rank="species" species="thaanumi">
