<geoCoordinate id="17D5502B247BBD22FD1DD914182FA471" box="[701,784,1878,1904]" degrees="7" direction="south" minutes="10" orientation="latitude" pageId="101" pageNumber="102" precision="925" value="-7.1666665">7°10’S</geoCoordinate>
