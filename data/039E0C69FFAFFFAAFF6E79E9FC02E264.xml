<emphasis id="B943616DFFAFFFA5FF6E79E9FD4EE0CA" bold="true" box="[178,674,906,928]" pageId="7" pageNumber="908">
<taxonomicName id="4C37C6FCFFAFFFA5FF6E79E9FE12E0CA" authority="(TAUSCHER 1806)" baseAuthorityName="TAUSCHER" baseAuthorityYear="1806" box="[178,510,906,928]" class="Insecta" family="Lymantriidae" genus="Orgyia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="908" phylum="Arthropoda" rank="species" species="dubia">
<emphasis id="B943616DFFAFFFA5FF6E79E9FED7E0CA" bold="true" box="[178,315,906,928]" italics="true" pageId="7" pageNumber="908">Orgyia dubia</emphasis>
