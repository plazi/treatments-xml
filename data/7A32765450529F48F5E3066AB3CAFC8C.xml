<taxonomicName id="359BBCC150529F49F5E3066AB3A9FA9C" authority="Deschodt and Davis" authorityName="Deschodt and Davis" box="[151,964,1294,1320]" class="Insecta" family="Scarabaeidae" genus="Scarabaeus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="507" phylum="Arthropoda" rank="species" species="soutpansbergensis" subGenus="Scarabaeolus">
