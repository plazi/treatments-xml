<collectingRegion id="49AA39873875FF8AFD07FDCB297AFD95" box="[743,844,548,573]" country="China" name="Guangxi" pageId="11" pageNumber="508">Guangxi</collectingRegion>
), 
<collectingCountry id="F379B7F53875FF8AFCBFFDCB2988FD95" box="[863,958,548,573]" name="Vietnam" pageId="11" pageNumber="508">Vietnam</collectingCountry>
