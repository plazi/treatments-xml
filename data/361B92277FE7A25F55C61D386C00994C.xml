<taxonomicName id="5D9F8CB59B18DA3E0DAD4FB37A455365" genus="Proctotrupoidea" lsidName="Proctotrupoidea perkinsi" pageId="0" pageNumber="7936" rank="species" species="perkinsi">perkinsi</taxonomicName>
