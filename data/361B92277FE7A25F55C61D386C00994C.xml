<taxonomicName id="09AF9F19E66D2EC4B197EEA1C3604A3B" authority="Townes" authorityName="Townes" class="Hexapoda" family="Proctotrupidae" genus="Tretoserphus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tretoserphus" order="Hymenoptera" pageId="0" pageNumber="7936" phylum="Arthropoda" rank="genus">Tretoserphus</taxonomicName>
