<taxonomicName id="BFDF4D50FF83D265FC09FECBFBE18D0D" box="[974,1104,297,323]" class="Gymnolaemata" family="Phidoloporidae" genus="Reteporella" kingdom="Animalia" order="Cheilostomatida" pageId="28" pageNumber="329" phylum="Bryozoa" rank="genus">
