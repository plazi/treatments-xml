<taxonomicName id="BFDF4D50FF83D265FCF1FF00FB488CB2" authority="Nardo (Bezerra &amp; Coelho 2006)" authorityName="Nardo (Bezerra &amp; Coelho" authorityYear="2006" box="[822,1273,225,252]" class="Demospongiae" family="Irciniidae" genus="Ircinia" kingdom="Animalia" order="Dictyoceratida" pageId="28" pageNumber="329" phylum="Porifera" rank="genus">
