<paragraph id="8B8436F16368FF8EFBECFAB0ABD0FAD0" blockId="6.[1040,1228,1280,1304]" box="[1040,1228,1280,1304]" pageId="6">
<heading id="D0CC819D6368FF8EFBECFAB0ABD0FAD0" bold="true" box="[1040,1228,1280,1304]" centered="true" fontSize="10" level="2" pageId="6" reason="2">
