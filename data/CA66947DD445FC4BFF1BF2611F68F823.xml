<materialsCitation id="F2A72F36D442FC4BFEF0F21F1DB9F90B" ID-GBIF-Occurrence="3314111301" collectingDate="2018-09-21" collectionCode="R, ZIN" collectorName="N. Simov &amp; Bekchiev, I &amp; P. Beron" country="Vietnam" elevation="2992" latitude="22.3056" location="Fansipan" longLatPrecision="1" longitude="103.77625" pageId="5" pageNumber="536" specimenCount="1" stateProvince="Lao Cai" typeStatus="holotype">
<typeStatus id="9D749BC9D442FC4BFEF0F21F1F9EF92F" box="[380,487,1662,1688]" pageId="5" pageNumber="536">Holotype</typeStatus>
, male, 
<collectingCountry id="3AD865FBD442FC4BFDBBF21F1CE0F92F" box="[567,665,1662,1688]" name="Vietnam" pageId="5" pageNumber="536">Vietnam</collectingCountry>
, 
<collectingRegion id="800BEB89D442FC4BFD2FF21E1D13F92F" box="[675,874,1662,1689]" country="Vietnam" name="Lao Cai" pageId="5" pageNumber="536">Lao Cai Province</collectingRegion>
, 
<location id="471073B0D442FC4BFCFAF21F1DA2F92F" LSID="urn:lsid:plazi:treatment:CA66947DD445FC4BFF1BF2611F68F823:471073B0D442FC4BFCFAF21F1DA2F92F" box="[886,987,1662,1688]" country="Vietnam" latitude="22.3056" longLatPrecision="1" longitude="103.77625" name="Fansipan" pageId="5" pageNumber="536" stateProvince="Lao Cai">Fansipan</location>
