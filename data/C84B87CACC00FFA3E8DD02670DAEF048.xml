<subSubSection id="08F86557CC00FFA3E8DD02C20C7CF7E9" pageId="22" pageNumber="23" type="description">
<paragraph id="405D36DCCC00FFA3E8DD02C20C7CF7E9" blockId="22.[151,1437,614,698]" pageId="22" pageNumber="23">
<emphasis id="7296EACECC00FFA3E8DD02C20E9AF7C8" box="[151,389,645,667]" italics="true" pageId="22" pageNumber="23">
<taxonomicName id="87E24D5FCC00FFA3E8DD02C20FF2F7C8" box="[151,237,645,667]" class="Hydrozoa" family="Lafoeidae" genus="Filellum" kingdom="Animalia" order="Leptothecata" pageId="22" pageNumber="23" phylum="Cnidaria" rank="genus">Filellum</taxonomicName>
