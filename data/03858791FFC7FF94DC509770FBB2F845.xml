<typeStatus id="54978825FFC7FF94DC3F90A7FC62F900" box="[855,945,1731,1755]" pageId="1" pageNumber="32">neotype</typeStatus>
of 
<emphasis id="B958EA95FFC7FF94DCB090A7FB6EF900" box="[984,1213,1731,1755]" italics="true" pageId="1" pageNumber="32">
<taxonomicName id="4C2C4D04FFC7FF94DCB090A7FB6AF900" box="[984,1209,1731,1755]" class="Liliopsida" family="Orchidaceae" genus="Cheirostylis" kingdom="Plantae" order="Asparagales" pageId="1" pageNumber="32" phylum="Tracheophyta" rank="species" species="seidenfadeniana">C. seidenfadeniana</taxonomicName>
