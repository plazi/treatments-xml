<taxonomicName id="4C2C4D04FFC7FF94DBAC90C7FA4AF960" box="[1220,1433,1699,1723]" class="Liliopsida" family="Orchidaceae" genus="Cheirostylis" kingdom="Plantae" order="Asparagales" pageId="1" pageNumber="32" phylum="Tracheophyta" rank="species" species="seidenfadeniana">
