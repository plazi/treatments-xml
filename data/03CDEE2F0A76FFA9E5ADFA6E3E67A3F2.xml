<taxonomicName id="4C6424BA0A76FFA9E5DDF8BC3BD7A106" authority="Gill, 1858" authorityName="Gill" authorityYear="1858" box="[143,452,1805,1827]" class="Actinopterygii" family="Loricariidae" genus="Pterygoplichthys" kingdom="Animalia" order="Siluriformes" pageId="60" pageNumber="61" phylum="Chordata" rank="genus">
