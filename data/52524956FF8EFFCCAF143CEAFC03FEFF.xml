</taxonomicName>
<collectionCode id="BCEA6085FF8EFFCFAF143999FED9F918" box="[264,345,1719,1745]" pageId="13" pageNumber="20">KOELZ</collectionCode>
, 
<number id="6C33EA67FF8EFFCFAF6C3999FE2BF918" box="[368,427,1719,1745]" pageId="13" pageNumber="20" value="1939.0">1939</number>
[Jalalabad, E 
<collectingCountry id="A2ECB8D0FF8EFFCFAC763999FD7CF918" box="[618,764,1719,1745]" name="Afghanistan" pageId="13" pageNumber="20">Afghanistan</collectingCountry>
]; 
<taxonomicName id="1DFB83C3FF8EFFCFAD013999FBD7F919" box="[797,1111,1719,1744]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="13" pageNumber="20" phylum="Chordata" rank="subSpecies" species="cinereus" subSpecies="caschmirensis">
<emphasis id="E88F2452FF8EFFCFAD013999FBD7F919" box="[797,1111,1719,1744]" italics="true" pageId="13" pageNumber="20">P. cinereus caschmirensis</emphasis>
</taxonomicName>
<bibRefCitation id="BE6A85B1FF8EFFCFAA743999FAABF918" author="HARTERT" box="[1128,1323,1719,1745]" pageId="13" pageNumber="48" refString="Hartert, E. (1905) Die Vogel der palaarktischen Fauna, I (Heft 3). Friedlander &amp; Sohn, Berlin, 144 pp, 241 - 384." type="book chapter" year="1905">
HARTERT, 
<number id="6C33EA67FF8EFFCFAAEC3999FAABF918" box="[1264,1323,1719,1745]" pageId="13" pageNumber="20" value="1905.0">1905</number>
</bibRefCitation>
[Gilgit].
</paragraph>
<paragraph id="DA44F840FF8EFFCFAF143800FC34F8B9" blockId="13.[264,1323,1838,1905]" pageId="13" pageNumber="20">
<emphasis id="E88F2452FF8EFFCFAF143800FDE7F881" bold="true" box="[264,615,1838,1864]" pageId="13" pageNumber="20">
