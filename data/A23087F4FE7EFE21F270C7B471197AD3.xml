<taxonomicName id="ED994D61FE7EFE21F270C40472707C58" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[122,310,581,602]" class="Malacostraca" family="Albuneidae" genus="Albunea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="392" pageNumber="393" phylum="Arthropoda" rank="species" species="symnista">
