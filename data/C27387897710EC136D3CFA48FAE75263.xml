<taxonomicName id="8DDA4D1C7710EC136D0CF9B0FD82524A" box="[312,608,1593,1618]" class="Insecta" family="Enderleinellidae" genus="Enderleinellus" kingdom="Animalia" order="Phthiraptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="marmotae">
