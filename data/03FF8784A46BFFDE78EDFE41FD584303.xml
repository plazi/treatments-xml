<taxonomicName id="4C564D11A46BFFDE7888FD21FC9D4358" authorityName="Milne-Edwards" authorityYear="1872" box="[700,819,751,774]" class="Mammalia" family="Talpidae" genus="Scaptonyx" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="11" phylum="Chordata" rank="genus">
