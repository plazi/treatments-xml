<taxonomicName id="4C564D11A46BFFDE7960FD7AFC7E4295" authorityName="Illiger" authorityYear="1811" box="[852,976,692,715]" class="Mammalia" family="Talpidae" genus="Condylura" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="11" phylum="Chordata" rank="genus">
