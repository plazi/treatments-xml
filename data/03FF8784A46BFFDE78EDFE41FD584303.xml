<taxonomicName id="4C564D11A46BFFDE78D6FCC2FCFE437D" authorityName="Guldenstadt" authorityYear="1777" box="[738,848,780,803]" class="Mammalia" family="Talpidae" genus="Desmana" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="11" phylum="Chordata" rank="genus">
