<taxonomicName id="4C564D11A46BFFDE7976FD21FC284358" authorityName="Linnaeus" authorityYear="1758" box="[834,902,751,774]" class="Mammalia" family="Talpidae" genus="Talpa" kingdom="Animalia" order="Soricomorpha" pageId="10" pageNumber="11" phylum="Chordata" rank="genus">
