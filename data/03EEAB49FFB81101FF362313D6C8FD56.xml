<collectingCountry id="F3505ACFFFA51101FEC627FAD5A8FD82" box="[359,426,511,536]" name="China" pageId="16" pageNumber="359">China</collectingCountry>
(Fujian, Guangxi, 
<taxonomicName id="4C4761DCFFA51101FDDF27FAD6D1FD82" box="[638,723,511,536]" class="Insecta" family="Sciaridae" genus="Taiwan" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="364" phylum="Arthropoda" rank="genus">
<collectingCountry id="F3505ACFFFA51101FDDF27FAD6D1FD82" box="[638,723,511,536]" name="Taiwan" pageId="16" pageNumber="359">Taiwan</collectingCountry>
</taxonomicName>
, Zhejiang, 
<figureCitation id="137C06DAFFA51101FCF127FAD7A3FD82" box="[848,929,511,536]" captionStart="FIGURE 11" captionStartId="20.[151,250,1457,1479]" captionTargetBox="[168,1423,207,1408]" captionTargetId="figure@20.[151,1436,195,1436]" captionTargetPageId="20" captionText="FIGURE 11. Geographical distribution of Dolichosciara from China." httpUri="https://zenodo.org/record/222863/files/figure.png" pageId="16" pageNumber="359">
Fig. 
<number id="3D8F0878FFA51101FC222405D7A3FD82" box="[899,929,512,536]" pageId="16" pageNumber="359" value="11.0">11</number>
