<taxonomicName id="4C714D30FF8D8935FC55FE9CCC93FEB4" authority="Schmidt, 1933" authorityName="Schmidt" authorityYear="1933" class="Amphibia" family="Hylidae" genus="Hyla" kingdom="Animalia" order="Anura" pageId="98" pageNumber="99" phylum="Chordata" rank="species" species="bromeliacia">
