<taxonomicName id="26CDF2A655EC5CDF4722E9D7A19FBB38" baseAuthorityName="Dejean" baseAuthorityYear="1826" class="Hexapoda" family="Carabidae" genus="Chlaenius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="613" pageNumber="614" phylum="Arthropoda" rank="species" species="rufipes">
<emphasis id="A0536CE9C72BD78821333BEC7CCAE01B" italics="true" pageId="613" pageNumber="614">Chlaenius rufipes</emphasis>
</taxonomicName>
