<taxonomicName id="3F15B2969568C3CE718E3E152295DD89" class="Hexapoda" family="Carabidae" genus="Chlaenius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="613" pageNumber="614" phylum="Arthropoda" rank="species" species="brevicollis">
