<quantity id="4CC366C14906FA18FD00FA55FF6DF9A0" metricMagnitude="2" metricUnit="m" metricValue="1.175" metricValueMax="2.2" metricValueMin="0.15" pageId="43" pageNumber="748" unit="m" value="117.5" valueMax="220.0" valueMin="15.0">15- 220 m</quantity>
(
<treatmentCitation id="0A9AED354906FA18FF72FA75FE37F9A3" author="BROCH H." box="[244,440,1512,1538]" page="26" pageId="43" pageNumber="748" year="1927">
<bibRefCitation id="EFAAB6D54906FA18FF72FA75FE37F9A3" author="BROCH H." box="[244,440,1512,1538]" pageId="43" pageNumber="748" pagination="11 - 38" refId="ref23041" refString="BROCH H. 1927 b. - Studies on Moroccan Cirripeds (Atlantic coast). Bulletin de la Societe des Sciences naturelles du Maroc 7: 11 - 38." type="journal article" year="1927">Broch 1927b: 26</bibRefCitation>
</treatmentCitation>
; 
<treatmentCitation id="0A9AED354906FA18FE4EFA75FD27F9A3" author="UTINOMI H." box="[456,680,1512,1538]" page="402" pageId="43" pageNumber="748" year="1959">
<bibRefCitation id="EFAAB6D54906FA18FE4EFA75FD27F9A3" author="UTINOMI H." box="[456,680,1512,1538]" pageId="43" pageNumber="748" pagination="400 - 404" refId="ref26211" refString="UTINOMI H. 1959. - Cirripedia Thoracica from the Western Mediterranean. Resultats des Campagnes du &quot; Professeur Lacaze-Duthiers &quot;. I: Algerie, 1952. Vie et Milieu 10 (4): 400 - 404." type="journal article" year="1959">Utinomi 1959: 402</bibRefCitation>
</treatmentCitation>
; 
<treatmentCitation id="0A9AED354906FA18FF11F995FEE9F983" author="STUBBINGS H. G." box="[151,358,1544,1570]" page="3" pageId="43" pageNumber="748" year="1963">
<bibRefCitation id="EFAAB6D54906FA18FF11F995FEE9F983" author="STUBBINGS H. G." box="[151,358,1544,1570]" pageId="43" pageNumber="748" pagination="1 - 39" refId="ref25909" refString="STUBBINGS H. G. 1963. - Cirripedia of the tropical South Atlantic coast of Africa. Expedition oceanographique belge dans les eaux cotieres africaines de l'Atlantique Sud (1948 - 1949), Resultats scientifiques, Bruxelles 3 (10): 1 - 39." type="journal article" year="1963">Stubbings 1963: 3</bibRefCitation>
</treatmentCitation>
, 
<treatmentCitation id="0A9AED354906FA18FEF1F995FE64F983" author="STUBBINGS H. G." box="[375,491,1544,1570]" page="287" pageId="43" pageNumber="748" year="1967">
<bibRefCitation id="EFAAB6D54906FA18FEF1F995FE64F983" author="STUBBINGS H. G." box="[375,491,1544,1570]" pageId="43" pageNumber="748" pagination="229 - 319" refId="ref26031" refString="STUBBINGS H. G. 1967. - The cirriped fauna of tropical West Africa. Bulletin of the British Museum of Natural History, Zoology 15 (6): 229 - 319, 1 pl." type="journal article" year="1967">1967: 287</bibRefCitation>
</treatmentCitation>
) but also known to range as far north as 
<collectingCountry id="F32C8BB44906FA18FE3BF9B5FD94F9E3" box="[445,539,1576,1602]" name="United Kingdom" pageId="43" pageNumber="748">England</collectingCountry>
(
<bibRefCitation id="EFAAB6D54906FA18FDA9F9B5FF5BF9C3" author="SOUTHWARD A. J." pageId="43" pageNumber="748" pagination="199 - 210" refId="ref25744" refString="SOUTHWARD A. J. 1995. - Occurrence in the English Channel of a warm-water cirripede, Solidobalanus fallax. Jounal of the Marine Biological Association of United Kingdom 75: 199 - 210." type="journal article" year="1995">Southward 1995</bibRefCitation>
). This is the first record for the Canary Islands.
</treatmentCitationGroup>
</paragraph>
