<paragraph id="8B84CB244906FA18FF11FA35FF66F923" blockId="43.[151,684,1415,1666]" pageId="43" pageNumber="748">
<treatmentCitationGroup id="AB2BEC0A4906FA18FF11FA35FF66F923" pageId="43" pageNumber="748">
<taxonomicName id="4C3BB0A74906FA18FF11FA35FEE5FA63" baseAuthorityName="Broch" baseAuthorityYear="1927" box="[151,362,1448,1474]" class="Maxillopoda" family="Archaeobalanidae" genus="Solidobalanus" kingdom="Animalia" order="Sessilia" pageId="43" pageNumber="748" phylum="Arthropoda" rank="species" species="fallax">
<emphasis id="B94F17364906FA18FF11FA35FEE5FA63" box="[151,362,1448,1474]" italics="true" pageId="43" pageNumber="748">Solidobalanus fallax</emphasis>
</taxonomicName>
