<taxonomicName id="ADEF4D21FFAAFFE584FD592BFEA4F9CD" box="[229,350,1637,1662]" class="Diplopoda" family="Parajulidae" genus="Paraiulus" higherTaxonomySource="CoL" kingdom="Animalia" order="Julida" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="dampfi">
