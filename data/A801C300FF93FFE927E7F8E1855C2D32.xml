<number id="96606031FF94FFEA2478FF6882752FDA" box="[776,828,188,212]" pageId="4" pageNumber="75" value="1.12">1.12</number>
, pronotum width at posterior part 
<number id="96606031FF94FFEA23CFFF6885BA2FDA" box="[1215,1267,188,212]" pageId="4" pageNumber="75" value="0.75">0.75</number>
, elytron length 
<number id="96606031FF94FFEA27E7FF0B81822FF9" box="[151,203,223,247]" pageId="4" pageNumber="75" value="2.64">2.64</number>
, elytron maximum width 
<number id="96606031FF94FFEA269AFF0B83572FF9" box="[490,542,223,247]" pageId="4" pageNumber="75" value="0.66">0.66</number>
, elytron width at humeral part 
<number id="96606031FF94FFEA2404FF0B82E12FF9" box="[884,936,223,247]" pageId="4" pageNumber="75" value="0.55">0.55</number>
, profemur 
<number id="96606031FF94FFEA2351FF0B851C2FF9" box="[1057,1109,223,247]" pageId="4" pageNumber="75" value="0.62">0.62</number>
, mesofemur 
<number id="96606031FF94FFEA2394FF0B84512FF9" box="[1252,1304,223,247]" pageId="4" pageNumber="75" value="0.67">0.67</number>
, metafemur 
<number id="96606031FF94FFEA27E7FED081822E12" box="[151,203,260,284]" pageId="4" pageNumber="75" value="0.79">0.79</number>
, protibia 
<number id="96606031FF94FFEA264EFED0803B2E12" box="[318,370,260,284]" pageId="4" pageNumber="75" value="0.46">0.46</number>
, mesotibia 
<number id="96606031FF94FFEA268AFED083672E12" box="[506,558,260,284]" pageId="4" pageNumber="75" value="0.61">0.61</number>
, metatibia 
<number id="96606031FF94FFEA25C3FED083AE2E12" box="[691,743,260,284]" pageId="4" pageNumber="75" value="0.73">0.73</number>
, protarsus 
<number id="96606031FF94FFEA2419FED082D42E12" box="[873,925,260,284]" pageId="4" pageNumber="75" value="0.53">0.53</number>
, mesotarsus 
<number id="96606031FF94FFEA2346FED085232E12" box="[1078,1130,260,284]" pageId="4" pageNumber="75" value="0.53">0.53</number>
, metatarsus 
<number id="96606031FF94FFEA238DFED084672E12" box="[1277,1326,260,284]" pageId="4" pageNumber="75" value="0.61">0.61</number>
