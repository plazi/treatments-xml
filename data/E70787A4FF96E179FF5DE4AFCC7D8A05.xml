<subSubSection id="27B46539FF92E179FF0DE0F9CA5989F6" box="[199,1318,439,464]" pageId="31" pageNumber="32" type="etymology">
<paragraph id="6F1136B2FF92E179FF0DE0F9CA5989F6" blockId="31.[151,1436,151,572]" box="[199,1318,439,464]" pageId="31" pageNumber="32">
<emphasis id="5DDAEAA0FF92E179FF0DE0F9CE3189E9" bold="true" box="[199,334,440,464]" inLineHeading="true" pageId="31" pageNumber="32" reason="1">Etymology.</emphasis>
