<materialsCitation id="3B6ED728FFCAFFD76CFE8944A549FAAE" box="[995,1299,1321,1345]" collectionCode="NMNS" pageId="17" pageNumber="18" specimenCode="NMNS 8174-010" specimenCount="1" typeStatus="paratype">
