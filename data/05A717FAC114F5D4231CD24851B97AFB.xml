<taxonomicName id="B9ECCC6EF52B399068652F84485BD7C3" class="Magnoliopsida" family="Apocynaceae" genus="Apocynum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Apocynum cannabinum" order="Gentianales" pageId="16" pageNumber="295" phylum="Tracheophyta" rank="species" species="cannabinum">Apocynum cannabinum</taxonomicName>
