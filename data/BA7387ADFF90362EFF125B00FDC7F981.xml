<quantity id="F5229B5EFF903628FC865A75FC33F8D8" box="[771,919,1884,1909]" metricMagnitude="1" metricUnit="m" metricValue="3.38" metricValueMax="4.57" metricValueMin="2.19" pageId="27" pageNumber="328" unit="m" value="33.8" valueMax="45.7" valueMin="21.9">21.9–45.7 m</quantity>
