<taxonomicName id="4C014D00476FFFADCBD7F9BDFDADF9BB" box="[476,557,1620,1643]" class="Maxillopoda" family="Diaixidae" genus="Diaixis" kingdom="Animalia" order="Calanoida" pageId="1" pageNumber="218" phylum="Arthropoda" rank="genus">
<emphasis id="B975EA91476FFFADCBD7F9BDFDADF9BB" box="[476,557,1620,1643]" italics="true" pageId="1" pageNumber="218">Diaixis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C014D00476FFFADC848F9BBFD4CF9BB" box="[579,716,1618,1643]" class="Maxillopoda" family="Scolecitrichidae" genus="Cenognatha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Calanoida" pageId="1" pageNumber="218" phylum="Arthropoda" rank="genus">
