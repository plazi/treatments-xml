<taxonomicName id="4C014D00476FFFADC870F9C7FD71F997" box="[635,753,1582,1607]" class="Maxillopoda" family="Diaixidae" genus="Ranthaxus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Calanoida" pageId="1" pageNumber="218" phylum="Arthropoda" rank="genus">
