<subSubSection id="C31F655E950FFFD353CF7334FC65F86B" pageId="5" pageNumber="523" type="distribution">
<paragraph id="8BBA36D5950FFFD353CF7334FDA1F8B3" blockId="5.[151,1436,1837,1971]" pageId="5" pageNumber="523">
<emphasis id="B971EAC7950FFFD353CF7334FEDEF89E" bold="true" box="[199,352,1837,1862]" pageId="5" pageNumber="523">Distribution.</emphasis>
<collectingRegion id="49C1F837950FFFD352637337FE15F89E" box="[363,427,1838,1862]" country="New Zealand" name="West Coast" pageId="5" pageNumber="523">Coast</collectingRegion>
of 
<collectingCountry id="F3127645950FFFD352DF7334FDA0F89E" box="[471,542,1837,1862]" name="Brazil" pageId="5" pageNumber="523">Brazil</collectingCountry>
, 
<collectingRegion id="49C1F837950FFFD351257334FDD0F89E" box="[557,622,1837,1862]" country="Brazil" name="Bahia" pageId="5" pageNumber="523">Bahia</collectingRegion>
(
