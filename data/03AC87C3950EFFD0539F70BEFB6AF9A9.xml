<collectingRegion id="49C1F837950EFFD2526472CBFE12F932" box="[364,428,1746,1770]" country="New Zealand" name="West Coast" pageId="4" pageNumber="522">Coast</collectingRegion>
of 
<collectingCountry id="F3127645950EFFD252D972C8FDA6F932" box="[465,536,1745,1770]" name="Brazil" pageId="4" pageNumber="522">Brazil</collectingCountry>
</materialsCitation>
, 
<materialsCitation id="3B6D3C88950EFFD2512B72C8FCF8F932" ID-GBIF-Occurrence="1656647337" box="[547,838,1745,1770]" country="Brazil" location="Bahia" pageId="4" pageNumber="522" specimenCount="1" stateProvince="Bahia" typeStatus="holotype">
