<taxonomicName id="4C4727AD6A32FFB96ECB99D5B54243AB" authority="(Gray, 1857)" authorityName="Gray" authorityYear="1857" baseAuthorityName="Gray" baseAuthorityYear="1857" box="[151,561,152,178]" class="Anthozoa" family="Myriopathidae" genus="Tanacetipathes" kingdom="Animalia" order="Antipatharia" pageId="9" pageNumber="376" phylum="Cnidaria" rank="species" species="hirta">
