<quantity id="5A261FAD80E7FF44EC6CC9711801090C" box="[263,348,1880,1904]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="227" pageNumber="228" unit="m" value="15.0" valueMax="30.0" valueMin="0.0">0–30 m</quantity>
