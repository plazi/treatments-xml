<collectingRegion id="5F1A7CAA80E7FF44EDF5C93918300954" box="[158,365,1807,1832]" country="Australia" name="Western Australia" pageId="227" pageNumber="228">Western Australia</collectingRegion>
), 
<collectingRegion id="5F1A7CAA80E7FF44ECEBC9381B150954" box="[384,584,1807,1832]" country="Australia" name="New South Wales" pageId="227" pageNumber="228">New South Wales</collectingRegion>
