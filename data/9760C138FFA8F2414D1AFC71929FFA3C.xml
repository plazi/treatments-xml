<taxonomicName id="D8C90BADFFA8F2414A04FBE59290FBFC" authorityName="Greuter" authorityYear="2003" baseAuthorityName="Arcang." box="[1062,1448,1090,1114]" class="Magnoliopsida" family="Asteraceae" genus="Limbarda" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="83" phylum="Tracheophyta" rank="subSpecies" species="crithmoides" subSpecies="longifolia">
<emphasis id="2DBDAC3CFFA8F2414A04FBE59238FBFC" box="[1062,1280,1090,1114]" italics="true" pageId="8" pageNumber="83">Limbarda crithmoides</emphasis>
