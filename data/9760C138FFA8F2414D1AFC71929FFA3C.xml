<collectingRegion id="DD0DBECCFFA8F2414DA6FBA593ACFBBC" box="[900,1172,1026,1050]" pageId="8" pageNumber="83">Secteur Incudine-Bavella</collectingRegion>
, 
<location id="1A1626F5FFA8F2414ABDFBA59286FBBC" LSID="urn:lsid:plazi:treatment:9760C138FFA8F2414D1AFC71929FFA3C:1A1626F5FFA8F2414ABDFBA59286FBBC" box="[1183,1470,1026,1050]" name="fond de la baie de Tizzano" pageId="8" pageNumber="83" stateProvince="Secteur Incudine-Bavella">fond de la baie de Tizzano</location>
