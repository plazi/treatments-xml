<taxonomicName id="4C2B4D0BFFFCC46AE01CFB001F7AFB78" authorityName=": Schmitz" authorityYear="1923" box="[1051,1134,1218,1237]" class="Insecta" family="Phoridae" genus="Dohrniphora" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="1943" phylum="Arthropoda" rank="species" species="dispar">
