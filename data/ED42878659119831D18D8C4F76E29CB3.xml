. 
<location id="6034604B59109831D068894B74A19CC7" LSID="urn:lsid:plazi:treatment:ED42878659119831D18D8C4F76E29CB3:6034604B59109831D068894B74A19CC7" box="[370,387,619,645]" country="Tanzania" name="It" pageId="69" pageNumber="70">It</location>
