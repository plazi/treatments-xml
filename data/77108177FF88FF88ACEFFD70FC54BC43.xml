<taxonomicName id="38B94BE2FF88FF88AEEEFC58FD22BC44" box="[662,763,1002,1027]" class="Insecta" family="Gerridae" genus="Eurygerris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="beieri">
<emphasis id="CDCDEC73FF88FF88AEEEFC58FD22BC44" box="[662,763,1002,1027]" italics="true" pageId="2" pageNumber="3">E. beieri</emphasis>
</taxonomicName>
