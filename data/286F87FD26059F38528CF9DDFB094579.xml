<taxonomicName id="67C64D6826049F385083FD7DFCC344A1" box="[664,841,692,716]" class="Insecta" family="Formicidae" genus="Calyptomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
