<taxonomicName id="4C43FAB39144FF86B75C930EFD61FC80" authorityName="Nel" authorityYear="2009" box="[346,670,778,802]" class="Insecta" family="Paragonophlebiidae" genus="Paragonophlebia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="7" pageNumber="63" phylum="Arthropoda" rank="species" species="inexpectata">
