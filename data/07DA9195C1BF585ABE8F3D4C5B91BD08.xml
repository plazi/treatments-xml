<geoCoordinate id="ACD1CEB4A63D1FA81958209F4C36D463" degrees="16" direction="north" minutes="12" orientation="latitude" precision="1" seconds="28.3" value="16.20786">16°12'28.3&quot;N</geoCoordinate>
