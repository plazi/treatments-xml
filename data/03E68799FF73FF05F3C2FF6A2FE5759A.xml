<taxonomicName id="4C4F4D0CFF73FF05F05DFEDC2D887506" authorityName="Zelazny" authorityYear="1981" box="[776,896,289,313]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="species" species="quinalayoi">
