' (in Matthews' hand, back corners also cut off); ' 
<taxonomicName id="4C4F819DE32CFFF086E35AEFFBCBEBD7" box="[1009,1136,942,967]" class="Bryopsida" family="Hypnaceae" genus="Ptilium" kingdom="Plantae" order="Hypnales" pageId="4" pageNumber="237" phylum="Bryophyta" rank="species" species="angulicolle">angulicolle</taxonomicName>
<typeStatus id="54F444BCE32CFFF0816B5AEEFB0AEBD7" box="[1145,1201,943,967]" pageId="4" pageNumber="237">
<collectingCounty id="62918292E32CFFF0816B5AEEFB0AEBD7" box="[1145,1201,943,967]" pageId="4" pageNumber="237">Type</collectingCounty>
