<taxonomicName id="4C404D77FD9B287CFB41D14BFA7DF89D" authority="Benoit" authorityName="Benoit" authorityYear="1954" box="[1256,1436,1796,1818]" class="Insecta" family="Dryinidae" genus="Neodryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="564" pageNumber="565" phylum="Arthropoda" rank="species" species="antiquus">
