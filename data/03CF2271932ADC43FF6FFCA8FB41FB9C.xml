<number id="3DAE8140932ADC43FC7FFFC5FC17FD04" box="[903,935,706,732]" pageId="4" pageNumber="63" value="31.0">31</number>
; VI, 
<number id="3DAE8140932ADC43FC19FFC5FBB2FD04" box="[993,1026,706,732]" pageId="4" pageNumber="63" value="27.0">27</number>
