<number id="3DAE8140932ADC43FF18FFC5FF4DFD04" box="[224,253,706,732]" pageId="4" pageNumber="63" value="17.0">17</number>
marginal between asters, 
<number id="3DAE8140932ADC43FDD4FFC5FDF9FD04" box="[556,585,706,732]" pageId="4" pageNumber="63" value="10.0">10</number>
anterior; III, 
<number id="3DAE8140932ADC43FD1EFFC5FCB7FD04" box="[742,775,706,732]" pageId="4" pageNumber="63" value="20.0">20</number>
; IV, 
<number id="3DAE8140932ADC43FCC4FFC5FCEDFD04" box="[828,861,706,732]" pageId="4" pageNumber="63" value="26.0">26</number>
