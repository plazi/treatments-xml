<collectionCode id="ED770BA2932ADC43FDA9FFEDFD2EFCDC" box="[593,670,746,772]" pageId="4" pageNumber="63">AWIV</collectionCode>
, 
<number id="3DAE8140932ADC43FD51FFEDFD50FCDC" box="[681,736,746,772]" pageId="4" pageNumber="63" value="0.47">0.47</number>
