<number id="3DAE8140932ADC43FE6BFF75FE03FD54" box="[403,435,626,652]" pageId="4" pageNumber="63" value="13.0">13</number>
; II, 
<number id="3DAE8140932ADC43FE1AFF75FDB2FD54" box="[482,514,626,652]" pageId="4" pageNumber="63" value="16.0">16</number>
; III–IV, 
<number id="3DAE8140932ADC43FD9CFF75FD34FD54" box="[612,644,626,652]" pageId="4" pageNumber="63" value="18.0">18</number>
; V, 
<number id="3DAE8140932ADC43FD49FF75FD61FD54" box="[689,721,626,652]" pageId="4" pageNumber="63" value="16.0">16</number>
; VI, 
<number id="3DAE8140932ADC43FCF3FF75FC9BFD54" box="[779,811,626,652]" pageId="4" pageNumber="63" value="15.0">15</number>
; VII, 
<number id="3DAE8140932ADC43FC97FF75FC3FFD54" box="[879,911,626,652]" pageId="4" pageNumber="63" value="14.0">14</number>
; VIII, 
<number id="3DAE8140932ADC43FC25FF75FC4DFD54" box="[989,1021,626,652]" pageId="4" pageNumber="63" value="12.0">12</number>
