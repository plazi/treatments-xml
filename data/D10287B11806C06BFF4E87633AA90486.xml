</taxonomicName>
(
<taxonomicName id="9EAB4D24180BC06BFB7B85753C620411" box="[1186,1310,1123,1150]" class="Magnoliopsida" family="Rubiaceae" kingdom="Plantae" order="Gentianales" pageId="26" pageNumber="327" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
