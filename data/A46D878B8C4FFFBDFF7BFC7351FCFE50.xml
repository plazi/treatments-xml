<taxonomicName id="EBC44D1E8C49FFBDFE62FE235152FE57" box="[398,537,450,475]" class="Insecta" family="Cicadellidae" genus="Rhabdotalebra" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="57" phylum="Arthropoda" rank="species" species="jamaicensis">
