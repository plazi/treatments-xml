<taxonomicName id="EBC44D1E8C49FFBDFDD0FF79518BFF3D" box="[572,704,152,177]" class="Insecta" family="Cicadellidae" genus="Rhabdotalebra" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="57" phylum="Arthropoda" rank="species" species="litoralensis">
