<taxonomicName id="34934D7DFFD6C96DFF2BFD62FD2A5EC4" authority="Lee &amp; Ahn" authorityName="Lee &amp; Ahn" box="[151,737,727,754]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="517" phylum="Arthropoda" rank="species" species="semidentiventris" status="sp. nov." subGenus="Datomicra">
<emphasis id="C1E7EAECFFD6C96DFF2BFD62FF2D5EC7" bold="true" box="[151,230,727,753]" italics="true" pageId="9" pageNumber="517">Atheta</emphasis>
(
<emphasis id="C1E7EAECFFD6C96DFF4BFD62FEB35EC7" bold="true" box="[247,376,727,753]" italics="true" pageId="9" pageNumber="517">Datomicra</emphasis>
) 
<emphasis id="C1E7EAECFFD6C96DFE35FD62FD855EC7" bold="true" box="[393,590,727,753]" italics="true" pageId="9" pageNumber="517">semidentiventris</emphasis>
Lee &amp; Ahn
</taxonomicName>
, 
<taxonomicNameLabel id="DAD45797FFD6C96DFD52FD6DFC875EC4" box="[750,844,728,754]" pageId="9" pageNumber="517" rank="species">sp. nov.</taxonomicNameLabel>
