<subSubSection id="02C940B6D42BFF82FF03FC5DA1E9FBD4" pageId="6" pageNumber="53" type="etymology">
<paragraph id="4A6C133DD42BFF82FF03FC5DA1E9FBD4" blockId="6.[151,1437,151,1229]" pageId="6" pageNumber="53">
<emphasis id="78A7CF2FD42BFF82FF03FC5DA275FC2A" bold="true" box="[197,339,978,1004]" pageId="6" pageNumber="53">Etymology.</emphasis>
<emphasis id="78A7CF2FD42BFF82FE9AFC5CA293FC2A" box="[348,437,979,1004]" italics="true" pageId="6" pageNumber="53">amydro</emphasis>
