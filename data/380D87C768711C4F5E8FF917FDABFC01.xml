<number id="066C24F6687F1C4F5DB3FE41FC46FDFA" box="[939,949,511,535]" pageId="8" pageNumber="47" value="1.0">1</number>
+ 
<number id="066C24F6687F1C4F5DDFFE41FC22FDFA" box="[967,977,511,535]" pageId="8" pageNumber="47" value="1.0">1</number>
+ 
<number id="066C24F6687F1C4F5DFBFE41FC00FDFA" box="[995,1011,511,535]" pageId="8" pageNumber="47" value="6.0">6</number>
