<taxonomicName id="4C014D455F64FFAFFDEBEF26FD084B4D" authorityName="De Meyer" authorityYear="1995" box="[578,679,1123,1149]" class="Insecta" family="Pipunculidae" genus="Tomosvaryella" kingdom="Animalia" order="Diptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="jubata">
