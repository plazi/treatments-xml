<taxonomicName id="4C1714EBFFDFFFA95A260173FC0CF9DE" authorityName="Blaimer &amp; Fisher" authorityYear="2013" box="[818,941,1538,1564]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="tricolor" status="stat. rev.">
<emphasis id="B963B37AFFDFFFA95A260173FC0CF9DE" box="[818,941,1538,1564]" italics="true" pageId="47" pageNumber="48">C. tricolor</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2500E01FFDFFFA95AA00173FBB9F9DE" box="[948,1048,1538,1564]" pageId="47" pageNumber="48">stat. rev.</taxonomicNameLabel>
