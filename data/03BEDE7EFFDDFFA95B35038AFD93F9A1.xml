<taxonomicName id="4C1714EBFFDFFFA95DE90173FAD7F9DE" authorityName="Blaimer &amp; Fisher" authorityYear="2013" baseAuthorityName="Dalla Torre" baseAuthorityYear="1893" box="[1277,1398,1538,1564]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="dentata" status="stat. nov.">
<emphasis id="B963B37AFFDFFFA95DE90173FAD7F9DE" box="[1277,1398,1538,1564]" italics="true" pageId="47" pageNumber="48">C. dentata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2500E01FFDFFFA959A90157FE8BF982" box="[189,298,1574,1600]" pageId="47" pageNumber="48">stat. nov.</taxonomicNameLabel>
and 
<taxonomicName id="4C1714EBFFDFFFA9587F0157FE50F9FD" authority="Blaimer &amp; Fisher, 2013" authorityName="Blaimer &amp; Fisher" authorityYear="2013" box="[363,497,1574,1600]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="ramamy" status="sp. nov.">
<emphasis id="B963B37AFFDFFFA9587F0157FE50F9FD" box="[363,497,1574,1600]" italics="true" pageId="47" pageNumber="48">C. ramamy</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2500E01FFDFFFA958E90157FDFAF982" box="[509,603,1574,1600]" pageId="47" pageNumber="48" rank="species">sp. nov.</taxonomicNameLabel>
