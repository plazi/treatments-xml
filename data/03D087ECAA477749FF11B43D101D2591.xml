<paragraph id="8BC636FAAA44774EFF40B64C175F209E" blockId="26.[151,1437,954,2025]" lastBlockId="27.[151,1436,152,216]" lastPageId="27" lastPageNumber="28" pageId="26" pageNumber="27">
<emphasis id="B90DEAE8AA44774FFF40B64C14E22538" bold="true" box="[198,499,1380,1406]" pageId="26" pageNumber="27">
Description of 
<typeStatus id="54C28858AA44774FFEF8B64C14FF2538" box="[382,494,1380,1406]" pageId="26" pageNumber="27">holotype</typeStatus>
