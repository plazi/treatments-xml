<number id="3DB124DDAA44774FFCC1B4E7167227AF" box="[839,867,1999,2025]" pageId="26" pageNumber="27" value="15.0">15</number>
/ 
<number id="3DB124DDAA44774FFCEBB4E7169827AF" box="[877,905,1999,2025]" pageId="26" pageNumber="27" value="15.0">15</number>
