<taxonomicName id="10675DB20B56D8E176AD4205D6FBB081" genus="S." lsidName="S. fuscipennis" pageId="17" pageNumber="30" rank="subspecies" species="fuscipennis" subspecies="rubripes">
<emphasis id="637942B5D179A33840782193A6583C4D" italics="true" pageId="17" pageNumber="30">S. fuscipennis rubripes</emphasis>
