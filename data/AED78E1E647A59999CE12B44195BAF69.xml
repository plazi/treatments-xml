<paragraph id="5C1F9198AA6D657DE3BC6FC40394D88F" pageId="17" pageNumber="30">
<bibRefCitation id="9BCB8DB389C26E325A4D1B9B32819FAD" author="Meyer, R" journalOrPublisher="Archiv fuer Naturgeschichte" pageId="23" pageNumber="36" pagination="1 - 12" refId="B37" refString="Meyer, R, 1924. Zur Bienengattung Sphecodes. Archiv fuer Naturgeschichte 90: 1 - 12" title="Zur Bienengattung Sphecodes" volume="90" year="1924">Meyer 1924</bibRefCitation>
