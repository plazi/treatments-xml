<taxonomicName id="7682C684FFE7FFFEA988FABFFDD8D058" box="[410,544,1286,1311]" class="Insecta" family="Mantispidae" genus="Buyda" kingdom="Animalia" order="Neuroptera" pageId="30" pageNumber="431" phylum="Arthropoda" rank="species" species="phthisica">
