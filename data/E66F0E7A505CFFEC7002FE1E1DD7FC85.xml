<specimenCode id="3E601717505CFFEC70EDFCE61A35FCE4" box="[925,1069,810,831]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="2" pageNumber="353">AMNH 5465</specimenCode>
