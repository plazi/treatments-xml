<geoCoordinate id="EFB9506EFFC9FF8BFC32FB67FBB1FB42" box="[897,1031,1158,1184]" degrees="08" direction="east" minutes="02.00" orientation="longitude" pageId="20" pageNumber="221" precision="9" value="8.033334">08°02.00’E</geoCoordinate>
