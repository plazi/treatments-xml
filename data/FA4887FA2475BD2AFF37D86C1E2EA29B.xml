<geoCoordinate id="17D5502B2472BD2BFDE6DD6B1994A042" box="[582,683,809,835]" degrees="10" direction="north" minutes="31" orientation="latitude" pageId="108" pageNumber="109" precision="925" value="10.516666">10°31’N</geoCoordinate>
and 
<geoCoordinate id="17D5502B2472BD2BFD46DD6B186DA042" box="[742,850,809,835]" degrees="66" direction="west" minutes="20" orientation="longitude" pageId="108" pageNumber="109" precision="925" value="-66.333336">66°20’W</geoCoordinate>
in the Coastal Cordillera in 
<collectingCountry id="0AF6767C2472BD2BFB04DD6B1E22A042" box="[1188,1309,809,835]" name="Venezuela" pageId="108" pageNumber="109">Venezuela</collectingCountry>
