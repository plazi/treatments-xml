<geoCoordinate id="17D5502B2475BD2CFD2CD89E19D5A5F2" box="[652,746,1756,1779]" degrees="10" direction="north" minutes="31" orientation="latitude" pageId="107" pageNumber="108" precision="925" value="10.516666">10°31’N</geoCoordinate>
, 
<geoCoordinate id="17D5502B2475BD2CFD5AD89E1863A5F2" box="[762,860,1756,1779]" degrees="66" direction="west" minutes="20" orientation="longitude" pageId="107" pageNumber="108" precision="925" value="-66.333336">66°20’W</geoCoordinate>
, 
<quantity id="B5199B092475BD2CFCCAD89E18D6A5F2" box="[874,1001,1756,1779]" metricMagnitude="2" metricUnit="m" metricValue="8.0" metricValueMax="9.0" metricValueMin="7.0" pageId="107" pageNumber="108" unit="m" value="800.0" valueMax="900.0" valueMin="700.0">700–900 m</quantity>
, 
<date id="065F102C2475BD2CFC58D89E1FD6A5F2" box="[1016,1257,1756,1779]" pageId="107" pageNumber="108" value="1993-02-19" valueMax="1993-02-22" valueMin="1993-02-19">19–22 February 1993</date>
