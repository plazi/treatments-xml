<quantity id="B5199B092472BD2BFF13DD121BC2A06B" box="[179,253,848,874]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="108" pageNumber="109" unit="m" value="800.0">800 m</quantity>
