<paragraph id="725E36EC2475BD2CFF37D8FE1909A412" blockId="107.[151,1436,1724,1812]" pageId="107" pageNumber="108">
Type: 
<collectingCountry id="0AF6767C2475BD2CFF7BD8FE1A42A5D2" box="[219,381,1724,1747]" name="Venezuela" pageId="107" pageNumber="108">VENEZUELA</collectingCountry>
