<paragraph id="8BBE6815D22E0260FC95FC6B54AE370D" blockId="33.[826,1468,926,1043]" pageId="33" pageNumber="2815">
<taxonomicName id="4C011396D22E0260FC95FC6B552F30A8" box="[828,1069,926,950]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="33" pageNumber="2815" phylum="Firmicutes" rank="subSpecies" species="sakei" subSpecies="carnosus">
<emphasis id="B975B407D22E0260FC95FC6B528A30A8" box="[828,904,926,950]" italics="true" pageId="33" pageNumber="2815">L. sakei</emphasis>
