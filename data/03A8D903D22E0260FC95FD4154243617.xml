<taxonomicName id="4C011396D22E0260FAD0FC6B55AC30CB" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="33" pageNumber="2815" phylum="Firmicutes" rank="subSpecies" species="curvatus" subSpecies="melibiosus">
