<taxonomicName id="C98D4D73FFD6FF94FE81F85734E2F80C" authorityName="Ragonot" authorityYear="1888" box="[310,405,1990,2011]" class="Insecta" family="Pyralidae" genus="Cathyalia" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="519" phylum="Arthropoda" rank="genus">
