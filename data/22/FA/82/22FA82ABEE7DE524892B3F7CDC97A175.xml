<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25940" class="Insecta" family="Formicidae" genus="Atta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Atta laevigata (Smith)" order="Hymenoptera" pageNumber="425" phylum="Arthropoda" rank="species" species="laevigata">Atta laevigata Fred</taxonomicName>
