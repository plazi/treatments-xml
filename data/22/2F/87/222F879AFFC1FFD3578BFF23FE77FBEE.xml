From 
<taxonomicName authority="Latin" authorityName="Latin" box="[405,630,980,1005]" class="Gastropoda" family="Endodontidae" genus="Mautodontha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="20" pageNumber="21" phylum="Mollusca" rank="species" species="occidentalis">
