<subSubSection pageId="0" pageNumber="63" type="reference_group">
<paragraph pageId="0" pageNumber="63">
<taxonomicName class="Insecta" family="Tenthredinidae" genus="Nematus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nematus platycerus" order="Hymenoptera" pageId="0" pageNumber="63" phylum="Arthropoda" rank="species" species="platycerus">Nematus platycerus</taxonomicName>
