<taxonomicName authority="ENGEL, 2001" authorityName="Engel" authorityYear="2001" box="[973,1170,1158,1179]" class="Insecta" family="Apidae" genus="Succinapis" kingdom="Animalia" order="Hymenoptera" pageId="116" phylum="Arthropoda" rank="species" species="goeleti" status="new species">Succinapis goeleti</taxonomicName>
