<collectingCountry box="[366,436,1901,1927]" name="China" pageId="33" pageNumber="34">China</collectingCountry>
(
<collectingCountry box="[451,542,1901,1927]" name="Taiwan" pageId="33" pageNumber="34">Taiwan</collectingCountry>
