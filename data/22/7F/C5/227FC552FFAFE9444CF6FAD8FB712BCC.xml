<number box="[561,678,1849,1875]" pageId="10" pageNumber="11" value="3.3499999999999996" valueMax="3.4" valueMin="3.3">3.30–3.40</number>
(n = 
<number box="[740,757,1849,1875]" pageId="10" pageNumber="11" value="2.0">2</number>
); females 
<number box="[878,995,1849,1875]" pageId="10" pageNumber="11" value="3.65" valueMax="3.8" valueMin="3.5">3.50–3.80</number>
(n = 
<number box="[1057,1088,1849,1875]" pageId="10" pageNumber="11" value="11.0">11</number>
