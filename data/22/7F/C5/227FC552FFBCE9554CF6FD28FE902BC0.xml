), II 
<number box="[1241,1292,1541,1567]" pageId="27" pageNumber="28" value="3.55">3.55</number>
(
<number box="[1309,1364,1541,1567]" pageId="27" pageNumber="28" value="1.1">1.10</number>
, 
<number box="[1377,1431,1541,1567]" pageId="27" pageNumber="28" value="1.15">1.15</number>
, 
<number box="[151,205,1581,1607]" pageId="27" pageNumber="28" value="0.8">0.80</number>
, 
<number box="[217,257,1581,1607]" pageId="27" pageNumber="28" value="0.5">0.5</number>
