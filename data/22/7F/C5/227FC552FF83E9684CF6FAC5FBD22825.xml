<number box="[211,264,712,738]" pageId="38" pageNumber="39" value="0.11">0.11</number>
, AME 
<number box="[348,402,712,738]" pageId="38" pageNumber="39" value="0.09">0.09</number>
, PME 
<number box="[482,536,712,738]" pageId="38" pageNumber="39" value="0.06">0.06</number>
, AME-AME 
<number box="[696,750,712,738]" pageId="38" pageNumber="39" value="0.13">0.13</number>
, AME-ALE 
<number box="[901,955,712,738]" pageId="38" pageNumber="39" value="0.19">0.19</number>
