<emphasis bold="true" box="[151,288,1995,2017]" pageId="72" pageNumber="73">FIGURE 33.</emphasis>
<taxonomicName authority="Samaai &amp; Pillay &amp; Janson, 2019" authorityName="Samaai &amp; Pillay &amp; Janson" authorityYear="2019" box="[293,536,1995,2017]" class="Demospongiae" family="Dysideidae" genus="Euryspongia" kingdom="Animalia" order="Dictyoceratida" pageId="72" pageNumber="73" phylum="Porifera" rank="species" species="coeruleum" status="sp. nov.">
<emphasis box="[293,536,1995,2017]" italics="true" pageId="72" pageNumber="73">Euryspongia coeruleum</emphasis>
</taxonomicName>
<emphasis bold="true" box="[542,623,1995,2016]" pageId="72" pageNumber="73">
<taxonomicNameLabel box="[542,623,1995,2016]" pageId="72" pageNumber="73" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
A. 
<taxonomicName box="[660,724,1996,2017]" class="Demospongiae" family="Irciniidae" genus="Ircinia" kingdom="Animalia" order="Dictyoceratida" pageId="72" pageNumber="73" phylum="Porifera" rank="species" species="situ">
