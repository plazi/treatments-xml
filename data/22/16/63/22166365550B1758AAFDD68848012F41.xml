<subSubSection pageId="5" pageNumber="6" type="nomenclature">
<paragraph pageId="5" pageNumber="6">
<taxonomicName LSID="22166365-550B-1758-AAFD-D68848012F41" authority="Stearn" authorityName="Stearn" authorityYear="1938" class="Magnoliopsida" family="Berberidaceae" genus="Epimedium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Epimedium leptorrhizum" order="Ranunculales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="leptorrhizum">Epimedium leptorrhizum Stearn</taxonomicName>
</paragraph>
</subSubSection>
<subSubSection pageId="5" pageNumber="6" type="reference_group">
<paragraph pageId="5" pageNumber="6">
<taxonomicName authorityName="Stearn" authorityYear="1938" class="Magnoliopsida" family="Berberidaceae" genus="Epimedium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Epimedium leptorrhizum" order="Ranunculales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="leptorrhizum">Epimedium leptorrhizum</taxonomicName>
