<caption pageId="1" pageNumber="58" startId="1.[151,269,1869,1893]" targetBox="[164,1392,211,1819]" targetPageId="1">
<paragraph blockId="1.[151,1436,1869,1996]" pageId="1" pageNumber="58">
<emphasis bold="true" box="[151,356,1869,1893]" pageId="1" pageNumber="58">
FIGURES 1–6. 
<emphasis box="[338,356,1869,1893]" italics="true" pageId="1" pageNumber="58">B</emphasis>
</emphasis>
ireta ssp., adults. 1. 
<emphasis box="[587,725,1870,1893]" italics="true" pageId="1" pageNumber="58">
