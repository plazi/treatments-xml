7. 
<taxonomicName authority="Agassiz, 2019" authorityName="Agassiz" authorityYear="2019" box="[181,502,1160,1187]" class="Insecta" family="Yponomeutidae" genus="Yponomeuta" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="griseomacula" status="sp. nov.">
<emphasis bold="true" box="[181,502,1160,1187]" italics="true" pageId="9" pageNumber="10">Yponomeuta griseomacula</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[509,603,1161,1187]" pageId="9" pageNumber="10" rank="species">sp. nov.</taxonomicNameLabel>
