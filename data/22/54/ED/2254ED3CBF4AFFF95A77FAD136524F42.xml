<quantity box="[706,845,844,870]" metricMagnitude="2" metricUnit="m" metricValue="6.705" metricValueMax="7.92" metricValueMin="5.49" pageId="40" pageNumber="41" unit="m" value="670.5" valueMax="792.0" valueMin="549.0">549–792 m</quantity>
