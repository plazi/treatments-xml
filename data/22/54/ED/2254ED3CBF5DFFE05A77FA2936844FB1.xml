<taxonomicName authority="Britt." authorityName="Britt." box="[351,695,1179,1205]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="species" species="alleghaniensis">
