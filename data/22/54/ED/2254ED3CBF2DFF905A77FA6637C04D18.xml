</taxonomicName>
, 
<taxonomicName box="[402,517,1506,1532]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
), alder (
<taxonomicName box="[612,721,1506,1532]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[612,679,1506,1531]" italics="true" pageId="65" pageNumber="66">Alnus</emphasis>
sp.
</taxonomicName>
