: 
<collectingCounty box="[715,847,828,854]" pageId="36" pageNumber="37">Cocke Co.</collectingCounty>
, 
<location LSID="urn:lsid:plazi:treatment:2254ED3CBF49FFF55A77FEFC36124DE6:AF220AF1BF49FFF5581EFCDB3153483A" box="[865,1085,828,854]" county="Cocke Co." name="Foothills Parkway" pageId="36" pageNumber="37" stateProvince="Tennessee">Foothills Parkway</location>
south overlook. (
