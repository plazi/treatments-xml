</taxonomicName>
, 
<taxonomicName box="[620,744,1770,1796]" class="Magnoliopsida" family="Aceraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="family">Aceraceae</taxonomicName>
