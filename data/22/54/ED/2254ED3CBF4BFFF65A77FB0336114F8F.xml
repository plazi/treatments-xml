<taxonomicName box="[599,797,1105,1131]" class="Magnoliopsida" family="Caryophyllaceae" kingdom="Plantae" order="Caryophyllales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="family">Caryophyllaceae</taxonomicName>
) as preferred hosts. Others hosts listed by Crumb include clover (
<taxonomicName box="[536,687,1145,1171]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[536,645,1145,1170]" italics="true" pageId="39" pageNumber="40">Trifolium</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName box="[700,814,1145,1171]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
), hedgemustard (
<taxonomicName box="[1014,1270,1145,1170]" class="Magnoliopsida" family="Brassicaceae" genus="Sisymbrium" kingdom="Plantae" order="Brassicales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="species" species="officinale">
<emphasis box="[1014,1270,1145,1170]" italics="true" pageId="39" pageNumber="40">Sisymbrium officinale</emphasis>
</taxonomicName>
(L.) Scop., 
<taxonomicName baseAuthorityName="Passoa and Hollingsworth" baseAuthorityYear="1996" box="[360,516,1185,1211]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
), white heath 
<taxonomicName box="[726,781,1185,1211]" class="Magnoliopsida" family="Asteraceae" genus="Aster" kingdom="Plantae" order="Asterales" pageId="39" pageNumber="93" phylum="Tracheophyta" rank="genus">aster</taxonomicName>
(
<taxonomicName box="[814,1143,1185,1210]" class="Magnoliopsida" family="Asteraceae" genus="Symphyotrichum" kingdom="Plantae" order="Asterales" pageId="39" pageNumber="40" phylum="Tracheophyta" rank="species" species="ericoides">
