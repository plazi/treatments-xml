, 
<location LSID="urn:lsid:plazi:treatment:2254ED3CBF63FFDF5A77FDB6365A4DF2:AF220AF1BF63FFDF5806FC48315F48A5" box="[889,1073,943,969]" county="Swain Co." name="Big Cove Road" pageId="14" pageNumber="15" stateProvince="North Carolina">Big Cove Road</location>
, site p. (
<specimenCount box="[1173,1306,943,969]" pageId="14" pageNumber="15" type="generic">1 specimen</specimenCount>
