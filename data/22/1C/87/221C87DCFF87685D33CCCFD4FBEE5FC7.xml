</taxonomicName>
has been recorded from 
<collectingCountry box="[972,1039,405,428]" name="Japan" pageId="19" pageNumber="20">Japan</collectingCountry>
, 
<collectingCountry box="[1053,1123,404,428]" name="China" pageId="19" pageNumber="20">China</collectingCountry>
, 
<collectingCountry box="[1137,1222,404,428]" name="Taiwan" pageId="19" pageNumber="20">Taiwan</collectingCountry>
, 
<collectingCountry box="[1237,1366,404,428]" name="Philippines" pageId="19" pageNumber="20">Philippines</collectingCountry>
, 
<collectingCountry name="Vietnam" pageId="19" pageNumber="20">Vietnam</collectingCountry>
, 
<collectingCountry box="[213,270,441,464]" name="Laos" pageId="19" pageNumber="20">Laos</collectingCountry>
, 
<collectingCountry box="[281,387,439,463]" name="Malaysia" pageId="19" pageNumber="20">Malaysia</collectingCountry>
, 
<collectingCountry box="[398,514,439,463]" name="Singapore" pageId="19" pageNumber="20">Singapore</collectingCountry>
, 
<collectingCountry box="[525,635,441,465]" name="Myanmar" pageId="19" pageNumber="20">Myanmar</collectingCountry>
