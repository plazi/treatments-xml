<quantity box="[1199,1330,1546,1571]" metricMagnitude="2" metricUnit="m" metricValue="3.65" metricValueMax="5.5" metricValueMin="1.8" pageId="8" pageNumber="553" unit="m" value="365.0" valueMax="550.0" valueMin="180.0">180–550 m</quantity>
