<quantity box="[829,944,620,645]" metricMagnitude="2" metricUnit="m" metricValue="2.25" metricValueMax="4.0" metricValueMin="0.5" pageId="5" pageNumber="550" unit="m" value="225.0" valueMax="400.0" valueMin="50.0">50–400 m</quantity>
