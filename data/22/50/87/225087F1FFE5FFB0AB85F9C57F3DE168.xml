to the 
<collectingCountry box="[508,637,620,645]" name="Philippines" pageId="5" pageNumber="550">Philippines</collectingCountry>
, 
<collectingRegion box="[650,785,620,645]" country="Australia" name="Queensland" pageId="5" pageNumber="550">Queensland</collectingRegion>
