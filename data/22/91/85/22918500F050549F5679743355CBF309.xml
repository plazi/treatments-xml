<subSubSection pageId="7" pageNumber="32" type="reference_group">
<paragraph pageId="7" pageNumber="32">
<taxonomicName class="Insecta" family="Staphylinidae" genus="Quedius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Quedius richteri" order="Coleoptera" pageId="7" pageNumber="32" phylum="Arthropoda" rank="species" species="richteri">Quedius richteri</taxonomicName>
