<paragraph id="952F2F1719CE99660367FFAA1BF38A3E" pageId="0" pageNumber="1">
<taxonomicName id="52F60F20224226DF7B7C5181010D2A19" baseAuthorityName="Alcock" baseAuthorityYear="1902" class="Anthozoa" family="Fungiacyathidae" genus="Fungiacyathus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Fungiacyathus (B.) sibogae" order="Scleractinia" pageId="0" pageNumber="1" phylum="Cnidaria" rank="species" species="sibogae" subGenus="B.">Fungiacyathus (B.) sibogae</taxonomicName>
