<taxonomicName id="4C5FF0E9FC66FFEFF1AEFC66FBCDFC40" authorityName="WELLS" authorityYear="1993" box="[956,1064,911,933]" class="Insecta" family="Hydroptilidae" genus="Plethus" kingdom="Animalia" order="Trichoptera" pageId="39" pageNumber="1048" phylum="Arthropoda" rank="species" species="berbulu">
