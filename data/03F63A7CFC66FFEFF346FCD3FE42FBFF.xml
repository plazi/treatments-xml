und 
<taxonomicName id="4C5FF0E9FC66FFEFF64FFC66FB3DFC40" authorityName="ULMER" authorityYear="1951" box="[1117,1240,912,933]" class="Insecta" family="Hydroptilidae" genus="Plethus" kingdom="Animalia" order="Trichoptera" pageId="39" pageNumber="1048" phylum="Arthropoda" rank="species" species="cruciatus">
