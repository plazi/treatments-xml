<quantity id="A434A8EFFFB6E42D6FE0FEEED2B2947D" box="[1189,1235,321,347]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="43" pageNumber="44" unit="in" value="5.0">5 in</quantity>
