<taxonomicName id="55D84D5AFFD71760FE4DFA4EE1485C6E" box="[503,816,1519,1556]" class="Mammalia" family="Ptilodontidae" genus="Mesodma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Multituberculata" pageId="61" pageNumber="378" phylum="Chordata" rank="species" species="primaevus">
