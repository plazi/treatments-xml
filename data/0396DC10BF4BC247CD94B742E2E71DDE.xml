<geoCoordinate id="EE0B0BC1BF4BC247CD93B557E62C1F2A" box="[203,340,866,891]" degrees="39" direction="north" minutes="06" orientation="latitude" pageId="68" pageNumber="221" precision="15" seconds="13" value="39.10361">39°06′13″ N</geoCoordinate>
