<geoCoordinate id="EE0B0BC1BF4BC247CC06B557E6981F2A" box="[350,480,866,891]" degrees="9" direction="west" minutes="14" orientation="longitude" pageId="68" pageNumber="221" precision="15" seconds="47" value="-9.246389">9°14′47″ W</geoCoordinate>
