<emphasis id="18A9EA99737BFFCDFF37FE96FE94AF75" bold="true" box="[198,341,301,327]" pageId="4" pageNumber="59">Comments.</emphasis>
<taxonomicName id="EDDD4D08737BFFCDFEAEFE96FD5FAF74" box="[351,670,301,326]" class="Insecta" family="Encyrtidae" genus="Copidosomopsis" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="nacoleiae">
