<taxonomicName id="4C6CB499A76CED34B17FF815FE49A2EA" box="[293,511,1994,2016]" class="Ophiuroidea" family="Ophiolepididae" genus="Ophiomusium" kingdom="Animalia" order="Ophiurida" pageId="16" pageNumber="17" phylum="Echinodermata" rank="species" species="mutum">
