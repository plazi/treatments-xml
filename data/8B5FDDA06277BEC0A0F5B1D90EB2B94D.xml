<taxonomicName id="952787EAB562550F52CBBF429AE80C66" class="Insecta" family="Staphylinidae" genus="Gymnusa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gymnusa" order="Coleoptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="genus">Gymnusa</taxonomicName>
