<taxonomicName id="B01FB11EE11FE0E1D53438F23DC9AA20" class="Chilopoda" family="Lithobiidae" genus="Monotarsobius" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="99" phylum="Arthropoda" rank="subSpecies" species="microps" subSpecies="wardaranus">
<pageBreakToken id="840F7EF4D019E498D5BDD9414557818C" pageId="0" pageNumber="99" start="start">Monotarsobius</pageBreakToken>
microps wardaranus
</taxonomicName>
<taxonomicNameLabel id="593CA4848BD4D8BF56D17B55D3EC87D2" pageId="0" pageNumber="99" rank="subSpecies">n. subsp.</taxonomicNameLabel>
