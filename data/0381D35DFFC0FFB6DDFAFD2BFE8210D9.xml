<taxonomicName id="4C2819C8FFC0FFB6DDC8FC95FD9D1727" authorityName="Crawford. To" authorityYear="1914" box="[490,610,776,800]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="1965" phylum="Arthropoda" rank="species" species="psylloptera">
