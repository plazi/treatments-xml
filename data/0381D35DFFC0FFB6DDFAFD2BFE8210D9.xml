<taxonomicName id="4C2819C8FFC0FFB6DE7DFBF5FC5B1078" authority="Crawford. The" authorityName="Crawford. They" authorityYear="1913" box="[607,932,1127,1151]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="1965" phylum="Arthropoda" rank="species" species="psylloptera">
