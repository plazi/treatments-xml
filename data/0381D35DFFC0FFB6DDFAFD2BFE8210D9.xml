<taxonomicName id="4C2819C8FFC0FFB6DDA3FCB4FDE21747" authorityName="Crawford. To" authorityYear="1914" box="[385,541,808,832]" class="Insecta" family="Liviidae" genus="Paurocephala" kingdom="Animalia" order="Hemiptera" pageId="79" pageNumber="1965" phylum="Arthropoda" rank="species" species="psylloptera">
