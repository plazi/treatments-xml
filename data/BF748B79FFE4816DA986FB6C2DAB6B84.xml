<taxonomicName id="F0DD41ECFFE4816DA834FB252CA0699C" authority="Gerlach, 1950" authorityName="Gerlach" authorityYear="1950" box="[293,673,1228,1250]" class="Adenophorea" family="Axonolaimidae" genus="Diplopeltula" kingdom="Animalia" order="Araeolaimida" pageId="15" pageNumber="259" phylum="Nematoda" rank="species" species="breviceps">
