<subSubSection id="AF9D33BE237BFF83990FF826A5ADC208" pageId="10" pageNumber="183" type="etymology">
<paragraph id="E7386035237BFF83990FF826A5ADC208" blockId="10.[831,1436,1993,2050]" pageId="10" pageNumber="183">
<emphasis id="D5F3BC27237BFF83990FF826A2E2CDE8" bold="true" box="[831,950,1993,2017]" pageId="10" pageNumber="183">Etymology</emphasis>
