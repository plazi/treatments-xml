<collectingCountry id="F360760AFFE2FFCAFC217773D3EFF8F3" box="[909,1023,1827,1853]" name="Argentina" pageId="26" pageNumber="27">Argentina</collectingCountry>
, 
<collectingCountry id="F360760AFFE2FFCAFBA77773D470F8F3" box="[1035,1120,1827,1853]" name="Bolivia" pageId="26" pageNumber="27">Bolivia</collectingCountry>
, 
<collectingCountry id="F360760AFFE2FFCAFBC07773D4A3F8F3" box="[1132,1203,1827,1853]" name="Brazil" pageId="26" pageNumber="27">Brazil</collectingCountry>
, 
<collectingCountry id="F360760AFFE2FFCAFB137773D578F8F3" box="[1215,1384,1827,1853]" name="French Guiana" pageId="26" pageNumber="27">French Guiana</collectingCountry>
, and 
<collectingCountry id="F360760AFFE2FFCAFF3B7717D11AF8AF" box="[151,266,1863,1889]" name="Venezuela" pageId="26" pageNumber="27">Venezuela</collectingCountry>
(
<bibRefCitation id="EFE64B6BFFE2FFCAFEB67717D1D3F8AF" author="Metcalf, Z. P." box="[282,451,1863,1889]" pageId="26" pageNumber="27" pagination="1 - 585" refId="ref71633" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
