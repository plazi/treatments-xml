<taxonomicName id="29C3448DC536FF8AFD40D2BE9799B62B" class="Insecta" family="Lycidae" genus="Metriorrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="259" phylum="Arthropoda" rank="species" species="inaequalis">
<emphasis id="DCB7E31CC536FF8AFD40D2BE9460B5CB" box="[747,769,1507,1530]" italics="true" pageId="6" pageNumber="259">M</emphasis>
. 
<emphasis id="DCB7E31CC536FF8AFF22D15E9799B62B" box="[137,248,1539,1562]" italics="true" pageId="6" pageNumber="259">inaequalis</emphasis>
</taxonomicName>
or 
<taxonomicName id="29C3448DC536FF8AFE8BD15E96F8B62B" authorityName="Hope" authorityYear="1831" box="[288,409,1539,1562]" class="Insecta" family="Omalisidae" genus="Omalysus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="259" phylum="Arthropoda" rank="species" species="lineatus">
<emphasis id="DCB7E31CC536FF8AFE8BD15E96F8B62B" box="[288,409,1539,1562]" italics="true" pageId="6" pageNumber="259">M. lineatus</emphasis>
