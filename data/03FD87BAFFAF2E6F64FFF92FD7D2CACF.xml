<subSubSection id="C34E6527FFAF2E6864AFF8FDD38FCD93" pageId="67" pageNumber="68" type="diagnosis">
<paragraph id="8BEB36ACFFAF2E6864AFF8FDD7C8CDB7" blockId="67.[151,1436,1823,2029]" pageId="67" pageNumber="68">
<emphasis id="B920EABEFFAF2E6864AFF8FDD30ECD6A" bold="true" box="[199,321,1860,1885]" pageId="67" pageNumber="68">Diagnosis.</emphasis>
