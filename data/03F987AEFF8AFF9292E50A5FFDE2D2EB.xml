<taxonomicName id="4C504D3BFF8AFF9193320FF4FDCCD3C5" authority="Soliman" authorityName="Soliman" box="[320,634,1948,1970]" class="Insecta" family="Chrysididae" genus="Oligogaster" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="198" phylum="Arthropoda" rank="species" species="kimseyae" status="spec. nov.">
