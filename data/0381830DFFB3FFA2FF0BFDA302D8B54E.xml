<collectingCountry id="F33F728BFFB3FFA2FEFBF8C801F6B505" box="[359,413,1804,1830]" name="Laos" pageId="1" pageNumber="391">Laos</collectingCountry>
(
<collectingRegion id="49ECFCF9FFB3FFA2FE31F8CF025CB505" box="[429,567,1803,1830]" country="Laos" name="Champasak" pageId="1" pageNumber="391">Champasak</collectingRegion>
