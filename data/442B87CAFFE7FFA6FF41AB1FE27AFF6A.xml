<taxonomicName id="0B824D5FFFE0FFA4FE8BABA2E59DF88D" authority="Matsumura" authorityName="Matsumura" box="[349,764,1785,1808]" class="Insecta" family="Cicadellidae" genus="Dryodurgades" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="51" phylum="Arthropoda" rank="species" species="formosanus">
