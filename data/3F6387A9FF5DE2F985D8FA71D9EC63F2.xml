<taxonomicName id="70CA4D3CFF5DE2F98143F9E0D8026362" authority="Sears, 1953" authorityName="Sears" authorityYear="1953" box="[1036,1292,1586,1611]" class="Hydrozoa" family="Abylidae" genus="Abyla" kingdom="Animalia" order="Siphonophorae" pageId="176" pageNumber="177" phylum="Cnidaria" rank="species" species="tottoni">
