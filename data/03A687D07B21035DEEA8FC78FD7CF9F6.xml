<quantity id="4CF79B237B21035FEF49F96BFE39F923" box="[374,511,1735,1759]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" metricValueMax="2.3" metricValueMin="2.1" pageId="89" pageNumber="90" unit="mm" value="2.2" valueMax="2.3" valueMin="2.1">2.1–2.3 mm</quantity>
, width 
<quantity id="4CF79B237B21035FEC6EF96BFD1DF923" box="[593,731,1735,1759]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" metricValueMax="1.8" metricValueMin="1.4" pageId="89" pageNumber="90" unit="mm" value="1.6" valueMax="1.8" valueMin="1.4">1.4–1.8 mm</quantity>
