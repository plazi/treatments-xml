(
<emphasis id="B912E0564011FF82FE53F9FCFDAAF9B5" bold="true" box="[447,603,1565,1589]" italics="true" pageId="9" pageNumber="390">Bianigryllacris</emphasis>
) 
<emphasis id="B912E0564011FF82FD87F9FFFD09F9B6" bold="true" box="[619,760,1566,1590]" italics="true" pageId="9" pageNumber="390">tenuispinacia</emphasis>
