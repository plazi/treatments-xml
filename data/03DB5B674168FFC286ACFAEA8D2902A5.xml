<paragraph id="8BCDEA714168FFC3868CF8898BDD0505" blockId="24.[192,1441,1313,1914]" box="[224,976,1858,1882]" pageId="24" pageNumber="23">
<taxonomicName id="4C7291F24168FFC3868CF8898A7F0505" authority="K. Johnson" authorityName="K. Johnson" box="[224,626,1858,1882]" class="Insecta" family="Lycaenidae" genus="Antrissima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="23" phylum="Arthropoda" rank="species" species="varicolor">
