<taxonomicName id="4C3FAD9CC8227E45FB86FB7AEF4BFAB3" authority="Ulmer Apicodorsal" authorityName="Ulmer Apicodorsal" class="Insecta" family="Leptoceridae" genus="Grumichella" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="159" phylum="Arthropoda" rank="species" species="flaveola">
