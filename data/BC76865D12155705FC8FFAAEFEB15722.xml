<paragraph id="3460374B12155706FD27FA7DFBAE53B9" blockId="53.[701,1228,1408,1759]" box="[701,1112,1408,1428]" pageId="53" pageNumber="53">
<taxonomicName id="F3DF4CC812155706FD27FA7DFC9153B9" box="[701,871,1408,1428]" class="Elasmobranchii" family="Rhincodontidae" genus="Rhincodon" kingdom="Animalia" order="Orectolobiformes" pageId="53" pageNumber="53" phylum="Chordata" rank="species" species="typus">
<emphasis id="06ABEB5912155706FD27FA7DFC9153B9" box="[701,871,1408,1428]" italics="true" pageId="53" pageNumber="53">
