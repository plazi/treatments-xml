<taxonomicName id="4C1B93AADE49FFF1FF11EAA3FED7FA0C" authorityName="Tate" authorityYear="1942" box="[162,265,1464,1490]" class="Mammalia" family="Vespertilionidae" genus="Pipistrellus" kingdom="Animalia" order="Chiroptera" pageId="24" pageNumber="229" phylum="Chordata" rank="species" species="anthonyi">
