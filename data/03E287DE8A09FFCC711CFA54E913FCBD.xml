<taxonomicName id="4C4B4D4B8A09FFCF7286F934EEECF8E1" authorityName="S. Colla. Three" authorityYear="1932" box="[494,648,1774,1801]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Troglomyces" higherTaxonomySource="GBIF" kingdom="Fungi" order="Laboulbeniales" pageId="9" pageNumber="10" phylum="Ascomycota" rank="genus">
<emphasis id="B93FEADA8A09FFCF7286F934EEECF8E1" bold="true" box="[494,648,1774,1801]" italics="true" pageId="9" pageNumber="10">Troglomyces</emphasis>
