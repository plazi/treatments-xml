<subSubSection id="C354654AFFE62225999AFDC2FE36FC96" pageId="5" pageNumber="235" type="distribution">
<paragraph id="8BF136C1FFE62225999AFDC2FE12FD79" blockId="5.[170,1437,311,814]" pageId="5" pageNumber="235">
<emphasis id="B93AEAD3FFE62225999AFDC2FEC4FDB1" box="[170,308,532,557]" italics="true" pageId="5" pageNumber="235">Verbreitung</emphasis>
