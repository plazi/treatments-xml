<collectingRegion id="3314FF9CFFAAFFB7FBF15513BAE6FEE7" box="[1061,1225,312,338]" country="United States of America" name="Louisiana" pageId="18" pageNumber="19">LOUISIANA</collectingRegion>
</materialsCitation>
. 
<materialsCitation id="41B83B23FFAAFFB7FB015513BEF7FECF" collectorName="The" location="Oklahoma" pageId="18" pageNumber="19" specimenCount="1" stateProvince="Oklahoma">
<collectorName id="5C2554A8FFAAFFB7FB015513BB2CFEE7" box="[1237,1283,312,338]" pageId="18" pageNumber="19">The</collectorName>
single 
