<quantity id="4C879B7FCA17905B444CFBC477C86E40" box="[1376,1451,1095,1119]" metricMagnitude="-2" metricUnit="m" metricValue="8.4" pageId="15" pageNumber="288" unit="mm" value="84.0">84 mm</quantity>
