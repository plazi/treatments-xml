<quantity id="4C879B7FCA17905B42AFFBE271876E64" box="[899,996,1121,1146]" metricMagnitude="-2" metricUnit="m" metricValue="6.44" pageId="15" pageNumber="288" unit="mm" value="64.4">64.4 mm</quantity>
); distal metatarsal ITU no No. (TDD = 
<quantity id="4C879B7FCA17905B44AFFBE2713B6E8D" metricMagnitude="-2" metricUnit="m" metricValue="7.95" pageId="15" pageNumber="288" unit="mm" value="79.5">79.5 mm</quantity>
