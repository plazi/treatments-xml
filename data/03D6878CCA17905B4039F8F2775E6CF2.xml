<quantity id="4C879B7FCA17905B42EDFBFF76436E8A" box="[961,1056,1148,1172]" metricMagnitude="-2" metricUnit="m" metricValue="4.95" pageId="15" pageNumber="288" unit="mm" value="49.5">49.5 mm</quantity>
