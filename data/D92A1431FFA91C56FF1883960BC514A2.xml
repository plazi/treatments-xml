<date id="253D83E7FFAA1C57FEF287EC0B8B17A6" box="[272,454,215,239]" pageId="10" pageNumber="589" value="1970-11-09" valueMax="1970-12-10" valueMin="1970-11-09">9.XI-10.XII.1970</date>
, HFAM1 (mission CNRS, RCP 225) 
<specimenCount id="47856EAEFFAA1C57FECE87CA0B031643" box="[300,334,241,266]" pageId="10" pageNumber="589" type="female">1 ♀</specimenCount>
