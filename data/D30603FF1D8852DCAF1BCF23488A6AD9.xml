<taxonomicName id="DFC9744D23FF49335D1C21D8C73A71A6" authorityName="Madge" authorityYear="1967" class="Hexapoda" family="Carabidae" genus="Lebia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="813" pageNumber="814" phylum="Arthropoda" rank="species" species="lecontei">
<emphasis id="3F2B44C9C201D6DE742A869548A75C95" italics="true" pageId="813" pageNumber="814">Lebia lecontei</emphasis>
