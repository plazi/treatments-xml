<taxonomicName id="4C7D32E3494B153D94C4FC82F334E919" authorityName="Pandelle" authorityYear="1896" box="[1229,1375,799,824]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="584" phylum="Arthropoda" rank="species" species="uncicurva">
