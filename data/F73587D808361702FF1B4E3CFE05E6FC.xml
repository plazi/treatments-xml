<taxonomicName id="B89C4D4D08361707FDE84D43FC66E10C" authority="Yasumatsu (1950)" authorityName="Yasumatsu" authorityYear="1950" box="[612,959,1604,1628]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="48" phylum="Arthropoda" rank="species" species="nobilis">
<emphasis id="4DE8EADC08361707FDE84D43FD0FE10C" box="[612,726,1604,1628]" italics="true" pageId="3" pageNumber="48">L. nobilis</emphasis>
