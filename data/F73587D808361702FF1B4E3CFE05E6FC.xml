<number id="C95424E908301701FD434842FCDDE40C" box="[719,772,836,860]" pageId="5" pageNumber="50" value="0.87">0.87</number>
; CI 96, 94; EL 
<number id="C95424E908301701FC3F4842FC3EE40C" box="[947,999,836,860]" pageId="5" pageNumber="50" value="0.14">0.14</number>
