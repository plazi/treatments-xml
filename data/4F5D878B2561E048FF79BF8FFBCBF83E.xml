<taxonomicName id="00F44D1E2561E048FC6EBD18FBF8F8F1" box="[896,1045,1882,1908]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="japonicum">A. japonicum</taxonomicName>
