<taxonomicName id="00F44D1E2561E048FB50BD18FF2AF815" baseAuthorityName="De Prins &amp; De Prins" baseAuthorityYear="2018" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="pseudosieboldianum">
