<taxonomicName id="00F44D1E2561E048FD54BD3CFBC6F815" authorityName="E.Murray" authorityYear="1966" baseAuthorityName="Trautv. &amp; C.A.Mey." box="[698,1067,1917,1944]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="subSpecies" species="caudatum" subSpecies="ukurundense">
<emphasis id="F580EA8F2561E048FD54BD3CFCAAF815" box="[698,839,1917,1944]" italics="true" pageId="32" pageNumber="33">A. caudatum</emphasis>
