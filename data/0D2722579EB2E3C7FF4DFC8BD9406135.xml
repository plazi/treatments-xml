<taxonomicName id="428EE8C29EB2E3C8FEE8F847DAE46484" authorityName="Quicke &amp; Butcher" authorityYear="2012" box="[306,472,1982,2004]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="215" pageNumber="216" phylum="Arthropoda" rank="species" species="trisphaeropyx" status="sp. nov.">
