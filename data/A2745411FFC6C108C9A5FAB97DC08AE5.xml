<caption id="7EA2B58FFFC4C109C9A5FA2F7D3F8D4A" httpUri="https://zenodo.org/record/176431/files/figure.png" pageId="6" pageNumber="55" targetBox="[187,1353,199,1533]" targetPageId="6">
<paragraph id="2A62E507FFC4C109C9A5FA2F7D3F8D4A" blockId="6.[151,1436,1534,1627]" pageId="6" pageNumber="55">
