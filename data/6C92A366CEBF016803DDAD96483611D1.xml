<taxonomicName id="092B964E42FE653ECACEF644A4678C79" class="Insecta" family="Cerambycidae" genus="Tessaromma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tessaromma" order="Coleoptera" pageId="209" pageNumber="210" phylum="Arthropoda" rank="genus">Tessaromma</taxonomicName>
