<taxonomicName id="998C4D33FFB4FFD1FCD6FCFCEA2FFCB5" box="[891,1044,817,843]" class="Arachnida" family="Phytoseiidae" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="56" phylum="Arthropoda" rank="family">Phytoseiidae</taxonomicName>
