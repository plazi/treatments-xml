<taxonomicName id="998C4D33FFB4FFD1FBF2FCC4EAEFFCDC" box="[1119,1236,777,802]" class="Magnoliopsida" family="Fabaceae" genus="Senna" kingdom="Plantae" order="Fabales" pageId="1" pageNumber="56" phylum="Tracheophyta" rank="species" species="siamea">
