<subSubSection id="C3361782C7711811FF714F52FDD8F8E6" box="[199,580,1810,1835]" pageId="11" pageNumber="508" type="distribution">
<paragraph id="8B934409C7711811FF714F52FDD8F8E6" blockId="11.[151,1437,1017,1979]" box="[199,580,1810,1835]" pageId="11" pageNumber="508">
<emphasis id="B958981BC7711811FF714F52FEFCF8E6" bold="true" box="[199,352,1810,1835]" pageId="11" pageNumber="508">Distribution.</emphasis>
<collectingCountry id="F33B0499C7711811FED14F52FE32F8E6" box="[359,430,1810,1835]" name="China" pageId="11" pageNumber="508">China</collectingCountry>
: 
<collectingCountry id="F33B0499C7711811FE0F4F53FDDCF8E6" box="[441,576,1811,1835]" name="Hong Kong" pageId="11" pageNumber="508">Hong Kong</collectingCountry>
