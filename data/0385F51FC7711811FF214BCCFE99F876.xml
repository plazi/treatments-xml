, PL 
<quantity id="4CD4E9ECC7711811FF094E8AFEF9F92F" box="[191,357,1738,1763]" metricMagnitude="-4" metricUnit="m" metricValue="5.75" metricValueMax="5.8" metricValueMin="5.699999999999999" pageId="11" pageNumber="508" unit="mm" value="0.575" valueMax="0.58" valueMin="0.57">0.57–0.58 mm</quantity>
, PW 
<quantity id="4CD4E9ECC7711811FE124E8AFDD6F92F" box="[420,586,1738,1763]" metricMagnitude="-4" metricUnit="m" metricValue="5.35" metricValueMax="5.4" metricValueMin="5.3" pageId="11" pageNumber="508" unit="mm" value="0.535" valueMax="0.54" valueMin="0.53">0.53–0.54 mm</quantity>
, EL 
<quantity id="4CD4E9ECC7711811FD364E8AFCBAF92F" box="[640,806,1738,1763]" metricMagnitude="-4" metricUnit="m" metricValue="6.65" metricValueMax="6.7" metricValueMin="6.6" pageId="11" pageNumber="508" unit="mm" value="0.665" valueMax="0.67" valueMin="0.66">0.66–0.67 mm</quantity>
, EW 
<quantity id="4CD4E9ECC7711811FCD04E8BFB90F92F" box="[870,1036,1739,1763]" metricMagnitude="-3" metricUnit="m" metricValue="1.045" metricValueMax="1.08" metricValueMin="1.01" pageId="11" pageNumber="508" unit="mm" value="1.045" valueMax="1.08" valueMin="1.01">1.01–1.08 mm</quantity>
, AL 
<quantity id="4CD4E9ECC7711811FBF34E8BFB77F92F" box="[1093,1259,1739,1763]" metricMagnitude="-3" metricUnit="m" metricValue="1.06" metricValueMax="1.09" metricValueMin="1.03" pageId="11" pageNumber="508" unit="mm" value="1.06" valueMax="1.09" valueMin="1.03">1.03–1.09 mm</quantity>
, AW 
<quantity id="4CD4E9ECC7711811FA9A4E8BFF5AF8CB" metricMagnitude="-3" metricUnit="m" metricValue="1.145" metricValueMax="1.2" metricValueMin="1.09" pageId="11" pageNumber="508" unit="mm" value="1.145" valueMax="1.2" valueMin="1.09">1.09–1.20 mm</quantity>
