<taxonomicName id="22F04E861918FFB8FF6BF906FE56F90F" authority="Caradja, 1910" authorityName="Caradja" authorityYear="1910" box="[151,400,1698,1720]" class="Insecta" family="Pyralidae" genus="Assara" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="286" phylum="Arthropoda" rank="species" species="korbi">
