th sternite. 
<number id="533827221918FFB8FBC7F927FB8CF92E" box="[1083,1098,1667,1689]" pageId="3" pageNumber="286" value="6.0">6</number>
. 
<taxonomicName id="22F04E861918FFB8FBA9F920FA46F92E" authority="Zincken, 1818" authorityName="Zincken" authorityYear="1818" box="[1109,1408,1667,1689]" class="Insecta" family="Pyralidae" genus="Assara" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="286" phylum="Arthropoda" rank="species" species="terebrella">
