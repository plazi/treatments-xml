</taxonomicName>
and 
<specimenCount box="[1273,1314,1428,1452]" pageId="5" pageNumber="453" type="female">1 ♀</specimenCount>
on 
<taxonomicName authority="D. Don" authorityName="D. Don" class="Insecta" family="Noctuidae" genus="Cryptomeria" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="453" phylum="Arthropoda" rank="species" species="japonica">
<emphasis italics="true" pageId="5" pageNumber="453">Cryptomeria japonica</emphasis>
D. Don
</taxonomicName>
(
<taxonomicName box="[683,823,1462,1486]" class="Pinopsida" family="Taxodiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="5" pageNumber="453" phylum="Tracheophyta" rank="family">Taxodiaceae</taxonomicName>
), 
<date box="[841,959,1462,1486]" pageId="5" pageNumber="453" value="2020-11-16">16/XI/2020</date>
