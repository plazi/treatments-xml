<emphasis bold="true" box="[194,388,608,635]" italics="true" pageId="20" pageNumber="270">Anaceratagallia</emphasis>
(
<emphasis bold="true" box="[405,599,608,635]" italics="true" pageId="20" pageNumber="270">Anaceratagallia</emphasis>
) 
<emphasis bold="true" box="[616,728,608,634]" italics="true" pageId="20" pageNumber="270">fragariae</emphasis>
