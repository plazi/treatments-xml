<collectingRegion box="[499,598,367,393]" country="Russia" name="Buryatiya" pageId="9" pageNumber="259">Buryatia</collectingRegion>
, the 
<collectingRegion box="[650,739,367,393]" country="Mongolia" name="Selenge" pageId="9" pageNumber="259">Selenga</collectingRegion>
<location LSID="urn:lsid:plazi:treatment:76154D29AE59CF35FF79F9F8FDC4FD4A:FB63AAE4AE56CF35FD07FEBBFCC2FE32" box="[745,885,367,393]" county="Transbaikalia" municipality="Southern" name="River Valley" pageId="9" pageNumber="259" stateProvince="Buryatiya">River Valley</location>
