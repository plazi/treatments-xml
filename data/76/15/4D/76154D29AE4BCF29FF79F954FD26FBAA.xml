<materialsCitation ID-GBIF-Occurrence="3014906303" collectingDate="2000-07-04" country="Russia" location="Dosang Railway Station" municipality="Lower Volga Region" pageId="20" pageNumber="270" specimenCount="1" stateProvince="Astrakhan'">
1. 
<collectingCountry box="[227,304,1946,1972]" name="Russia" pageId="20" pageNumber="270">Russia</collectingCountry>
, 
<collectingMunicipality box="[315,549,1946,1972]" pageId="20" pageNumber="270">Lower Volga Region</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:76154D29AE4BCF29FF79F954FD26FBAA:FB63AAE4AE4BCF28FDC1F84EFC89F80F" box="[559,830,1946,1972]" country="Russia" municipality="Lower Volga Region" name="Dosang Railway Station" pageId="20" pageNumber="270" stateProvince="Astrakhan'">Dosang Railway Station</location>
ca. 
<locationDeviation box="[875,1016,1946,1972]" location="North" pageId="20" pageNumber="270">
