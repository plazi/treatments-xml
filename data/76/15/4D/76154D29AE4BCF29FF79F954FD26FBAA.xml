<paragraph blockId="21.[151,1437,150,1041]" pageId="21" pageNumber="271">
<materialsCitation ID-GBIF-Occurrence="3014906317" collectingDate="2004-07-15" country="Russia" location="Dyakovka Village" municipality="Lower Volga Region" pageId="21" pageNumber="271" specimenCount="1" stateProvince="Saratov">
2. 
<collectingCountry box="[228,305,151,177]" name="Russia" pageId="21" pageNumber="271">Russia</collectingCountry>
, 
<collectingMunicipality box="[318,556,151,177]" pageId="21" pageNumber="271">Lower Volga Region</collectingMunicipality>
, 
<collectingRegion box="[568,737,151,177]" country="Russia" name="Saratov" pageId="21" pageNumber="271">Saratov Oblast</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:76154D29AE4BCF29FF79F954FD26FBAA:FB63AAE4AE4ACF29FD00FF43FC0EFF0A" box="[750,953,151,177]" country="Russia" municipality="Lower Volga Region" name="Dyakovka Village" pageId="21" pageNumber="271" stateProvince="Saratov">Dyakovka Village</location>
ca. 
<locationDeviation box="[1001,1280,150,177]" location="Krasny Kut" pageId="21" pageNumber="271">
<quantity box="[1001,1073,150,177]" metricMagnitude="4" metricUnit="m" metricValue="3.5" pageId="21" pageNumber="271" unit="km" value="35.0">35 km</quantity>
SSW Krasny Kut
