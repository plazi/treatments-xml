<emphasis box="[740,952,583,608]" italics="true" pageId="6" pageNumber="7">Fistularia petimba</emphasis>
</taxonomicName>
, previously 
<taxonomicName authority="Klunzinger, 1871" authorityName="Klunzinger" authorityYear="1871" class="Actinopterygii" family="Fistulariidae" genus="Fistularia" kingdom="Animalia" order="Syngnathiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="villosa">
<emphasis box="[1097,1295,583,608]" italics="true" pageId="6" pageNumber="7">Fistularia villosa</emphasis>
Klunzinger, 
<number box="[151,207,620,644]" pageId="6" pageNumber="7" value="1871.0">1871</number>
</taxonomicName>
(see 
<bibRefCitation author="Avdeev" box="[270,437,620,645]" pageId="6" pageNumber="7" refString="Avdeev, V. V. (1979 a) New species of the genus Cymothoa Fabricius (Isopoda, Cymothoidae) from the Indian Ocean. Parazitologiya, Leningrad, 13, 223 - 234." type="book chapter" year="1979" yearSuffix="a">
Avdeev 
<number box="[365,424,620,644]" pageId="6" pageNumber="7" value="1979.0">1979</number>
a
</bibRefCitation>
; 
<bibRefCitation author="Williams" box="[450,687,620,645]" pageId="6" pageNumber="7" refString="Williams, E. H. Jr., Bunkley-Williams, L. &amp; Pitlik, T. (2000) Three new records for Micronesia of cymothoid isopods (Crustacea) parasitic on fishes. Pacific Science, 54 (2), 157 - 158." type="journal article" year="2000">
Williams 
<emphasis box="[561,620,620,645]" italics="true" pageId="6" pageNumber="7">et al.</emphasis>
<number box="[628,687,620,644]" pageId="6" pageNumber="7" value="2000.0">2000</number>
</bibRefCitation>
; 
<bibRefCitation author="Rameshkumar" box="[701,998,620,645]" pageId="6" pageNumber="7" refString="Rameshkumar, G., Ravichandran, S., Sivasubramanian, K. &amp; Trilles, J. - P. (2013) New occurrence of parasitic isopods from Indian fishes. Journal of Parasitic Diseases, 37, 42 - 46." type="journal article" year="2013">
Rameshkumar 
<emphasis box="[872,931,620,645]" italics="true" pageId="6" pageNumber="7">et al.</emphasis>
<number box="[940,998,620,644]" pageId="6" pageNumber="7" value="2013.0">2013</number>
</bibRefCitation>
) and bluespotted cornetfish 
<taxonomicName authority="Ruppell, 1838 (Williams et al. 2000)" authorityName="Ruppell, 1838 (Williams et al." authorityYear="2000" class="Actinopterygii" family="Fistulariidae" genus="Fistularia" kingdom="Animalia" order="Syngnathiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="commersonii">
<emphasis italics="true" pageId="6" pageNumber="7">Fistularia commersonii</emphasis>
Rüppell, 
<number box="[405,461,656,680]" pageId="6" pageNumber="7" value="1838.0">1838</number>
(
<bibRefCitation author="Williams" box="[476,710,655,680]" pageId="6" pageNumber="7" refString="Williams, E. H. Jr., Bunkley-Williams, L. &amp; Pitlik, T. (2000) Three new records for Micronesia of cymothoid isopods (Crustacea) parasitic on fishes. Pacific Science, 54 (2), 157 - 158." type="journal article" year="2000">
Williams 
<emphasis box="[586,642,655,680]" italics="true" pageId="6" pageNumber="7">et al.</emphasis>
<number box="[649,710,656,680]" pageId="6" pageNumber="7" value="2000.0">2000</number>
