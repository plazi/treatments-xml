<collectingCountry box="[731,819,1489,1513]" name="Ukraine" pageId="7" pageNumber="9">Ukraine</collectingCountry>
, 
<collectingCountry box="[830,929,1489,1513]" name="Hungary" pageId="7" pageNumber="9">Hungary</collectingCountry>
