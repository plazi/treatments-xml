<taxonomicName authorityName="Liu &amp; Wang" authorityYear="2019" box="[798,908,1813,1837]" class="Insecta" family="Crambidae" genus="Tylostega" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="209" phylum="Arthropoda" rank="species" species="vittiformis">
