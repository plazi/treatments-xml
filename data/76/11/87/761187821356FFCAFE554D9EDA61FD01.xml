(Lesson, 1831)
</taxonomicName>
(
<taxonomicName box="[553,718,1373,1397]" class="Aves" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="2662" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
, 
<taxonomicName box="[732,871,1373,1397]" class="Aves" family="Tyrannidae" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="2662" phylum="Chordata" rank="family">Tyrannidae</taxonomicName>
), Guajuvira Farm, Fênix, 
<collectingRegion box="[125,278,1405,1429]" country="Brazil" name="Parana" pageId="11" pageNumber="2662">Paraná State</collectingRegion>
, 
<collectingCountry box="[293,360,1405,1429]" name="Brazil" pageId="11" pageNumber="2662">Brazil</collectingCountry>
(23 
<emphasis bold="true" box="[410,420,1405,1429]" italics="true" pageId="11" pageNumber="2662">°</emphasis>
539S, 51 
<emphasis bold="true" box="[522,532,1405,1429]" italics="true" pageId="11" pageNumber="2662">°</emphasis>
579W), 
<date box="[626,815,1405,1429]" pageId="11" pageNumber="2662" value="2003-03-27">27 March 2003</date>
, coll. A. Bispo. 
<typeStatus box="[1011,1119,1405,1429]" pageId="11" pageNumber="2662">Holotype</typeStatus>
and 
<typeStatus box="[125,235,1437,1461]" pageId="11" pageNumber="2662">paratypes</typeStatus>
