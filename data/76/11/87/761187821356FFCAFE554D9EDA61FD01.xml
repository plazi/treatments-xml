Male 
<typeStatus box="[200,304,1342,1366]" pageId="11" pageNumber="2662">holotype</typeStatus>
, 
<specimenCount box="[324,458,1342,1366]" pageId="11" pageNumber="2662" type="male">seven male</specimenCount>
and 
<specimenCount box="[532,681,1342,1366]" pageId="11" pageNumber="2662" type="female">eight female</specimenCount>
