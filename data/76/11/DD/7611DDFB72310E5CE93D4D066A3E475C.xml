<subSubSection pageId="5" pageNumber="76" type="nomenclature">
<paragraph pageId="5" pageNumber="76">
<taxonomicName LSID="urn:lsid:zoobank.org:act:22B8C261-0710-4C23-85B6-54EA58333235" authority="Valim &amp; Palma" class="Insecta" family="Philopteridae" genus="Philopteroides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Philopteroides pilgrimi" order="Psocodea" pageId="5" pageNumber="76" phylum="Arthropoda" rank="species" species="pilgrimi">Philopteroides pilgrimi Valim &amp; Palma</taxonomicName>
<taxonomicNameLabel pageId="5" pageNumber="76">sp. n.</taxonomicNameLabel>
