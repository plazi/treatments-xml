<emphasis italics="true" pageId="42" pageNumber="55">Orphnebius dilatatus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Assing" authorityYear="2016" box="[903,1023,617,641]" class="Insecta" family="Staphylinidae" genus="Orphnebius" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="55" phylum="Arthropoda" rank="species" species="extensus">
<emphasis box="[903,1023,617,641]" italics="true" pageId="42" pageNumber="55">O. extensus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Assing" authorityYear="2016" box="[1043,1158,617,641]" class="Insecta" family="Staphylinidae" genus="Orphnebius" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="55" phylum="Arthropoda" rank="species" species="serratus">
<emphasis box="[1043,1158,617,641]" italics="true" pageId="42" pageNumber="55">O. serratus</emphasis>
