<number box="[599,611,829,855]" pageId="15" pageNumber="35" value="1.0">1</number>
+1, 1+ 
<number box="[693,705,829,855]" pageId="15" pageNumber="35" value="1.0">1</number>
and 
<number box="[762,774,829,855]" pageId="15" pageNumber="35" value="1.0">1</number>
