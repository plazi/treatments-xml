; 
<materialsCitation ID-GBIF-Occurrence="2625647674" box="[693,911,348,375]" country="Finland" pageId="11" pageNumber="12">
hind wing 
<quantity box="[818,911,349,375]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="11" pageNumber="12" unit="mm" value="1.4">1.4 mm</quantity>
