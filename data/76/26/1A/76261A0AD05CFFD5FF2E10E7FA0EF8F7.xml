<taxonomicName authority="Mason" authorityName="Mason" authorityYear="1981" box="[593,784,906,932]" class="Bacillariophyceae" family="Not" genus="Iconella" higherTaxonomySource="CoL" kingdom="Chromista" order="Not" pageId="10" pageNumber="501" phylum="Ochrophyta" rank="genus">
