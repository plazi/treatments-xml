<taxonomicName authority="(Nixon, 1965)" authorityName="Nixon" authorityYear="1965" baseAuthorityName="Nixon" baseAuthorityYear="1965" class="Bacillariophyceae" family="Not" genus="Iconella" higherTaxonomySource="CoL" kingdom="Chromista" order="Not" pageId="8" pageNumber="499" phylum="Ochrophyta" rank="species" species="isus">
