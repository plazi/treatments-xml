<location LSID="urn:lsid:plazi:treatment:763A87EEFFDBFF82FF02FCCBFDE17956:FB4C6023FFDBFF85FF56FB15FED879C5" box="[226,307,1224,1248]" country="Costa Rica" county="Wing" municipality="Abdomen" name="Pleuron" pageId="19" pageNumber="56">Pleuron</location>
