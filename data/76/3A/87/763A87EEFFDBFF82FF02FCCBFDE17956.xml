<location LSID="urn:lsid:plazi:treatment:763A87EEFFDBFF82FF02FCCBFDE17956:FB4C6023FFDBFF85FC88FB3AFC9F79DA" box="[828,884,1255,1279]" country="Costa Rica" county="Wing" municipality="Abdomen" name="Head" pageId="19" pageNumber="56">Head</location>
