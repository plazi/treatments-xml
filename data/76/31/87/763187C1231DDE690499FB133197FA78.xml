<collectingCountry box="[977,1038,1369,1391]" name="Oman" pageId="0" pageNumber="1291">Oman</collectingCountry>
und 
<collectingCountry box="[178,221,1399,1420]" name="Iran" pageId="0" pageNumber="1291">Iran</collectingCountry>
