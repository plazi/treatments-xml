<taxonomicName id="87E24D5FCC1EFFBDE8DD02BF0E43F65E" box="[151,348,760,781]" class="Hydrozoa" family="Bougainvilliidae" genus="Bimeria" kingdom="Animalia" order="Anthoathecata" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="corynopsis">
