</taxonomicName>
and 
<taxonomicName id="4C754D5EFF928711B225FF6CFC38AB12" box="[276,369,253,276]" class="Magnoliopsida" family="Rhamnaceae" genus="Zizyphus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="12" pageNumber="381" phylum="Tracheophyta" rank="genus">
<emphasis id="B901EACFFF928711B225FF6CFC38AB12" box="[276,369,253,276]" italics="true" pageId="12" pageNumber="381">Zizyphus</emphasis>
