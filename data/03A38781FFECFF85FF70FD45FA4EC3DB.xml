F–G); inhabiting 
<taxonomicName id="4C0A4D14FFECFF85FB1EFA44FA43C3FD" box="[1273,1430,1455,1476]" class="Cycadopsida" family="Cycadaceae" genus="Cycas" kingdom="Plantae" order="Cycadales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="pectinata">
<emphasis id="B97EEA85FFECFF85FB1EFA44FA43C3FD" box="[1273,1430,1455,1476]" italics="true" pageId="10" pageNumber="11">Cycas pectinata</emphasis>
</taxonomicName>
, 
<collectingRegion id="49CEF875FFECFF85FF06FA26FEF0C3DB" box="[225,293,1485,1506]" country="India" name="Assam" pageId="10" pageNumber="11">Assam</collectingRegion>
, 
<collectingCountry id="F31D7607FFECFF85FEC9FA26FE8AC3DB" box="[302,351,1485,1506]" name="India" pageId="10" pageNumber="11">India</collectingCountry>
................................................................ 
<emphasis id="B97EEA85FFECFF85FB81FA27FA4EC3DB" bold="true" box="[1126,1435,1484,1506]" pageId="10" pageNumber="11">
<taxonomicName id="4C0A4D14FFECFF85FB81FA27FACFC3DB" box="[1126,1306,1484,1506]" class="Insecta" family="Erotylidae" genus="Cycadophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="assamensis" status="new species" subGenus="Strobilophila">
