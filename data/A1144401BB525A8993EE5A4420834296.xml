<paragraph id="14C9C08225E6F9422EF077EE4CAF70DC" pageId="24" pageNumber="25">Notes on types.</paragraph>
<paragraph id="6BE8316F92445132BB48DE8E74BA349C" pageId="24" pageNumber="25">
<taxonomicName id="42B5BD3F28FCA15D239CF296A73E4BE4" authorityName="Schmidt-Goebel" authorityYear="1846" class="Hexapoda" family="Carabidae" genus="Plochionus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="fenestratus">
<emphasis id="EBEB981EC378219A2867EC6BEA276B9D" bold="true" italics="true" pageId="24" pageNumber="25">Plochionus fenestratus</emphasis>
</taxonomicName>
<normalizedToken id="E67646E36621E0BEBD94BF3B4B6C9B7E" originalValue="Schmidt-Göbel">Schmidt-Goebel</normalizedToken>
