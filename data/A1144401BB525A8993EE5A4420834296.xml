<geoCoordinate id="EB3F175C90182D58FA5188540D0B6804" degrees="21" direction="north" minutes="56" orientation="latitude" precision="925" value="21.933332">21°56'N</geoCoordinate>
, 
<geoCoordinate id="350BFA59DE7B4513D797B3A8D38FC228" degrees="86" direction="east" minutes="32" orientation="longitude" precision="925" value="86.53333">86°32'E</geoCoordinate>
