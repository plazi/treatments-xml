<taxonomicName id="3AEFA67F30542DC447611CD8DDC24565" authorityName="Chaudoir" authorityYear="1877" class="Insecta" family="Carabidae" genus="Anchista" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="glabra">
