<taxonomicName id="335F6B783BBFA7B12CFA3D21EF7C4655" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25244" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus brevis Brown" order="Hymenoptera" pageId="14" pageNumber="563" phylum="Arthropoda" rank="species" species="brevis">brevis</taxonomicName>
