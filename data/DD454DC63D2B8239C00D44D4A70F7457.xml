<taxonomicName id="2900C356B63CCBC5C46BB1FAED46C71D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25236" class="Insecta" family="Formicidae" genus="Anochetus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Anochetus agilis Emery" order="Hymenoptera" pageId="14" pageNumber="563" phylum="Arthropoda" rank="species" species="agilis">agilis</taxonomicName>
