<elevation id="0078D1C6FF8AFFEBFBB1FF02FB58284C" box="[1132,1178,234,258]" metricMagnitude="1" metricUnit="m" metricValue="7.0" pageId="20" pageNumber="1874" unit="m" value="70.0">
