<taxonomicName id="4C37C7FFE1170D57DEB44C32FD1DFC89" authority="Willd." authorityName="Willd." box="[426,692,823,849]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="5" pageNumber="82" phylum="Tracheophyta" rank="species" species="mangium">
