<collectingCountry id="F33D18FB302D4839155AFCF7FE731A05" box="[359,426,835,861]" name="China" pageId="5" pageNumber="131">China</collectingCountry>
(
<collectingRegion id="49EE9689302D48391584FCF7FDC91A05" box="[441,528,835,861]" country="China" name="Beijing" pageId="5" pageNumber="131">Beijing</collectingRegion>
