<subSubSection id="15B7655F15334F61FF7DE8DCFA65D4D9" box="[199,1418,476,501]" pageId="5" pageNumber="352" type="etymology">
<paragraph id="5D1236D415334F61FF7DE8DCFA65D4D9" blockId="5.[151,1437,151,501]" box="[199,1418,476,501]" pageId="5" pageNumber="352">
<emphasis id="6FD9EAC615334F61FF7DE8DCFEA1D4D8" bold="true" box="[199,334,476,501]" pageId="5" pageNumber="352">Etymology.</emphasis>
