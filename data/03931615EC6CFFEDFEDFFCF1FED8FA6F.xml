<taxonomicName id="4C3ADC80EC6CFFEDFE8BFAAFFD34FADF" authority="(Muller, 1864)" baseAuthorityName="Muller" baseAuthorityYear="1864" box="[348,678,1367,1393]" class="Malacostraca" family="Hyperiidae" genus="Hyperoche" kingdom="Animalia" order="Amphipoda" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="martinezi">
<emphasis id="B94E7B11EC6CFFEDFE8BFAAFFE62FADE" box="[348,496,1367,1392]" italics="true" pageId="28" pageNumber="29">H. martinezi</emphasis>
(
<bibRefCitation id="EFABDAF2EC6CFFEDFE28FAAFFD0FFADF" author="Muller, F." box="[511,669,1367,1393]" pageId="28" pageNumber="29" refId="ref28398" refString="Muller, F. (1864) Fur Darwin. Wilhelm Engelmann: Leipzig, 91 pp., 65 figs." type="book" year="1864">Müller, 1864</bibRefCitation>
)
</taxonomicName>
; 
