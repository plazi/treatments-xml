Region
</collectingRegion>
(
<collectingCountry id="F9CA79CAB662FFC0FC77F9BF0181F9E5" box="[908,970,1562,1587]" name="India" pageId="17" pageNumber="18">India</collectingCountry>
, 
<collectingCountry id="F9CA79CAB662FFC0FC23F9BF0603F9E5" box="[984,1096,1562,1587]" name="Indonesia" pageId="17" pageNumber="18">Indonesia</collectingCountry>
, 
<collectingCountry id="F9CA79CAB662FFC0FBACF9BE06DBF9E4" box="[1111,1168,1563,1586]" name="Laos" pageId="17" pageNumber="18">Laos</collectingCountry>
, 
<collectingCountry id="F9CA79CAB662FFC0FB64F9BF0743F9E5" box="[1183,1288,1562,1587]" name="Malaysia" pageId="17" pageNumber="18">Malaysia</collectingCountry>
