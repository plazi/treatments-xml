, 
<collectingCountry id="F9CA79CAB662FFC0FF6CF99B02A7F981" box="[151,236,1598,1623]" name="Taiwan" pageId="17" pageNumber="18">Taiwan</collectingCountry>
, 
<collectingCountry id="F9CA79CAB662FFC0FF0CF99B0310F981" box="[247,347,1598,1623]" name="Thailand" pageId="17" pageNumber="18">Thailand</collectingCountry>
