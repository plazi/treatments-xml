<taxonomicName id="09974D4FFFC72E42B6AB1B0DFCBD9043" box="[688,796,1885,1907]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="53" pageNumber="14" phylum="Tracheophyta" rank="genus">Epigynum</taxonomicName>
, anterior. G, L, 
<taxonomicName id="09974D4FFFC72E42B7A31B0DFB839043" box="[952,1058,1885,1907]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="53" pageNumber="14" phylum="Tracheophyta" rank="genus">Epigynum</taxonomicName>
