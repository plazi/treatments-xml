<taxonomicName id="A2E51DCE1506CB20DFF0A2ED7E03A04D" authority="Latreille, 1806" authorityName="Latreille" authorityYear="1806" class="Insecta" family="Chrysididae" genus="Hedychrum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hedychrum bidentulum" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="bidentulum">Hedychrum bidentulum</taxonomicName>
