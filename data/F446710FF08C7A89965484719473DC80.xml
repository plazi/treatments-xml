<taxonomicName id="B93F1BD6A59C5DCC21AFCA5891EBA731" authority="Latreille, 1806" authorityName="Latreille" authorityYear="1806" class="Insecta" family="Chrysididae" genus="Hedychrum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hedychrum bidentulum" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="bidentulum">Hedychrum bidentulum</taxonomicName>
