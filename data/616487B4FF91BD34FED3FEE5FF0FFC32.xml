: one female, Playas, Guayas Province, 
<geoCoordinate id="8CF95065FF91BD34FC99FE70FDB6FDFA" box="[834,964,500,526]" direction="south" orientation="latitude" pageId="3" pageNumber="4" precision="9" value="-2.6371667">
02º 
<number id="5F052485FF91BD34FCB0FE70FDC3FDFA" box="[875,945,500,526]" pageId="3" pageNumber="4" value="38.23">38.23</number>
’S
</geoCoordinate>
<geoCoordinate id="8CF95065FF91BD34FC17FE70FA39FDFA" box="[972,1099,500,526]" direction="west" orientation="longitude" pageId="3" pageNumber="4" precision="92" value="-80.388336">
<number id="5F052485FF91BD34FC17FE70FD98FDFA" box="[972,1002,500,526]" pageId="3" pageNumber="4" value="80.0">80</number>
º 
<number id="5F052485FF91BD34FC28FE70FA5AFDFA" box="[1011,1064,500,526]" pageId="3" pageNumber="4" value="23.3">23.3</number>
’W
</geoCoordinate>
, 
<date id="9D731062FF91BD34FB8CFE70FA80FDFA" box="[1111,1266,500,526]" pageId="3" pageNumber="4" value="2001-07-29">
<number id="5F052485FF91BD34FB8CFE70FA06FDFA" box="[1111,1140,500,526]" pageId="3" pageNumber="4" value="29.0">29</number>
