), ca. 
<quantity id="4CBCC3D8FFC2F219FB37FF621D24FF2A" box="[1196,1368,150,177]" metricMagnitude="3" metricUnit="m" metricValue="3.7" metricValueMax="4.0" metricValueMin="3.4" pageId="4" pageNumber="155" unit="m" value="3700.0" valueMax="4000.0" valueMin="3400.0">
