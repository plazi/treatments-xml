<geoCoordinate id="EE7008FAFFC2F219FB70FF621D44FF2B" box="[1259,1336,150,176]" degrees="4,000" direction="west" orientation="longitude" pageId="4" pageNumber="155" precision="55" value="-4.0">–4,000</geoCoordinate>
