aff. 
<taxonomicName id="77C50C83DC2BA568FE558F29FE2BF8A0" authority="Dendy" authorityName="Dendy" authorityYear="1913" box="[421,485,1827,1849]" pageId="89" pageNumber="90" rank="species" species="indica">
