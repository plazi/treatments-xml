<taxonomicName id="77C50C83DC2BA568FED58F29FE2BF8A0" box="[293,485,1827,1849]" class="Calcarea" family="Grantiidae" genus="Grantia" higherTaxonomySource="CoL" kingdom="Animalia" order="Leucosolenida" pageId="89" pageNumber="90" phylum="Porifera" rank="species" species="indica">
