<taxonomicName id="1CEE67BEFFF0E85AEF0DFE8F5443AE18" authorityName="Freeman" authorityYear="1940" box="[599,775,256,281]" class="Insecta" family="Pentatomidae" genus="Nezara" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="immaculata">N. immaculata</taxonomicName>
