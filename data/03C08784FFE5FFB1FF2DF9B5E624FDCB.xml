female, 
<emphasis id="B91DEA80FFE1FFB1FC83FEA8E33CFEC5" bold="true" box="[825,1107,360,383]" pageId="4" pageNumber="449">
<collectingCountry id="F37E7602FFE1FFB1FC83FEA8E4B9FEC5" box="[825,982,360,383]" name="United States of America" pageId="4" pageNumber="449">United States</collectingCountry>
, 
<collectingRegion id="49ADF870FFE1FFB1FC52FEA8E33CFEC5" box="[1000,1107,360,383]" country="United States of America" name="Delaware" pageId="4" pageNumber="449">Delaware</collectingRegion>
</emphasis>
, New Castle Co, Hockessin, Ashland Nature Center, 
<date id="FFD71052FFE1FFB1FE23FE4AE560FE1B" box="[409,527,394,417]" pageId="4" pageNumber="449" value="2015-08-03">3.viii.2015</date>
, on flowers of 
<taxonomicName id="4C694D11FFE1FFB1FD0AFE49E335FE1B" authority=", (A. B. Mitchell)" authorityName="A.B. Mitchell" box="[688,1114,393,417]" class="Liliopsida" family="Poaceae" genus="Andropogon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="4" pageNumber="449" phylum="Magnoliophyta" rank="species" species="virginicus">
