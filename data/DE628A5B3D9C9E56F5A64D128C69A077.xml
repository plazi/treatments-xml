<taxonomicName id="658B95D9F942D1A3E3AD03DB765F609D" class="Gastropoda" family="Diplommatinidae" genus="Diplommatina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diplommatina pomatiaeformis" order="Architaenioglossa" pageId="39" pageNumber="40" phylum="Mollusca" rank="species" species="pomatiaeformis">
