<emphasis id="B93BEA9DFFD9FFAFF392F86F2F497394" bold="true" box="[199,321,1938,1963]" inLineHeading="true" pageId="58" pageNumber="59" reason="1">Diagnosis.</emphasis>
<taxonomicName id="4C4F4D0CFFD9FFAFF21EF86E2C237394" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[331,555,1939,1963]" class="Insecta" family="Derbidae" genus="Muiralevu" kingdom="Animalia" order="Hemiptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="dimidius">
