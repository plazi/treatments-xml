<number id="3DA524F7FFA6FFE5FCC1A92BFCA2860F" box="[822,837,1866,1890]" pageId="9" pageNumber="364" value="2.0">2</number>
(prolateral 
<number id="3DA524F7FFA6FFE5FC3AA92BFC3D860F" box="[973,986,1866,1890]" pageId="9" pageNumber="364" value="1.0">1</number>
