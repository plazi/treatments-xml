<taxonomicName id="F1AF96A256C66482BF1E9EFF43E3FED1" authority="Koch, 1956" authorityName="Koch" authorityYear="1956" class="Insecta" family="Tenebrionidae" genus="Monodius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Monodius lamottei" order="Coleoptera" pageId="21" pageNumber="102" phylum="Arthropoda" rank="species" species="lamottei">Monodius lamottei</taxonomicName>
