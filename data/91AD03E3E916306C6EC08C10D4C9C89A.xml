<taxonomicName id="317186AB6B8B8B26C6574082141DB84B" LSID="http://zoobank.org/BB59F1A1-2C83-4E2E-B0EC-E956CE1350B9" authority="Staines" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia amba" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="amba">Cephaloleia amba Staines</taxonomicName>
