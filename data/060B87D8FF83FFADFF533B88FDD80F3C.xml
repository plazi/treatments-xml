<taxonomicName id="49A24D4DFF83FFAFFD7C3AFDFBD709D1" authority="Zetterstedt, 1845" authorityName="Zetterstedt" authorityYear="1845" box="[696,1045,2008,2031]" class="Polychaeta" family="Orbiniidae" genus="Aricia" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Annelida" rank="species" species="moriens">
