<taxonomicName id="7EE386B39C562A1CEEF15A93FD6E59E5" authority="(Villers, 1789)" baseAuthorityName="Villers" baseAuthorityYear="1789" box="[423,744,1196,1221]" class="Insecta" family="Myrmeleontidae" genus="Acanthaclisis" kingdom="Animalia" order="Neuroptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="occitanica">
