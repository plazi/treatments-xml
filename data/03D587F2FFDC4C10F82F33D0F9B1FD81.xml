<taxonomicName id="4C7C4D67FFDC4C17F40E3D5EF3C7F7B9" authorityName="G. S. Miller" authorityYear="1899" box="[2987,3135,2089,2128]" class="Mammalia" family="Emballonuridae" genus="Peropteryx" kingdom="Animalia" order="Chiroptera" pageId="19" pageNumber="368" phylum="Chordata" rank="species" species="trinitatis">
