<taxonomicName id="4C7C4D67FFDC4C17F40C3D2AF39EF76D" authorityName="Wagner" authorityYear="1843" box="[2985,3174,2141,2180]" class="Mammalia" family="Emballonuridae" genus="Emballonura" kingdom="Animalia" order="Chiroptera" pageId="19" pageNumber="368" phylum="Chordata" rank="species" species="macrotis">
