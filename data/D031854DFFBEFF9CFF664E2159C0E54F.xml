<taxonomicName id="9F984FD8FFBEFF9CFF664E215916E725" authority="Newstead, 1917" authorityName="Newstead" authorityYear="1917" box="[151,592,290,316]" class="Insecta" family="Coccidae" genus="Pulvinaria" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="492" phylum="Arthropoda" rank="species" species="elongata">
