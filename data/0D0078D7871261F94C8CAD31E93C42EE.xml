<taxonomicName id="75A89F43200BF867A28F7A6C21040052" authority="(L.) Maxim. (1879)" authorityName="Maxim." authorityYear="1879" baseAuthorityName="Linnaeus" baseAuthorityYear="1753" class="Magnoliopsida" family="Rosaceae" genus="Physocarpus" kingdom="Plantae" order="Rosales" phylum="Tracheophyta" rank="species" species="opulifolius">
