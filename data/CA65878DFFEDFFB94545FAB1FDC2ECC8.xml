<taxonomicName id="85CC4D18FFEDFFB84545FAB1FE12EF29" authorityName="Boie" authorityYear="1827" box="[294,396,1325,1351]" class="Reptilia" family="Elapidae" kingdom="Animalia" order="Squamata" pageId="24" pageNumber="435" phylum="Chordata" rank="family">ELAPIDAE</taxonomicName>
