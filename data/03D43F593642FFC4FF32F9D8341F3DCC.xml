<taxonomicName id="4C7DF5CC365DFFC4FEFCFB7037453E7C" authorityName="Köhler" authorityYear="2019" box="[345,513,1269,1295]" class="Insecta" family="Sciaridae" genus="Pseudolycoriella" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="wernermohrigi">
<emphasis id="B909525D365DFFC4FEFCFB7037453E7C" box="[345,513,1269,1295]" italics="true" pageId="48" pageNumber="49">wernermohrigi</emphasis>
</taxonomicName>
