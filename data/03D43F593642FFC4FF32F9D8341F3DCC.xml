</taxonomicName>
<taxonomicName id="4C7DF5CC365DFFC4FD9FFA9C37873E40" authority=", Psl." authorityName="Psl." box="[570,707,1305,1331]" class="Insecta" family="Sciaridae" genus="Pseudolycoriella" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="huttoni">
<emphasis id="B909525D365DFFC4FD9FFA9C37CD3E40" box="[570,649,1305,1331]" italics="true" pageId="48" pageNumber="49">huttoni</emphasis>
