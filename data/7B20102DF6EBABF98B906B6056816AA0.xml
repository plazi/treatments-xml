<taxonomicName id="04C38E84F2EB03B1BFD72BEBD93A5963" class="Arachnida" family="Nemastomatidae" genus="Trilasma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trilasma tropicum" order="Opiliones" pageId="22" pageNumber="31" phylum="Arthropoda" rank="species" species="tropicum">Trilasma tropicum</taxonomicName>
