<taxonomicName id="6FBA94EDFDFE5C696B0BF0966F9C744B" authorityName="Klatt" authorityYear="1896" class="Magnoliopsida" family="Asteraceae" genus="Vernonia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vernonia collina" order="Asterales" pageId="43" pageNumber="92" phylum="Tracheophyta" rank="species" species="collina">
