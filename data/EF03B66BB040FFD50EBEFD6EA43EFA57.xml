<taxonomicName id="A0AA7CFEB040FFD50F09FB9CA279FBB7" box="[288,517,1109,1133]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="barbilabris">
