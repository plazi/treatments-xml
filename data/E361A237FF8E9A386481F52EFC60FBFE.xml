<taxonomicName id="ACC868A2FF8E9A3866F7F78EFC9FFC4E" box="[737,837,989,1015]" class="Gastropoda" family="Turridae" kingdom="Animalia" order="Neogastropoda" pageId="7" pageNumber="38" phylum="Mollusca" rank="family">Turridae</taxonomicName>
