<taxonomicName id="ACC868A2FF8E9A3860C0F7E6FED0FC4E" authority="Fleming, 1828" authorityName="Fleming" authorityYear="1828" class="Gastropoda" family="Muricidae" kingdom="Animalia" order="Neogastropoda" pageId="7" pageNumber="38" phylum="Mollusca" rank="family">
