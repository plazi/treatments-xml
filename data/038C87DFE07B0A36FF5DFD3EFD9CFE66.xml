wide across eyes and 
<quantity id="4CDD9B2CE07B0A31FC25FAB8FBC3FAF0" box="[1007,1141,1323,1349]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" metricValueMax="1.1" metricValueMin="0.9" pageId="3" pageNumber="560" unit="mm" value="1.0" valueMax="1.1" valueMin="0.9">0.9–1.1 mm</quantity>
