long, 
<quantity id="4CDD9B2CE07B0A31FD6DFADCFC98FADC" box="[679,814,1359,1385]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" metricValueMax="1.3" metricValueMin="1.2" pageId="3" pageNumber="560" unit="mm" value="1.25" valueMax="1.3" valueMin="1.2">1.2–1.3 mm</quantity>
