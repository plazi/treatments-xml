<taxonomicName id="F582C9814D64FFD44F41FEE85F8F9E12" authority="Kailola 1976" authorityName="Kailola" authorityYear="1976" box="[297,667,260,282]" class="Actinopterygii" family="Apogonidae" genus="Apogon" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="285" phylum="Chordata" rank="species" species="albimaculosus">
