<taxonomicName id="E2E5847F609C7CCCCA7896A00EDAA5DD" LSID="http://zoobank.org/1FC70495-DB98-45B3-857B-6CF146CB6025" authority="Sharkey" authorityName="Sharkey" authorityYear="2018" class="Insecta" family="Braconidae" genus="Zosteragathis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zosteragathis perknos" order="Hymenoptera" pageId="4" pageNumber="229" phylum="Arthropoda" rank="species" species="perknos" status="sp. n.">Zosteragathis perknos Sharkey</taxonomicName>
<taxonomicNameLabel id="BAF955CD412E421C889F1C32EB1CB6F8" pageId="4" pageNumber="229">sp. n.</taxonomicNameLabel>
