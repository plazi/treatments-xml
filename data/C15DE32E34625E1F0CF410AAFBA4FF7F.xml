<quantity id="8E0CFFDD34625E1C0CAD10C6FF2EFE74" box="[206,220,475,499]" metricMagnitude="-3" metricUnit="kg" metricValue="3.0" pageId="5" pageNumber="297" unit="g" value="3.0">3</quantity>
