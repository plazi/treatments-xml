<emphasis id="B905EAE8FF944F33FF3F0A4A5508F9A3" bold="true" box="[199,352,361,386]" pageId="4" pageNumber="341">Distribution.</emphasis>
<collectingCountry id="F366766AFF944F33FE9F0A4A55C2F9A3" box="[359,426,361,386]" name="China" pageId="4" pageNumber="341">China</collectingCountry>
(
<collectingRegion id="49B5F818FF944F33FE410A4A5672F9A3" box="[441,538,361,386]" country="China" name="Sichuan" pageId="4" pageNumber="341">Sichuan</collectingRegion>
