<taxonomicName id="4C174D43FFE1FFC598EAFD98F8333280" authority="(Gunther, 1877)" baseAuthorityName="Gunther" baseAuthorityYear="1877" box="[151,626,618,645]" class="Actinopterygii" family="Sebastidae" genus="Sebastolobus" kingdom="Animalia" order="Scorpaeniformes" pageId="112" pageNumber="113" phylum="Chordata" rank="species" species="macrochir">
<emphasis id="B963EAD2FFE1FFC598EAFD98FBF13280" bold="true" box="[151,432,618,645]" italics="true" pageId="112" pageNumber="113">Sebastolobus macrochir</emphasis>
