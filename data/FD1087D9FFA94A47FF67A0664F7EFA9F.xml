<taxonomicName id="B2B94D4CFFB64A47FF21A5194F75FD34" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[209,378,632,657]" class="Actinopterygii" family="Labridae" genus="Labrus" kingdom="Animalia" order="Perciformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="mixtus">Labrus mixtus</taxonomicName>
