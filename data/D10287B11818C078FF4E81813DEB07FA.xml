<taxonomicName id="9EAB4D241818C078FD4687FA3A83076A" authority="Cheeseman" authorityName="Cheeseman" authorityYear="1886" box="[671,1023,1771,1798]" class="Magnoliopsida" family="Rubiaceae" genus="Coprosma" kingdom="Plantae" order="Gentianales" pageId="9" pageNumber="310" phylum="Tracheophyta" rank="species" species="areolata">
