<taxonomicName id="4C19DEEAFFC8FFE277756B204544FEC5" authority="Lamarck, 1815" authorityName="Lamarck" authorityYear="1815" box="[302,630,318,343]" class="Anthozoa" family="Gorgoniidae" genus="Gorgonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="38" pageNumber="39" phylum="Cnidaria" rank="species" species="penna">
