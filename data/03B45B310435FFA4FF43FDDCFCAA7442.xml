<quantity id="4CE547C20435FFA5FD82FA23FFEE70D5" box="[598,687,1499,1523]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="18" pageNumber="19" unit="mm" value="1.2">1.2 mm</quantity>
