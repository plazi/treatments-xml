<quantity id="4CE547C20435FFA5FE45FA13FCF470D5" box="[401,437,1499,1529]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="18" pageNumber="19" unit="in" value="1.0">1 in</quantity>
