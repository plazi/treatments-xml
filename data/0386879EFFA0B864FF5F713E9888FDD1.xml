<number id="3DE724AFFFA0B864FB8171949884FFE1" box="[1097,1163,1448,1474]" pageId="11" pageNumber="52" value="1907.0">1907</number>
</date>
: 
<specimenCount id="9D29FD01FFA0B864FB5F719498E4FFE1" box="[1175,1259,1448,1474]" pageId="11" pageNumber="52" type="male">
<number id="3DE724AFFFA0B864FB5F719498A9FFE1" box="[1175,1190,1448,1474]" pageId="11" pageNumber="52" value="1.0">1</number>
