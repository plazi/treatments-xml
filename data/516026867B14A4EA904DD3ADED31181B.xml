<taxonomicName id="895F651084720ED9B71646B225FF9F3D" class="Liliopsida" family="Poaceae" genus="Agropyron" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Agropyron" order="Poales" pageId="2" pageNumber="83" phylum="Tracheophyta" rank="genus">
