<taxonomicName id="8C3BB3FAEAD31E4B0726A898A7553C11" class="Liliopsida" family="Poaceae" genus="Phragmites" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Phragmites" order="Poales" pageId="2" pageNumber="83" phylum="Tracheophyta" rank="genus">
<emphasis id="55B61B36EC72261681FE930877D850C0" italics="true" pageId="2" pageNumber="83">Phragmites</emphasis>
