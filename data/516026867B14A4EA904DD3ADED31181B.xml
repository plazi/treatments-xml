<taxonomicName id="9C9E52E0FBB10E564E702B5FDB6E0D32" class="Liliopsida" family="Poaceae" genus="Fargesia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fargesia" order="Poales" pageId="2" pageNumber="83" phylum="Tracheophyta" rank="genus">
<emphasis id="88637730BEFFC965DD6A90F9A6BB42DB" italics="true" pageId="2" pageNumber="83">Fargesia</emphasis>
