<taxonomicName id="331D49178675CC186281393F289476C0" authority="Rübsaamen, 1899" authorityName="Rübsaamen" authorityYear="1899" class="Insecta" family="Poaceae" genus="Coccomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Coccomorpha" order="Diptera" pageId="2" pageNumber="83" phylum="Arthropoda" rank="family">Poaceae</taxonomicName>
