<collectionCode id="A0F7409B9A58FFAD5CE0FA00A5A5F98F" box="[419,481,1534,1559]" httpUri="http://grbio.org/cool/23qr-v2gj" name="Instituto Biologico de Sao Paulo" pageId="4" pageNumber="57">IBSP</collectionCode>
