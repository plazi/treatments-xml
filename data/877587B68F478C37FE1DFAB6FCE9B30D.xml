<paragraph id="0F6336A08F478C36FEE5FA89FC85B72B" blockId="2.[270,1103,1332,1620]" box="[270,808,1332,1354]" pageId="2" pageNumber="62">
<treatmentCitationGroup id="2FCC118E8F478C36FEE5FA89FC85B72B" box="[270,808,1332,1354]" pageId="2" pageNumber="62">
<treatmentCitation id="8E7D10B18F478C36FEE5FA89FD63B72B" author="Franz" box="[270,718,1332,1354]" page="700" pageId="2" pageNumber="62" year="1967">
<taxonomicName id="C8DC4D238F478C36FEE5FA89FD63B72B" authority="Franz 1967: 700" authorityName="Franz" authorityPageNumber="700" authorityYear="1967" box="[270,718,1332,1354]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="62" phylum="Arthropoda" rank="species" species="scydmaenoides">
<emphasis id="3DA8EAB28F478C36FEE5FA89FDB1B728" box="[270,540,1333,1354]" italics="true" pageId="2" pageNumber="62">Euconnus scydmaenoides</emphasis>
<bibRefCitation id="6B4D4B518F478C36FDCEFA88FD63B72B" author="Franz" box="[549,718,1332,1353]" pageId="2" pageNumber="62" pagination="611 - 724" refId="ref3610" refString="Franz, H. 1967. Zur Kenntnis der Scydmaenidenfauna von Lateinamerika. II Teil: Scydmaeniden aus dem subtropischen und tropischen Amerika. Biologie de l'Amerique Australe 3: 611 - 724." type="journal article" year="1967">Franz 1967:700</bibRefCitation>
</taxonomicName>
</treatmentCitation>
