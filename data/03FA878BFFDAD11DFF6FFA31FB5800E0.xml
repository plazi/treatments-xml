<taxonomicName id="4C534D1EFFDAD11DFEBDF90DFE0F00A8" box="[325,434,1691,1715]" class="Insecta" family="Phoridae" genus="Puliciphora" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="69" phylum="Arthropoda" rank="species" species="rufipes">
