<taxonomicName id="BD5C65FF942A7E2F4CF6C7F49364EBDF" class="Insecta" family="Diaspididae" genus="Poliaspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Poliaspis attenuata" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="attenuata">Poliaspis attenuata</taxonomicName>
