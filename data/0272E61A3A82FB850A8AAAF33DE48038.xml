<taxonomicName id="00CDAC5B0C7864CA190AD7F065E653D4" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34056" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole tetrica Forel" lsidName-HNS="Pheidole tetrica Forel" order="Hymenoptera" pageNumber="517" phylum="Arthropoda" rank="species" species="tetrica">tetrica</taxonomicName>
