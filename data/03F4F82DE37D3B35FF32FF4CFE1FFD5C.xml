<taxonomicName id="4C5D32B8E37D3B35FEF9FF43FDD4FF58" box="[348,521,151,177]" class="Insecta" family="Chrysomelidae" genus="Gibbobruchus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="519" phylum="Arthropoda" rank="genus">
