<collectionCode id="ED409DBF2234FFE3F105FC9CFD21FCF2" LSID="urn:lsid:biocol.org:col:34988" box="[594,693,839,865]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="2" pageNumber="27">MNHN</collectionCode>
