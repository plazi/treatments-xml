<taxonomicName id="1B3AD0B6370F8E7F7214D27DA823C685" authorityName="Godwin-Austen" authorityYear="1915" class="Gastropoda" family="Polypodiaceae" genus="Cyclophorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyclophorus (Glossostylus) koboensis" order="Polypodiales" pageId="0" pageNumber="43" phylum="Mollusca" rank="species" species="koboensis" subGenus="Glossostylus">Cyclophorus (Glossostylus) koboensis</taxonomicName>
