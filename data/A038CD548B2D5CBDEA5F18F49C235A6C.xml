, 
<taxonomicName id="3419CC341852256C0B7BBC23D5A8533F" genus="B." lsidName="B. rosenbaueri" pageId="3" pageNumber="210" rank="species" species="rosenbaueri">B. rosenbaueri</taxonomicName>
