, 
<collectingCountry id="F36C95D38364763DFAFBFBDE124CFBB6" box="[1304,1389,1076,1101]" name="Tunisia" pageId="17" pageNumber="318">Tunisia</collectingCountry>
) to 
<collectingCountry id="F36C95D38364763DFF74FBBD1626FB8B" box="[151,263,1111,1136]" name="Mongolia" pageId="17" pageNumber="318">Mongolia</collectingCountry>
, 
<collectingCountry id="F36C95D38364763DFEF2FBBD1675FB8B" box="[273,340,1111,1136]" name="China" pageId="17" pageNumber="318">China</collectingCountry>
