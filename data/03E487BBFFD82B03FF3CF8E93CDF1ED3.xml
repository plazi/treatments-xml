<paragraph id="8BF236ADFFD82B04FF3CF8E93A2018CD" blockId="11.[151,887,1906,1932]" box="[151,887,1906,1932]" pageId="11" pageNumber="12">
<heading id="D0BA81C1FFD82B04FF3CF8E93A2018CD" bold="true" box="[151,887,1906,1932]" fontSize="11" level="1" pageId="11" pageNumber="12" reason="1">
<emphasis id="B939EABFFFD82B04FF3CF8E93A2018CD" bold="true" box="[151,887,1906,1932]" pageId="11" pageNumber="12">
<taxonomicName id="4C4D4D2EFFD82B04FF3CF8E93BD018CD" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[151,647,1906,1932]" class="Insecta" family="Polycentropodidae" genus="Nyctiophylax" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="vestitus" status="new species" subGenus="Paranyctiophylax">
<emphasis id="B939EABFFFD82B04FF3CF8E9386018CD" bold="true" box="[151,311,1906,1932]" italics="true" pageId="11" pageNumber="12">Nyctiophylax</emphasis>
(
<emphasis id="B939EABFFFD82B04FEECF8E93B4B18CD" bold="true" box="[327,540,1906,1932]" italics="true" pageId="11" pageNumber="12">Paranyctiophylax</emphasis>
) 
<emphasis id="B939EABFFFD82B04FD85F8E93BD018CD" bold="true" box="[558,647,1906,1932]" italics="true" pageId="11" pageNumber="12">vestitus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A20A57C4FFD82B04FD3DF8E93A7218CD" box="[662,805,1906,1932]" pageId="11" pageNumber="12" rank="species">new species</taxonomicNameLabel>
