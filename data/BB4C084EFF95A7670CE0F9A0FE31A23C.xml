<taxonomicName id="F4E5C2DBFF94A7610DF0F8F3FDDCA1FA" authority="Buck &amp; Marshall, 2009" authorityName="Buck &amp; Marshall" authorityYear="2009" box="[391,621,1917,1940]" class="Insecta" family="Sphaeroceridae" genus="Leptocera" kingdom="Animalia" order="Diptera" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="tenuispina" status="sp.n.">
