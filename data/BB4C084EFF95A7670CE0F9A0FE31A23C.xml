<taxonomicName id="F4E5C2DBFF93A7660F34F8A9FB93A150" box="[835,1058,1831,1854]" class="Insecta" family="Sphaeroceridae" genus="Leptocera" kingdom="Animalia" order="Diptera" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="fontinalis">
