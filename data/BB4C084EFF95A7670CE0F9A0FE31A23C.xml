<taxonomicName id="F4E5C2DBFF92A767084EFED7FB1BA71C" authority="Buck &amp; Marshall, 2009" authorityName="Buck &amp; Marshall" authorityYear="2009" box="[1081,1194,345,370]" class="Insecta" family="Sphaeroceridae" genus="Leptocera" kingdom="Animalia" order="Diptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="kanata" status="sp.n.">
