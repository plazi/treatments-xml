</taxonomicName>
<taxonomicNameLabel id="1AA2D831FF92A7670F65FE0EFCF8A7F4" box="[786,841,384,410]" pageId="93" pageNumber="94" rank="species">sp.n.</taxonomicNameLabel>
because 
<taxonomicName id="F4E5C2DBFF92A7670FC1FE0EFB94A7F7" authority="Buck &amp; Marshall, 2009" authorityName="Buck &amp; Marshall" authorityYear="2009" box="[950,1061,384,409]" class="Insecta" family="Sphaeroceridae" genus="Leptocera" kingdom="Animalia" order="Diptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="kanata" status="sp.n.">
<emphasis id="0191654AFF92A7670FC1FE0EFB94A7F7" box="[950,1061,384,409]" italics="true" pageId="93" pageNumber="94">L. kanata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="1AA2D831FF92A767085AFE0EFBD5A7F4" box="[1069,1124,384,410]" pageId="93" pageNumber="94" rank="species">sp.n.</taxonomicNameLabel>
