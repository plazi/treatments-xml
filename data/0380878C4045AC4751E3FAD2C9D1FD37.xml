<taxonomicName id="4C294D194042AC4554F0F8F5CC8FF89F" authority="Radoszkowski" authorityName="Radoszkowski" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="221" phylum="Arthropoda" rank="species" species="mlokossewiczi">
