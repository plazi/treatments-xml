<taxonomicName id="4C2C4D1CFFA4FFDD559B100AFBD0FA8D" box="[958,1125,1344,1369]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="praelineata">
