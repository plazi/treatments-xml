<taxonomicName id="64F3424B8C058483C6AE6E815DA0E0A3" class="Mammalia" family="Geomyidae" genus="Thomomys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thomomys" order="Rodentia" pageId="37" pageNumber="68" phylum="Chordata" rank="genus">Thomomys</taxonomicName>
