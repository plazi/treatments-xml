<caption id="DF6B663CC672A200F56FFD45F3A6473C" ID-DOI="http://doi.org/10.5281/zenodo.3749057" ID-Zenodo-Dep="3749057" httpUri="https://zenodo.org/record/3749057/files/figure.png" inLine="true" pageId="4" pageNumber="231" targetBox="[1882,2682,576,1139]" targetPageId="4">
<paragraph id="8BAB36B4C672A200F56FFD45F3A6473C" blockId="4.[2710,3516,575,1146]" pageId="4" pageNumber="231">
<emphasis id="B960EAA6C672A200F56FFD45F4664040" bold="true" box="[2711,2953,735,774]" pageId="4" pageNumber="231">Distribution.</emphasis>
<emphasis id="B960EAA6C672A200F461FD45F45B4040" box="[2969,2996,735,774]" italics="true" pageId="4" pageNumber="231">E</emphasis>
New 
<collectingCountry id="F3037624C672A200F3D1FD45F35A4040" box="[3113,3253,735,774]" name="Guinea-Bissau" pageId="4" pageNumber="231">Guinea</collectingCountry>
