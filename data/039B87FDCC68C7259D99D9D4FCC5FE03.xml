<taxonomicName id="4C324D68CC68C72599EED811FABBFED2" authorityName="Diels" authorityYear="1925" box="[1248,1366,338,363]" class="Magnoliopsida" family="Annonaceae" genus="Polyalthia" kingdom="Plantae" order="Magnoliales" pageId="16" pageNumber="49" phylum="Tracheophyta" rank="species" species="decora">
