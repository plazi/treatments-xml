<taxonomicName id="4C204D1FFFA6215ADA0BF9CEFC0B6417" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[828,932,1624,1648]" class="Bacilli" family="Lactobacillaceae" genus="Lactiplantibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="43" pageNumber="2825" phylum="Firmicutes" rank="species" species="daoliensis">
