<quantity id="4CF5B794FFEEFF82FCD5FCF4FC01645A" box="[863,956,864,888]" metricMagnitude="-1" metricUnit="m" metricValue="1.8" pageId="13" pageNumber="464" unit="mm" value="180.0">180 mm</quantity>
