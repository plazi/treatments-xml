<heading id="F5635A2B625A7B4E51B5CCFFFE5DF94A" bold="true" box="[151,472,1690,1716]" fontSize="11" level="1" pageId="21" pageNumber="22" reason="1">
<emphasis id="9CE03155625A7B4E51B5CCFFFE5DF94A" bold="true" box="[151,472,1690,1716]" pageId="21" pageNumber="22">
<number id="185CFF60625A7B4E51B5CCFFFF33F94A" box="[151,182,1690,1716]" pageId="21" pageNumber="22" value="15.0">15</number>
. 
<taxonomicName id="699496C4625A7B4E51E1CCFFFE5DF94A" box="[195,472,1690,1716]" class="Insecta" family="Chrysomelidae" genus="Lema" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="coronata" subGenus="Lema">
<emphasis id="9CE03155625A7B4E51E1CCFFFE82F94A" bold="true" box="[195,263,1690,1716]" italics="true" pageId="21" pageNumber="22">Lema</emphasis>
(
<emphasis id="9CE03155625A7B4E503ACCFFFED9F94A" bold="true" box="[280,348,1690,1716]" italics="true" pageId="21" pageNumber="22">Lema</emphasis>
) 
<emphasis id="9CE03155625A7B4E504FCCFFFE5DF94A" bold="true" box="[365,472,1690,1716]" italics="true" pageId="21" pageNumber="22">coronata</emphasis>
