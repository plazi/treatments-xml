<collectingCountry id="96B301B8FFBCFF82FE95853447FFF891" box="[359,477,1860,1885]" name="Venezuela" pageId="6" pageNumber="281">Venezuela</collectingCountry>
, 
<collectingCountry id="96B301B8FFBCFF82FE1A8534440EF891" box="[488,556,1860,1885]" name="Brazil" pageId="6" pageNumber="281">Brazil</collectingCountry>
