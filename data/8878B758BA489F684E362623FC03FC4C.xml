<quantity id="C729ABABBA499F174F2D2223FBF9FD8A" box="[894,1024,558,585]" metricMagnitude="1" metricUnit="m" metricValue="8.0" metricValueMax="8.5" metricValueMin="7.5" pageId="63" pageNumber="64" unit="m" value="80.0" valueMax="85.0" valueMin="75.0">
