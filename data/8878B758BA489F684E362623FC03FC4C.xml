<paragraph id="006E064EBA499F174CEE21E5FDE9FD15" blockId="63.[189,1399,445,726]" pageId="63" pageNumber="64">
<materialsCitation id="B0B90C13BA499F174CEE21E5FDE9FD15" ID-GBIF-Occurrence="2625219307" collectingDate="VI / 1 - 4" collectionCode="SMF" country="Svalbard and Jan Mayen" elevation="80" latitude="76.38333" location="Expedition" longLatPrecision="950" longitude="15.95" municipality="Shelf" pageId="63" pageNumber="64" specimenCode="SMF XXX 864, SMF XXX 865" specimenCount="1" typeStatus="holotype">
