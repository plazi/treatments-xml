<taxonomicName id="C7D17DCDBA499F174E3A2785FBCDF860" authority="(O. F. Muller, 1776)" baseAuthorityName="O. F. Muller" baseAuthorityYear="1776" box="[617,1076,1928,1955]" class="Bivalvia" family="Pectinidae" genus="Chlamys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pectinoida" pageId="63" pageNumber="64" phylum="Mollusca" rank="species" species="islandica">
