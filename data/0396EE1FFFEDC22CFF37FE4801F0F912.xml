<taxonomicName id="4C3F248AFFEDC22CFC1FF90A0101F912" box="[959,1039,1677,1700]" class="Dothideomycetes" family="Tubeufiaceae" genus="Helicopsis" kingdom="Fungi" order="Tubeufiales" pageId="4" pageNumber="198" phylum="Ascomycota" rank="species" species="persica">persica</taxonomicName>
