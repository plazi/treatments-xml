<collectingCountry id="F3381BF9B60AFF80FC16FC20FC79FC1B" box="[915,1023,971,993]" name="Bermuda" pageId="13" pageNumber="204">Bermuda</collectingCountry>
</materialsCitation>
, 
<materialsCitation id="3B475134B60AFF80FB8FFC20FBA7FBE4" collectorName="Galapagos Is. &amp; Hawaii" country="New Zealand, Australia, Moluccas, Philippines" location="Pacific islands" pageId="13" pageNumber="204" specimenCount="1" typeStatus="holotype">
