<geoCoordinate id="10B8E679B0BA501E111FAC90280B1153" degrees="5" direction="north" minutes="26" orientation="latitude" precision="925" value="5.4333334">5°26'N</geoCoordinate>
, 
<geoCoordinate id="70A895243E2C0DEB313DBA23AA535AD6" degrees="102" direction="east" minutes="44" orientation="longitude" precision="925" value="102.73333">102°44'E</geoCoordinate>
