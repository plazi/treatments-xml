<date id="FFBB1034FFC1FFB7FDF3FADC39F3FA29" pageId="122" pageNumber="122" value="1913-10">October 1913</date>
and 
<date id="FFBB1034FFC1FFB7FF2EFAFF382CFA29" box="[209,331,1400,1421]" pageId="122" pageNumber="122" value="1914-05">May 1914</date>
