<paragraph id="E53C1CE89A6FC496F34BFA7DA994572C" pageId="9" pageNumber="114">Type species.</paragraph>
<paragraph id="AC0A070368276931964B48E4F1CAED61" pageId="9" pageNumber="114">
<taxonomicName id="8F43B64BCD395E22C0DD7AE2925D2D53" class="Insecta" family="Termitidae" genus="Nanotermes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nanotermes isaacae" order="Blattodea" pageId="9" pageNumber="114" phylum="Arthropoda" rank="species" species="isaacae">Nanotermes isaacae</taxonomicName>
