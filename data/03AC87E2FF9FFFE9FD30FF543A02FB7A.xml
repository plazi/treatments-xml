<specimenCode id="DBA39E8FFF9FFFE9FC21FBB13D55FBEF" box="[990,1074,1078,1099]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="36" pageNumber="36">300340</specimenCode>
