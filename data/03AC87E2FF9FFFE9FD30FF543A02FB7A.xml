<specimenCode id="DBA39E8FFF9FFFE9FBB1FB9E3B88FBEF" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="36" pageNumber="36">AMNH 296564</specimenCode>
