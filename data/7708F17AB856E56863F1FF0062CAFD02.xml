(
<quantity id="3859ED89B856E568663DFE2B63C0FEAD" metricMagnitude="0" metricUnit="m" metricValue="2.04216" pageId="116" pageNumber="117" unit="ft" value="6.7">6,700 ft</quantity>
), 
<date id="8B1F66ACB856E56863A6FE4F623CFEAE" box="[192,336,471,497]" pageId="116" pageNumber="117" value="1962-07-18">18 July 1962</date>
