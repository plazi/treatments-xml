<taxonomicName id="4C1F4D47F5225C0850FF85BEA65A79B8" authority="Snyder &amp; Emerson, 1949" authorityName="Snyder &amp; Emerson" authorityYear="1949" class="Insecta" family="Termitidae" genus="Anoplotermes" kingdom="Animalia" order="Isoptera" pageId="3" pageNumber="18" phylum="Arthropoda" rank="species" species="hageni">
