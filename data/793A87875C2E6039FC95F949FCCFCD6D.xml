<caption id="A5EC66195C2F6039FF72FB7FFDADCE61" ID-DOI="http://doi.org/10.5281/zenodo.5437990" ID-Zenodo-Dep="5437990" httpUri="https://zenodo.org/record/5437990/files/figure.png" pageId="39" pageNumber="418" startId="39.[155,233,1251,1270]" targetBox="[406,1187,208,1218]" targetPageId="39">
<paragraph id="F12C36915C2F6039FF72FB7FFDADCE61" blockId="39.[155,1435,1251,1300]" pageId="39" pageNumber="418">
<emphasis id="C3E7EA835C2F6039FF72FB7FFEDCCF84" bold="true" box="[155,272,1251,1270]" pageId="39" pageNumber="418">Figure 34.</emphasis>
<taxonomicName id="36934D125C2F6039FEF2FB78FE35CF84" authorityName="Eibye-Jacobsen" authorityYear="1991" box="[283,505,1251,1270]" class="Polychaeta" family="Phyllodocidae" genus="Paranaitis" kingdom="Animalia" order="Phyllodocida" pageId="39" pageNumber="418" phylum="Annelida" rank="species" species="uschakovi">
<emphasis id="C3E7EA835C2F6039FEF2FB78FE35CF84" box="[283,505,1251,1270]" italics="true" pageId="39" pageNumber="418">Paranaitis uschakovi</emphasis>
</taxonomicName>
