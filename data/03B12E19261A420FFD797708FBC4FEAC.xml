<taxonomicName id="4C18E48C26184211FE877761FE58F961" authorityName="Nomura &amp; Leschen" authorityYear="2015" baseAuthorityName="Nomura &amp; Leschen" baseAuthorityYear="2015" box="[270,438,1662,1681]" class="Insecta" family="Staphylinidae" genus="Parkerola" kingdom="Animalia" order="Coleoptera" pageId="14" phylum="Arthropoda" rank="species" species="gigantea">
