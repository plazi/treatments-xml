<emphasis id="7FB81656FFDFB3504481FBF0C93D0F7D" bold="true" box="[199,315,1081,1105]" inLineHeading="true" pageId="67" pageNumber="68" reason="1">Remarks.</emphasis>
<taxonomicName id="8ACCB1C7FFDFB3504504FBF3C9F50F7D" authorityName="Hart" authorityYear="1964" box="[322,499,1082,1105]" class="Malacostraca" family="Alpheidae" genus="Betaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="setosus">
