<taxonomicName id="8ACCB1C7FFDFB3504089FBF0C94E0F59" authority="Stimpson, 1860" authorityName="Stimpson" authorityYear="1860" class="Malacostraca" family="Porcellanidae" genus="Pachycheles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="rudis">
