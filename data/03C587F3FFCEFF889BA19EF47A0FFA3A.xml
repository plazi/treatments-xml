<taxonomicName id="4C6C4D66FFCDFF88999D9B2B7D07FAD1" box="[683,798,1352,1377]" class="Agaricomycetes" family="Geastraceae" genus="Geastrum" kingdom="Fungi" order="Geastrales" pageId="98" pageNumber="99" phylum="Basidiomycota" rank="genus">
<emphasis id="B918EAF7FFCDFF88999D9B2B7D07FAD1" box="[683,798,1352,1377]" italics="true" pageId="98" pageNumber="99">Geastrum</emphasis>
</taxonomicName>
.
</paragraph>
</subSubSection>
<subSubSection id="C376656EFFCDFF889BF09B0C7A0FFA3A" box="[198,1046,1391,1418]" pageId="98" pageNumber="99" type="etymology">
<paragraph id="8BD336E5FFCDFF889BF09B0C7A0FFA3A" blockId="98.[151,1437,152,1418]" box="[198,1046,1391,1418]" pageId="98" pageNumber="99">
<emphasis id="B918EAF7FFCDFF889BF09B0C7F4AFA39" bold="true" box="[198,339,1391,1417]" pageId="98" pageNumber="99">Etymology.</emphasis>
