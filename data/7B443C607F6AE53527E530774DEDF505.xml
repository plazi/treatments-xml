<paragraph id="F6F6B2B2FB913434D5804B001A357355" pageId="6" pageNumber="69">
<taxonomicName id="000B4E695A0E712DF80648F22240C936" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crematogaster aberrans" order="Hymenoptera" pageId="6" pageNumber="69" phylum="Arthropoda" rank="species" species="aberrans">Crematogaster aberrans</taxonomicName>
