<collectingCountry id="36924CDD0060645CFD9BFB09FD7FE4A6" box="[595,662,1160,1183]" name="Nepal" pageId="6" pageNumber="551">Nepal</collectingCountry>
. 
<taxonomicName id="898577CE0060645CFD55FB09FEFAE486" authorityName="Song &amp; Liang" authorityYear="2007" class="Insecta" family="Dictyopharidae" genus="Tenguna" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="551" phylum="Arthropoda" rank="species" species="medogensis">
<emphasis id="7CF1D05F0060645CFD55FB09FEFAE486" italics="true" pageId="6" pageNumber="551">Tenguna medogensis</emphasis>
</taxonomicName>
