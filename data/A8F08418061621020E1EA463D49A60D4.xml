<taxonomicName id="07BCB901EE90A15D07F676F0CF1DFAD8" class="Arachnida" family="Theraphosidae" genus="Brachypelma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachypelma" order="Araneae" pageId="6" pageNumber="81" phylum="Arthropoda" rank="genus">Brachypelma</taxonomicName>
