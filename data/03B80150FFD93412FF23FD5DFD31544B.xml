<subSubSection id="C30BE3CDFFD93412FF23FCE6FC76534B" box="[151,965,769,794]" pageId="6" pageNumber="443" type="etymology">
<paragraph id="8BAEB046FFD93412FF23FCE6FC76534B" blockId="6.[151,1436,769,1050]" box="[151,965,769,794]" pageId="6" pageNumber="443">
<emphasis id="B9656C54FFD93412FF23FCE6FEAD534B" bold="true" box="[151,286,769,794]" pageId="6" pageNumber="443">Etymology.</emphasis>
<emphasis id="B9656C54FFD93412FE90FCE5FED45348" box="[292,359,770,793]" italics="true" pageId="6" pageNumber="443">trans-</emphasis>
