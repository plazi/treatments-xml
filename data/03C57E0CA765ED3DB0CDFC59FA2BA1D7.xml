<number id="3DA4DD3DA765ED3DB1DEFBEAFE7DA147" box="[388,459,1077,1101]" pageId="25" pageNumber="26" value="40923.0">40923</number>
<emphasis id="B9181308A765ED3DB188FBEBFD8DA147" box="[466,571,1076,1101]" italics="true" pageId="25" pageNumber="26">Albatross</emphasis>
Station 
<number id="3DA4DD3DA765ED3DB2CDFBEBFD78A147" box="[663,718,1076,1101]" pageId="25" pageNumber="26" value="5428.0">5428</number>
eastern Palawan, 
<quantity id="4C9462FFA765ED3DB3CCFBEAFC50A147" box="[918,998,1077,1101]" metricMagnitude="3" metricUnit="m" metricValue="2.021" pageId="25" pageNumber="26" unit="m" value="2021.0">2021m</quantity>
