<subSubSection id="BC9B1B98D7968E25C8F89B10CB10660B" pageId="2" pageNumber="63" type="type species">
<paragraph id="CA256AC7E27A5C13105DF76CB62F2D74" pageId="2" pageNumber="63">Type species.</paragraph>
<paragraph id="40D1F3D5E34977B66A0C973644607515" pageId="2" pageNumber="63">
<taxonomicName id="8A2369465CE05CDE2818F4BB05028DE1" authority="Whitfield &amp; Fernández-Triana, 2012" authorityName="Whitfield &amp; Fernández-Triana" authorityYear="2012" class="Insecta" family="Braconidae" genus="Mariapanteles" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Mariapanteles felipei" order="Hymenoptera" pageId="2" pageNumber="63" phylum="Arthropoda" rank="species" species="felipei">Mariapanteles felipei</taxonomicName>
