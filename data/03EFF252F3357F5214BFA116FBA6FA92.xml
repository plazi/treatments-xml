<taxonomicName id="4C4638C7F3357F521675A6BEFCAAFB5A" authorityName="Harold" authorityYear="1879" box="[605,857,1228,1253]" class="Insecta" family="Carabidae" genus="Scarites" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="250" phylum="Arthropoda" rank="subSpecies" species="tenebricosus" subSpecies="fatalis">
