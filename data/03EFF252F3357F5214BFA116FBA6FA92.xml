<taxonomicName id="4C4638C7F3357F5210EBA6BEFA83FB5A" baseAuthorityName="Boheman" baseAuthorityYear="1848" box="[1219,1392,1228,1253]" class="Insecta" family="Carabidae" genus="Prothymidia" kingdom="Animalia" order="Coleoptera" pageId="49" pageNumber="250" phylum="Arthropoda" rank="species" species="angusticollis">
