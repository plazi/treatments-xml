<collectingCountry id="90DB3AC9FFA8FFA8FF01FD94FF474305" box="[178,247,570,591]" name="France" pageId="4" pageNumber="829">France</collectingCountry>
only in 
<collectingCountry id="90DB3AC9FFA8FFA8FEF8FD97FE114305" box="[331,417,569,591]" name="France" pageId="4" pageNumber="829">Corsica</collectingCountry>
!), occurs in 
<collectingCountry id="90DB3AC9FFA8FFA8FDABFD97FD114305" box="[536,673,569,591]" name="Saudi Arabia" pageId="4" pageNumber="829">Saudi Arabia</collectingCountry>
, 
<collectingCountry id="90DB3AC9FFA8FFA8FD19FD94FD434305" box="[682,755,570,591]" name="Yemen" pageId="4" pageNumber="829">Yemen</collectingCountry>
