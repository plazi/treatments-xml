Figs 
<number id="63516420FFD9FF89FF2DFAB9FED5FA28" box="[218,307,1393,1419]" pageId="12" pageNumber="55" value="2.555" valueMax="4.11" valueMin="1.0">1–4, 11</number>
</figureCitation>
, 
<figureCitation id="4DA26A82FFD9FF89FEB7FAB9FE6DFA28" box="[320,395,1393,1419]" captionStart="FIGURES 36 – 38" captionStartId="13.[151,270,1501,1525]" captionTargetBox="[180,1403,600,1463]" captionTargetId="figure@13.[151,1436,575,1495]" captionTargetPageId="13" captionText="FIGURES 36 – 38. Umbothynnus webbiensis male: 36. hypopygium; 37. genitalia. lateral; 38. genitalia, dorsal. Scale line = 0.5 mm." httpUri="https://zenodo.org/record/184904/files/figure.png" pageId="12" pageNumber="55">
<number id="63516420FFD9FF89FEB7FAB9FE6DFA28" box="[320,395,1393,1419]" pageId="12" pageNumber="55" value="37.0" valueMax="38.0" valueMin="36.0">36–38</number>
