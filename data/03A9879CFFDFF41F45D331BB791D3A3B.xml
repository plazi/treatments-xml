<taxonomicName id="4C004D09FFDFF41D475733E07F9339E1" box="[531,650,1966,1990]" class="Insecta" family="Bibionidae" genus="Penthetria" kingdom="Animalia" order="Diptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
