<typeStatus id="E55B41AFFFA2B250F6ECFBFEFD31A142" box="[677,725,1074,1097]" pageId="25" pageNumber="228">TYPE</typeStatus>
SPECIES
</collectionCode>
. — 
<taxonomicName id="FDE0848EFFA2B250F719FBFFFB78A141" authority="Lendenfeld, 1891" authorityName="Lendenfeld" authorityYear="1891" box="[848,1180,1075,1099]" class="Calcarea" family="Sycanthidae" genus="Sycantha" kingdom="Animalia" order="Leucosolenida" pageId="25" pageNumber="228" phylum="Porifera" rank="species" species="tenella">
