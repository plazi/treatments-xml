</bibRefCitation>
, 
<bibRefCitation id="EF9B4B065466FFB0FBE069A26C52DD83" author="Watson" box="[1042,1315,192,218]" pageId="12" pageNumber="34" refString="Watson, J. A. L .. &amp; Abbey, H. M. (1993) Atlas of Australian Termites. CSIRO Division of Entomology, Canberra, 155 pp." type="book" year="1993">
Watson &amp; Abbey 
<number id="3DC224D05466FFB0FB1669A26C52DD83" box="[1252,1315,192,218]" pageId="12" pageNumber="13" value="1993.0">1993</number>
</bibRefCitation>
: 
<number id="3DC224D05466FFB0FADC69A26C2CDD83" box="[1326,1373,192,218]" pageId="12" pageNumber="13" value="110.0">110</number>
).
