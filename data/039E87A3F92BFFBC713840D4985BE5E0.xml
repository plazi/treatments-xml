</taxonomicName>
<taxonomicNameLabel id="A27057DCF92BFFBC755147C998BBE426" box="[1368,1418,1010,1031]" pageId="22" pageNumber="327" rank="species">n.sp.</taxonomicNameLabel>
<collectionCode id="ED26AE70F92BFFBC759A47C9988AE426" box="[1427,1467,1010,1031]" country="Australia" name="Australian Museum" pageId="22" pageNumber="327" type="Museum">AM</collectionCode>
