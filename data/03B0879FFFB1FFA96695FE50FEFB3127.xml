<emphasis id="B96DEA9BFFB3FFB7678FFA5BFF5932AA" bold="true" box="[160,255,1501,1524]" pageId="15" pageNumber="953">Figure 5.</emphasis>
<taxonomicName id="4C194D0AFFB3FFB76627FA5BFDD032AA" authority=", Ssanggyesa" authorityName="Ssanggyesa" box="[264,630,1501,1524]" class="Gymnostomatea" family="Spathidiidae" genus="Spathidium" kingdom="Chromista" order="Spathidiida" pageId="15" pageNumber="953" phylum="Ciliophora" rank="species" species="papilliferum">
