<taxonomicName id="E24FF1F4F6809D373B75F3D106DE2F5C" authority="Walckenaer, 1805" authorityName="Walckenaer" authorityYear="1805" class="Arachnida" family="Araneidae" genus="Epeira" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epeira anatipes" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="anatipes">Epeira anatipes</taxonomicName>
