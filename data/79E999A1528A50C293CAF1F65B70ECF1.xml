<taxonomicName id="CE87E4AB477C61DF36F1D0B4A05A13EC" authority="Martinez, 2020" authorityName="Martinez" authorityYear="2020" class="Insecta" family="Noctuidae" genus="Cicadoforma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cicadoforma" order="Lepidoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="genus">
<emphasis id="D4E54138CB36151CAD0B3D53805D7A55" italics="true" pageId="0" pageNumber="71">Cicadoforma</emphasis>
</taxonomicName>
and 
<taxonomicName id="9B1075AE9C8F9230FFF6E93912BA42C8" authority="Martinez, 2020" authorityName="Martinez" authorityYear="2020" class="Insecta" family="Noctuidae" genus="Cicadomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cicadomorphus" order="Lepidoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="genus">
<emphasis id="E6236A8935A1DE04E6C8B3A2F1C83347" italics="true" pageId="0" pageNumber="71">Cicadomorphus</emphasis>
</taxonomicName>
