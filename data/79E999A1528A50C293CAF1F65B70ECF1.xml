<emphasis id="D5DE85396DF41EDC5F73A6E6DD0E5B9D" bold="true" pageId="0" pageNumber="71">12</emphasis>
<taxonomicName id="8AFB029331FCFAF4BA7E9ABFFBFB988A" authority="Martinez, 2020" authorityName="Martinez" authorityYear="2020" baseAuthorityName="Martinez" baseAuthorityYear="2020" class="Insecta" family="Noctuidae" genus="Cicadoforma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cicadoforma vau-nigrum" order="Lepidoptera" pageId="0" pageNumber="71" phylum="Arthropoda" rank="species" species="vau-nigrum">
