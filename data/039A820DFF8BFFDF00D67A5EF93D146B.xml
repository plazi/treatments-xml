<quantity id="4CCB9EFEFF8CFFDF048E7DDBFE5A15C8" box="[1176,1306,193,217]" metricMagnitude="2" metricUnit="m" metricValue="3.445" metricValueMax="3.5" metricValueMin="3.3899999999999997" pageId="4" pageNumber="3" unit="m" value="344.5" valueMax="350.0" valueMin="339.0">339–350m</quantity>
