<emphasis id="B902EAACB030FFA90D5BFD392DE0D94F" bold="true" box="[799,1412,762,787]" pageId="1" pageNumber="2">
<taxonomicName id="4C764D3DB030FFA90D5BFD392CFED94F" authorityName="Friese" authorityYear="1899" box="[799,1178,762,787]" class="Insecta" family="Apidae" genus="Epicharis" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="iheringii" subGenus="Epicharitides">
<emphasis id="B902EAACB030FFA90D5BFD392BECD94F" bold="true" box="[799,904,762,787]" italics="true" pageId="1" pageNumber="2">Epicharis</emphasis>
(
<emphasis id="B902EAACB030FFA90DD2FD392C48D94F" bold="true" box="[918,1068,762,787]" italics="true" pageId="1" pageNumber="2">Epicharitides</emphasis>
