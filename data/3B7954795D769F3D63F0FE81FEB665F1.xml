<taxonomicName id="74D09EEC5D769F3D63F0FA00FE5065AE" authorityName="Adrain &amp; Karim" authorityYear="2012" box="[151,402,1446,1470]" class="Trilobita" family="Bathyuridae" genus="Pseudoolenoides" kingdom="Animalia" order="Proetida" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="fossilmountainensis">
