<taxonomicName id="71DF4D7DA56BFFDEAF8C424DFDDB1C16" authority="Kriechbaumer, 1895" authorityName="Kriechbaumer" authorityYear="1895" box="[191,582,1105,1130]" class="Dothideomycetes" family="Pseudoperisporiaceae" genus="Acanthostoma" kingdom="Fungi" pageId="6" pageNumber="1120" phylum="Ascomycota" rank="genus">
