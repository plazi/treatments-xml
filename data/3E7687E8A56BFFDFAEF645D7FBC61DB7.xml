<taxonomicName id="71DF4D7DA56BFFDEACF74268FBFE1CF1" authorityName="Kriechbaumer" authorityYear="1895" box="[964,1123,1140,1165]" class="Dothideomycetes" family="Pseudoperisporiaceae" genus="Acanthostoma" kingdom="Fungi" pageId="6" pageNumber="1120" phylum="Ascomycota" rank="genus">
<emphasis id="84ABEAECA56BFFDEACF74268FBFE1CF1" box="[964,1123,1140,1165]" italics="true" pageId="6" pageNumber="1120">Acanthostoma</emphasis>
</taxonomicName>
