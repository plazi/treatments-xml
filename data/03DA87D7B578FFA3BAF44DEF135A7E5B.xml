<taxonomicName id="4C734D42B578FFA0B8F64B6A1428791B" authority="" authorityName="" box="[661,840,1305,1329]" class="Actinopterygii" family="Cyprinidae" genus="Osteocheilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="1" pageNumber="148" phylum="Chordata" rank="species" species="sarawakensis">
<collectingCountry id="F3647651B578FFA0B8F64B6A15C8791A" box="[661,680,1305,1328]" name="Iceland" pageId="1" pageNumber="148">
<emphasis id="B907EAD3B578FFA0B8F64B6A15C8791A" box="[661,680,1305,1328]" italics="true" pageId="1" pageNumber="148">O</emphasis>
</collectingCountry>
