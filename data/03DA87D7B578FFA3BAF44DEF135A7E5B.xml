<collectionCode id="ED62AE04B578FFA0BB574B6916E1791B" LSID="urn:lsid:biocol.org:col:34795" box="[308,385,1306,1329]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="1" pageNumber="148">FMNH</collectionCode>
<number id="3DBB24E6B578FFA0BBE94B6916AD791B" box="[394,461,1306,1329]" pageId="1" pageNumber="148" value="68530.0">68530</number>
