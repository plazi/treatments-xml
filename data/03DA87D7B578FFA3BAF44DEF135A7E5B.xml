<taxonomicName id="4C734D42B578FFA0B99A4A5413C87814" authority="" authorityName="" box="[1017,1192,1062,1086]" class="Actinopterygii" family="Cyprinidae" genus="Osteocheilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="1" pageNumber="148" phylum="Chordata" rank="species" species="sarawakensis">
