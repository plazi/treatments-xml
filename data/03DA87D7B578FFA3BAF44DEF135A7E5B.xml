<typeStatus id="54C88863B578FFA0BE264A8413CB7924" box="[1093,1195,1271,1294]" pageId="1" pageNumber="148">paratypes</typeStatus>
in 
<collectionCode id="ED62AE04B578FFA0BEB24A84127E7924" LSID="urn:lsid:biocol.org:col:34795" box="[1233,1310,1271,1294]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="1" pageNumber="148">FMNH</collectionCode>
<number id="3DBB24E6B578FFA0BF444A84120D7924" box="[1319,1389,1271,1294]" pageId="1" pageNumber="148" value="68530.0">68530</number>
