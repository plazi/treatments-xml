<collectionCode id="ED62AE04B578FFA0BB8C4AC1155C78E3" LSID="urn:lsid:biocol.org:col:34795" box="[495,572,1202,1225]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="1" pageNumber="148">FMNH</collectionCode>
<number id="3DBB24E6B578FFA0B8214AC115E778E3" box="[578,647,1202,1225]" pageId="1" pageNumber="148" value="68908.0">68908</number>
