<collectionCode id="ED62AE04B57BFFA3BE1A4F5113A67D13" LSID="urn:lsid:biocol.org:col:34795" box="[1145,1222,290,313]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="2" pageNumber="149">FMNH</collectionCode>
<number id="3DBB24E6B57BFFA3BEAD4F5112717D13" box="[1230,1297,290,313]" pageId="2" pageNumber="149" value="68908.0">68908</number>
as 
<typeStatus id="54C88863B57BFFA3BF564F5112FB7D13" box="[1333,1435,290,313]" pageId="2" pageNumber="149">paratypes</typeStatus>
of 
<taxonomicName id="4C734D42B57BFFA3BAD54F36160A7D76" authority="" authorityName="" box="[182,362,324,348]" class="Actinopterygii" family="Cyprinidae" genus="Osteocheilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="2" pageNumber="149" phylum="Chordata" rank="species" species="sarawakensis">
<collectingCountry id="F3647651B57BFFA3BAD54F3617A97D76" box="[182,201,325,348]" name="Iceland" pageId="2" pageNumber="149">
<emphasis id="B907EAD3B57BFFA3BAD54F3617A97D76" box="[182,201,325,348]" italics="true" pageId="2" pageNumber="149">O</emphasis>
</collectingCountry>
. 
<emphasis id="B907EAD3B57BFFA3BAB94F37160A7D76" box="[218,362,324,348]" italics="true" pageId="2" pageNumber="149">sarawakensis</emphasis>
</taxonomicName>
. Correspondence regarding catalog numbers for the 
<number id="3DBB24E6B57BFFA3B9CD4F3614837D76" box="[942,995,325,348]" pageId="2" pageNumber="149" value="1993.0">1993</number>
publication indicates that 
<specimenCount id="9D75FD48B57BFFA3BF6B4F3612FB7D76" box="[1288,1435,325,348]" pageId="2" pageNumber="149" type="generic">
<number id="3DBB24E6B57BFFA3BF6B4F3612437D76" box="[1288,1315,325,348]" pageId="2" pageNumber="149" value="20.0">20</number>
specimens
