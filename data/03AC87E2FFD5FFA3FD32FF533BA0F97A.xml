<geoCoordinate id="EE315033FFD5FFA3FBB4FD8B3DCEFD86" box="[1099,1193,524,546]" degrees="145.53" direction="east" orientation="longitude" pageId="110" pageNumber="110" precision="555" value="145.53">145.53E</geoCoordinate>
(Times Atlas), 
<collectingRegion id="49C1F816FFD5FFA3FCB2FDAE3ABEFD9B" box="[845,985,553,575]" country="Australia" name="Queensland" pageId="110" pageNumber="110">Queensland</collectingRegion>
