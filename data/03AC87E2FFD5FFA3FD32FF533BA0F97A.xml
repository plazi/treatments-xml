<location id="8EDA602FFFD5FFA3FCE8FD8B3AB6FD85" LSID="urn:lsid:plazi:treatment:03AC87E2FFD5FFA3FD32FF533BA0F97A:8EDA602FFFD5FFA3FCE8FD8B3AB6FD85" box="[791,977,524,546]" country="Australia" latitude="-25.05" longLatPrecision="749" longitude="145.53" municipality="Mathews Collection" name="Minnie Downs" pageId="110" pageNumber="110" stateProvince="Queensland">Minnie Downs</location>
, 
<geoCoordinate id="EE315033FFD5FFA3FC19FD8B3D51FD86" box="[998,1078,524,546]" degrees="25.05" direction="south" orientation="latitude" pageId="110" pageNumber="110" precision="555" value="-25.05">25.05S</geoCoordinate>
