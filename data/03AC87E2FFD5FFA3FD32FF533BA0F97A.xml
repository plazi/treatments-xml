<materialsCitation id="3B6D3CA9FFD5FFA3FD47FE6B3AB7FD3C" collectingDate="1882-01-11" collectionCode="AMNH" country="Australia" latitude="-25.05" location="Minnie Downs" longLatPrecision="749" longitude="145.53" municipality="Mathews Collection" pageId="110" pageNumber="110" specimenCode="AMNH 691687" specimenCount="1" stateProvince="Queensland" typeStatus="holotype">
<typeStatus id="54BE8856FFD5FFA3FD47FE6B3A5FFDA7" box="[696,824,492,516]" pageId="110" pageNumber="110">HOLOTYPE</typeStatus>
: 
<emphasis id="B971EAE6FFD5FFA3FCB2FE693D60FDA7" bold="true" box="[845,1031,494,515]" pageId="110" pageNumber="110">
<specimenCode id="DBA39E8FFFD5FFA3FCB2FE693D60FDA7" box="[845,1031,494,515]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="110" pageNumber="110">AMNH 691687</specimenCode>
</emphasis>
