<taxonomicName id="73914D7AFFB0FFC1CCEE154AC27EB027" authority="Pelsue &amp; Zhang, 2005" authorityName="Pelsue &amp; Zhang" authorityYear="2005" box="[202,348,1067,1086]" class="Insecta" family="Curculionidae" genus="Curculio" kingdom="Animalia" order="Coleoptera" pageId="4" phylum="Arthropoda" rank="species" species="dhilloni" status="n. sp.">
