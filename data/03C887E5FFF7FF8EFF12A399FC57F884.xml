</emphasis>
<taxonomicName id="4C614D70FFF7FF8EFEBCA3EEFD1DFA8C" authority="Monniot, 1972" authorityName="Monniot" authorityYear="1972" box="[313,757,1265,1290]" class="Arachnida" family="Halacaridae" genus="Scaptognathides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="28" pageNumber="229" phylum="Arthropoda" rank="species" species="planus">
