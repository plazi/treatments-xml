<collectingCountry id="E5C9F2D8814DFEEEE928C8FB1D8A0897" box="[1091,1239,1746,1771]" name="South Africa" pageId="329" pageNumber="330">South Africa</collectingCountry>
, 
<collectingCountry id="E5C9F2D8814DFEEEE98FC8FB1C360897" box="[1252,1387,1746,1771]" name="Madagascar" pageId="329" pageNumber="330">Madagascar</collectingCountry>
