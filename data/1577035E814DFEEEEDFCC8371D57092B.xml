<quantity id="5A261FAD814DFEEEEE62C9171A03092A" box="[777,862,1854,1879]" metricMagnitude="1" metricUnit="m" metricValue="2.9" metricValueMax="5.8" metricValueMin="0.0" pageId="329" pageNumber="330" unit="m" value="29.0" valueMax="58.0" valueMin="0.0">0–58 m</quantity>
