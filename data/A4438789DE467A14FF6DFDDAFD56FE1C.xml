<taxonomicName id="EBEA4D1CDE467A14FABAF828FF70FE8C" authority="They" authorityName="They" class="Insecta" family="Aradidae" genus="Calisius" kingdom="Animalia" lastPageId="9" lastPageNumber="372" order="Hemiptera" pageId="8" pageNumber="371" phylum="Arthropoda" rank="genus">
