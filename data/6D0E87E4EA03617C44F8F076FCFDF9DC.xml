<taxonomicName id="22A74D71EA03617C4582F0ECFDBBFA21" baseAuthorityName="KRAATZ" baseAuthorityYear="1862" box="[456,593,1498,1520]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1193" phylum="Arthropoda" rank="species" species="spinicollis">
