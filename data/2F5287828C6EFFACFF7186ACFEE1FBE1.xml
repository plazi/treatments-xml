<caption id="F384661C8C6CFFA3FF718436FEFCFEB8" ID-DOI="http://doi.org/10.5281/zenodo.4928152" ID-Zenodo-Dep="4928152" httpUri="https://zenodo.org/record/4928152/files/figure.png" pageId="7" pageNumber="375" startId="7.[151,264,1620,1644]" targetBox="[159,1423,189,1585]" targetPageId="7">
<paragraph id="A74436948C6CFFA3FF718436FEFCFEB8" blockId="7.[151,1436,1620,1788]" pageId="7" pageNumber="375">
<emphasis id="958FEA868C6CFFA3FF718436FEAFFE28" bold="true" box="[151,335,1620,1644]" pageId="7" pageNumber="375">FIGURES 20–24</emphasis>
. 
<taxonomicName id="60FB4D178C6CFFA3FEB88436FDA8FE28" authority="Golovatch, 2021" authorityName="Golovatch" authorityYear="2021" box="[350,584,1620,1644]" class="Diplopoda" family="Paradoxosomatidae" genus="Hedinomorpha" kingdom="Animalia" order="Polydesmida" pageId="7" pageNumber="375" phylum="Arthropoda" rank="species" species="jaegeri" status="spec. nov.">
<emphasis id="958FEA868C6CFFA3FEB88436FDA8FE28" box="[350,584,1620,1644]" italics="true" pageId="7" pageNumber="375">Hedinomorpha jaegeri</emphasis>
