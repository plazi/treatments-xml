<taxonomicName id="F1814D29E31CA28F73FBEE5BEE3F4037" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="3" pageNumber="219" phylum="Tracheophyta" rank="variety" species="anthoceros" variety="anthoceros">D. anthoceros var. anthoceros</taxonomicName>
