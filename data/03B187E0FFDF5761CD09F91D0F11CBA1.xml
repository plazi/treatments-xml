<taxonomicName id="4C184D75FFDF575ECDFBF91D0C83CC81" authority="Koch, 1836" authorityName="Koch" authorityYear="1836" box="[506,733,1756,1782]" class="Arachnida" family="Pionidae" genus="Tiphys" kingdom="Animalia" order="Trombidiformes" pageId="31" pageNumber="32" phylum="Arthropoda" rank="genus">
