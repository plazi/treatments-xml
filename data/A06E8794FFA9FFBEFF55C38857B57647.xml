<paragraph id="28783682FFA9FFBEFF55C3CE50C37276" blockId="1.[151,1436,222,355]" pageId="1" pageNumber="280">
<taxonomicName id="EFC74D01FFA9FFBEFF55C3CE500B73E2" authority="Curletti, 2003" authorityName="Curletti" authorityYear="2003" box="[151,546,222,247]" class="Insecta" family="Buprestidae" genus="Agrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="280" phylum="Arthropoda" rank="species" species="extrarmatus">
