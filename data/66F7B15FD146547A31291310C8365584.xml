, 
<taxonomicName id="DF52584C5C7F4697724C7DA5EA2C4FEF" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1863" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="93" pageNumber="94" phylum="Mollusca" rank="species" species="luridus">
