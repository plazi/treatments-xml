<taxonomicName id="4C3FD3ECFFA6FF85F32AFDF8EA7F7F5B" baseAuthorityName="KIESENWETTER" baseAuthorityYear="1850" box="[1054,1143,554,581]" class="Insecta" family="Staphylinidae" genus="Atheta" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="1114" phylum="Arthropoda" rank="species" species="reyi">
