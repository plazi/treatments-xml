, 
<collectionCode id="E021CA540540D854B8A00188274E8A98" country="China" lsid="urn:lsid:biocol.org:col:13092" name="Nanjing University" type="Herbarium">N</collectionCode>
<collectingCountry id="BAECECB8EE73D8EDD114F136B83C3B14" name="Vietnam">Vietnam</collectingCountry>
, SW 
<collectingCountry id="13413254972A12D56D9405895C0555A1" name="China">China</collectingCountry>
(
<collectingRegion id="C5CAA9E0E108B4524F9D7B4BF034500E" country="China" name="Yunnan">Yunnan</collectingRegion>
