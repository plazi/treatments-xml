<taxonomicName id="4C3B4D349549FFAECB065D5AFB1C271F" authorityName=": Barnard" authorityYear="1949" box="[1126,1253,1879,1905]" class="Elasmobranchii" family="Oxynotidae" genus="Oxynotus" kingdom="Animalia" order="Squaliformes" pageId="32" pageNumber="33" phylum="Chordata" rank="species" species="centrina">
