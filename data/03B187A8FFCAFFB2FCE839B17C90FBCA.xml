. 
<typeStatus id="54A3881CFFCAFFB2FBA8384B7B7FFEC7" box="[1026,1078,280,304]" pageId="76">Type</typeStatus>
species 
<taxonomicName id="4C184D3DFFCAFFB2FD7738697B52FEA5" authority="(Meunier)" baseAuthorityName="Meunier" baseAuthorityYear="1905" box="[733,1051,314,338]" class="Insecta" family="Phoridae" genus="Archiphora" kingdom="Animalia" order="Diptera" pageId="76" phylum="Arthropoda" rank="species" species="robusta">
