<taxonomicName id="D715618101B4F6B8FD666BB7BF9E2E99" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:144388" class="Insecta" family="Formicidae" genus="Solenopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Solenopsis exigua Forel" order="Hymenoptera" pageNumber="395" phylum="Arthropoda" rank="species" species="exigua">Solenopsis exigua</taxonomicName>
