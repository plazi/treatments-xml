<taxonomicName id="4C6FC9CCFFC10B67FB25FE7DFAC5FE45" box="[1185,1330,403,427]" class="Insecta" family="Eumenidae" genus="Symmorphus" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="459" phylum="Arthropoda" rank="genus">
