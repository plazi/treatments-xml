<taxonomicName id="F9C94D499C31FF8B8D5217FBFD49FE65" authorityName="Chapuis" authorityYear="1878" box="[534,748,445,468]" class="Insecta" family="Chrysomelidae" genus="Rhagiosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="198" phylum="Arthropoda" rank="species" species="madagascariense">
