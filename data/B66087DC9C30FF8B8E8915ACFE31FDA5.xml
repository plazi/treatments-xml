<taxonomicName id="F9C94D499C31FF8B8C5817FBFC37FE65" authorityName="Sekerka" authorityYear="2007" box="[796,914,445,468]" class="Insecta" family="Chrysomelidae" genus="Rhagiosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="198" phylum="Arthropoda" rank="species" species="chapuisi" status="nom. nov.">
