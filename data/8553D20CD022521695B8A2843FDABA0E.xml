<taxonomicName id="98F3A6EBD7A600B3D77B454D20600E2A" authorityName="Narahara-Nakano, Kakui &amp; Tomikawa" authorityYear="2016" class="Malacostraca" family="Opisidae" genus="Opisa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Opisa takafuminakanoi" order="Amphipoda" pageId="0" pageNumber="99" phylum="Arthropoda" rank="species" species="takafuminakanoi">
