<emphasis id="00E46BC16B6750DCCFB6597492E57AB2" bold="true" pageId="2" pageNumber="3">A</emphasis>
<taxonomicName id="E57B7CB313E4EAE6AE577FD2D3DC25A4" baseAuthorityName="Aldrich" baseAuthorityYear="1889" class="Gastropoda" family="Endodontidae" genus="Philalanka" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Philalanka kusana" order="Stylommatophora" pageId="2" pageNumber="3" phylum="Mollusca" rank="species" species="kusana">
