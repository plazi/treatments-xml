<paragraph id="0F6336A08F468C37FEE7FB43FBE0B776" blockId="3.[268,1101,1252,1620]" box="[268,1101,1279,1300]" pageId="3" pageNumber="63">
<taxonomicName id="C8DC4D238F468C37FEE7FB43FD0FB776" authority="O'Keefe" authorityName="O'Keefe" authorityYear="2000" box="[268,674,1279,1300]" class="Insecta" family="Staphylinidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="63" phylum="Arthropoda" rank="species" species="paramaracayanus">
