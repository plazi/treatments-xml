<collectingRegion id="6F16F2CCFFFDFFC0FC28FB67ED3CFB26" box="[897,1063,1195,1221]" country="Colombia" name="Cundinamarca" pageId="44" pageNumber="45">Cundinamarca</collectingRegion>
, 
