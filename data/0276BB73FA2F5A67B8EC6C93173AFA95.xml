<taxonomicName id="5F47D168AF4515F353077B00E4BA4F6D" LSID="0276BB73-FA2F-5A67-B8EC-6C93173AFA95" authority="Fr." authorityName="Fr." class="Pyrenomycetes" family="Diatrypaceae" genus="Diatrype" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Diatrype" order="Xylariales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="genus">Diatrype Fr.</taxonomicName>
