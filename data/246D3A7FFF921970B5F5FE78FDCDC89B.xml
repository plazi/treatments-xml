<taxonomicName id="6BC4F0EAFF921970B43AFC85FDD3C89B" authority="agilis" authorityName="agilis (SMITH" authorityYear="1858" box="[381,621,892,913]" class="Insecta" family="Crabronidae" genus="Dasyproctus" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="1591" phylum="Arthropoda" rank="subSpecies" species="agilis" subSpecies="agilis">
