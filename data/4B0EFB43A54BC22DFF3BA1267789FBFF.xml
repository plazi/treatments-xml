<caption id="97D81ADDA548C222FF3BA0467122FAA3" httpUri="https://zenodo.org/record/160646/files/figure.png" pageId="7" pageNumber="238" targetBox="[197,1411,264,1215]" targetPageId="7">
<paragraph id="C3184A55A548C222FF3BA0467122FAA3" blockId="7.[151,1436,1300,1384]" pageId="7" pageNumber="238">
