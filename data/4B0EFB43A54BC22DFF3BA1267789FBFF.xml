<taxonomicName id="04A731D6A548C222FEB6A046771AFAE2" box="[282,597,1300,1322]" class="Malacostraca" family="Hyalidae" genus="Protohyale" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="238" phylum="Arthropoda" rank="species" species="magnaocularis" status="sp. nov." subGenus="Boreohyale">
<emphasis id="F1D39647A548C222FEB6A04674C5FAE2" box="[282,394,1300,1321]" italics="true" pageId="7" pageNumber="238">Protohyale</emphasis>
(
<emphasis id="F1D39647A548C222FE34A04774E0FAE1" box="[408,431,1301,1322]" italics="true" pageId="7" pageNumber="238">B.</emphasis>
) 
<emphasis id="F1D39647A548C222FE11A046771AFAE2" box="[445,597,1300,1321]" italics="true" pageId="7" pageNumber="238">magnaocularis</emphasis>
