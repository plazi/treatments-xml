<subSubSection id="DF110A7C4E45FDD599D6274C7282832C" pageId="20" pageNumber="57" type="materials_examined">
<paragraph id="EBE9ABEA0D725255DAC2E087437E51D0" pageId="20" pageNumber="57">Material examined.</paragraph>
<paragraph id="53C77213EAC83427D58A5E7D91CEC047" pageId="20" pageNumber="57">
