<geoCoordinate id="FD4B506C72F4EC3DE6B7D695329E44C6" direction="south" orientation="latitude" precision="55" value="-16.644">16.644°S</geoCoordinate>
, 
<geoCoordinate id="D04444935E736CBE56A0B2DBAF1FE048" direction="east" orientation="longitude" precision="555" value="145.49">145.49°E</geoCoordinate>
