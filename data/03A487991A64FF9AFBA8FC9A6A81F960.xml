<taxonomicName id="4C0D4D0C1A64FF9AFBA8FC9A6AB5FCBF" authorityName="Filhol" authorityYear="1881" box="[1029,1239,791,818]" class="Mammalia" family="Ursidae" genus="Amphicynodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carnivora" pageId="4" pageNumber="604" phylum="Chordata" rank="species" species="sp. 1">
