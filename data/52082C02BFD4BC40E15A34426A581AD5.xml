<geoCoordinate id="AA73B0927DE5CAE02A84EFE6A1167B60" direction="south" orientation="latitude" precision="15" value="-5.1055555">05°06'20&quot;S</geoCoordinate>
, 
<geoCoordinate id="B4867C3F3CE08A4DE9744D41C14D7F56" direction="east" orientation="longitude" precision="15" value="119.284164">119°17'03&quot;E</geoCoordinate>
), June 4, 1997; southeast Likuan, Bunaken, North Sulawesi (
<geoCoordinate id="9458901A71C9F673504564685D9C6900" direction="north" orientation="latitude" precision="925" value="1.6">01°36'N</geoCoordinate>
