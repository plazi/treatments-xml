33. 
<taxonomicName id="F0C5759C50513A55AC0F23D681171848" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:134583" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica angusticollis Jerdon" lsidName-HNS="Formica angusticollis Jerdon" order="Hymenoptera" pageNumber="120" phylum="Arthropoda" rank="species" species="angusticollis">Formica angusticollis</taxonomicName>
