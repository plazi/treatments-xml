, 
<geoCoordinate id="EE74503AFF95FFAEFF6F385EFEA6F92D" box="[237,288,1644,1668]" degrees="4" direction="south" minutes="8" orientation="latitude" pageId="7" pageNumber="291" precision="925" value="-4.133333">S4 8</geoCoordinate>
, 
<geoCoordinate id="EE74503AFF95FFAEFEAC385EFEE7F92D" box="[302,353,1644,1668]" degrees="5" direction="south" minutes="8" orientation="latitude" pageId="7" pageNumber="291" precision="925" value="-5.133333">S5 8</geoCoordinate>
