<collectingCountry id="DDFB6DD5595D2B5089A4A1588DD2566C" box="[656,702,1336,1359]" name="Iran" pageId="0" pageNumber="385">Iran</collectingCountry>
. 
<materialsCitation id="15842718595D2B5089FCA1578B3D5698" ID-GBIF-Occurrence="2830980304" collectingDate="1999-06-08" collectingDateMax="1999-07-10" collectingDateMin="1999-06-08" collectorName="The &amp; Mattingly" country="Iran" county="Rasht" elevation="500" latitude="26.2556" location="Mohammad-Reza Abai in Boghan Village" longLatPrecision="6" longitude="60.7556" municipality="University of Medical Sciences" pageId="0" pageNumber="385" specimenCount="11" specimenCount-female="11" stateProvince="Gilan">
