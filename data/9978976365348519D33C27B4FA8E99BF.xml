<paragraph id="116E26756534851AD18D2627FDC89C34" blockId="97.[189,1398,782,844]" pageId="97" pageNumber="98">
<taxonomicName id="D6D15DF66534851AD18D2627FD0C9C50" authority="Muller &amp; Troschel, 1842" authorityName="Muller &amp; Troschel" authorityYear="1842" box="[189,756,782,808]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="97" pageNumber="98" phylum="Echinodermata" rank="species" species="spinulosa">
