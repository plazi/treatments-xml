<taxonomicName id="D6D15DF66534851AD2D02627FEFE9C34" authority="(Bruzelius, 1805)" baseAuthorityName="Bruzelius" baseAuthorityYear="1805" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiacantha" kingdom="Animalia" order="Ophiurida" pageId="97" pageNumber="98" phylum="Echinodermata" rank="species" species="bidentata">
