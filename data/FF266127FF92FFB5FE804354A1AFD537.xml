<taxonomicName id="B08FABB2FF90FFB5FD3F43ACA104D6EC" box="[695,844,484,509]" class="Insecta" family="Reduviidae" genus="Bactrodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="femoratus">
