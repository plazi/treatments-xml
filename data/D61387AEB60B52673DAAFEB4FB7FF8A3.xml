<collectionCode id="38ABAE7DB60B52653EC8FD6FFBCAFD0D" box="[1013,1075,679,703]" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="12" pageNumber="213">CNC</collectionCode>
