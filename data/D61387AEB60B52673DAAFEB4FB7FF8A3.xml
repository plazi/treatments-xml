, 
<number id="E872249FB60B52653E5DFD6FFC97FD0D" box="[864,878,679,703]" pageId="12" pageNumber="213" value="2.0">2</number>
♀, 
<collectionCode id="38ABAE7DB60B52653EA5FD6FFC10FD0C" box="[920,1001,679,702]" httpUri="http://grbio.org/cool/m1dy-7gjd" name="Kasetsart University Museum of Fisheries" pageId="12" pageNumber="213">KUMF</collectionCode>
