<taxonomicName id="E484A58C07A1425DC1043D076FFF7009" authorityName="Owen" authorityYear="1874" class="Reptilia" family="Criorhynchidae" genus="Criorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pterosauria" pageId="42" pageNumber="43" phylum="Chordata" rank="genus">
