<taxonomicName id="96FC4D11FFC7FFCB1D49FAEC5C1BFA89" box="[769,874,1376,1400]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="pilosa">
<emphasis id="6388EA80FFC7FFCB1D49FAEC5C1BFA89" box="[769,874,1376,1400]" italics="true" pageId="46" pageNumber="47">M. pilosa</emphasis>
</taxonomicName>
