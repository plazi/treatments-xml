<taxonomicName id="F48845D0CBA60233E47D4F61B47D7AF9" authority="Mocsáry, 1904" authorityName="Mocsary" authorityYear="1904" class="Insecta" family="Chrysididae " genus="Cleptidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cleptidea" order="Hymenoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="genus">
