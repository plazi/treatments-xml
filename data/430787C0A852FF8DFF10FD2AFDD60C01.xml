<taxonomicName id="0CAE4D55A852FF8DFD90FAD1FDD90C01" baseAuthorityName="P. L. Sclater &amp; Salvin" baseAuthorityYear="1876" box="[517,612,1284,1307]" class="Aves" family="Thraupidae" genus="Creurgops" kingdom="Animalia" order="Passeriformes" pageId="74" pageNumber="74" phylum="Chordata" rank="species" species="dentatus">
