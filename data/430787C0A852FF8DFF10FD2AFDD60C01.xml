<geoCoordinate id="AE9A5011A852FF8DFF56FB3DFE9C0DE7" box="[195,289,1256,1277]" degrees="70.00" direction="west" orientation="longitude" pageId="74" pageNumber="74" precision="555" value="-70.0">70.00W</geoCoordinate>
