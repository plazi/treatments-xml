<taxonomicName id="1D5CC2961046284F9CB7D966DCD4AC6C" LSID="http://zoobank.org/AA7FC364-5DBB-4768-8EEB-8BA7B60ECB16" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus ricacosta" order="Hymenoptera" pageId="402" pageNumber="403" phylum="Arthropoda" rank="species" species="ricacosta">Heterospilus ricacosta Marsh</taxonomicName>
