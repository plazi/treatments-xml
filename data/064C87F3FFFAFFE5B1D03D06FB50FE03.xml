<paragraph id="8E5A36E5FFFAFFE4B1D03DFDFB81FA76" blockId="4.[151,1039,1444,1497]" box="[151,1039,1475,1497]" pageId="4" pageNumber="529">
TL: 
<collectingCountry id="F6F27675FFFAFFE4B1843DFAFE9EFA76" box="[195,272,1475,1497]" name="Taiwan" pageId="4" pageNumber="529">Taiwan</collectingCountry>
