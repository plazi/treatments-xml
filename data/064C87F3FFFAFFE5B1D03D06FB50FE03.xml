<quantity id="491D9B00FFFAFFE4B3503DFAFDE9FA76" box="[535,615,1475,1497]" metricMagnitude="3" metricUnit="m" metricValue="2.4" pageId="4" pageNumber="529" unit="m" value="2400.0">2400 m</quantity>
