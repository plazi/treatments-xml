<quantity id="A66F9B737D8AFF4BFCBEFB14FC2DFB18" box="[853,905,1216,1242]" metricMagnitude="1" metricUnit="m" metricValue="4.6" pageId="158" pageNumber="159" unit="m" value="46.0">46m</quantity>
