<number id="3DEB661FFFE4A45FFEE5F917FED479E6" box="[335,348,1689,1713]" pageId="36" pageNumber="37" value="3.0">3</number>
, E 
<number id="3DEB661FFFE4A45FFED1F917FE0279E6" box="[379,394,1689,1713]" pageId="36" pageNumber="37" value="2.0">2</number>
