<taxonomicName id="4C694D029911FFA078D0D8B4FEACBE8D" authorityName="Schoenherr" authorityYear="1825" box="[144,270,1235,1260]" class="Insecta" family="Erirhinidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="family">
