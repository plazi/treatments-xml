<heading id="D0D981DBFFDDFF84FF1224BF919FFB0F" bold="true" box="[151,1259,1167,1194]" fontSize="11" level="1" pageId="18" pageNumber="419" reason="1">
<emphasis id="B95AEAA5FFDDFF84FF1224BF919FFB0F" bold="true" box="[151,1259,1167,1194]" pageId="18" pageNumber="419">
<taxonomicName id="4C2E4D34FFDDFF84FF1224BF94B8FB0C" authority="Javanmardi &amp; Vogler &amp; Joger, 2019" authorityName="Javanmardi &amp; Vogler &amp; Joger" authorityYear="2019" box="[151,460,1167,1194]" class="Reptilia" family="Sphaerodactylidae" genus="Saurodactylus" kingdom="Animalia" order="Squamata" pageId="18" pageNumber="419" phylum="Chordata" rank="species" species="splendidus" status="sp. nov.">
<emphasis id="B95AEAA5FFDDFF84FF1224BF94B8FB0C" bold="true" box="[151,460,1167,1194]" italics="true" pageId="18" pageNumber="419">Saurodactylus splendidus</emphasis>
