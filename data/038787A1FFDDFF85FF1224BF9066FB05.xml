<quantity id="4CD69B52FFDCFF85FCFA23119692FC9D" box="[895,998,801,827]" metricMagnitude="-3" metricUnit="m" metricValue="7.06" pageId="19" pageNumber="420" unit="mm" value="7.06">7.06 mm</quantity>
; HW = 
<quantity id="4CD69B52FFDCFF85FBBA231091EDFC9D" box="[1087,1177,800,827]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="19" pageNumber="420" unit="mm" value="4.5">4.5 mm</quantity>
; HD = 
<quantity id="4CD69B52FFDCFF85FB6923119020FC9D" box="[1260,1364,801,827]" metricMagnitude="-3" metricUnit="m" metricValue="2.82" pageId="19" pageNumber="420" unit="mm" value="2.82">2.82 mm</quantity>
; ED = 
<quantity id="4CD69B52FFDCFF85FF122375958BFCF9" box="[151,255,837,863]" metricMagnitude="-3" metricUnit="m" metricValue="1.24" pageId="19" pageNumber="420" unit="mm" value="1.24">1.24 mm</quantity>
