<taxonomicName id="4C610AA2FFA8FFB84921FD0BFC0AFD42" authority="Nakane, 1969" authorityName="Nakane" authorityYear="1969" box="[620,961,696,721]" class="Insecta" family="Lycidae" genus="Cautires" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="1493" phylum="Arthropoda" rank="species" species="yausai">
<emphasis id="B915AD33FFA8FFB84921FD0BFCD2FD42" bold="true" box="[620,793,696,720]" italics="true" pageId="31" pageNumber="1493">Cautires yausai</emphasis>
