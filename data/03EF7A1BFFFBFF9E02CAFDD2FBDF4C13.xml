<typeStatus id="54FD75AFFFFBFF9F0380FD0BFF3F4C8A" box="[168,227,677,703]" pageId="24" pageNumber="25">TYPE</typeStatus>
SPECIES: 
<taxonomicName id="4C46B08EFFFBFF9F0271FD0AFCAC4C8A" authority="Jakubski, 1965" authorityName="Jakubski" authorityYear="1965" box="[345,880,676,703]" class="Insecta" family="Margarodidae" genus="Heteromargarodes" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="americanus">
<emphasis id="B932171FFFFBFF9F0271FD0AFD644C8B" box="[345,696,676,702]" italics="true" pageId="24" pageNumber="25">Heteromargarodes americanus</emphasis>
<bibRefCitation id="EFD7B6FCFFFBFF9F01EBFD0AFCAC4C8A" author="Jakubski, A. W." box="[707,880,676,703]" pageId="24" pageNumber="25" refId="ref36851" refString="Jakubski, A. W. 1965. A critical revision of the families Margarodidae and Termitococcidae (Hemiptera, Coccoidea). London: Trustees of the British Museum (Natural History)." type="book" year="1965">Jakubski, 1965</bibRefCitation>
</taxonomicName>
