<taxonomicName id="30AB2FA5FFC5FF9B684845F97E9C6FF5" authority="Zlotorzycka, 1964 a" authorityName="Zlotorzycka" authorityYear="1964" box="[311,697,901,927]" class="Insecta" family="Philopteridae" genus="Maculinirmus" kingdom="Animalia" order="Psocodea" pageId="55" pageNumber="56" phylum="Arthropoda" rank="genus">
