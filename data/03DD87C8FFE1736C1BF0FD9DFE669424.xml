<collectingRegion id="49B0F83CFFE1736D1C8EFAF6FB259209" box="[1212,1264,1309,1333]" country="United Kingdom" name="York" pageId="88" pageNumber="365">York</collectingRegion>
<location id="8EAB6005FFE1736D1CC8FAF6FAEB9209" LSID="urn:lsid:plazi:treatment:03DD87C8FFE1736C1BF0FD9DFE669424:8EAB6005FFE1736D1CC8FAF6FAEB9209" box="[1274,1342,1309,1333]" county="Type" municipality="Duke" name="Island" pageId="88" pageNumber="365" stateProvince="York">Island</location>
