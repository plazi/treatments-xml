<taxonomicName id="4C744D5DFFE1736D1B61FAF5FC219209" box="[851,1012,1310,1333]" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="88" pageNumber="365" phylum="Chordata" rank="species" species="musavora">
