<taxonomicName id="5A2065A4A7E2568A3FBACBAC219DCCDA" authority="Cuvier, 1817" authorityName="Cuvier" authorityYear="1817" class="Aves" family="Artamidae" genus="Ocypterus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ocypterus" order="Passeriformes" pageId="8" pageNumber="475" phylum="Chordata" rank="genus">Ocypterus</taxonomicName>
