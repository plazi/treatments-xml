and 
<taxonomicName id="59AC10B42027FF9AFAFCF9D3FAED2A0B" authority="Tang &amp; Li, 2010" authorityName="Tang &amp; Li" authorityYear="2010" box="[1296,1395,1627,1651]" class="Arachnida" family="Thomisidae" genus="Spilosynema" kingdom="Animalia" order="Araneae" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="ravum" status="sp. nov.">
