<taxonomicName id="96D253DBFFD7117EFC929C24819CAB68" authorityName="Sternberg" authorityYear="1953" box="[884,1227,1833,1866]" class="Reptilia" family="Hadrosauridae" genus="Brachylophosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="1" phylum="Chordata" rank="species" species="canadensis">
<emphasis id="63A6F44AFFD7117EFC929C24819CAB68" bold="true" box="[884,1227,1833,1866]" italics="true" pageId="1">Brachylophosaurus canadensis</emphasis>
