<taxonomicName id="9C0FF06A76564EF66A7B787706F18ADD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33647" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName="Pheidole capensis Mayr" lsidName-HNS="Pheidole capensis Mayr" order="Hymenoptera" pageNumber="746" phylum="Arthropoda" rank="species" species="capensis">P. capensis</taxonomicName>
