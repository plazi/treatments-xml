, and 
<taxonomicName id="80C92092B6AFE208FA326A14A116E576" class="Actinopteri" genus="H." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="H. torrenticola" order="Characiformes" pageId="0" pageNumber="25" phylum="Chordata" rank="species" species="torrenticola">
<emphasis id="462085791E534B9FFA13A414D2B91425" italics="true" pageId="0" pageNumber="25">H. torrenticola</emphasis>
