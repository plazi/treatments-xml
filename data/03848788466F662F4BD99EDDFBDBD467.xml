<taxonomicName id="4C2D4D1D466F662E48C59A78FBB0D00C" baseAuthorityName="Erichson" baseAuthorityYear="1840" box="[912,1061,1590,1611]" class="Insecta" family="Staphylinidae" genus="Cafius" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="133" phylum="Arthropoda" rank="species" species="bistriatus">Cafius bistriatus</taxonomicName>
