<taxonomicName id="540FCB8D030BFB2DABC1C69776F3C479" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181941" class="Insecta" family="Formicidae" genus="Pheidole" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole sculptior Forel" order="Hymenoptera" pageNumber="41" phylum="Arthropoda" rank="species" species="sculptior">sculptior</taxonomicName>
