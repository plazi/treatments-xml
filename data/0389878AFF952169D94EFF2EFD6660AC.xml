<taxonomicName id="4C204D1FFF952169DBD7FE36FF4163B1" authorityName="Wei and Gu" authorityYear="2019" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="24" pageNumber="2806" phylum="Firmicutes" rank="species" species="jidongensis">
