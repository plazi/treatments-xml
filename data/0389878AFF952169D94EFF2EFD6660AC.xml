<taxonomicName id="4C204D1FFF952169D80AFE36FE7763DF" authorityName="Wei and Gu" authorityYear="2019" box="[317,472,416,440]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="24" pageNumber="2806" phylum="Firmicutes" rank="species" species="kedongensis">
