<taxonomicName id="4C7C1B8DFFFFFFC9BB5AF809E3A2C2C1" authority="Mainda, 2020" authorityName="Mainda" authorityYear="2020" box="[605,731,2008,2032]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="407" phylum="Arthropoda" rank="species" species="praedator" status="sp. nov.">
