long, 
<quantity id="82189B0D4A73E002FD12FB8AFD7EFBDF" box="[649,734,1046,1073]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="31" pageNumber="32" unit="mm" value="1.7">1.7 mm</quantity>
