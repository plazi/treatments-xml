<taxonomicName id="4C1D4D47FFDF4E51208705D05B34D7AC" authorityName="Wight &amp; Arnott K" authorityYear="1834" box="[430,593,158,185]" class="Magnoliopsida" family="Capparaceae" genus="Capparis" kingdom="Plantae" order="Brassicales" pageId="1" pageNumber="126" phylum="Tracheophyta" rank="species" species="diversifolia">
<emphasis id="B969EAD6FFDF4E51208705D05B34D7AC" box="[430,593,158,185]" italics="true" pageId="1" pageNumber="126">C. diversifolia</emphasis>
</taxonomicName>
