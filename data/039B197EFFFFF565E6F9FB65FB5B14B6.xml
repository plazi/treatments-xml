<taxonomicName id="4C32D3EBFFFEF565E243FF23FB2814B9" baseAuthorityName="Lamouroux" baseAuthorityYear="1816" box="[1069,1201,224,248]" class="Hydrozoa" family="Sertulariidae" genus="Sertularia" kingdom="Animalia" order="Leptothecata" pageId="33" pageNumber="34" phylum="Cnidaria" rank="species" species="turbinata">
<emphasis id="B946747AFFFEF565E243FF23FB2814B9" box="[1069,1201,224,248]" italics="true" pageId="33" pageNumber="34">S. turbinata</emphasis>
