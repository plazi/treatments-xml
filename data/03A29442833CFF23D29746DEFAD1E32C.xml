<emphasis id="B97FF946833CFF22D29746DEFD14E385" bold="true" box="[151,722,1688,1714]" pageId="4" pageNumber="55">
<taxonomicName id="4C0B5ED7833CFF22D29746DEFD44E385" authority="Pinkaew and Zhang" authorityName="Pinkaew and Zhang" box="[151,642,1688,1714]" class="Insecta" family="Tortricidae" genus="Fibuloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="55" phylum="Arthropoda" rank="species" species="khaoyai" status="n. sp.">
<emphasis id="B97FF946833CFF22D29746DEFEB9E385" bold="true" box="[151,383,1688,1714]" italics="true" pageId="4" pageNumber="55">Fibuloides khaoyai</emphasis>
Pinkaew and Zhang
</taxonomicName>
, 
