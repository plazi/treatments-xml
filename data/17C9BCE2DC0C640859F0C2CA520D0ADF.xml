<taxonomicName id="F1509C33956CE1E6D23E9D0938F90BCE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:152" family="Formicidae" lsidName-HNS="Formicidae Latreille" rank="family">Formicidae</taxonomicName>
