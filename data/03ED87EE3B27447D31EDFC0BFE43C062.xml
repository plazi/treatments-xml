<taxonomicName id="4C444D7B3B27447D35BFFA7BFAE0C3D6" box="[1221,1325,1492,1518]" class="Insecta" family="Bethylidae" genus="Odontepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="slides">
