<taxonomicName id="7CAE68F5A569ED75938716E2A18D6553" authorityName="Achterberg" authorityYear="2021" class="Insecta" family="Braconidae" genus="Phanerotoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phanerotoma glabritemporalis" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="glabritemporalis">
