<taxonomicName id="4C0974AE646AFFDFFD3DFD70FCB85595" authorityName="Gill" authorityYear="1873" box="[741,889,742,767]" class="Mammalia" family="Phocidae" genus="Histriophoca" kingdom="Animalia" order="Carnivora" pageId="96" pageNumber="97" phylum="Chordata" rank="genus">
