<emphasis id="B97DD33F646AFFDFFCE7FD54FC4C55B1" box="[831,909,706,731]" italics="true" pageId="96" pageNumber="97">
<taxonomicName id="4C0974AE646AFFDFFCE7FD54FC4855B1" authorityName="Linnaeus" authorityYear="1758" box="[831,905,706,731]" class="Mammalia" family="Phocidae" genus="Phoca" kingdom="Animalia" order="Carnivora" pageId="96" pageNumber="97" phylum="Chordata" rank="genus">Phoca</taxonomicName>
