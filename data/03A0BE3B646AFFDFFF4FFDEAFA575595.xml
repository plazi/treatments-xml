<taxonomicName id="4C0974AE646AFFDFFF4FFD54FEEA55B1" authorityName="Gill" authorityYear="1873" box="[151,299,706,731]" class="Mammalia" family="Phocidae" genus="Histriophoca" kingdom="Animalia" order="Carnivora" pageId="96" pageNumber="97" phylum="Chordata" rank="genus">
