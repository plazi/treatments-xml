<taxonomicName id="4C261AF8660FFF9509FED5B5FAA7FF17" box="[1171,1302,218,243]" class="Magnoliopsida" family="Bignoniaceae" genus="Adenocalymma" kingdom="Plantae" order="Lamiales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="comosum">
