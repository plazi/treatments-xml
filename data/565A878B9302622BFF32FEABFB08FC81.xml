<taxonomicName id="19F34D1E9302622BFAAEFD55FAC0FD64" authority="Fallén, 1810" authorityName="Fallén" authorityYear="1810" box="[1291,1353,729,752]" class="Insecta" family="Muscidae" genus="Lispa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
