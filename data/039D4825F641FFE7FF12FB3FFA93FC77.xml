<td id="76D5920DF647001CFD31FDD8FC03FD96" box="[692,945,608,631]" gridcol="1" gridrow="6" pageId="4" pageNumber="5">yes</td>
<td id="76D5920DF647001CFC55FDD8FB1FFD96" box="[976,1197,608,631]" gridcol="2" gridrow="6" pageId="4" pageNumber="5">yes</td>
<td id="76D5920DF647001CFB40FDD8FA2EFD96" box="[1221,1436,608,631]" gridcol="3" gridrow="6" pageId="4" pageNumber="5">no</td>
</tr>
