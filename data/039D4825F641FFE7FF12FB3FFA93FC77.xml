<td id="76D5920DF647001CFD31FDB2FC03FDC0" box="[692,945,522,545]" gridcol="1" gridrow="4" pageId="4" pageNumber="5">narrow</td>
<td id="76D5920DF647001CFC55FDB2FB1FFDC0" box="[976,1197,522,545]" gridcol="2" gridrow="4" pageId="4" pageNumber="5">stout</td>
<td id="76D5920DF647001CFB40FDB2FA2EFDC0" box="[1221,1436,522,545]" gridcol="3" gridrow="4" pageId="4" pageNumber="5">stout</td>
</tr>
