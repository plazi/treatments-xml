<subSubSection id="21D4D5C5384BFFABB48FFC15FC03EF87" pageId="13" pageNumber="566" type="reference_group">
<paragraph id="6971864E384BFFABB48FFC15FC03EF87" blockId="13.[675,1211,913,1161]" pageId="13" pageNumber="566">
<taxonomicName id="AECEFDCD384BFFABB48FFC15FC37E8C4" authorityName="Guinot" authorityYear="2006" baseAuthorityName="Rathbun" baseAuthorityYear="1909" box="[677,931,944,970]" class="Malacostraca" family="Hexapodidae" genus="Hexalaughlia" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="566" phylum="Arthropoda" rank="species" species="chuenensis" status="n. comb.">
