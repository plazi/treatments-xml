<taxonomicName id="F0D519D2FFDC2B00A561FDC39DCBFD83" authorityName="COIFFAIT" authorityYear="1963" box="[358,482,600,621]" class="Insecta" family="Staphylinidae" genus="Quedius" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="248" phylum="Arthropoda" rank="species" species="job">
