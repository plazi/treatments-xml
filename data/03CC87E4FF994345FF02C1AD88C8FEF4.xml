</taxonomicName>
? 
<emphasis id="B911EAE0FF9A4345FC8EC4678BBEFE8D" box="[795,903,260,285]" italics="true" pageId="30" pageNumber="31">lasiolepis</emphasis>
(
<taxonomicName id="4C654D71FF9A4345FC34C4678C24FE8D" box="[929,1053,260,285]" class="Magnoliopsida" family="Salicaceae" kingdom="Plantae" order="Malpighiales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="family">Salicaceae</taxonomicName>
), and 
<taxonomicName id="4C654D71FF9A4345FBE2C4678DA5FE8D" box="[1143,1436,260,285]" class="Magnoliopsida" family="Lauraceae" genus="Umbellularia" kingdom="Plantae" order="Laurales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="californica">
