<taxonomicName id="4C4F4D0CFFDFFFA9F1A8FC2E2D7877D5" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[765,880,979,1002]" class="Insecta" family="Derbidae" genus="Muiralevu" kingdom="Animalia" order="Hemiptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="parvus" status="sp. nov.">
