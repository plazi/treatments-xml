<taxonomicName id="EEC9F9A9FFBFFF8AF287AF45A0BCA72A" authority="Lea, 1909: 186" authorityName="Lea" authorityPageNumber="186" authorityYear="1909" box="[151,748,1080,1103]" class="Insecta" family="Curculionidae" genus="Melanteriosoma" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="variety" species="costatum" variety="tasmaniense">
<emphasis id="1BBD5E38FFBFFF8AF287AF45A3CFA72B" box="[151,415,1080,1103]" italics="true" pageId="23" pageNumber="24">Melanteriosoma costatum</emphasis>
