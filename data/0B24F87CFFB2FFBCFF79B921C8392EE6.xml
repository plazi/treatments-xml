<materialsCitation id="33E54337FFB2FFBCFE6FB96DCB4B2AF6" ID-GBIF-Occurrence="2433522090" collectingDate="1989-02-14" collectingDateMax="1989-03-06" collectingDateMin="1989-02-14" collectionCode="MIZA" collectorName="J. DeMarmels &amp; A. Chacon" country="Venezuela" elevation="160" latitude="2.036111" location="Base Camp and surroundings, on the upper Rio Mavaca, left bank" longitude="-65.113884" pageId="10" pageNumber="66" specimenCount="1" specimenCount-female="1" stateProvince="Amazonas">
<specimenCount id="958B82E3FFB2FFBCFE6FB96DC8F72AB4" box="[424,472,2166,2194]" pageId="10" pageNumber="67" type="female">1♀</specimenCount>
