<emphasis id="B97C6774FFDDFF83FCD5FC46EF7AFC12" box="[791,833,949,975]" italics="true" pageId="6" pageNumber="248">gd6</emphasis>
, 
<emphasis id="B97C6774FFDDFF83FC92FC46EF42FC12" box="[848,889,949,975]" italics="true" pageId="6" pageNumber="248">gd8</emphasis>
and 
<emphasis id="B97C6774FFDDFF83FC70FC46EFE7FC12" box="[946,988,949,975]" italics="true" pageId="6" pageNumber="248">gd9</emphasis>
), 18 pairs of dorsal setae and two pairs of sub-lateral setae: 
<emphasis id="B97C6774FFDDFF83FEB8FC29EDABFC29" box="[378,400,986,1012]" italics="true" pageId="6" pageNumber="248">j1</emphasis>
28; 
<emphasis id="B97C6774FFDDFF83FE00FC29EDE3FC29" box="[450,472,986,1012]" italics="true" pageId="6" pageNumber="248">j3</emphasis>
33, 34; 
<emphasis id="B97C6774FFDDFF83FDF6FC29EE71FC29" box="[564,586,986,1012]" italics="true" pageId="6" pageNumber="248">j4</emphasis>
17; 
<emphasis id="B97C6774FFDDFF83FDBEFC29EEA9FC29" box="[636,658,986,1012]" italics="true" pageId="6" pageNumber="248">
j 
<date id="FFB69DA6FFDDFF83FD44FC29EEA9FC29" box="[646,658,986,1012]" pageId="6" pageNumber="248" value="2017-05-16">5</date>
