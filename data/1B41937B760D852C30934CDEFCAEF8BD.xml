species: 
<taxonomicName id="54E859EE760D852C31224CA5FD54F924" authority="Distant, 1918" authorityName="Distant" authorityYear="1918" box="[294,656,1524,1546]" class="Insecta" family="Cicadellidae" genus="Empoascanara" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="prima">
