<taxonomicName id="9FE5DEE0E0530DF914B130CAD277C457" class="Insecta" family="Lampyridae" genus="Luciola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Luciola" order="Coleoptera" pageId="159" pageNumber="160" phylum="Arthropoda" rank="genus">Luciola</taxonomicName>
