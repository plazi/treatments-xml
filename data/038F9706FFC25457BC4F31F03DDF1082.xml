<taxonomicName id="4C265D93FFC25457BC1633FC3D5410C4" box="[884,1083,659,683]" class="Insecta" family="Dynastidae" genus="Phileurus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="584" phylum="Arthropoda" rank="species" species="undetermined">
