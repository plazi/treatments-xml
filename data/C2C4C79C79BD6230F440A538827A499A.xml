<taxonomicName id="81673E353AB58EC8D635FB623C848523" LSID-ZBK="urn:lsid:zoobank.org:act:E0FC1BFA-4268-4955-BBF3-891B2146C227" class="Actinopterygii" family="Ariidae" genus="Arius" kingdom="Animalia" lsidName-ZBK="Arius caelatus Valenciennes 1840:66" order="Siluriformes" pageNumber="46" phylum="Chordata" rank="species" species="caelatus">A. caelatus</taxonomicName>
