[[ 
<taxonomicName id="9BAC556439C22E6C40800CDDB53CAB27" LSID-ZBK="urn:lsid:zoobank.org:act:E7DA85A9-DDF7-44BF-BD07-1F567EE506ED" class="Actinopterygii" family="Rivulidae" genus="Moema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Moema apurinan Costa 2004:3" order="Cyprinodontiformes" pageNumber="11" phylum="Chordata" rank="species" species="apurinan">Moema apurinan Costa</taxonomicName>
