<taxonomicName id="5BC34D56FFECF82416F1F91AFCE28145" authorityName="Humes" authorityYear="1996" box="[687,822,1773,1799]" class="Maxillopoda" family="Dirivultidae" genus="Aphotopontius" kingdom="Animalia" order="Siphonostomatoida" pageId="2" pageNumber="303" phylum="Arthropoda" rank="species" species="atlanteus">
<emphasis id="AEB7EAC7FFECF82416F1F91AFD148145" box="[687,704,1773,1799]" italics="true" pageId="2" pageNumber="303">A</emphasis>
