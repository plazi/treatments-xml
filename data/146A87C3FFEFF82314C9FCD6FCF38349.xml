<taxonomicName id="5BC34D56FFECF8241732F952FB9880FD" authority="Lee &amp; Kim &amp; Kim, 2020" authorityName="Lee &amp; Kim &amp; Kim" authorityYear="2020" box="[876,1100,1701,1727]" class="Maxillopoda" family="Dirivultidae" genus="Aphotopontius" kingdom="Animalia" order="Siphonostomatoida" pageId="2" pageNumber="303" phylum="Arthropoda" rank="species" species="kiost" status="n. sp.">
