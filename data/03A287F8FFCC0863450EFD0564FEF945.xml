<taxonomicName id="4C0B4D6DFFCC086346FAFD4F637AFD3D" authority="Grote, 1883" authorityName="Grote" authorityYear="1883" box="[764,1036,732,758]" class="Insecta" family="Noctuidae" genus="Trichoclea" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
