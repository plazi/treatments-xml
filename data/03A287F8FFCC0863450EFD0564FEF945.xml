<taxonomicName id="4C0B4D6DFFCC0863450EFC976523FCD5" authority="Ochsenheimer, 1816" authorityName="Ochsenheimer" authorityYear="1816" box="[264,597,772,798]" class="Insecta" family="Noctuidae" genus="Anarta" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
<emphasis id="B97FEAFCFFCC0863450EFC97662FFCD6" box="[264,345,772,797]" italics="true" pageId="35" pageNumber="36">Anarta</emphasis>
Ochsenheimer, 
<number id="3DC324C9FFCC0863461CFC976523FCD5" box="[538,597,772,798]" pageId="35" pageNumber="36" value="1816.0">1816</number>
</taxonomicName>
