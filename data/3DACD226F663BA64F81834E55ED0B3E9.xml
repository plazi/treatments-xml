<taxonomicName id="00D1FB82D12205CE0D107CA06E605F0A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:36108" class="Insecta" family="Formicidae" genus="Simopone" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Simopone grandidieri Forel" order="Hymenoptera" pageNumber="248" phylum="Arthropoda" rank="species" species="grandidieri">S. Grandidieri</taxonomicName>
