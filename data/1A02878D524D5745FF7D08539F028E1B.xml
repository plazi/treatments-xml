<taxonomicName id="55AB4D18524D5745FE830A3D990A8E1B" authority="Ansorge, 1994" authorityName="Ansorge" authorityYear="1994" box="[361,855,769,794]" class="Insecta" family="Tanyderidae" genus="Nannotanyderus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="126" phylum="Arthropoda" rank="species" species="krzeminskii">
