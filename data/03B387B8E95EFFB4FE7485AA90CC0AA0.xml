<collectingCountry id="F30D763EE95EFFB4FD128BAE914D0AF2" box="[733,827,2451,2493]" name="India" pageId="21" pageNumber="184">India</collectingCountry>
, 
<collectingCountry id="F30D763EE95EFFB4FC968BAE91B40AF2" box="[857,962,2451,2493]" name="China" pageId="21" pageNumber="184">China</collectingCountry>
, and 
<collectingCountry id="F30D763EE95EFFB4FBFC8BAE96AD0AF2" box="[1075,1243,2451,2493]" name="Australia" pageId="21" pageNumber="184">Australia</collectingCountry>
. Now recorded from 
<collectingCountry id="F30D763EE95EFFB4F94C8BAE949A0AF2" box="[1667,1772,2451,2493]" name="Guam" pageId="21" pageNumber="184">Guam</collectingCountry>
