<number id="625F4613FFA6605CFB10762FFAB30E76" box="[1278,1286,799,812]" pageId="7" pageNumber="54" value="3.0">
<emphasis id="E6E38826FFA6605CFB10762FFAB30E76" box="[1278,1286,799,812]" italics="true" pageId="7" pageNumber="54">3</emphasis>
