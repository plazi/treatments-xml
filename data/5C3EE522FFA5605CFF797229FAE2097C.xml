<emphasis id="E6E38826FFA6605CFF3A77DAFF450E5B" box="[212,240,746,769]" italics="true" pageId="7" pageNumber="54">ag</emphasis>
<number id="625F4613FFA6605CFF1E77C8FF4D0E5F" box="[240,248,760,773]" pageId="7" pageNumber="54" value="1.0">
<emphasis id="E6E38826FFA6605CFF1E77C8FF4D0E5F" box="[240,248,760,773]" italics="true" pageId="7" pageNumber="54">1</emphasis>
</number>
; setae 
<emphasis id="E6E38826FFA6605CFEA077DAFEDF0E5B" box="[334,362,746,769]" italics="true" pageId="7" pageNumber="54">an</emphasis>
<number id="625F4613FFA6605CFE8477C8FEC70E5F" box="[362,370,760,773]" pageId="7" pageNumber="54" value="1.0">
<emphasis id="E6E38826FFA6605CFE8477C8FEC70E5F" box="[362,370,760,773]" italics="true" pageId="7" pageNumber="54">1</emphasis>
</number>
and 
<emphasis id="E6E38826FFA6605CFE5F77DAFE780E5B" box="[433,461,746,769]" italics="true" pageId="7" pageNumber="54">an</emphasis>
<number id="625F4613FFA6605CFE2277C8FE610E5F" box="[460,468,760,773]" pageId="7" pageNumber="54" value="2.0">
<emphasis id="E6E38826FFA6605CFE2277C8FE610E5F" box="[460,468,760,773]" italics="true" pageId="7" pageNumber="54">2</emphasis>
</number>
