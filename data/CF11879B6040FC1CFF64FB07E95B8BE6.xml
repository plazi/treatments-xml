<taxonomicName id="80B84D0E6040FC1FFBA0F81EEE888CC2" authorityName="Ma &amp; Shih &amp; Ren &amp; Wang" authorityYear="2020" box="[1107,1272,2007,2033]" class="Insecta" family="Osmylidae" genus="Mirokempynus" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="95" phylum="Arthropoda" rank="genus">
