<taxonomicName id="6254334DF4F52D9A1AE97909A3AB531A" authority="Verh." authorityName="Verh." class="Diplopoda" family="Polydesmidae" genus="Polydesmus" higherTaxonomySource="CoL" kingdom="Fungi" order="Polydesmida" pageId="0" pageNumber="98" phylum="Ascomycota" rank="species" species="herzegowinensis">
<pageBreakToken id="21444CD20E7AF9261869B834AF85B54C" pageId="0" pageNumber="98" start="start">Polydesmus</pageBreakToken>
