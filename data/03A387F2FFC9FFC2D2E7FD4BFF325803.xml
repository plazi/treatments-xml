<taxonomicName id="4C0A4D67FFC9FFC2D02EF8D6FC155855" authority="sardous" authorityName="sardous (Perris" authorityYear="1869" box="[606,978,1811,1836]" class="Insecta" family="Brachyceridae" genus="Raymondiellus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="322" phylum="Arthropoda" rank="subSpecies" species="sardous" subSpecies="sardous">
