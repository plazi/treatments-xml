<taxonomicName id="4C4E4D03FFE8FFCD31DAFB20FD11CE02" authority="Ashe" authorityName="Ashe" box="[446,703,1279,1305]" class="Insecta" family="Staphylinidae" genus="Siagotanyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="species" species="rufa">
