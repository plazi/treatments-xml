<taxonomicName id="E89D4D202008E355FF1DBC14FDADFAAF" authority="(Schein-Fatton, 1985)" baseAuthorityName="Schein-Fatton" baseAuthorityYear="1985" box="[212,613,1294,1317]" class="Bivalvia" family="Propeamussiidae" genus="Catillopecten" kingdom="Animalia" order="Pectinida" pageId="22" pageNumber="99" phylum="Mollusca" rank="species" species="vulcani">
