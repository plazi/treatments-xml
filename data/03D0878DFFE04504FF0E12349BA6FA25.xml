<subSubSection id="C3636510FFE04504FF0E123498D3FCE6" box="[151,493,828,855]" pageId="1" pageNumber="502" type="nomenclature">
<paragraph id="8BC6369BFFE04504FF0E123498D3FCE6" blockId="1.[151,493,828,855]" box="[151,493,828,855]" pageId="1" pageNumber="502">
<heading id="D08E81F7FFE04504FF0E123498D3FCE6" bold="true" box="[151,493,828,855]" fontSize="11" level="1" pageId="1" pageNumber="502" reason="1">
<emphasis id="B90DEA89FFE04504FF0E123498D3FCE6" bold="true" box="[151,493,828,855]" pageId="1" pageNumber="502">
Genus 
<taxonomicName id="4C794D18FFE04504FF77123598D3FCE6" authority="Cook, 1899" authorityName="Cook" authorityYear="1899" box="[238,493,828,855]" class="Chilopoda" family="Ballophilidae" genus="Ityphilus" kingdom="Animalia" order="Geophilomorpha" pageId="1" pageNumber="502" phylum="Arthropoda" rank="genus">
<emphasis id="B90DEA89FFE04504FF7712359864FCE7" bold="true" box="[238,346,828,854]" italics="true" pageId="1" pageNumber="502">Ityphilus</emphasis>
Cook, 
<number id="3DB124BCFFE04504FE2B123498D3FCE6" box="[434,493,829,855]" pageId="1" pageNumber="502" value="1899.0">1899</number>
</taxonomicName>
