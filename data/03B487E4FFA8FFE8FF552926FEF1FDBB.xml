<taxonomicName id="4C1D4D71FFAFFFE9FCDE2B0EFC4BF9F1" authority="Bassi &amp; I- &amp; To, 2012" authorityName="Bassi &amp; I- &amp; To" authorityYear="2012" box="[796,938,1563,1585]" class="Insecta" family="Crambidae" genus="Crambus" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="281" phylum="Arthropoda" rank="species" species="frescobaldii" status="sp. n.">
