<taxonomicName id="4C1D4D71FFAFFFE9FED92B5BFE60F9A4" authority="Bassi &amp; I- &amp; To, 2012" authorityName="Bassi &amp; I- &amp; To" authorityYear="2012" box="[283,385,1614,1636]" class="Insecta" family="Crambidae" genus="Crambus" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="281" phylum="Arthropoda" rank="species" species="berliozi" status="n. sp.">
