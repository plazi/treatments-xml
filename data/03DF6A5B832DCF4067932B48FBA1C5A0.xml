<subSubSection id="C36C88C6832DCF4067C32807FC20C450" box="[199,898,1553,1578]" pageId="19" pageNumber="20" type="etymology">
<paragraph id="8BC9DB4D832DCF4067C32807FC20C450" blockId="19.[151,1437,1481,2010]" box="[199,898,1553,1578]" pageId="19" pageNumber="20">
<emphasis id="B902075F832DCF4067C32807FEECC450" bold="true" box="[199,334,1553,1577]" inLineHeading="true" pageId="19" pageNumber="20" reason="1">Etymology.</emphasis>
