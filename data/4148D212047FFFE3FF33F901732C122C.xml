<geoCoordinate id="ACD505C30460FFE3FF7BFE10772C11C0" box="[223,318,477,503]" degrees="38" direction="north" minutes="28" orientation="latitude" pageId="48" pageNumber="49" precision="925" value="38.466667">38°28'N</geoCoordinate>
