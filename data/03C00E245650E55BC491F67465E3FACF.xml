<taxonomicName id="4C69C4B1564EE55BC55AF19A65E0FC37" authority="BELOKOBYLSKIJ, 1998" authorityName="BELOKOBYLSKIJ" authorityYear="1998" box="[834,1270,995,1021]" class="Insecta" family="Braconidae" genus="Apronopa" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="ussuricola">
