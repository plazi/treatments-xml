<taxonomicName id="4C4F4D0CFF87FFF1F20CFF1C2FCD74C7" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[345,453,225,248]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="pacata" status="sp. nov.">
