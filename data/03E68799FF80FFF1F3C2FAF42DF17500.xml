<taxonomicNameLabel id="A20857E6FF87FFF1F1DFFF1D2CEA74C8" box="[650,738,224,247]" pageId="100" pageNumber="101" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName id="4C4F4D0CFF87FFF1F04EFF1C2DA174C7" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[795,937,224,248]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="tridentata" status="sp. nov.">
<emphasis id="B93BEA9DFF87FFF1F04EFF1C2DA174C7" box="[795,937,224,248]" italics="true" pageId="100" pageNumber="101">R. tridentata</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20857E6FF87FFF1F0E7FF1D2A0474C8" box="[946,1036,224,247]" pageId="100" pageNumber="101" rank="species">sp. nov.</taxonomicNameLabel>
, all three also from Borneo, and 
<taxonomicName id="4C4F4D0CFF87FFF1F6D1FF1C2EE37523" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="ramosa" status="sp. nov.">
<emphasis id="B93BEA9DFF87FFF1F6D1FF1C2EE37523" italics="true" pageId="100" pageNumber="101">R. ramosa</emphasis>
