<taxonomicName id="4C4F4D0CFF87FFF1F1B1FF412D9E74EB" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[740,918,188,212]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="magna">
