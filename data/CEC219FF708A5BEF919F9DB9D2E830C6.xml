<taxonomicName id="DBEF7CAB9022D991AC044C101CD48054" class="Hexapoda" family="Platygastridae" genus="Trimorus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trimorus" order="Hymenoptera" pageId="0" pageNumber="523" phylum="Arthropoda" rank="genus">
<emphasis id="3D60D9D8415507D6E3E0ADEE52404DD4" italics="true" pageId="0" pageNumber="523">Trimorus</emphasis>
