<emphasis id="32C2CEEE2C7863B15CFA1293219C0DD7" bold="true" pageId="0" pageNumber="523">C, D, F</emphasis>
<taxonomicName id="CA3A914349AB707465E84120BF7A3F94" baseAuthorityName="Nixon" baseAuthorityYear="1936" class="Hexapoda" family="Platygastridae" genus="Trimorus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Trimorus carus" order="Hymenoptera" pageId="0" pageNumber="523" phylum="Arthropoda" rank="species" species="carus">
