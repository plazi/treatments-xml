<emphasis box="[694,935,1769,1794]" italics="true" pageId="17" pageNumber="18">
<taxonomicName box="[694,795,1769,1794]" class="Insecta" family="Noctuidae" genus="Schinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="brunnea">brunnea</taxonomicName>
. 
<taxonomicName box="[806,931,1769,1794]" class="Insecta" family="Noctuidae" genus="Schinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="albafascia">albafascia</taxonomicName>
