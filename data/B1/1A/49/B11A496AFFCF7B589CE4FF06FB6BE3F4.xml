; TAL: 
<quantity box="[617,713,882,909]" metricMagnitude="-1" metricUnit="m" metricValue="2.95" pageId="11" pageNumber="12" unit="mm" value="295.0">295 mm</quantity>
; TOL: 
<quantity box="[792,902,883,909]" metricMagnitude="0" metricUnit="m" metricValue="1.01" pageId="11" pageNumber="12" unit="mm" value="1010.0">1010 mm</quantity>
