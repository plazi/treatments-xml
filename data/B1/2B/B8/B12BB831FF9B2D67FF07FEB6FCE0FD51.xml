<subSubSection box="[151,1217,485,511]" pageId="9" pageNumber="10" type="etymology">
<paragraph blockId="9.[151,1437,485,751]" box="[151,1217,485,511]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[151,291,485,511]" pageId="9" pageNumber="10">Etymology.</emphasis>
