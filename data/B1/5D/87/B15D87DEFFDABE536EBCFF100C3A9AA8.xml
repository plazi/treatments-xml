<taxonomicName authority="(Alcock, 1900)" authorityName="Naruse &amp; Ng" authorityYear="2020" baseAuthorityName="Alcock" baseAuthorityYear="1900" box="[279,597,1630,1653]" class="Malacostraca" family="Sesarmidae" genus="Scandarma" kingdom="Animalia" order="Decapoda" pageId="48" pageNumber="492" phylum="Arthropoda" rank="species" species="finni">
