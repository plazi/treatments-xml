<paragraph pageId="1" pageNumber="283">
<taxonomicName class="Arachnida" family="Liacaridae" genus="Liacarus" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="1" pageNumber="283" phylum="Arthropoda" rank="species" species="schweizeri">Liacarus schweizeri</taxonomicName>
