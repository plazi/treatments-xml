<subSubSection pageId="9" pageNumber="72" type="distinguishing features">
<paragraph pageId="9" pageNumber="72">Distinguishing features.</paragraph>
<paragraph pageId="9" pageNumber="72">Male with +/- rectangular yellowish face pattern (Fig. 5a); female unknown.</paragraph>
</subSubSection>
<subSubSection pageId="9" pageNumber="72" type="distribution">
<paragraph pageId="9" pageNumber="72">General distribution.</paragraph>
<paragraph pageId="9" pageNumber="72">Endemic to the Azores.</paragraph>
</subSubSection>
<subSubSection pageId="9" pageNumber="72" type="distribution">
<paragraph pageId="9" pageNumber="72">Distribution in the Azores.</paragraph>
<paragraph pageId="9" pageNumber="72">Pico, Montanha do Pico.</paragraph>
</subSubSection>
