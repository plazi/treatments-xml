<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Grego &amp; Mumladze" authorityYear="2020" class="Gastropoda" family="Hydrobiidae" genus="Hausdorfenia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hausdorfenia pseudohauffenia" order="Littorinimorpha" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="pseudohauffenia">
