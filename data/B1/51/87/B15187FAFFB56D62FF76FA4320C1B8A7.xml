<collectionCodeEnum box="[1194,1214,1528,1551]" pageId="25" pageNumber="26">F</collectionCodeEnum>
; 13A 
<emphasis box="[1283,1296,1528,1551]" italics="true" pageId="25" pageNumber="26">–</emphasis>
<collectionCode box="[1296,1318,1527,1551]" country="Denmark" httpUri="http://grbio.org/cool/na0h-6vfh" name="University of Copenhagen" pageId="25" pageNumber="26">C</collectionCode>
