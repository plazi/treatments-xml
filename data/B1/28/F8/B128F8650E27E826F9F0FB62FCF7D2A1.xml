<paragraph blockId="1.[1537,1989,1188,1227]" box="[1537,1989,1188,1227]" pageId="1" pageNumber="28">
<heading box="[1537,1989,1188,1227]" centered="true" fontSize="10" level="2" pageId="1" pageNumber="28" reason="2">
<emphasis bold="true" box="[1537,1725,1188,1227]" pageId="1" pageNumber="28">Review of</emphasis>
<taxonomicName authorityName="Just" authorityYear="1983" box="[1741,1989,1188,1227]" class="Malacostraca" family="Ischyroceridae" genus="Caribboecetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="28" phylum="Arthropoda" rank="genus">
