</taxonomicName>
<emphasis bold="true" pageId="4" pageNumber="5">
<taxonomicNameLabel pageId="4" pageNumber="5" rank="genus">n. gen.</taxonomicNameLabel>
n. sp.
</emphasis>
, (b, d) 
<taxonomicName authority="Farré &amp; Pujade-Villar, 2007" authorityName="Farré &amp; Pujade-Villar" authorityYear="2007" box="[363,623,1806,1829]" class="Insecta" family="Figitidae" genus="Scutimica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="transcarinata" status="n. gen.">
