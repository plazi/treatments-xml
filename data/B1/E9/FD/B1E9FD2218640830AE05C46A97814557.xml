[
<taxonomicName class="Arachnida" family="Suctobelbidae" genus="Suctobelba" genus.completionEvidence="missing" genus.completionRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="138" phylum="Arthropoda" rank="species" species="tuberculata">tuberculata</taxonomicName>
