<caption id="DF4E67AEA20BFFECFF36FAE05D8D8967" ID-DOI="http://doi.org/10.5281/zenodo.4754448" ID-Zenodo-Dep="4754448" httpUri="https://zenodo.org/record/4754448/files/figure.png" pageId="4" pageNumber="14" startId="4.[184,234,1283,1307]" targetBox="[184,1403,307,1252]" targetPageId="4">
