<taxonomicName id="4C3E4D239B29C960E2BCFE7FFE9EFE68" authorityName="SCHUBERT" authorityYear="1911" box="[221,299,424,446]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="829" phylum="Arthropoda" rank="species" species="pilus">
