<taxonomicName id="4C72B9D6927CFFB7FEBBFF4477D51E60" authorityName="Freund &amp; Wagner" authorityYear="2003" box="[313,439,237,261]" class="Insecta" family="Acrididae" genus="Barombia" kingdom="Animalia" order="Orthoptera" pageId="41" pageNumber="1955" phylum="Arthropoda" rank="species" species="caerulea">
