<taxonomicName id="4C474D62FFC7DE0DFEABD09FFE11ACF6" authorityName="Jałoszyński" authorityYear="2017" box="[281,444,398,424]" class="Insecta" family="Staphylinidae" genus="Cephennodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="genus">
