<taxonomicName id="4C564D67E279BF53FF216B177A75F812" authority="Kieffer, 1897" authorityName="Kieffer" authorityYear="1897" box="[238,519,1898,1924]" class="Insecta" family="Cecidomyiidae" genus="Baldratia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
