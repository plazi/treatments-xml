</emphasis>
and 
<taxonomicName id="4C24CD85802D9E12FAB76B82FE80C509" authority="Matile. For" authorityName="Matile. For" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="teocchi">
