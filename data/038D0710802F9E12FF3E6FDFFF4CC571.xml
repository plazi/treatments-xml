<taxonomicName id="4C24CD85802D9E12FD656BAAFCD5C50A" box="[716,846,232,257]" class="Insecta" family="Mycetophilidae" genus="Aphanizophleps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="flavipes">
