(
<figureCitation id="131FAA83802F9E10FF366F82FF72C0D1" box="[159,233,1216,1242]" captionStart="FIGURE 7" captionStartId="13.[151,255,1967,1991]" captionTargetBox="[337,1156,473,1915]" captionTargetId="figure@13.[312,1283,448,1935]" captionTargetPageId="13" captionText="FIGURE 7. Manota phyllochaeta sp. n. (holotype). A. Antennal flagellomere 4, lateral view. B. Hypopygium, dorsal view. C. Hypopygium, ventral view. Scale for A 0.05 mm, for B and C 0.10 mm." httpUri="https://zenodo.org/record/181518/files/figure.png" pageId="12" pageNumber="13">
Figs 
<number id="3DECA421802F9E10FF736F82FF72C0D1" box="[218,233,1216,1242]" pageId="12" pageNumber="13" value="7.0">7</number>
</figureCitation>
