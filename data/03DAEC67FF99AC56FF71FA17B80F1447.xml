<taxonomicName id="4C7326F2FF99AC56FCDFF95BB9D215FF" box="[825,943,1698,1723]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="5" pageNumber="61" phylum="Tracheophyta" rank="species" species="dentata">
<emphasis id="B9078163FF99AC56FCDFF95BB9D215FF" box="[825,943,1698,1723]" italics="true" pageId="5" pageNumber="61">Q. dentata</emphasis>
</taxonomicName>
