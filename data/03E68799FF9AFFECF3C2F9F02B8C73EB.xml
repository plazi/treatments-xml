<taxonomicName id="4C4F4D0CFF9AFFECF3C2F9F02F827218" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[151,394,1549,1575]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="121" pageNumber="122" phylum="Arthropoda" rank="species" species="foliata">
