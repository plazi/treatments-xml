<emphasis id="BBF9444E3151FFD3FC47FD901117FDE2" bold="true" box="[984,1036,525,551]" pageId="43" pageNumber="44">FCI</emphasis>
=1.12–1.16 (m=1.14, 
<collectingRegion id="4B4956BE3151FFD3FA84FD901058FDE2" box="[1307,1347,525,551]" country="Serbia" name="Sid" pageId="43" pageNumber="44">SD</collectingRegion>
=0.02); 
<emphasis id="BBF9444E3151FFD3FF08FDAF15D5FD89" bold="true" box="[151,206,562,588]" pageId="43" pageNumber="44">LCI</emphasis>
=1.56–1.64 (m=1.60, 
<collectingRegion id="4B4956BE3151FFD3FE78FDAF170AFD89" box="[487,529,562,588]" country="Serbia" name="Sid" pageId="43" pageNumber="44">SD</collectingRegion>
=0.05); 
<emphasis id="BBF9444E3151FFD3FDE9FDAF17F1FD89" bold="true" box="[630,746,562,588]" pageId="43" pageNumber="44">HW/IOS</emphasis>
=1.66–1.67, m=1.66, 
<collectingRegion id="4B4956BE3151FFD3FB9DFDAF1130FD89" box="[1026,1067,562,588]" country="Serbia" name="Sid" pageId="43" pageNumber="44">SD</collectingRegion>
