<treatmentCitationGroup id="AB0C6837FF957833FF08FD56FDAFFCFC" box="[151,543,762,785]" pageId="5" pageNumber="6">
<taxonomicName id="4C1C349AFF957833FF08FD56FDABFCFC" authority="Uhler, 1892b: 175" authorityName="Uhler" authorityPageNumber="175" authorityYear="1892" box="[151,539,762,785]" class="Insecta" family="Cicadellidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="sordidata">
