<taxonomicName id="4C0F4D1A9C6AFFF2FB80FE268AE9FE75" box="[1025,1196,469,493]" class="Insecta" family="Notonectidae" genus="Buenoa" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="platycnemis">
