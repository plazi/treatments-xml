<taxonomicName id="4C0F4D1A9C6AFFF2FB6EFE268B20FE75" box="[1263,1381,469,493]" class="Insecta" family="Notonectidae" genus="Buenoa" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="pallens">
<emphasis id="B97BEA8B9C6AFFF2FB6EFE268B20FE75" box="[1263,1381,469,493]" italics="true" pageId="12" pageNumber="113">B. pallens</emphasis>
