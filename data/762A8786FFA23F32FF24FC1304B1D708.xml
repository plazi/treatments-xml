<taxonomicName id="39834D13FFA33F32FBE4FB3B04B1D42A" authority="" box="[1111,1436,1263,1285]" class="Insecta" family="Syrphidae" genus="Xanthandrus" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="102" phylum="Arthropoda" rank="subGenus" subGenus="Afroxanthandrus">
