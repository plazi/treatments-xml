</paragraph>
</subSubSection>
<subSubSection id="C33D698DD80AFF9EBF96E2C62D5CF971" box="[192,1354,1699,1723]" pageId="30" pageNumber="29" type="materials_examined">
<paragraph id="8B983A06D80AFF9EBF96E2C62D5CF971" blockId="30.[192,1354,1699,1723]" box="[192,1354,1699,1723]" pageId="30" pageNumber="29">
<emphasis id="B953E614D80AFF9EBF96E2C6299CF971" bold="true" box="[192,394,1699,1723]" pageId="30" pageNumber="29">Type material.</emphasis>
<materialsCitation id="3B4F305BD80AFF9EBEC4E2C62D5CF971" box="[402,1354,1699,1723]" collectionCode="USNM" country="Honduras" location="Miami" pageId="30" pageNumber="29" specimenCount="1" typeStatus="holotype">
<typeStatus id="549C84A4D80AFF9EBEC4E2C62A17F971" box="[402,513,1699,1723]" pageId="30" pageNumber="29">Holotype</typeStatus>
male; ex 
<collectingCountry id="F3307A96D80AFF9EBD23E2C62AF9F971" box="[629,751,1699,1723]" name="Honduras" pageId="30" pageNumber="29">Honduras</collectingCountry>
on orchid plants, 
<location id="8EF86CDDD80AFF9EBC9DE2C62C0BF971" LSID="urn:lsid:plazi:treatment:038E8B10D80AFF9EBF96E7E62BF4F8F1:8EF86CDDD80AFF9EBC9DE2C62C0BF971" box="[971,1053,1699,1723]" country="Honduras" name="Miami" pageId="30" pageNumber="29">Miami</location>
