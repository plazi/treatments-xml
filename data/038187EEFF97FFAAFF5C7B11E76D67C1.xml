<taxonomicName id="4C284D7BFF90FFAAFDE578BBE48E648C" authority="Theischinger &amp; D &amp; Martin, 2018" authorityName="Theischinger &amp; D &amp; Martin" authorityYear="2018" box="[523,757,427,448]" class="Insecta" family="Limoniidae" genus="Styringomyia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="1591" phylum="Arthropoda" rank="species" species="baroalba" status="nov.sp.">
<emphasis id="B95CEAEAFF90FFAAFDE578BBE48E648C" box="[523,757,427,448]" italics="true" pageId="4" pageNumber="1591">Styringomyia baroalba</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A26F5791FF90FFAAFD1278BBE533648C" box="[764,840,427,448]" pageId="4" pageNumber="1591" rank="species">nov.sp.</taxonomicNameLabel>
, 
<taxonomicName id="4C284D7BFF90FFAAFCBD78BBE5B1648C" box="[851,970,427,448]" class="Insecta" family="Limoniidae" genus="Styringomyia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="1591" phylum="Arthropoda" rank="species" species="bancrofti">
