<taxonomicName id="E9E84D1DBC437073D157F860FD44DCDE" authority="K. Viets, 1935" authorityName="K. Viets" authorityYear="1935" box="[344,710,1978,2000]" class="Arachnida" family="Hygrobatidae" genus="Hygrobates" kingdom="Animalia" order="Trombidiformes" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="hamatus">
