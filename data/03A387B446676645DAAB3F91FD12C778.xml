<taxonomicName id="4C0A4D2146656645D82A39CFFC1AC5B8" authority="Chace, 1939" authorityName="Chace" authorityYear="1939" box="[534,949,1180,1202]" class="Malacostraca" family="Squillidae" genus="Fennerosquilla" kingdom="Animalia" order="Stomatopoda" pageId="3" pageNumber="146" phylum="Arthropoda" rank="species" species="heptacantha">
