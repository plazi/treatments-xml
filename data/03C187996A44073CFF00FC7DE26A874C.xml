<taxonomicName id="4C684D0C6A44073CFF00FC7DE34681D9" authority="(Luederwaldt)" authorityName="Luederwaldt" baseAuthorityName="Luederwaldt" baseAuthorityYear="1925" box="[151,621,907,932]" class="Insecta" family="Scarabaeidae" genus="Dichotomius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="416" phylum="Arthropoda" rank="species" species="sexdentatus">
