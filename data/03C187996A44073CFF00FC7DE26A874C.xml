and 
<taxonomicName id="4C684D0C6A44073CFD30FC7AE53481D9" authority="(Luederwaldt)" authorityName="Luederwaldt" baseAuthorityName="Luederwaldt" baseAuthorityYear="1935" box="[679,1055,907,932]" class="Insecta" family="Scarabaeidae" genus="Dichotomius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="416" phylum="Arthropoda" rank="species" species="simplicicornis">
