µm, st 
<number id="3D9624C5FFEBB611FCF6FB12FCF36D89" box="[841,855,1158,1182]" pageId="5" pageNumber="280" value="2.0">2</number>
<number id="3D9624C5FFEBB611FCDEFB11FC036D89" box="[865,935,1157,1182]" pageId="5" pageNumber="280" value="44.0" valueMax="46.0" valueMin="42.0">42–46</number>
