<taxonomicName id="FBC5BEFBFFE4FFDEFF3AFB5CAF14FB3B" authority="Chant &amp; McMurtry, 1994: 258" authorityName="Chant &amp; McMurtry" authorityPageNumber="258" authorityYear="1994" box="[151,620,1161,1183]" class="Arachnida" family="Phytoseiidae" genus="Typhloseiopsis" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="47" phylum="Arthropoda" rank="genus">
