<taxonomicName id="2EA53B8EE01BFFC3FEDA25B6FE36FD16" authorityName="Montandon" authorityYear="1910" box="[313,452,733,757]" class="Insecta" family="Nepidae" genus="Ranatra" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="65" phylum="Arthropoda" rank="species" species="spinifrons">
