<taxonomicName id="F9964D16FFA7FFC1010FBDA0FCB58673" authority="Serov &amp; Wilson, 1995" authorityName="Serov &amp; Wilson" authorityYear="1995" box="[547,886,2031,2068]" class="Malacostraca" family="Stenetriidae" genus="Stenetrium" kingdom="Animalia" order="Isopoda" pageId="34" pageNumber="73" phylum="Arthropoda" rank="species" species="adrianae" status="n.sp.">
<emphasis id="0CE2EA87FFA7FFC1010FBDA0FCB58673" box="[547,886,2031,2068]" italics="true" pageId="34" pageNumber="73">Stenetrium adrianae</emphasis>
