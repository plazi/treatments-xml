<taxonomicName id="878B3886FF5D24E1C955A0DF77A227F2" authorityName="Yin" authorityYear="2022" box="[586,709,403,429]" class="Insecta" family="Staphylinidae" genus="Tribasodites" kingdom="Animalia" order="Coleoptera" pageId="150" pageNumber="151" phylum="Arthropoda" rank="species" species="constrictus">
