<taxonomicName id="4C1D79EDFFB6FFDCFB40273EFE740671" authority="Root and Hoffman" authorityName="Root and Hoffman" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="villosipennis">
