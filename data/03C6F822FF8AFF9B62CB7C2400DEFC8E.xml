<taxonomicName id="4C6F32B7FF89FF9B61A37AAB000BFF4E" box="[1023,1112,224,248]" class="Insecta" family="Aleyrodidae" genus="Paraleyrodes" kingdom="Animalia" order="Hemiptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="minei">
