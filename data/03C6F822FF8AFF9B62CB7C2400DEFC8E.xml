<taxonomicName id="4C6F32B7FF89FF9B616F7B4E0040FEA5" box="[819,1043,260,284]" class="Liliopsida" family="Poaceae" genus="Miscanthus" kingdom="Plantae" order="Poales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="species" species="sinensis">
