<taxonomicName id="4C454D71FFD4FFACC9E91961FF1CED78" baseAuthorityName="Wileman" baseAuthorityYear="1911" class="Insecta" family="Pyralidae" genus="Termioptycha" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="173" phylum="Arthropoda" rank="species" species="bilineata">
