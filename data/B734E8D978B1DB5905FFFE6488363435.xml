<subSubSection id="83D7CE5F94E63719A4B3668C4F78E849" pageId="null" pageNumber="249" type="nomenclature">
<paragraph id="7D9A3B0383ACD907459950E895F28656" pageId="null" pageNumber="249">
<taxonomicName id="AB312C884D2B3F626BCE9CEC2F1E1D64" authority="L." class="Liliopsida" family="Poaceae" genus="Anthoxanthum" kingdom="Plantae" order="Poales" pageId="null" pageNumber="249" phylum="Tracheophyta" rank="species" species="odoratum">
<pageBreakToken id="DB9F513A21EBC2B4CBAD98F81039A0A8" pageId="null" pageNumber="249" start="start">Anthoxanthum</pageBreakToken>
<normalizedToken id="0BD22B007ECA1A7815D5FCCA3646B9D4" originalValue="odorátum" pageId="null" pageNumber="249">odoratum</normalizedToken>
<authorityName id="C9D5DE6DF88D43AC895DE60CDEAB49D5" pageId="null" pageNumber="249">L.</authorityName>
</taxonomicName>
</paragraph>
