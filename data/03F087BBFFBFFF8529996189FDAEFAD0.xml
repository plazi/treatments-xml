<taxonomicName id="4C594D2EFFBFFF85289B645DFD97FAF5" box="[405,619,1347,1371]" class="Insecta" family="Staphylinidae" genus="Xenopygus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="73" phylum="Arthropoda" rank="species" species="bicolor">
