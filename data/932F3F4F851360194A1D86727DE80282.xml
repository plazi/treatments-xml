<taxonomicName id="1456769BB96A554C0288A1C114A0C447" class="Actinopterygii" family="Cichlidae" genus="Neolamprologus" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="40" phylum="Chordata" rank="species" species="falcicula">N. falcicula</taxonomicName>
