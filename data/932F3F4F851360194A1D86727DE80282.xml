<taxonomicName id="40EA92DA4D542425D66DDF39C85A7E1F" class="Actinopterygii" family="Cichlidae" genus="Neolamprologus" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="40" phylum="Chordata" rank="species" species="gracilis">N. gracilis</taxonomicName>
