<taxonomicName id="008D4D0D6964EC7DFEA2FD69A186FD3B" box="[306,451,698,725]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="2" pageNumber="245" phylum="Tracheophyta" rank="species" species="parlatoris">
