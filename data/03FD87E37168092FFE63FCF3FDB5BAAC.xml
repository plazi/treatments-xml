<paragraph id="8BEB36F57168092FFEFBFCD1FB41BDBC" blockId="21.[313,1167,852,908]" box="[313,1167,885,908]" pageId="21" pageNumber="2673">
<taxonomicName id="4C544D767168092FFEFBFCD1FC4CBDBC" authority="Pena Cantero et al., 1997: 345 - 347" authorityName="Pena Cantero" authorityPageNumber="345 - 347" authorityYear="1997" box="[313,898,885,908]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="21" pageNumber="2673" phylum="Cnidaria" rank="species" species="australis">
<emphasis id="B920EAE77168092FFEFBFCD1FDC3BDBB" box="[313,525,886,907]" italics="true" pageId="21" pageNumber="2673">Staurotheca australis</emphasis>
Peña Cantero 
<emphasis id="B920EAE77168092FFD68FCD0FD18BDBB" box="[682,726,886,908]" italics="true" pageId="21" pageNumber="2673">et al</emphasis>
., 1997: 345–347
</taxonomicName>
