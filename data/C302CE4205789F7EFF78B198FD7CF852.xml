<taxonomicName id="8CAB04D705799F7EFE1EB679FD55F80A" box="[497,667,1910,1934]" class="Insecta" family="Chrysopidae" genus="Ungla" kingdom="Animalia" order="Neuroptera" pageId="19" pageNumber="195" phylum="Arthropoda" rank="species" species="martinsi" status="sp. nov.">
