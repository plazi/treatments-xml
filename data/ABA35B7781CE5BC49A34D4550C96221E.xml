<taxonomicName id="B628A2B0D91C4B324BEA0CCCB117EDF3" family="Gonyleptidae" genus="G." kingdom="Animalia" lsidName="G. pugilator" order="Opiliones" pageId="0" pageNumber="91" rank="species" species="pugilator">
