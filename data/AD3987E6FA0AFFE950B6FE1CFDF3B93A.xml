<taxonomicName id="E2904D73FA0AFFE951F0FC2DFDDBBDD2" box="[465,630,974,1000]" class="Gastropoda" family="Trochomorphidae" genus="Trochomorpha" kingdom="Animalia" order="Stylommatophora" pageId="56" pageNumber="57" phylum="Mollusca" rank="genus">
