<taxonomicName id="4C424D3EFFD9FFC5FF6870003789F980" authority="Novotna-Dvorakova, 1963" authorityName="Novotna-Dvorakova" authorityYear="1963" box="[151,918,1556,1582]" class="Eurotatoria" family="Brachionidae" genus="Brachionus" kingdom="Animalia" order="Ploima" pageId="7" phylum="Rotifera" rank="variety" species="caudatus" variety="indica">
