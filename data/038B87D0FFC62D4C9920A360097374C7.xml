<taxonomicName id="4C224D45FFC62D4C99ABA1560A1C7467" authority="Mull. Hal." authorityName="Mull. Hal." box="[236,604,1586,1610]" class="Bryopsida" family="Pottiaceae" genus="Barbula" kingdom="Plantae" order="Pottiales" pageId="5" pageNumber="158" phylum="Bryophyta" rank="species" species="pallidoviridis">
