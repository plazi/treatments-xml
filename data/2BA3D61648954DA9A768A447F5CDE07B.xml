<taxonomicName id="BC34C78463B46A37E5357D716251E003" authority="Berlese, 1885" authorityName="Berlese" authorityYear="1885" class="Arachnida" family="Lohmanniidae" genus="Angelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="pyriformis">Angelia pyriformis Berlese</taxonomicName>
