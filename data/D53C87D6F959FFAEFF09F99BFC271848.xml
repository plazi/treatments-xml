<taxonomicName id="9A954D43F959FFAEFAE5FCCAFC2A1848" authorityName="WAHL &amp; SIME" authorityYear="2002" class="Insecta" family="Ichneumonidae" genus="Humbert" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">Humbert Humbert</taxonomicName>
