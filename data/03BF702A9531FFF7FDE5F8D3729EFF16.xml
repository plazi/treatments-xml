<taxonomicName id="4C16BABF9530FFF7FE28FEA370ADFF32" box="[858,960,295,321]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="species" species="affinis">
<emphasis id="B9621D2E9530FFF7FE28FEA370ADFF32" box="[858,960,295,321]" italics="true" pageId="53" pageNumber="54">Q. affinis</emphasis>
</taxonomicName>
, and 
<taxonomicName id="4C16BABF9530FFF7FE89FEA37707FF33" box="[1019,1130,295,321]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="species" species="rugosa">
