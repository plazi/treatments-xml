<number id="6A5E24E9FFE6FFF5FF21998CFF5584AC" box="[243,252,210,231]" pageId="3" pageNumber="432" value="1.0">1</number>
parallel sided (narrow at apex), 
<date id="A828100EFFE6FFF5FDE1998CFDE284AC" box="[563,587,210,231]" pageId="3" pageNumber="432">
<number id="6A5E24E9FFE6FFF5FDE1998CFD9684AC" box="[563,575,210,231]" pageId="3" pageNumber="432" value="2.0">2</number>
