<taxonomicName id="2B8862CCFFC74F2BE1570A28B7C1FBC6" authority="sensu Sowerby, 1840" authorityName="sensu Sowerby" authorityYear="1840" box="[113,460,1061,1085]" class="Bivalvia" family="Ostreidae" genus="Ostrea" kingdom="Animalia" order="Ostreida" pageId="40" pageNumber="57" phylum="Mollusca" rank="species" species="callifera">
<emphasis id="DEFCC55DFFC74F2BE1570A28B6F0FBC6" box="[113,253,1061,1085]" italics="true" pageId="40" pageNumber="57">Ostrea callifera</emphasis>
