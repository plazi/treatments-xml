<taxonomicName id="4C364D38FFB6FFD65AE8B733FE59FEAB" baseAuthorityName="Denis &amp; Schiffermuller" baseAuthorityYear="1775" box="[388,495,303,324]" class="Insecta" family="Crambidae" genus="Schoenobius" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="181" phylum="Arthropoda" rank="species" species="gigantellus">
