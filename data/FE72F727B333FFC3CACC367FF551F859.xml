(
<bibRefCitation id="124A3BC0B333FFC3C9EA36D2F066FC11" author="Simon" box="[945,1102,928,954]" pageId="1" pageNumber="36" refString="Simon, E. (1893) Histoire naturelle das araignees. Paris, 1, 257 - 488" type="journal article" year="1893">
Simon, 
<number id="C0135416B333FFC3CE5536D2F066FC11" box="[1038,1102,928,954]" pageId="1" pageNumber="31" value="1893.0">1893</number>
</bibRefCitation>
