[= 
<taxonomicName id="B1DB3DB2B333FFC3C95F36D2F07FFC11" authority="Simon, 1893" authorityName="Simon" authorityYear="1893" box="[772,1111,928,954]" class="Arachnida" family="Gnaphosidae" genus="Coreodrassus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="31" phylum="Arthropoda" rank="species" species="lancearius">
