<taxonomicName id="8295EDB1FF94D80512D13C98BDCCFB73" authority="Zhong, Yang &amp; Morse" authorityName="Zhong, Yang &amp; Morse" box="[151,706,1159,1185]" class="Insecta" family="Polycentropodidae" genus="Plectrocnemia" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="huangi" status="sp. nov.">
<emphasis id="77E14A20FF94D80512D13C98BEADFB73" bold="true" box="[151,419,1159,1185]" italics="true" pageId="2" pageNumber="3">Plectrocnemia huangi</emphasis>
Zhong, Yang &amp; Morse
</taxonomicName>
, 
<taxonomicNameLabel id="6CD2F75BFF94D80510893C98BC22FB73" box="[719,812,1159,1185]" pageId="2" pageNumber="3" rank="species">sp. nov.</taxonomicNameLabel>
