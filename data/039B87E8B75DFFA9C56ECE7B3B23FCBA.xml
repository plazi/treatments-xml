<quantity id="4CCA9B1BB752FFA9C09ECA963E0FFD42" metricMagnitude="-4" metricUnit="m" metricValue="3.2" pageId="8" pageNumber="9" unit="mm" value="0.32">0.32 mm</quantity>
, Edist 
<quantity id="4CCA9B1BB752FFA9C5EECABE3EC4FD42" box="[392,499,684,710]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="8" pageNumber="9" unit="mm" value="0.34">0.34 mm</quantity>
