<taxonomicName id="36216A7D69C24ED9216D72353DFE568D" class="Magnoliopsida" family="Brassicaceae" genus="Nasturtium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Nasturtium" order="Brassicales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="genus">Nasturtium</taxonomicName>
