species: 
<taxonomicName id="2CF44D0AFFED9531A562FF45B8E8BA21" authority="Spinola, 1808" authorityName="Spinola" authorityYear="1808" box="[294,660,237,263]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="38" phylum="Arthropoda" rank="species" species="deflagrator">
<emphasis id="D980EA9BFFED9531A562FF45BB90BA21" box="[294,492,237,263]" italics="true" pageId="19" pageNumber="38">Bracon deflagrator</emphasis>
Spinola, 1808
</taxonomicName>
