<taxonomicName id="DFE6E4B48649FF85FB15FE2BFA41FE27" authorityName="Jendek &amp; Nakladal" authorityYear="2019" box="[1255,1409,475,501]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="78" phylum="Arthropoda" rank="species" species="acacivorus">
