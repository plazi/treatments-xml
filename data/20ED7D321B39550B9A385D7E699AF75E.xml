<taxonomicName id="C517E31C2C512FAFC9C3AD4F04EC705B" LSID="20ED7D32-1B39-550B-9A38-5D7E699AF75E" authority="Li &amp; He &amp; Chen, 2020" authorityName="Li &amp; He &amp; Chen" authorityYear="2020" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon (Glabrobracon) leptotes" order="Hymenoptera" pageId="0" pageNumber="209" phylum="Arthropoda" rank="species" species="leptotes" status="sp. nov." subGenus="Glabrobracon">Bracon (Glabrobracon) leptotes</taxonomicName>
