<emphasis id="B926EA93FFE2CF02FF10F8F43A509D90" bold="true" box="[151,317,1904,1927]" pageId="4" pageNumber="5">FIGURES 1–8.</emphasis>
<taxonomicName id="4C524D02FFE2CF02FECFF8F539059D93" authority="Braet" authorityName="Braet" box="[328,616,1904,1926]" class="Insecta" family="Braconidae" genus="Ascogaster" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="kibalensis" status="sp. nov.">
<emphasis id="B926EA93FFE2CF02FECFF8F5394A9D93" box="[328,551,1904,1926]" italics="true" pageId="4" pageNumber="5">Ascogaster kibalensis</emphasis>
Braet
</taxonomicName>
<taxonomicNameLabel id="A21557E8FFE2CF02FDF5F8F539AB9D93" box="[626,710,1905,1926]" pageId="4" pageNumber="5" rank="species">
