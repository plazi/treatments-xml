<taxonomicName id="4C5897AB8C07FFFB3F68F893AE1BF836" authority="Martins-Neto, 1999" authorityName="Martins-Neto" authorityYear="1999" box="[300,783,1901,1924]" class="Insecta" family="Cooloolidae" genus="Notocearagryllus" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="23" phylum="Arthropoda" rank="species" species="dutrae">
