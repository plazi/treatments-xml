<taxonomicName id="CD1A7F5F6CC5C7B2ADFBBE732AEE04E6" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24915" class="Insecta" family="Formicidae" genus="Rogeria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Rogeria Emery" order="Hymenoptera" pageNumber="27" phylum="Arthropoda" rank="genus">Rogeria</taxonomicName>
