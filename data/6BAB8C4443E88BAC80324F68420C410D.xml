<taxonomicName id="B6498D5687B4FFB5E8D32A2F51EE8560" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24913" class="Insecta" family="Formicidae" genus="Lordomyrma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Lordomyrma Emery" order="Hymenoptera" pageNumber="27" phylum="Arthropoda" rank="genus">Lordomyrma</taxonomicName>
