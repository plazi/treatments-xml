(
<taxonomicName id="EEE52D68BEF11B9069D74F33D57DFCC9" class="Magnoliopsida" family="Rosaceae" genus="Alchemilla" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="340" phylum="Tracheophyta" rank="species" species="vestita">
