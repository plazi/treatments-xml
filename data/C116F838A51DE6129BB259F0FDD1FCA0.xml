<taxonomicName id="8EBF32ADA51DE6129ACC5B05FDD1FCA0" authority="Fiers and Rutledge." authorityName="Fiers and Rutledge." box="[193,557,851,875]" class="Maxillopoda" family="Laophontidae" genus="Folioquinpes" kingdom="Animalia" order="Harpacticoida" pageId="17" pageNumber="396" phylum="Arthropoda" rank="genus">
