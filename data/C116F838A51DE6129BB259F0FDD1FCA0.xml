<taxonomicName id="8EBF32ADA51DE61299425B62FBB3FC80" authority="Daday" authorityName="Daday" authorityYear="1903" box="[847,1103,819,844]" class="Maxillopoda" family="Laophontidae" genus="Onychocamptus" kingdom="Animalia" order="Harpacticoida" pageId="17" pageNumber="396" phylum="Arthropoda" rank="genus">
<emphasis id="7BCB953CA51DE61299425B62FC01FC87" box="[847,1021,820,844]" italics="true" pageId="17" pageNumber="396">Onychocamptus</emphasis>
