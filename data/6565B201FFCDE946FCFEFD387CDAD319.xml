<taxonomicName id="2ACC7894FFCCE946FBAEFDD87B6AD219" authorityName="Wagler" authorityYear="1830" box="[1032,1105,523,549]" class="Reptilia" family="Blanidae" genus="Blanus" kingdom="Animalia" order="Squamata" pageId="27" pageNumber="566" phylum="Chordata" rank="genus">
