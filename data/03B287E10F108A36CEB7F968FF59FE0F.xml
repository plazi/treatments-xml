<taxonomicName id="4C1B4D740F178A36CC60FF61FD77FF7F" box="[576,718,224,248]" class="Insecta" family="Chorotypidae" genus="Glaphyrosoma" kingdom="Animalia" order="Orthoptera" pageId="14" pageNumber="562" phylum="Arthropoda" rank="species" species="dentatum">
