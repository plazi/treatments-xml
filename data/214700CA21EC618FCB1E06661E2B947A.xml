<taxonomicName id="30ECA1568BF8378E50E85DE2EAF0A11B" authorityName="Kirby" authorityYear="1837" class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="577" pageNumber="578" phylum="Arthropoda" rank="species" species="discors">
