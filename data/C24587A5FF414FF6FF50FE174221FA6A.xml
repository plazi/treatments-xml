<taxonomicName id="8DEC4D30FF414FF6FD6BF81544DEFAB6" authority="Mistshenko" authorityName="Mistshenko" box="[684,935,1349,1374]" class="Insecta" family="Pamphagidae" genus="Nocaracris" kingdom="Animalia" order="Orthoptera" pageId="167" pageNumber="168" phylum="Arthropoda" rank="species" species="curtus">
