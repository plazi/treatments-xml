<collectingCountry id="F3154DA0FF9EFFACFC21FE4F0B3DFE01" box="[997,1087,404,429]" name="Uganda" pageId="4" pageNumber="71">Uganda</collectingCountry>
, 
<collectingCountry id="F3154DA0FF9EFFACFB8EFE4F0BA0FE01" box="[1098,1186,404,429]" name="Zambia" pageId="4" pageNumber="71">Zambia</collectingCountry>
