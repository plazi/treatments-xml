</taxonomicName>
<taxonomicNameLabel id="0D9C8E5F3B14FFA28042FE68FD13FE6F" box="[579,655,415,437]" pageId="3" pageNumber="382" rank="species">
<emphasis id="16AF33243B14FFA28042FE68FD13FE6F" bold="true" box="[579,655,415,437]" pageId="3" pageNumber="382">sp. nov.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="E3DB94B53B14FFA2809AFE68FD48FE6E" authority="Kondo &amp; Rodríguez &amp; Díaz &amp; Luna &amp; Goenaga, 2018" authorityName="Kondo &amp; Rodríguez &amp; Díaz &amp; Luna &amp; Goenaga" authorityYear="2018" box="[667,724,415,436]" class="Insecta" family="Coccidae" genus="Cryptinglisia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="382" phylum="Arthropoda" rank="species" species="ica" status="sp. nov.">
<emphasis id="16AF33243B14FFA2809AFE68FD48FE6E" box="[667,724,415,436]" italics="true" pageId="3" pageNumber="382">C. ica</emphasis>
