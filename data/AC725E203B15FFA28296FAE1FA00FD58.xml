<taxonomicNameLabel id="0D9C8E5F3B14FFA280DBFE68FCBAFE6F" box="[730,806,415,437]" pageId="3" pageNumber="382" rank="species">
<emphasis id="16AF33243B14FFA280DBFE68FCBAFE6F" bold="true" box="[730,806,415,437]" pageId="3" pageNumber="382">sp. nov.</emphasis>
</taxonomicNameLabel>
, and 
<taxonomicName id="E3DB94B53B14FFA2815BFE68FC25FE6E" baseAuthorityName="Brain" baseAuthorityYear="1920" box="[858,953,415,436]" class="Insecta" family="Coccidae" genus="Cryptinglisia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="382" phylum="Arthropoda" rank="species" species="zizyphi">
<emphasis id="16AF33243B14FFA2815BFE68FC25FE6E" box="[858,953,415,436]" italics="true" pageId="3" pageNumber="382">C. zizyphi</emphasis>
