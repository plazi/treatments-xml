<collectingCountry id="F37C56AA32005C45FE91D7D6ADF3F961" box="[334,443,1762,1786]" name="Malaysia" pageId="24" pageNumber="23">Malaysia</collectingCountry>
(
<collectingRegion id="49AFD8D832005C45FE16D7D6AE5EF961" box="[457,534,1762,1786]" country="Malaysia" name="Sabah" pageId="24" pageNumber="23">Sabah</collectingRegion>
