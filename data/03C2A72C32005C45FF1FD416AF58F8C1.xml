<taxonomicName id="4C6B6DB932005C45FBF6D7D7A8F8F961" baseAuthorityName="Hendel" baseAuthorityYear="1915" box="[1065,1200,1762,1786]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="23" phylum="Arthropoda" rank="species" species="latifrons">
