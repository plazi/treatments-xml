<taxonomicName id="CE8F4D68D73D395CFB2BFC68FA889BB3" box="[1206,1355,920,944]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="91" pageNumber="92" phylum="Arthropoda" rank="species" species="multivorus">
<emphasis id="3BFBEAF9D73D395CFB2BFC68FA889BB3" box="[1206,1355,920,944]" italics="true" pageId="91" pageNumber="92">T. multivorus</emphasis>
</taxonomicName>
