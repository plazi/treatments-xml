<quantity id="4C8B9B07A31D234BB5A0FCF7FDE1FC9D" box="[408,523,804,828]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" metricValueMax="1.5" metricValueMin="1.2" pageId="5" pageNumber="202" unit="mm" value="13.5" valueMax="15.0" valueMin="12.0">12-15 mm</quantity>
