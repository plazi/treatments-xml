<taxonomicName id="4C734D61A31D234BB616FCB6FCEAFCDD" box="[558,768,869,892]" class="Magnoliopsida" family="Lamiaceae" genus="Marrubium" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="202" phylum="Tracheophyta" rank="species" species="vulgare">
