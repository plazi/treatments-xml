<taxonomicName id="4C734D61A31D234BB714FCB7FC4CFCDD" box="[812,934,868,892]" class="Magnoliopsida" family="Lamiaceae" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="202" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
