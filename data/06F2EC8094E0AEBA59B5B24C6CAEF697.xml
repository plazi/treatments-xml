<taxonomicName id="057DEF5E07465069C5F16D1AECEB9B8C" class="Magnoliopsida" family="Solanaceae" genus="Solanum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Solanum pygmaeum" order="Centrales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="pygmaeum">
<emphasis id="F67662CA83DC08601A0B9B289E40A1F7" italics="true" pageId="0" pageNumber="1">Solanum pygmaeum</emphasis>
