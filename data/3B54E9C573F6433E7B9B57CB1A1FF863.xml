<taxonomicName id="1221AF061875E71D8D36287864F6DE31" authority="Bezzi, 1924" authorityName="Bezzi" authorityYear="1924" class="Insecta" family="Mydidae" genus="Mydaselpis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mydaselpis" order="Diptera" pageId="9" pageNumber="42" phylum="Arthropoda" rank="genus">Mydaselpis Bezzi, 1924</taxonomicName>
