<quantity id="4CAD9B6DB629FFE6FF02FF502689FEC8" box="[147,218,247,274]" metricMagnitude="2" metricUnit="m" metricValue="2.5" pageId="9" pageNumber="238" unit="m" value="250.0">250 m</quantity>
