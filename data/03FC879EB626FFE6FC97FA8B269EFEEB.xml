<location id="8E8A6053B628FFE7FD34F9CE2557F959" LSID="urn:lsid:plazi:treatment:03FC879EB626FFE6FC97FA8B269EFEEB:8E8A6053B628FFE7FD34F9CE2557F959" box="[677,772,1641,1667]" country="Vietnam" county="Known" municipality="Djijuih" name="Endemic" pageId="8" pageNumber="237">Endemic</location>
in 
<collectingCountry id="F3427618B628FFE7FCB2F9CE25D1F959" box="[803,898,1641,1667]" name="Vietnam" pageId="8" pageNumber="237">Vietnam</collectingCountry>
, 
<location id="8E8A6053B628FFE7FC1BF9CD2230F959" LSID="urn:lsid:plazi:treatment:03FC879EB626FFE6FC97FA8B269EFEEB:8E8A6053B628FFE7FC1BF9CD2230F959" box="[906,1123,1641,1668]" country="Vietnam" county="Known" municipality="Djijuih" name="Haut Donai District" pageId="8" pageNumber="237">Haut Donai District</location>
, 
<collectingMunicipality id="6B8EACF2B628FFE7FBFDF9CE22E9F959" box="[1132,1210,1641,1667]" pageId="8" pageNumber="237">Djijuih</collectingMunicipality>
(Dalat). 
<collectingCounty id="628B4E04B628FFE7FC91F92D2503F97E" box="[768,848,1674,1700]" pageId="8" pageNumber="237">Known</collectingCounty>
only from the 
<typeStatus id="54EE882AB628FFE7FC62F92D2276F97E" box="[1011,1061,1674,1700]" pageId="8" pageNumber="237">type</typeStatus>
</materialsCitation>
.
</paragraph>
<paragraph id="8BEA3688B629FFE6FFE0FF7F269CFF2A" blockId="9.[113,646,216,305]" box="[113,207,216,242]" pageId="9" pageNumber="238">HABITAT</paragraph>
<paragraph id="8BEA3688B629FFE6FFE0FF5F269EFEEB" blockId="9.[113,646,216,305]" pageId="9" pageNumber="238">
