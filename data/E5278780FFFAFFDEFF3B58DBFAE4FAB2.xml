<number id="DB4624B1FFFAFFDEFD84590CFD8DFAEA" box="[552,563,1312,1338]" pageId="23" pageNumber="54" value="1.0">1</number>
M, 
<collectionCode id="0B9FAE53FFFAFFDEFDF3590CFD37FAEA" box="[607,649,1312,1338]" pageId="23" pageNumber="54">TN</collectionCode>
, Blount Co., Elkmont, 
<date id="19301056FFFAFFDEFC3B590CFBA5FAEA" box="[919,1051,1312,1338]" pageId="23" pageNumber="54" value="1937-07-06">
