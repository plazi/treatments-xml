<taxonomicName id="AA8E4D15FFFAFFDEFE525964FDD6FAB1" box="[510,616,1352,1377]" class="Arachnida" family="Myobiidae" genus="Radfordia" kingdom="Animalia" order="Prostigmata" pageId="23" pageNumber="54" phylum="Arthropoda" rank="species" species="affinis">
