Ex 
<taxonomicName id="AA8E4D15FFFAFFDEFF13590CFEDBFAE9" box="[191,357,1312,1337]" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="23" pageNumber="54" phylum="Chordata" rank="species" species="musculus">
