<taxonomicName id="1C9F35850E286EBDDA68A3B19A48FC84" authority="Jacquelin du Val, 1857" authorityName="Jacquelin du Val" authorityYear="1857" lsidName="" pageId="168" pageNumber="169" rank="subFamily" subFamily="Mycetaeinae">Mycetaeinae Jacquelin du Val, 1857</taxonomicName>
