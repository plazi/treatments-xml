<taxonomicName id="EE5777A9BA8B8B8EC312219309614301" class="Arachnida" family="Suctobelbidae" genus="Suctobelba" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="387" phylum="Arthropoda" rank="species" species="trigona">Suctobelba trigona</taxonomicName>
