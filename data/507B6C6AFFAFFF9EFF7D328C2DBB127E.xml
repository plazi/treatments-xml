<taxonomicName id="1FD2A6FFFFAFFF9EFC3A32F82D3D125A" authorityName="Bluthgen" authorityYear="1940" box="[976,1144,1521,1547]" class="Insecta" family="Eumenidae" genus="Knemodynerus" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="201" phylum="Arthropoda" rank="genus">
