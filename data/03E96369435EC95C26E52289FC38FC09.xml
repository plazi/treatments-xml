<collectingCountry id="F35792EF435EC95C22AD22E5FAB3FCB9" box="[1247,1327,767,792]" name="Turkey" pageId="57" pageNumber="58">Turkey</collectingCountry>
(
<bibRefCitation id="EFD1AF8E435EC95C234C22E5FF7CFC9D" author="Kazanci" pageId="57" pageNumber="58" refString="Kazanci, N. (1983 b) Researches on the Plecoptera (Insecta) species of Turkey. Hacettepe Bulletin of Natural and Engineering, 12, 85 - 99." type="journal article" year="1983" yearSuffix="b">
Kazancı 
<number id="3D88C058435EC95C26E52339FF4DFC9D" box="[151,209,803,828]" pageId="57" pageNumber="58" value="1983.0">1983</number>
b
</bibRefCitation>
; Kazancı 
<number id="3D88C058435EC95C273C233EFE17FC9D" box="[334,395,804,828]" pageId="57" pageNumber="58" value="2008.0">2008</number>
