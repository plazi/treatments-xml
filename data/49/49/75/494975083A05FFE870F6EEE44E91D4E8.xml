<caption ID-DOI="http://doi.org/10.5281/zenodo.5072169" ID-Zenodo-Dep="5072169" box="[151,727,1854,1876]" httpUri="https://zenodo.org/record/5072169/files/figure.png" pageId="8" pageNumber="33" startId="8.[151,244,1854,1875]" targetBox="[151,1436,191,1833]" targetPageId="8">
<paragraph blockId="8.[151,727,1854,1876]" box="[151,727,1854,1876]" pageId="8" pageNumber="33">
