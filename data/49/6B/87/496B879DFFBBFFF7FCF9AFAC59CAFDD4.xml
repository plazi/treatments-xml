<collectingCountry box="[1219,1287,1749,1773]" name="Brazil" pageId="12" pageNumber="617">Brazil</collectingCountry>
, 
<collectingCountry box="[1297,1415,1749,1773]" name="Guatemala" pageId="12" pageNumber="617">Guatemala</collectingCountry>
, 
