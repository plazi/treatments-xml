<collectingCountry box="[909,1021,1783,1807]" name="Nicaragua" pageId="12" pageNumber="617">Nicaragua</collectingCountry>
, 
<collectingCountry box="[1034,1121,1783,1806]" name="Panama" pageId="12" pageNumber="617">Panama</collectingCountry>
, and 
<collectingCountry box="[1180,1293,1783,1807]" name="Venezuela" pageId="12" pageNumber="617">Venezuela</collectingCountry>
; introduced to 
<collectingCountry box="[846,919,1817,1840]" name="France" pageId="12" pageNumber="617">France</collectingCountry>
and 
<collectingCountry box="[978,1029,1816,1840]" name="Italy" pageId="12" pageNumber="617">Italy</collectingCountry>
