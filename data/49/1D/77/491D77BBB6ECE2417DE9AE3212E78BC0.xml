<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:137353" class="Insecta" family="Formicidae" genus="Atta" kingdom="Animalia" lsidName-HNS="Atta (Cyphomyrmex) olitrix subsp. lecta Forel" order="Hymenoptera" pageNumber="295" phylum="Arthropoda" rank="species" species="olitrix" subGenus="Cyphomyrmex">Atta (Cyphomyrmex) olitrix Forel</taxonomicName>
(
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28540" class="Insecta" family="Formicidae" genus="Cyphomyrmex" kingdom="Animalia" lsidName-HNS="Cyphomyrmex olitor Forel" order="Hymenoptera" pageNumber="295" phylum="Arthropoda" rank="species" species="olitor">Cyphom. olitor</taxonomicName>
) 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:245419" class="Insecta" family="Formicidae" genus="Cyphomyrmex" kingdom="Animalia" lsidName-HNS="Cyphomyrmex olitor subsp. lecta" order="Hymenoptera" pageNumber="295" phylum="Arthropoda" rank="subSpecies" species="olitor" subSpecies="lecta">subsp. lecta</taxonomicName>
