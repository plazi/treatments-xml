<taxonomicName authority="Lepeletier &amp; Serville, 1825" authorityName="Lepeletier &amp; Serville" authorityYear="1825" class="Insecta" family="Melyridae" genus="Pelecophora" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pelecophora" order="Coleoptera" pageId="164" pageNumber="165" phylum="Arthropoda" rank="genus">Pelecophora</taxonomicName>
