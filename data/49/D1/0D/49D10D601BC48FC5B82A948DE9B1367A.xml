<taxonomicName class="Insecta" family="Hesperiidae" genus="Nascus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nascus broteas" order="Lepidoptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="species" species="broteas">Nascus broteas</taxonomicName>
, 
<taxonomicName class="Insecta" family="Hesperiidae" genus="Nascus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nascus solon" order="Lepidoptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="species" species="solon">Nascus solon</taxonomicName>
, 
<taxonomicName class="Insecta" family="Hesperiidae" genus="Nascus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nascus" order="Lepidoptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="genus">Nascus</taxonomicName>
