<subSubSection pageId="81" pageNumber="82" type="nomenclature">
<paragraph pageId="81" pageNumber="82">
<taxonomicName LSID="http://zoobank.org/0FBD808F-DCCA-41F7-BE15-1E0E988B646C" authority="Hansson" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus victoriapookae" order="Hymenoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="victoriapookae">Euplectrus victoriapookae Hansson</taxonomicName>
