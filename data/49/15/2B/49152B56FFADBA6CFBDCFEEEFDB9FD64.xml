<heading bold="true" box="[1025,1283,349,373]" centered="true" fontSize="10" level="2" pageId="76" pageNumber="967" reason="2">
<emphasis bold="true" box="[1025,1283,349,373]" pageId="76" pageNumber="967">
<taxonomicName authority="Schubart &amp; Ng, 2020" authorityName="Schubart &amp; Ng" authorityYear="2020" box="[1025,1148,349,373]" class="Malacostraca" family="Sesarmidae" genus="Migmarma" kingdom="Animalia" order="Decapoda" pageId="76" pageNumber="967" phylum="Arthropoda" rank="genus" status="new genus">
