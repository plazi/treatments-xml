<accessionNumber box="[751,901,152,178]" httpUri="http://www.uniprot.org/uniprot/USM31216" pageId="15" pageNumber="16">
<collectionCode box="[751,816,152,178]" httpUri="http://grbio.org/cool/11pi-mg53" name="Universiti Sains Malaysia" pageId="15" pageNumber="16">USM</collectionCode>
<number box="[825,901,152,178]" pageId="15" pageNumber="16" value="31216.0">31216</number>
</accessionNumber>
and 
<accessionNumber box="[962,1112,152,178]" httpUri="http://www.uniprot.org/uniprot/USM33917" pageId="15" pageNumber="16">
<collectionCode box="[962,1027,152,178]" httpUri="http://grbio.org/cool/11pi-mg53" name="Universiti Sains Malaysia" pageId="15" pageNumber="16">USM</collectionCode>
<number box="[1036,1112,152,178]" pageId="15" pageNumber="16" value="33917.0">33917</number>
