<subSubSection pageId="28" pageNumber="529" type="materials_examined">
<paragraph blockId="28.[151,1437,1539,1852]" pageId="28" pageNumber="529">
<emphasis bold="true" box="[199,433,1539,1564]" pageId="28" pageNumber="529">Material examined.</emphasis>
