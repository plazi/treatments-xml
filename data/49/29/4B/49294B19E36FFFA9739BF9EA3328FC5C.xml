<collectingCountry box="[201,302,908,932]" name="Armenia" pageId="47" pageNumber="48">Armenia</collectingCountry>
, Azerbaijan, Central Asia, northern 
<collectingCountry box="[714,784,908,932]" name="China" pageId="47" pageNumber="48">China</collectingCountry>
, 
<collectingCountry box="[796,907,908,932]" name="Mongolia" pageId="47" pageNumber="48">Mongolia</collectingCountry>
, Siberia, Russian Far East, 
<collectingCountry box="[1223,1290,908,931]" name="Japan" pageId="47" pageNumber="48">Japan</collectingCountry>
, Alaska, and north-western 
<collectingCountry box="[313,397,943,967]" name="Canada" pageId="47" pageNumber="48">Canada</collectingCountry>
(
