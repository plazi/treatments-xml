<taxonomicName authorityName="AT" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[911,1039,1478,1502]" class="Insecta" family="Saldidae" genus="Salda" kingdom="Animalia" order="Hemiptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="littoralis">
