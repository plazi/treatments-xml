<collectingCountry box="[438,537,1791,1815]" name="Turkey" pageId="46" pageNumber="47">Anatolia</collectingCountry>
, Transcaucasia, 
<collectingCountry box="[728,793,1791,1815]" name="Israel" pageId="46" pageNumber="47">Israel</collectingCountry>
, 
<collectingCountry box="[809,910,1791,1815]" name="Lebanon" pageId="46" pageNumber="47">Lebanon</collectingCountry>
, 
<collectingCountry box="[925,987,1791,1815]" name="Syria" pageId="46" pageNumber="47">Syria</collectingCountry>
, 
<collectingCountry box="[1002,1050,1792,1815]" name="Iraq" pageId="46" pageNumber="47">Iraq</collectingCountry>
