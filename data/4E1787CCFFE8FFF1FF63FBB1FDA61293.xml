<typeStatus id="19058878FFE8FFF1FF63FBD6FF631319" box="[178,225,1095,1115]" pageId="10" pageNumber="1459">Type</typeStatus>
species: 
<taxonomicName id="01BE4D59FFE8FFF1FEE5FBD6FDD91319" authority="HALIDAY 1838" authorityName="HALIDAY" authorityYear="1838" box="[308,603,1095,1115]" class="Insecta" family="Braconidae" genus="Alysia" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="1459" phylum="Arthropoda" rank="species" species="picinervis">
<emphasis id="F4CAEAC8FFE8FFF1FEE5FBD6FE491319" box="[308,459,1095,1115]" italics="true" pageId="10" pageNumber="1459">Alysia picinervis</emphasis>
