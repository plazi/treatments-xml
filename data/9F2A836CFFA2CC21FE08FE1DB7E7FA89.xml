<taxonomicName id="D08349F9FFA2CC21FD83FC8EB601FCAD" box="[608,792,840,864]" class="Arachnida" family="Phytoseiidae" genus="Phytoseius" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="540" phylum="Arthropoda" rank="species" species="amba">
