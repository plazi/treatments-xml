<collectingCountry id="63C97619FFDAD578FB06533E2A2AFD6C" box="[1235,1320,619,645]" name="Austria" pageId="11" pageNumber="522">Austria</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FAE0533E2A9AFD6C" box="[1333,1432,619,645]" name="Belgium" pageId="11" pageNumber="522">Belgium</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FF4253DA2FF8FD40" box="[151,250,655,681]" name="Bulgaria" pageId="11" pageNumber="522">Bulgaria</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FEDC53DA2E5CFD40" box="[265,350,655,681]" name="Croatia" pageId="11" pageNumber="522">Croatia</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FEB853DA2D24FD40" box="[365,550,655,681]" name="Czech Republic" pageId="11" pageNumber="522">Czech Republic</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FDE053DA2D8FFD40" box="[565,653,655,681]" name="Finland" pageId="11" pageNumber="522">Finland</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FD4953DA2C07FD40" box="[668,773,655,681]" name="Germany" pageId="11" pageNumber="522">Germany</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FCC153DA2C64FD40" box="[788,870,655,681]" name="Greece" pageId="11" pageNumber="522">Greece</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FCA053DA2CABFD40" box="[885,937,655,681]" name="Italy" pageId="11" pageNumber="522">Italy</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FC6D53DA2B0BFD40" box="[952,1033,655,681]" name="Poland" pageId="11" pageNumber="522">Poland</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FBCD53DA2B82FD40" box="[1048,1152,655,681]" name="Romania" pageId="11" pageNumber="522">Romania</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FB5A53DA2BDEFD40" box="[1167,1244,655,681]" name="Russia" pageId="11" pageNumber="522">Russia</collectingCountry>
