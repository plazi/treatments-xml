<collectingCountry id="63C97619FFDAD578FB3E53DA2A2CFD40" box="[1259,1326,655,681]" name="Spain" pageId="11" pageNumber="522">Spain</collectingCountry>
, 
<collectingCountry id="63C97619FFDAD578FAE853DA2A9AFD40" box="[1341,1432,655,681]" name="Sweden" pageId="11" pageNumber="522">Sweden</collectingCountry>
