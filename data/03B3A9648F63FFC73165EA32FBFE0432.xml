<geoCoordinate id="EE2E7EB58F63FFC930E3ED6DFCB50369" box="[677,776,1071,1095]" degrees="23" direction="south" minutes="08,6" orientation="latitude" pageId="7" pageNumber="696" precision="92" value="-23.143333">23°08,6’S</geoCoordinate>
