<taxonomicName id="4C1A63F18F6DFFC73237EDCFFE820389" authorityName="Abdou &amp; Bouchet" authorityYear="2000" box="[113,319,1165,1191]" class="Gastropoda" family="Endodontidae" genus="Minidonta" kingdom="Animalia" order="Stylommatophora" pageId="9" pageNumber="698" phylum="Mollusca" rank="species" species="vallonia">
