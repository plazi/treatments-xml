<geoCoordinate id="EE2E7EB58F63FFC93155ED6DFC2D0369" box="[787,912,1071,1095]" degrees="135" direction="west" minutes="01,8" orientation="longitude" pageId="7" pageNumber="696" precision="92" value="-135.03">135°01,8’W</geoCoordinate>
