<taxonomicName id="63DDCB9AA0021473FE84E43BFE502E29" authority="Gómez, 2022" authorityName="Gómez" authorityYear="2022" box="[278,448,1801,1825]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="mvuvii" status="sp. nov.">
