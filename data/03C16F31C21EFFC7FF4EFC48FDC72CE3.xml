<taxonomicName id="4C68A5A4C21EFFC7FEA2FAA5FDE22C77" authorityName="Mayer" authorityYear="1953" box="[379,536,1372,1398]" class="Insecta" family="Sciomyzidae" genus="Trypetolimnia" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="92" phylum="Arthropoda" rank="genus">
