<paragraph id="2DF546C913444E460A24C583120491B4" pageId="1" pageNumber="44">
<taxonomicName id="5FA52BBE16EAA1CC6D787A3ADFC5EBC3" class="Insecta" family="Delphacidae" genus="Tropidocephala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tropidocephala dingi" order="Hemiptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="dingi">Tropidocephala dingi</taxonomicName>
