<bibRefCitation id="A72317D9C94EFF855DDFF9F3FC9B1C16" author="Wakiya" box="[727,882,1548,1572]" firstAuthor="Wakiya" pageId="44" pageNumber="45" pagination="139 - 292" refId="ref53869" refString="Wakiya, Y. (1924) The carangoid fishes of Japan. Annals of the Carnegie Museum, 15, 139 - 292." type="journal article" year="1924">Wakiya, 1924</bibRefCitation>
</taxonomicName>
and replaced by 
<taxonomicName id="04B211ABC94EFF855B27F9F3FF1B1C79" authority="Whitley. The" authorityName="Whitley. The" authorityYear="1951" class="Actinopterygii" family="Carangidae" genus="Scomberoides" kingdom="Animalia" order="Perciformes" pageId="44" pageNumber="45" phylum="Chordata" rank="species" species="oshimae">
