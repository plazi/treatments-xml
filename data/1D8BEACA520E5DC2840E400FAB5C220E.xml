<subSubSection id="B3E5F4F5E7351C6A6BA907FB8B7233D0" pageId="0" pageNumber="49" type="etymology">
<paragraph id="E09A8A8455455FD6B46FA32BF5AD781C" pageId="0" pageNumber="49">Etymology.</paragraph>
<paragraph id="327CB5DC194E57E7481637302983380D" pageId="0" pageNumber="49">
<emphasis id="6BB0877362B8F7DEF81072C1278CD70B" italics="true" pageId="0" pageNumber="49">Touhoensis</emphasis>
