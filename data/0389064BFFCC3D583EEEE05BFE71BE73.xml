<quantity id="4CD81AB8FFCC3D583EBFE616FF32BE73" box="[198,255,1362,1387]" metricMagnitude="-1" metricUnit="m" metricValue="4.826" pageId="54" pageNumber="55" unit="in" value="19.0">19 in</quantity>
