<quantity id="4CD4CFCF59338C51C70EEA2CFD5EFDBD" box="[547,650,609,633]" metricMagnitude="-4" metricUnit="m" metricValue="8.6" pageId="13" pageNumber="182" unit="mm" value="0.86">0.86 mm</quantity>
