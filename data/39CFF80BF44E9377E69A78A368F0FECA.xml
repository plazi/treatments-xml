<taxonomicName id="641FA379229D43F438B52C1AF236A02B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33999" authority="Borgmeier" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole sensitiva Borgmeier" order="Hymenoptera" pageNumber="231" phylum="Arthropoda" rank="species" species="sensitiva">Pheidole sensitiva Borgmeier</taxonomicName>
