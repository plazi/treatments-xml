<taxonomicName id="4C46ACCDF130FF99FF05F832FD538B8E" authority="Ezcurra de Drago, 1979: 110" authorityName="Ezcurra de Drago" authorityPageNumber="110" authorityYear="1979" box="[151,663,1962,1984]" class="Demospongiae" family="Spongillidae" genus="Corvoheteromeyenia" kingdom="Animalia" order="Haplosclerida" pageId="1" pageNumber="352" phylum="Porifera" rank="genus">
