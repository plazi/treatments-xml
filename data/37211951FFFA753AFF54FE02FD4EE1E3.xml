<location id="BA57FE9CFFFA753AFF21FD83FEFBE767" LSID="urn:lsid:plazi:treatment:37211951FFFA753AFF54FE02FD4EE1E3:BA57FE9CFFFA753AFF21FD83FEFBE767" box="[219,371,601,625]" name="North America" pageId="10" pageNumber="97">North America</location>
