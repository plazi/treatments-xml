<collectionCode id="D9993082FFFA753AFD67FD60FD24E7C7" box="[669,684,698,721]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="10" pageNumber="97" type="Herbarium">P</collectionCode>
