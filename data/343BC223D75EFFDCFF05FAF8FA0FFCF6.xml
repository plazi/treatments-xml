<taxonomicName id="7B9208B6D75EFFDDFE2DFAF8FD0DFA31" authorityName="Belle" authorityYear="1970" box="[447,670,1402,1429]" class="Insecta" family="Gomphidae" genus="Phyllogomphoides" kingdom="Animalia" order="Odonata" pageId="10" pageNumber="11" phylum="Arthropoda" rank="genus">
