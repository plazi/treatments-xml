</taxonomicName>
and 
<taxonomicName id="038DE1CF7C39006AD3467CFFFCA4FE7A" box="[623,793,450,475]" class="Insecta" family="Aleyrodidae" genus="Aleurocanthus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
