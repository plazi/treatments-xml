<taxonomicName id="038DE1CF7C39006AD5EE7CD5FACEFDA0" box="[1223,1395,488,513]" class="Insecta" family="Aleyrodidae" genus="Aleurocanthus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
