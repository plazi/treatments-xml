(
<bibRefCitation id="A01CE7BD7C39006AD0597CFFFD91FE7D" author="Ashmead" box="[368,556,450,476]" pageId="26" pageNumber="27" refString="Ashmead, W. M. (1885) The orange Aleurodes (Aleurodes citri n. sp.). Florida Dispatch, 2, 704." type="journal article" year="1885">
