<taxonomicName id="038DE1CF7C39006AD2467CD5FC78FDA0" box="[879,965,488,514]" class="Insecta" family="Aleyrodidae" genus="Dialeurodes" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="citri">
