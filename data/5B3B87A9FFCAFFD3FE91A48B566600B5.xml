<collectionCode id="B583AE7AFFCBFFD3FE60A784501606A9" box="[510,574,1164,1183]" collectionName="Germany, Muenchen [= Munich], Zoologische Staatssammlung" country="Germany" httpUri="http://grbio.org/cool/a6ms-py1z" name="Zoologische Staatssammlung" pageId="63" pageNumber="228">ZSMC</collectionCode>
