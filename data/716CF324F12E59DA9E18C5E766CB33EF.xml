<subSubSection id="D8D28F0F27524EFEE47EECFD604DB1E3" pageId="5" pageNumber="408" type="materials_examined">
<paragraph id="D970A1DEEE971624C783E54731372ACD" pageId="5" pageNumber="408">Material examined.</paragraph>
<paragraph id="04894366387479CC1A47E309313B262E" pageId="5" pageNumber="408">
