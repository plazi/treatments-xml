<paragraph id="85372E0B8F052F2BB0F4FF7170B7A94C" pageNumber="13">
<taxonomicName id="63F7112A0EC7AFF4A1490EFBFEFAB65B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:240993" authority="Kieffer, 1904" authorityName="Kieffer" authorityYear="1904" class="Insecta" family="Bethylidae" genus="Pseudisobrachium" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Pseudisobrachium rotundum Waichert &amp; Azevedo" order="Hymenoptera" pageNumber="13" phylum="Arthropoda" rank="species" species="rotundum">Pseudisobrachium rotundum</taxonomicName>
