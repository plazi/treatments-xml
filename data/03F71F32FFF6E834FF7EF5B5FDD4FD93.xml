<collectionCode id="ED4F36E1FFF9E834FAD6F0A9FA2FFF5A" box="[1343,1431,152,175]" country="USA" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="8" pageNumber="391" type="Museum">NMNH</collectionCode>
, others 
<collectionCode id="ED4F36E1FFF9E834FF0BF08DFE90FF20" box="[226,296,188,213]" pageId="8" pageNumber="391">TAUI</collectionCode>
)
</materialsCitation>
; 
<materialsCitation id="3B36A479FFF9E834FED2F08DFB6AFF26" ID-GBIF-Occurrence="3326190309" box="[315,1234,188,213]" collectingDate="2012-05-05" collectingDateMax="2012-05-25" collectingDateMin="2012-05-05" collectionCode="DM" collectorName="D. Mifsud" country="Malta" location="Migra Ferha" pageId="8" pageNumber="391" specimenCount="1" specimenCount-pupa="1" typeStatus="paratype">
<specimenCount id="9D5865ADFFF9E834FED2F08DFE3EFF26" box="[315,390,188,213]" pageId="8" pageNumber="391" type="pupa">1 pupa</specimenCount>
(coated, on SEM stub), same data as holotype, in private collection of 
<collectionCode id="ED4F36E1FFF9E834FB48F08DFB6AFF26" box="[1185,1234,188,211]" country="New Zealand" name="Dominion Museum" pageId="8" pageNumber="391">DM</collectionCode>
