<quantity id="2D139B47B95BFFD04D7D033CFB922C78" box="[1024,1081,260,285]" metricMagnitude="-1" metricUnit="m" metricValue="4.318" pageId="11" pageNumber="328" unit="in" value="17.0">17 in</quantity>
