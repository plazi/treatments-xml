Genus 
<taxonomicName id="F9CB4D38FF9DD650FF16F976FE4CF937" authority="Larsen" authorityName="Larsen" box="[238,452,1740,1766]" class="Malacostraca" family="Colletteidae" genus="Cetiopyge" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tanaidacea" pageId="13" pageNumber="530" phylum="Arthropoda" rank="genus">
