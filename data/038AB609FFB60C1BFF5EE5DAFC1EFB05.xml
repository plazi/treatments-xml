<subSubSection id="C3395494FFB60C1BFF0EE11EFE23FB3E" pageId="14" pageNumber="46" type="etymology">
<paragraph id="8B9C071FFFB60C1BFF0EE11EFB06FBDB" blockId="14.[151,1437,260,1205]" box="[199,1165,1106,1131]" pageId="14" pageNumber="46">
<emphasis id="B957DB0DFFB60C1BFF0EE11EFEC5FBDA" bold="true" box="[199,334,1107,1131]" pageId="14" pageNumber="46">Etymology.</emphasis>
