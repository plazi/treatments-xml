<paragraph id="D5975C85A8F2773C03CC8AF0E309DA17" pageId="0" pageNumber="17">
<taxonomicName id="0FED00A0E19719D547A0C1995B019AF4" authority="Linnæus, 1758" authorityName="Linnæus" authorityYear="1758" class="Insecta" family="Blaberidae" genus="Aranea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aranea lentiginosa" order="Blattodea" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="lentiginosa">Aranea lentiginosa</taxonomicName>
