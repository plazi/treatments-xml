<taxonomicName id="4C3B4D0E732DAB7F43D5FA18FCACFA79" authority="(Cockerell 1897)" authorityName="Cockerell" authorityYear="1897" baseAuthorityName="Cockerell" baseAuthorityYear="1897" box="[151,824,1480,1506]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="species" species="lustrans" subGenus="Hemihalictus">
<emphasis id="B94FEA9F732DAB7F43D5FA18FCACFA79" bold="true" box="[151,824,1480,1506]" pageId="88" pageNumber="89">
<emphasis id="B94FEA9F732DAB7F43D5FA18FEA9FA79" bold="true" box="[151,317,1480,1506]" italics="true" pageId="88" pageNumber="89">Lasioglossum</emphasis>
(
<emphasis id="B94FEA9F732DAB7F420CFA18FE65FA79" bold="true" box="[334,497,1480,1506]" italics="true" pageId="88" pageNumber="89">Hemihalictus</emphasis>
) 
<emphasis id="B94FEA9F732DAB7F4140FA18FDF0FA79" bold="true" box="[514,612,1480,1506]" italics="true" pageId="88" pageNumber="89">lustrans</emphasis>
(Cockerell 1897)
</emphasis>
</taxonomicName>
</heading>
</paragraph>
