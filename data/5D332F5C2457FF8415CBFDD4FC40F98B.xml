<taxonomicName id="129AE5C92457FF841172FA92FB0BFA3B" box="[1070,1195,1466,1491]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="456" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
