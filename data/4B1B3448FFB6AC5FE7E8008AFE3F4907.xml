<emphasis id="F1C6594CFFB6AC5FE7E8008AFD304DEE" bold="true" box="[151,666,152,178]" pageId="16" pageNumber="500">
<taxonomicName id="04B2FEDDFFB6AC5FE7E8008AFD9E4DEE" box="[151,564,152,178]" class="Insecta" family="Hydroptilidae" genus="Oxyethira" kingdom="Animalia" order="Trichoptera" pageId="16" pageNumber="500" phylum="Arthropoda" rank="species" species="singularis" status="sp. nov." subGenus="Tanytrichia">
<emphasis id="F1C6594CFFB6AC5FE7E8008AFEBA4DEE" bold="true" box="[151,272,152,178]" italics="true" pageId="16" pageNumber="500">Oxyethira</emphasis>
(
<emphasis id="F1C6594CFFB6AC5FE65F008AFE014DEE" bold="true" box="[288,427,152,178]" italics="true" pageId="16" pageNumber="500">Tanytrichia</emphasis>
) 
<emphasis id="F1C6594CFFB6AC5FE6C3008AFD9E4DEE" bold="true" box="[444,564,152,178]" italics="true" pageId="16" pageNumber="500">singularis</emphasis>
