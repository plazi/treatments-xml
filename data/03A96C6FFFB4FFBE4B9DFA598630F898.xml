(
<bibRefCitation id="EF91A088FFB4FFBE4A03F8CD8496F89B" box="[265,421,1841,1866]" pageId="2" pageNumber="3" refId="ref21531" refString="Mathur, R. N. (1975) Psyllidae of the Indian subcontinent. Indian Council of Agricultural Research, New Delhi, 429 pp." type="book">Mathur, 1975</bibRefCitation>
)
</taxonomicName>
, 
