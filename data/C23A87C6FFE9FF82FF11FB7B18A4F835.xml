<emphasis id="78E7EAC2FFE9FF82FB89F94A1883F90D" box="[1039,1157,1754,1778]" italics="true" pageId="90" pageNumber="91">
(
<number id="FC5B24F7FFE9FF82FB91F94A1844F90D" box="[1047,1090,1754,1778]" pageId="90" pageNumber="91" value="310.0">310</number>
<date id="3E2D1010FFE9FF82FBCDF94A187BF90D" box="[1099,1149,1754,1778]" pageId="90" pageNumber="91" value="2010-10">
x 
<number id="FC5B24F7FFE9FF82FBE6F94A187BF90D" box="[1120,1149,1754,1778]" pageId="90" pageNumber="91" value="10.0">10</number>
