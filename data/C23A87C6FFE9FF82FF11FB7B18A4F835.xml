<collectionCode id="2C82AE15FFE9FF82FE8AFA031D5BFA54" LSID="urn:lsid:biocol.org:col:34871" box="[268,349,1427,1451]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="90" pageNumber="91">USNM</collectionCode>
<number id="FC5B24F7FFE9FF82FEE3FA031DCCFA54" box="[357,458,1427,1451]" pageId="90" pageNumber="91" value="1229139.0">1229139</number>
, Carrie Bow Cay forereef cave, 
<quantity id="8D6B9B35FFE9FF82FCB4FA021F69FA55" box="[818,879,1426,1451]" metricMagnitude="1" metricUnit="m" metricValue="2.6" pageId="90" pageNumber="91" unit="m" value="26.0">26 m</quantity>
