<taxonomicName id="7659829240A884757F89EB6FD7CF1275" class="Insecta" family="Bombyliidae" genus="Poecilanthrax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Poecilanthrax" order="Diptera" pageId="21" pageNumber="70" phylum="Arthropoda" rank="genus">Poecilanthrax</taxonomicName>
