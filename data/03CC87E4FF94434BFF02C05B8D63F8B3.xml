<collectionCode id="ED74AE37FF94434BFB93C2688C78F8B3" box="[1030,1089,1803,1827]" httpUri="http://grbio.org/cool/9iaf-dk63" name="Ohio State University" pageId="16" pageNumber="17">OSU</collectionCode>
