<collectionCode id="9CF4C8832F68FFD29DF7F9DB3B55650C" LSID="urn:lsid:biocol.org:col:34838" box="[510,574,1633,1659]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="15" pageNumber="292">SMF</collectionCode>
