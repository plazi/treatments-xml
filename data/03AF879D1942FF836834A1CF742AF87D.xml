<quantity id="4CFE9B6E1943FF846B5CA2C371EBFD2A" box="[362,455,676,702]" metricMagnitude="-3" metricUnit="m" metricValue="6.2" pageId="99" pageNumber="100" unit="mm" value="6.2">6.2 mm</quantity>
