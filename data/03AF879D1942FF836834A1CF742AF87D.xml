<quantity id="4CFE9B6E1943FF846822A2C3725DFD2A" box="[532,625,676,702]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="99" pageNumber="100" unit="mm" value="4.8">4.8 mm</quantity>
