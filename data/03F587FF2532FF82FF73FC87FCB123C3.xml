<collectingCountry id="F34B76792532FF82FE83F990FD7C237B" box="[359,644,1573,1598]" name="South Africa" pageId="3" pageNumber="396">Republic of South Africa</collectingCountry>
, 
<collectingCountry id="F34B76792532FF82FD6BF990FD12237B" box="[655,746,1573,1598]" name="Uganda" pageId="3" pageNumber="396">Uganda</collectingCountry>
