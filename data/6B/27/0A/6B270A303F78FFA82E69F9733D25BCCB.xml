<collectionCode LSID="urn:lsid:biocol.org:col:34980" box="[868,957,1860,1886]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34980" name="Nova Scotia Museum" pageId="10" pageNumber="59">NSMC</collectionCode>
