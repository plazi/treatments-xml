<number box="[1264,1323,1820,1846]" pageId="10" pageNumber="59" value="1982.0">1982</number>
(
<emphasis bold="true" box="[274,477,1860,1886]" pageId="10" pageNumber="59">Queens County:</emphasis>
Charlottetown, 
<date box="[666,856,1860,1886]" pageId="10" pageNumber="59" value="1982-08-22">
