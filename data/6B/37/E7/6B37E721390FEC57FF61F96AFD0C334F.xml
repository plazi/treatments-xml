– 
<number box="[943,971,553,578]" pageId="16" pageNumber="527" value="15.0">15</number>
</date>
) [
<number box="[1006,1016,554,578]" pageId="16" pageNumber="527" value="1.0">1</number>
– 
<number box="[1035,1045,554,578]" pageId="16" pageNumber="527" value="1.0">1</number>
–0], IV (
<date box="[1170,1312,554,578]" pageId="16" pageNumber="527" value="2012-02-01" valueMax="2012-03-02" valueMin="2012-02-01">
<number box="[1170,1209,554,578]" pageId="16" pageNumber="527" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
