</date>
) [
<number box="[667,677,554,578]" pageId="16" pageNumber="527" value="1.0">1</number>
– 
<number box="[696,742,554,578]" pageId="16" pageNumber="527" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
], III (
<date box="[829,971,553,578]" pageId="16" pageNumber="527" value="1915-01-01" valueMax="1915-03-02" valueMin="1915-01-01">
<number box="[829,870,553,578]" pageId="16" pageNumber="527" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
