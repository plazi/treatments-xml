<number box="[1424,1436,545,566]" pageId="1" pageNumber="19" value="4.0">4</number>
</keyLead>
</paragraph>
<paragraph blockId="1.[151,1437,222,1505]" box="[151,1436,574,595]" pageId="1" pageNumber="19">
<keyLead box="[151,1436,574,595]" pageId="1" pageNumber="19">
- 
