<number box="[953,965,692,713]" pageId="1" pageNumber="19" value="3.0">3</number>
hamuli. 
<collectingCountry box="[1049,1097,692,713]" name="India" pageId="1" pageNumber="19">Hind</collectingCountry>
femur 
<number box="[1166,1196,692,713]" pageId="1" pageNumber="19" value="3.5">3.5</number>
