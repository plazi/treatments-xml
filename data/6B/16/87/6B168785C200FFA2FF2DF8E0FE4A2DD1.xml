<paragraph blockId="1.[151,1437,1773,2014]" pageId="1" pageNumber="146">
<emphasis bold="true" box="[199,334,1773,1797]" pageId="1" pageNumber="146">Etymology.</emphasis>
<taxonomicName box="[344,397,1773,1797]" class="Insecta" family="Gomphidae" genus="Phaenandrogomphus" kingdom="Animalia" order="Odonata" pageId="1" pageNumber="146" phylum="Arthropoda" rank="species" species="safei">
