. 
<taxonomicName box="[519,788,620,644]" class="Insecta" family="Tortricidae" genus="Eupinivora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="277" phylum="Arthropoda" rank="species" species="angulicosta">
