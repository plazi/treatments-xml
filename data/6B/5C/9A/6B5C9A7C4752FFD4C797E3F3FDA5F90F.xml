</bibRefCitation>
(as 
<taxonomicName authority="Pjatnitskii, 1929" authorityName="Pjatnitskii" authorityYear="1929" box="[151,636,1372,1397]" class="Insecta" family="Curculionidae" genus="Hypothenemus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="404" phylum="Arthropoda" rank="species" species="lezhavai">
<emphasis box="[151,427,1372,1397]" italics="true" pageId="3" pageNumber="404">Hypothenemus lezhavai</emphasis>
Pjatnitskii, 
<number box="[580,636,1372,1396]" pageId="3" pageNumber="404" value="1929.0">1929</number>
</taxonomicName>
and 
<taxonomicName authority="Wollaston, 1860" authorityName="Wollaston" authorityYear="1860" box="[708,1230,1372,1397]" class="Insecta" family="Curculionidae" genus="Hypothenemus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="404" phylum="Arthropoda" rank="species" species="aspericollis">
<emphasis box="[708,1022,1372,1397]" italics="true" pageId="3" pageNumber="404">Hypothenemus aspericollis</emphasis>
Wollaston, 
<number box="[1174,1230,1372,1397]" pageId="3" pageNumber="404" value="1860.0">1860</number>
</taxonomicName>
(see 
<bibRefCitation author="Ghahari" pageId="3" pageNumber="404" refString="Ghahari, H. &amp; Colonelli, E. (2012) Curculionoidea from Golestan province, Northern Iran. Fragmenta Entomologica, 44, 101 - 161. http: // dx. doi. org / 10.4081 / fe. 2012.37" type="journal article" year="2012">
Ghahari &amp; Colonelli 
<number box="[262,321,1408,1432]" pageId="3" pageNumber="404" value="2012.0">2012</number>
</bibRefCitation>
