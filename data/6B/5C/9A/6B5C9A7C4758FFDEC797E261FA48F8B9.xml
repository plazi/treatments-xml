<emphasis bold="true" box="[151,394,1477,1502]" pageId="9" pageNumber="410">
Distribution in 
<collectingCountry box="[334,389,1477,1502]" name="Iran" pageId="9" pageNumber="410">Iran</collectingCountry>
.
</emphasis>
<collectingCountry box="[401,452,1478,1501]" name="Iran" pageId="9" pageNumber="410">Fars</collectingCountry>
, Isfahan, Khorasan, Zanjan (
<bibRefCitation author="Modarres" box="[773,1013,1477,1502]" pageId="9" pageNumber="410" refString="Modarres Awal, M. (1997) Platypodidae, Scolytidae. In: Modarres Awal, M. (Ed.), List of agricultural pests and their natural enemies in Iran. Ferdowsi University Press, Mashhad, pp. 185, 192 - 194." type="book chapter" year="1997">
Modarres Awal 
<number box="[954,1013,1478,1502]" pageId="9" pageNumber="410" value="1997.0">1997</number>
</bibRefCitation>
), Guilan (
<bibRefCitation author="Samin" box="[1124,1327,1477,1502]" pageId="9" pageNumber="410" refString="Samin, N., Sakenin, H. &amp; Rastegar, J. (2011) A study of the species composition of Scolytidae (Coleoptera) of north and northwestern Iran. Amurian Zoological Journal, 3, 265 - 267." type="journal article" year="2011">
Samin 
<emphasis box="[1204,1254,1477,1502]" italics="true" pageId="9" pageNumber="410">et al</emphasis>
. 
<number box="[1268,1327,1478,1502]" pageId="9" pageNumber="410" value="2011.0">2011</number>
</bibRefCitation>
), Kerman (
<bibRefCitation author="Modarres" box="[158,406,1513,1538]" pageId="9" pageNumber="410" refString="Modarres Awal, M. (1997) Platypodidae, Scolytidae. In: Modarres Awal, M. (Ed.), List of agricultural pests and their natural enemies in Iran. Ferdowsi University Press, Mashhad, pp. 185, 192 - 194." type="book chapter" year="1997">
Modarres Awal 
<number box="[347,406,1514,1538]" pageId="9" pageNumber="410" value="1997.0">1997</number>
</bibRefCitation>
; 
<bibRefCitation author="Nejad" box="[421,729,1513,1538]" pageId="9" pageNumber="410" refString="Nejad Ghaderi, S. S., Nozari, J., Asadi, M. &amp; Naveh, V. H. (2014) Genetic diversity study of Pistachio twig borer (Chaetoptelius vestitus) populations in Kerman province. Proceedings of 21 st Iranian Plant Protection Congress, 2014, 741." type="journal article" year="2014">
Nejad Ghaderi 
<emphasis box="[599,651,1513,1538]" italics="true" pageId="9" pageNumber="410">et al</emphasis>
. 
<number box="[669,729,1514,1538]" pageId="9" pageNumber="410" value="2014.0">2014</number>
</bibRefCitation>
), Semnan (
<bibRefCitation author="Borumand" box="[860,1050,1513,1538]" pageId="9" pageNumber="410" refString="Borumand, H. (1998) Insects of Iran: the list of coleoptera in the insect collection of Plant Pests and Diseases Research Institute: Coleoptera (XXIV), Curculionoidea, Fam. 162, 166 - 171 (Anthribidae, Attelabidae, Brentidae, Apionidae, Curculionidae, Scolytidae, Platypodidae. Plants Pests and Diseases Research Institute, Tehran, 110 pp." type="book chapter" year="1998">
Borumand 
<number box="[991,1050,1514,1538]" pageId="9" pageNumber="410" value="1998.0">1998</number>
</bibRefCitation>
), 
<collectingCountry box="[1074,1119,1514,1537]" name="Iran" pageId="9" pageNumber="410">Iran</collectingCountry>
(no locality cited) (
<bibRefCitation author="Knizek" pageId="9" pageNumber="410" refString="Knizek, M. (2011) Platypodinae and Scolytinae. In: Lobl, I. &amp; Smetana, A. (Eds.), Catalogue of Palaearctic Coleoptera. Vol. 7. Curculionoidea I. Apollo Books, Stenstrup, pp. 201 - 251." type="book" year="2011">
Knížek 
<number box="[151,211,1550,1574]" pageId="9" pageNumber="410" value="2011.0">2011</number>
</bibRefCitation>
