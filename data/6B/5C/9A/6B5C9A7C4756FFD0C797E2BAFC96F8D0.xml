<taxonomicName box="[856,920,1628,1651]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="408" phylum="Tracheophyta" rank="genus">
<emphasis box="[856,920,1628,1651]" italics="true" pageId="7" pageNumber="408">Picea</emphasis>
</taxonomicName>
and 
<taxonomicName box="[981,1045,1628,1651]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="7" pageNumber="408" phylum="Tracheophyta" rank="genus">
<emphasis box="[981,1045,1628,1651]" italics="true" pageId="7" pageNumber="408">Pinus</emphasis>
</taxonomicName>
