<taxonomicName authority="Skalitzky, 1876" authorityName="Skalitzky" authorityYear="1876" box="[151,623,1268,1293]" class="Insecta" family="Curculionidae" genus="Scolytus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="419" phylum="Arthropoda" rank="subSpecies" species="kirschii" subSpecies="kirschii">
<emphasis bold="true" box="[151,623,1268,1293]" pageId="18" pageNumber="419">
<emphasis bold="true" box="[151,436,1268,1293]" italics="true" pageId="18" pageNumber="419">Scolytus kirschii kirschii</emphasis>
Skalitzky, 
<number box="[567,623,1268,1293]" pageId="18" pageNumber="419" value="1876.0">1876</number>
</emphasis>
</taxonomicName>
