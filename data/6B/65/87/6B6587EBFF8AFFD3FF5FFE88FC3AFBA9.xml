<emphasis bold="true" box="[151,274,1951,1975]" pageId="4" pageNumber="95">FIGURE 2.</emphasis>
<taxonomicName authority="Przystałkowska, 2022" authorityName="Przystałkowska" authorityYear="2022" box="[280,456,1952,1975]" class="Insecta" family="Arctiidae" genus="Apisa" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="95" phylum="Arthropoda" rank="species" species="atrovenosa" status="sp. n.">
<emphasis box="[280,456,1952,1975]" italics="true" pageId="4" pageNumber="95">Apisa atrovenosa</emphasis>
</taxonomicName>
<emphasis bold="true" box="[461,517,1952,1976]" pageId="4" pageNumber="95">
