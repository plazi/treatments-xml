species. 28 
<taxonomicName authority="Baly, 1886" authorityName="Baly" authorityYear="1886" class="Insecta" family="Chrysomelidae" genus="Paridea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paridea (Semacia) sexmaculata" order="Coleoptera" pageId="9" pageNumber="92" phylum="Arthropoda" rank="species" species="sexmaculata" subGenus="Semacia">Paridea (Semacia) sexmaculata</taxonomicName>
