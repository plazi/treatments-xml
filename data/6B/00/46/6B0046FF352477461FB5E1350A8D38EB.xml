<taxonomicName authority="Ogloblin, 1936" authorityName="Ogloblin" authorityYear="1936" class="Insecta" family="Chrysomelidae" genus="Paraulaca" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paraulaca taiwana" order="Coleoptera" pageId="10" pageNumber="93" phylum="Arthropoda" rank="species" species="taiwana">
