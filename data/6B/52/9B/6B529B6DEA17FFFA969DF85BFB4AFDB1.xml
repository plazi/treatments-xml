<subSubSection pageId="6" pageNumber="7" type="description">
<paragraph blockId="6.[170,794,410,499]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[170,315,410,435]" pageId="6" pageNumber="7">Description:</emphasis>
<typeStatus box="[321,424,410,435]" pageId="6" pageNumber="7">Holotype</typeStatus>
male: total length 
<quantity box="[636,722,410,435]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="6" pageNumber="7" unit="mm" value="2.4">2.4 mm</quantity>
; maximum width of body 
