, 
<collectingCountry box="[494,587,1378,1404]" name="Georgia" pageId="2" pageNumber="75">Georgia</collectingCountry>
, 
<collectingCountry box="[597,672,1378,1404]" name="Russia" pageId="2" pageNumber="75">Russia</collectingCountry>
