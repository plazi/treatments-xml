<subSubSection box="[199,999,795,820]" pageId="3" pageNumber="72" type="etymology">
<paragraph blockId="3.[151,1436,151,1030]" box="[199,999,795,820]" pageId="3" pageNumber="72">
<emphasis bold="true" box="[199,334,795,819]" pageId="3" pageNumber="72">Etymology.</emphasis>
