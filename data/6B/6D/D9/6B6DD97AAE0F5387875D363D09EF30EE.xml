<subSubSection pageId="0" pageNumber="53" type="reference_group">
<paragraph pageId="0" pageNumber="53">
= 
<taxonomicName class="Insecta" family="Staphylinidae" genus="Bledius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bledius philadelphicus" order="Coleoptera" pageId="0" pageNumber="53" phylum="Arthropoda" rank="species" species="philadelphicus">Bledius philadelphicus</taxonomicName>
