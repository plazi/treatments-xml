<taxonomicName genus="C." lsidName="C. daemeli" pageId="0" pageNumber="81" rank="species" species="daemeli">
<emphasis italics="true" pageId="0" pageNumber="81">C. daemeli</emphasis>
</taxonomicName>
, neither in 
<normalizedToken originalValue="MusÃ©um">Museum</normalizedToken>
national 
<normalizedToken originalValue="dâHistoire">d'Histoire</normalizedToken>
naturelle, Paris (MNHN) nor in the NHMUK. 
<taxonomicName class="Insecta" family="Dytiscidae" genus="Copelatus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Copelatus daemeli" order="Coleoptera" pageId="0" pageNumber="81" phylum="Arthropoda" rank="species" species="daemeli">
<emphasis italics="true" pageId="0" pageNumber="81">Copelatus daemeli</emphasis>
</taxonomicName>
