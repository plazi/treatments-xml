<subSubSection pageId="6" pageNumber="195" type="etymology">
<paragraph blockId="6.[160,1155,1170,1231]" pageId="6" pageNumber="195">
<emphasis bold="true" box="[160,291,1170,1196]" italics="true" pageId="6" pageNumber="195">Etymology.</emphasis>
