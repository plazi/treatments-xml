species: 
<taxonomicName box="[294,544,292,314]" class="Insecta" family="Issidae" genus="Tetricodissus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="79" phylum="Arthropoda" rank="species" species="pandlineus" status="sp. nov.">
