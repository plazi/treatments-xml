and 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28514" class="Insecta" family="Formicidae" genus="Cyphoidris" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphoidris werneri Bolton" order="Hymenoptera" pageNumber="26" phylum="Arthropoda" rank="species" species="werneri">C. werneri</taxonomicName>
