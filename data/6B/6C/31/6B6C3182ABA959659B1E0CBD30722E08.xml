<geoCoordinate degrees="25.4833" direction="north" orientation="latitude" precision="5" value="25.4833">25.4833N</geoCoordinate>
, 
<geoCoordinate degrees="107.4500" direction="east" orientation="longitude" precision="5" value="107.45">107.4500E</geoCoordinate>
