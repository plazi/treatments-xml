(
<bibRefCitation author="MAYR G. &amp; POHL B. &amp; HARTMAN S. &amp; PETERS D. S." box="[684,874,659,685]" pageId="2" pageNumber="619" pagination="97 - 116" refId="ref4767" refString="MAYR G., POHL B., HARTMAN S. &amp; PETERS D. S. 2007. - The tenth skeletan specimen of Archaeopteryx. Zoological Journal of the Linnean Society 149: 97 - 116." type="journal article" year="2007">
Mayr 
<emphasis box="[754,808,659,685]" italics="true" pageId="2" pageNumber="619">et al.</emphasis>
2007
</bibRefCitation>
), 
<taxonomicName authority="Gao et al., 2008" authorityName="Gao" authorityYear="2008" box="[899,1209,659,686]" class="Reptilia" family="Paraves incertae sedis" genus="Zhongornis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Paraves incertae sedis" pageId="2" pageNumber="619" phylum="Chordata" rank="genus">
<emphasis box="[899,1018,659,685]" italics="true" pageId="2" pageNumber="619">Zhongornis</emphasis>
<bibRefCitation author="GAO C. &amp; CHIAPPE L. M. &amp; MENG Q. &amp; O'CONNOR J. K. &amp; WANG X. &amp; CHENG X. &amp; LIU J." box="[1027,1209,659,686]" pageId="2" pageNumber="619" pagination="775 - 791" refId="ref4479" refString="GAO C., CHIAPPE L. M., MENG Q., O'CONNOR J. K., WANG X., CHENG X. &amp; LIU J. 2008. - A new basal lineage of Early Cretaceous birds from China and its implications on the evolution of the avian tail. Palaeontology 51: 775 - 791." type="journal article" year="2008">
Gao 
<emphasis box="[1082,1137,659,685]" italics="true" pageId="2" pageNumber="619">et al.</emphasis>
, 2008
</bibRefCitation>
</taxonomicName>
(
<bibRefCitation author="GAO C. &amp; CHIAPPE L. M. &amp; MENG Q. &amp; O'CONNOR J. K. &amp; WANG X. &amp; CHENG X. &amp; LIU J." box="[686,852,691,717]" pageId="2" pageNumber="619" pagination="775 - 791" refId="ref4479" refString="GAO C., CHIAPPE L. M., MENG Q., O'CONNOR J. K., WANG X., CHENG X. &amp; LIU J. 2008. - A new basal lineage of Early Cretaceous birds from China and its implications on the evolution of the avian tail. Palaeontology 51: 775 - 791." type="journal article" year="2008">
Gao 
<emphasis box="[737,787,691,717]" italics="true" pageId="2" pageNumber="619">et al.</emphasis>
