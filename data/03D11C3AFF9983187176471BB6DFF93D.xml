<taxonomicName id="4C78D6AFFF9A831871AC440BB4A2F945" authority="(Berg.) Rusby" authorityName="(Berg.) Rusby" box="[466,913,1644,1670]" class="Magnoliopsida" family="Lacistemataceae" genus="Lacistema" kingdom="Plantae" order="Malpighiales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="species" species="aggregatum">
