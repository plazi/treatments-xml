) and 
<taxonomicName id="9AB14D46552573141D78FC5B764362AF" authority="Zeller" authorityName="Zeller" box="[802,1054,943,968]" class="Insecta" family="Pyralidae" genus="Etiella" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="behrii">
