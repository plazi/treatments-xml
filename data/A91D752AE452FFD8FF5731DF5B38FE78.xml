<collectingCountry id="59A384ACE453FFD8FEA7326E5FE0FE94" box="[359,426,331,357]" name="China" pageId="9" pageNumber="460">China</collectingCountry>
(
<collectingRegion id="E3700ADEE453FFD8FE79326E5C5FFE94" box="[441,533,331,357]" country="China" name="Yunnan" pageId="9" pageNumber="460">Yunnan</collectingRegion>
