<caption id="ADFF6FD3FFE6FFB11198D6B0CAC1FA58" ID-DOI="http://doi.org/10.5281/zenodo.5300693" ID-Zenodo-Dep="5300693" box="[151,1418,1486,1510]" httpUri="https://zenodo.org/record/5300693/files/figure.png" pageId="4" pageNumber="545" startId="4.[151,250,1486,1510]" targetBox="[151,1436,630,1460]" targetPageId="4">
