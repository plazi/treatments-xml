<taxonomicName id="3E8044D8FFE6FFB1106CD6B0CD2DFA58" authorityName="Pictet &amp; Saussure" authorityYear="1893" box="[355,614,1486,1510]" class="Insecta" family="Gryllacrididae" genus="Eugryllacris" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="545" phylum="Arthropoda" rank="species" species="vaginalis">
