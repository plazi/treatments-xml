<taxonomicName id="4BD808B04075FF90FEB2AD36FD869078" authority="Moraes et al., 1989" authorityName="Moraes et al." authorityYear="1989" box="[264,639,839,862]" class="Arachnida" family="Phytoseiidae" genus="Euseius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="lokele">
