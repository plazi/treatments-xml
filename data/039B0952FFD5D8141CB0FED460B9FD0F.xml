<taxonomicName id="4C32C3C7FFD5D8141F49FDD46195FDCF" authorityName="Rambur" authorityYear="1842" box="[825,971,578,602]" class="Insecta" family="Panorpidae" genus="Panorpa" kingdom="Animalia" order="Mecoptera" pageId="11" pageNumber="10" phylum="Arthropoda" rank="species" species="rufescens">
