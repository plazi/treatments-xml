<taxonomicName id="4C35CBB4FFF6FFD17413FE881D8C552B" authorityName="Thomas" authorityYear="1904" box="[809,956,348,374]" class="Mammalia" family="Vespertilionidae" genus="Pipistrellus" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="266" phylum="Chordata" rank="species" species="crassulus">
