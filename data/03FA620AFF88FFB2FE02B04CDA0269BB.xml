<taxonomicName id="4C53A89FFF88FFB2FD15B747DC45697B" authority="(Krekich-Strassoldo, 1928)" baseAuthorityName="Krekich-Strassoldo" baseAuthorityYear="1928" box="[664,1083,1072,1096]" class="Insecta" family="Anthicidae" genus="Endomia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="170" phylum="Arthropoda" rank="species" species="gratiosa">
