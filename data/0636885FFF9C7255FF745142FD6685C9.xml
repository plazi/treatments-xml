<location id="8B406F92FF9C7256FDF15297FDEA8384" LSID="urn:lsid:plazi:treatment:0636885FFF9C7255FF745142FD6685C9:8B406F92FF9C7256FDF15297FDEA8384" box="[530,615,1692,1717]" country="India" name="Rebang" pageId="45" pageNumber="46" stateProvince="Assam">Rebang</location>
