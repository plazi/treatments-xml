<taxonomicName id="6B9D2CD8C650FFD0FF5F1ED8FDDC1CB1" authority="Berest, 1988" authorityName="Berest" authorityYear="1988" box="[151,552,979,1005]" class="Insecta" family="Cecidomyiidae" genus="Peromyia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="259" phylum="Arthropoda" rank="species" species="emarginata">
<emphasis id="9EE98B49C650FFD0FF5F1ED8FE641CB0" bold="true" box="[151,400,979,1004]" italics="true" pageId="4" pageNumber="259">Peromyia emarginata</emphasis>
Berest, 
<number id="1A55457CC650FFD0FE391EDFFDDC1CB1" box="[497,552,980,1005]" pageId="4" pageNumber="259" value="1988.0">1988</number>
</taxonomicName>
—new junior synonym of 
<taxonomicName id="6B9D2CD8C650FFD0FC961ED8FB011CB1" authority="Kieffer, 1901" authorityName="Kieffer" authorityYear="1901" box="[862,1269,979,1005]" class="Insecta" family="Cecidomyiidae" genus="Peromyia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="259" phylum="Arthropoda" rank="species" species="fungicola">
<emphasis id="9EE98B49C650FFD0FC961ED8FBB71CB0" bold="true" box="[862,1091,979,1004]" italics="true" pageId="4" pageNumber="259">Peromyia fungicola</emphasis>
(Kieffer, 
<number id="1A55457CC650FFD0FB7B1EDFFB181CB1" box="[1203,1260,980,1005]" pageId="4" pageNumber="259" value="1901.0">1901</number>
)
</taxonomicName>
</emphasis>
</heading>
</paragraph>
