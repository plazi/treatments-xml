<taxonomicName id="4C5B348CA554FFF36E3CBBC4CFC9FA0F" authorityName="Mengual" authorityYear="2012" box="[1346,1435,1425,1446]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="65" pageNumber="164" phylum="Arthropoda" rank="species" species="shirakii">shirakii</taxonomicName>
