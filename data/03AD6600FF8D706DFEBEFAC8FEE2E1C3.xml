<taxonomicName id="4C04AC95FF8E706CFC78FB40FBE6E07F" authorityName="Theischinger" authorityYear="1988" box="[913,1031,1279,1304]" class="Insecta" family="Gripopterygidae" genus="Dinotoperla" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="23" phylum="Arthropoda" rank="species" species="inermis">D. inermis</taxonomicName>
