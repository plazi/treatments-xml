<taxonomicName id="4C0CC6BAFFDBFFB3FC12D90A65337FDB" box="[993,1114,1913,1938]" class="Reptilia" family="Natricidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="24" pageNumber="376" phylum="Chordata" rank="family">Natricidae</taxonomicName>
