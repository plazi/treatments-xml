species: 
<taxonomicName id="4C494D01FFD6FFEBFEEA7A06FD76F9D9" authority="Robinson, 1869" authorityName="Robinson" authorityYear="1869" box="[294,663,1542,1567]" class="Insecta" family="Tortricidae" genus="Conchylis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="218" phylum="Arthropoda" rank="species" species="bunteana">
<emphasis id="B93DEA90FFD6FFEBFEEA7A06FE11F9D8" box="[294,496,1542,1566]" italics="true" pageId="23" pageNumber="218">Conchylis bunteana</emphasis>
