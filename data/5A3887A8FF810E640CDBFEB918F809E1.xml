<quantity id="15699B5BFF810E640CECFC2D1F240BF8" box="[1021,1146,899,923]" metricMagnitude="-1" metricUnit="m" metricValue="4.115" metricValueMax="4.3" metricValueMin="3.9299999999999997" pageId="16" pageNumber="391" unit="mm" value="411.5" valueMax="430.0" valueMin="393.0">393-430 µm</quantity>
, H = 
<quantity id="15699B5BFF810E640BA2FC2D1E700BF8" box="[1203,1326,899,923]" metricMagnitude="-1" metricUnit="m" metricValue="2.975" metricValueMax="3.59" metricValueMin="2.36" pageId="16" pageNumber="391" unit="mm" value="297.5" valueMax="359.0" valueMin="236.0">236-359 µm</quantity>
