<date id="3E2D1010FFE0FF8BFB92FF0818A7FF4F" box="[1044,1185,152,176]" pageId="83" pageNumber="84" value="2007-06-28">
<number id="FC5B24F7FFE0FF8BFB92FF081836FF4F" box="[1044,1072,152,176]" pageId="83" pageNumber="84" value="28.0">28</number>
