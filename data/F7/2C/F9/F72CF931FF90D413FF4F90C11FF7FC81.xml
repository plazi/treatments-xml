<taxonomicName authorityName="McLachlan" authorityYear="1898" box="[1256,1436,1973,1995]" class="Insecta" family="Sericostomatidae" genus="Sericostoma" kingdom="Animalia" order="Trichoptera" pageId="15" pageNumber="498" phylum="Arthropoda" rank="species" species="mesopotamicum">
