<taxonomicName authority="McLachlan 1898" authorityName="McLachlan" authorityYear="1898" box="[309,836,1795,1822]" class="Insecta" family="Sericostomatidae" genus="Sericostoma" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="490" phylum="Arthropoda" rank="species" species="mesopotamicum">
