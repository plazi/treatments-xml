and 
<taxonomicName box="[1270,1358,583,606]" class="Arachnida" family="Cranaidae" genus="Phareicranaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="ortizi">
<emphasis box="[1270,1358,583,606]" italics="true" pageId="2" pageNumber="61">P. ortizi</emphasis>
