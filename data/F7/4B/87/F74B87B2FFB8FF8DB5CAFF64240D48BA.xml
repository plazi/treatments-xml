<taxonomicName box="[151,254,654,678]" class="Arachnida" family="Cranaidae" genus="Phareicranaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="divisor">
<emphasis box="[151,254,654,678]" italics="true" pageId="2" pageNumber="61">P. divisor</emphasis>
</taxonomicName>
, 
<taxonomicName box="[269,392,654,678]" class="Arachnida" family="Cranaidae" genus="Phareicranaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="hermosa">
<emphasis box="[269,392,654,678]" italics="true" pageId="2" pageNumber="61">P. hermosa</emphasis>
</taxonomicName>
and 
<taxonomicName box="[447,587,654,678]" class="Arachnida" family="Cranaidae" genus="Phareicranaus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="singularis">
