<subSubSection pageId="9" pageNumber="568" type="diagnosis">
<paragraph blockId="9.[151,1436,1189,1684]" pageId="9" pageNumber="568">
<emphasis bold="true" box="[199,421,1225,1250]" pageId="9" pageNumber="568">Diagnosis. Length.</emphasis>
(Male) body 
<quantity box="[574,738,1225,1249]" metricMagnitude="-2" metricUnit="m" metricValue="1.185" metricValueMax="1.25" metricValueMin="1.1199999999999999" pageId="9" pageNumber="568" unit="mm" value="11.85" valueMax="12.5" valueMin="11.2">11.2–12.5 mm</quantity>
, wing 
<quantity box="[813,951,1225,1249]" metricMagnitude="-3" metricUnit="m" metricValue="8.85" metricValueMax="9.2" metricValueMin="8.5" pageId="9" pageNumber="568" unit="mm" value="8.85" valueMax="9.2" valueMin="8.5">8.5–9.2 mm</quantity>
; (female) body 9.0– 
<quantity box="[1177,1277,1225,1249]" metricMagnitude="-2" metricUnit="m" metricValue="1.19" pageId="9" pageNumber="568" unit="mm" value="11.9">11.9 mm</quantity>
