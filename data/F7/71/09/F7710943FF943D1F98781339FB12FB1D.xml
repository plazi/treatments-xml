<taxonomicName authority="Causey." authorityName="Causey." box="[469,741,1843,1866]" class="Diplopoda" family="Trichopetalidae" genus="Trigenotyla" kingdom="Animalia" order="Chordeumatida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="vaga">
<taxonomicName authority="Causey" authorityName="Causey" box="[469,737,1843,1866]" class="Diplopoda" family="Trichopetalidae" genus="Trigenotyla" higherTaxonomySource="CoL" kingdom="Animalia" order="Chordeumatida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="vaga">
<taxonomicName box="[469,650,1843,1866]" class="Diplopoda" family="Trichopetalidae" genus="Trigenotyla" higherTaxonomySource="CoL" kingdom="Animalia" order="Chordeumatida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="vaga">
<emphasis box="[469,650,1843,1866]" italics="true" pageId="24" pageNumber="25">
<taxonomicName box="[469,592,1843,1866]" class="Diplopoda" family="Trichopetalidae" genus="Trigenotyla" higherTaxonomySource="CoL" kingdom="Animalia" order="Chordeumatida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">Trigenotyla</taxonomicName>
<taxonomicName authority="Causey." authorityName="Causey." box="[598,650,1843,1866]" pageId="24" pageNumber="25" rank="species" species="vaga">vaga</taxonomicName>
