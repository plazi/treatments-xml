<typeStatus box="[504,606,505,529]" pageId="72" pageNumber="73">lectotype</typeStatus>
for 
<taxonomicName box="[655,779,505,529]" class="Insecta" family="Formicidae" genus="Iridomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="mjobergi">
