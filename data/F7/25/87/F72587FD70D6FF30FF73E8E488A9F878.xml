<taxonomicName authorityName="Brues" authorityYear="1939" box="[1237,1397,1921,1945]" class="Insecta" family="Formicidae" genus="Ctenobethylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="168" pageNumber="169" phylum="Arthropoda" rank="genus">
