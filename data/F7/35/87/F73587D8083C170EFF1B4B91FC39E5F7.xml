</taxonomicName>
and 
<emphasis box="[1015,1144,620,644]" italics="true" pageId="10" pageNumber="55">
<taxonomicName box="[1015,1139,620,644]" class="Insecta" family="Formicidae" genus="Lordomyrma" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="55" phylum="Arthropoda" rank="species" species="idianale">L. idianale</taxonomicName>
