<taxonomicName authority="Heller" authorityName="Heller" authorityYear="1920" box="[930,1433,1620,1644]" class="Insecta" family="Erotylidae" genus="Episcaphula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="72" phylum="Arthropoda" rank="species" species="tamburinea" subGenus="Cosmoscaphula">
