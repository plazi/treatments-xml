<collectingRegion box="[151,269,1628,1652]" country="Spain" name="Andalucia" pageId="12" pageNumber="48">Andalucía</collectingRegion>
, 
<collectingCountry box="[280,347,1628,1652]" name="Spain" pageId="12" pageNumber="48">Spain</collectingCountry>
.
</paragraph>
