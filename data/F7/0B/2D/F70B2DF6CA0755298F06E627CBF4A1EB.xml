<taxonomicName authorityName="Segadilha &amp; Serejo" authorityYear="2020" class="Malacostraca" family="Metapseudidae" genus="Apseudomorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Apseudomorpha brasiliensis" order="Tanaidacea" pageId="0" pageNumber="723" phylum="Arthropoda" rank="species" species="brasiliensis">
<emphasis italics="true" pageId="0" pageNumber="723">Apseudomorpha brasiliensis</emphasis>
</taxonomicName>
sp. nov. (circle) and 
<taxonomicName authorityName="Segadilha &amp; Serejo" authorityYear="2020" class="Malacostraca" family="Pseudozeuxidae" genus="Pseudozeuxo" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudozeuxo fischeri" order="Tanaidacea" pageId="0" pageNumber="723" phylum="Arthropoda" rank="species" species="fischeri">
<emphasis italics="true" pageId="0" pageNumber="723">Pseudozeuxo fischeri</emphasis>
</taxonomicName>
sp. nov. (square) on the study area. 
<emphasis bold="true" pageId="0" pageNumber="723">A-C</emphasis>
