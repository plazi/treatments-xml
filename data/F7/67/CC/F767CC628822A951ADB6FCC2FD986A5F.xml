<subSubSection box="[199,1163,1041,1065]" pageId="13" pageNumber="14" type="etymology">
<paragraph blockId="13.[151,1437,933,2037]" box="[199,1163,1041,1065]" pageId="13" pageNumber="14">
<emphasis box="[199,333,1041,1065]" inLineHeading="true" pageId="13" pageNumber="14" reason="1">
<emphasis bold="true" box="[199,327,1041,1065]" pageId="13" pageNumber="14">Etymology</emphasis>
.
</emphasis>
