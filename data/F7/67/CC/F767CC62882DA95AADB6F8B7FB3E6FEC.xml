<paragraph blockId="4.[151,1436,151,967]" box="[199,535,260,284]" pageId="4" pageNumber="5">
<taxonomicName authority=", Kinsey" authorityName="Kinsey" box="[199,479,260,284]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="species" species="eduardi">
