, 
<taxonomicName class="Gastropoda" family="Bulimulidae" genus="Kuschelenia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="24" pageNumber="25" phylum="Mollusca" rank="species" species="bicolor" subGenus="Vermiculatus">
