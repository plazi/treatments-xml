<subSubSection pageId="24" pageNumber="25" type="type locality">
<paragraph pageId="24" pageNumber="25">Type locality.</paragraph>
<paragraph pageId="24" pageNumber="25">&quot;provinciae Peruviae Xagua&quot; (see remarks).</paragraph>
</subSubSection>
