<taxonomicName authority="Candeze, 1865" authorityName="Candeze" authorityYear="1865" class="Insecta" family="Elateridae" genus="Alaolacon" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Alaolacon" order="Coleoptera" pageId="7" pageNumber="92" phylum="Arthropoda" rank="genus">Alaolacon</taxonomicName>
/ 
<taxonomicName lsidName="cyanipennis" pageId="7" pageNumber="92" rank="species" species="cyanipennis">cyanipennis</taxonomicName>
/ Cdz. &quot;; &quot; 
<taxonomicName authority="Candeze, 1865" authorityName="Candeze" authorityYear="1865" class="Insecta" family="Elateridae" genus="Alaolacon" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Alaolacon" order="Coleoptera" pageId="7" pageNumber="92" phylum="Arthropoda" rank="genus">Alaolacon</taxonomicName>
