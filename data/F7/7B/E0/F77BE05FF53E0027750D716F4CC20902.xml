<normalizedToken originalValue="aufhält" pageId="1" pageNumber="452">aufhaelt</normalizedToken>
(
<normalizedToken originalValue="Wolfshöhle" pageId="1" pageNumber="452">Wolfshoehle</normalizedToken>
und 
<normalizedToken originalValue="Absturzhöhle" pageId="1" pageNumber="452">Absturzhoehle</normalizedToken>
).
</materialsCitation>
</paragraph>
