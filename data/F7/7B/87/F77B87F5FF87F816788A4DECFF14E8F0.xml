<number box="[990,1051,1556,1580]" pageId="2" pageNumber="473" value="1881.0">1881</number>
: 
<number box="[1054,1098,1556,1580]" pageId="2" pageNumber="473" value="131.0">131</number>
, 
<bibRefCitation author="Bodenheimer" box="[1114,1336,1556,1581]" pageId="2" pageNumber="473" refString="Bodenheimer, F. S. (1937) Prodromus Faunae Palestinae. Memoires a l'Institut d'Egypte, 32, 1 - 286." type="journal article" year="1937">
Bodenheimer 
<number box="[1278,1336,1556,1581]" pageId="2" pageNumber="473" value="1937.0">1937</number>
