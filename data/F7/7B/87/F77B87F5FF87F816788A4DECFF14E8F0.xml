<taxonomicName authority="Stein, 1857" authorityName="Stein" authorityYear="1857" box="[412,943,1519,1544]" class="Insecta" family="Nabidae" genus="Prostemma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="473" phylum="Arthropoda" rank="species" species="aeneicolle" subGenus="Prostemma">
