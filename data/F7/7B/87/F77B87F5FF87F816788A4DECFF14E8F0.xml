<number box="[283,313,1556,1581]" pageId="2" pageNumber="473" value="29.0">29</number>
as 
<taxonomicName box="[360,629,1556,1581]" class="Insecta" family="Nabidae" genus="Metastemma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="473" phylum="Arthropoda" rank="species" species="oeneocolle">
<emphasis box="[360,629,1556,1581]" italics="true" pageId="2" pageNumber="473">Metastemma oeneocolle</emphasis>
</taxonomicName>
