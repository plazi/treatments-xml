<collectingCountry box="[243,344,1122,1147]" name="Lebanon" pageId="20" pageNumber="491">Lebanon</collectingCountry>
, 
<collectingCountry box="[355,400,1123,1146]" name="Iraq" pageId="20" pageNumber="491">Iraq</collectingCountry>
and 
<collectingCountry box="[454,499,1123,1146]" name="Iran" pageId="20" pageNumber="491">Iran</collectingCountry>
(
<bibRefCitation author="Kerzhner" box="[514,693,1122,1147]" pageId="20" pageNumber="491" refString="Kerzhner, I. M. (1996) Family Nabidae A. Costa. Damsel bugs. In: Aukema, B. &amp; Rieger, C. (Eds.), Catalogue of the Heteroptera of the Palaearctic Region. Vol. 2. Netherlands Entomological Society, pp. 84 - 107." type="book" year="1996">
Kerzhner, 
<number box="[633,693,1122,1147]" pageId="20" pageNumber="491" value="1996.0">1996</number>
