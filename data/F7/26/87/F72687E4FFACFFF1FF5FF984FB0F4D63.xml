<collectingCountry box="[1054,1158,188,213]" name="Tanzania" pageId="60" pageNumber="61">Tanzania</collectingCountry>
, 
<collectingCountry box="[1169,1231,188,213]" name="Democratic Republic of the Congo" pageId="60" pageNumber="61">
<collectingRegion box="[1169,1231,188,213]" country="Angola" name="Zaire" pageId="60" pageNumber="61">Zaire</collectingRegion>
</collectingCountry>
, and 
<collectingCountry box="[1289,1378,188,213]" name="Zambia" pageId="60" pageNumber="61">Zambia</collectingCountry>
. 
