<collectingCountry box="[359,447,188,213]" name="Malawi" pageId="60" pageNumber="61">Malawi</collectingCountry>
, northeastern 
<taxonomicName box="[604,701,188,213]" class="Magnoliopsida" family="Aizoaceae" genus="Namibia" kingdom="Plantae" order="Caryophyllales" pageId="60" pageNumber="129" phylum="Tracheophyta" rank="genus">
<collectingCountry box="[604,701,188,213]" name="Namibia" pageId="60" pageNumber="61">Namibia</collectingCountry>
</taxonomicName>
