<collectingRegion box="[578,754,586,611]" country="United States of America" name="South Carolina" pageId="4" pageNumber="537">
South 
<collectingRegion box="[655,754,586,611]" country="Puerto Rico" name="Carolina" pageId="4" pageNumber="537">Carolina</collectingRegion>
</collectingRegion>
, 
<collectingCountry box="[771,830,587,611]" name="United States of America" pageId="4" pageNumber="537">USA</collectingCountry>
, 
<collectingCountry box="[846,951,586,611]" name="Bahamas" pageId="4" pageNumber="537">Bahamas</collectingCountry>
, 
<collectingCountry box="[968,1027,586,611]" name="Cuba" pageId="4" pageNumber="537">Cuba</collectingCountry>
west to Gulf of 
<collectingRegion box="[1237,1325,586,611]" country="Mexico" name="Mexico" pageId="4" pageNumber="537">
<collectingCountry box="[1237,1325,586,611]" name="Mexico" pageId="4" pageNumber="537">Mexico</collectingCountry>
</collectingRegion>
, south to 
<collectingCountry box="[151,261,622,647]" name="Suriname" pageId="4" pageNumber="537">Suriname</collectingCountry>
, including the Antilles. 
<collectingRegion box="[536,620,623,646]" country="Sierra Leone" name="Eastern" pageId="4" pageNumber="537">Eastern</collectingRegion>
Atlantic: from northwest 
<collectingCountry box="[918,982,622,647]" name="Spain" pageId="4" pageNumber="537">Spain</collectingCountry>
