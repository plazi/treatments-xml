<subSubSection pageId="0" pageNumber="129" type="host">
<paragraph pageId="0" pageNumber="129">Host plants.</paragraph>
<paragraph pageId="0" pageNumber="129">
<taxonomicName authority="Lee &amp; Beenen, 2020" authorityName="Lee &amp; Beenen" authorityYear="2020" class="Insecta" family="Mniaceae" genus="Taiwanoshaira" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="129" phylum="Arthropoda" rank="family">Mniaceae</taxonomicName>
