° 
<number id="3DBDFC20A73F891EFF56FA79FF06F9D4" box="[200,232,1533,1559]" pageId="9" pageNumber="10" value="39.0">39</number>
'N
</geoCoordinate>
<geoCoordinate id="EE4188C0A73F891EFE92FA79FEB3F9D4" box="[268,349,1533,1559]" direction="east" orientation="longitude" pageId="9" pageNumber="10" precision="925" value="3.1833334">
<number id="3DBDFC20A73F891EFE92FA79FEF5F9D4" box="[268,283,1533,1559]" pageId="9" pageNumber="10" value="3.0">3</number>
