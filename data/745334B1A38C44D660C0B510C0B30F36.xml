<taxonomicName id="A6C9470327AB2D7A99B5EA0D2F32794C" authorityName="Nutt., Gen. Amer. Pl. 2: 115" authorityYear="1818" class="Magnoliopsida" family="Leguminosae" genus="Wisteria" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Wisteria" order="Fabales" pageId="26" pageNumber="27" phylum="Tracheophyta" rank="genus">
