<taxonomicName id="4C334D69FFD46178FCA6FA3EFD53FA0A" authority="(Dettmann &amp; Clifford 1992)" baseAuthorityName="Dettmann &amp; Clifford" baseAuthorityYear="1992" class="Polypodiopsida" family="Anemiaceae" kingdom="Plantae" order="Schizaeales" pageId="21" pageNumber="108" phylum="Tracheophyta" rank="family">
