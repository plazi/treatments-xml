<taxonomicName id="B6F0FB0B83EBD52E1C5EDF0E1CE59D9B" LSID="F0D67FCC-EBBF-581B-9BAD-8232E67A1D62" authority="Hern. - Restr., L. W. Hou, L. Cai &amp; Crous" authorityName="Hern. - Restr., L. W. Hou, L. Cai &amp; Crous" authorityYear="2020" class="Dothideomycetes" family="Didymellaceae" genus="Vandijckomycella" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Vandijckomycella joseae" order="Pleosporales" pageId="0" pageNumber="49" phylum="Ascomycota" rank="species" species="joseae" status="sp. nov.">Vandijckomycella joseae Hern.-Restr., L. W. Hou, L. Cai &amp; Crous</taxonomicName>
