<quantity id="E3329B2CB810FF82FC2FE720E841FCEB" box="[984,1099,854,878]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" metricValueMax="3.0" metricValueMin="1.5" pageId="6" pageNumber="54" unit="mm" value="2.25" valueMax="3.0" valueMin="1.5">1.5–3 mm</quantity>
