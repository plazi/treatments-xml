<taxonomicName id="3F2EEC573AEDFD9B0B8B5C89430F0B58" authorityName="Castro &amp; Den Heyer" authorityYear="2008" class="Arachnida" family="Cunaxidae" genus="Cunaxatricha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Trombidiformes" pageId="54" pageNumber="55" phylum="Arthropoda" rank="genus">
