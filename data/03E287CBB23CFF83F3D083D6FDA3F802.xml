<taxonomicName id="4C4B4D5EB233FF83F1258235FB14F8AE" authority="McCracken &amp; Egbert, 1922" authorityName="McCracken &amp; Egbert" authorityYear="1922" box="[610,1157,1881,1907]" class="Insecta" family="Cynipidae" genus="Andricus" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="confertus">
