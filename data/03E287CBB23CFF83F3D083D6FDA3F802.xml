<taxonomicName id="4C4B4D5EB233FF83F7F28235FA0DF8AE" authority="Nee" authorityName="Nee" authorityYear="1801" box="[1205,1436,1881,1907]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="species" species="lobata">
