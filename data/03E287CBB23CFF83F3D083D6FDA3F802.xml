<taxonomicName id="4C4B4D5EB233FF83F3E68211FEC7F845" box="[161,342,1917,1944]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="24" pageNumber="56" phylum="Tracheophyta" rank="species" species="section">
