<taxonomicName id="4C63EFE7FFCCFF9F85AB129AC22AB215" box="[188,293,1266,1291]" class="Insecta" family="Blaberidae" genus="Epilampra" kingdom="Animalia" order="Blattodea" pageId="18" pageNumber="319" phylum="Arthropoda" rank="species" species="laevis">
