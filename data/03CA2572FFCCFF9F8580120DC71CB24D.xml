<collectingRegion id="49A75A86FFCCFF9F815212A6C795B3F8" box="[1093,1178,1231,1254]" country="China" name="Yunnan" pageId="18" pageNumber="319">Yunnan</collectingRegion>
(
<collectingCountry id="F374D4F4FFCCFF9F81BD12A7C7FFB3F9" box="[1194,1264,1230,1255]" name="China" pageId="18" pageNumber="319">China</collectingCountry>
