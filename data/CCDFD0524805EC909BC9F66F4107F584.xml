<taxonomicName id="7D25397C4EA99F662D4C1AE2D04AD8A2" class="Insecta" family="Curculionidae" genus="Trachodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trachodes" order="Coleoptera" pageId="271" pageNumber="272" phylum="Arthropoda" rank="genus">Trachodes</taxonomicName>
