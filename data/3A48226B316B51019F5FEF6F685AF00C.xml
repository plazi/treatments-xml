<taxonomicName id="40A10401D727E7BFFFEE338D804975B5" class="Magnoliopsida" family="Asteraceae" genus="Cyanthillium" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="37" pageNumber="62" phylum="Tracheophyta" rank="species" species="cinereum">
