<quantity id="4C889B385E1DFFE91B125196FE24F901" box="[277,388,1677,1702]" metricMagnitude="-3" metricUnit="m" metricValue="3.75" metricValueMax="4.0" metricValueMin="3.5" pageId="37" pageNumber="192" unit="mm" value="3.75" valueMax="4.0" valueMin="3.5">3.5-4 mm</quantity>
