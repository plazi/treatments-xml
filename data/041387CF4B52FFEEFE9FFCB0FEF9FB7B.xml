<taxonomicName id="4BBA4D5A4B52FFEEFE15FCB0FC94FC3C" authority="Bequaert" box="[386,823,884,910]" class="Insecta" family="Vespidae" genus="Mischocyttarus" kingdom="Animalia" order="Hymenoptera" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="picturatus">
