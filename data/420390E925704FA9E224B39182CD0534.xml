<subSubSection id="39E523B2A2A9814F674BF4C0653CC251" pageId="null" pageNumber="582" type="nomenclature">
<paragraph id="2EF59D04C89A4A1ABE164FAB42388B52" pageId="null" pageNumber="582">
<taxonomicName id="073330CCF4CB2C4042A721C5B90AE7E8" authority="L." class="Magnoliopsida" family="Fabaceae" genus="Vicia" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="582" phylum="Tracheophyta" rank="species" species="cracca">
<pageBreakToken id="8880957EB7B4BD170793515F44BF5C3C" pageId="null" pageNumber="582">Vicia</pageBreakToken>
<normalizedToken id="B627CE7A6D127C4BE680B7B8DE781184" originalValue="Crácca" pageId="null" pageNumber="582">Cracca</normalizedToken>
<authorityName id="50688781B1677A7F376430316F654964" pageId="null" pageNumber="582">L.</authorityName>
</taxonomicName>
