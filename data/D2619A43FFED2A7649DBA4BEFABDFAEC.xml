<taxonomicName id="9DC850D6FFED2A764856A5D6FE64FE0F" box="[282,409,439,465]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
: 
<taxonomicName id="9DC850D6FFED2A7648E9A5D6FE01FE0F" box="[421,508,439,465]" class="Magnoliopsida" family="Asteraceae" genus="Carlina" kingdom="Plantae" order="Asterales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="genus">
