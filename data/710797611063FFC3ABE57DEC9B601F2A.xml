<taxonomicName id="3EAE5DF41063FFC4ABE57DEC98F41B1F" authority="BREVICERA HULL, 1946: 7" authorityName="BREVICERA HULL" authorityPageNumber="7" authorityYear="1946" box="[162,670,1272,1294]" class="Insecta" family="Syrphidae" genus="Quichuana" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="83" phylum="Arthropoda" rank="species" species="inca">
<emphasis id="CBDAFA651063FFC4ABE57DEC9B841B1E" box="[162,494,1272,1293]" italics="true" pageId="11" pageNumber="83">QUICHUANA INCA BREVICERA</emphasis>
<bibRefCitation id="9D3F5B861063FFC4AABE7DEC98F41B1F" author="Hull FM" box="[505,670,1272,1294]" pageId="11" pageNumber="83" pagination="1 - 17" refId="ref46526" refString="Hull FM. 1946. The genus Quichuana Knab. American Museum Novitates 1317: 1 - 17." type="journal article" year="1946">HULL, 1946: 7</bibRefCitation>
