<taxonomicName id="4C364D345464B515FCF136FB5C0E02DC" authorityName="Emeljanov" authorityYear="2000" box="[842,1091,1942,1968]" class="Insecta" family="Cixiidae" genus="Monomalpha" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="378" phylum="Arthropoda" rank="species" species="fletcheri">
