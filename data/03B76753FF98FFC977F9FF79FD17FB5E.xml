<emphasis id="B96A0A57FF98FFC977F9FF30FEC2FF3F" box="[151,381,222,244]" italics="true" pageId="45" pageNumber="46">Polyrhachis cubaensis</emphasis>
</taxonomicName>
subsp. 
<taxonomicName id="4C1EADC6FF98FFC976A2FF30FD7BFF3F" authority="Forel, 1910b: 30" authorityName="Forel" authorityPageNumber="30" authorityYear="1910" box="[460,708,222,244]" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" order="Hymenoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="wilmsi">
