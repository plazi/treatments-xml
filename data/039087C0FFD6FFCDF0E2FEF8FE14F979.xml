. 
<taxonomicName id="4C394D55FFD6FFCEF14AF83AFDA7F85E" authority="Olmi &amp; Marletta &amp; Guglielmino &amp; Speranza, 2016" authorityName="Olmi &amp; Marletta &amp; Guglielmino &amp; Speranza" authorityYear="2016" box="[319,588,1945,1967]" class="Insecta" family="Sclerogibbidae" genus="Protosclerogibba" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="128" phylum="Arthropoda" rank="species" species="australis" status="sp. nov.">
