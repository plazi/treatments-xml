<taxonomicName id="71E84D35174E4E14FB98FDDFFB64FDF4" baseAuthorityName="Kieffer" baseAuthorityYear="1911" box="[1115,1268,512,536]" class="Insecta" family="Curculionidae" genus="Aulacus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="albimanus">
