<quantity id="4C849B579778FFE5FBFFDADBA94FE4C3" box="[1043,1114,143,167]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="16" pageNumber="1016" unit="m" value="600.0">600 m</quantity>
alt.
