<location id="8EDA602FFFC2FFB4FB85F9F53A47F900" LSID="urn:lsid:plazi:treatment:03AC87E2FFC2FFB7FD19FC5E3B3BFDEF:8EDA602FFFC2FFB4FB85F9F53A47F900" county="Mathews" latitude="-17.1" longLatPrecision="767" longitude="145.52" municipality="North" name="Mount Sapphiri" pageId="121" pageNumber="121" stateProvince="Queensland">Mount Sapphiri</location>
(
<emphasis id="B971EAE6FFC2FFB4FCB5F9173A06F900" bold="true" box="[842,865,1680,1700]" italics="true" pageId="121" pageNumber="121">5</emphasis>
