<location id="8EDA602FFFC2FFB4FCEFFA593A2AFA50" LSID="urn:lsid:plazi:treatment:03AC87E2FFC2FFB7FD19FC5E3B3BFDEF:8EDA602FFFC2FFB4FCEFFA593A2AFA50" box="[784,845,1502,1524]" county="Mathews" latitude="-17.1" longLatPrecision="767" longitude="145.52" municipality="North" name="Olive" pageId="121" pageNumber="121" stateProvince="Queensland">Olive</location>
