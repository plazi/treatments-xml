), 
<geoCoordinate id="EE315033FFC2FFB4FB88F9083DA0F900" box="[1143,1223,1679,1700]" degrees="17.10" direction="south" orientation="latitude" pageId="121" pageNumber="121" precision="555" value="-17.1">17.10S</geoCoordinate>
, 
<geoCoordinate id="EE315033FFC2FFB4FD42F92B3A7CF966" box="[701,795,1708,1730]" degrees="145.52" direction="east" orientation="longitude" pageId="121" pageNumber="121" precision="555" value="145.52">145.52E</geoCoordinate>
(
<collectionCode id="ED14AE31FFC2FFB4FCCEF92B3AFDF965" box="[817,922,1708,1729]" pageId="121" pageNumber="121">USBGN</collectionCode>
