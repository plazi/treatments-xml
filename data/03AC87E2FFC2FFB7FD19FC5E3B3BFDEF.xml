<location id="8EDA602FFFC2FFB4FD37F99E3AF8F98B" LSID="urn:lsid:plazi:treatment:03AC87E2FFC2FFB7FD19FC5E3B3BFDEF:8EDA602FFFC2FFB4FD37F99E3AF8F98B" box="[712,927,1561,1583]" county="Mathews" latitude="-17.1" longLatPrecision="767" longitude="145.52" municipality="North" name="Johnstone River" pageId="121" pageNumber="121" stateProvince="Queensland">Johnstone River</location>
.’’ 
<location id="8EDA602FFFC2FFB4FC3DF99E3AB1F98A" LSID="urn:lsid:plazi:treatment:03AC87E2FFC2FFB7FD19FC5E3B3BFDEF:8EDA602FFFC2FFB4FC3DF99E3AB1F98A" box="[962,982,1561,1582]" county="Mathews" latitude="-17.1" longLatPrecision="767" longitude="145.52" municipality="North" name="It" pageId="121" pageNumber="121" stateProvince="Queensland">It</location>
