<taxonomicName id="4C0974AE644FFFFAFE13FEA1FD445625" authorityName=": Ellerman &amp; Morrison-Scott" authorityYear="1951" box="[459,645,310,335]" class="Mammalia" family="Mustelidae" genus="Martes" kingdom="Animalia" order="Carnivora" pageId="69" pageNumber="70" phylum="Chordata" rank="species" species="flavigula">
