<paragraph id="8BD17E1DC0241D64FF6CAAC8FCD094EC" blockId="16.[189,887,960,986]" box="[189,887,960,986]" pageId="16" pageNumber="17">
<heading id="D099C971C0241D64FF6CAAC8FCD094EC" bold="true" box="[189,887,960,986]" fontSize="11" level="3" pageId="16" pageNumber="17" reason="3">
