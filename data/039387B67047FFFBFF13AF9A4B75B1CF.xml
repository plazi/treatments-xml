<subSubSection id="C320652B7047FFFBFF13AFC24BBEB5E5" box="[151,944,980,1005]" pageId="1" pageNumber="582" type="materials_examined">
<paragraph id="8B8536A07047FFFBFF13AFC24BBEB5E5" blockId="1.[151,1437,980,1991]" box="[151,944,980,1005]" pageId="1" pageNumber="582">
