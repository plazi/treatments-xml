<collectingRegion id="8A658FC5FFC9577AFAE1E2B7FEBFCD98" country="United States of America" name="California" pageId="14" pageNumber="13">California</collectingRegion>
, 
<collectingRegion id="8A658FC5FFC9577AFE93E297FEC4CD98" box="[279,374,674,698]" country="United States of America" name="Nevada" pageId="14" pageNumber="13">Nevada</collectingRegion>
