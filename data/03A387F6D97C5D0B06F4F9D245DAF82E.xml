<taxonomicName id="4C0A4D63D97C5D0B059AF8E7405CF8FB" box="[1017,1143,1795,1819]" class="Insecta" family="Miridae" genus="Auricillocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="coonoor">
