<subSubSection id="07DF35BFB907FFBCFF5EFC37FB051131" pageId="11" pageNumber="39" type="materials_examined">
<paragraph id="4F7A6634B907FFBCFF5EFC37FBCB16C4" blockId="11.[151,1437,965,1476]" box="[151,1044,965,991]" pageId="11" pageNumber="39">
<emphasis id="7DB1BA26B907FFBCFF5EFC37FE9416C4" bold="true" box="[151,331,965,991]" pageId="11" pageNumber="39">
<typeStatus id="907ED896B907FFBCFF5EFC37FF0D16C4" box="[151,210,965,991]" pageId="11" pageNumber="39">Type</typeStatus>
