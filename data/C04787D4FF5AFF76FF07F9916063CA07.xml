<taxonomicName id="8FEE4D41FF5AFF71FD09F94D6105CC43" authorityName="Hoogstraal, Trapido &amp; Kohls" authorityYear="1965" box="[665,1094,1770,1796]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="179" pageNumber="180" phylum="Arthropoda" rank="subSpecies" species="papuana" subSpecies="nadchatrami">
