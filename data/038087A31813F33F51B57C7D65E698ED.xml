<taxonomicName id="4C294D361813F33F51B57C7D631F9BE4" authority="Chace, 1942" authorityName="Chace" authorityYear="1942" box="[151,588,1396,1422]" class="Malacostraca" family="Chirostylidae" genus="Gastroptychus" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="385" phylum="Arthropoda" rank="species" species="affinis">
