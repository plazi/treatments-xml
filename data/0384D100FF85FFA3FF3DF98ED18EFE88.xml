</taxonomicName>
b and 
<taxonomicName id="4C2D1B95FF87FFA2FD3FF962D1EDF8B1" authority="Harris et al. 2002" authorityName="Harris et al." authorityYear="2002" box="[661,1005,1769,1792]" class="Insecta" family="Hydroptilidae" genus="Flintiella" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="67" phylum="Arthropoda" rank="species" species="yanamona">
<emphasis id="B959BC04FF87FFA2FD3FF962D0A3F8B1" box="[661,675,1769,1792]" italics="true" pageId="2" pageNumber="67">F</emphasis>
. 
<emphasis id="B959BC04FF87FFA2FD18F962D122F8B1" box="[690,802,1769,1792]" italics="true" pageId="2" pageNumber="67">yanamona</emphasis>
<bibRefCitation id="EFBC1DE7FF87FFA2FC80F962D1EDF8B1" author="Harris" box="[810,1005,1769,1792]" pageId="2" pageNumber="68" refString="Harris, S. C.; Flint, O. S., Jr. &amp; Holzenthal, R. W. (2002 b) Review of the Neotropical genus Flintiella (Trichoptera: Hydroptilidae: Stactobiini). Journal of the New York Entomological Society 110, 65 - 90." type="journal article" year="2002" yearSuffix="b">
