<taxonomicName id="4C39CEFBFFAAD668E601F903FC63F95F" authorityName=", Shewell" authorityYear="1953" box="[807,929,1737,1762]" class="Insecta" family="Agromyzidae" genus="Melanagromyza" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="157" phylum="Arthropoda" rank="species" species="minima">
