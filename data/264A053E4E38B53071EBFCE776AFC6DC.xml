<paragraph id="AE5CB4284E38B5307179FA0776AFC6DC" blockId="20.[113,646,1484,1701]" pageId="20" pageNumber="519">
<taxonomicName id="69E3CFAB4E38B5307179FA077656C67C" authorityName="Chen" authorityYear="1993" box="[113,286,1516,1541]" class="Malacostraca" family="Ethusidae" genus="Ethusa" kingdom="Animalia" order="Decapoda" pageId="20" pageNumber="519" phylum="Arthropoda" rank="species" species="curvipes">
