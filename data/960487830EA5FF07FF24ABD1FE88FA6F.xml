<taxonomicName id="D9AD4D160EA5FF07FEA1AAA4FE7EFE29" authority="Gerecke, 2003" authorityName="Gerecke" authorityYear="2003" box="[296,387,436,452]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="158" pageNumber="299" phylum="Arthropoda" rank="species" species="stygophilus" status="ssp.n.">stygophilus</taxonomicName>
