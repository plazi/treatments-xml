<taxonomicName id="4C574D75F84AFF9FFBC9176AFAC16F4E" box="[1124,1328,1304,1328]" class="Insecta" family="Notonectidae" genus="Martarega" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="419" phylum="Arthropoda" rank="species" species="oriximinaensis">
