<taxonomicName id="4C574D75F84AFF9FFA901686FA6D6F6D" box="[1341,1436,1268,1292]" class="Insecta" family="Notonectidae" genus="Martarega" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="419" phylum="Arthropoda" rank="species" species="siolii">
