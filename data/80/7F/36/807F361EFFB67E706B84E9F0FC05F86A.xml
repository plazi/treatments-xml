<subSubSection pageId="2" pageNumber="3" type="etymology">
<paragraph blockId="2.[151,1436,866,1932]" pageId="2" pageNumber="3">
<emphasis bold="true" box="[198,339,1746,1772]" pageId="2" pageNumber="3">Etymology.</emphasis>
The specific designation 
<taxonomicName box="[653,835,1747,1772]" class="Arachnida" family="Eriophyidae" genus="Epitrimerus" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="dendranthemae">
<emphasis box="[653,835,1747,1772]" italics="true" pageId="2" pageNumber="3">dendranthemae</emphasis>
</taxonomicName>
