<number box="[1226,1255,1266,1292]" pageId="5" pageNumber="6" value="50.0">50</number>
(
<number box="[1274,1350,1266,1292]" pageId="5" pageNumber="6" value="50.0" valueMax="52.0" valueMin="48.0">48–52</number>
