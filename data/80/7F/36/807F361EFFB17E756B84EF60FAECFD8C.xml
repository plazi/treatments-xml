<number box="[981,1010,1346,1372]" pageId="5" pageNumber="6" value="10.0">10</number>
(
<number box="[1029,1105,1346,1372]" pageId="5" pageNumber="6" value="10.5" valueMax="11.0" valueMin="10.0">10–11</number>
