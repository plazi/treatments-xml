<number box="[911,940,1426,1452]" pageId="5" pageNumber="6" value="20.0">20</number>
(
<number box="[957,1032,1426,1452]" pageId="5" pageNumber="6" value="20.0" valueMax="21.0" valueMin="19.0">19–21</number>
