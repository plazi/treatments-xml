) 
<number box="[1259,1274,1306,1332]" pageId="5" pageNumber="6" value="2.0">2</number>
(
<number box="[1293,1340,1306,1332]" pageId="5" pageNumber="6" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis box="[427,442,1347,1372]" italics="true" pageId="5" pageNumber="6">d</emphasis>
) 
<number box="[460,475,1346,1372]" pageId="5" pageNumber="6" value="6.0">6</number>
(
<number box="[494,541,1346,1372]" pageId="5" pageNumber="6" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
