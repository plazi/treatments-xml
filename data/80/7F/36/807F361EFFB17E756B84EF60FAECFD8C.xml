<number box="[1009,1038,1266,1292]" pageId="5" pageNumber="6" value="55.0">55</number>
(
<number box="[1057,1134,1266,1292]" pageId="5" pageNumber="6" value="54.5" valueMax="57.0" valueMin="52.0">52–57</number>
