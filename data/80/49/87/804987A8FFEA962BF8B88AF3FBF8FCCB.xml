<emphasis bold="true" box="[198,619,732,756]" pageId="1" pageNumber="68">Summary of nomenclatural changes</emphasis>
: 
<taxonomicName box="[637,718,733,756]" class="Insecta" family="Gryllidae" genus="Gyabus" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="68" phylum="Arthropoda" rank="genus">
<emphasis box="[637,718,733,756]" italics="true" pageId="1" pageNumber="68">Gyabus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[728,842,732,756]" pageId="1" pageNumber="68">nom. nov.</emphasis>
pro 
<taxonomicName authority="Bei-Bienko, 1949" authorityName="Bei-Bienko" authorityYear="1949" box="[908,1253,733,756]" class="Insecta" family="Curculionidae" genus="Pachymastax" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="68" phylum="Arthropoda" rank="genus">
<emphasis box="[908,1050,733,756]" italics="true" pageId="1" pageNumber="68">Pachymastax</emphasis>
<bibRefCitation author="Bei-Bienko" box="[1060,1253,733,756]" pageId="1" pageNumber="68" refString="Bei-Bienko, G. Ya. (1949) Revision of Acrididae subfam. Gomphomastacinae (Orthoptera: Eumastacidae). (in Russian). Comptes rendus de l'Acad e mie des Sciences de l'URSS, 64 (5), 731 - 734." type="journal article" year="1949">
Bei-Bienko, 
<number box="[1200,1253,733,756]" pageId="1" pageNumber="68" value="1949.0">1949</number>
</bibRefCitation>
</taxonomicName>
(non 
<bibRefCitation author="Wollaston" pageId="1" pageNumber="68" refString="Wollaston, T. V. (1877) Coleoptera Sanctae-Helenae. J. van Voorst. London, 256 pp." type="book" year="1877">
Wollaston, 
<number box="[151,208,768,791]" pageId="1" pageNumber="68" value="1877.0">1877</number>
</bibRefCitation>
); 
<taxonomicName authority="Bei-Bienko, 1949" authorityName="Bei-Bienko" authorityYear="1949" box="[227,637,768,791]" class="Insecta" family="Gryllidae" genus="Gyabus" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="68" phylum="Arthropoda" rank="species" species="fusiformis" status="comb. nov.">
<emphasis box="[227,422,768,791]" italics="true" pageId="1" pageNumber="68">Gyabus fusiformis</emphasis>
(
<bibRefCitation author="Bei-Bienko" box="[436,629,768,791]" pageId="1" pageNumber="68" refString="Bei-Bienko, G. Ya. (1949) Revision of Acrididae subfam. Gomphomastacinae (Orthoptera: Eumastacidae). (in Russian). Comptes rendus de l'Acad e mie des Sciences de l'URSS, 64 (5), 731 - 734." type="journal article" year="1949">
Bei-Bienko, 
<number box="[574,629,768,791]" pageId="1" pageNumber="68" value="1949.0">1949</number>
</bibRefCitation>
)
</taxonomicName>
