<taxonomicName class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis aenea" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="aenea">
