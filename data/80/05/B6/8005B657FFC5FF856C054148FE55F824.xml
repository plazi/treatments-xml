<emphasis bold="true" box="[189,426,837,863]" pageId="6" pageNumber="7">Material examined</emphasis>
</heading>
</paragraph>
<paragraph blockId="6.[189,1399,884,1051]" box="[234,348,884,910]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[234,348,884,910]" pageId="6" pageNumber="7">
<typeStatus box="[234,348,884,910]" pageId="6" pageNumber="7">Holotype</typeStatus>
</emphasis>
</paragraph>
<paragraph blockId="6.[189,1399,884,1051]" pageId="6" pageNumber="7">
<materialsCitation ID-GBIF-Occurrence="2628167303" collectionCode="BUB" country="Myanmar" location="Kachin State" pageId="6" pageNumber="7" specimenCode="BUB 2885, ZFMK-TRI000813" specimenCount="1" stateProvince="Kachin State" typeStatus="holotype">
<collectingCountry box="[189,342,920,946]" name="Myanmar" pageId="6" pageNumber="7">MYANMAR</collectingCountry>
– 
<emphasis bold="true" box="[373,554,919,946]" pageId="6" pageNumber="7">
<collectingRegion box="[373,536,919,945]" country="Myanmar" name="Kachin" pageId="6" pageNumber="7">Kachin State</collectingRegion>
•
</emphasis>
<specimenCount box="[562,586,920,946]" pageId="6" pageNumber="7" type="male">♂</specimenCount>
; exact locality unknown; Mid-Cretaceous Burmese amber inclusion; former Patrick Müller collection, 
<specimenCode box="[598,731,954,981]" pageId="6" pageNumber="7">BUB 2885</specimenCode>
; 
