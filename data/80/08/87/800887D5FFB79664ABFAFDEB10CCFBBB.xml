<paragraph blockId="5.[151,1437,151,1112]" pageId="5" pageNumber="344">
<emphasis box="[199,361,153,176]" italics="true" pageId="5" pageNumber="344">Measurements</emphasis>
. Length 
<quantity box="[475,572,151,176]" metricMagnitude="-3" metricUnit="m" metricValue="3.15" pageId="5" pageNumber="344" unit="mm" value="3.15">3.15mm</quantity>
; length / width of head 0.15/ 
<quantity box="[946,1042,151,176]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="5" pageNumber="344" unit="mm" value="0.55">0.55mm</quantity>
; pronotum length / width across anterolateral expansions 0.65/ 
<quantity box="[488,582,188,213]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="5" pageNumber="344" unit="mm" value="1.2">1.20mm</quantity>
; scutellum length / width 0.3/ 
<quantity box="[915,997,188,212]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="5" pageNumber="344" unit="mm" value="0.4">0.4mm</quantity>
