<collectorName box="[452,635,796,818]" pageId="11" pageNumber="218">W. D. McLellen</collectorName>
: 
<collectorName box="[650,703,796,818]" pageId="11" pageNumber="218">Host</collectorName>
: 
<collectorName box="[720,811,796,818]" pageId="11" pageNumber="218">Creosote</collectorName>
bush (
<specimenCount box="[895,967,796,818]" pageId="11" pageNumber="218" type="male">1 male</specimenCount>
) (
