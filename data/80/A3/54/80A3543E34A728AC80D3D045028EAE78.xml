and 
<taxonomicName class="Insecta" family="Psychidae" genus="Bankesia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bankesia" order="Lepidoptera" pageId="1" pageNumber="32" phylum="Arthropoda" rank="genus">Bankesia</taxonomicName>
