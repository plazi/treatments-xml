<quantity box="[381,489,1871,1895]" metricMagnitude="-3" metricUnit="m" metricValue="6.55" metricValueMax="8.0" metricValueMin="5.1" pageId="9" pageNumber="202" unit="mm" value="6.55" valueMax="8.0" valueMin="5.1">5,1 à 8 mm</quantity>
