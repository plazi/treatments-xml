<quantity box="[527,666,1875,1899]" metricMagnitude="-3" metricUnit="m" metricValue="3.05" metricValueMax="3.2" metricValueMin="2.9" pageId="8" pageNumber="199" unit="mm" value="3.05" valueMax="3.2" valueMin="2.9">2.9–3.2 mm</quantity>
