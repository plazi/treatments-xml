(
<typeStatus box="[543,641,219,243]" pageId="9" pageNumber="442">holotype</typeStatus>
<quantity box="[651,742,220,244]" metricMagnitude="-3" metricUnit="m" metricValue="3.8" pageId="9" pageNumber="442" unit="mm" value="3.8">3.8 mm</quantity>
), width 
<quantity box="[838,973,220,244]" metricMagnitude="-3" metricUnit="m" metricValue="1.35" metricValueMax="1.4" metricValueMin="1.3" pageId="9" pageNumber="442" unit="mm" value="1.35" valueMax="1.4" valueMin="1.3">1.3–1.4 mm</quantity>
(
<typeStatus box="[991,1088,219,243]" pageId="9" pageNumber="442">holotype</typeStatus>
<quantity box="[113,199,252,276]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="9" pageNumber="442" unit="mm" value="1.4">1.4 mm</quantity>
).
</paragraph>
</subSubSection>
