1 21
</date>
(19–21), 
<date box="[637,726,413,441]" pageId="2" pageNumber="395" value="1922-05-02" valueMax="1922-05-02" valueMin="1922-05-02">
<emphasis box="[637,649,413,436]" italics="true" pageId="2" pageNumber="395">v</emphasis>
2 
<emphasis box="[657,683,413,436]" italics="true" pageId="2" pageNumber="395">–v</emphasis>
2 22
</date>
(22–23), 
<emphasis box="[836,859,413,436]" italics="true" pageId="2" pageNumber="395">sc</emphasis>
2 
<emphasis box="[867,904,413,436]" italics="true" pageId="2" pageNumber="395">–sc</emphasis>
2 45 (43–46), 
<emphasis box="[1057,1069,413,436]" italics="true" pageId="2" pageNumber="395">c</emphasis>
1– 
<emphasis box="[1091,1103,413,436]" italics="true" pageId="2" pageNumber="395">c</emphasis>
1 19 (19–21), 
