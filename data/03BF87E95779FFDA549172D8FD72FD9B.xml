: 
<taxonomicName id="4C164D7C5776FFDB552070FBFE4CFC1E" authorityName="Kratochwil &amp; Scheuchl" authorityYear="2013" box="[259,428,907,928]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="345" phylum="Arthropoda" rank="species" species="dourada">
