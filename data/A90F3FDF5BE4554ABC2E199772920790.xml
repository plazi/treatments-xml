<emphasis id="2F598E5AD5AE1ABA2772EB42EEE17393" italics="true" pageId="0" pageNumber="199">A. copaiba</emphasis>
</taxonomicName>
sp. nov. was reared 
<emphasis id="C6AB738BADBFFAB556DB8C851E258071" bold="true" pageId="0" pageNumber="199">B</emphasis>
<taxonomicName id="08E0B42D9E3DD8DBE890404AD78AF4F5" genus="A." lsidName="A. ilexaffinis" pageId="0" pageNumber="199" rank="species" species="ilexaffinis">
<emphasis id="0661CCC51FF1E98F6FAE1DAFE13B70E7" italics="true" pageId="0" pageNumber="199">A. ilexaffinis</emphasis>
</taxonomicName>
sp. nov., head, frontal view 
<emphasis id="641E3B70EACD8E17B797AC262402AE31" bold="true" pageId="0" pageNumber="199">C</emphasis>
<taxonomicName id="D6D4F0F9EB92757A86E97B1206B31562" genus="A." lsidName="A. quarentenus" pageId="0" pageNumber="199" rank="species" species="quarentenus">
<emphasis id="4542432CF6F7DE04DE806740E87917E1" italics="true" pageId="0" pageNumber="199">A. quarentenus</emphasis>
</taxonomicName>
sp. nov., head, frontal view 
<emphasis id="A3B845F7FA83934FD8D60A614B3D984D" bold="true" pageId="0" pageNumber="199">D</emphasis>
<taxonomicName id="EBC5D8F840493674740FA0D48F261CAA" genus="A." lsidName="A. inquilinus" pageId="0" pageNumber="199" rank="species" species="inquilinus">
<emphasis id="95760C56502A47243249A61879422D2A" italics="true" pageId="0" pageNumber="199">A. inquilinus</emphasis>
</taxonomicName>
sp. nov., head, frontal view 
<emphasis id="7633E8DD04CCE11EEAA368CAB2FEB467" bold="true" pageId="0" pageNumber="199">E</emphasis>
<taxonomicName id="B66E8C924BF58A2E9228B5EB6BC5C9F5" genus="A." lsidName="A. vassununga" pageId="0" pageNumber="199" rank="species" species="vassununga">
<emphasis id="DAC3354419CAD3ED913958D7B955E1C9" italics="true" pageId="0" pageNumber="199">A. vassununga</emphasis>
