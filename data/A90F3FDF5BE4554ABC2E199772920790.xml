<taxonomicName id="2A57AD08073063C463F6CD96A336545A" authorityName="Linnaeus" authorityYear="1762" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Fabales" pageId="0" pageNumber="199" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
