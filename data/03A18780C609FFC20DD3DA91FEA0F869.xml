<quantity id="4CF09B73C609FFC20C44DE83FED6F804" box="[256,342,1950,1975]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="65" pageNumber="66" unit="m" value="1000.0">1000 m</quantity>
