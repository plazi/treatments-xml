<taxonomicName id="9FDF4D173E2AFFCEF685FB38DC8AFAC4" authority="Greenbaum &amp; Villanueva &amp; Kusamba &amp; Aristote &amp; Branch, 2011" authorityName="Greenbaum &amp; Villanueva &amp; Kusamba &amp; Aristote &amp; Branch" authorityYear="2011" box="[917,1181,1262,1283]" class="Reptilia" family="Lacertidae" genus="Congolacerta" kingdom="Animalia" order="Squamata" pageId="14" pageNumber="927" phylum="Chordata" rank="species" species="asukului" status="sp. nov.">Congolacerta asukului</taxonomicName>
</emphasis>
<taxonomicNameLabel id="719857FD3E2AFFCEF1BDFB39DD1DFAC3" box="[1197,1290,1263,1284]" pageId="14" pageNumber="927" rank="species">sp. nov.</taxonomicNameLabel>
(described below); 
<taxonomicName id="9FDF4D173E2AFFCEF690FADBDD59FAE4" authority="(Tornier, 1902)" baseAuthorityName="Tornier" baseAuthorityYear="1902" box="[896,1358,1293,1315]" class="Reptilia" family="Lacertidae" genus="Congolacerta" kingdom="Animalia" order="Squamata" pageId="14" pageNumber="927" phylum="Chordata" rank="species" species="vauereselli">
