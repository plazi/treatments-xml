<taxonomicName id="FAF04D54FF89FFE4FC5A9D06FB9ADB31" authorityName="Lamarck" authorityYear="1801" box="[1021,1106,861,884]" class="Anthozoa" family="Agariciidae" genus="Pavona" kingdom="Animalia" order="Scleractinia" pageId="111" pageNumber="112" phylum="Cnidaria" rank="genus">
