<taxonomicName id="6ED2019AF61AFFADFE26A0A1DB07FE7F" authority="Topsent, 1898: 112" authorityName="Topsent" authorityPageNumber="112" authorityYear="1898" box="[446,827,444,470]" class="Demospongiae" family="Tethyidae" genus="Halicometes" kingdom="Animalia" order="Hadromerida" pageId="2" phylum="Porifera" rank="genus">
