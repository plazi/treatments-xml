<taxonomicName id="4C0F22D9FF95FFDFEEDF43DBB06DF877" authority="Cramer, 1779" authorityName="Cramer" authorityYear="1779" box="[710,1070,1942,1967]" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="520" phylum="Arthropoda" rank="species" species="micilia">
