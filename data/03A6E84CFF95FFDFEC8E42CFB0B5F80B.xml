<taxonomicName id="4C0F22D9FF95FFDFE8F443DAB1DBF877" authority="Hubner" authorityName="Hubner" box="[1261,1432,1942,1967]" class="Insecta" family="Arctiidae" genus="Agyrta" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="520" phylum="Arthropoda" rank="genus">
