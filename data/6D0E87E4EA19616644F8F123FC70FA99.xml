<taxonomicName id="22A74D71EA19616644F8F1C1FEBFFADC" baseAuthorityName="BENICK" baseAuthorityYear="1970" box="[178,341,1271,1293]" class="Insecta" family="Staphylinidae" genus="Geostiba" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="1215" phylum="Arthropoda" rank="species" species="lucens">
