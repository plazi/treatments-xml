sp. n., 
<taxonomicName id="9F17C2C47A1BDF86963F84EC3A7030B1" genus="L." lsidName="L. lattuensis" pageId="2" pageNumber="19" rank="species" species="lattuensis">L. lattuensis</taxonomicName>
