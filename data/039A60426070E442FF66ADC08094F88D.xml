and 
<taxonomicName id="4C33AAD76077E442FC66A81D86E1F9E8" authority="Lambkin" authorityName="Lambkin" box="[919,1257,1545,1569]" class="Insecta" family="Therevidae" genus="Collessiama" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Diptera" pageId="4" pageNumber="100" phylum="Arthropoda" rank="species" species="damani" status="sp. nov.">
<emphasis id="B9470D466077E442FC66A81D8675F9E8" box="[919,1149,1545,1569]" italics="true" pageId="4" pageNumber="100">Collessiama damani</emphasis>
Lambkin
