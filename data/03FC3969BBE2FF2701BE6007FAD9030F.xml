<quantity id="4CAD259ABBE3FF22028D61DBFC2A0565" box="[932,1022,1782,1807]" metricMagnitude="3" metricUnit="m" metricValue="1.25" pageId="153" pageNumber="154" unit="m" value="1250.0">1250 m</quantity>
