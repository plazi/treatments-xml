<collectionCode id="ED2176C0405EFFD8E0F807443438FAC1" LSID="urn:lsid:biocol.org:col:35271" box="[701,786,1359,1383]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35271" name="Royal Belgian Institute of Natural Sciences" pageId="5" pageNumber="156">RBINS</collectionCode>
, No. IG 
<number id="3DF8FC22405EFFD8E13407443490FAC1" box="[881,954,1359,1383]" pageId="5" pageNumber="156" value="33082.0">33082</number>
, RIR 
<number id="3DF8FC22405EFFD8E1BC0745330DFAC1" box="[1017,1063,1358,1383]" pageId="5" pageNumber="156" value="246.0">246</number>
; 
<number id="3DF8FC22405EFFD8E67407443315FAC0" box="[1073,1087,1359,1382]" pageId="5" pageNumber="156" value="7.0">7</number>
