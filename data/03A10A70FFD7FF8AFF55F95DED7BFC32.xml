(23–25); 
<geoCoordinate id="EE3CDDA1FFD4FF8AFE68FEDAEDD0FE9E" box="[426,491,297,323]" direction="south" orientation="latitude" pageId="15" pageNumber="257" precision="925" value="-2.45">
