<quantity id="FC2D019DFFB34846AFADF99CFDF69F5A" box="[456,570,1567,1592]" metricMagnitude="-2" metricUnit="m" metricValue="9.270999999999999" metricValueMax="9.652" metricValueMin="8.89" pageId="27" pageNumber="428" unit="in" value="3.65" valueMax="3.8" valueMin="3.5">3.5–3.8 in</quantity>
