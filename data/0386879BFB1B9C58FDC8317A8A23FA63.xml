<taxonomicName id="4C2F4D0EFB1B9C67FDC8317A8AD8FAB7" authorityName="Matthew" authorityYear="1899" box="[580,708,1297,1323]" class="Mammalia" family="Leptictidae" genus="Palaeictops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pilosa" pageId="31" pageNumber="32" phylum="Chordata" rank="genus">
