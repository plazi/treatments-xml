<taxonomicName id="4C514D4BFFE5FFAA9E0A0EFED5FDFE06" box="[587,708,439,464]" class="Hydrozoa" family="Hydractiniidae" genus="Stylactaria" kingdom="Animalia" order="Anthoathecata" pageId="19" pageNumber="20" phylum="Cnidaria" rank="genus">
