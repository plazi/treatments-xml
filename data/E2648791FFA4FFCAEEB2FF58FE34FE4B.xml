<taxonomicName id="ADCD4D04FFA4FFCAEF72FEF4FD50FEFE" authority="Melander, 1903" authorityName="Melander" authorityYear="1903" box="[343,685,332,359]" class="Insecta" family="Mutillidae" genus="Mutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="583" phylum="Arthropoda" rank="species" species="impar">
