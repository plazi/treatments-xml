<paragraph id="77031116F205FE97FABE57D1612E04AB" pageId="0" pageNumber="309">
<taxonomicName id="BDE8D8F97FFA3E17BB4E82182B835E7A" authorityName="Zhou" authorityYear="2006" class="Insecta" family="Panorpidae" genus="Panorpa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Panorpa stigmosa" order="Mecoptera" pageId="0" pageNumber="309" phylum="Arthropoda" rank="species" species="stigmosa">Panorpa stigmosa</taxonomicName>
