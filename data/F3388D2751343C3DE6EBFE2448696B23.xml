<quantity id="BC6991D451343C3DE42FFD1D48D36DFE" box="[595,697,654,679]" metricMagnitude="-4" metricUnit="m" metricValue="5.8999999999999995" pageId="24" pageNumber="175" unit="mm" value="0.59">0.59 mm</quantity>
