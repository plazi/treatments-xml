<quantity id="BC6991D451343C3DE7DCFD1D486D6DFE" box="[416,519,654,679]" metricMagnitude="-4" metricUnit="m" metricValue="5.1" pageId="24" pageNumber="175" unit="mm" value="0.51">0.51 mm</quantity>
