wide, 
<quantity id="BC6991D451343C3DE4C3FDF9494F6DDA" box="[703,805,618,643]" metricMagnitude="-4" metricUnit="m" metricValue="5.8999999999999995" pageId="24" pageNumber="175" unit="mm" value="0.59">0.59 mm</quantity>
