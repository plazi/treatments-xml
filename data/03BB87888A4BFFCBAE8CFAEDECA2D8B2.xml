<taxonomicName id="4C124D1D8A4BFFCDAE8CFAEDEA3ADBED" authority="(Spinola)" baseAuthorityName="Spinola" baseAuthorityYear="1851" box="[213,560,1403,1426]" class="Insecta" family="Apidae" genus="Isepeolus" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="luctuosus">
