<taxonomicName id="F3962B00A29713700A792098589468A9" class="Magnoliopsida" family="Cannabaceae" genus="Celtis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Celtis iguanaea" order="Rosales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="iguanaea">
