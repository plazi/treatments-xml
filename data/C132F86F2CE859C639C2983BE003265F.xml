<taxonomicName id="B6A0F566088575C220209536B1D275B2" LSID="http://zoobank.org/D6CCF6F9-9452-4074-A71C-B8891F894008" authority="Girón &amp; Short, 2018" authorityName="Girón &amp; Short" authorityYear="2018" class="Insecta" family="Hydrophilidae" genus="Crucisternum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crucisternum toboganensis" order="Coleoptera" pageId="7" pageNumber="120" phylum="Arthropoda" rank="species" species="toboganensis" status="sp. n.">Crucisternum toboganensis</taxonomicName>
