<taxonomicName id="608CF09ED236FFB18398FAF8AC11FA97" authorityName="Mayr" authorityYear="1879" box="[665,787,1381,1406]" class="Insecta" family="Formicidae" genus="Lioponera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
