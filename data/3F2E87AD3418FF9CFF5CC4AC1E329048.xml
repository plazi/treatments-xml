<quantity id="707F9B5E3418FF9CFD28C4FE18D693F2" box="[710,766,460,482]" metricMagnitude="4" metricUnit="m" metricValue="2.2" pageId="3" pageNumber="272" unit="km" value="22.0">22 km</quantity>
