<subSubSection id="C331F4FEFFACFFF4FF35372AFB8FA69B" lastPageId="6" lastPageNumber="7" pageId="5" pageNumber="6" type="distribution">
<paragraph id="8B94A775FFACFFF7FF35372AFCBDA013" blockId="5.[151,1436,1767,2009]" box="[199,816,1839,1864]" pageId="5" pageNumber="6">
<emphasis id="B95F7B67FFACFFF7FF35372AFEEDA01C" bold="true" box="[199,352,1839,1864]" pageId="5" pageNumber="6">Distribution.</emphasis>
<collectingCountry id="F33CE7E5FFACFFF7FE95372AFE27A013" box="[359,426,1839,1863]" name="China" pageId="5" pageNumber="6">China</collectingCountry>
(Gansu) (
<figureCitation id="1310BBF0FFACFFF7FDE8372AFDF0A013" box="[538,637,1839,1863]" captionStart="FIGURE 108" captionStartId="36.[151,250,1975,1998]" captionTargetBox="[216,1367,1120,1943]" captionTargetId="figure@36.[191,1396,1118,1959]" captionTargetPageId="36" captionText="FIGURE 108. Geographical distribution of species of P. pallidistigma group in China (2)" httpUri="https://zenodo.org/record/280494/files/figure.png" pageId="5" pageNumber="6">
Fig. 
<number id="3DE3B552FFACFFF7FDBD372AFDF0A013" box="[591,637,1839,1863]" pageId="5" pageNumber="6" value="108.0">108</number>
</figureCitation>
); 
<collectingCountry id="F33CE7E5FFACFFF7FD633735FD59A013" box="[657,724,1840,1863]" name="Japan" pageId="5" pageNumber="6">Japan</collectingCountry>
, 
<collectingCountry id="F33CE7E5FFACFFF7FD2C372AFCA5A013" box="[734,808,1839,1863]" name="Russia" pageId="5" pageNumber="6">Russia</collectingCountry>
