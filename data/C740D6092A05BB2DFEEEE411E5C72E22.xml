<collectingRegion id="8D2DA9FD2A0ABB2DFF1CE4FFE5F12DBA" box="[190,413,385,409]" country="Brazil" name="Rio de Janeiro" pageId="104" pageNumber="2334">Rio de Janeiro State</collectingRegion>
