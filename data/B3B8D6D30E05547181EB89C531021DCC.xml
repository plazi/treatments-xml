<taxonomicName id="320847BA588275B9E0641E396B4B3DA5" class="Magnoliopsida" family="Asteraceae" genus="Adenocaulon" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Adenocaulon bicolor" order="Asterales" pageId="1" pageNumber="122" phylum="Tracheophyta" rank="species" species="bicolor">Adenocaulon bicolor</taxonomicName>
