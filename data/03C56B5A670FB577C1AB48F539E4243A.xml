<taxonomicName id="4C6CA1CF670FB577C1A24E233906243A" baseAuthorityName="Ter-Minasian" baseAuthorityYear="1967" box="[158,284,1363,1387]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="346" pageNumber="347" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
