<taxonomicName id="4C6CA1CF670FB577C2C54E5F3C4D2417" box="[1017,1111,1327,1350]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="346" pageNumber="347" phylum="Tracheophyta" rank="species" species="jacea">
<emphasis id="B918065E670FB577C2C54E5F3C4D2417" box="[1017,1111,1327,1350]" italics="true" pageId="346" pageNumber="347">C. jacea</emphasis>
</taxonomicName>
and other 
<taxonomicName id="4C6CA1CF670FB577C5F24E5F3D592417" baseAuthorityName="Ter-Minasian" baseAuthorityYear="1967" box="[1230,1347,1327,1350]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="346" pageNumber="347" phylum="Tracheophyta" rank="genus">
<emphasis id="B918065E670FB577C5F24E5F3D592417" box="[1230,1347,1327,1350]" italics="true" pageId="346" pageNumber="347">Centaurea</emphasis>
