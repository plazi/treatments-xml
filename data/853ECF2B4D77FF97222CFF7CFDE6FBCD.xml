<geoCoordinate id="68A318FA4D75FF9720FDFDDBFED4FD63" box="[219,290,637,658]" degrees="97" direction="east" minutes="39" orientation="longitude" pageId="10" pageNumber="605" precision="925" value="97.65">97°39′E</geoCoordinate>
, young forest, closed canopy, 
<quantity id="CA6FD3D84D75FF97220BFDDBFD91FD63" box="[557,615,638,658]" metricMagnitude="2" metricUnit="m" metricValue="3.5" pageId="10" pageNumber="605" unit="m" value="350.0">350 m</quantity>
