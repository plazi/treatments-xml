species: 
<taxonomicName id="4C41BFD3256CFF9FB25CFE6FFD20FE0E" authority="Bosc, 1802" authorityName="Bosc" authorityYear="1802" box="[375,705,440,465]" class="Polychaeta" family="Spionidae" genus="Polydora" kingdom="Animalia" order="Spionida" pageId="11" pageNumber="11" phylum="Annelida" rank="species" species="cornuta">
<emphasis id="B9351842256CFF9FB25CFE6FFDD8FE0F" box="[375,569,441,465]" italics="true" pageId="11" pageNumber="11">Polydora cornuta</emphasis>
<bibRefCitation id="EFD0B9A1256CFF9FB169FE6EFD20FE0E" author="Bosc LAG" box="[578,705,440,464]" pageId="11" pageNumber="11" refId="ref7586" refString="Bosc LAG. 1802. Histoire naturelle des vers, contenant leur description et leurs moeurs, avec figures dessine &quot; es d'apres nature. Vol. 1. Paris: Deterville. 324 p." type="book" year="1802">Bosc, 1802</bibRefCitation>
