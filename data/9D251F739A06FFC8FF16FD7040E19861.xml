</taxonomicName>
(62–66) and 
<taxonomicName id="D28CD5E69A07FFCFFC83F8FF42E39E10" box="[770,923,1879,1901]" class="Insecta" family="Miridae" genus="Cylapocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="520" phylum="Arthropoda" rank="species" species="plectipennis">
