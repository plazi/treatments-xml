<taxonomicName id="26DE4D4FFFAEC02AFD41FD92771DF85E" box="[743,875,1967,1991]" class="Insecta" family="Tephritidae" genus="Euphranta" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="447" phylum="Arthropoda" rank="species" species="songhkla">
