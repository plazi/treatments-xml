<emphasis id="B932F8510910FF8C1D81FE9C132DFEB1" bold="true" box="[199,299,332,357]" pageId="22" pageNumber="23">Ecology.</emphasis>
<emphasis id="B932F8510910FF8C1C72FE9C1024FEB0" box="[308,546,332,356]" italics="true" pageId="22" pageNumber="23">
<taxonomicName id="4C465FC00910FF8C1C72FE9C13EBFEB0" authorityName="Mikhailov" authorityYear="1990" box="[308,493,332,356]" class="Arachnida" family="Clubionidae" genus="Clubiona" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pseudosimilis">C. pseudosimilis</taxonomicName>
