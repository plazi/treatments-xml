<taxonomicName id="2941277936EB62944A32B8C9CA5724FF" LSID="urn:lsid:zoobank.org:act:E8CE7AB5-4FDB-4196-B216-BA959B8B2D6F" authority="Wesmael, 1837" authorityName="Wesmael" authorityYear="1837" class="Insecta" family="Braconidae" genus="Therophilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Therophilus parasper" order="Hymenoptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="parasper">Therophilus parasper</taxonomicName>
