</paragraph>
<paragraph id="3D40E77DFFDDFFC0FF38FF3443CAA7E4" blockId="23.[163,1069,203,255]" pageId="23" pageNumber="1376">
<materialsCitation id="8D97ED20FFDDFFC0FF38FF3443F2A7F9" box="[163,692,204,230]" collectionCode="PM, HT, PT" pageId="23" pageNumber="1376" specimenCount="1" specimenCount-female="1">
