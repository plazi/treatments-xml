<quantity id="FA074A98FFDCFFC1FE8DF9D84019A12D" box="[278,351,1568,1590]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" pageId="22" pageNumber="1375" unit="mm" value="4.6">4,6mm</quantity>
