, 
<taxonomicName id="EC3A08E60EF0C7C5F2ED32E98A6F9422" class="Dicotyledoneae" family="Malvaceae" genus="Hibiscus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Malvales" pageId="15" pageNumber="20" phylum="Angiospermae" rank="species" species="tiliaceus">
