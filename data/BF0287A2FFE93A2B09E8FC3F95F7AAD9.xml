</emphasis>
<taxonomicName id="F0AB4D37FFE93A2B0843FB929521AB70" authority="Burmeister, 1838: 430" authorityName="Burmeister" authorityPageNumber="430" authorityYear="1838" box="[316,778,1052,1079]" class="Insecta" family="Philopteridae" genus="Nirmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="7" pageNumber="259" phylum="Arthropoda" rank="species" species="uncinosus">
