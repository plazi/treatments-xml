<emphasis id="B93AEA9FFFAA2B2763AEFB00FD5BE3B9" bold="true" box="[663,671,1251,1265]" pageId="12" pageNumber="13">1</emphasis>
</number>
and 
<emphasis id="B93AEA9FFFAA2B2763E1FB30FD2FE3A5" bold="true" box="[728,747,1235,1261]" pageId="12" pageNumber="13">T</emphasis>
<number id="3D8624AAFFAA2B2763D5FB00FD30E3B9" box="[748,756,1251,1265]" pageId="12" pageNumber="13" value="2.0">
<emphasis id="B93AEA9FFFAA2B2763D5FB00FD30E3B9" bold="true" box="[748,756,1251,1265]" pageId="12" pageNumber="13">2</emphasis>
</number>
; pseudopore nearest 
<emphasis id="B93AEA9FFFAA2B2762D5FB30FBC3E3A5" bold="true" box="[1004,1031,1235,1261]" pageId="12" pageNumber="13">M</emphasis>
