<subSubSection id="C3546506FFAA2B2761FEFA0DFAD5E14F" box="[199,1297,1518,1544]" pageId="12" pageNumber="13" type="etymology">
<paragraph id="8BF1368DFFAA2B2761FEFA0DFAD5E14F" blockId="12.[151,1437,266,1667]" box="[199,1297,1518,1544]" pageId="12" pageNumber="13">
<emphasis id="B93AEA9FFFAA2B2761FEFA0DFE97E140" bold="true" box="[199,339,1518,1544]" pageId="12" pageNumber="13">Etymology.</emphasis>
