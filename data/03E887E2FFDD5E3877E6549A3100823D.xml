<paragraph id="8BFE36F4FFDD5E3877E6576F360C8148" blockId="6.[168,574,1547,1595]" box="[168,574,1576,1595]" pageId="6" pageNumber="393">
<treatmentCitationGroup id="AB5111DAFFDD5E3877E6576F360C8148" box="[168,574,1576,1595]" pageId="6" pageNumber="393">
<taxonomicName id="4C414D77FFDD5E3877E6576F35628149" baseAuthorityName="Dunn" baseAuthorityYear="1931" box="[168,336,1576,1594]" class="Amphibia" family="Craugastoridae" genus="Pristimantis" kingdom="Animalia" order="Anura" pageId="6" pageNumber="393" phylum="Chordata" rank="species" species="gaigei">
