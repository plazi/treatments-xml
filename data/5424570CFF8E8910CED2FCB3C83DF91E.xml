BL 
<quantity id="1B754BFFFF8E8910CF1BFB38CBEBFB27" box="[350,487,1231,1255]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.2" metricValueMin="1.9" pageId="17" pageNumber="18" unit="mm" value="2.05" valueMax="2.2" valueMin="1.9">1.9–2.2 mm</quantity>
