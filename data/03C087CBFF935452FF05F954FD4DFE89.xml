<paragraph id="8BD636DDFF935453FF05F89CFC11F8F3" blockId="22.[151,922,1801,1823]" box="[151,922,1801,1823]" pageId="22" pageNumber="273">
Scopocireae 
<bibRefCitation id="EFF84B2CFF935453FE8BF89CFE15F8F3" author="Simon" box="[281,414,1801,1823]" pageId="22" pageNumber="273" refString="Simon, E. (1901) Histoire naturelle des araignees. Vol. 2. Roret, Paris, 288 pp. [pp. 381 - 668]" type="book chapter" year="1901">
Simon, 
<number id="3DA124FAFF935453FEFAF89CFE15F8F3" box="[360,414,1801,1823]" pageId="22" pageNumber="273" value="1901.0">1901</number>
</bibRefCitation>
: 442, 1048 (
<typeStatus id="54D2887FFF935453FD88F89CFDC5F8F3" box="[538,590,1801,1823]" pageId="22" pageNumber="273">Type</typeStatus>
genus: 
<taxonomicName id="4C694D5EFF935453FD0EF89FFC05F8F3" authority="Simon, 1900" authorityName="Simon" authorityYear="1900" box="[668,910,1801,1823]" class="Arachnida" family="Salticidae" genus="Scopocira" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="273" phylum="Arthropoda" rank="genus">
<emphasis id="B91DEACFFF935453FD0EF89FFC89F8F3" box="[668,770,1802,1823]" italics="true" pageId="22" pageNumber="273">Scopocira</emphasis>
Simon, 
<number id="3DA124FAFF935453FCCAF89CFC05F8F3" box="[856,910,1801,1823]" pageId="22" pageNumber="273" value="1900.0">1900</number>
</taxonomicName>
