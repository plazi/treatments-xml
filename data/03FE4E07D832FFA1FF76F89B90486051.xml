and 
<taxonomicName id="4C578492D833FFA6FF76FF54905664E9" authority="(Kirschbaum, 1868)" authorityName="Kirschbaum" authorityYear="1868" baseAuthorityName="Kirschbaum" baseAuthorityYear="1868" box="[151,636,188,213]" class="Insecta" family="Cicadellidae" genus="Psammotettix" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="479" phylum="Arthropoda" rank="species" species="helvolus">
<emphasis id="B9232303D833FFA6FF76FF5493B964E8" box="[151,403,188,212]" italics="true" pageId="9" pageNumber="479">Psammotettix helvolus</emphasis>
(Kirschbaum, 1868)
</taxonomicName>
(
<taxonomicName id="4C578492D833FFA6FD6BFF55913064E9" box="[650,794,188,213]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="479" phylum="Arthropoda" rank="family">Cicadellidae</taxonomicName>
