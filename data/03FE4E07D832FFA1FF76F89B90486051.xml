<taxonomicName id="4C578492D833FFA6FC57FF709777648C" authority="(Kirschbaum, 1868)" authorityName="Kirschbaum" authorityYear="1868" baseAuthorityName="Kirschbaum" baseAuthorityYear="1868" box="[950,1373,151,176]" class="Insecta" family="Cicadellidae" genus="Diplocolenus" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="479" phylum="Arthropoda" rank="species" species="nigrifrons">
