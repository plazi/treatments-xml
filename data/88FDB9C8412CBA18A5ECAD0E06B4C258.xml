<taxonomicName id="C8667033360196141256294D372DF6E2" class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="478" phylum="Tracheophyta" rank="genus">Gnaphalium</taxonomicName>
