<taxonomicName id="4C0690C2FFEFFFFF679AFCE0FD82FCF6" authority="Hreblay &amp; L. Ronkay, 1998" authorityName="Hreblay &amp; L. Ronkay" authorityYear="1998" box="[151,593,826,850]" class="Insecta" family="Noctuidae" genus="Charanyctycia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="175" phylum="Arthropoda" rank="genus">
