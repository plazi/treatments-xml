<taxonomicName id="4C0690C2FFEFFFFF6326FCE0FE85FCD5" authority="Hreblay &amp; L. Ronkay, 1998" authorityName="Hreblay &amp; L. Ronkay" authorityYear="1998" class="Insecta" family="Noctuidae" genus="Charanyctycia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="175" phylum="Arthropoda" rank="species" species="laudeti">
