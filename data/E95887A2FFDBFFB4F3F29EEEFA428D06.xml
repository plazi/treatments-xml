<typeStatus id="BE4A8816FFDBFFB4F7FF9FB1FB298D4E" box="[1178,1270,1516,1540]" pageId="7" pageNumber="150">syntype</typeStatus>
(s) from 
<collectingCountry id="19E67624FFDBFFB4F63D9FB1FA438D4F" box="[1368,1436,1516,1541]" name="Brazil" pageId="7" pageNumber="150">Brazil</collectingCountry>
(
<collectionCode id="07E0AE71FFDBFFB4F3FB9C4DFF288D62" LSID="urn:lsid:biocol.org:col:34988" box="[158,247,1552,1576]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="7" pageNumber="150">MNHN</collectionCode>
