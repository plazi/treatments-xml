<collectionCode id="07E0AE71FFDBFFB4F1B89C4DFCE68D62" LSID="urn:lsid:biocol.org:col:34988" box="[733,825,1552,1576]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="7" pageNumber="150">MNHN</collectionCode>
