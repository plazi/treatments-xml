<quantity id="A6099B51FFDBFFB4F0629FB1FC8D8D4E" box="[775,850,1516,1540]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="7" pageNumber="150" unit="mm" value="10.0">10mm</quantity>
