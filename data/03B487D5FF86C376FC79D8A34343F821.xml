, inferior appendage; 
<emphasis id="B969EAD1FF83C376FC0FD8BC44D5F955" bold="true" box="[935,958,1677,1695]" pageId="6" pageNumber="339">db</emphasis>
, dorsal branch; 
<emphasis id="B969EAD1FF83C376FBEFD8BC433CF955" bold="true" box="[1095,1111,1677,1695]" pageId="6" pageNumber="339">ib</emphasis>
, inferior branch; 
<emphasis id="B969EAD1FF83C376FB4FD8BC4396F955" bold="true" box="[1255,1277,1677,1695]" pageId="6" pageNumber="339">pa</emphasis>
