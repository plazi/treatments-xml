<emphasis id="B969EAD1FF83C376FEFBD8924631F97F" bold="true" box="[339,346,1699,1717]" pageId="6" pageNumber="339">t</emphasis>
, tergite; 
<emphasis id="B969EAD1FF83C376FE0BD89246D3F97F" bold="true" box="[419,440,1699,1717]" pageId="6" pageNumber="339">vb</emphasis>
