<collectionCode id="ED3EAE2FFF89A74F3A410371FD76738C" box="[563,652,1393,1417]" country="Spain" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35275" lsid="urn:lsid:biocol.org:col:35275" name="Museo Nacional de Ciencias Naturales" pageId="29" pageNumber="833" type="Museum">MNCN</collectionCode>
