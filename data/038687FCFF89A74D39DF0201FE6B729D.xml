<quantity id="4CD79B0FFF89A74F3B1B0310FC38732C" box="[873,962,1296,1321]" metricMagnitude="-1" metricUnit="m" metricValue="1.45" pageId="29" pageNumber="833" unit="mm" value="145.0">145 mm</quantity>
high (
<collectionCode id="ED3EAE2FFF89A74F3C6D0311FB8E732C" box="[1055,1140,1297,1321]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="29" pageNumber="833" type="Museum">USNM</collectionCode>
1003327); 
<emphasis id="B95BEAF8FF89A74F394B0331FE68734C" box="[313,402,1328,1353]" italics="true" pageId="29" pageNumber="833">702/465</emphasis>
