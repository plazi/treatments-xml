<taxonomicName id="4C691CA99313FF89FD192F82FBFFFF0D" authority="Walker, 1853" authorityName="Walker" authorityYear="1853" box="[755,1048,151,177]" class="Insecta" family="Bittacidae" genus="Bittacus" kingdom="Animalia" order="Mecoptera" pageId="3" pageNumber="63" phylum="Arthropoda" rank="species" species="indicus">
