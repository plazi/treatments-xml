<emphasis id="BCEBE55BFF84724EFF7452D5FE9183C5" bold="true" box="[151,284,1758,1780]" pageId="53" pageNumber="54">Type species</emphasis>
: 
<taxonomicName id="499F42CAFF84724EFEC952D5FD1583C5" authority="Bolivar, 1887" authorityName=": Bolivar" authorityYear="1887" box="[298,664,1758,1780]" class="Insecta" family="Tetrigidae" genus="Gavialidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="aurivillii">
<emphasis id="BCEBE55BFF84724EFEC952D5FD8583C5" box="[298,520,1758,1780]" italics="true" pageId="53" pageNumber="54">Gavialidium aurivillii</emphasis>
<bibRefCitation id="EA0E44B8FF84724EFDED52D5FD1583C5" box="[526,664,1758,1780]" pageId="53" pageNumber="54" refId="ref35849" refString="Bolivar, I. (1887) Essai sur les Acridiens de la tribu des Tettigidae. Annales de la Societ Entomologique de Belgique, 31, 175 - 313, pls. 4 - 5." type="journal article">Bolivar, 1887</bibRefCitation>
</taxonomicName>
(= 
<taxonomicName id="499F42CAFF84724EFD5852D5FC4883C5" baseAuthorityName="Bolivar" baseAuthorityYear="1887" box="[699,965,1758,1780]" class="Insecta" family="Tetrigidae" genus="Tagaloscelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="aurivillii">
