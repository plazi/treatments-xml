<taxonomicName id="8481A4BDDD9ECEC01F53DD2CC3AED290" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" infraspecific-rank="var." kingdom="Animalia" lsidName="Chrysis succincta var. sparsepunctata" order="Hymenoptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="variety" species="succincta" variety="sparsepunctata">Chrysis succincta var. sparsepunctata</taxonomicName>
