<taxonomicName id="EA6C20C8B91EC508B2C4DA6E06007A92" LSID="http://zoobank.org/EBEAF140-BE40-4802-B1A3-8477C58276C4" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus impressicollis" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="impressicollis">Trigonopterus impressicollis Riedel</taxonomicName>
