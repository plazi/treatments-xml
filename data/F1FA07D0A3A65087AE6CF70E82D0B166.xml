<subSubSection id="B288E71CC70F92191929FEA526647BCB" pageId="0" pageNumber="57" type="materials_examined">
<paragraph id="FAA100459B9008DD35C2CA7D072AC4A1" pageId="0" pageNumber="57">Type.</paragraph>
<paragraph id="27C9149A1DDCD3A55FE81A311013E61F" pageId="0" pageNumber="57">
