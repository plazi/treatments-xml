<taxonomicName id="45B34D70FFA09B0FFE4F261240ABED29" authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Ii" baseAuthorityYear="1964" box="[507,724,1597,1621]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="22" pageNumber="677" phylum="Arthropoda" rank="species" species="pseudomitsukurii">
