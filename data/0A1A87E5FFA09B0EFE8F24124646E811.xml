</taxonomicName>
and 
<taxonomicName id="45B34D70FFA09B0FFCBA26124107ED29" authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Liu and Wang" baseAuthorityYear="1983" box="[782,888,1597,1621]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="22" pageNumber="677" phylum="Arthropoda" rank="species" species="tenella">
