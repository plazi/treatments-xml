</taxonomicName>
(
<taxonomicName id="4C754D4EFF21FF1272FFFB32FC21FEA5" box="[733,896,1195,1219]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="173" pageNumber="174" phylum="Chordata" rank="order">Passeriformes</taxonomicName>
