<taxonomicName id="4C3E4D60FFD9FFD636D5F153FD81FC94" authorityName="Kapp" authorityYear="2019" box="[430,555,771,793]" class="Insecta" family="Staphylinidae" genus="Oligota" kingdom="Animalia" order="Coleoptera" pageId="55" pageNumber="642" phylum="Arthropoda" rank="species" species="viti">
