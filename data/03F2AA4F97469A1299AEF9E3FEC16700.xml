<taxonomicName id="4C5B60DA97469A119875F8F0FCBE6711" authority="Krauss, 1862" authorityName="Krauss" authorityYear="1862" box="[332,684,1816,1841]" class="Mammalia" family="Dasypodidae" genus="Dasypus" kingdom="Animalia" order="Cingulata" pageId="9" pageNumber="280" phylum="Chordata" rank="species" species="kappleri">
