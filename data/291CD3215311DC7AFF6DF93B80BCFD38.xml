<collectingCountry id="D9A222A75310DC7AFE9DFD7582ABFD38" box="[359,454,655,681]" name="Pakistan" pageId="7" pageNumber="458">Pakistan</collectingCountry>
and 
<collectingCountry id="D9A222A75310DC7AFE07FD75815AFD38" box="[509,567,655,681]" name="India" pageId="7" pageNumber="458">India</collectingCountry>
