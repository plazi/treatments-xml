<taxonomicName id="809FE4A95351FFDCFE84F8A3FE7D67F8" box="[288,470,1912,1935]" class="Insecta" family="Braconidae" genus="Iare" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="35" phylum="Arthropoda" rank="species" species="cheguevarai">
