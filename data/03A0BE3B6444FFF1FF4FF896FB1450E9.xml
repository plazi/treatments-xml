<taxonomicName id="4C0974AE6444FFF1FF36F896FE355070" authority="Oken, 1816" authorityName="Oken" authorityYear="1816" box="[238,500,1792,1818]" class="Mammalia" family="Felidae" genus="Panthera" kingdom="Animalia" order="Carnivora" pageId="78" pageNumber="79" phylum="Chordata" rank="genus">
