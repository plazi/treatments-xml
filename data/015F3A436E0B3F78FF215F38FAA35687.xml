<paragraph id="89498B556E0B3F78FF715FEEFE1F5614" blockId="5.[151,1437,1214,1490]" pageId="5" pageNumber="206">
= 
<taxonomicName id="4EF6F0D66E0B3F78FF565FEEFD3E57AC" authority="Enslin, 1912b: 104" authorityName="Enslin" authorityPageNumber="104" authorityYear="1912" box="[224,669,1249,1274]" class="Insecta" family="Tenthredinidae" genus="Tenthredella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="206" phylum="Arthropoda" rank="species" species="celsia">
