<taxonomicName id="4EF6F0D66E0B3F78FDD65E0BFD73564B" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[608,720,1287,1310]" class="Insecta" family="Tenthredinidae" genus="Tenthredo" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="206" phylum="Arthropoda" rank="species" species="caspica">
