<taxonomicName id="A0C319E2895E8B3DFF39FD50FDC7FD69" authority="Martin" authorityName="Martin" authorityYear="1950" box="[151,514,729,756]" class="Trematoda" family="Heterophyidae" genus="Phocitremoides" kingdom="Animalia" order="Plagiorchiida" pageId="22" pageNumber="481" phylum="Platyhelminthes" rank="species" species="ovale">
