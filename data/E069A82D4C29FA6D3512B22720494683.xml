<normalizedToken id="2185851E1D2B40C92345A8784ACB23B6" originalValue="Gözne">Goezne</normalizedToken>
, 
<geoCoordinate id="FD91F2D325AD53A17EE7379B55DFFCCC" direction="north" orientation="latitude" precision="1" value="36.988472">36°59'18.5&quot;N</geoCoordinate>
<geoCoordinate id="54968845ED1D33194E920BBE5EB96ADB" direction="east" orientation="longitude" precision="15" value="34.50528">34°30'19&quot;E</geoCoordinate>
, on 
<taxonomicName id="4E3E4A9444A2D878935BEE42D8C43DC2" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus cerris" order="Fagales" pageId="5" pageNumber="258" phylum="Tracheophyta" rank="species" species="cerris">Quercus cf. cerris</taxonomicName>
