<taxonomicName id="4C16BABF9544FF83F981FC3D76EEFDA1" box="[1267,1411,952,979]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="myrtifolia">
<emphasis id="B9621D2E9544FF83F981FC3D76EEFDA1" box="[1267,1411,952,979]" italics="true" pageId="65" pageNumber="66">Q. myrtifolia</emphasis>
