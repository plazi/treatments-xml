<taxonomicName id="4C16BABF9544FF83FF44FC597066FD85" authorityName="Ashmead" authorityYear="1885" box="[566,779,988,1015]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="medullae">
