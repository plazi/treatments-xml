<taxonomicName id="4C16BABF9544FF83FEFCFC3D7091FDA1" box="[910,1020,953,979]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="incana">
<emphasis id="B9621D2E9544FF83FEFCFC3D7091FDA1" box="[910,1020,953,979]" italics="true" pageId="65" pageNumber="66">Q. incana</emphasis>
