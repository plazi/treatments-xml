<taxonomicName id="4C16BABF9544FF83F978FC3D77DBFDA1" box="[1034,1206,952,979]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="marilandica">
