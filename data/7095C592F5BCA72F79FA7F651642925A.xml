<taxonomicName id="7569B4E884231649D8C9AC7F426D0ADB" authority="Rossi, 1790" authorityName="Rossi" authorityYear="1790" class="Insecta" family="Carabidae" genus="Harpalus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Harpalus (Harpalus) dimidiatus" order="Coleoptera" pageId="0" pageNumber="1082" phylum="Arthropoda" rank="species" species="dimidiatus" subGenus="Harpalus">Harpalus (Harpalus) dimidiatus (Rossi, 1790)</taxonomicName>
