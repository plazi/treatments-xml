<emphasis id="35ADEAF3FF8CCD10FF586F31DD5FF9D6" bold="true" box="[240,275,1613,1630]" pageId="13" pageNumber="552">A-C</emphasis>
, 
<taxonomicName id="C0D94D62FF8CCD10FE886F31DD3DF9D6" authorityName="Fitzinger" authorityYear="1826" box="[288,369,1613,1630]" class="Reptilia" family="Lamprophiidae" genus="Malpolon" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="552" phylum="Chordata" rank="genus">
