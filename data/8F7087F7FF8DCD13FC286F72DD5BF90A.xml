<taxonomicName id="C0D94D62FF8CCD10FF3F6F1EDD55F9FB" authorityName="Guichenot" authorityYear="1850" box="[151,281,1634,1651]" class="Reptilia" family="Colubridae" genus="Macroprotodon" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="552" phylum="Chordata" rank="genus">
