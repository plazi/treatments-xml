<caption id="53A66669FF8CCD10FF3F6F31D817F9FB" ID-DOI="http://doi.org/10.5281/zenodo.4650508" ID-Zenodo-Dep="4650508" httpUri="https://zenodo.org/record/4650508/files/figure.png" pageId="13" pageNumber="552" startId="13.[151,162,1613,1630]" targetBox="[152,1267,180,1596]" targetPageId="13">
<paragraph id="076636E1FF8CCD10FF3F6F31D817F9FB" blockId="13.[151,1248,1613,1651]" pageId="13" pageNumber="552">
