(
<figureCitation id="D9A42A12FF97E378FF08FF75E0603BA1" box="[159,257,186,211]" captionStart="FIGURES 2" captionStartId="8.[151,264,1957,1980]" captionTargetBox="[166,1425,307,1931]" captionTargetId="figure@8.[161,1425,301,1937]" captionTargetPageId="8" captionText="FIGURES 2A–2F. Holotypes of Agrilus, habitus in dorsal view. 2A, A. yuk sp. nov. (7.2 mm); 2B, A. xis sp. nov. (5.2 mm); 2C, A. qom sp. nov. (4.3 mm); 2D, A. jum sp. nov. (4.4 mm); 2E, A. ika sp. nov. (6.6 mm); 2F, A. kon sp. nov. (4.1 mm)." figureDoi="http://doi.org/10.5281/zenodo.3713729" httpUri="https://zenodo.org/record/3713729/files/figure.png" pageId="9" pageNumber="458">Figs. 2C</figureCitation>
, 
<figureCitation id="D9A42A12FF97E378FE9AFF75E0443BA1" box="[269,293,186,211]" captionStart="FIGURES 5" captionStartId="18.[151,264,1932,1955]" captionTargetBox="[161,1425,233,1869]" captionTargetId="figure@18.[161,1425,233,1869]" captionTargetPageId="18" captionText="FIGURES 5A–5N. Aedeagi of Agrilus holotypes. 5A, A. xia sp. nov.; 5B, A. hergovitsi sp. nov.; 5C, A. zao sp. nov.; 5D, A. wos sp. nov.; 5E, A. uxo sp. nov.; 5F, A. xen sp. nov.; 5G, A. yuk sp. nov.; 5H, A. xis sp. nov.; 5I, A. qom sp. nov.; 5J, A. jum sp. nov.; 5K, A. ika sp. nov.; 5L, A. kon sp. nov.; 5M, A. hik sp. nov.; 5N, A. yoa sp. nov." figureDoi="http://doi.org/10.5281/zenodo.3713735" httpUri="https://zenodo.org/record/3713735/files/figure.png" pageId="9" pageNumber="458">
<date id="35211057FF97E378FE9AFF75E0443BA1" box="[269,293,186,211]" pageId="9" pageNumber="458">5I</date>
</figureCitation>
