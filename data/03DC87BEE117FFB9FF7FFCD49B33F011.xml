<emphasis id="B901EABAE116FFBAFE63F8079EB3F4AC" bold="true" box="[395,450,1936,1960]" pageId="9" pageNumber="10">I–M:</emphasis>
<taxonomicName id="4C754D2BE116FFBAFE20F8069CFEF4AC" authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[456,911,1936,1960]" class="Insecta" family="Melolonthidae" genus="Amiserica" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="zhongtiaoshanensis" status="new species">
<emphasis id="B901EABAE116FFBAFE20F8069DD8F4AC" box="[456,681,1936,1960]" italics="true" pageId="9" pageNumber="10">A. zhongtiaoshanensis</emphasis>
