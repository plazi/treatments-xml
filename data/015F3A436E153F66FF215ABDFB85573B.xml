<number id="3F3E99726E153F66FD425936FCF95107" box="[756,858,570,594]" pageId="27" pageNumber="228" value="269.5" valueMax="270.0" valueMin="269.0">269–270</number>
. 
<typeStatus id="564D35F76E153F66FCD15936FC645107" box="[871,967,570,594]" pageId="27" pageNumber="228">Syntype</typeStatus>
