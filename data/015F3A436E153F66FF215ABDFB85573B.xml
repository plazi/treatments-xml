<paragraph id="89498B556E153F66FF215936FDCB51CF" blockId="27.[151,1437,570,1134]" pageId="27" pageNumber="228">
<typeStatus id="564D35F76E153F66FF215936FF535107" box="[151,240,570,594]" pageId="27" pageNumber="228">TYPES</typeStatus>
. 
<taxonomicName id="4EF6F0D66E153F66FF485937FE4B5106" authority="Sabariensis" authorityName="Sabariensis" box="[254,488,571,595]" class="Insecta" family="Tenthredinidae" genus="Allantus" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="228" phylum="Arthropoda" rank="genus">
