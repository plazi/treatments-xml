<location id="8EF76064FFD5FF91FE5727C2F851D4B4" LSID="urn:lsid:plazi:treatment:038187A9FFD5FF91FF7D2085F8F9D787:8EF76064FFD5FF91FE5727C2F851D4B4" box="[436,527,1175,1199]" county="Leg" municipality="Ventral region" name="Median" pageId="68" pageNumber="1024">Median</location>
