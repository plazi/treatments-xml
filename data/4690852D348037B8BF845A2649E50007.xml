<taxonomicName id="7B2BC95363260E6AF057D80B3588686E" authority="L." class="Magnoliopsida" family="Apiaceae" genus="Cicuta" kingdom="Plantae" order="Apiales" pageId="null" pageNumber="873" phylum="Tracheophyta" rank="species" species="virosa">
