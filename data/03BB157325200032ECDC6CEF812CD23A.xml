<specimenCount id="9D146FEC25230032EF506E3A814ED362" box="[795,831,1609,1633]" pageId="10" pageNumber="285" type="generic">
<number id="3DDAB64225230032EF506E3A8157D362" box="[795,806,1609,1633]" pageId="10" pageNumber="285" value="1.0">1</number>
♂
</specimenCount>
, 
<collectionCode id="ED033CA025230032EF076E3A81F4D362" LSID="urn:lsid:biocol.org:col:34838" box="[844,901,1609,1633]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="10" pageNumber="285">SMF</collectionCode>
, 
<number id="3DDAB64225230032EFC46E3A81BBD362" box="[911,970,1609,1633]" pageId="10" pageNumber="285" value="2004.0">2004</number>
