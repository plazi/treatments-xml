<taxonomicName id="ACF3A5DB231313252A32EBF64E6BB80E" authorityName="Brooks" authorityYear="1988" class="Insecta" family="Apidae" genus="Amegilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amegilla epaphrodita" order="Hymenoptera" pageId="0" pageNumber="45" phylum="Arthropoda" rank="species" species="epaphrodita">Amegilla (Asaropoda) epaphrodita</taxonomicName>
