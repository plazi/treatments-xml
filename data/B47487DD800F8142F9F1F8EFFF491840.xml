, 
<collectingCountry id="44CA765B80318142FA44FF30FCDA1BE0" box="[802,880,189,213]" name="Belize" pageId="33" pageNumber="34">Belize</collectingCountry>
, 
<collectingCountry id="44CA765B80318142FA1BFF30FC6A1BE0" box="[893,960,189,213]" name="Chile" pageId="33" pageNumber="34">Chile</collectingCountry>
, 
