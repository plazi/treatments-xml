<collectingCountry id="44CA765B80318142F831FF30FE191BE0" box="[343,435,189,213]" name="Canada" pageId="33" pageNumber="34">Canada</collectingCountry>
, 
<collectingCountry id="44CA765B80318142F8A6FF30FDB31BE0" box="[448,537,189,213]" name="Mexico" pageId="33" pageNumber="34">Mexico</collectingCountry>
