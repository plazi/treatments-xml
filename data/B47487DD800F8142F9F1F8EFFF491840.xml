, 
<collectingCountry id="44CA765B80318142FD47FF30FB081BE0" box="[1057,1186,189,213]" name="Guatemala" pageId="33" pageNumber="34">Guatemala</collectingCountry>
, 
<collectingCountry id="44CA765B80318142FDC9FF30FB441BE0" box="[1199,1262,189,213]" name="India" pageId="33" pageNumber="34">India</collectingCountry>
, 
<collectingCountry id="44CA765B80318142FD9DFF33FAEB1BE0" box="[1275,1345,190,213]" name="Japan" pageId="33" pageNumber="34">Japan</collectingCountry>
, 
