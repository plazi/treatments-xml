<collectionCode id="DFC54DDBDC7CFFFAA88FFD3CFCAFFD37" box="[712,783,672,693]" country="USA" httpUri="http://grbio.org/cool/daw7-8h44" name="The Cleveland Museum of Natural History" pageId="47" pageNumber="48" type="Museum">CMNH</collectionCode>
and 
<collectionCode id="DFC54DDBDC7CFFFAA905FD3CFC2AFD37" box="[834,906,672,693]" pageId="47" pageNumber="48">WOPC</collectionCode>
