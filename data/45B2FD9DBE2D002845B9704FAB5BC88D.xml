<taxonomicName id="D43F346E2B3A1CB84950777C1970D63D" LSID="http://zoobank.org/C5EE9217-E9E0-4170-B867-47484CBE8A3C" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus seis" order="Hymenoptera" pageId="406" pageNumber="407" phylum="Arthropoda" rank="species" species="seis">Heterospilus seis Marsh</taxonomicName>
