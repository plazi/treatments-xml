<taxonomicName id="4C3CDAC2FFDACA7AFF6B175EFCE2F8BF" authority="Takaoka &amp; Davies, 1995" authorityName="Takaoka &amp; Davies" authorityYear="1995" box="[193,886,1876,1901]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="sheilae" subGenus="Gomphostilbia">
<emphasis id="B9487D53FFDACA7AFF6B175EFEA7F8BF" bold="true" box="[193,307,1876,1901]" italics="true" pageId="19" pageNumber="20">Simulium</emphasis>
(
<emphasis id="B9487D53FFDACA7AFEE9175EFE79F8BF" bold="true" box="[323,493,1876,1901]" italics="true" pageId="19" pageNumber="20">Gomphostilbia</emphasis>
) 
<emphasis id="B9487D53FFDACA7AFE56175EFDD9F8BF" bold="true" box="[508,589,1876,1901]" italics="true" pageId="19" pageNumber="20">sheilae</emphasis>
