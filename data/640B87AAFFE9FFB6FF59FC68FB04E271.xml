<emphasis id="DED6EAAEFFEFFFB7FF59FA63FE63E4E0" bold="true" box="[192,391,1458,1482]" pageId="4" pageNumber="3">Type material.</emphasis>
<materialsCitation id="5CCA3CE1FFEFFFB7FE14FA62FE9CE720" collectingDate="2005-04-04" collectingDateMax="2005-04-10" collectingDateMin="2005-04-04" collectorName="Z. H. Falin" country="United States of America" county="Jefferson Co." latitude="39.223" location="Rd." longLatPrecision="11" longitude="-95.404" municipality="The Falin Property" pageId="4" pageNumber="3" specimenCount="1" stateProvince="Kansas" typeStatus="paratype">
