<quantity id="CC349B12FF99FFD5FD71307DF44BFDFC" box="[691,775,546,573]" metricMagnitude="3" metricUnit="m" metricValue="1.165" pageId="9" pageNumber="228" unit="m" value="1165.0">
<elevation id="80E1D1C4FF99FFD5FD71307DF44BFDFC" box="[691,775,546,573]" metricMagnitude="3" metricUnit="m" metricValue="1.165" pageId="9" pageNumber="228" unit="m" value="1165.0">1165 m</elevation>
</quantity>
