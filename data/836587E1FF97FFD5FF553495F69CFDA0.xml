<location id="0E13602CFF99FFD5FE6933E8F546FE10" LSID="urn:lsid:plazi:treatment:836587E1FF97FFD5FF553495F69CFDA0:0E13602CFF99FFD5FE6933E8F546FE10" box="[427,522,439,465]" country="Guinea-Bissau" county="Liberian Nimba Mountains" municipality="The" name="Guinean" pageId="9" pageNumber="228">Guinean</location>
