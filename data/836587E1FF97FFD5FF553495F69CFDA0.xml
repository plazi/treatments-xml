<location id="0E13602CFF99FFD5FDA533E8F47FFE10" LSID="urn:lsid:plazi:treatment:836587E1FF97FFD5FF553495F69CFDA0:0E13602CFF99FFD5FDA533E8F47FFE10" box="[615,819,439,465]" country="Guinea-Bissau" county="Liberian Nimba Mountains" municipality="The" name="Nimba Mountains" pageId="9" pageNumber="228">Nimba Mountains</location>
