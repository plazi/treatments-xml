at about 
<quantity id="CC349B12FF99FFD5FDCB3385F5CAFE34" box="[521,646,474,501]" metricMagnitude="2" metricUnit="m" metricValue="8.25" metricValueMax="8.5" metricValueMin="8.0" pageId="9" pageNumber="228" unit="m" value="825.0" valueMax="850.0" valueMin="800.0">
<elevation id="80E1D1C4FF99FFD5FDCB3385F5CAFE34" box="[521,646,474,501]" metricMagnitude="2" metricUnit="m" metricValue="8.25" metricValueMax="8.5" metricValueMin="8.0" pageId="9" pageNumber="228" unit="m" value="825.0" valueMax="850.0" valueMin="800.0">800-850 m</elevation>
</quantity>
. 
<location id="0E13602CFF99FFD5FD523384F59DFE34" LSID="urn:lsid:plazi:treatment:836587E1FF97FFD5FF553495F69CFDA0:0E13602CFF99FFD5FD523384F59DFE34" box="[656,721,475,501]" country="Guinea-Bissau" county="Liberian Nimba Mountains" municipality="The" name="There" pageId="9" pageNumber="228">There</location>
