<number id="3DFC24C0FFA4187FFF43FF1E13B8FF08" box="[173,224,224,248]" pageId="5" pageNumber="292" value="1.33">1.33</number>
, IV = 
<number id="3DFC24C0FFA4187FFEC6FF211204FF08" box="[296,348,223,248]" pageId="5" pageNumber="292" value="1.45">1.45</number>
; rostrum: I = 
<number id="3DFC24C0FFA4187FFE1BFF1E1171FF08" box="[501,553,224,248]" pageId="5" pageNumber="292" value="0.91">0.91</number>
