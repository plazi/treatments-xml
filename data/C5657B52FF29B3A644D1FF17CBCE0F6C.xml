<collectingCounty id="A412B2C8FF29B3A64562FC51C9AA089C" box="[292,428,920,944]" pageId="181" pageNumber="182">Tartar Strait</collectingCounty>
and 
<collectingMunicipality id="AD17503EFF29B3A645A1FC51CA110883" box="[487,535,920,943]" pageId="181" pageNumber="182">East</collectingMunicipality>
<collectingRegion id="8F0804A6FF29B3A64667FC51CA81089C" box="[545,647,920,944]" country="Russia" name="Sakhalin" pageId="181" pageNumber="182">Sakhalin</collectingRegion>
; 
<location id="48139C9FFF29B3A646D2FC51CB03089C" LSID="urn:lsid:plazi:treatment:C5657B52FF29B3A644D1FF17CBCE0F6C:48139C9FFF29B3A646D2FC51CB03089C" box="[660,773,920,944]" county="Tartar Strait" municipality="East" name="Skidegate" pageId="181" pageNumber="182" stateProvince="Sakhalin">Skidegate</location>
, 
<location id="48139C9FFF29B3A64755FC51CC24089C" LSID="urn:lsid:plazi:treatment:C5657B52FF29B3A644D1FF17CBCE0F6C:48139C9FFF29B3A64755FC51CC24089C" box="[787,1058,920,944]" county="Tartar Strait" municipality="East" name="Queen Charlotte Sound" pageId="181" pageNumber="182" stateProvince="Sakhalin">Queen Charlotte Sound</location>
, 
<collectingRegion id="8F0804A6FF29B3A64076FC51CCF2089C" box="[1072,1268,920,944]" country="Canada" name="British Columbia" pageId="181" pageNumber="182">British Columbia</collectingRegion>
to 
<location id="48139C9FFF29B3A6415BFC51CD91089C" LSID="urn:lsid:plazi:treatment:C5657B52FF29B3A644D1FF17CBCE0F6C:48139C9FFF29B3A6415BFC51CD91089C" box="[1309,1431,920,944]" county="Tartar Strait" municipality="East" name="San Diego" pageId="181" pageNumber="182" stateProvince="Sakhalin">San Diego</location>
