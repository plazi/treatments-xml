<quantity id="4CAF9B03FF8EFFF5FB4AFEC3FAD0FE98" box="[1162,1305,340,364]" metricMagnitude="2" metricUnit="m" metricValue="6.4" metricValueMax="10.8" metricValueMin="2.0" pageId="41" pageNumber="42" unit="m" value="640.0" valueMax="1080.0" valueMin="200.0">200–1080 m</quantity>
