<quantity id="4CAF9B03FF8EFFF5FD2BFEA7FCF4FEB4" box="[747,829,304,328]" metricMagnitude="-2" metricUnit="m" metricValue="8.0" pageId="41" pageNumber="42" unit="mm" value="80.0">80 mm</quantity>
