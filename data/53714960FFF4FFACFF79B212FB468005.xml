<taxonomicName id="1CD883F5FFF4FFACFEDFB13DFCBC8005" authority="Denis and Schiffermuller" authorityName="Denis and Schiffermuller" box="[305,783,1572,1596]" class="Insecta" family="Tortricidae" genus="Tortrix" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="asperana">
