)................ 
<taxonomicName authority="Jacoby" authorityName="Jacoby. In" authorityYear="1881" box="[1105,1440,365,389]" class="Insecta" family="Chrysomelidae" genus="Beltia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="nicaraguensis">
