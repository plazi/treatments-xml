<taxonomicName baseAuthorityName="Motschulsky" baseAuthorityYear="1860" class="Hexapoda" family="Chrysomelidae" genus="Medythia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Medythia nigrobilineata" order="Coleoptera" pageId="0" pageNumber="729" phylum="Arthropoda" rank="species" species="nigrobilineata">
