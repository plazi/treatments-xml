<taxonomicName authorityName="Latreille" authorityYear="1802" class="Insecta" family="Chrysomelidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="729" phylum="? arthropoda" rank="family">Chrysomelidae</taxonomicName>
