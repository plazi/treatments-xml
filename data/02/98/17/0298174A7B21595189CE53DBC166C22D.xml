<taxonomicName baseAuthorityName="Motschulsky" baseAuthorityYear="1858" class="Hexapoda" family="Chrysomelidae" genus="Medythia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Medythia suturalis" order="Coleoptera" pageId="0" pageNumber="729" phylum="Arthropoda" rank="species" species="suturalis">
<emphasis italics="true" pageId="0" pageNumber="729">Medythia suturalis</emphasis>
</taxonomicName>
