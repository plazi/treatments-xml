<taxonomicName authority="Steiner, 1918" authorityName="Steiner" authorityYear="1918" box="[569,876,1400,1422]" class="Chromadorea" family="Desmodoridae" genus="Zalonema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="6" pageNumber="507" phylum="Nematoda" rank="species" species="megalosoma">
