, 
<taxonomicName authority="castus" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="33" pageNumber="34" phylum="Mollusca" rank="subSpecies" species="castus" subGenus="Mesembrinus" subSpecies="castus">
