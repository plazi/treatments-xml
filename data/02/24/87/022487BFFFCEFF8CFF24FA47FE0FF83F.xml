<taxonomicName authorityName="Krefft" authorityYear="1871" box="[786,928,1663,1689]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="19" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
