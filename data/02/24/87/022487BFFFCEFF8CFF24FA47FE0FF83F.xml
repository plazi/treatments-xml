<quantity box="[230,359,1662,1689]" metricMagnitude="0" metricUnit="m" metricValue="4.3561" metricValueMax="4.5212" metricValueMin="4.191" pageId="19" pageNumber="220" unit="in" value="171.5" valueMax="178.0" valueMin="165.0">165–178 in</quantity>
