<taxonomicName authorityName="Krefft" authorityYear="1871" box="[151,286,187,213]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="25" pageNumber="203" phylum="Nematoda" rank="genus">gnathosoma</taxonomicName>
