<taxonomicName authorityName="Ausserer" authorityYear="1871" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="27" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
