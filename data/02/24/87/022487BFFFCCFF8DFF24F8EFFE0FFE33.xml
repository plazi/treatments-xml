<quantity box="[378,506,151,177]" metricMagnitude="0" metricUnit="m" metricValue="4.318" metricValueMax="4.3942" metricValueMin="4.2418" pageId="18" pageNumber="219" unit="in" value="170.0" valueMax="173.0" valueMin="167.0">167–173 in</quantity>
female, 
<quantity box="[604,676,150,177]" metricMagnitude="0" metricUnit="m" metricValue="3.937" pageId="18" pageNumber="219" unit="in" value="155.0">155 in</quantity>
