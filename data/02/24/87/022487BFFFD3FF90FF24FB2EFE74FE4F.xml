female, 
<quantity box="[952,1078,1411,1438]" metricMagnitude="0" metricUnit="m" metricValue="8.8011" metricValueMax="8.9662" metricValueMin="8.636" pageId="14" pageNumber="215" unit="in" value="346.5" valueMax="353.0" valueMin="340.0">340–353 in</quantity>
male, 
<quantity box="[1152,1278,1411,1438]" metricMagnitude="0" metricUnit="m" metricValue="8.0137" metricValueMax="8.331199999999999" metricValueMin="7.6962" pageId="14" pageNumber="215" unit="in" value="315.5" valueMax="328.0" valueMin="303.0">303–328 in</quantity>
DN, 
<quantity metricMagnitude="0" metricUnit="m" metricValue="6.4135" metricValueMax="6.4262" metricValueMin="6.4008" pageId="14" pageNumber="215" unit="in" value="252.5" valueMax="253.0" valueMin="252.0">252–253 in</quantity>
PN, 
<quantity box="[232,304,1447,1474]" metricMagnitude="0" metricUnit="m" metricValue="4.7498" pageId="14" pageNumber="215" unit="in" value="187.0">187 in</quantity>
LV. 
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[359,463,1447,1473]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
width 291 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[601,706,1447,1473]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
length 397) in female, 267 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[1030,1135,1447,1473]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
length 340) in male. 
<taxonomicName authorityName="Krefft" authorityYear="1871" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="14" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
length 179 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[391,496,1483,1509]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
length 397) in female, 167 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[821,926,1483,1509]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
