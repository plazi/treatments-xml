<taxonomicName authorityName="Krefft" authorityYear="1871" box="[1172,1314,1483,1509]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="14" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
width 102 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[159,264,1519,1545]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
length 397) in female, 90 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[577,682,1519,1545]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
