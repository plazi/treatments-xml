female, 
<quantity box="[243,372,1699,1725]" metricMagnitude="0" metricUnit="m" metricValue="2.7178000000000004" metricValueMax="2.8701999999999996" metricValueMin="2.5654000000000003" pageId="30" pageNumber="231" unit="in" value="107.0" valueMax="113.0" valueMin="101.0">101–113 in</quantity>
