female, 
<quantity box="[449,575,1591,1617]" metricMagnitude="0" metricUnit="m" metricValue="8.5852" metricValueMax="9.2964" metricValueMin="7.874" pageId="15" pageNumber="216" unit="in" value="338.0" valueMax="366.0" valueMin="310.0">310–366 in</quantity>
DN, 
<quantity box="[635,761,1590,1617]" metricMagnitude="0" metricUnit="m" metricValue="6.2611" metricValueMax="6.4262" metricValueMin="6.096" pageId="15" pageNumber="216" unit="in" value="246.5" valueMax="253.0" valueMin="240.0">240–253 in</quantity>
PN. 
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[816,920,1591,1617]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
width 
<quantity box="[997,1123,1590,1617]" metricMagnitude="0" metricUnit="m" metricValue="7.0739" metricValueMax="7.9248" metricValueMin="6.223" pageId="15" pageNumber="216" unit="in" value="278.5" valueMax="312.0" valueMin="245.0">245–312 in</quantity>
female. 
<taxonomicName authorityName="Krefft" authorityYear="1871" box="[1218,1360,1591,1617]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="15" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
length 
<quantity box="[151,278,1627,1653]" metricMagnitude="0" metricUnit="m" metricValue="6.057899999999999" metricValueMax="6.603999999999999" metricValueMin="5.511800000000001" pageId="15" pageNumber="216" unit="in" value="238.5" valueMax="260.0" valueMin="217.0">217–260 in</quantity>
female, 
<quantity box="[373,499,1627,1653]" metricMagnitude="0" metricUnit="m" metricValue="4.445" metricValueMax="4.6482" metricValueMin="4.2418" pageId="15" pageNumber="216" unit="in" value="175.0" valueMax="183.0" valueMin="167.0">167–183 in</quantity>
DN, 
<quantity box="[559,630,1627,1653]" metricMagnitude="0" metricUnit="m" metricValue="3.2257999999999996" pageId="15" pageNumber="216" unit="in" value="127.0">127 in</quantity>
PN. Ratio of 
<taxonomicName authorityName="Krefft" authorityYear="1871" box="[781,916,1627,1653]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="15" pageNumber="203" phylum="Nematoda" rank="genus">gnathosoma</taxonomicName>
