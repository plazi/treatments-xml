DN, 
<quantity metricMagnitude="0" metricUnit="m" metricValue="3.7464999999999997" metricValueMax="3.81" metricValueMin="3.683" pageId="33" pageNumber="234" unit="in" value="147.5" valueMax="150.0" valueMin="145.0">145–150 in</quantity>
PN, 
<quantity box="[233,362,439,465]" metricMagnitude="0" metricUnit="m" metricValue="3.1369" metricValueMax="3.2257999999999996" metricValueMin="3.048" pageId="33" pageNumber="234" unit="in" value="123.5" valueMax="127.0" valueMin="120.0">120–127 in</quantity>
LV. 
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[417,521,439,465]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
width 147 (
<taxonomicName authorityName="Ausserer" authorityYear="1871" box="[660,765,439,465]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="33" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
length 219) in female, 140 (
