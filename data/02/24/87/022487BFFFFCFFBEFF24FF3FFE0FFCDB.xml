<collectingCountry box="[1286,1437,655,681]" name="Saint Kitts and Nevis" pageId="33" pageNumber="234">San Cristobal</collectingCountry>
(
<collectingRegion box="[159,279,691,717]" country="Ecuador" name="Galapagos" pageId="33" pageNumber="234">Galapagos</collectingRegion>
Is.) [
<collectingCountry box="[336,438,690,717]" name="Ecuador" pageId="33" pageNumber="234">Ecuador</collectingCountry>
