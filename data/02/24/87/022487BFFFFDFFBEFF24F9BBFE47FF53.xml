<taxonomicName authorityName="Krefft" authorityYear="1871" box="[339,474,1843,1869]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="32" pageNumber="203" phylum="Nematoda" rank="genus">gnathosoma</taxonomicName>
