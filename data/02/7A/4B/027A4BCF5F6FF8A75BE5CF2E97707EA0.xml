<taxonomicName authority="Guenée, 1839" authorityName="Guenée" authorityYear="1839" class="Insecta" family="Noctuidae" genus="Taeniocampa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Taeniocampa perforata" order="Lepidoptera" pageId="12" pageNumber="151" phylum="Arthropoda" rank="species" species="perforata">Taeniocampa perforata</taxonomicName>
