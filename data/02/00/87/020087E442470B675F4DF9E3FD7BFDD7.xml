<subSubSection box="[264,1262,1767,1793]" pageId="6" pageNumber="7" type="materials_examined">
<paragraph blockId="6.[264,1323,1767,1873]" box="[264,1262,1767,1793]" pageId="6" pageNumber="7">
<emphasis box="[264,414,1767,1792]" italics="true" pageId="6" pageNumber="7">
