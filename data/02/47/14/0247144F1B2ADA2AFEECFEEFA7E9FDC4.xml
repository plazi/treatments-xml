<paragraph blockId="3.[269,671,285,309]" box="[269,671,285,309]" pageId="3" pageNumber="704">
<heading box="[269,671,285,309]" centered="true" fontSize="9" level="2" pageId="3" pageNumber="704" reason="4">
<emphasis bold="true" box="[269,353,285,309]" pageId="3" pageNumber="704">GENUS</emphasis>
<taxonomicName authority="Wedmann &amp; Makarkin, 2007" authorityName="Wedmann &amp; Makarkin" authorityYear="2007" box="[362,543,285,309]" class="Insecta" family="Mantispidae" genus="Symphrasites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="704" phylum="Arthropoda" rank="genus" status="GEN. NOV.">SYMPHRASITES</taxonomicName>
<emphasis bold="true" box="[550,671,285,309]" pageId="3" pageNumber="704">
<taxonomicNameLabel box="[550,671,285,309]" pageId="3" pageNumber="704" rank="genus">GEN. NOV.</taxonomicNameLabel>
</emphasis>
