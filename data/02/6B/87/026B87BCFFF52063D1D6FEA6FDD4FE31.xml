<taxonomicName authorityName="Evenhuis" authorityYear="2016" box="[425,577,1408,1433]" class="Insecta" family="Strongylophthalmyiidae" genus="Strongylophthalmyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="133" phylum="Arthropoda" rank="species" species="malayensis">
