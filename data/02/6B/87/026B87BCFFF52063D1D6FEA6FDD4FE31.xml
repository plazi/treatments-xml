<taxonomicName authorityName="Hennig" authorityYear="1940" box="[506,667,1341,1366]" class="Insecta" family="Strongylophthalmyiidae" genus="Strongylophthalmyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="133" phylum="Arthropoda" rank="species" species="immaculata">
