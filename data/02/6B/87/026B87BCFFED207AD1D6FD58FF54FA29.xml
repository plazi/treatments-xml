</taxonomicName>
and 
<taxonomicName authority="Galinskaya &amp; Shatalkin, 2018" authorityName="Galinskaya &amp; Shatalkin" authorityYear="2018" box="[1341,1436,1539,1561]" class="Insecta" family="Strongylophthalmyiidae" genus="Strongylophthalmyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="125" phylum="Arthropoda" rank="species" species="strigosa" status="n. sp.">
