<location LSID="urn:lsid:plazi:treatment:0241110208415B55FF0AFCC38E07FEAA:8F37F6CF08435B55FABBFF218B95FEF6" municipality="Dr Soltesz" name="Hungarian Natural History Museum" pageId="3" pageNumber="440" stateProvince="Budapest">Hungarian Natural History Museum</location>
