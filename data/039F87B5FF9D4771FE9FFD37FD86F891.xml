<paragraph id="8B8936A3FF9D4771FE9FFD37FD98FCAC" blockId="89.[242,594,751,810]" box="[272,594,751,775]" pageId="89" pageNumber="188">
<heading id="D0C181CFFF9D4771FE9FFD37FD98FCAC" box="[272,594,751,775]" fontSize="10" level="2" pageId="89" pageNumber="188" reason="2">
<taxonomicName id="4C364D20FF9D4771FE9FFD37FDDBFCAD" authority="Kuschel, 2008" authorityName="Kuschel" authorityYear="2008" box="[272,529,751,774]" class="Insecta" family="Curculionidae" genus="Strabonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="89" pageNumber="188" phylum="Arthropoda" rank="species" species="albiscutum" status="n. sp.">
<emphasis id="B942EAB1FF9D4771FE9FFD37FDDBFCAD" bold="true" box="[272,529,751,774]" italics="true" pageId="89" pageNumber="188">Strabonus albiscutum</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27157CAFF9D4771FD96FD28FD98FCAC" box="[537,594,752,775]" pageId="89" pageNumber="188" rank="species">n. sp.</taxonomicNameLabel>
