<taxonomicName id="47DB4D4EFFBEFF8DFBFDF92D8C68F95C" authorityName="Rensberger" authorityYear="1983" box="[457,586,1666,1687]" class="Mammalia" family="Allomyidae" genus="Parallomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="25" pageNumber="794" phylum="Chordata" rank="genus">
