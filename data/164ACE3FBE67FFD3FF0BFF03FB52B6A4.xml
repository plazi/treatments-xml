<subSubSection id="D6F92CA2BE67FFD3FF0BFEF8FD9DB614" pageId="64" pageNumber="65" type="materials_examined">
<paragraph id="9E5C7F29BE67FFD3FF0BFEF8FDC7B470" blockId="64.[151,1437,293,1002]" box="[151,512,293,317]" pageId="64" pageNumber="65">
<emphasis id="AC97A33BBE67FFD3FF0BFEF8FEE3B470" bold="true" box="[151,292,293,317]" pageId="64" pageNumber="65">
<typeStatus id="4158C18BBE67FFD3FF0BFEF8FF15B470" box="[151,210,293,317]" pageId="64" pageNumber="65">Type</typeStatus>
