<taxonomicName id="4C6FDC97FFA9FFA4876F1C66C81FE123" authority="WILLMANN, 1928" authorityName="WILLMANN" authorityYear="1928" box="[522,813,1065,1087]" class="Arachnida" family="Brachychthoniidae" genus="Brachychthonius" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="1781" phylum="Arthropoda" rank="species" species="berlesei">
