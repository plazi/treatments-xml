<taxonomicName id="4C6B4D774C6C2B58D67C63A3FC7ADBC6" authority="Silva-Briano, Dieu &amp; Dumont, 1999" authorityName="Silva-Briano, Dieu &amp; Dumont" authorityYear="1999" box="[272,902,1159,1184]" class="Branchiopoda" family="Macrothricidae" genus="Macrothrix" kingdom="Animalia" order="Diplostraca" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="vietnamensis">
