<taxonomicName id="4C6B4D774C6C2B58D7FB6386FE08DB9D" authority="(Fischer)" baseAuthorityName="Fischer" box="[151,500,1196,1220]" class="Branchiopoda" family="Macrothricidae" genus="Macrothrix" kingdom="Animalia" order="Diplostraca" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="laticornis">
