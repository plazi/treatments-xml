<collectingCountry id="E8A79991FFFBFFC9FC72FEC1FC32FE89" box="[897,957,295,320]" name="Chile" pageId="5" pageNumber="6">Chile</collectingCountry>
(including 
<taxonomicName id="57B0A282FFFBFFC9FBBAFEC1FAB7FE89" authority="Metropolitan" authorityName="Metropolitan" box="[1097,1336,295,320]" class="Gastropoda" family="Cerithiidae" genus="Colina" kingdom="Animalia" pageId="5" pageNumber="6" phylum="Mollusca" rank="genus">Colina, Metropolitan</taxonomicName>
<location id="956F8FDAFFFBFFC9FAB7FEC1FA17FE89" LSID="urn:lsid:plazi:treatment:18196817FFFAFFC9FF64F9B0FB62FE19:956F8FDAFFFBFFC9FAB7FEC1FA17FE89" box="[1348,1432,295,320]" country="Chile" county="Meyen" latitude="-33.2" longLatPrecision="723" longitude="-70.68" municipality="Colina" name="Region" pageId="5" pageNumber="6" stateProvince="Santiago">Region</location>
, 
<geoCoordinate id="F584BFC6FFFBFFC9FF64FEAAFF7CFEAD" box="[151,243,332,356]" direction="south" orientation="latitude" pageId="5" pageNumber="6" precision="555" value="-33.2">33.20°S</geoCoordinate>
, 
<geoCoordinate id="F584BFC6FFFBFFC9FF0DFEAAFEE7FEAC" box="[254,360,332,357]" direction="west" orientation="longitude" pageId="5" pageNumber="6" precision="555" value="-70.68">70.68°W</geoCoordinate>
).
