<collectingCountry id="E8A79991FFFBFFC9FEEBFEC1FED7FE89" box="[280,344,295,320]" name="Chile" pageId="5" pageNumber="6">Chile</collectingCountry>
. 
<location id="956F8FDAFFFBFFC9FE9BFEC1FE1BFE89" LSID="urn:lsid:plazi:treatment:18196817FFFAFFC9FF64F9B0FB62FE19:956F8FDAFFFBFFC9FE9BFEC1FE1BFE89" box="[360,404,295,320]" country="Chile" county="Meyen" latitude="-33.2" longLatPrecision="723" longitude="-70.68" municipality="Colina" name="The" pageId="5" pageNumber="6" stateProvince="Santiago">The</location>
<typeStatus id="4F0B67A3FFFBFFC9FE6CFECEFE40FE89" box="[415,463,296,320]" pageId="5" pageNumber="6">type</typeStatus>
