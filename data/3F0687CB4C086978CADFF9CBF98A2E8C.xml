and 
<taxonomicName id="70AF4D5E4C076979C870FBC8F89E2C74" authorityName="Gorochov" authorityYear="1988" box="[568,727,1089,1113]" class="Insecta" family="Haglotettigoniidae" genus="Phyllotrella" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="species" species="planidorsalis">
