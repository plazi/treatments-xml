<emphasis id="85DBEACF4C076979C8ACFBCBF8BF2C74" bold="true" box="[740,758,1089,1113]" pageId="8" pageNumber="409">A</emphasis>
. 
<taxonomicName id="70AF4D5E4C076979C94BFBC8F9C12C74" authorityName="Gorochov" authorityYear="1988" box="[771,904,1089,1113]" class="Insecta" family="Haglotettigoniidae" genus="Zvenella" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="species" species="geniculata">
<emphasis id="85DBEACF4C076979C94BFBC8F9C12C74" box="[771,904,1089,1113]" italics="true" pageId="8" pageNumber="409">Z. geniculata</emphasis>
</taxonomicName>
