5. 
<taxonomicName id="83A234FAFA0D4FC5E81D566AB56135DD" LSID="7BA57CE8-E1BF-50FD-A5B8-74973727F170" class="Insecta" family="Baetidae" genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis roulade" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="roulade">Labiobaetis roulade</taxonomicName>
