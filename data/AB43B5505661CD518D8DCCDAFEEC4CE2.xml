<taxonomicName id="E4EA7FC55661CD518DD5CCDAFE6C4E88" authorityName="Gauld" authorityYear="1991" box="[207,405,402,428]" class="Insecta" family="Ichneumonidae" genus="Clistopyga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="328" phylum="Arthropoda" rank="species" species="eldae">
