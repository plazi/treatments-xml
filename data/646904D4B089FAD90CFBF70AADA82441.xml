<taxonomicName id="1B8E107C68A687157C9E42AC88196F83" class="Liliopsida" family="Marantaceae" genus="Calathea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Calathea" order="Zingiberales" pageId="206" pageNumber="207" phylum="Tracheophyta" rank="genus">Calathea</taxonomicName>
sp. (
<taxonomicName id="3FF9E3D81537CE30CAA7AEC8D4F2A0B4" genus="Marantaceae" lsidName="Marantaceae" pageId="206" pageNumber="207" rank="genus">Marantaceae</taxonomicName>
).
</paragraph>
</subSubSection>
