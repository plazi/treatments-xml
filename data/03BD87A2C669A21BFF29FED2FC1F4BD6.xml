<taxonomicName id="4C144D37C669A21BFBFFFD49FA7641BC" box="[1031,1433,723,762]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="31" pageNumber="258" phylum="Chordata" rank="species" species="ridleyiwas">
