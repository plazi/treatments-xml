<taxonomicName id="C5C38D97646D1231FCB5FBF0FB88FB9A" box="[895,1052,1106,1130]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="393" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
