<subSubSection id="538D38D1B83FFE87CB92FB59FBC23D0F" box="[199,1069,1260,1284]" pageId="282" pageNumber="283" type="etymology">
<paragraph id="1B286B5AB83FFE87CB92FB59FBC23D0F" blockId="282.[151,1436,1187,1896]" box="[199,1069,1260,1284]" pageId="282" pageNumber="283">
<emphasis id="29E3B748B83FFE87CB92FB59FEA13D0F" bold="true" box="[199,334,1260,1284]" inLineHeading="true" pageId="282" pageNumber="283" reason="1">Etymology.</emphasis>
