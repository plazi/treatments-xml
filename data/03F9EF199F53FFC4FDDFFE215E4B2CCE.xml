<taxonomicName id="4C50258C9F53FFC7FC3FF9C95E4B2830" authority="Champion, 1924" authorityName="Champion" authorityYear="1924" box="[989,1399,1576,1603]" class="Insecta" family="Limnichidae" genus="Acontosceles" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="hydroporoides">
