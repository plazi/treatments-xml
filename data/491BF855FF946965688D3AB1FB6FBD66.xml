<collectingCountry id="B9A509D3FF9469656ABA39EBFD3EBAD2" box="[672,742,973,998]" name="China" pageId="11" pageNumber="512">China</collectingCountry>
, 
<collectingCountry id="B9A509D3FF9469656AEE39EBFCB2BAD2" box="[756,874,973,998]" name="Cambodia" pageId="11" pageNumber="512">Cambodia</collectingCountry>
, 
<collectingCountry id="B9A509D3FF9469656B6239E8FBD9BAD2" box="[888,1025,974,998]" name="Hong Kong" pageId="11" pageNumber="512">Hong Kong</collectingCountry>
