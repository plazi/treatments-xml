<collectingCountry id="B9A509D3FF9469656C1439E8FB9FBAD1" box="[1038,1095,974,997]" name="Laos" pageId="11" pageNumber="512">Laos</collectingCountry>
, 
<collectingCountry id="B9A509D3FF9469656C4F39E8FB7CBAD1" box="[1109,1188,974,997]" name="Macao" pageId="11" pageNumber="512">Macao</collectingCountry>
