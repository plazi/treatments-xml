<collectingCountry id="B9A509D3FF9469656CA839EBFAC4BAD2" box="[1202,1308,973,998]" name="Malaysia" pageId="11" pageNumber="512">Malaysia</collectingCountry>
, 
<collectingCountry id="B9A509D3FF9469656D3039E8FA41BAD2" box="[1322,1433,974,998]" name="Myanmar" pageId="11" pageNumber="512">Myanmar</collectingCountry>
, 
<collectingCountry id="B9A509D3FF946965688D39D7FEC0BD3E" box="[151,280,1009,1034]" name="Philippines" pageId="11" pageNumber="512">Philippines</collectingCountry>
, 
