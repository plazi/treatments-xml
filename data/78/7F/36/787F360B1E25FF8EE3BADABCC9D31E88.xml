<location LSID="urn:lsid:plazi:treatment:787F360B1E25FF8EE3BADABCC9D31E88:F509D1C61E25FF8FE36CD9CCCCBF1CF5" box="[327,487,1671,1695]" name="Tanala Forest" pageId="8" pageNumber="1102">Tanala Forest</location>
[Fort-Carnot] (
<collectionCode box="[665,762,1671,1695]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="8" pageNumber="1102">MNHN</collectionCode>
