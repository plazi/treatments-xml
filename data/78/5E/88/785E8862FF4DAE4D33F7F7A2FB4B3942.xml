<subSubSection box="[437,810,722,747]" pageId="20" pageNumber="1098" type="nomenclature">
<paragraph blockId="20.[437,810,722,747]" box="[437,810,722,747]" pageId="20" pageNumber="1098">
<heading box="[437,810,722,747]" centered="true" fontSize="36" level="2" pageId="20" pageNumber="1098" reason="7">
<taxonomicName authority="Silvestri, 1909" authorityName="Silvestri" authorityYear="1909" box="[437,810,722,747]" class="Insecta" family="Rhinotermitidae" genus="Schedorhinotermes" kingdom="Animalia" order="Blattodea" pageId="20" pageNumber="1098" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[437,646,722,747]" italics="true" pageId="20" pageNumber="1098">Schedorhinotermes</emphasis>
<bibRefCitation author="Silvestri F" box="[653,810,723,747]" pageId="20" pageNumber="1098" pagination="279 - 314" refId="ref12059" refString="Silvestri F. 1909. Isoptera. In: Michaelsen W, Hartmeyer R, editors. Die Fauna sudwest- Australiens, 2 (17), Jena. p. 279 - 314." type="book chapter" year="1909">Silvestri, 1909</bibRefCitation>
