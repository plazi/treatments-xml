<typeStatus box="[977,1025,799,823]" pageId="5" pageNumber="348">type</typeStatus>
locality, 
<taxonomicName box="[1127,1210,799,823]" class="Arachnida" family="Mysmenidae" genus="Mysmenopsis" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="366" phylum="Arthropoda" rank="species" species="otonga" status="n. sp.">Otonga</taxonomicName>
