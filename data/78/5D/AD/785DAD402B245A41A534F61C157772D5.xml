as 
<taxonomicName class="Insecta" family="Psychodidae" genus="Telmatoscopus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Telmatoscopus" order="Diptera" pageId="0" pageNumber="15" phylum="Arthropoda" rank="genus">
