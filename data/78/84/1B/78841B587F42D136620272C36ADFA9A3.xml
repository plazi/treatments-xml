<taxonomicName class="Mammalia" family="Muridae" genus="Rattus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rattus norvegicus" order="Rodentia" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="norvegicus">Rattus norvegicus</taxonomicName>
