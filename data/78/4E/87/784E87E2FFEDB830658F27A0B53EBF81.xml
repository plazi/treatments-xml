<date box="[1290,1432,1108,1134]" pageId="13" pageNumber="219" value="1947-08-22">
<collectingDate box="[1290,1432,1108,1134]" pageId="13" pageNumber="219" value="1947-08-22">22.VIII 1947</collectingDate>
</date>
</emphasis>
