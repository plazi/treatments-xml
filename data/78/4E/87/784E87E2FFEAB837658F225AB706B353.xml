<materialsCitation accessionNumber="SOA 47199" collectingDate="1968-08-11" collectionCode="SOA" country="Bulgaria" location="Cheshmedjiev" pageId="10" pageNumber="216" specimenCount="1" stateProvince="Plovdiv" typeStatus="lectotype">
