</taxonomicName>
” = 
<taxonomicName authority="(Lyon 1907)" baseAuthorityName="Lyon" baseAuthorityYear="1907" box="[703,1035,150,176]" class="Mammalia" family="Spalacidae" genus="Eospalax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="106" phylum="Chordata" rank="species" species="cansus">
