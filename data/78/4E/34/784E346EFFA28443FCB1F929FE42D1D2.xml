as 
<emphasis bold="true" box="[432,446,150,176]" pageId="4" pageNumber="106">“</emphasis>
<taxonomicName box="[446,662,150,176]" class="Mammalia" family="Muridae" genus="Myospalax" kingdom="Animalia" order="Rodentia" pageId="4" pageNumber="106" phylum="Chordata" rank="species" species="scansus">
