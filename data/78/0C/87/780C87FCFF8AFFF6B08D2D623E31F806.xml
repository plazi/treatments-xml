<collectionCode box="[815,902,1399,1425]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="3" pageNumber="592" type="Museum">USNM</collectionCode>
