<taxonomicName authority="(Stebbing, 1888)" baseAuthorityName="Stebbing" baseAuthorityYear="1888" box="[839,1165,1617,1641]" class="Liliopsida" family="Amaryllidaceae" genus="Amaryllis" kingdom="Plantae" order="Asparagales" pageId="70" pageNumber="199" phylum="Tracheophyta" rank="species" species="haswelli">
