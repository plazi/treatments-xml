1. 
<normalizedToken originalValue="Ähren" pageId="null" pageNumber="409">Aehren</normalizedToken>
10-30 mm lang; 
<normalizedToken originalValue="Blütenstand" pageId="null" pageNumber="409">Bluetenstand</normalizedToken>
bis 6 cm lang; die 
<normalizedToken originalValue="größten" pageId="null" pageNumber="409">groessten</normalizedToken>
<normalizedToken originalValue="Hochblätter" pageId="null" pageNumber="409">Hochblaetter</normalizedToken>
viel 
<normalizedToken originalValue="länger" pageId="null" pageNumber="409">laenger</normalizedToken>
als der 
<normalizedToken originalValue="Blütenstand" pageId="null" pageNumber="409">Bluetenstand</normalizedToken>
</td>
<td align="left" pageId="null" pageNumber="409" valign="bottom">
<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Scirpus" kingdom="Plantae" order="Poales" pageId="null" pageNumber="409" phylum="Tracheophyta" rank="species" species="maritimus">
