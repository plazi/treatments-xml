<quantity box="[1114,1304,1125,1151]" metricMagnitude="-4" metricUnit="m" metricValue="1.035" metricValueMax="1.33" metricValueMin="0.74" pageId="17" pageNumber="64" unit="mm" value="0.1035" valueMax="0.133" valueMin="0.074">
