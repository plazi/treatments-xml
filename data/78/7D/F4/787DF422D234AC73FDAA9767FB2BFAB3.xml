<geoCoordinate box="[1177,1254,1125,1151]" degrees="0.133" direction="west" orientation="longitude" pageId="17" pageNumber="64" precision="55" value="-0.133">–0.133</geoCoordinate>
mm
</quantity>
in diameter (n=25, avg: 
<quantity box="[288,404,1161,1187]" metricMagnitude="-5" metricUnit="m" metricValue="9.0" pageId="17" pageNumber="64" unit="mm" value="0.09">0.090 mm</quantity>
, std: 
<quantity box="[464,582,1161,1187]" metricMagnitude="-5" metricUnit="m" metricValue="1.3" pageId="17" pageNumber="64" unit="mm" value="0.013">0.013 mm</quantity>
), the primary rosette is 
<quantity box="[846,1038,1161,1187]" metricMagnitude="-5" metricUnit="m" metricValue="1.45" metricValueMax="1.9" metricValueMin="1.0" pageId="17" pageNumber="64" unit="mm" value="0.014499999999999999" valueMax="0.019" valueMin="0.01">
