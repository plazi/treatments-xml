<quantity box="[151,343,1715,1741]" metricMagnitude="-5" metricUnit="m" metricValue="1.15" metricValueMax="2.0" metricValueMin="0.3" pageId="15" pageNumber="62" unit="mm" value="0.0115" valueMax="0.02" valueMin="0.003">
