mm
</quantity>
long (n=19, avg: 
<quantity box="[1126,1243,261,287]" metricMagnitude="-5" metricUnit="m" metricValue="3.0" pageId="25" pageNumber="72" unit="mm" value="0.03">0.030 mm</quantity>
, std: 
<quantity box="[1305,1424,260,287]" metricMagnitude="-6" metricUnit="m" metricValue="5.0" pageId="25" pageNumber="72" unit="mm" value="0.005">0.005 mm</quantity>
