<taxonomicName id="4C234D543D4FFF84FF689F31FD3FFD0A" authority="Marcus 1936" authorityName="Marcus" authorityYear="1936" box="[151,698,631,657]" class="Eutardigrada" family="Hypsibiidae" genus="Diphascon" kingdom="Animalia" order="Parachela" pageId="17" pageNumber="18" phylum="Tardigrada" rank="species" species="pingue" subGenus="Diphascon">
<emphasis id="B957EAC53D4FFF84FF689F31FD3FFD0A" bold="true" box="[151,698,631,657]" pageId="17" pageNumber="18">
<emphasis id="B957EAC53D4FFF84FF689F31FD95FD0A" bold="true" box="[151,528,631,657]" italics="true" pageId="17" pageNumber="18">Diphascon (Diphascon) pingue</emphasis>
<bibRefCitation id="EFB24B263D4FFF84FDE79F31FD3FFD0A" author="Marcus" box="[536,698,631,657]" pageId="17" pageNumber="29" refString="Marcus, E. (1936) Tardigrada. Das tierreich, Vol. 66. W. D. Gruyter. Berlin and Leipzig." type="book" year="1936">
Marcus 
<number id="3DEB24F03D4FFF84FD809F31FD3FFD0A" box="[639,698,631,657]" pageId="17" pageNumber="18" value="1936.0">1936</number>
</bibRefCitation>
