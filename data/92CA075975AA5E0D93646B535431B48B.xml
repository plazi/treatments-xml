<paragraph id="EAA8C0E871D4C7B6DF332595BFFE56A8" pageId="0" pageNumber="1">
<taxonomicName id="840340836E4B03985CF8EBA23CAFDF7D" authorityName="Tomlin" authorityYear="1948" class="Gastropoda" family="Cyclophoridae" genus="Alycaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Alycaeus balingensis" order="Gastropoda" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="balingensis">Alycaeus balingensis</taxonomicName>
