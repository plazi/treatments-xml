<taxonomicName id="D24F7C8DD79EF6E1AFFDDE17D95B7263" class="Insecta" family="Cerambycidae" genus="Glenea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glenea" order="Coleoptera" pageId="216" pageNumber="217" phylum="Arthropoda" rank="genus">Glenea</taxonomicName>
