<emphasis id="F1B8184FFFB4EE0A5158E34EFBF6FAF4" box="[828,1125,1360,1383]" italics="true" pageId="3" pageNumber="594">
<taxonomicName id="04CCBFDEFFB4EE0A5158E34EFC53FAF4" box="[828,960,1360,1383]" class="Aves" family="Psittacidae" genus="Cyanopsitta" kingdom="Animalia" order="Psittaciformes" pageId="3" pageNumber="594" phylum="Chordata" rank="genus">Cyanopsitta</taxonomicName>
