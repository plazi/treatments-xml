<taxonomicName id="04CCBFDEFFB4EE0A56FCE34EFA92FAF4" box="[1176,1281,1360,1383]" class="Aves" family="Psittacidae" genus="Primolius" kingdom="Animalia" order="Psittaciformes" pageId="3" pageNumber="594" phylum="Chordata" rank="genus">
