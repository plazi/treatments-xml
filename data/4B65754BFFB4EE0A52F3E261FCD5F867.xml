<taxonomicName id="04CCBFDEFFB4EE0A51AEE34EFBF2FAF4" box="[970,1121,1360,1383]" class="Aves" family="Psittacidae" genus="Orthopsittaca" kingdom="Animalia" order="Psittaciformes" pageId="3" pageNumber="594" phylum="Chordata" rank="genus">Orthopsittaca</taxonomicName>
