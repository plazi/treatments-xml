<taxonomicName id="EE05E4596AFD6B3505AEFE1C3717600C" authority="Reuter" class="Magnoliopsida" family="Orobanchaceae" genus="Orobanche" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="259" phylum="Tracheophyta" rank="species" species="laserpitii-sileris">
