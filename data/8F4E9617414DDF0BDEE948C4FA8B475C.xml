<collectingRegion id="C523E9E3414DDF0BDEE949DCFF5C44CC" box="[136,222,1416,1442]" country="China" name="Yunnan" pageId="4" pageNumber="42">Yunnan</collectingRegion>
as 
<typeStatus id="D85C99A3414DDF0BDF6749DCFEF744CC" box="[262,373,1416,1442]" pageId="4" pageNumber="42">paratypes</typeStatus>
