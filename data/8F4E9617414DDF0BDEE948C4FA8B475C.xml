<taxonomicName id="C0E75C82414DDF0BDD134A4CFA87475C" authority="Lecomte (1913: 84)" authorityName="Lecomte" authorityPageNumber="84" authorityYear="1913" box="[882,1285,1560,1586]" class="Magnoliopsida" family="Lauraceae" genus="Litsea" kingdom="Plantae" order="Laurales" pageId="4" pageNumber="42" phylum="Tracheophyta" rank="species" species="moupinensis">
