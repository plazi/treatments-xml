<superScript id="A5F49BDAFFAC985BF2210461296DF6A8" attach="left" box="[288,297,1916,1928]" fontSize="5" pageId="9" pageNumber="376">st</superScript>
instar. (25) 2 
<superScript id="A5F49BDAFFAC985BF2B704662980F6A8" attach="left" box="[438,452,1915,1928]" fontSize="5" pageId="9" pageNumber="376">nd</superScript>
instar. (26) 3 
<superScript id="A5F49BDAFFAC985BF15004662A18F6A8" attach="left" box="[593,604,1915,1928]" fontSize="5" pageId="9" pageNumber="376">rd</superScript>
instar. (27) 4 
<superScript id="A5F49BDAFFAC985BF1E804662AB0F6A8" attach="left" box="[745,756,1915,1928]" fontSize="5" pageId="9" pageNumber="376">th</superScript>
instar. (28) 5 
<superScript id="A5F49BDAFFAC985BF08004662BC8F6A8" attach="left" box="[897,908,1915,1928]" fontSize="5" pageId="9" pageNumber="376">th</superScript>
