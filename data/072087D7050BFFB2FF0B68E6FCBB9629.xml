<geoCoordinate id="EABD5006050BFFB2FCF76F9EFC3E94E5" box="[875,1014,1195,1222]" degrees="29" direction="north" minutes="45" orientation="latitude" pageId="13" pageNumber="66" precision="15" seconds="11" value="29.753056">29°45’11”N</geoCoordinate>
