3. 
<taxonomicName id="A221B01FEF07A270F5D24762A042CC57" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29976" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Leptogenys" kingdom="Animalia" lsidName-HNS="Leptogenys incisa Forel" order="Hymenoptera" pageNumber="113" phylum="Arthropoda" rank="subGenus" species="incisa" status="n. sp.">LEPTOGENYS INCISA</taxonomicName>
