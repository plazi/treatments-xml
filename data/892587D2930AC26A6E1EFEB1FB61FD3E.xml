<taxonomicName id="C68C4D479309C26A6B85FD99FE8AFD8D" class="Insecta" family="Ichneumonidae" genus="Amazopimpla" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="matogrossoensis">
