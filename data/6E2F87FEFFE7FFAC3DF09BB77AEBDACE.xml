<quantity id="217E9B0DFFE7FFAC3CC19C1478CCDB5A" box="[422,511,1591,1615]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="25" pageNumber="394" unit="mm" value="3.1">3.1 mm</quantity>
, penultimate flagellomere 
