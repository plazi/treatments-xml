<bibRefCitation id="D58C7BF84F7A1524E84A30B2A0B31A18" author="Pateff, P." journalOrPublisher="Arbeiten der Bulgarischen Naturforschenden Gesellschaft" pageId="0" pageNumber="25295" pagination="1 - 46" title="Kenntnis der Suesswasserrhizopoden von Bulgarien" volume="11" year="1924">Pateff 1924</bibRefCitation>
, 
<bibRefCitation id="4D059A27B05A80A2AB14FE639328CD62" author="Golemansky, V." journalOrPublisher="Bulletin de l'Institut de Zoologie et Musee de Sofia" pageId="0" pageNumber="25295" pagination="171 - 187" title="On the species composition and distribution of the rhizopod fauna of upper Smolian Lakes" volume="27" year="1968">Golemansky 1968</bibRefCitation>
, 
<bibRefCitation id="E95406EAAD380B57B7FC600CE9F4EA79" author="Golemansky, V." editor="Beron, P." journalOrPublisher="Pensoft &amp; National Museumof Natural Histoty, Sofia" pageId="0" pageNumber="25295" title="Biodiversity of Bulgaria. 3. Biodiversity of Western Rhodopes (Bulgaria and Greece) I" year="2006">Golemansky et al. 2006</bibRefCitation>
); Rila Mt. (
<bibRefCitation id="07A157FFCDEBDC8EC0D82B5D3B0BB83E" author="Golemansky, V." journalOrPublisher="Acta Zoologica Bulgarica" pageId="0" pageNumber="25295" pagination="3 - 9" title="Testate amoebae (Protozoa, Rhizopoda) in the watercatchment area and littoral of the &quot; Beli Iskar &quot; dam" volume="46" year="1993">Golemansky and Todorov 1993</bibRefCitation>
, 
<bibRefCitation id="4C28E3228021C469FE3D6660D4B17038" author="Todorov, M." editor="Golemansky, V." journalOrPublisher="Professor Marin Drinov Academic PublIshing House, Sofia" pageId="0" pageNumber="25295" title="Biodiversity and Evolution of Glacial Water Ecosystems in the Rila Mountains" year="2000">Todorov and Golemansky 2000</bibRefCitation>
, 
<bibRefCitation id="488AA82EB269667CA2E1A89495D11D42" author="Todorov, M." journalOrPublisher="Acta Zoologica Bulgarica" pageId="0" pageNumber="25295" pagination="243 - 252" title="Testate amoebae (Protozoa, Rhizopoda) of the Ribni Ezera Glacial Lakes in the Rila Mountains (South-West Bulgaria)" volume="56" year="2004">Todorov 2004</bibRefCitation>
, 
<bibRefCitation id="7E8D557290087CC6380B36ACBB8EBB16" author="Todorov, M." journalOrPublisher="Acta Zoologica Bulgarica" pageId="0" pageNumber="25295" pagination="13 - 23" title="Testate amoebae (Protozoa, Rhizopoda) of the glacial lakes Smradlivo ezero in the Rila National Park (Southwestern Bulgaria)" volume="57" year="2005">Todorov 2005</bibRefCitation>
