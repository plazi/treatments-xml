<collectionCode id="7A86AE13FFF3FFABFEE869D9FE85F569" box="[290,311,240,256]" pageId="21" pageNumber="26">IN</collectionCode>
<collectionCode id="7A86AE13FFF3FFABFEF469D9FED3F569" box="[318,353,240,256]" country="France" name="University of Montpellier" pageId="21" pageNumber="26">MM</collectionCode>
