<paragraph id="98BC29C20B8F2AD76F8D0803D7E97D4B" pageId="0" pageNumber="187">
<taxonomicName id="9F1C4936D4496C3068F062AE030C9E42" authority="" authorityName="Konow" authorityYear="1903" class="Insecta" family="Tenthredinidae" genus="Pteronus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pteronus pallens" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="pallens">
<emphasis id="B943E2163F82BB1EDC17C3CCCDDB5478" italics="true" pageId="0" pageNumber="187">Pteronus pallens</emphasis>
</taxonomicName>
