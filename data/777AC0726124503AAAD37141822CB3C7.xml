<subSubSection id="3DB939531C306FC30E18E94F4701F6D6" pageId="0" pageNumber="187" type="reference_group">
<paragraph id="409A1E212BFB842377B516FF428EA15F" pageId="0" pageNumber="187">
<taxonomicName id="EE2D7520EAFD046414BC398D55A1DA55" authority="" authorityName="Konow" authorityYear="1903" class="Insecta" family="Tenthredinidae" genus="Pteronus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pteronus pallens" order="Hymenoptera" pageId="0" pageNumber="187" phylum="Arthropoda" rank="species" species="pallens">Pteronus pallens</taxonomicName>
