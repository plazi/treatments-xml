<heading id="4C6081A75605FFFDE89AFA1EFD98FA6D" bold="true" box="[151,531,1434,1460]" fontSize="11" level="1" pageId="43" pageNumber="44" reason="1">
<emphasis id="25E3EAD95605FFFDE89AFA1EFD98FA6D" bold="true" box="[151,531,1434,1460]" pageId="43" pageNumber="44">
<taxonomicName id="D0974D485605FFFDE89AFA1EFEFEFA6D" box="[151,373,1434,1460]" class="Insecta" family="Philopotamidae" genus="Chimarra" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="lavensis" status="new species">
<emphasis id="25E3EAD95605FFFDE89AFA1EFEFEFA6D" bold="true" box="[151,373,1434,1460]" italics="true" pageId="43" pageNumber="44">Chimarra lavensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="3ED057A25605FFFDE98EFA1EFD98FA6D" box="[387,531,1434,1460]" pageId="43" pageNumber="44" rank="species">new species</taxonomicNameLabel>
</emphasis>
