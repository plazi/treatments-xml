<paragraph id="B08EB04AB16752E38134439D49A1CCF8" pageId="1" pageNumber="86">
<taxonomicName id="128554A8B6BC56C5C7F71609B5F3ACDD" authority="Meyrick, 1897" authorityName="Meyrick" authorityYear="1897" class="Insecta" family="Oecophoridae" genus="Hieromantis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hieromantis" order="Lepidoptera" pageId="1" pageNumber="86" phylum="Arthropoda" rank="genus">Hieromantis Meyrick, 1897</taxonomicName>
