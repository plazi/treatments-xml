<taxonomicName id="30CF2932FF585FDBA2117902ED1FEBDE" authority="(L.) Tausch" class="Gymnolaemata" family="Cymuloporidae" genus="Crepis" kingdom="Animalia" order="Cheilostomatida" pageId="null" pageNumber="590" phylum="Bryozoa" rank="species" species="praemorsa">
