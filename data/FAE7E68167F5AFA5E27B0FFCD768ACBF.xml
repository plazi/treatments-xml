<taxonomicName id="6E87A62F589F513ACCFC1B1A27E0D097" authorityName="Hopkins" authorityYear="1915" class="Insecta" family="Curculionidae" genus="Webbia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Webbia" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="147570E011B37F28D0EEC14E44CB2088" italics="true" pageId="0" pageNumber="1">Webbia</emphasis>
</taxonomicName>
, 
<taxonomicName id="EBB65470C9AE9E18D2153A68DC5045B1" authorityName="Reitter" authorityYear="1913" class="Insecta" family="Curculionidae" genus="Xylosandrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xylosandrus" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="B15165AB68B0185CE41A8E72C018A8DD" italics="true" pageId="0" pageNumber="1">Xylosandrus</emphasis>
</taxonomicName>
