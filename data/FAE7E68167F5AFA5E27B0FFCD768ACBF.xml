<taxonomicName id="7667EAC2607357547AD6B51524AA2D75" authorityName="Hagedorn" authorityYear="1912" class="Insecta" family="Curculionidae" genus="Cyclorhipidion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyclorhipidion" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="AE9CA6250FE264C8E77C7566502F2388" italics="true" pageId="0" pageNumber="1">Cyclorhipidion</emphasis>
</taxonomicName>
, 
<taxonomicName id="7213CAD24112E1B9180D98C06BD85A59" authorityName="Browne" authorityYear="1961" class="Insecta" family="Curculionidae" genus="Pseudowebbia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudowebbia" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="390CEFC2F6B7F7E55E2AA358C02A5942" italics="true" pageId="0" pageNumber="1">Pseudowebbia</emphasis>
</taxonomicName>
, 
<taxonomicName id="9EA6406E4D04F09A0EDC73E9E6B69062" authority="Hulcr &amp; Cognato, 2010" authorityName="Hulcr &amp; Cognato" authorityYear="2010" class="Insecta" family="Curculionidae" genus="Truncaudum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Truncaudum" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="C4A530415B22F1A6183A3B17BDE13620" italics="true" pageId="0" pageNumber="1">Truncaudum</emphasis>
</taxonomicName>
