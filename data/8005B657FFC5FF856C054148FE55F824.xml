<paragraph id="08130741FFC5FF856EF942A2FBE0FCD1" blockId="6.[189,1398,737,799]" pageId="6" pageNumber="7">
<taxonomicName id="CFAC7CC2FFC5FF856EF942A2F854FD34" authority="Wichard &amp; Espeland &amp; Müller &amp; Wang, 2020" authorityName="Wichard &amp; Espeland &amp; Müller &amp; Wang" authorityYear="2020" box="[189,517,737,763]" class="Insecta" family="Odontoceridae" genus="Palaeopsilotreta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="burmanica" status="sp. nov.">
<emphasis id="3AD8DB53FFC5FF856EF942A2F854FD34" box="[189,517,737,763]" italics="true" pageId="6" pageNumber="7">Palaeopsilotreta burmanica</emphasis>
</taxonomicName>
<taxonomicNameLabel id="21EB6628FFC5FF856C4F42A1F835FD33" box="[523,612,738,764]" pageId="6" pageNumber="7" rank="species">sp. nov.</taxonomicNameLabel>
