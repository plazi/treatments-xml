<taxonomicName id="4C4A2EB3FF9AFFA6FEC67C7A009F814D" authorityName="He" authorityYear="2021" box="[341,471,295,321]" class="Insecta" family="Gryllidae" genus="Parapentacentrus" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="146" phylum="Arthropoda" rank="species" species="brevipennis">
