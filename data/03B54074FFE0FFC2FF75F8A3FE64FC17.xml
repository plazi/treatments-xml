<taxonomicName id="4C1C8AE1FFE3FFC3FAFFF88AFE03F8FD" authority="Staeger, 1839" authorityName="Staeger" authorityYear="1839" class="Insecta" family="Ceratopogonidae" genus="Ceratopogon" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="419" phylum="Arthropoda" rank="species" species="pictipennis">
