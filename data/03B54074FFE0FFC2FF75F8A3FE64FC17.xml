<taxonomicName id="4C1C8AE1FFE3FFC3FD34F88AFC37F882" authority="Kieffer, 1922" authorityName="Kieffer" authorityYear="1922" box="[726,959,1819,1841]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="419" phylum="Arthropoda" rank="species" species="saevus">
<emphasis id="B9682D70FFE3FFC3FD34F88AFD6EF882" box="[726,742,1819,1841]" italics="true" pageId="6" pageNumber="419">C</emphasis>
