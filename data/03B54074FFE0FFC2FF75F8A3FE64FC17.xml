<taxonomicName id="4C1C8AE1FFE3FFC3FE57F88AFD38F882" authority="Kieffer, 1923" authorityName="Kieffer" authorityYear="1923" box="[437,688,1819,1841]" class="Insecta" family="Ceratopogonidae" genus="Ceratopogon" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="419" phylum="Arthropoda" rank="species" species="sahariensis">
