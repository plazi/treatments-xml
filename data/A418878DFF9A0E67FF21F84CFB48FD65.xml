<collectingCountry id="54A6760BFF990E67FC9EFF2EFCAEFF6D" box="[808,892,225,251]" name="Turkey" pageId="6" pageNumber="89">Turkey</collectingCountry>
; 
<collectingRegion id="EE75F879FF990E67FC30FF2FFC16FF6D" box="[902,964,224,251]" country="Turkey" name="Izmir" pageId="6" pageNumber="89">İzmir</collectingRegion>
