<geoCoordinate id="4985505CFF990E67FE50FECAFDB4FE89" box="[486,614,260,287]" degrees="27" direction="east" minutes="25.41" orientation="longitude" pageId="6" pageNumber="89" precision="9" value="27.4235">E 27°25.41</geoCoordinate>
, 
<specimenCount id="3AB7FD12FF990E67FDC6FECBFD6CFE89" box="[624,702,260,287]" pageId="6" pageNumber="89" type="generic">
<quantity id="EB499B7EFF990E67FDC6FECBFD68FE89" box="[624,698,260,287]" metricMagnitude="2" metricUnit="m" metricValue="3.5" pageId="6" pageNumber="89" unit="m" value="350.0">350 m</quantity>
