<specimenCount id="3AB7FD12FF990E67FE4CFE82FD96FEF1" box="[506,580,333,359]" pageId="6" pageNumber="89" type="generic">
<quantity id="EB499B7EFF990E67FE4CFE82FD92FEF1" box="[506,576,333,359]" metricMagnitude="2" metricUnit="m" metricValue="1.3" pageId="6" pageNumber="89" unit="m" value="130.0">130m</quantity>
.
</specimenCount>
, 
<date id="580F105BFF990E67FDF9FE82FD09FEF1" box="[591,731,333,359]" pageId="6" pageNumber="89" value="2018-07-01">01.VII.2018</date>
