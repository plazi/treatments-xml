<taxonomicName id="C7F14D37FFFC8A1AF6ADEEE8F583C813" authorityName="Temminck" authorityYear="1834" box="[2401,2566,4425,4464]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="51" pageNumber="330" phylum="Chordata" rank="species" species="trifoliatus">
