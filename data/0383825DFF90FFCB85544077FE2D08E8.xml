<paragraph id="8B95334BFF90FFC984B141D7FCBD0FB5" blockId="25.[714,1247,1447,1666]" box="[714,859,1447,1473]" pageId="25" pageNumber="234">DESCRIPTION</paragraph>
<paragraph id="8B95334BFF90FFC984B141B8FB380C88" blockId="25.[714,1247,1447,1666]" pageId="25" pageNumber="234">
Chez 
<taxonomicName id="4C2A48C8FF90FFC9857741B7FC9C0FE8" baseAuthorityName="Rang" baseAuthorityYear="1828" box="[780,890,1479,1506]" class="Gastropoda" family="Creseidae" genus="Creseis" kingdom="Animalia" order="Pteropoda" pageId="25" pageNumber="234" phylum="Mollusca" rank="species" species="virgula">
