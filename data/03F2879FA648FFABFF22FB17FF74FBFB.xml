<taxonomicName id="4C5B4D0AA646FFAAFEE2F874FD2EF814" authority="Spaeth." authorityName="Spaeth." box="[343,641,1953,1975]" class="Insecta" family="Chrysomelidae" genus="Herminella" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="265" phylum="Arthropoda" rank="species" species="marshalli">
