<taxonomicName id="4C3B4D44FF9AFFB104EFF80FFD529285" authority="Fr." authorityName="Fr." box="[453,698,1994,2019]" class="Agaricomycetes" family="Cantharellaceae" genus="Hydnum" kingdom="Fungi" order="Cantharellales" pageId="17" pageNumber="250" phylum="Basidiomycota" rank="species" species="scabrosum">
