<taxonomicName id="2DFE4D65C318CD39FA5FF9A956AF26FD" authorityName="El-Hawagry" authorityYear="1999" box="[271,464,1650,1673]" class="Insecta" family="Bombyliidae" genus="Anthrax" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="610" phylum="Arthropoda" rank="species" species="greatheadi">
