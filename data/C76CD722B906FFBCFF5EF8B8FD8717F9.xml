, PL 
<quantity id="883DCBD1B907FFBCFD90FD50FD1A17A7" box="[601,709,674,700]" metricMagnitude="-3" metricUnit="m" metricValue="1.12" pageId="11" pageNumber="39" unit="mm" value="1.12">1.12 mm</quantity>
, PW 
<quantity id="883DCBD1B907FFBCFCCAFD50FCB017A7" box="[771,879,674,700]" metricMagnitude="-3" metricUnit="m" metricValue="1.92" pageId="11" pageNumber="39" unit="mm" value="1.92">1.92 mm</quantity>
, HW 
<quantity id="883DCBD1B907FFBCFC7BFD50FBC117A7" box="[946,1054,674,700]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="11" pageNumber="39" unit="mm" value="2.2">2.20 mm</quantity>
