<paragraph id="8BADAA53FFECFFA1FF6588A9FEFFFBEA" blockId="24.[189,1399,969,1066]" pageId="24" pageNumber="25">
<taxonomicName id="4C12D1D0FFECFFA1FF6588A9FDB7FC23" baseAuthorityName="Flossner &amp; Frey" baseAuthorityYear="1970" box="[189,531,969,995]" class="Branchiopoda" family="Chydoridae" genus="Flavalona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anomopoda" pageId="24" pageNumber="25" phylum="Arthropoda" rank="subSpecies" species="rustica" subSpecies="americana">
