<geoCoordinate id="EE005009FFDD292BEF4F377FB117F889" box="[851,891,1879,1901]" degrees="4" direction="south" orientation="latitude" pageId="9" pageNumber="704" precision="55555" value="-4.0">4°S</geoCoordinate>
), 
<collectingCountry id="F323765EFFDD292BEF8D377FB1A4F888" box="[913,968,1879,1900]" name="Peru" pageId="9" pageNumber="704">Peru</collectingCountry>
