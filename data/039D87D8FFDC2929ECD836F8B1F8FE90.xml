<geoCoordinate id="EE005009FFDD292BE874377FB6F3F889" box="[1128,1183,1879,1901]" degrees="43" direction="south" orientation="latitude" pageId="9" pageNumber="704" precision="55555" value="-43.0">43°S</geoCoordinate>
