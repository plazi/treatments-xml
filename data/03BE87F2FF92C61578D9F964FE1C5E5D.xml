<date id="FFA91024FF92C6177973F84BFE1658CE" box="[317,435,1990,2016]" pageId="33" pageNumber="34" value="1994-05-20">
<number id="3DDF24C3FF92C6177973F84BFEF858CE" box="[317,349,1990,2016]" pageId="33" pageNumber="34" value="20.0">20</number>
