<date id="152B0CBF46495D455F0AFD8FFD417956" box="[587,700,515,534]" pageId="41" pageNumber="41" value="2008-08-23">
<collectingDate id="056FF55746495D455F0AFD8FFD417956" box="[587,700,515,534]" pageId="41" pageNumber="41" value="2008-08-23">23Aug. 2008</collectingDate>
</date>
, M. 
<collectorName id="CC604FA946495D455F9EFD8FFCEB7956" box="[735,790,515,534]" pageId="41" pageNumber="41">Batley</collectorName>
, 
<emphasis id="53E1F66D46495D455DE7FD97FF44796E" box="[166,185,539,558]" italics="true" pageId="41" pageNumber="41">ex</emphasis>
