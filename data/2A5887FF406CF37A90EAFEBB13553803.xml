<quantity id="65099B0C406CF37A9084FC5711263FD6" box="[249,334,928,955]" metricMagnitude="-3" metricUnit="m" metricValue="6.3" pageId="6" pageNumber="477" unit="mm" value="6.3">6.3 mm</quantity>
