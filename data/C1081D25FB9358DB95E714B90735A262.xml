<taxonomicName id="9811417CDD965F7A2ABF6D3F3AF14C68" class="Magnoliopsida" family="Solanaceae" genus="Solanum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="" order="Solanales" pageId="246" pageNumber="247" phylum="Tracheophyta" rank="species" species="sousae">
