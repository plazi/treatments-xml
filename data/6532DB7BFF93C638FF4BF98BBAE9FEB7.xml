<paragraph id="ED246A6DFF93C639FF4BF92BB9ABF8A6" blockId="6.[192,1440,1715,1835]" pageId="6" pageNumber="5">
<taxonomicName id="2A9B11EEFF93C639FF4BF92BBB63F886" authority="Schuster y Reyes-Castillo 1990: 12 - 15" authorityName="Schuster y Reyes-Castillo" authorityPageNumber="12 - 15" authorityYear="1990" box="[192,885,1779,1804]" class="Insecta" family="Passalidae" genus="Ogyges" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="crassulus">
