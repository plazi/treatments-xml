<paragraph blockId="7.[151,531,788,848]" box="[151,531,788,815]" pageId="7" pageNumber="264">
<heading bold="true" box="[151,531,788,815]" fontSize="11" level="1" pageId="7" pageNumber="264" reason="1">
<emphasis bold="true" box="[151,531,788,815]" pageId="7" pageNumber="264">
<taxonomicName box="[151,457,788,814]" class="Insecta" family="Trogossitidae" genus="Globorentonium" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="264" phylum="Arthropoda" rank="species" species="lescheni" status="sp. n.">
<emphasis bold="true" box="[151,457,788,814]" italics="true" pageId="7" pageNumber="264">Globorentonium lescheni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[465,531,789,815]" pageId="7" pageNumber="264" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
