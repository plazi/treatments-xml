<subSubSection pageId="12" pageNumber="269" type="etymology">
<paragraph blockId="12.[151,1437,151,1040]" pageId="12" pageNumber="269">
<emphasis bold="true" box="[199,328,980,1005]" pageId="12" pageNumber="269">Etymology</emphasis>
