<taxonomicName authority="(Poulain et al. 2011)" baseAuthorityName="Poulain" baseAuthorityYear="2011" box="[488,852,1886,1912]" class="Myxomycetes" family="Stemonitidaceae" genus="Meriderma" kingdom="Protozoa" order="Stemonitales" pageId="28" pageNumber="203" phylum="Mycetozoa" rank="genus">
