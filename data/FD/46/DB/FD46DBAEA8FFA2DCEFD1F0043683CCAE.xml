<taxonomicName class="Hexapoda" family="Vespidae" genus="Pachymenes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="5" pageNumber="136" phylum="Arthropoda" rank="species" species="flavospinosus">
