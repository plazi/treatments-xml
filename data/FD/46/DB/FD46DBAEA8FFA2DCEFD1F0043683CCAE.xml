<taxonomicName class="Insecta" family="Eumenidae" genus="Apodynerus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="5" pageNumber="136" phylum="Arthropoda" rank="species" species="flavispinosus">
