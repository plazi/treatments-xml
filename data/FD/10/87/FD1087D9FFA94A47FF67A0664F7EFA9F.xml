* 
<taxonomicName authority="Valenciennes, 1839" authorityName="Valenciennes" authorityYear="1839" box="[209,402,272,297]" class="Actinopterygii" family="Labridae" genus="Bodianus" kingdom="Animalia" order="Perciformes" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="scrofa">Bodianus scrofa</taxonomicName>
