(
<number box="[1070,1134,541,567]" pageId="3" pageNumber="4" value="1984.0">1984</number>
</bibRefCitation>
: 
<number box="[1146,1247,541,567]" pageId="3" pageNumber="4" value="179.5" valueMax="180.0" valueMin="179.0">179-180</number>
), and Stehmann in 
<bibRefCitation author="Quero" box="[181,400,581,607]" pageId="3" pageNumber="25" refString="Quero, J. C., Hureau, J. C., Karrer, C., Post, A. &amp; Saldanha, L. (1990) Check-list of the fishes of the eastern tropical Atlantic. 3 volumes. Lisbon." type="book chapter" year="1990">
Quéro 
<emphasis box="[261,320,581,606]" italics="true" pageId="3" pageNumber="4">et al.</emphasis>
(
<number box="[336,400,581,607]" pageId="3" pageNumber="4" value="1990.0">1990</number>
</bibRefCitation>
: 
<number box="[411,443,581,607]" pageId="3" pageNumber="4" value="35.0">35</number>
), under the name 
<taxonomicName box="[653,967,581,606]" class="Elasmobranchii" family="Rajidae" genus="Raja" kingdom="Animalia" order="Rajiformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="fullonica" subGenus="Leucoraja">
