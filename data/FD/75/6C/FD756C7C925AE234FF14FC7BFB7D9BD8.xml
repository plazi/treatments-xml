<taxonomicName box="[1250,1398,1267,1291]" class="Insecta" family="Coreidae" genus="Acanthocephala" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="34" phylum="Arthropoda" rank="species" species="terminalis">
