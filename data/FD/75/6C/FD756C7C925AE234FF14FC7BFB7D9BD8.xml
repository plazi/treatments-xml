(
<number box="[1132,1191,1338,1362]" pageId="4" pageNumber="34" value="1976.0">1976</number>
) (
<specimenCount box="[1215,1308,1338,1362]" pageId="4" pageNumber="34" type="adult">
<number box="[1215,1230,1338,1362]" pageId="4" pageNumber="34" value="4.0">4</number>
