<subSubSection pageId="47" pageNumber="1973" type="etymology">
<paragraph blockId="47.[160,1155,433,562]" box="[160,285,433,458]" pageId="47" pageNumber="1973">
<emphasis bold="true" box="[160,285,433,458]" italics="true" pageId="47" pageNumber="1973">Etymology</emphasis>
</paragraph>
<paragraph blockId="47.[160,1155,433,562]" pageId="47" pageNumber="1973">
