des 
<taxonomicName class="Magnoliopsida" family="Asteraceae" genus="Chrysanthemum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="571" phylum="Tracheophyta" rank="genus">Chrysanthemum</taxonomicName>
<taxonomicName authority="L." class="Magnoliopsida" family="Asteraceae" genus="Leucanthemum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="571" phylum="Tracheophyta" rank="genus">
