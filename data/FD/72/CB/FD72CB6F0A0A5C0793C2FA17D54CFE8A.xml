X. 
<emphasis bold="true" box="[151,169,1960,1981]" pageId="6" pageNumber="7">D</emphasis>
—male sternite VII; 
<emphasis bold="true" box="[387,404,1960,1981]" pageId="6" pageNumber="7">E</emphasis>
—male sternite VIII; 
<emphasis bold="true" box="[630,645,1960,1981]" pageId="6" pageNumber="7">F</emphasis>
—male sternite IX; 
<emphasis bold="true" box="[855,872,1959,1982]" pageId="6" pageNumber="7">G</emphasis>
, 
<emphasis bold="true" box="[887,907,1960,1981]" pageId="6" pageNumber="7">H</emphasis>
—aedeagus in ventral view; 
