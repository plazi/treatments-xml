<taxonomicName authorityName=", Rathbun" authorityYear="1904" box="[1334,1434,1805,1826]" class="Malacostraca" family="Pandalidae" genus="Pandalopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="aleutica">
