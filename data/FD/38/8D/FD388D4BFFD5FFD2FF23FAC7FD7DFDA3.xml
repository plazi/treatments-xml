<subSubSection box="[199,1135,1622,1649]" pageId="32" pageNumber="33" type="etymology">
<paragraph blockId="32.[151,1437,1441,2045]" box="[199,1135,1622,1649]" pageId="32" pageNumber="33">
<emphasis bold="true" box="[199,338,1622,1648]" pageId="32" pageNumber="33">Etymology:</emphasis>
