<subSubSection box="[199,1135,1117,1143]" pageId="30" pageNumber="31" type="etymology">
<paragraph blockId="30.[151,1437,1117,2043]" box="[199,1135,1117,1143]" pageId="30" pageNumber="31">
<emphasis bold="true" box="[199,338,1117,1143]" pageId="30" pageNumber="31">Etymology:</emphasis>
