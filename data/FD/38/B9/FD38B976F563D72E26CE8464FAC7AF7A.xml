<quantity box="[1376,1456,2003,2027]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" metricValueMax="7.0" metricValueMin="6.0" pageId="1" pageNumber="20" unit="mm" value="6.5" valueMax="7.0" valueMin="6.0">6-7 mm</quantity>
