<location LSID="urn:lsid:plazi:treatment:FD2787A09F67B863FF68FF43FA52F806:7051606D9F67B862FD3FFDDBFCD0FDBB" box="[704,787,582,608]" country="Thailand" county="Hua Phan Prov." latitude="20.2" longLatPrecision="1268" longitude="103.98333" municipality="Mt. Phu Pane" name="St. Jaki" pageId="2" pageNumber="236" stateProvince="Nan">St. Jaki</location>
and 
<location LSID="urn:lsid:plazi:treatment:FD2787A09F67B863FF68FF43FA52F806:7051606D9F67B862FCB6FDDAFCB6FDBA" box="[841,885,583,609]" country="Thailand" county="Hua Phan Prov." latitude="20.2" longLatPrecision="1268" longitude="103.98333" municipality="Mt. Phu Pane" name="Lao" pageId="2" pageNumber="236" stateProvince="Nan">
<collectorName box="[841,885,583,609]" pageId="2" pageNumber="236">Lao</collectorName>
</location>
collectors lgt. (
