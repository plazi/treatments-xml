</taxonomicName>
<taxonomicNameLabel box="[970,1064,762,785]" pageId="2" pageNumber="403" rank="species">sp. nov.</taxonomicNameLabel>
, 
<taxonomicName authority="Alcock, 1904" authorityName="Alcock" authorityYear="1904" box="[1080,1431,761,785]" class="Amphibia" family="Chikilidae" genus="Chikila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gymnophiona" pageId="2" pageNumber="403" phylum="Chordata" rank="species" species="fulleri">
