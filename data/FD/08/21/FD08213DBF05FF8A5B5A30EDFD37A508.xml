<emphasis bold="true" box="[264,283,1051,1075]" pageId="2" pageNumber="33">
<number box="[264,278,1051,1075]" pageId="2" pageNumber="33" value="2.0">2</number>
.
</emphasis>
<taxonomicName box="[298,456,1051,1074]" class="Insecta" family="Phasmatidae" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="33" phylum="Arthropoda" rank="subFamily" subFamily="Eurycanthinae">Eurycanthinae</taxonomicName>
