<emphasis bold="true" box="[264,283,1085,1109]" pageId="2" pageNumber="33">
<number box="[264,278,1085,1109]" pageId="2" pageNumber="33" value="3.0">3</number>
.
</emphasis>
<taxonomicName box="[298,487,1086,1109]" class="Insecta" family="Heteropterygidae" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="33" phylum="Arthropoda" rank="family">Heteropterygidae</taxonomicName>
