<taxonomicName id="4C694D5FBE2EFF861D397009BEDAF8E5" box="[1030,1180,1858,1880]" class="Actinopterygii" family="Cyprinidae" genus="Rectoris" kingdom="Animalia" order="Cypriniformes" pageId="7" pageNumber="62" phylum="Chordata" rank="species" species="longibarbus">
<emphasis id="B91DEACEBE2EFF861D397009BEDAF8E5" box="[1030,1180,1858,1880]" italics="true" pageId="7" pageNumber="62">R. longibarbus</emphasis>
</taxonomicName>
(˔); 
<taxonomicName id="4C694D5FBE2EFF861DE67009BF15F8E5" box="[1241,1363,1858,1880]" class="Actinopterygii" family="Cyprinidae" genus="Rectoris" kingdom="Animalia" order="Cypriniformes" pageId="7" pageNumber="62" phylum="Chordata" rank="species" species="mutabilis">
<emphasis id="B91DEACEBE2EFF861DE67009BF15F8E5" box="[1241,1363,1858,1880]" italics="true" pageId="7" pageNumber="62">R. mutabilis</emphasis>
</taxonomicName>
(●); 
<taxonomicName id="4C694D5FBE2EFF861CB97009BB43F8CA" class="Actinopterygii" family="Cyprinidae" genus="Rectoris" kingdom="Animalia" order="Cypriniformes" pageId="7" pageNumber="62" phylum="Chordata" rank="species" species="posehensis">
<emphasis id="B91DEACEBE2EFF861CB97009BB43F8CA" italics="true" pageId="7" pageNumber="62">R. posehensis</emphasis>
