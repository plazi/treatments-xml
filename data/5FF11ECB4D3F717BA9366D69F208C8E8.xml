<paragraph id="316F06145C9FA6691DE7DF385DECA1F3" pageId="3" pageNumber="16">
<taxonomicName id="6E98C3B45CD7CC375558551503C9C93E" class="Arachnida" family="Sparassidae" genus="Diminutella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diminutella cortina" order="Araneae" pageId="3" pageNumber="16" phylum="Arthropoda" rank="species" species="cortina">Diminutella cortina</taxonomicName>
