<paragraph id="8BD53039FFC0FF9A23EFFB726D8D4EE1" blockId="1.[151,1437,1213,2029]" box="[151,709,1213,1238]" pageId="1" pageNumber="64">
<emphasis id="B91EEC2BFFC0FF9A23EFFB726E7A4EE2" bold="true" box="[151,306,1213,1238]" pageId="1" pageNumber="64">
<typeStatus id="54D18E9BFFC0FF9A23EFFB726F9A4EE1" box="[151,210,1213,1237]" pageId="1" pageNumber="64">Type</typeStatus>
