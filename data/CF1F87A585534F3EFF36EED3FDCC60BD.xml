<heading id="1C4181DF85534F3EFF36EED3FD4E61A5" box="[151,680,1729,1755]" fontSize="11" level="2" pageId="4" pageNumber="42" reason="3">
<taxonomicName id="80B64D3085534F3EFF36EED3FD4E61A5" authority="(Mont.) Maas Geest." box="[151,680,1729,1755]" class="Agaricomycetes" family="Hericiaceae" genus="Dentipellis" kingdom="Fungi" order="Russulales" pageId="4" pageNumber="42" phylum="Basidiomycota" rank="species" species="leptodon">
<emphasis id="75C2EAA185534F3EFF36EED3FEFF61A5" bold="true" box="[151,281,1729,1755]" italics="true" pageId="4" pageNumber="42">Dentipellis</emphasis>
cf. 
<emphasis id="75C2EAA185534F3EFEE7EED3FE4861A5" bold="true" box="[326,430,1729,1755]" italics="true" pageId="4" pageNumber="42">leptodon</emphasis>
