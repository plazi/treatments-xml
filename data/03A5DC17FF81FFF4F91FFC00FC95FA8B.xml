, 
<collectingCountry id="F31B2D91FF81FFF4FF7AFB2AFC84FB4B" name="Mozambique" pageId="1" pageNumber="198">Mozambique</collectingCountry>
and 
<collectingCountry id="F31B2D91FF81FFF4F9FDFB0AFBCAFB4B" box="[948,1061,1184,1208]" name="Zimbabwe" pageId="1" pageNumber="198">Zimbabwe</collectingCountry>
(
<bibRefCitation id="EF9D10F0FF81FFF4FE73FB0AFB32FB4B" author="ZIELKE, E." box="[1082,1245,1184,1208]" pageId="1" pageNumber="198" pagination="1 - 199" refId="ref5276" refString="ZIELKE, E. 1971 a: Revision der Muscinae der athiopischen Region. - Series Entomologica 7: 1 - 199." type="journal article" year="1971">ZIELKE 1971a</bibRefCitation>
