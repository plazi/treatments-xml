<collectingCountry id="F31B2D91FF81FFF4F979FA8AFC96FACB" box="[816,889,1312,1336]" name="Ghana" pageId="1" pageNumber="198">Ghana</collectingCountry>
, 
<collectingCountry id="F31B2D91FF81FFF4F9CDFA8AFBA8FACB" box="[900,1095,1312,1336]" name="Equatorial Guinea" pageId="1" pageNumber="198">Equatorial Guinea</collectingCountry>
and 
<collectingCountry id="F31B2D91FF81FFF4FE32FA8AFB31FACB" box="[1147,1246,1312,1336]" name="Tanzania" pageId="1" pageNumber="198">Tanzania</collectingCountry>
