<quantity id="4CEF12DBDB15FF88FD83B21BFD00F85C" box="[602,709,1906,1932]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="25" pageNumber="26" unit="mm" value="4.4">4.40 mm</quantity>
; PW 
<quantity id="4CEF12DBDB15FF88FCDCB21BFCB4F85C" box="[773,881,1906,1932]" metricMagnitude="-3" metricUnit="m" metricValue="1.55" pageId="25" pageNumber="26" unit="mm" value="1.55">1.55 mm</quantity>
