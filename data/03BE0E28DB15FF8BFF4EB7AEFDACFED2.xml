<quantity id="4CEF12DBDB15FF88FC7FB21BFBC6F85C" box="[934,1027,1906,1932]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="25" pageNumber="26" unit="mm" value="0.8">0.8 mm</quantity>
; EL 
<quantity id="4CEF12DBDB15FF88FBE3B21BFB52F85C" box="[1082,1175,1906,1932]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="25" pageNumber="26" unit="mm" value="3.6">3.6 mm</quantity>
