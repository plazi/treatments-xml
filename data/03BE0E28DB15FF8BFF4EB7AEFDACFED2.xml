<quantity id="4CEF12DBDB15FF88FB01B21BFA81F85C" box="[1240,1348,1906,1932]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" pageId="25" pageNumber="26" unit="mm" value="2.75">2.75 mm</quantity>
