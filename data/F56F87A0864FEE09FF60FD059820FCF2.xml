<taxonomicName id="BAC64D35864CEE09FECFFC829917FCF1" authorityName="Ogloblin" authorityYear="1934" box="[312,537,792,817]" class="Insecta" family="Mymaridae" genus="Erythmelus" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="verticillatus" subGenus="Erythmelus">
