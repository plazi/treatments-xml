Voet
</taxonomicName>
and 
<taxonomicName id="319D4D2CFFA7FF81FD83F9DDB66AE912" authority="Crotch" authorityName="Crotch" authorityYear="1873" box="[512,793,1537,1561]" class="Insecta" family="Erotylidae" genus="Cypherotylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="24" phylum="Arthropoda" rank="genus">
