<paragraph id="F28F6FEF8A16CB9E5615EC11E2E7B1D3" pageId="24" pageNumber="49">
<taxonomicName id="BECF2B7BCE0E9628BC41C3E3016C1A96" class="Magnoliopsida" family="Asteraceae" genus="Acilepis" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="24" pageNumber="49" phylum="Tracheophyta" rank="species" species="tonkinensis">
<emphasis id="5B981FF2DE4182E0FCE0ADBAEC97EDA0" italics="true" pageId="24" pageNumber="49">Acilepis tonkinensis</emphasis>
</taxonomicName>
