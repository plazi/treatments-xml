, MI 
<number id="3DCF24B6FF8CFFC4FB7DFCCCE8A9FC95" box="[1180,1256,845,871]" pageId="14" pageNumber="15" value="67.0" valueMax="71.0" valueMin="63.0">63–71</number>
, SL 
<number id="3DCF24B6FF8CFFC4FAFEFCCCE9D9FC95" box="[1311,1432,845,871]" pageId="14" pageNumber="15" value="0.6599999999999999" valueMax="0.74" valueMin="0.58">0.58–0.74</number>
, SI 
<number id="3DCF24B6FF8CFFC4FF59FCF4ED45FC7D" box="[184,260,885,911]" pageId="14" pageNumber="15" value="72.5" valueMax="76.0" valueMin="69.0">69–76</number>
