<taxonomicName id="1DC04D26FFFDFFD6FD81FA7F40C20892" authorityName="Latreille" authorityYear="1802" box="[589,723,1443,1465]" class="Insecta" family="Bostrichidae" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="417" phylum="Arthropoda" rank="family">Bostrichidae</taxonomicName>
