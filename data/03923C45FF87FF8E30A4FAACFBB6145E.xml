<taxonomicName id="4C3BF6D0FF87FF8E30A4FAACFD5A13E0" authority="(Jaekel, 1895)" baseAuthorityName="Jaekel" baseAuthorityYear="1895" box="[188,719,1395,1422]" class="Elasmobranchii" family="Lamnidae" genus="Otodus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lamniformes" pageId="6" pageNumber="125" phylum="Chordata" rank="species" species="sokolowi" subGenus="Carcharocles">
