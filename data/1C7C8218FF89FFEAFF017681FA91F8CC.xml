<specimenCode id="C4739B75FF89FFEAFF44747AFE40FD07" box="[210,426,619,646]" collectionCode="MNHN" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="16" pageNumber="408">MNHN 1991.2864</specimenCode>
