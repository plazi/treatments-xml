<taxonomicName id="E9F54D66FFFBFE40FDB8FB35F86ECAE9" authority="Wang 1990" authorityName="Wang" authorityYear="1990" box="[518,811,1197,1219]" class="Insecta" family="Tephritidae" genus="Acrorellia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="55" phylum="Arthropoda" rank="species" species="sinica">
