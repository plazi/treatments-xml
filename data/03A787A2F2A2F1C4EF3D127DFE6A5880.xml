[
<taxonomicName id="4C0E4D37F2A2F1C4EF34127CFDDD5954" authority="Rothschild" authorityName="Rothschild" authorityYear="1921" box="[110,604,1040,1063]" class="Aves" family="Nectariniidae" genus="Aethopyga" kingdom="Animalia" order="Passeriformes" pageId="150" pageNumber="150" phylum="Chordata" rank="subSpecies" species="seheriae" subSpecies="viridicauda">
