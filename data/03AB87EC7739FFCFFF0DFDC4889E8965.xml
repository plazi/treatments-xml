<geoCoordinate id="EE36503D7739FFCFFBA8FCAF8FFF8E61" box="[1074,1261,818,845]" degrees="14" direction="south" minutes="30" orientation="latitude" pageId="7" pageNumber="48" precision="1" seconds="21.09" value="-14.505858">
