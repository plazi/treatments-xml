<taxonomicName id="05824D0F2540E130FF627E0E4943F9F7" box="[151,360,1618,1641]" class="Amphibia" family="Centrolenidae" genus="Hyalinobatrachium" kingdom="Animalia" order="Anura" pageId="2" pageNumber="27" phylum="Chordata" rank="genus">Hyalinobatrachium</taxonomicName>
