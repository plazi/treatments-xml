<taxonomicName id="4C3FD3ECFFACFF8FF5AEFE8AED387C6D" authorityName="Assing" authorityYear="2010" box="[666,816,344,371]" class="Insecta" family="Staphylinidae" genus="Astenus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="1108" phylum="Arthropoda" rank="species" species="shavrini">
<emphasis id="B94B747DFFACFF8FF5AEFE8AED387C6D" box="[666,816,344,371]" italics="true" pageId="3" pageNumber="1108">A. shavrini</emphasis>
</taxonomicName>
