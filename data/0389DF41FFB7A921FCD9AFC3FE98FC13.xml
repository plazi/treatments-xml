<paragraph id="8B9F6E57FFB4A921FF03AEC6FF09FCDB" blockId="34.[144,759,711,978]" pageId="34" pageNumber="757">
<taxonomicName id="4C2015D4FFB4A921FF03AEC6FD5AFD3D" authority="pulchra (Staudinger, 1898)" authorityName="pulchra (Staudinger" authorityYear="1898" baseAuthorityName="Staudinger" baseAuthorityYear="1898" box="[144,687,742,764]" class="Insecta" family="Geometridae" genus="Microbaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="757" phylum="Arthropoda" rank="subSpecies" species="pulchra" subSpecies="pulchra">
