<pageBreakToken id="8E980439794849889058D6A42A90C213" pageId="null" pageNumber="327" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="2AD2A1213BD19EDB11E518254474F56B" authority="L." class="Magnoliopsida" family="Caprifoliaceae" genus="Valeriana" kingdom="Plantae" order="Dipsacales" pageId="null" pageNumber="327" phylum="Tracheophyta" rank="species" species="montana">
