<paragraph id="07DBB43CB0AB7113E53FE90EB087FAFE" pageId="null" pageNumber="746">
<pageBreakToken id="99A62D8125ACCF6FB6A7C334FF3D77FA" pageId="null" pageNumber="746" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="EA3648A987CAE7F645C523511414738C" authority="L." class="Insecta" family="Hesperiidae" genus="Viola" kingdom="Animalia" order="Lepidoptera" pageId="null" pageNumber="746" phylum="Arthropoda" rank="species" species="canina">
