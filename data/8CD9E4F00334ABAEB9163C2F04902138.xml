<bibRefCitation id="D78E25D79E270C769E0AEB8821E5B057" author="Zacher, F" journalOrPublisher="Zeitschrift fuer angewandte Entomologie" pageId="13" pageNumber="72" pagination="460 - 480" title="Die Naehrpflanzen der Samenkaefer." volume="33" year="1952">Zacher (1952)</bibRefCitation>
stated that 
<taxonomicName id="97B148BDD2BA8C1483EF06A3F6B13967" class="Magnoliopsida" family="Fabaceae" genus="Lablab" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lablab niger" order="Fabales" pageId="5" pageNumber="64" phylum="Tracheophyta" rank="species" species="niger">Lablab niger</taxonomicName>
