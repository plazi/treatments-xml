<taxonomicName id="2E3658B18D77CA1BA3F0BE167EB17B3A" class="Magnoliopsida" family="Fabaceae" genus="Vigna" higherTaxonomySource="CoL" infraspecific-rank="subsp." kingdom="Plantae" lsidName="Vigna unguiculata" order="Fabales" pageId="5" pageNumber="64" phylum="Tracheophyta" rank="subspecies" species="unguiculata" subspecies="stenophylla">Vigna unguiculata subsp. stenophylla</taxonomicName>
