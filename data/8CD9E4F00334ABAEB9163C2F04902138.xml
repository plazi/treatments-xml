<taxonomicName id="DED4412519F66E9CCD2CF033D7936191" authority="Borowiec, 1987" authorityName="Borowiec" authorityYear="1987" class="Insecta" family="Chrysomelidae" genus="Decellebruchus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Decellebruchus atrolineatus" order="Coleoptera" pageId="4" pageNumber="63" phylum="Arthropoda" rank="species" species="atrolineatus">Decellebruchus atrolineatus</taxonomicName>
: 
