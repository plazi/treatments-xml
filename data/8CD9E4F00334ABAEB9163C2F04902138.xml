<taxonomicName id="04B33C68CF4865FB16607A21D3F4AFEF" class="Magnoliopsida" family="Fabaceae" genus="Medicago" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Medicago sativa" order="Fabales" pageId="5" pageNumber="64" phylum="Tracheophyta" rank="species" species="sativa">Medicago sativa</taxonomicName>
