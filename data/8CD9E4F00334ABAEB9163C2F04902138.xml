<taxonomicName id="46026F7FD27F8A980E6671679A49E38B" class="Bivalvia" family="Phaseolidae" genus="Phaseolus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phaseolus vulgaris" order="Nuculanida" pageId="5" pageNumber="64" phylum="Mollusca" rank="species" species="vulgaris">Phaseolus vulgaris</taxonomicName>
