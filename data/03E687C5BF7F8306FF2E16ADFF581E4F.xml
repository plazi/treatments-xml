<paragraph id="8BF036D3BF7F8306FF7E1398FC751DF2" blockId="8.[151,1436,613,1792]" pageId="8" pageNumber="9">
<emphasis id="B93BEAC1BF7F8306FF7E1398FDA71DD5" bold="true" box="[199,515,1153,1178]" pageId="8" pageNumber="9">
Measurements of 
<typeStatus id="54F48871BF7F8306FE251398FDA71DD5" box="[412,515,1154,1178]" pageId="8" pageNumber="9">holotype</typeStatus>
</emphasis>
: height 
<quantity id="4CB79B36BF7F8306FDD9139BFD631DD6" box="[608,711,1153,1177]" metricMagnitude="-3" metricUnit="m" metricValue="4.39" pageId="8" pageNumber="9" unit="mm" value="4.39">4.39 mm</quantity>
, width 
<quantity id="4CB79B36BF7F8306FCA0139BFC241DD6" box="[793,896,1153,1177]" metricMagnitude="-3" metricUnit="m" metricValue="1.68" pageId="8" pageNumber="9" unit="mm" value="1.68">1.68 mm</quantity>
, body whorl height 
<quantity id="4CB79B36BF7F8306FBD8139BFB6D1DD6" box="[1121,1225,1153,1177]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="8" pageNumber="9" unit="mm" value="2.3">2.30 mm</quantity>
, body whorl width 
<quantity id="4CB79B36BF7F8306FF2E13BFFF591DF2" box="[151,253,1189,1213]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="8" pageNumber="9" unit="mm" value="1.6">1.60 mm</quantity>
, aperture height 
<quantity id="4CB79B36BF7F8306FE0113BFFDBA1DF2" box="[440,542,1189,1213]" metricMagnitude="-3" metricUnit="m" metricValue="1.35" pageId="8" pageNumber="9" unit="mm" value="1.35">1.35 mm</quantity>
, aperture width 
<quantity id="4CB79B36BF7F8306FD6A13BFFC9D1DF2" box="[723,825,1189,1213]" metricMagnitude="-3" metricUnit="m" metricValue="1.01" pageId="8" pageNumber="9" unit="mm" value="1.01">1.01 mm</quantity>
, 
