<taxonomicName id="4C404D77FEC22B25FDE7D5C3FD0CFC20" authority="Latreille, 1804" authorityName="Olmi &amp; Copeland &amp; Noort" authorityYear="2019" box="[590,749,909,935]" class="Insecta" family="Dryinidae" genus="Dryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="365" pageNumber="366" phylum="Arthropoda" rank="species" species="dentatiforceps">
