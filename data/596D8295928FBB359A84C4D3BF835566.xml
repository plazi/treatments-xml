<taxonomicName id="6C43BBDDA860C04AC35B453D5DE5BCCC" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:186584" class="Insecta" family="Formicidae" genus="Ectatomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ectatomma (Gnamptogenys) rimulosum (Roger)" lsidName-HNS="Ectatomma (Gnamptogenys) rimulosum (Roger)" order="Hymenoptera" pageNumber="20" phylum="Arthropoda" rank="species" species="rimulosum" subGenus="Gnamptogenys">E. (G.) rimulosum Rog.</taxonomicName>
