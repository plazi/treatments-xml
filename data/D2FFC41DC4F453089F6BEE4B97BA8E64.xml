<taxonomicName id="E275E0D514EEDD986F3FE1E8A7B294A4" LSID="D2FFC41D-C4F4-5308-9F6B-EE4B97BA8E64" authority="Chaudoir, 1842" authorityName="Chaudoir" authorityYear="1842" class="Insecta" family="Carabidae" genus="Brachinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Brachinus (Brachinus) elegans" order="Coleoptera" pageId="0" pageNumber="57889" phylum="Arthropoda" rank="species" species="elegans" subGenus="Brachinus">Brachinus (Brachinus) elegans Chaudoir, 1842</taxonomicName>
