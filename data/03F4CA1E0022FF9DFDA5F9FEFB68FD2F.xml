<paragraph id="8BE27B080021FF9DFEC9FE79FB68FD2F" blockId="11.[336,1289,405,734]" pageId="11" pageNumber="318">
<taxonomicName id="4C5D008B0021FF9DFEC9FE79F9B3FE5E" authorityName="Mahunka" authorityYear="1984" box="[336,432,405,431]" class="Arachnida" family="Humerobatidae" genus="Afroleius" kingdom="Animalia" order="Sarcoptiformes" pageId="11" pageNumber="318" phylum="Arthropoda" rank="species" species="deformis">
<emphasis id="B929A71A0021FF9DFEC9FE79F9B3FE5E" box="[336,432,405,431]" italics="true" pageId="11" pageNumber="318">deformis</emphasis>
</taxonomicName>
, centralpartofthenotogasterfoveate (smoothin 
<taxonomicName id="4C5D008B0021FF9DFB80FE79FC98FE5E" authorityName="Mahunka" authorityYear="1984" box="[1049,1179,405,431]" class="Arachnida" family="Humerobatidae" genus="Afroleius" kingdom="Animalia" order="Sarcoptiformes" pageId="11" pageNumber="318" phylum="Arthropoda" rank="species" species="deformis">
<emphasis id="B929A71A0021FF9DFB80FE79FC98FE5E" box="[1049,1179,405,431]" italics="true" pageId="11" pageNumber="318">A. deformis</emphasis>
</taxonomicName>
