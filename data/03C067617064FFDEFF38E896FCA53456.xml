<emphasis id="B91D0A657064FFD9FD3FEC1AFCC33241" box="[655,813,1816,1842]" italics="true" pageId="3" pageNumber="254">
<taxonomicName id="4C69ADF47064FFD9FD3FEC1AFCC73241" authorityName="Wijayaw., Camporesi, A.J.L.Phillips &amp; K.D.Hyde" authorityYear="2016" box="[655,809,1816,1842]" class="Leotiomycetes" genus="Apiculospora" kingdom="Fungi" pageId="3" pageNumber="254" phylum="Ascomycota" rank="genus">Apiculospora</taxonomicName>
