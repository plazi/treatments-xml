<taxonomicName id="4C69ADF47064FFD9FC0FEC1AFBB83241" authorityName="Wijayaw., Camporesi, A.J.L.Phillips &amp; K.D.Hyde" authorityYear="2016" box="[959,1110,1816,1842]" class="Leotiomycetes" genus="Apiculospora" kingdom="Fungi" pageId="3" pageNumber="254" phylum="Ascomycota" rank="genus">
