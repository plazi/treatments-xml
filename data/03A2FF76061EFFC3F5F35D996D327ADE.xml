<collectingRegion id="49CF8082061EFFC3F5BA5C2D6EFF7A76" box="[251,420,807,829]" country="Austria" name="Niederoesterreich" pageId="14" pageNumber="355">Niederösterreich</collectingRegion>
(H. MELZER schriftl. Mitt.), 
<collectingRegion id="49CF8082061EFFC3F78D5C2D6C397A76" box="[716,866,807,829]" country="Austria" name="Oberoesterreich" pageId="14" pageNumber="355">Oberösterreich</collectingRegion>
(
<bibRefCitation id="EF9A3391061EFFC3F6335C2D6CBA7A76" author="ESSL F" box="[882,993,807,829]" pageId="14" pageNumber="355" pagination="205 - 244" refId="ref14120" refString="ESSL F. (1999): Floristische Beobachtungen aus dem ostlichen Oberosterreich und dem angrenzenden Niederosterreich. - Beitr. Naturk. Oberosterreichs 7: 205 - 244." type="journal article" year="1999">ESSL 1999</bibRefCitation>
) und der 
<collectingRegion id="49CF8082061EFFC3F5F35C4E6E7A7A11" box="[178,289,836,858]" country="Austria" name="Steiermark" pageId="14" pageNumber="355">Steiermark</collectingRegion>
