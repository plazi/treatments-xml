<bibRefCitation id="EFD94B74FF8DFF9EFEC2FE9F71EF713C" author="Schileyko" box="[323,694,268,294]" pageId="24" pageNumber="31" refString="Schileyko, A. A. (1998) Treatise on recent terrestrial pulmonate molluscs. Part. 2. Gastrocoptidae, Hypselostomatidae, Vertiginidae, Truncatellinidae, Pachnodidae, Enidae, Sagdidae. Ruthenica, Supplement 2, 129 - 261." type="journal article" year="1998">
Schileyko (
<number id="3D8024A2FF8DFF9EFE49FE9F7151713C" box="[456,520,268,294]" pageId="24" pageNumber="25" value="1998.0">1998</number>
: 
<number id="3D8024A2FF8DFF9EFD92FE9F711B713C" box="[531,578,268,294]" pageId="24" pageNumber="25" value="190.0">190</number>
, fig. 
<number id="3D8024A2FF8DFF9EFDFCFE9F71F7713C" box="[637,686,268,294]" pageId="24" pageNumber="25" value="234.0">234</number>
)
