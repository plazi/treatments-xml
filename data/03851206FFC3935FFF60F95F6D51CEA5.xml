<taxonomicName id="4C2CD893FFCC935FFDDDFA046C7DCC8E" box="[554,633,1413,1435]" class="Insecta" family="Tenebrionidae" genus="Lechius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="32" phylum="Arthropoda" rank="genus">
