<taxonomicName id="4C2CD893FFCD935EFE10F8016C4ACE80" box="[487,590,1920,1941]" class="Insecta" family="Tenebrionidae" genus="Lechius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="31" phylum="Arthropoda" rank="species" species="steineri">
