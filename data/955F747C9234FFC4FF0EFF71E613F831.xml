. Prothorax: length, 
<number id="AB3ED74D9234FFC3FCEAF8D2E685F8A3" box="[883,933,1844,1869]" pageId="3" pageNumber="242" value="1.3">1.30</number>
/ 
<number id="AB3ED74D9234FFC3FC37F8D2E6FEF8A3" box="[942,990,1844,1869]" pageId="3" pageNumber="242" value="1.45">1.45</number>
/ 
<number id="AB3ED74D9234FFC3FC7EF8D2E179F8A3" box="[999,1113,1844,1869]" pageId="3" pageNumber="242" value="1.375" valueMax="1.4" valueMin="1.35">1.35–1.40</number>
; anterior width, 
<number id="AB3ED74D9234FFC3FAB2F8D2E07AF8A3" box="[1323,1370,1844,1869]" pageId="3" pageNumber="242" value="1.05">1.05</number>
/ 
<number id="AB3ED74D9234FFC3FAFAF8D2E0B4F8A3" box="[1379,1428,1844,1869]" pageId="3" pageNumber="242" value="1.1">1.10</number>
/ 
<number id="AB3ED74D9234FFC3FF0EF8B1E42AF89E" box="[151,266,1879,1904]" pageId="3" pageNumber="242" value="1.0750000000000002" valueMax="1.1" valueMin="1.05">1.05–1.10</number>
; posterior width, 
<number id="AB3ED74D9234FFC3FE4DF8B1E725F89E" box="[468,517,1879,1904]" pageId="3" pageNumber="242" value="1.05">1.05</number>
