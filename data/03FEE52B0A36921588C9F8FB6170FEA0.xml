<taxonomicName id="4C572FBE0A36921088C9F8FB6231F8E3" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[151,562,1804,1830]" class="Insecta" family="Pieridae" genus="Leptidea" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="425" phylum="Arthropoda" rank="species" species="sinapis">
