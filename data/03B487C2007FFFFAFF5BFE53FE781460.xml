<taxonomicName id="4C1D4D57007FFFFAFDC5FC12FD051604" authorityName="Archibald &amp; Cannings" authorityYear="2021" box="[521,683,1015,1041]" class="Insecta" family="Sieblosiidae" genus="Whetwhetaksa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="109" pageNumber="110" phylum="Arthropoda" rank="genus">
<emphasis id="B969EAC6007FFFFAFDC5FC12FD051604" box="[521,683,1015,1041]" italics="true" pageId="109" pageNumber="110">Whetwhetaksa</emphasis>
