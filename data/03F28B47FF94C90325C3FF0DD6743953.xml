Fain (
<number id="3D932876FF94C9032583FF44D2473A98" box="[215,273,222,246]" pageId="8" pageNumber="68" value="1992.0">1992</number>
)
</bibRefCitation>
described 
<taxonomicName id="4C5B41D2FF94C90324C6FF7AD1553A99" box="[402,515,224,247]" class="Arachnida" family="Neothrombiidae" genus="Neosilphitrombium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acarina" pageId="8" pageNumber="68" phylum="Arthropoda" rank="species" species="gratum">
