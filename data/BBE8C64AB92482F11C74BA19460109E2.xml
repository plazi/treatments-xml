<taxonomicName id="33F993AA2FABE33A190E050F3B141A2D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141875" class="Insecta" family="Formicidae" genus="Pachycondyla" kingdom="Animalia" lsidName-HNS="Pachycondyla (Ectomomyrmex) sauteri Forel" order="Hymenoptera" pageNumber="49" phylum="Arthropoda" rank="species" species="sauteri" subGenus="Ectomomyrmex">Pachycondyla (Ectomomyrmex) Sauteri</taxonomicName>
