<taxonomicName id="30944D004A11FFAEFF5AF97C6C659358" box="[151,528,1788,1815]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="aenescens">
<emphasis id="C5E0EA914A11FFAEFF5AF97C6C659358" bold="true" box="[151,528,1788,1815]" italics="true" pageId="8" pageNumber="9">Morphostenophanes aenescens</emphasis>
</taxonomicName>
species group (
<taxonomicName id="30944D004A11FFAEFD1CF97D6D3E9358" box="[721,843,1789,1815]" class="Insecta" family="Tenebrionidae" genus="Morphostenophanes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="aenescens">
