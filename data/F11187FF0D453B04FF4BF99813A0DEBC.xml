47. 
<emphasis id="4BCCEAFB0D453B04FF04F998112FDE13" bold="true" box="[224,637,1549,1570]" pageId="8" pageNumber="234">
<taxonomicName id="BEB84D6A0D453B04FF04F998112BDE13" authority="(Schedl, 1959)" baseAuthorityName="Schedl" baseAuthorityYear="1959" box="[224,633,1549,1570]" class="Insecta" family="Curculionidae" genus="Monarthrum" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="234" phylum="Arthropoda" rank="species" species="gracilior">
