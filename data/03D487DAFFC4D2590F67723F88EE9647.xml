(n = 
<number id="3DB524EBFFC4D25B0FDC740D88CA9017" box="[435,452,1116,1142]" pageId="49" pageNumber="50" value="6.0">6</number>
).
</paragraph>
<paragraph id="8BC236CCFFC4D25B0F5774D58B42908F" blockId="49.[264,1325,716,1862]" pageId="49" pageNumber="50">
Total length 
<number id="3DB524EBFFC4D25B0FA474D588F190FF" box="[459,511,1156,1182]" pageId="49" pageNumber="50" value="2.03">2.03</number>
(
<number id="3DB524EBFFC4D25B0C7E74D58B1390FF" box="[529,541,1156,1182]" pageId="49" pageNumber="50" value="1.0">1</number>
) mm. Wing length 
<number id="3DB524EBFFC4D25B0D6C74D58A7590FF" box="[771,891,1156,1182]" pageId="49" pageNumber="50" value="1.26" valueMax="1.32" valueMin="1.2">1.20–1.32</number>
, 
<quantity id="4C859B29FFC4D25B0DE874D58AFD90FF" box="[903,1011,1156,1182]" metricMagnitude="-3" metricUnit="m" metricValue="1.26" pageId="49" pageNumber="50" unit="mm" value="1.26">1.26 mm</quantity>
