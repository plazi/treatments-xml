<number id="3DB524EBFFC4D25B0F3E75758882915F" box="[337,396,1316,1342]" pageId="49" pageNumber="50" value="10.0" valueMax="12.0" valueMin="8.0">8–12</number>
(
<number id="3DB524EBFFC4D25B0FCB757588BB915F" box="[420,437,1316,1342]" pageId="49" pageNumber="50" value="2.0">2</number>
