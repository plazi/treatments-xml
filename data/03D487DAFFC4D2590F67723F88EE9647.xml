<number id="3DB524EBFFC4D25B0DEC75258AA191EF" box="[899,943,1396,1422]" pageId="49" pageNumber="50" value="128.0">128</number>
(
<number id="3DB524EBFFC4D25B0DD075258AC191EF" box="[959,975,1396,1422]" pageId="49" pageNumber="50" value="2.0">2</number>
