<taxonomicName id="4C3A4D74FFD5CE2FB16FFE1AC3F1FDB7" authorityName="Moore" authorityYear="1878" box="[828,962,505,528]" class="Insecta" family="Erebidae" genus="Mahavira" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="281" phylum="Arthropoda" rank="species" species="flavicollis">
