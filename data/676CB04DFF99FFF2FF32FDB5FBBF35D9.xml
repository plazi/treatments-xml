<taxonomicName id="28C57AD8FF99FFF2FF4BFDB5FAD433F4" authority="Edgar, 2008" authorityName="Edgar" authorityYear="2008" box="[238,392,631,657]" class="Malacostraca" family="Tanaididae" genus="Austrotanais" kingdom="Animalia" order="Tanaidacea" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis id="DDB1DD49FF99FFF2FF4BFDB5FAD433F4" bold="true" box="[238,392,631,657]" italics="true" pageId="11" pageNumber="12">Austrotanais</emphasis>
