<typeStatus id="54D74CD5EE094A28AEE03634908A1F4F" box="[151,210,522,548]" pageId="34" pageNumber="437">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName id="4C6C89F4EE094A28AF4D363592BA1F4F" authority="Zheng, 1995" authorityName="Zheng" authorityYear="1995" box="[314,738,521,548]" class="Insecta" family="Tetrigidae" genus="Yunnantettix" kingdom="Animalia" order="Orthoptera" pageId="34" pageNumber="437" phylum="Arthropoda" rank="species" species="bannaensis">
