</taxonomicName>
from 
<taxonomicName id="4C1FEAC87501FFF4DB89F8E0FAB610B4" baseAuthorityName="Reeve" baseAuthorityYear="1843" box="[1222,1358,1910,1933]" class="Gastropoda" family="Turridae" genus="Turris" kingdom="Animalia" order="Neogastropoda" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="garnonsii">
