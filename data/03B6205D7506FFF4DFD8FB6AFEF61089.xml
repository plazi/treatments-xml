<taxonomicName id="4C1FEAC87501FFF4DAC8F8E0FEFF1088" baseAuthorityName="Linne" baseAuthorityYear="1758" class="Gastropoda" family="Turridae" genus="Turris" kingdom="Animalia" order="Neogastropoda" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="babylonia">
