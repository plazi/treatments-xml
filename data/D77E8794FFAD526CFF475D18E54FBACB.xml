</taxonomicName>
<emphasis id="6DA3EA90FFAC526FFF475FA8E7A6BFD2" bold="true" box="[151,231,1982,2006]" pageId="9" pageNumber="295">
<taxonomicNameLabel id="769057EBFFAC526FFF475FA8E7A6BFD2" box="[151,231,1982,2006]" pageId="9" pageNumber="295" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, paratype, slide No. DZH08087; 17, 
<taxonomicName id="98D74D01FFAC526FFD8C5FA8E44ABFD2" authority="Wang &amp; Liu, 2019" authorityName="Wang &amp; Liu" authorityYear="2019" box="[604,779,1982,2006]" class="Insecta" family="Oecophoridae" genus="Promalactis" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="295" phylum="Arthropoda" rank="species" species="scalpelliformis" status="sp. nov.">
<emphasis id="6DA3EA90FFAC526FFD8C5FA8E44ABFD2" box="[604,779,1982,2006]" italics="true" pageId="9" pageNumber="295">P. scalpelliformis</emphasis>
</taxonomicName>
<emphasis id="6DA3EA90FFAC526FFCC05FA8E421BFD2" bold="true" box="[784,864,1982,2006]" pageId="9" pageNumber="295">
<taxonomicNameLabel id="769057EBFFAC526FFCC05FA8E421BFD2" box="[784,864,1982,2006]" pageId="9" pageNumber="295" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
