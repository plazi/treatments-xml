<number id="3D92B77BFF82FFAE913DF934E1251EAD" box="[538,594,1749,1774]" pageId="9" pageNumber="380" value="1916.0">1916</number>
</taxonomicName>
were treated as junior subjective synonyms of 
<taxonomicName id="4C5ADEDFFF82FFAE9752F937E6231EAE" box="[1141,1364,1750,1773]" class="Insecta" family="Cossidae" genus="Strigocossus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="380" phylum="Arthropoda" rank="species" species="crassa">
<emphasis id="B92E794EFF82FFAE9752F937E6231EAE" box="[1141,1364,1750,1773]" italics="true" pageId="9" pageNumber="380">Strigocossus crassa</emphasis>
</taxonomicName>
while taxa 
<taxonomicName id="4C5ADEDFFF82FFAE93F4F918E1061F51" authority="Aurivillius, 1910" authorityName="Aurivillius" authorityYear="1910" box="[211,625,1785,1810]" class="Insecta" family="Cossidae" genus="Xyleutes" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="380" phylum="Arthropoda" rank="species" species="sjostedti">
