<typeStatus id="67028800AF78FFB0FB67FE4EFAE6FE46" box="[1177,1335,464,490]" pageId="8" pageNumber="7">LECTOTYPE</typeStatus>
/ 
<taxonomicName id="7FB94D21AF78FFB0FAC1FE4EFE72FDA7" authority="Gorham" authorityName="Gorham" authorityYear="1884" class="Insecta" family="Lampyridae" genus="Photinus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="ignis-fatuus">
