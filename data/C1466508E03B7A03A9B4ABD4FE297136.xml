</taxonomicName>
from 
<taxonomicName id="8EEFAF9DE03B7A03AAC7AABAFB347016" authority="Jacobi, 1917" authorityName="Jacobi" authorityYear="1917" box="[996,1267,505,529]" class="Insecta" family="Cercopidae" genus="Amberana" kingdom="Animalia" order="Hemiptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="uncinita">
