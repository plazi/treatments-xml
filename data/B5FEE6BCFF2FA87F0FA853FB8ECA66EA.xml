: 24 
<taxonomicName id="99F4A2402973DD5710BDCF42D6E4388A" class="Insecta" family="Carabidae" genus="Singilis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Singilis filicornis" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="filicornis">Singilis filicornis</taxonomicName>
