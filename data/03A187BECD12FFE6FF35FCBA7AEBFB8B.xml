<taxonomicName id="4C084D2BCD12FFE6FF35FCF17958FC87" authority="Cook, 1896 a: 12" authorityName="Cook" authorityPageNumber="12" authorityYear="1896" box="[151,537,873,895]" class="Diplopoda" family="Platyrhacidae" genus="Psammodesmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polydesmida" pageId="22" pageNumber="267" phylum="Arthropoda" rank="species" species="cos">
<emphasis id="B97CEABACD12FFE6FF35FCF17A22FC87" box="[151,355,873,895]" italics="true" pageId="22" pageNumber="267">Psammodesmus cos</emphasis>
<treatmentCitation id="0AA910B9CD12FFE6FEC8FCF17958FC87" author="" box="[362,537,873,895]" page="12" pageId="22" pageNumber="267">
<bibRefCitation id="EF994B59CD12FFE6FEC8FCF17AB0FC87" box="[362,497,873,895]" pageId="22" pageNumber="267" refId="ref15722" refString="Cook, O. F. (1896 a) New american Platyrhacidae. Brandtia, 12, 51 - 54." type="journal article">Cook, 1896a</bibRefCitation>
: 12
</treatmentCitation>
</taxonomicName>
: 52 (original description); 
<treatmentCitation id="0AA910B9CD12FFE6FC8FFCF17F44FC87" author="" box="[813,1029,873,895]" page="304" pageId="22" pageNumber="267">
<bibRefCitation id="EF994B59CD12FFE6FC8FFCF17895FC87" box="[813,980,873,895]" pageId="22" pageNumber="267" refId="ref16103" refString="Hoffman, R. L. (1953 b) Psammodesmus, a neglected milliped genus (Polydesmida: Platyrhacidae). Journal of the Washington Academy of Sciences, 43, 299 - 304." type="journal article">Hoffman 1953b</bibRefCitation>
: 304
</treatmentCitation>
(in dichotomous key); 
<treatmentCitation id="0AA910B9CD12FFE6FB5AFCF17A61FC66" author="" page="49" pageId="22" pageNumber="267">
<bibRefCitation id="EF994B59CD12FFE6FB5AFCF17BBFFC66" pageId="22" pageNumber="267" refId="ref17075" refString="Martinez-Torres, D. (2016) Revision taxonomica y distribucion geografica de los milpies del genero Psammodesmus Cook, 1896 (Diplopoda: Polydesmida: Platyrhacidae). MSc Thesis in Biology, Universidad Nacional de Colombia, Facultad de Ciencias, Departamento de Biologia, Bogota D. C., Colombia, pp. 1 - 182." type="book chapter">Martínez-Torres 2016</bibRefCitation>
: 49
</treatmentCitation>
