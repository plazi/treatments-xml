<taxonomicName id="4C13DDB7D84B967F794BFBE5FB30FBB6" authorityName="Nielson" authorityYear="2015" box="[1061,1201,1059,1083]" class="Insecta" family="Cicadellidae" genus="Taharana" kingdom="Animalia" order="Hemiptera" pageId="63" pageNumber="62" phylum="Arthropoda" rank="species" species="biavicula">
