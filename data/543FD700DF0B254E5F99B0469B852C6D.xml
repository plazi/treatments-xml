<taxonomicName id="EEA0827F1C39C21F097F2ADD7CBBC8F5" LSID-ZBK="urn:lsid:zoobank.org:act:AB54E069-3D43-4302-90AA-DADED5490A0B" class="Elasmobranchii" family="Scyliorhinidae" genus="Halaelurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Holohalaelurus Halaelurus Fowler 1934:235" order="Carcharhiniformes" pageNumber="3" phylum="Chordata" rank="subGenus" subGenus="Holohalaelurus">Halaelurus (Holohalaelurus) Fowler, 1934</taxonomicName>
