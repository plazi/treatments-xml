<taxonomicName id="BC127CFF6DD80AB5ED7CA2F993C032AA" LSID="http://zoobank.org/334BDA58-C792-4808-8B47-F70AE0C55B75" class="Malacostraca" family="Agnaridae" genus="Protracheoniscus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Protracheoniscus sarii" order="Isopoda" pageId="4" pageNumber="63" phylum="Arthropoda" rank="species" species="sarii">Protracheoniscus sarii</taxonomicName>
