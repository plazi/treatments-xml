<paragraph id="8868D18D41D4C23E4E2807BC8373B499" pageId="22" pageNumber="85">
<taxonomicName id="BD4BB8D4D286912A23741EE8C8D9A716" class="Insecta" family="Formicidae" genus="Crematogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crematogaster tumidula" order="Hymenoptera" pageId="22" pageNumber="85" phylum="Arthropoda" rank="species" species="tumidula">
