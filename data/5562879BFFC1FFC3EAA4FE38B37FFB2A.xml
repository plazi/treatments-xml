<taxonomicName id="1ACB4D0EFFC1FFC3EAA4FDA5B25EFD89" authority="Richards, 1964: 44" authorityName="Richards" authorityPageNumber="44" authorityYear="1964" box="[151,568,547,570]" class="Insecta" family="Sphaeroceridae" genus="Frutillaria" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="propinqua">
<emphasis id="EFBFEA9FFFC1FFC3EAA4FDA5B114FD8A" box="[151,370,548,570]" italics="true" pageId="19" pageNumber="20">Frutillaria propinqua</emphasis>
<treatmentCitation id="5C6A109CFFC1FFC3EB4BFDA2B25EFD89" author="Richards" box="[376,568,547,569]" page="44" pageId="19" pageNumber="20" year="1964">
<bibRefCitation id="B95A4B7CFFC1FFC3EB4BFDA2B273FD89" author="Richards" box="[376,533,547,569]" pageId="19" pageNumber="20" refString="Richards, O. W. (1964) Three new species of Frutillaria Rich. (Diptera: Sphaeroceridae) from Chile. Proceedings of the Royal Entomological Society of London Series B, 33, 44 - 46." type="journal article" year="1964">
Richards, 
<number id="6B0324AAFFC1FFC3EBECFDA2B273FD89" box="[479,533,547,569]" pageId="19" pageNumber="20" value="1964.0">1964</number>
</bibRefCitation>
