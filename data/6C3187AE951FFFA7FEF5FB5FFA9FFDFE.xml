<collectingRegion id="265CF85A951FFFA7FE3DF907FDB3F907" box="[408,529,1727,1749]" country="France" name="Haute-Normandie" pageId="45" pageNumber="846">Normandy</collectingRegion>
, 
<collectingRegion id="265CF85A951FFFA7FD85F907FD30F907" box="[544,658,1727,1749]" country="France" name="Bourgogne" pageId="45" pageNumber="846">Burgundy</collectingRegion>
, and Castellane, 
<collectingCountry id="9C8F7628951FFFA7FEA6F966FEF4F921" box="[259,342,1758,1779]" name="France" pageId="45" pageNumber="846">France</collectingCountry>
</materialsCitation>
