<collectingRegion id="49D3F822FF3DFF1999BDFE45F84031D4" box="[448,513,439,465]" country="Brazil" name="Bahia" pageId="172" pageNumber="173">Bah√≠a</collectingRegion>
Blanca, central 
<collectingRegion id="49D3F822FF3DFF199AC5FE45F92331D4" box="[696,866,439,465]" country="Mexico" name="Baja California" pageId="172" pageNumber="173">Baja California</collectingRegion>
(
<bibRefCitation id="EF864B31FF3DFF199B0FFE45FE1E31D4" author="Miller, D. J. &amp; Lea, R. N." box="[882,1119,439,465]" pageId="172" pageNumber="173" refId="ref302113" refString="Miller, D. J. &amp; Lea, R. N. (1972) Guide to the coastal marine fishes of California. California Department of Fish and Game Fish Bulletin, 157." type="book" year="1972">Miller and Lea 1972</bibRefCitation>
). Depth: surface (
<bibRefCitation id="EF864B31FF3DFF199D58FE45FB7331F0" author="Miller, D. J. &amp; Lea, R. N." pageId="172" pageNumber="173" refId="ref302113" refString="Miller, D. J. &amp; Lea, R. N. (1972) Guide to the coastal marine fishes of California. California Department of Fish and Game Fish Bulletin, 157." type="book" year="1972">Miller and Lea 1972</bibRefCitation>
), intertidal to 
<quantity id="4CEF9B25FF3DFF1999B0FE29F85231F0" box="[461,531,475,501]" metricMagnitude="2" metricUnit="m" metricValue="1.01" pageId="172" pageNumber="173" unit="m" value="101.0">101 m</quantity>
(
<quantity id="4CEF9B25FF3DFF199A5FFE29F82631F3" box="[546,615,475,502]" metricMagnitude="2" metricUnit="m" metricValue="1.008888" pageId="172" pageNumber="173" unit="ft" value="331.0">331 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FF3DFF199AC2FE29F9A731F0" author="Ruiz-Campos, G. &amp; Gonzalez-Guzman, S. &amp; Ramirez-Valdez, A. &amp; Gonzalez-Acosta, A. F. &amp; Castro-Aguirre, J. L. &amp; Aguero, J." box="[703,998,475,501]" pageId="172" pageNumber="173" pagination="210 - 220" refId="ref319076" refString="Ruiz-Campos, G., Gonzalez-Guzman, S., Ramirez-Valdez, A., Gonzalez-Acosta, A. F., Castro-Aguirre, J. L. &amp; de la Cruz- Aguero, J. (2010 b) Composition, density and biogeographic affinities of the rocky intertidal fishes on the western coast of the Baja California Peninsula, Mexico. CalCOFI Reports, 51, 210 - 220." type="journal article" year="2010">
Ruiz-Campos 
<emphasis id="B963EAD2FF3DFF199B22FE29F9D731F0" box="[863,918,475,501]" italics="true" pageId="172" pageNumber="173">et al.</emphasis>
2010b
</bibRefCitation>
; max.: 
<bibRefCitation id="EF864B31FF3DFF199C4BFE29FF6331F0" author="Weinberg, K. L. &amp; Wilkins, M. E. &amp; Shaw, F. R. &amp; Zimmerman, M." box="[1078,1314,475,501]" pageId="172" pageNumber="173" refId="ref332794" refString="Weinberg, K. L., Wilkins, M. E., Shaw, F. R. &amp; Zimmerman, M. (2002) The 2001 Pacific west coast bottom trawl survey of groundfish resources: estimates of distribution, abundance, and length and age composition. National Marine Fisheries Service Technical Memorandum, NMFS - AFSC - 128." type="book" year="2002">
Weinberg 
<emphasis id="B963EAD2FF3DFF199CD5FE29FEA131F0" box="[1192,1248,475,501]" italics="true" pageId="172" pageNumber="173">et al.</emphasis>
2002
</bibRefCitation>
). Formerly 
<taxonomicName id="4C174D43FF3DFF1998BBFE0DFBF9321C" authorityName="Agassiz" authorityYear="1861" box="[198,440,511,537]" class="Actinopterygii" family="Embiotocidae" genus="Hyperprosopon" kingdom="Animalia" order="Perciformes" pageId="172" pageNumber="173" phylum="Chordata" rank="species" species="anale">
<emphasis id="B963EAD2FF3DFF1998BBFE0DFBF9321C" box="[198,440,511,537]" italics="true" pageId="172" pageNumber="173">Hyperprosopon anale</emphasis>
</taxonomicName>
, we follow the genetic studies of 
<bibRefCitation id="EF864B31FF3DFF199B53FE0DFE1E321C" author="Longo, G. &amp; Bernardi, G." box="[814,1119,510,537]" pageId="172" pageNumber="173" pagination="55 - 63" refId="ref296858" refString="Longo, G. &amp; Bernardi, G. (2015) The evolutionary history of the embiotocid surfperch radiation based on genome-wide RAD sequence data. Molecular Phylogenetics and Evolution, 88, 55 - 63. https: // doi. org / 10.1016 / j. ympev. 2015.03.027" type="journal article" year="2015">Longo and Bernardi (2015)</bibRefCitation>
and 
<bibRefCitation id="EF864B31FF3DFF199CE8FE0DFF2C321C" author="Longo, G. C. &amp; Bernardi, G. &amp; Lea, R. N." box="[1173,1389,511,537]" pageId="172" pageNumber="173" pagination="591 - 596" refId="ref296910" refString="Longo, G. C., Bernardi, G. &amp; Lea, R. N. (2018) Taxonomic revisions within Embiotocidae (Teleostei, Perciformes) based on molecular phylogenetics. Zootaxa, 4482, 591 - 596. https: // doi. org / 10.11646 / zootaxa. 4482.3.10" type="journal article" year="2018">
Longo 
<emphasis id="B963EAD2FF3DFF199C98FE0DFF5D321C" box="[1253,1308,511,537]" italics="true" pageId="172" pageNumber="173">et al.</emphasis>
(2018)
</bibRefCitation>
and place this species in the genus 
<taxonomicName id="4C174D43FF3DFF199A63FDD1F8903238" authorityName="Gill" authorityYear="1862" box="[542,721,547,573]" class="Actinopterygii" family="Embiotocidae" genus="Hypocritichthys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="172" pageNumber="173" phylum="Chordata" rank="genus">
<emphasis id="B963EAD2FF3DFF199A63FDD1F8903238" box="[542,721,547,573]" italics="true" pageId="172" pageNumber="173">Hypocritichthys</emphasis>
</taxonomicName>
.
</paragraph>
