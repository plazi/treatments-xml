<emphasis id="94D1EAB3015A667CFEDDFF18FEEC46AA" box="[264,365,189,215]" italics="true" pageId="4" pageNumber="33">Stolliana</emphasis>
</taxonomicName>
also 
<taxonomicName id="61A54D22015A667CFE72FF18FD1146AA" box="[423,656,189,215]" class="Insecta" family="Pamphagidae" genus="Pamphagus" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="sordidus">
<emphasis id="94D1EAB3015A667CFE72FF18FD1146AA" box="[423,656,189,215]" italics="true" pageId="4" pageNumber="33">Pamphagus sordidus</emphasis>
</taxonomicName>
[presently 
<taxonomicName id="61A54D22015A667CFCDBFF18FB4546AA" authority="(Walker, 1870)" baseAuthorityName="Walker" baseAuthorityYear="1870" box="[782,1220,188,215]" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="sordidum">
<emphasis id="94D1EAB3015A667CFCDBFF18FB9346AA" box="[782,1042,189,215]" italics="true" pageId="4" pageNumber="33">Pagopedilum sordidum</emphasis>
(
<bibRefCitation id="C2344B50015A667CFBF5FF18FB3D46AA" author="Walker, F." box="[1056,1212,188,215]" pageId="4" pageNumber="33" pagination="425 - 604" refId="ref13004" refString="Walker, F. (1870) Catalogue of the Specimens of Dermaptera Saltatoria in the Collection of the British Museum. British Museum, 3, 425 - 604. [http: // www. archive. org / details / catalogueofspeci 03 britrich]" type="journal article" year="1870">Walker, 1870</bibRefCitation>
)
</taxonomicName>
], 
<taxonomicName id="61A54D22015A667CFB03FF18FF404686" authority="Burmeister, 1838" authorityName="Saussure" authorityYear="1887" class="Insecta" family="Pamphagidae" genus="Xiphocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="bradyana">
<emphasis id="94D1EAB3015A667CFB03FF18FF404686" italics="true" pageId="4" pageNumber="33">Xiphocera bradyana</emphasis>
</taxonomicName>
[presently 
<taxonomicName id="61A54D22015A667CFEEBFF44FCA14686" authority="(Saussure, 1887)" baseAuthorityName="Saussure" baseAuthorityYear="1887" box="[318,800,224,251]" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="bradyanum">
<emphasis id="94D1EAB3015A667CFEEBFF44FDDB4686" box="[318,602,225,251]" italics="true" pageId="4" pageNumber="33">Pagopedilum bradyanum</emphasis>
(Saussure, 1887)
</taxonomicName>
], 
<taxonomicName id="61A54D22015A667CFCE7FF44FB8E4686" authority="Burmeister, 1838" authorityName="Burmeister" authorityYear="1838" box="[818,1039,225,251]" class="Insecta" family="Pamphagidae" genus="Xiphocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="mannula">
<emphasis id="94D1EAB3015A667CFCE7FF44FB8E4686" box="[818,1039,225,251]" italics="true" pageId="4" pageNumber="33">Xiphocera mannula</emphasis>
</taxonomicName>
(presently synonymous with 
<taxonomicName id="61A54D22015A667CFA8DFF47FEF94762" baseAuthorityName="Saussure" baseAuthorityYear="1887" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="bradyanum">
<emphasis id="94D1EAB3015A667CFA8DFF47FEF94762" italics="true" pageId="4" pageNumber="33">Pagopedilum bradyanum</emphasis>
</taxonomicName>
), and two species presently belonging to the genus 
<taxonomicName id="61A54D22015A667CFC6EFEA0FBD34762" authorityName="Dirsh" authorityYear="1958" box="[955,1106,261,287]" class="Insecta" family="Pamphagidae" genus="Lobosceliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFC6EFEA0FBD34762" box="[955,1106,261,287]" italics="true" pageId="4" pageNumber="33">Lobosceliana</emphasis>
</taxonomicName>
[
<taxonomicName id="61A54D22015A667CFBB7FEA0FF4E473E" authority="Kirby, 1902" authorityName="Kirby" authorityYear="1902" class="Insecta" family="Dzhajloutshellidae" genus="Xiphicera" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="compressa">
<emphasis id="94D1EAB3015A667CFBB7FEA0FACE4762" box="[1122,1359,261,287]" italics="true" pageId="4" pageNumber="33">Xiphicera compressa</emphasis>
<bibRefCitation id="C2344B50015A667CFA83FEA0FF4E473E" author="Kirby, W. F." pageId="4" pageNumber="33" pagination="93 - 101" refId="ref12337" refString="Kirby, W. F. (1902) List of a small collection of orthopterous insects formed by Sir Harry Johnston in British East Africa and Uganda in 1899 and 1900, with descriptions of five new species. Proceedings of the Zoological Society of London, 1902 (1), 93 - 101. [http: // www. biodiversitylibrary. org / item / 98524  page / 131 / mode / 1 up]" type="journal article" year="1902">Kirby, 1902</bibRefCitation>
</taxonomicName>
= 
<taxonomicName id="61A54D22015A667CFF3EFE8CFD33473E" authority="(Schaum, 1853)" baseAuthorityName="Schaum" baseAuthorityYear="1853" box="[235,690,296,323]" class="Insecta" family="Pamphagidae" genus="Lobosceliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="loboscelis">
<emphasis id="94D1EAB3015A667CFF3EFE8CFE79473E" box="[235,504,297,323]" italics="true" pageId="4" pageNumber="33">Lobosceliana loboscelis</emphasis>
(Schaum, 1853)
</taxonomicName>
] and to the genus 
<taxonomicName id="61A54D22015A667CFCAFFE8CFB9D473E" authorityName="Dirsh" authorityYear="1958" box="[890,1052,297,323]" class="Insecta" family="Pamphagidae" genus="Transvaaliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFCAFFE8CFB9D473E" box="[890,1052,297,323]" italics="true" pageId="4" pageNumber="33">Transvaaliana</emphasis>
</taxonomicName>
(
<taxonomicName id="61A54D22015A667CFBFEFE8CFA1D473E" authority="Kirby, 1902" authorityName="Kirby" authorityYear="1902" box="[1067,1436,297,323]" class="Insecta" family="Dzhajloutshellidae" genus="Xiphicera" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="granulosa">
<emphasis id="94D1EAB3015A667CFBFEFE8CFA93473E" box="[1067,1298,297,323]" italics="true" pageId="4" pageNumber="33">Xiphicera granulosa</emphasis>
<bibRefCitation id="C2344B50015A667CFACDFE8CFA1D473E" author="Kirby, W. F." box="[1304,1436,297,323]" pageId="4" pageNumber="33" pagination="93 - 101" refId="ref12337" refString="Kirby, W. F. (1902) List of a small collection of orthopterous insects formed by Sir Harry Johnston in British East Africa and Uganda in 1899 and 1900, with descriptions of five new species. Proceedings of the Zoological Society of London, 1902 (1), 93 - 101. [http: // www. biodiversitylibrary. org / item / 98524  page / 131 / mode / 1 up]" type="journal article" year="1902">Kirby, 1902</bibRefCitation>
</taxonomicName>
= 
<taxonomicName id="61A54D22015A667CFF7BFEE8FE48471A" baseAuthorityName="W. F. Kirby" baseAuthorityYear="1902" box="[174,457,333,359]" class="Insecta" family="Pamphagidae" genus="Transvaaliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="granulosa">
<emphasis id="94D1EAB3015A667CFF7BFEE8FE48471A" box="[174,457,333,359]" italics="true" pageId="4" pageNumber="33">Transvaaliana granulosa</emphasis>
</taxonomicName>
).
</paragraph>
</subSubSection>
<subSubSection id="EEBF652A015A667CFF12FED4FAF045FE" pageId="4" pageNumber="33" type="reference_group">
<paragraph id="A61A36A1015A667CFF12FED4FE7844D6" blockId="4.[151,1437,152,899]" pageId="4" pageNumber="33">
<taxonomicName id="61A54D22015A667CFF12FED4FEDD47F6" authorityName="Karsch" authorityYear="1896" box="[199,348,369,395]" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFF12FED4FEDD47F6" box="[199,348,369,395]" italics="true" pageId="4" pageNumber="33">Pagopedilum</emphasis>
</taxonomicName>
and 
<taxonomicName id="61A54D22015A667CFE4CFED4FE7F47F6" authorityName="Bolivar" authorityYear="1916" box="[409,510,369,395]" class="Insecta" family="Pamphagidae" genus="Stolliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFE4CFED4FE7F47F6" box="[409,510,369,395]" italics="true" pageId="4" pageNumber="33">Stolliana</emphasis>
</taxonomicName>
are very similar and difficult to separate; further, some species are known only from the female sex. In both genera the pronotal crest may be more or less high and the prosternal tubercle is highly variable [see the detail of prosternal process in the figure of 
<taxonomicName id="61A54D22015A667CFC90FE1CFBFB47AE" authorityName="Karsch" authorityYear="1896" box="[837,1146,441,467]" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="subcruciatum">
<emphasis id="94D1EAB3015A667CFC90FE1CFBFB47AE" box="[837,1146,441,467]" italics="true" pageId="4" pageNumber="33">Pagopedilum subcruciatum</emphasis>
</taxonomicName>
by 
<bibRefCitation id="C2344B50015A667CFB72FE1CFACA47A9" author="Karsch, F. A. F." box="[1191,1355,441,468]" pageId="4" pageNumber="33" pagination="242 - 359" refId="ref12288" refString="Karsch, F. A. F. (1896) Neue Orthopteren aus dem tropischen Afrika. Stettiner Entomologische Zeitung, 57, 242 - 359." type="journal article" year="1896">Karsch (1896)</bibRefCitation>
, reproduced in Fig. 21 and also the prosternal process of some specimens of different species in Figs. 7-10 and 19-20]. Both genera have the abdomino-femoral stridulatory organ below the Krauss’ organ, and the males of both genera have the hind wings atrophied. Within the male genitalia, it is possible to find small differences in the teeth position of pseudolophi on the epiphallus and in the shape of apical valves of penis (Figs. 29-40); these differences do not justify the erection of different genera. In conclusion, the following synonymy is proposed: 
<taxonomicName id="61A54D22015A667CFB7CFDC8FF4E44D6" authority="Karsch, 1896" authorityName="Karsch" authorityYear="1896" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFB7CFDC8FABF44FA" box="[1193,1342,621,647]" italics="true" pageId="4" pageNumber="33">Pagopedilum</emphasis>
<bibRefCitation id="C2344B50015A667CFA93FDC8FF4E44D6" author="Karsch, F. A. F." pageId="4" pageNumber="33" pagination="242 - 359" refId="ref12288" refString="Karsch, F. A. F. (1896) Neue Orthopteren aus dem tropischen Afrika. Stettiner Entomologische Zeitung, 57, 242 - 359." type="journal article" year="1896">Karsch, 1896</bibRefCitation>
</taxonomicName>
= 
<taxonomicName id="61A54D22015A667CFF38FD34FE7444D6" authority="Bolivar, 1916" authorityName="Bolivar" authorityYear="1916" box="[237,501,657,683]" class="Insecta" family="Pamphagidae" genus="Stolliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFF38FD34FED344D6" box="[237,338,657,683]" italics="true" pageId="4" pageNumber="33">Stolliana</emphasis>
<bibRefCitation id="C2344B50015A667CFE8CFD34FE7444D6" author="Bolivar, I." box="[345,501,657,683]" pageId="4" pageNumber="33" refId="ref11882" refString="Bolivar, I. (1916) Orthoptera. Fam. Acrididae. Subfam. Pamphaginae. Genera Insectorum, 170, 40 pp., 1 pl. [http: // www. biodiversitylibrary. org / item / 105422  page / 13 / mode / 1 up]" type="book" year="1916">Bolívar, 1916</bibRefCitation>
</taxonomicName>
.
</paragraph>
<paragraph id="A61A36A1015A667CFF12FD10FAF045FE" blockId="4.[151,1437,152,899]" pageId="4" pageNumber="33">
According to 
<bibRefCitation id="C2344B50015A667CFEBDFD10FE7B44B2" author="Dirsh" box="[360,506,692,719]" pageId="4" pageNumber="33" pagination="299 - 400" refId="ref11934" refString="Dirsh V. M. (1958) Revision of the group Portheti (Orthoptera: Acridoidea). Eos, Revista espanola de Entomologia, 34, 299 - 400" type="journal article" year="1958">Dirsh (1958)</bibRefCitation>
and 
<bibRefCitation id="C2344B50015A667CFDE2FD10FD6744B2" author="Johnsen, P." box="[567,742,693,719]" pageId="4" pageNumber="33" pagination="1 - 22" refId="ref12246" refString="Johnsen, P. (1990) A new genus and three new species of Acridoidea with notes on some lesser known species from southern Africa (Orthoptera). Natura Jutlandica, 23 (1), 1 - 22." type="journal article" year="1990">Johnsen (1990)</bibRefCitation>
four species are presently known in the genus 
<taxonomicName id="61A54D22015A667CFAC5FD10FAF444B2" authorityName="Bolivar" authorityYear="1916" box="[1296,1397,693,719]" class="Insecta" family="Pamphagidae" genus="Stolliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="genus">
<emphasis id="94D1EAB3015A667CFAC5FD10FAF444B2" box="[1296,1397,693,719]" italics="true" pageId="4" pageNumber="33">Stolliana</emphasis>
</taxonomicName>
: 
<taxonomicName id="61A54D22015A667CFA52FD10FE02448E" authority="(Stal, 1875)" baseAuthorityName="Stal" baseAuthorityYear="1875" class="Insecta" family="Pamphagidae" genus="Stolliana" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="33" phylum="Arthropoda" rank="species" species="sabulosa">
<emphasis id="94D1EAB3015A667CFA52FD10FF7A448E" italics="true" pageId="4" pageNumber="33">S. sabulosa</emphasis>
(Stål, 1875)
</taxonomicName>
(
<typeStatus id="791E8803015A667CFE44FD7CFE74448E" box="[401,501,729,755]" pageId="4" pageNumber="33">holotype</typeStatus>
<specimenCount id="B0A3FD28015A667CFE2FFD7CFD8E448E" box="[506,527,729,755]" pageId="4" pageNumber="33" type="male">♂</specimenCount>
from 
<collectingCountry id="DEB27631015A667CFD84FD7CFD35448E" box="[593,692,729,755]" name="Namibia" pageId="4" pageNumber="33">Namibia</collectingCountry>
