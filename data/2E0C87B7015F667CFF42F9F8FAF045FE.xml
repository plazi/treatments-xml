), 
<taxonomicName id="61A54D22015C667AFB18F921FF4E40BC" authority="Bolivar, 1915" authorityName="Bolivar" authorityYear="1915" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="martini">
<emphasis id="94D1EAB3015C667AFB18F921FABA40E0" box="[1229,1339,1668,1693]" italics="true" pageId="2" pageNumber="31">P. martini</emphasis>
Bolívar, 1915
</taxonomicName>
(
<typeStatus id="791E8803015C667AFF35F902FEC540BC" box="[224,324,1703,1729]" pageId="2" pageNumber="31">holotype</typeStatus>
<specimenCount id="B0A3FD28015C667AFE99F90DFEE040BF" box="[332,353,1704,1730]" pageId="2" pageNumber="31" type="female">♀</specimenCount>
from Natal, 
<collectingCountry id="DEB27631015C667AFE23F902FD0A40BF" box="[502,651,1703,1730]" name="South Africa" pageId="2" pageNumber="31">South Africa</collectingCountry>
), 
<taxonomicName id="61A54D22015C667AFD75F90DFB8540BC" authority="(Saussure, 1887)" baseAuthorityName="Saussure" baseAuthorityYear="1887" box="[672,1028,1703,1729]" class="Insecta" family="Pamphagidae" genus="Pagopedilum" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="bradyanum">
<emphasis id="94D1EAB3015C667AFD75F90DFCBC40BC" box="[672,829,1703,1729]" italics="true" pageId="2" pageNumber="31">P. bradyanum</emphasis>
(Saussure, 1887)
</taxonomicName>
(
<typeStatus id="791E8803015C667AFBC1F902FBF940BC" box="[1044,1144,1703,1729]" pageId="2" pageNumber="31">holotype</typeStatus>
<specimenCount id="B0A3FD28015C667AFB54F90DFB1740BF" box="[1153,1174,1704,1730]" pageId="2" pageNumber="31" type="female">♀</specimenCount>
from Transvaal, 
<collectingCountry id="DEB27631015C667AFA8EF902FF60409B" name="South Africa" pageId="2" pageNumber="31">South Africa</collectingCountry>
) [
<bibRefCitation id="C2344B50015C667AFF22F96EFE0D4098" author="Dirsh" box="[247,396,1738,1765]" pageId="2" pageNumber="31" pagination="299 - 400" refId="ref11934" refString="Dirsh V. M. (1958) Revision of the group Portheti (Orthoptera: Acridoidea). Eos, Revista espanola de Entomologia, 34, 299 - 400" type="journal article" year="1958">Dirsh (1958)</bibRefCitation>
