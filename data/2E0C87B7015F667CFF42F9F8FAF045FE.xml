) with 
<taxonomicName id="61A54D22015C667AFEF0F955FDDE4174" authority="Saussure, 1887" authorityName="Saussure" authorityYear="1887" box="[293,607,1775,1801]" class="Insecta" family="Pamphagidae" genus="Xiphocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="31" phylum="Arthropoda" rank="species" species="bradyana">
