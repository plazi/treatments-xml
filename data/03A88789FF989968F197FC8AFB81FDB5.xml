<collectingCountry id="F316760FFF9A9968F067FEDAFE62FE7E" box="[359,426,369,395]" name="China" pageId="22" pageNumber="23">China</collectingCountry>
(
<collectingRegion id="49C5F87DFF9A9968F0B9FEDAFDE8FE79" box="[441,544,369,396]" country="China" name="Guangxi" pageId="22" pageNumber="23">Guangxi</collectingRegion>
