<taxonomicName id="4C31CBD2211C4F19FCD46D1705DF3B4B" authority="Sayed" authorityName="Sayed" authorityYear="1950" box="[857,1155,1078,1102]" class="Arachnida" family="Tenuipalpidae" genus="Aegyptobia" kingdom="Animalia" order="Trombidiformes" pageId="20" pageNumber="448" phylum="Arthropoda" rank="species" species="tragardhi">
