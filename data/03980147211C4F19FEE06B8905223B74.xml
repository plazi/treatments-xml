</taxonomicName>
(
<taxonomicName id="4C31CBD2211C4F19FF2B6D7800183B74" authorityName="Berlese" authorityYear="1913" box="[166,324,1113,1137]" class="Arachnida" family="Tenuipalpidae" kingdom="Animalia" order="Trombidiformes" pageId="20" pageNumber="448" phylum="Arthropoda" rank="family">Tenuipalpidae</taxonomicName>
