<taxonomicName id="4C144D37C661A213FAD9F118FA7F4DEF" authorityName="Temminck" authorityYear="1834" box="[1313,1424,3714,3753]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="23" pageNumber="250" phylum="Chordata" rank="species" species="bicolor">
