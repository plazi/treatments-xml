<taxonomicName id="88D54D474C0722167132684FFDFBD807" authority="Yao &amp; Li" authorityName="Yao &amp; Nie &amp; Li" authorityYear="2019" box="[299,591,1856,1881]" class="Arachnida" family="Pholcidae" genus="Khorata" kingdom="Animalia" order="Araneae" pageId="19" pageNumber="355" phylum="Arthropoda" rank="species" species="wenshan" status="sp. nov.">
