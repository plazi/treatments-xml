<taxonomicName id="F69F7DB80A6D560F2711F0387D1D0E0C" class="Magnoliopsida" family="Asteraceae" genus="Monosis" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="57" pageNumber="82" phylum="Tracheophyta" rank="species" species="parishii">
