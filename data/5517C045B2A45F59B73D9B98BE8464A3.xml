<subSubSection id="17B9FE86D5BF96CEA2113D041D5B25E6" pageId="57" pageNumber="82" type="diagnostic characters">
<paragraph id="B8D6FE4DCE08C52E5B1BD1CC4EF5A3C6" pageId="57" pageNumber="82">Diagnostic characters.</paragraph>
<paragraph id="12CB20FE2B3284870754547471F40BEF" pageId="57" pageNumber="82">
<taxonomicName id="09F3BCA9BA82E4BF0217FC666169802E" class="Magnoliopsida" family="Asteraceae" genus="Monosis" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asterales" pageId="57" pageNumber="82" phylum="Tracheophyta" rank="species" species="volkameriifolia">
<emphasis id="111336990E396CDD3D2EF4F30CD4496E" italics="true" pageId="57" pageNumber="82">Monosis volkameriifolia</emphasis>
</taxonomicName>
