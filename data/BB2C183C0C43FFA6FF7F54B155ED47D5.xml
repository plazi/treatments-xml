<collectionCode id="559431EF0C43FFA5FEA0503555DC4162" box="[328,393,1591,1615]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="1" pageNumber="572">INHS</collectionCode>
Insect Collection 775,583 [
<specimenCount id="258362A30C43FFA5FD28503557764162" box="[704,803,1590,1615]" pageId="1" pageNumber="572" type="female">1 female</specimenCount>
] (
<collectionCode id="559431EF0C43FFA5FCD1503557D54162" box="[825,896,1591,1615]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34797" name="Illinois Natural History Survey" pageId="1" pageNumber="572">INHS</collectionCode>
