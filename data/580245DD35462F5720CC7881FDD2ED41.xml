<taxonomicName id="0D7B2716D06EFEABA3E68E71C36DE58F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147239" authority="" authorityName="" class="Insecta" family="Formicidae" genus="Sphaerocrema" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Sphaerocrema Santschi" lsidName-HNS="Sphaerocrema Santschi" order="Hymenoptera" phylum="Arthropoda" rank="genus">Sphaerocrema</taxonomicName>
