<taxonomicName id="7C38A0D5A5A5DE525BA4D51C37CD3325" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230577" class="Insecta" family="Formicidae" genus="Crematogaster" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crematogaster angusticeps Santschi" lsidName-HNS="Crematogaster angusticeps Santschi" order="Hymenoptera" phylum="Arthropoda" rank="subSpecies" species="luciae" subSpecies="angusticeps">Cr. luciae For. angusticeps</taxonomicName>
