<subSubSection id="C353AFC6FFAFFFA5FF21D39966C8FE9B" pageId="47" pageNumber="48" type="materials_examined">
<paragraph id="8BF6FC4DFFAFFFA5FF21D39966C8FE9B" blockId="47.[151,1437,151,789]" pageId="47" pageNumber="48">
<emphasis id="B93D205FFFAFFFA5FF21D3996590FF37" bold="true" box="[199,384,151,176]" pageId="47" pageNumber="48">
<typeStatus id="54F242EFFFAFFFA5FF21D3996512FF37" box="[199,258,151,176]" pageId="47" pageNumber="48">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54F242EFFFAFFFA5FE74D3966636FF37" box="[402,550,152,176]" pageId="47" pageNumber="48">HOLOTYPE</typeStatus>
. ♂, N.E. 
<collectingCountry id="F35EBCDDFFAFFFA5FD55D3966712FF28" box="[691,770,152,175]" name="Myanmar" pageId="47" pageNumber="48">Burma</collectingCountry>
, Kambaiti, 
<quantity id="4CB151A8FFAFFFA5FC7CD39667ECFF28" box="[922,1020,152,176]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="47" pageNumber="48" unit="m" value="2000.0">2000 m</quantity>
