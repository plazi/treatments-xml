</collectionCode>
. — 
<taxonomicName id="4C7F52EE4476B12BFCC911C0FB37E4C8" authority="(Filhol, 1877)" baseAuthorityName="Filhol" baseAuthorityYear="1877" box="[838,1206,431,455]" class="Reptilia" family="Lacertidae" genus="Pseudeumeces" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="572" phylum="Chordata" rank="species" species="cadurcensis">
