<taxonomicName id="CC03922BF2C48AAD87AD276DB90088C4" LSID="D9566E82-100C-564E-A27F-C3564BCE35FE" class="Insecta" family="Eulophidae" genus="Tetrastichus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetrastichus hylotomarum group" order="Hymenoptera" pageId="0" pageNumber="59177" phylum="Arthropoda" rank="species" species="hylotomarum group">Tetrastichus hylotomarum group</taxonomicName>
