<taxonomicName id="69E3CFAB4E63B56B724CFA677370C5DC" authorityName="Chen" authorityYear="2000" box="[836,1080,1420,1445]" class="Malacostraca" family="Ethusidae" genus="Ethusina" kingdom="Animalia" order="Decapoda" pageId="79" pageNumber="578" phylum="Arthropoda" rank="species" species="vanuatuensis">
