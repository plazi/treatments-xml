<emphasis id="29DDEAC0FF94FFADFF011AE95C3FFD59" bold="true" box="[197,320,741,767]" pageId="12" pageNumber="33">Remarks:</emphasis>
Specimen 
<number id="AD6124F5FF94FFADFE051AE95C81FD59" box="[449,510,741,767]" pageId="12" pageNumber="33" value="1743.0">1743</number>
/ 
<number id="AD6124F5FF94FFADFDCC1AE95F6AFD59" box="[520,533,741,767]" pageId="12" pageNumber="33" value="5.0">5</number>
, labelled as “ 
<typeStatus id="C4128870FF94FFADFD6A1AE95F8DFD59" box="[686,754,741,767]" pageId="12" pageNumber="33">typus</typeStatus>
