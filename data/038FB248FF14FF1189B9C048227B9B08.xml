gen. sp. (17); 
<taxonomicName id="4C2678DDFF16FF118B26C5C623289D9D" box="[520,866,980,1005]" class="Actinopterygii" family="Esocidae" genus="Esox" kingdom="Animalia" order="Esociformes" pageId="167" pageNumber="168" phylum="Chordata" rank="subSpecies" species="americanus" subSpecies="vermiculatus">
