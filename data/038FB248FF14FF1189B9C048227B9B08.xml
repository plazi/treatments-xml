<taxonomicName id="4C2678DDFF16FF118D16C5BC25709DB8" box="[1080,1338,943,968]" class="Actinopterygii" family="Sciaenidae" genus="Aplodinotus" kingdom="Animalia" order="Perciformes" pageId="167" pageNumber="168" phylum="Chordata" rank="species" species="grunniens">
<emphasis id="B952DF4CFF16FF118D16C5BC25709DB8" box="[1080,1338,943,968]" italics="true" pageId="167" pageNumber="168">Aplodinotus grunniens</emphasis>
