<collectingRegion id="C605F82E3076C474FEA4A0E8D543AED7" box="[443,562,1208,1230]" country="Brazil" name="Sao Paulo" pageId="12" pageNumber="49">São Paulo</collectingRegion>
, 
<collectingCountry id="7CD6765C3076C474FD5EA0E8D5F6AED7" box="[577,647,1208,1230]" name="Brazil" pageId="12" pageNumber="49">Brazil</collectingCountry>
(
<bibRefCitation id="60504B3D3076C474FD86A0E8D7A7AEF4" author="Marcus E" pageId="12" pageNumber="49" pagination="3 - 133" refId="ref16000" refString="Marcus E. 1945 a. Sobre Catenulida brasileiros. Boletins da Facultade de Filosofia, Ciencias e Letras Zoologia 10: 3 - 133." type="journal article" year="1945">Marcus, 1945a</bibRefCitation>
).
</paragraph>
