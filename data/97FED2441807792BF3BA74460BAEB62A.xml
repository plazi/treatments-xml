and 
<taxonomicName id="D346C0511C03A2D89EEF917DCCC39E2F" class="Insecta" family="Asilidae" genus="Oratostylum" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="119" phylum="Arthropoda" rank="genus">Oratostylum</taxonomicName>
