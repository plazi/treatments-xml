: 
<number id="3DF09053FFA0FFA6982CF8669B514ACE" box="[261,276,1964,1988]" pageId="47" pageNumber="48" value="1.0">1</number>
: 
<number id="3DF09053FFA0FFA69830F8669B6D4ACE" box="[281,296,1964,1988]" pageId="47" pageNumber="48" value="1.0">1</number>
; 
<number id="3DF09053FFA0FFA6981EF8669B0D4ACE" box="[311,328,1964,1988]" pageId="47" pageNumber="48" value="3.0">3</number>
: 
<number id="3DF09053FFA0FFA69864F8669B254ACE" box="[333,352,1964,1988]" pageId="47" pageNumber="48" value="2.0">2</number>
: 
<number id="3DF09053FFA0FFA6984CF8669B3D4ACE" box="[357,376,1964,1988]" pageId="47" pageNumber="48" value="2.0">2</number>
: 
<number id="3DF09053FFA0FFA69854F8669BD54ACE" box="[381,400,1964,1988]" pageId="47" pageNumber="48" value="2.0">2</number>
