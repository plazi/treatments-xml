<taxonomicName id="77CDFEE4FF918A2584521F5BFD2DF897" authority="Strand" authorityName="Strand" box="[333,666,1863,1887]" class="Arachnida" family="Ctenidae" genus="Enoploctenus" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="70" phylum="Arthropoda" rank="species" species="inazensis">
<taxonomicName id="77CDFEE4FF918A2584521F5BFDFEF897" box="[333,585,1863,1887]" class="Arachnida" family="Ctenidae" genus="Enoploctenus" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="70" phylum="Arthropoda" rank="species" species="inazensis">
<emphasis id="82B95975FF918A2584521F5BFDFEF897" box="[333,585,1863,1887]" italics="true" pageId="3" pageNumber="70">
Enoploctenus 
