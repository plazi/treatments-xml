<quantity id="4CB878D2FFE45F4CBE4BFC5BFE2AC9C8" box="[289,415,908,932]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="2" pageNumber="31" unit="mm" value="0.85" valueMax="0.9" valueMin="0.8">0.8-0.9 mm</quantity>
, rostrum to 
<quantity id="4CB878D2FFE45F4CBD74FC5BFDDAC9C8" box="[542,623,908,932]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" pageId="2" pageNumber="31" unit="mm" value="9.5">9.5 mm</quantity>
.................... .............................................................. 
