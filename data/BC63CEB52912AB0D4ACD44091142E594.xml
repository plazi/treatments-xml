<paragraph id="7CAFD438AA5259A6FDD030DAA4425E7A" pageId="0" pageNumber="195">Etymology.</paragraph>
<paragraph id="D967946553DE471E4BF1D16F9A6F8F47" pageId="0" pageNumber="195">
Greek 
<taxonomicName id="7CF3C9390C692BA082977AFF011B5987" lsidName="gaiophanes" pageId="0" pageNumber="195" rank="species" species="gaiophanes">gaiophanes</taxonomicName>
