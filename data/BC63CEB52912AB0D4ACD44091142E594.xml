(
<geoCoordinate id="BFBBE402179565AB1994E020C494B6FE" degrees="23.57278" direction="south" orientation="latitude" precision="1" value="-23.57278">-23.57278</geoCoordinate>
, 
<geoCoordinate id="A321401BEAF29E85E986C6C2F8040A65" degrees="15.81028" direction="east" orientation="longitude" precision="1" value="15.81028">15.81028</geoCoordinate>
).
</paragraph>
