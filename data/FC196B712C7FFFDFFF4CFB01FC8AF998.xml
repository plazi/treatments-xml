<treatment id="FC196B712C7FFFDFFF4CFB01FC8AF998" ID-DOI="http://doi.org/10.5281/zenodo.6243101" ID-GBIF-Taxon="119372771" ID-Zenodo-Dep="6243101" LSID="urn:lsid:plazi:treatment:FC196B712C7FFFDFFF4CFB01FC8AF998" httpUri="http://treatment.plazi.org/id/FC196B712C7FFFDFFF4CFB01FC8AF998" lastPageId="2" lastPageNumber="35" pageId="1" pageNumber="34">
<subSubSection id="3CAA89EC2C7FFFDCFF4CFB01FE52F896" box="[151,475,1882,1908]" pageId="1" pageNumber="34" type="nomenclature">
<paragraph id="740FDA672C7FFFDCFF4CFB01FE52F896" blockId="1.[151,475,1882,1908]" box="[151,475,1882,1908]" pageId="1" pageNumber="34">
<heading id="2F476D0B2C7FFFDCFF4CFB01FE52F896" bold="true" box="[151,475,1882,1908]" fontSize="11" level="1" pageId="1" pageNumber="34" reason="1">
<emphasis id="46C406752C7FFFDCFF4CFB01FE52F896" bold="true" box="[151,475,1882,1908]" pageId="1" pageNumber="34">
<taxonomicName id="B3B0A1E42C7FFFDCFF4CFB01FEC3F896" box="[151,330,1882,1908]" class="Malacostraca" family="Kotumsaridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="34" phylum="Arthropoda" rank="family">Kotumsaridae</taxonomicName>
