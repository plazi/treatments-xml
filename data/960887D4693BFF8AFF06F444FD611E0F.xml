<quantity id="D9599B276938FF8CFC39F2D6FB931FDB" box="[936,1024,153,179]" metricMagnitude="-3" metricUnit="m" metricValue="3.9" pageId="10" pageNumber="412" unit="mm" value="3.9">3.9 mm</quantity>
