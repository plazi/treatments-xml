<quantity id="D9599B276938FF8CFD14F2D6FD4E1FDB" box="[645,733,153,179]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="10" pageNumber="412" unit="mm" value="4.8">4.8 mm</quantity>
