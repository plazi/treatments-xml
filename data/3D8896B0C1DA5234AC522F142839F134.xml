<taxonomicName id="5C765D36197D9F4E223121CCE00F26AA" baseAuthorityName="L. Koch" baseAuthorityYear="1875" class="Arachnida" family="Sparassidae" genus="Thelcticopis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thelcticopis severa" order="Araneae" pageId="0" pageNumber="105" phylum="Arthropoda" rank="species" species="severa">
<emphasis id="616EB363324FF5D4AEE19599D739A520" italics="true" pageId="0" pageNumber="105">Thelcticopis severa</emphasis>
