<taxonomicName id="4C7E4D4FFFBF9113987FFEB3FD6EFE41" box="[556,640,369,392]" class="Insecta" family="Phlaeothripidae" genus="Strepterothrips" kingdom="Animalia" order="Thysanoptera" pageId="10" pageNumber="335" phylum="Arthropoda" rank="species" species="apterus">
