<quantity id="5A261FAD808DFF2EEC26CD9918FF0DBB" box="[333,418,944,968]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="137" pageNumber="138" unit="m" value="15.0" valueMax="30.0" valueMin="0.0">0–30 m</quantity>
