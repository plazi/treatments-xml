<emphasis id="AFAA6E5A808DFF2EEDACCD4E18AD0DFC" bold="true" box="[199,496,871,896]" pageId="137" pageNumber="138">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA808DFF2EEC9DCD411BDB0DFC" box="[502,646,871,896]" country="Papua New Guinea" name="New Ireland" pageId="137" pageNumber="138">New Ireland</collectingRegion>
