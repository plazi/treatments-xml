<collectingCountry id="E5C9F2D8808DFF2EEEA0CD4E1D650DFC" box="[971,1080,871,896]" name="Indonesia" pageId="137" pageNumber="138">Indonesia</collectingCountry>
east to 
<collectingCountry id="E5C9F2D8808DFF2EE9E6CD4E1D930DFC" box="[1165,1230,871,896]" name="Palau" pageId="137" pageNumber="138">Palau</collectingCountry>
, 
<collectingRegion id="5F1A7CAA808DFF2EE9B2CD411C580D03" box="[1241,1285,872,895]" country="Micronesia" name="Yap" pageId="137" pageNumber="138">Yap</collectingRegion>
(
<collectingCountry id="E5C9F2D8808DFF2EE878CD4E1CC80DFC" box="[1299,1429,871,896]" name="Micronesia" pageId="137" pageNumber="138">Micronesia</collectingCountry>
