<taxonomicName id="39C7C309279B3A8C4C837EE87DC7B73B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179590" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus maculatus manzer Forel" lsidName-HNS="Camponotus maculatus manzer Forel" order="Hymenoptera" pageNumber="452" phylum="Arthropoda" rank="subSpecies" species="maculatus" subSpecies="manzer">Camponotus maculatus F. subsp. manzer</taxonomicName>
