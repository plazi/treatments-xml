<taxonomicName id="8FEE4D41FF8DFFA6FD73FEA46148CA5A" authorityName="in Floch and Abonnenc" authorityYear="1940" box="[739,1035,259,285]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="100" pageNumber="101" phylum="Arthropoda" rank="species" species="auricularium">
