<quantity id="4CD9A39F397BFFBDFF26FA90FEC42956" box="[214,291,1356,1378]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="51" pageNumber="716" unit="mm" value="0.2">0.2 mm</quantity>
. (78, 79) 
<taxonomicName id="4C2175F9397BFFBDFE7BFA90FD442956" authorityName="Lobl &amp; Liang Tang" authorityYear="2013" box="[395,675,1356,1378]" class="Insecta" family="Staphylinidae" genus="Pseudobironium" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="716" phylum="Arthropoda" rank="species" species="antennatum" status="sp. nov.">
<emphasis id="B955D268397BFFBDFE7BFA90FD442956" box="[395,675,1356,1378]" italics="true" pageId="51" pageNumber="716">Pseudobironium antennatum</emphasis>
</taxonomicName>
