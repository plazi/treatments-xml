<materialsCitation id="3B7E3CE8C863FF95FEE6FDE1FD34A72D" ID-GBIF-Occurrence="3042095302" box="[332,657,575,602]" collectionCode="ZIN" pageId="34" pageNumber="185" specimenCode="ZIN 4367" specimenCount="1" typeStatus="holotype">
