<taxonomicName id="6ACB4D39FFEE277CF9F9FE19E11882BF" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" box="[646,809,395,419]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="challaensis">
