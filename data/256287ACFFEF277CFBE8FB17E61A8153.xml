</taxonomicName>
, 
<taxonomicName id="6ACB4D39FFEE277CF83FFE19E19182BF" authorityName="Watts &amp; Humphreys" authorityYear="2006" box="[832,928,395,419]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="exilis">
<emphasis id="9FBFEAA8FFEE277CF83FFE19E19182BF" box="[832,928,395,419]" italics="true" pageId="43" pageNumber="44">L. exilis</emphasis>
