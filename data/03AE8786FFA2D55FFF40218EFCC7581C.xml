<collectingCountry id="F3107600FFA2D55FFF1D229AFEE95E8F" box="[202,311,909,935]" name="Barbados" pageId="31" pageNumber="232">Barbados</collectingCountry>
, 
<collectingCountry id="F3107600FFA2D55FFE92229AFE705E8F" box="[325,430,909,935]" name="Bahamas" pageId="31" pageNumber="232">Bahamas</collectingCountry>
, 
<collectingCountry id="F3107600FFA2D55FFE6A229AFDC45E8F" box="[445,538,909,935]" name="Jamaica" pageId="31" pageNumber="232">Jamaica</collectingCountry>
, 
<collectingCountry id="F3107600FFA2D55FFDFF2299FD6A5E8F" box="[552,692,909,936]" name="Puerto Rico" pageId="31" pageNumber="232">Puerto Rico</collectingCountry>
, 
<collectingCountry id="F3107600FFA2D55FFD15229AFC925E8F" box="[706,844,909,935]" name="Guadeloupe" pageId="31" pageNumber="232">Guadeloupe</collectingCountry>
