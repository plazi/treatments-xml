<subSubSection id="C37B76BFFFD6DD2F34C7FA59FCDEFA44" box="[151,876,1466,1492]" pageId="7" pageNumber="8" type="nomenclature">
<paragraph id="8BDE2534FFD6DD2F34C7FA59FCDEFA44" blockId="7.[151,876,1466,1492]" box="[151,876,1466,1492]" pageId="7" pageNumber="8">
<heading id="D0969258FFD6DD2F34C7FA59FCDEFA44" bold="true" box="[151,876,1466,1492]" fontSize="11" level="1" pageId="7" pageNumber="8" reason="1">
<emphasis id="B915F926FFD6DD2F34C7FA59FCDEFA44" bold="true" box="[151,876,1466,1492]" pageId="7" pageNumber="8">
Superfamily 
<taxonomicName id="4C615EB7FFD6DD2F3569FA59FCDEFA44" authority="Bowman &amp; Gruner, 1973" authorityName="Bowman &amp; Gruner" authorityYear="1973" box="[313,876,1466,1492]" class="Malacostraca" kingdom="Animalia" order="Amphipoda" pageId="7" pageNumber="8" phylum="Arthropoda" rank="superFamily" superFamily="Lanceoloidea">
LANCEOLOIDEA 
<bibRefCitation id="EFF058C5FFD6DD2F3661FA59FCDEFA44" author="Bowman, T. E. &amp; Gruner, H. - E." box="[561,876,1466,1492]" pageId="7" pageNumber="8" pagination="1 - 64" refId="ref73457" refString="Bowman, T. E. &amp; Gruner, H. - E. (1973) The families and genera of Hyperiidea (Crustacea: Amphipoda). Smithsonian Contributions to Zoology No. 146, 1 - 64." type="book chapter" year="1973">Bowman &amp; Gruner, 1973</bibRefCitation>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
