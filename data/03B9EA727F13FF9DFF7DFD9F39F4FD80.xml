</emphasis>
<taxonomicName id="4C1020E77F12FF9DFE83FCBF3F8DFCED" authority="Hoek, 1883" authorityName="Hoek" authorityYear="1883" box="[361,756,844,869]" class="Maxillopoda" family="Scalpellidae" genus="Scalpellum" kingdom="Animalia" order="Pedunculata" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="balanoides">
