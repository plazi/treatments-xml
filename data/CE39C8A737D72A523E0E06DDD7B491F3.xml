<taxonomicName id="D30C0BD8D9CE7B0FEE03317A02305B0E" authority="L." class="Magnoliopsida" family="Plantaginaceae" genus="Veronica" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="197" phylum="Tracheophyta" rank="species" species="triphyllos">
