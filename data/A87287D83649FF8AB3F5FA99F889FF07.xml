<taxonomicName id="E7DB4D4D3649FF8CB3F5FA99F9C2FAFC" authority="Liang &amp; Webb, 2002" authorityName="Liang &amp; Webb" authorityYear="2002" box="[429,706,1326,1350]" class="Insecta" family="Cercopidae" genus="Circulocercopis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="738" phylum="Arthropoda" rank="species" species="fasciatus" status="sp. nov.">
