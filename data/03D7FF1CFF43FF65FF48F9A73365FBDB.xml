<taxonomicName id="4C7E3589FF43FF65FD54F878356CFCDE" baseAuthorityName="Arrow" baseAuthorityYear="1900" box="[732,1040,790,814]" class="Insecta" family="Rutelidae" genus="Epichalcoplethis" kingdom="Animalia" order="Coleoptera" pageId="136" pageNumber="135" phylum="Arthropoda" rank="species" species="velutipes">
