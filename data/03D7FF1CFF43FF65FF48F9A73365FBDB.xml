<taxonomicName id="4C7E3589FF43FF65FD66F85935DDFCBE" authority="velutipes (Arrow, 1900)" authorityName="velutipes (Arrow" authorityYear="1900" baseAuthorityName="Arrow" baseAuthorityYear="1900" box="[750,1185,822,846]" class="Insecta" family="Rutelidae" genus="Epichalcoplethis" kingdom="Animalia" order="Coleoptera" pageId="136" pageNumber="135" phylum="Arthropoda" rank="subSpecies" species="velutipes" subSpecies="velutipes">
