<taxonomicName id="6EBD4D3D4514EB1DFE7C0FE3FCD65497" authority="Heiss 2008" authorityName="Heiss" authorityYear="2008" box="[486,785,466,491]" class="Insecta" family="Aradidae" genus="Antsirabenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="85" phylum="Arthropoda" rank="genus">
