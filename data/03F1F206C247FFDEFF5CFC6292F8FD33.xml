<number id="3D905137C247FFDFFF1CFC4096A0FC38" box="[215,243,942,967]" pageId="10" pageNumber="111" value="23.0">23</number>
</figureCitation>
a–f, 
<number id="3D905137C247FFDFFEEEFC409712FC38" box="[293,321,942,967]" pageId="10" pageNumber="111" value="24.0">24</number>
a–f, 
