<materialsCitation id="3B1B3CEFFFE74B62FE0EFA10BD28DCF6" collectingDate="2019-10-20" collectionCode="N" country="China" location="Forestry University" pageId="4" pageNumber="153" specimenCode="BJFC-S1778, CFCC 54041" specimenCount="1" stateProvince="Beijing" typeStatus="holotype">
