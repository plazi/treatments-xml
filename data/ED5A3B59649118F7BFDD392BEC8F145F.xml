<taxonomicName id="E1F8593416A9FA80A593FBC638DDE023" class="Gastropoda" family="Plectopylidae" genus="Endothyrella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Endothyrella dolakhaensis" order="Stylommatophora" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="dolakhaensis">Endothyrella dolakhaensis</taxonomicName>
