<taxonomicName id="1CD04D477846FFF76D27F91EFCA8EFB5" authority="Butler, 1871: 282" authorityName="Butler" authorityPageNumber="282" authorityYear="1871" box="[382,844,1726,1751]" class="Insecta" family="Nymphalidae" genus="Euptychia" kingdom="Animalia" order="Lepidoptera" pageId="15" pageNumber="14" phylum="Arthropoda" rank="species" species="languida">
