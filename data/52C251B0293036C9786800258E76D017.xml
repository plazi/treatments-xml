<normalizedToken id="DDE87B34DFB44CBDA0004271AA0C32B7" originalValue="Valparaíso">Valparaiso</normalizedToken>
(
<bibRefCitation id="D2892F806E61D4C5570CB677EA61D746" author="Hupe, LH" journalOrPublisher="Historia Fisica y Politica de Chile. Zoologica" pageId="36" pageNumber="85" pagination="1 - 499" refId="B72" refString="Hupe, LH, 1854. Fauna Chilena. Molluscos. Historia Fisica y Politica de Chile. Zoologica 8: 1 - 499" title="Fauna Chilena. Molluscos." volume="8" year="1854">
<normalizedToken id="923E3441B2552D14F939EDF4135EAC19" originalValue="Hupé">Hupe</normalizedToken>
1854
</bibRefCitation>
, 
<bibRefCitation id="B472722755C4D6983174A4202189E7EE" author="Dall, WH" journalOrPublisher="Proceedings of the Academy of Natural Sciences of Philadelphia" pageId="34" pageNumber="83" pagination="696 - 716" refId="B41" refString="Dall, WH, 1903. Synopsis of the Carditacea and of the American species. Proceedings of the Academy of Natural Sciences of Philadelphia 54: 696 - 716" title="Synopsis of the Carditacea and of the American species." volume="54" year="1903">Dall 1903</bibRefCitation>
, 
<bibRefCitation id="2497B1ED987A4421553316B975B501B8" author="Ramorino, L" journalOrPublisher="Revista de Biologia Marina y Oceanografia" pageId="39" pageNumber="88" pagination="15 - 34" refId="B134" refString="Ramorino, L, 1968. Pelecypoda del fondo de la Bahia de Valparaiso. Revista de Biologia Marina y Oceanografia 13: 15 - 34" title="Pelecypoda del fondo de la Bahia de Valparaiso." volume="13" year="1968">Ramorino 1968</bibRefCitation>
