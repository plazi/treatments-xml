(
<emphasis id="DFB1EAE86A21FFDE0892FC87FDBDB55B" bold="true" box="[161,599,814,841]" pageId="2" pageNumber="289">
<taxonomicName id="2AC54D796A21FFDE0892FC87FECAB55B" box="[161,288,815,841]" class="Adenophorea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nematoda" pageId="2" pageNumber="289" phylum="Nematoda" rank="order">Nematoda</taxonomicName>
: 
<taxonomicName id="2AC54D796A21FFDE091FFC86FE5AB55B" authorityName="Cobbold" authorityYear="1864" box="[300,432,814,841]" class="Secernentea" family="Oxyuridae" kingdom="Animalia" order="Ascaridida" pageId="2" pageNumber="289" phylum="Nematoda" rank="family">Oxyuridae</taxonomicName>
