<taxonomicName id="4C554D39FFD7670FFD953201D18FF219" box="[574,702,768,791]" class="Insecta" family="Anthicidae" genus="Lemodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="punctata">
