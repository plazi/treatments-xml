), 
<emphasis id="C5F2EAF1FF85FFCDFCE43CE4FCBAFF4E" box="[851,877,224,248]" italics="true" pageId="7" pageNumber="561">
c 
<number id="414E24C4FF85FFCDFCD73CE4FCBAFF4E" box="[864,877,224,248]" pageId="7" pageNumber="561" value="1.0">1</number>
</emphasis>
<number id="414E24C4FF85FFCDFCC23CDBFC48FF41" box="[885,927,223,247]" pageId="7" pageNumber="561" value="260.0">260</number>
(
<number id="414E24C4FF85FFCDFC183CDBFBC0FF41" box="[943,1047,223,247]" pageId="7" pageNumber="561" value="255.0" valueMax="265.0" valueMin="245.0">245–265</number>
), 
