<taxonomicName id="43E3BEB7D026FFC020A1837FB7914755" box="[511,746,568,592]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="266" phylum="Arthropoda" rank="species" species="ternatensis">
