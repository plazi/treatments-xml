</taxonomicName>
; 2, 
<taxonomicName id="4C6E8CE6387DFF82FB3FFB582F6DFB64" baseAuthorityName="Hancock" baseAuthorityYear="1912" box="[1247,1371,1206,1228]" class="Insecta" family="Tetrigidae" genus="Bolivaritettix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="500" phylum="Arthropoda" rank="species" species="chinensis">
