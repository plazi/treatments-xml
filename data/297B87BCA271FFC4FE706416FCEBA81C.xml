, elytra 
<quantity id="662A9B4FA271FFC5FDFB64C8FD71A8E4" box="[578,662,468,492]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="12" pageNumber="423" unit="mm" value="1.1">1.1 mm</quantity>
, antenna 
<quantity id="662A9B4FA271FFC5FD4264C8FCB7A8E4" box="[763,848,468,492]" metricMagnitude="-4" metricUnit="m" metricValue="4.0" pageId="12" pageNumber="423" unit="mm" value="0.4">0.4 mm</quantity>
