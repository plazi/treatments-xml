</emphasis>
<typeStatus id="EF088812FF9C3025FE6FFC47FE0BF3BF" box="[394,506,925,951]" pageId="8" pageNumber="53">Holotype</typeStatus>
male, 
<collectingCountry id="48A47620FF9C3025FDABFC47FD43F3BF" box="[590,690,925,951]" name="China" pageId="8" pageNumber="53">CHINA</collectingCountry>
: Yunnan, Wuding, 
<collectingCountry id="48A47620FF9C3025FC7FFC47FBE1F3BF" box="[922,1040,925,951]" name="Sierra Leone" pageId="8" pageNumber="53">Shizishan</collectingCountry>
(
<geoCoordinate id="55875077FF9C3025FBC4FC47FB62F3BF" box="[1057,1171,925,951]" direction="north" orientation="latitude" pageId="8" pageNumber="53" precision="925" value="25.533333">
N 
<number id="867B2497FF9C3025FBA4FC47FB93F3BF" box="[1089,1122,925,951]" pageId="8" pageNumber="53" value="25.0">25</number>
° 
<number id="867B2497FF9C3025FB89FC47FB7FF3BF" box="[1132,1166,925,951]" pageId="8" pageNumber="53" value="32.0">32</number>
’
</geoCoordinate>
