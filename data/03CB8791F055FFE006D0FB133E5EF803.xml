<collectingCountry id="F3757617F055FFE006D0F9A63DAFF9C7" box="[151,254,1586,1610]" name="Australia" pageId="4" pageNumber="58">Australia</collectingCountry>
and 
<collectingCountry id="F3757617F055FFE00772F9A63CCAF9C7" box="[309,411,1586,1610]" name="Thailand" pageId="4" pageNumber="58">Thailand</collectingCountry>
