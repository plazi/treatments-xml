<number id="4F784C5AEA7B1540FB8CFE6B87E58D8B" box="[1133,1194,427,449]" pageId="21" pageNumber="22" value="25.5" valueMax="27.0" valueMin="24.0">24–27</number>
% 
<collectionCode id="9FA1C6B8EA7B1540FB22FE6B87AF8D8B" box="[1219,1248,427,449]" pageId="21" pageNumber="22">SL</collectionCode>
)............. 
