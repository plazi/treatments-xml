<number id="4F784C5AEA7B1540FA91FE9386C68D23" box="[1392,1417,339,361]" pageId="21" pageNumber="22" value="30.0">30</number>
% 
<collectionCode id="9FA1C6B8EA7B1540FF00FEB083B18DCC" box="[225,254,368,390]" pageId="21" pageNumber="22">SL</collectionCode>
