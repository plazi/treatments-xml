<collectionCode id="CFD0AE16FFB51E07F0B4FB6BFF03FAC2" LSID="urn:lsid:biocol.org:col:34871" box="[151,236,1273,1299]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="11" pageNumber="64">USNM</collectionCode>
<number id="1F0924F4FFB51E07F0D0FB6BFED0FAC2" box="[243,319,1273,1299]" pageId="11" pageNumber="64" value="20157.0">20157</number>
