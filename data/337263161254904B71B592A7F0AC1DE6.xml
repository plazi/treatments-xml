<taxonomicName id="7CDBA9831254904B71B59309F3751865" authority="Schmitz, 1927b: 71" authorityName="Schmitz" authorityPageNumber="71" authorityYear="1927" box="[151,593,313,335]" class="Insecta" family="Phoridae" genus="Diploneura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="40" phylum="Arthropoda" rank="species" species="pachycera">
