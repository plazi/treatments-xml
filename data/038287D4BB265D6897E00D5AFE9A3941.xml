<taxonomicName id="4C2B4D41BB295D6895630AFFFD4F3A8D" authorityName="Lopes" authorityYear="1946" box="[532,658,403,429]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="fraterna">
