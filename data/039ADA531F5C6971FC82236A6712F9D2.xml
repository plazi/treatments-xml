(
<location id="8EEC3D9E1F536971FCA02308677AFCA2" LSID="urn:lsid:plazi:treatment:039ADA531F5C6971FC82236A6712F9D2:8EEC3D9E1F536971FCA02308677AFCA2" box="[834,1025,804,826]" country="France" name="Corveissiat cave" pageId="8" pageNumber="538">Corveissiat cave</location>
, 
