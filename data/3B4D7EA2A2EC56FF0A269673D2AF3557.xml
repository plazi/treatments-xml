<taxonomicName id="89739A79C850E02ABFFD675FE75EE74A" class="Polychaeta" family="Onuphidae" genus="Onuphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Onuphis lepta" order="Eunicida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="lepta">Onuphis lepta</taxonomicName>
