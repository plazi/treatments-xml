<taxonomicName id="4C0FCAF28A37FFC5FEF2CE639081FE92" box="[349,677,332,357]" class="Insecta" family="Pentatomidae" genus="Euschistus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="567" phylum="Arthropoda" rank="species" species="baranowskii" subGenus="Euschistus">
