from eastern 
<collectingCountry id="F3547613FFA7714764F5AAF1FCE5FF4F" box="[691,791,167,193]" name="Ecuador" pageId="10" pageNumber="69">Ecuador</collectingCountry>
</emphasis>
</heading>
</paragraph>
