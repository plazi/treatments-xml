<taxonomicName id="4C3D3391FFE2FFF6FC21FEBEFBFFFEFA" authorityName="Guidetti &amp; Rebecchi &amp; Bertolani &amp; Jonsson &amp; Kristensen &amp; Cesari" authorityYear="2016" box="[947,1101,351,372]" class="Eutardigrada" family="Richtersiidae" genus="Diaforobiotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Parachela" pageId="9" pageNumber="843" phylum="Tardigrada" rank="genus">Diaforobiotus</taxonomicName>
