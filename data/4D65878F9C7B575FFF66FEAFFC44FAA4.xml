(
<specimenCode id="956A9EE29C7B575FFDE7FB63FD3DFB50" box="[534,665,1159,1184]" collectionCode="LAM" country="USA" lsid="urn:lsid:biocol.org:col:15651" name="Natural History Museum of Los Angeles County" pageId="2" pageNumber="186" type="Herbarium">LAM 2791</specimenCode>
