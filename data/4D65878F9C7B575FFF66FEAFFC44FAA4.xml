<collectingRegion id="0708F87B9C7B575FFCEFFB80FBD9FB8D" box="[798,1149,1124,1149]" country="Australia" name="Australian Capital Territory" pageId="2" pageNumber="186">Australian Capital Territory</collectingRegion>
</emphasis>
, 
<taxonomicName id="02CC4D1A9C7B575FFB63FB80FAF0FB8D" authority="Sands" authorityName="Sands" box="[1170,1364,1124,1149]" class="Magnoliopsida" family="Casuarinaceae" genus="Casuarina" kingdom="Plantae" order="Fagales" pageId="2" pageNumber="186" phylum="Tracheophyta" rank="genus">Casuarina Sands</taxonomicName>
, from 
<location id="C01360429C7B575FFF66FB63FF4AFB50" LSID="urn:lsid:plazi:treatment:4D65878F9C7B575FFF66FEAFFC44FAA4:C01360429C7B575FFF66FB63FF4AFB50" box="[151,238,1159,1184]" name="Epacrid" pageId="2" pageNumber="186" stateProvince="Australian Capital Territory">Epacrid</location>
with lichen, 
<date id="B17210599C7B575FFE71FB63FDACFB50" box="[384,520,1159,1184]" pageId="2" pageNumber="186" value="1995-08-13">
