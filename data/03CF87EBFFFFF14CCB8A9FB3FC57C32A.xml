<paragraph id="8BD936FDFFFFF14DCB8A9FF9FCDBC4E5" blockId="30.[151,884,1501,1523]" box="[151,884,1501,1523]" pageId="30" pageNumber="431">
<treatmentCitationGroup id="AB7611D3FFFFF14DCB8A9FF9FCDBC4E5" box="[151,884,1501,1523]" pageId="30" pageNumber="431">
<taxonomicName id="4C664D7EFFFFF14DCB8A9FF9FED6C4E5" baseAuthorityName="Weber" baseAuthorityYear="1892" box="[151,377,1501,1523]" class="Malacostraca" family="Talitridae" genus="Talorchestia" kingdom="Animalia" order="Amphipoda" pageId="30" pageNumber="431" phylum="Arthropoda" rank="species" species="martensii">
