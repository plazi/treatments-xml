<date id="4E059088E60FFFD0FF5C23AEAEF5CECA" box="[151,243,1445,1469]" pageId="8" pageNumber="323" value="1993-04">IV.1993</date>
</collectingDate>
. 
<collectorName id="974ED39EE60FFFD0FF3523ADAF81CECA" box="[254,391,1446,1469]" pageId="8" pageNumber="323">J.E. Wappes</collectorName>
col. (
<collectionCode id="5CAA2E8DE60FFFD0FE0E23AEAC1ECECA" box="[453,536,1445,1469]" pageId="8" pageNumber="323">ACMT</collectionCode>
for deposit in 
<collectionCode id="5CAA2E8DE60FFFD0FD7023AEAD00CECA" box="[699,774,1445,1469]" country="USA" httpUri="http://grbio.org/cool/jekt-x82m" name="Florida State Collection of Arthropods, The Museum of Entomology" pageId="8" pageNumber="323">FSCA</collectionCode>
