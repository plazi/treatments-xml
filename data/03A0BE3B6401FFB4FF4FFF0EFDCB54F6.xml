<paragraph id="8BB60F2D6401FFB4FF4FFE20FDC056A1" blockId="11.[151,1436,222,459]" box="[151,513,437,459]" pageId="11" pageNumber="12">
<treatmentCitationGroup id="AB1928036401FFB4FF4FFE20FDC056A1" box="[151,513,437,459]" pageId="11" pageNumber="12">
<treatmentCitation id="0AA8293C6401FFB4FF4FFE20FE3C56A1" author="Smith &amp; Jo" box="[151,509,437,459]" page="39" pageId="11" pageNumber="12" year="2018">
