<taxonomicName id="4C48BFEE5A7EFF99FC06F894A336F897" authority="Jurine (in Panzer)" box="[943,1345,1874,1898]" class="Insecta" family="Stephanidae" genus="Stephanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="coronatus">
