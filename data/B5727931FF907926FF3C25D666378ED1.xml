<taxonomicName id="FADBB3A4FF907926FA862717631F8ED1" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="33" pageNumber="34" phylum="Annelida" rank="species" species="omeimontis">
<emphasis id="0FAF1435FF907926FA862717631F8ED1" italics="true" pageId="33" pageNumber="34">Amynthas omeimontis</emphasis>
</taxonomicName>
