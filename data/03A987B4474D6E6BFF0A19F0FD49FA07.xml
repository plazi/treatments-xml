<taxonomicName id="4C004D21474D6E6BFDE91E38FBEEFAEB" authority="Kieffer, 1925" authorityName="Kieffer" authorityYear="1925" box="[628,1102,1322,1347]" class="Insecta" family="Ceratopogonidae" genus="Thysanognathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="557" phylum="Arthropoda" rank="species" species="nilogenes">
