<taxonomicName id="4C004D21474D6E6BFA191E39FDC4FACF" authority="(Ingram &amp; Macfie 1922)" authorityName="Ingram &amp; Macfie" authorityYear="1922" baseAuthorityName="Ingram &amp; Macfie" baseAuthorityYear="1922" class="Insecta" family="Ceratopogonidae" genus="Alluaudomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="557" phylum="Arthropoda" rank="species" species="melanosticta">
