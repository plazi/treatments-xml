<heading id="6F7CC5581770A95F989DFF5C7B4CFF4D" bold="true" box="[151,783,151,177]" fontSize="11" level="1" pageId="65" pageNumber="66" reason="1">
<emphasis id="06FFAE261770A95F989DFF5C7B4CFF4D" bold="true" box="[151,783,151,177]" pageId="65" pageNumber="66">
<taxonomicName id="F38B09B71770A95F989DFF5C7A31FF4D" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,626,151,177]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="soinha" status="new species">
<emphasis id="06FFAE261770A95F989DFF5C79CEFF4D" bold="true" box="[151,397,151,177]" italics="true" pageId="65" pageNumber="66">Hydropsyche soinha</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D1770A95F9A75FF5C7B4CFF4D" box="[639,783,151,177]" pageId="65" pageNumber="66" rank="species">new species</taxonomicNameLabel>
