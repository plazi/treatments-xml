<paragraph id="8BB21D11FF95FFECFF61EDC8FB508134" blockId="5.[151,1437,1931,2037]" lastBlockId="6.[151,1436,152,778]" lastPageId="6" lastPageNumber="63" pageId="5" pageNumber="62">
<emphasis id="B979C103FF95FFEFFF61EDC8FEBA8793" bold="true" box="[199,263,1931,1957]" pageId="5" pageNumber="62">Male</emphasis>
(
<number id="3DC50F36FF95FFEFFEBCEDC8FE978793" box="[282,298,1931,1957]" pageId="5" pageNumber="62" value="3.0">3</number>
). Mean length of head seta 
