<taxonomicName id="4C294D45FFE4FFA7EDC6665DFE3F7FC6" box="[413,509,1386,1412]" class="Arachnida" family="Eriophyidae" genus="Tetra" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="lucidi">
<emphasis id="B95DEAD4FFE4FFA7EDC6665DFE697FC6" box="[413,427,1387,1412]" italics="true" pageId="3" pageNumber="4">T</emphasis>
