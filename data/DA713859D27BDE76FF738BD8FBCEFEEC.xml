<taxonomicName id="95D8F2CCD27BDE77FDBB8A04FCF5FEB1" authority="Sjostedt" authorityName="Sjostedt" authorityYear="1909" box="[607,849,330,355]" class="Insecta" family="Phalangopsidae" genus="Opilionacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
