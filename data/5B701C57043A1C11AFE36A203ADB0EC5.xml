<taxonomicName id="175B24086CF2B7B2BB20DE8F7D56DC15" authorityName="LeConte" authorityYear="1846" class="Insecta" family="Carabidae" genus="Rhadine" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="738" pageNumber="739" phylum="Arthropoda" rank="species" species="larvalis">Rhadine larvalis</taxonomicName>
