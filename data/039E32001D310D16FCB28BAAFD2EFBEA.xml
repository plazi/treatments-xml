<emphasis id="B9435F041D330D16FFB08D37FF46FC8A" bold="true" box="[113,174,797,821]" pageId="6" pageNumber="494">Host.</emphasis>
<taxonomicName id="4C37F8951D330D16FF778D37FE98FC8A" box="[182,368,797,821]" class="Liliopsida" family="Poaceae" genus="Gigantochloa" kingdom="Plantae" order="Poales" pageId="6" pageNumber="494" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B9435F041D330D16FF778D37FEA1FC8A" box="[182,329,797,821]" italics="true" pageId="6" pageNumber="494">Gigantochloa</emphasis>
sp.
</taxonomicName>
(
<taxonomicName id="4C37F8951D330D16FE418D37FE35FC8A" authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[384,477,797,821]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="6" pageNumber="494" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
: 
<taxonomicName id="4C37F8951D330D16FE288D37FD60FC8A" authorityName="Luerssen" authorityYear="1893" box="[489,648,797,821]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="6" pageNumber="494" phylum="Tracheophyta" rank="subFamily" subFamily="Bambusoideae">Bambusoideae</taxonomicName>
) (
<figureCitation id="130C9F931D330D16FD5E8D37FD0BFC8A" box="[671,739,797,821]" captionStart="Fig" captionStartId="4.[813,848,564,586]" captionTargetBox="[813,1451,189,548]" captionTargetId="figure-727@4.[813,1451,189,548]" captionTargetPageId="4" captionText="Fig. 4. Host plant, Gigantochloa sp." figureDoi="http://doi.org/10.5281/zenodo.5344650" httpUri="https://zenodo.org/record/5344650/files/figure.png" pageId="6" pageNumber="494">Fig. 4</figureCitation>
