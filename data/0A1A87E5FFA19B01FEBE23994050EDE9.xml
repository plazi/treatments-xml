<collectingCountry id="FAA47663FFAE9B01FD4326724141ED09" box="[759,830,1629,1653]" name="China" pageId="24" pageNumber="679">China</collectingCountry>
Sea off 
<collectingRegion id="4077F811FFAE9B01FC1526724652ED09" box="[929,1069,1629,1653]" country="China" name="Guangdong" pageId="24" pageNumber="679">Guangdong</collectingRegion>
, southern 
<collectingCountry id="FAA47663FFAE9B01FF3E265242AEEDE9" box="[138,209,1661,1685]" name="China" pageId="24" pageNumber="679">China</collectingCountry>
(
<bibRefCitation id="E6224B02FFAE9B01FF56265243AAEDE9" author="Liu R &amp; Wang S" box="[226,469,1661,1685]" pageId="24" pageNumber="679" pagination="522 - 530" refId="ref25072" refString="Liu R, Wang S. 1983. On three new species of Mysidacea (Crustacea) from the coastal waters of Guangdong, China. Oceanologia et Limnologia Sinica 14: 522 - 530. (Chi with Eng abstract and description)." type="journal article" year="1983">Liu and Wang 1983</bibRefCitation>
, 
