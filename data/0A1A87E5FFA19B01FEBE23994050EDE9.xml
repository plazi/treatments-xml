<paragraph id="820C36F3FFAE9B01FF11257542D0EE8F" blockId="24.[138,1185,1207,1523]" pageId="24" pageNumber="679">
<taxonomicName id="45B34D70FFAE9B01FF11257543BFEE0E" authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Liu and Wang" baseAuthorityYear="1983" box="[165,448,1370,1394]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="24" pageNumber="679" phylum="Arthropoda" rank="species" species="meridionalis">
