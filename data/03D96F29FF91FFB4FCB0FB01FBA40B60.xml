<taxonomicName id="4C70A5BCFF91FFB4FC58FB01FBF70ACE" authorityName="A. Milne-Edwards" authorityYear="1875" box="[946,1060,1213,1236]" class="Malacostraca" family="Epialtidae" genus="Rochinia" kingdom="Animalia" order="Decapoda" pageId="52" pageNumber="497" phylum="Arthropoda" rank="genus">
