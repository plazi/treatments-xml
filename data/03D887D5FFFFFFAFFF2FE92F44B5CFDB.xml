<taxonomicName id="4C714D40FFFCFFAFFB30EE2A4026CFFF" box="[1160,1305,872,896]" class="Insecta" family="Issidae" genus="Gergithoides" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="44" phylum="Arthropoda" rank="genus">
