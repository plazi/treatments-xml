<taxonomicName id="4C7B4D12FFB4B344FEED4DB5FE6AFB6F" authorityName="C.A.Viraktamath" authorityYear="2011" box="[321,443,1164,1188]" class="Insecta" family="Cicadellidae" genus="Japanagallia" kingdom="Animalia" order="Hemiptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="malaisei">
