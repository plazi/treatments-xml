<treatmentCitation id="777810B9FFF5BB7BFF32FBCA6CD7FB62" author="New TR &amp; Thornton IWB" page="73" pageId="64" pageNumber="715" year="1975">
<taxonomicName id="31D94D2BFFF5BB7BFF32FBCA6CD7FB62" authority="New &amp; Thornton, 1975: 73" authorityName="New &amp; Thornton" authorityYear="1975" class="Insecta" family="Psocidae" genus="Trichadenotecnum" kingdom="Animalia" order="Psocodea" pageId="64" pageNumber="715" phylum="Arthropoda" rank="species" species="pichincha">
<emphasis id="C4ADEABAFFF5BB7BFF32FBCA6DA2FB83" box="[144,475,1118,1139]" italics="true" pageId="64" pageNumber="715">Trichadenotecnum pichincha</emphasis>
<bibRefCitation id="92484B59FFF5BB7BFE46FBCB6CD7FB62" author="New TR &amp; Thornton IWB" pageId="64" pageNumber="715" pagination="27 - 80" refId="ref37624" refString="New TR, Thornton IWB. 1975. Psocomorpha (Psocoptera) collected on recent expeditions to South America. Journal of Entomology B 44: 27 - 80." type="journal article" year="1975">New &amp; Thornton, 1975: 73</bibRefCitation>
</taxonomicName>
