<taxonomicName id="97DDE7DA921F0A1255C635F433B2CB97" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181884" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole hedlundorum Wilson" lsidName-HNS="Pheidole hedlundorum Wilson" order="Hymenoptera" pageNumber="367" phylum="Arthropoda" rank="species" species="hedlundorum">hedlundorum</taxonomicName>
