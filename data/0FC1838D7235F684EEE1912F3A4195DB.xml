<taxonomicName id="B5D842C82CFEDBD68C5DA7554C32AE65" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181918" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole palenquensis Wilson" lsidName-HNS="Pheidole palenquensis Wilson" order="Hymenoptera" pageNumber="367" phylum="Arthropoda" rank="species" species="palenquensis">palenquensis</taxonomicName>
