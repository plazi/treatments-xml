<taxonomicName id="952D24FCE31DC058C8B73C59C1466BE5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181895" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole lustrata Wilson" lsidName-HNS="Pheidole lustrata Wilson" order="Hymenoptera" pageNumber="367" phylum="Arthropoda" rank="species" species="lustrata">lustrata</taxonomicName>
, 
<taxonomicName id="F5A00D2FDBC3066519D1937B69EA92B8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181907" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole monteverdensis Wilson" lsidName-HNS="Pheidole monteverdensis Wilson" order="Hymenoptera" pageNumber="367" phylum="Arthropoda" rank="species" species="monteverdensis">monteverdensis</taxonomicName>
