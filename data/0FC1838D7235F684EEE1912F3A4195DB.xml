<taxonomicName id="CB2ED3D750F289EF108ABB56FC1BADF3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33744" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole flavens Roger" lsidName-HNS="Pheidole flavens Roger" order="Hymenoptera" pageNumber="367" phylum="Arthropoda" rank="species" species="flavens">flavens</taxonomicName>
