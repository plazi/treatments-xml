<taxonomicName id="4C084D15C670FFBB0CB6DAB2FD1AFC74" authorityName="KITS &amp; MARSHALL" authorityYear="2013" box="[498,666,942,966]" class="Insecta" family="Sphaeroceridae" genus="Antrops" kingdom="Animalia" order="Diptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="coniobaptos">
