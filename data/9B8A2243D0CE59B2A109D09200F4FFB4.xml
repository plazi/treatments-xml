<emphasis id="78061356C59BFB8380C8CE56F254FE02" bold="true" pageId="0" pageNumber="1">Figure 16.</emphasis>
<taxonomicName id="71DEB12791FA8A00DB476418926BFB08" authorityName="Kaltenbach &amp; Garces &amp; Gattolliat" authorityYear="2020" class="Insecta" family="Baetidae" genus="Labiobaetis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Labiobaetis sabordoi" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="sabordoi">
