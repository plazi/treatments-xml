<collectingCountry id="12A64584FFFA517DFA8CFBC64CEECD8A" name="Nicaragua" pageId="9" pageNumber="10">Nicaragua</collectingCountry>
, 
<collectingCountry id="12A64584FFFA517DFF17FBFE4D55CD8A" box="[243,339,1136,1162]" name="Panama" pageId="9" pageNumber="10">Panama</collectingCountry>
