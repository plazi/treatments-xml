<emphasis box="[290,405,577,602]" italics="true" pageId="6" pageNumber="7">
<typeStatus box="[290,402,577,602]" pageId="6" pageNumber="7">Holotype</typeStatus>
.
</emphasis>
Male, 
<collectingCountry box="[495,607,577,603]" name="Brunei" pageId="6" pageNumber="7">BRUNEI</collectingCountry>
, 
<taxonomicName box="[623,667,577,603]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="23" phylum="Arthropoda" rank="species" species="ulu">Ulu</taxonomicName>
<collectingRegion box="[678,816,577,603]" country="Brunei" name="Temburong" pageId="6" pageNumber="7">Temburong</collectingRegion>
, 
<date box="[832,1021,577,603]" pageId="6" pageNumber="7" value="1982-02-14" valueMax="1982-03-09" valueMin="1982-02-14">14.ii.–9.iii.1982</date>
, Malaise trap, M. C. Day leg. (in 
<collectionCode box="[151,225,616,642]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14908" name="University of Nottingham" pageId="6" pageNumber="7">NHM</collectionCode>
