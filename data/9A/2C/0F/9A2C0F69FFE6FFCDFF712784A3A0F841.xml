<typeStatus box="[286,397,1927,1952]" pageId="14" pageNumber="15">Holotype</typeStatus>
.
</emphasis>
Male, 
<collectingCountry box="[485,648,1926,1952]" name="Indonesia" pageId="14" pageNumber="15">INDONESIA</collectingCountry>
, 
<collectingRegion box="[661,762,1926,1952]" country="Indonesia" name="Sumatera Utara" pageId="14" pageNumber="15">Sumatra</collectingRegion>
, 
<collectingRegion box="[774,961,1926,1952]" country="Indonesia" name="Sumatera Utara" pageId="14" pageNumber="15">Sumatera Utara</collectingRegion>
, Semangat Gunung, 
<quantity box="[1207,1299,1926,1952]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="14" pageNumber="15" unit="m" value="1300.0">1300 m</quantity>
, Malaise in jungle, 
<date box="[239,369,1965,1991]" pageId="14" pageNumber="15" value="1992-03-16">16.iii.1992</date>
, H. Hippa (in 
<collectionCode box="[535,625,1965,1991]" country="Canada" httpUri="http://grbio.org/cool/8z6v-f8is" name="Saskatchewan Museum of Natural History" pageId="14" pageNumber="15">SMNH</collectionCode>
