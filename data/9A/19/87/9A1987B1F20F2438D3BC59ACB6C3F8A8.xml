<specimenCount box="[635,658,1302,1325]" pageId="2" pageNumber="261" type="male">♂</specimenCount>
, 
<quantity box="[669,758,1301,1325]" metricMagnitude="-3" metricUnit="m" metricValue="8.2" pageId="2" pageNumber="261" unit="mm" value="8.2">8.2 mm</quantity>
; 
<specimenCount box="[769,790,1302,1325]" pageId="2" pageNumber="261" type="female">♀</specimenCount>
