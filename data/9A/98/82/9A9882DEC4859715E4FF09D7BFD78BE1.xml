<subSubSection pageId="null" pageNumber="11" type="nomenclature">
<paragraph pageId="null" pageNumber="11">
<taxonomicName class="Magnoliopsida" family="Oleaceae" genus="Jasminum" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="11" phylum="Tracheophyta" rank="genus">Jasminum</taxonomicName>
<normalizedToken originalValue="frúticans" pageId="null" pageNumber="11">fruticans</normalizedToken>
