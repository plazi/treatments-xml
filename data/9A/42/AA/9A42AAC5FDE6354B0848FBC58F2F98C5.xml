[[ 
<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:57E11E2F-4328-4234-9DC1-1808B3E8A72C" class="Actinopterygii" family="Characidae" genus="Moenkhausia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Moenkhausia Eigenmann 1903:145" order="Characiformes" pageNumber="61" phylum="Chordata" rank="genus">Moenkhausia Eigenmann</taxonomicName>
