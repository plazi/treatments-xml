<taxonomicName authority="Newman, 1838" authorityName="Newman" authorityYear="1838" class="Insecta" family="Cephidae" genus="Phylloecus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Phylloecus faunus" order="Hymenoptera" pageId="0" pageNumber="65" phylum="Arthropoda" rank="species" species="faunus">Phylloecus faunus Newman, 1838</taxonomicName>
