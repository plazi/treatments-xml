<caption box="[151,874,1699,1720]" httpUri="https://zenodo.org/record/1049723/files/figure.png" pageId="53" pageNumber="54" targetBox="[151,1436,771,1677]" targetPageId="53">
<paragraph blockId="53.[151,874,1699,1720]" box="[151,874,1699,1720]" pageId="53" pageNumber="54">
<emphasis bold="true" box="[151,269,1699,1720]" pageId="53" pageNumber="54">FIGURE 63</emphasis>
. 
<taxonomicName authority="Gibbs" authorityName="Gibbs" box="[280,556,1699,1720]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="arantium">
