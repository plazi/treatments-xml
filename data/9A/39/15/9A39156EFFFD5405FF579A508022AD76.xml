<number box="[903,960,502,526]" pageId="2" pageNumber="27" value="8524.0">8524</number>
, 
<collectionCode box="[976,1054,502,526]" pageId="2" pageNumber="27">CRBA</collectionCode>
<number box="[1054,1110,502,526]" pageId="2" pageNumber="27" value="8525.0">8525</number>
