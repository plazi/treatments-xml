<number box="[883,925,574,598]" pageId="2" pageNumber="27" value="106.0">106</number>
spec. from LOC179, 70 spec. from LOC 
<number box="[1390,1432,574,598]" pageId="2" pageNumber="27" value="180.0">180</number>
