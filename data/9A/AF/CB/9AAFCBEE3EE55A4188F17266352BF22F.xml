<taxonomicName authorityName="Haldeman" authorityYear="1850" class="Insecta" family="Platygastridae" genus="Amitus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Hemiptera" pageId="0" pageNumber="115" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
, 
<taxonomicName authorityName="Haldeman" authorityYear="1850" class="Insecta" family="Platygastridae" genus="Amitus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="0" pageNumber="115" phylum="Arthropoda" rank="subOrder" subOrder="Sternorrhyncha">Sternorrhyncha</taxonomicName>
, 
<taxonomicName authorityName="Westwood" authorityYear="1840" class="Insecta" family="Aleyrodidae" kingdom="Animalia" lsidName="" order="Hemiptera" pageId="0" pageNumber="115" phylum="Arthropoda" rank="family">Aleyrodidae</taxonomicName>
) (
