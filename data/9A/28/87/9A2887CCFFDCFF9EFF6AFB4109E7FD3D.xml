<subSubSection box="[199,1179,655,680]" pageId="4" type="etymology">
<paragraph blockId="4.[151,1436,151,680]" box="[199,1179,655,680]" pageId="4">
<emphasis bold="true" box="[199,326,655,680]" pageId="4">Etymology</emphasis>
