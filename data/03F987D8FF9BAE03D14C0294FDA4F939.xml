<taxonomicName id="4C504D4DFF9CAE02D2A602DAFEBFFB0E" authority="SALAzAR-Vallejo, 2012" authorityName="SALAzAR-Vallejo" authorityYear="2012" box="[202,360,1155,1172]" class="Polychaeta" family="Flabelligeridae" genus="Trophoniella" kingdom="Animalia" order="Terebellida" pageId="28" phylum="Annelida" rank="species" species="fauveli" status="n. sp.">
