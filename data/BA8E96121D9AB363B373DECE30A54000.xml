<paragraph id="46BD1CD2538AD6B0D40567A3175FC21E" pageId="29" pageNumber="30">
<taxonomicName id="61651ECFFCAA33F5B774623A4F07497B" authority="Berlese, 1913" authorityName="Berlese" authorityYear="1913" class="Arachnida" family="Protoplophoridae" genus="Arthroplophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" phylum="Arthropoda" rank="species" species="paradoxa">Arthroplophora paradoxa</taxonomicName>
