See 
<taxonomicName id="4C504D0E3271FFB2FCD1F97CFC127B83" box="[876,991,1745,1766]" class="Reptilia" family="Iguanodontia incertae sedis" genus="Siamodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ornithischia" pageId="60" pageNumber="152" phylum="Chordata" rank="genus">
