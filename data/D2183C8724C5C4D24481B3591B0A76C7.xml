<taxonomicName id="E5E59A115429D3AC273526DA24827D9E" family="Annonaceae" genus="X." kingdom="Plantae" lsidName="X. globosa" order="Magnoliales" pageId="38" pageNumber="39" rank="species" species="globosa">
