<collectingCountry id="F31223F08FA6FF1BFF74FADD41193A12" box="[218,271,1381,1404]" name="Peru" pageId="242" pageNumber="243">Peru</collectingCountry>
and northwestern 
<collectingCountry id="F31223F08FA6FF1BFE53FADD42523A12" box="[509,580,1381,1404]" name="Brazil" pageId="242" pageNumber="243">Brazil</collectingCountry>
