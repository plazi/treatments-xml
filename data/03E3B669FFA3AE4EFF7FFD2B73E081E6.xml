<taxonomicName id="4C4A7CFCFFA3AE4EFF7FFD2B7153870B" authorityName="Ahmad and Sturhan" authorityYear="2002" box="[144,292,766,790]" class="Adenophorea" family="Nordiidae" genus="Oriverutoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="13" pageNumber="1266" phylum="Nematoda" rank="genus">
<emphasis id="B93EDB6DFFA3AE4EFF7FFD2B7153870B" box="[144,292,766,790]" italics="true" pageId="13" pageNumber="1266">Oriverutoides</emphasis>
