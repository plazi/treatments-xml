<taxonomicName id="4C6F1183FFEDFFACFCC2FE8CFBB4FEE6" box="[795,1080,288,314]" class="Magnoliopsida" family="Boraginaceae" genus="Lithospermum" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="25" phylum="Tracheophyta" rank="species" species="officinale">
<emphasis id="B91BB612FFEDFFACFCC2FE8CFBB4FEE6" box="[795,1080,288,314]" italics="true" pageId="6" pageNumber="25">Lithospermum officinale</emphasis>
</taxonomicName>
und 
<taxonomicName id="4C6F1183FFEDFFACFF4EFEEEFE12FE80" box="[151,414,322,348]" class="Magnoliopsida" family="Boraginaceae" genus="Pulmonaria" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="25" phylum="Tracheophyta" rank="species" species="officinalis">
<emphasis id="B91BB612FFEDFFACFF4EFEEEFE12FE80" box="[151,414,322,348]" italics="true" pageId="6" pageNumber="25">Pulmonaria officinalis</emphasis>
</taxonomicName>
