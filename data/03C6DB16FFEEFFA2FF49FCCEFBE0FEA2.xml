<collectingCountry id="F3782A90FFEEFFAFFF49FB83FEABFB95" box="[144,295,1071,1097]" name="Germany" pageId="5" pageNumber="24">Deutschland</collectingCountry>
, Nordspanien, 
<collectingCountry id="F3782A90FFEEFFAFFE00FB83FDD1FB95" box="[473,605,1071,1097]" name="France" pageId="5" pageNumber="24">Frankreich</collectingCountry>
, 
<collectingCountry id="F3782A90FFEEFFAFFDB0FB83FD43FB95" box="[617,719,1071,1097]" name="Switzerland" pageId="5" pageNumber="24">Schweiz</collectingCountry>
