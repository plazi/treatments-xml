und 
<taxonomicName id="4C6F1183FFEDFFACFE53FEC9FD1DFEA2" box="[394,657,356,383]" class="Magnoliopsida" family="Boraginaceae" genus="Asperugo" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="25" phylum="Tracheophyta" rank="species" species="procumbens">
<emphasis id="B91BB612FFEDFFACFE53FEC9FD1DFEA2" box="[394,657,356,383]" italics="true" pageId="6" pageNumber="25">Asperugo procumbens</emphasis>
