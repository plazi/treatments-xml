<paragraph id="8BD06A00FFEDFFACFF4EFCB2FD3BFC1D" blockId="6.[151,1135,730,1676]" pageId="6" pageNumber="25">
<emphasis id="B91BB612FFEDFFACFF4EFCB2FEC8FCE5" bold="true" box="[151,324,798,825]" italics="true" pageId="6" pageNumber="25">Identifikation:</emphasis>
Flügelspannweite 
<quantity id="4C97C7E5FFEDFFACFDF1FCB2FD3AFCE5" box="[552,694,798,825]" metricMagnitude="-2" metricUnit="m" metricValue="1.725" metricValueMax="1.85" metricValueMin="1.6" pageId="6" pageNumber="25" unit="mm" value="17.25" valueMax="18.5" valueMin="16.0">16–18.5 mm</quantity>
