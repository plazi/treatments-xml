<taxonomicName id="4C6F1183FFEDFFACFF3BFC29FEEFFC42" baseAuthorityName="Haworth" baseAuthorityYear="1828" box="[226,355,900,926]" class="Insecta" family="Ethmiidae" genus="Ethmia" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="25" phylum="Arthropoda" rank="species" species="dodecea">
