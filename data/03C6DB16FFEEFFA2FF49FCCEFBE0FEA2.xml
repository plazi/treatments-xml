<taxonomicName id="4C6F1183FFEDFFACFC4DFEEFFBEBFE80" box="[916,1127,322,348]" class="Magnoliopsida" family="Boraginaceae" genus="Borago" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="25" phylum="Tracheophyta" rank="species" species="officinalis">
<emphasis id="B91BB612FFEDFFACFC4DFEEFFBEBFE80" box="[916,1127,322,348]" italics="true" pageId="6" pageNumber="25">Borago officinalis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6F1183FFEDFFACFF4EFEC8FEC6FEA2" box="[151,330,356,382]" class="Magnoliopsida" family="Boraginaceae" genus="Cerinthe" kingdom="Plantae" order="Boraginales" pageId="6" pageNumber="25" phylum="Tracheophyta" rank="species" species="major">
<emphasis id="B91BB612FFEDFFACFF4EFEC8FEC6FEA2" box="[151,330,356,382]" italics="true" pageId="6" pageNumber="25">Cerinthe major</emphasis>
