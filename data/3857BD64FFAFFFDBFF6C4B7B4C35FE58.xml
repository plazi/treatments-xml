<subSubSection id="F8E45FF9FFAFFFD5FFB34B4A4AC5FA94" box="[106,695,1339,1362]" pageId="7" pageNumber="434" type="type_taxon">
<paragraph id="B0410C72FFAFFFD5FFB34B4A4AC5FA94" blockId="7.[106,695,1339,1362]" box="[106,695,1339,1362]" pageId="7" pageNumber="434">
