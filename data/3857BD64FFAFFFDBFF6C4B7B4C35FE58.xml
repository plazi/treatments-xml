<typeStatus id="6F45B2D0FFAFFFD5FFB64B4A48EBFA94" box="[111,153,1340,1361]" pageId="7" pageNumber="434">type</typeStatus>
species: 
<taxonomicName id="77FE77F1FFAFFFD5FF334B4A4A58FA94" authority="Faust, 1889" authorityName="Faust, Yunakov" authorityYear="1889" box="[234,554,1340,1361]" class="Insecta" family="Curculionidae" genus="Synaptorhinus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="434" phylum="Arthropoda" rank="species" species="simplex">
