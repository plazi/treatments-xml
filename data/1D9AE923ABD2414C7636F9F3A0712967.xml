<taxonomicName id="6066B0CF38FECAEE7BBF8DE7DFE17860" authority="Bonvicino, Lima &amp; Almeida, 2003" authorityName="Bonvicino, Lima &amp; Almeida" authorityYear="2003" class="Mammalia" family="Cricetidae" genus="Calomys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calomys tocantinsi" order="Rodentia" pageId="17" pageNumber="122" phylum="Chordata" rank="species" species="tocantinsi">
<pageBreakToken id="967CC6D52094FECF2D243C524FEB2DC0" pageId="17" pageNumber="122" start="start">Calomys</pageBreakToken>
tocantinsi Bonvicino, Lima &amp; Almeida, 2003
</taxonomicName>
