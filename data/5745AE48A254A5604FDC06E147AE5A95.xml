<taxonomicName id="7A2DDBADBF218AA1EB268AB366A4F28C" class="Insecta" family="Formicidae" genus="Brachycistis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="crassa">Brachycistis crassa</taxonomicName>
