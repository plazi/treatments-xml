<taxonomicName id="F900EBDD72039FC880ECF37B438A7927" class="Dicotyledoneae" family="Malvaceae" genus="Abutilon" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Abutilon" order="Malvales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="genus">
