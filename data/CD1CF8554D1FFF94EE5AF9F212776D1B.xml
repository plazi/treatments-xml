<quantity id="824DE4A64D18FF96ED8AFA2C15B16ACE" box="[728,898,1503,1529]" metricMagnitude="-4" metricUnit="m" metricValue="2.55" metricValueMax="2.6" metricValueMin="2.5" pageId="4" pageNumber="39" unit="mm" value="0.255" valueMax="0.26" valueMin="0.25">0.25–0.26 mm</quantity>
