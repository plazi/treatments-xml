<quantity id="824DE4A64D18FF96EDECF9DC154B697E" box="[702,888,1583,1609]" metricMagnitude="-4" metricUnit="m" metricValue="8.7" metricValueMax="8.8" metricValueMin="8.6" pageId="4" pageNumber="39" unit="mm" value="0.87" valueMax="0.88" valueMin="0.86">0.86–0.88 mm</quantity>
