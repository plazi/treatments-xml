<collectionCode id="65EC86A54C7AFF8DFC95FE996CE7FE2E" box="[836,919,379,402]" pageId="28" pageNumber="29">BMNH</collectionCode>
, 
<collectionCode id="65EC86A54C7AFF8DFC70FE996C9BFE2E" box="[929,1003,379,402]" httpUri="http://grbio.org/cool/s8gp-ktsi" name="Transvaal Museum" pageId="28" pageNumber="29">TMSA</collectionCode>
