<quantity id="C405B3854C7AFF8DFE6CFE316E96FE56" box="[445,486,467,490]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="28" pageNumber="29" unit="in" value="1.0">1 in</quantity>
