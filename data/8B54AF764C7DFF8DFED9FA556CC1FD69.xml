<collectionCode id="65EC86A54C7AFF8DFBDDFE996B28FE2E" box="[1036,1112,379,402]" httpUri="http://grbio.org/cool/94p5-pepz" name="Iziko Museums of Cape Town" pageId="28" pageNumber="29">SAMC</collectionCode>
