<quantity id="4C9B9B1C7008892FFD8AFC534BE46AC8" box="[1154,1214,999,1023]" metricMagnitude="1" metricUnit="m" metricValue="2.7" pageId="42" pageNumber="43" unit="m" value="27.0">27 m</quantity>
