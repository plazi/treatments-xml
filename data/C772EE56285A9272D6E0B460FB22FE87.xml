<number id="F9134D67285B9271D709B6C2FE55F941" box="[382,415,1730,1755]" pageId="17" pageNumber="88" value="86.0">86</number>
: 
<number id="F9134D67285B9271D7D3B6C2FE0DF941" box="[420,455,1730,1755]" pageId="17" pageNumber="88" value="42.0">42</number>
