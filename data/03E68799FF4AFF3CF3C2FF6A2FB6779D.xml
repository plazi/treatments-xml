<subSubSection id="C3556504FF4AFF3CF392FCBF2FB6779D" pageId="169" pageNumber="170" type="diagnosis">
<paragraph id="8BF0368FFF4AFF3CF392FCBF2FB6779D" blockId="169.[151,1436,258,930]" pageId="169" pageNumber="170">
<emphasis id="B93BEA9DFF4AFF3CF392FCBF2F497764" bold="true" box="[199,321,834,859]" inLineHeading="true" pageId="169" pageNumber="170" reason="1">Diagnosis.</emphasis>
<taxonomicName id="4C4F4D0CFF4AFF3CF21FFCBE2C217764" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[330,553,835,859]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="species" species="media">
<emphasis id="B93BEA9DFF4AFF3CF21FFCBE2C217764" box="[330,553,835,859]" italics="true" pageId="169" pageNumber="170">Saccharodite media</emphasis>
</taxonomicName>
resembles 
<taxonomicName id="4C4F4D0CFF4AFF3CF1F9FCBE2D127765" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[684,794,835,859]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="species" species="nanus" status="sp. nov.">
