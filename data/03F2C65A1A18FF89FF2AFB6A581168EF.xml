<taxonomicName id="4C5B0CCF1A1AFF8EFED2F84A594B6DDA" authorityName="Ramakrishna &amp; Margabandhu" authorityYear="1931" box="[367,590,1946,1970]" class="Insecta" family="Aeolothripidae" genus="Mymarothrips" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="garuda">
