species:
</emphasis>
<taxonomicName authority="Carter" authorityName="Carter" authorityYear="1885" box="[323,702,1172,1198]" class="Hexactinellida" family="Tretodictyidae" genus="Hexactinella" kingdom="Animalia" order="Hexactinosida" pageId="6" pageNumber="59" phylum="Porifera" rank="species" species="ventilabrum">
