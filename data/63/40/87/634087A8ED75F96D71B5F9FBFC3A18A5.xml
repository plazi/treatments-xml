<paragraph blockId="1.[151,1437,1673,2022]" box="[151,760,1673,1698]" pageId="1" pageNumber="404">
<emphasis bold="true" box="[151,306,1673,1698]" pageId="1" pageNumber="404">Type species.</emphasis>
<taxonomicName authority="Nonato, 1966" authorityName="Nonato" authorityYear="1966" box="[313,756,1673,1698]" class="Polychaeta" family="Sternaspidae" genus="Petersenaspis" kingdom="Animalia" order="Terebellida" pageId="1" pageNumber="404" phylum="Annelida" rank="species" species="capillata">
