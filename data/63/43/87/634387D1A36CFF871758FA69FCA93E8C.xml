<paragraph pageId="73" pageNumber="70">
<table box="[174,1369,265,517]" gridcols="5" gridrows="3" pageId="73" pageNumber="70">
<tr box="[174,1369,265,287]" gridrow="0" pageId="73" pageNumber="70">
