<collectingCountry box="[577,664,189,215]" name="Kuwait" pageId="8" pageNumber="259">Kuwait</collectingCountry>
, 
<collectingCountry box="[676,738,189,215]" name="United Arab Emirates" pageId="8" pageNumber="259">UAE</collectingCountry>
), 
