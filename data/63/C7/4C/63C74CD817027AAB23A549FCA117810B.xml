<paragraph pageId="10" pageNumber="141">
<taxonomicName class="Gastropoda" family="Lymnaeidae" genus="Radix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Radix peregra" pageId="10" pageNumber="141" phylum="Mollusca" rank="species" species="peregra">Radix peregra</taxonomicName>
