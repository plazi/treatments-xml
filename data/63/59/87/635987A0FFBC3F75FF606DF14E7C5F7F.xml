<collectingCountry box="[359,426,1125,1150]" name="China" pageId="15" pageNumber="86">China</collectingCountry>
(
<collectingRegion box="[441,517,1125,1150]" country="China" name="Fujian" pageId="15" pageNumber="86">Fujian</collectingRegion>
, 
<collectingRegion box="[528,706,1125,1150]" country="China" name="Nei Mongol" pageId="15" pageNumber="86">
Inner 
<collectingCountry box="[594,706,1125,1150]" name="Mongolia" pageId="15" pageNumber="86">Mongolia</collectingCountry>
</collectingRegion>
, 
<figureCitation box="[717,799,1125,1150]" captionStart="FIGURE 16" captionStartId="26.[151,250,1941,1963]" captionTargetBox="[195,1380,327,1864]" captionTargetId="figure@26.[151,1436,303,1919]" captionTargetPageId="26" captionText="FIGURE 16. Geographical distribution of Mohrigia from China. The different coloured shapes refer to different collecting sites of the species." httpUri="https://zenodo.org/record/837187/files/figure.png" pageId="15" pageNumber="86">Fig. 16</figureCitation>
