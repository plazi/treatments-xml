(
<collectingRegion box="[441,531,1797,1821]" country="China" name="Yunnan" pageId="21" pageNumber="92">Yunnan</collectingRegion>
, 
<figureCitation box="[542,624,1796,1821]" captionStart="FIGURE 16" captionStartId="26.[151,250,1941,1963]" captionTargetBox="[195,1380,327,1864]" captionTargetId="figure@26.[151,1436,303,1919]" captionTargetPageId="26" captionText="FIGURE 16. Geographical distribution of Mohrigia from China. The different coloured shapes refer to different collecting sites of the species." httpUri="https://zenodo.org/record/837187/files/figure.png" pageId="21" pageNumber="92">Fig. 16</figureCitation>
