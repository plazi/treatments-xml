<title pageId="null" pageNumber="794">
<subSubSection pageId="null" pageNumber="794" type="nomenclature">
<paragraph pageId="null" pageNumber="794">
<taxonomicName authority="L." class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" kingdom="Plantae" order="Caryophyllales" pageId="null" pageNumber="794" phylum="Tracheophyta" rank="species" species="plumarius">
<pageBreakToken pageId="null" pageNumber="794">Dianthus</pageBreakToken>
<normalizedToken originalValue="plumárius" pageId="null" pageNumber="794">plumarius</normalizedToken>
<authorityName pageId="null" pageNumber="794">L.</authorityName>
</taxonomicName>
</paragraph>
