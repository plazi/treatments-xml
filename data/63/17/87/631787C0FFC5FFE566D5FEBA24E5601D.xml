<subSubSection lastPageId="7" lastPageNumber="440" pageId="5" pageNumber="438" type="materials_examined">
<paragraph blockId="5.[151,1437,1909,2006]" pageId="5" pageNumber="438">
<emphasis bold="true" box="[199,378,1909,1934]" inLineHeading="true" pageId="5" pageNumber="438" reason="1">Type material.</emphasis>
<materialsCitation collectingDate="1981-10-01" collectionCode="R, INPA" collectorName="J. A. Rafael" country="Brazil" county="Dulke" determinerName="Santis" elevation="15" location="Amazonas" municipality="Manaus" pageId="5" pageNumber="438" specimenCount="1" specimenCount-female="1" typeStatus="holotype">
<typeStatus box="[389,537,1909,1934]" pageId="5" pageNumber="438">HOLOTYPE</typeStatus>
<specimenCount box="[549,570,1910,1933]" pageId="5" pageNumber="438" type="female">♀</specimenCount>
, labelled as follows: “ 
