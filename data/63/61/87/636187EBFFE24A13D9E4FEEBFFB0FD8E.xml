and 
<taxonomicName authorityName="Bates" authorityYear="1880" box="[1155,1387,152,176]" class="Insecta" family="Cerambycidae" genus="Eburia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="294" phylum="Arthropoda" rank="species" species="macrotaenia" subGenus="Eburia">
<emphasis box="[1155,1179,153,176]" italics="true" pageId="5" pageNumber="294">E.</emphasis>
