<taxonomicName authority="Emeljanov" authorityName="Emeljanov" box="[860,1200,500,525]" class="Insecta" family="Hemiptera (awaiting allocation)" genus="Atropidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="397" phylum="Arthropoda" rank="species" species="lapilla">
<emphasis box="[860,1065,500,524]" italics="true" pageId="1" pageNumber="397">Atropidius lapilla</emphasis>
Emeljanov
</taxonomicName>
and both occur on 
<taxonomicName authority="(Trin.) Nevski." authorityName="Nevski." baseAuthorityName="Trin." box="[151,593,535,560]" class="Liliopsida" family="Poaceae" genus="Achnatherum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="1" pageNumber="397" phylum="Tracheophyta" rank="species" species="splendens">
