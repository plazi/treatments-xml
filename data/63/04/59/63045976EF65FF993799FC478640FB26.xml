Perhaps 
<taxonomicName authority="Chen" authorityName="Chen" authorityYear="2002" box="[331,748,1161,1186]" class="Insecta" family="Delphacidae" genus="Elachodelphax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="399" phylum="Arthropoda" rank="species" species="xingjiangensis">
