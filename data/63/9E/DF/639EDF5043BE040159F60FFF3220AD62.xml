<taxonomicName authorityName="C. O. Waterhouse" authorityYear="1884" class="Insecta" family="Curculionidae" genus="Palaechthus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="19" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="19" pageNumber="34">Palaechthus</emphasis>
</taxonomicName>
