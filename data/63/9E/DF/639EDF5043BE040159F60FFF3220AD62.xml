<subSubSection pageId="19" pageNumber="34" type="diagnosis">
<paragraph pageId="19" pageNumber="34">Diagnosis.</paragraph>
<paragraph pageId="19" pageNumber="34">Small to very small (3.7-6.5 mm); vestiture of seta-like scales and setae; pronotum subcylindrical.</paragraph>
</subSubSection>
<subSubSection pageId="19" pageNumber="34" type="relationships">
<paragraph pageId="19" pageNumber="34">Relationships.</paragraph>
<paragraph pageId="19" pageNumber="34">
<taxonomicName authorityName="Brinck" authorityYear="1948" class="Insecta" family="Curculionidae" genus="Tristanodes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="19" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="19" pageNumber="34">Tristanodes</emphasis>
</taxonomicName>
