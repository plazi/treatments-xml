(
<taxonomicName authorityName="Bleeker" authorityYear="1859" box="[244,368,1104,1123]" class="Actinopterygii" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="875" phylum="Chordata" rank="order">Perciformes</taxonomicName>
: 
<taxonomicName box="[378,464,1104,1123]" class="Actinopterygii" family="Cottidae" kingdom="Animalia" order="Scorpaeniformes" pageId="4" pageNumber="875" phylum="Chordata" rank="family">Cottidae</taxonomicName>
), 
<emphasis box="[480,593,1103,1123]" italics="true" pageId="4" pageNumber="875">
Gobio 
<taxonomicName box="[542,593,1103,1123]" class="Actinopterygii" family="Cottidae" genus="Cottus" kingdom="Animalia" order="Scorpaeniformes" pageId="4" pageNumber="875" phylum="Chordata" rank="species" species="gobio">gobio</taxonomicName>
</emphasis>
, 
<taxonomicName authority="leuciscus" box="[605,783,1103,1123]" class="Actinopterygii" family="Cyprinidae" genus="Leuciscus" kingdom="Animalia" order="Cypriniformes" pageId="4" pageNumber="875" phylum="Chordata" rank="species" species="leuciscus">
