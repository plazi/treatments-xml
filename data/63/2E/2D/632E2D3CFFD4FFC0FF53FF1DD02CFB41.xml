<number box="[151,183,469,495]" pageId="9" pageNumber="10" value="38.0">38</number>
; genitoaggenital plate 
<number box="[448,478,469,495]" pageId="9" pageNumber="10" value="88.0">88</number>
<date box="[478,525,469,495]" pageId="9" pageNumber="10" value="1958-10">
x 
