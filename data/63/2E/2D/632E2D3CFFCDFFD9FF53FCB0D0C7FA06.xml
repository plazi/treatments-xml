with 
<taxonomicName authority="Nicolet, 1855" authorityName="Nicolet" authorityYear="1855" box="[588,1221,1424,1450]" class="Arachnida" family="Phthiracaridae" genus="Steganacarus" kingdom="Animalia" order="Sarcoptiformes" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="magnus" subGenus="Steganacarus">
