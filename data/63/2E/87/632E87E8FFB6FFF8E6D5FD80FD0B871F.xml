<subSubSection pageId="9" pageNumber="10" type="materials_examined">
<paragraph blockId="9.[151,1437,721,1324]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[151,383,721,747]" pageId="9" pageNumber="10">Material examined.</emphasis>
