<taxonomicName authorityName="E.Horak" authorityYear="1977" class="Insecta" family="Strophariaceae" genus="Melanotus" higherTaxonomySource="GBIF" kingdom="Fungi" lsidName="Melanotus communis" order="Agaricales" pageId="0" pageNumber="79" phylum="Arthropoda" rank="species" species="communis">
<emphasis italics="true" pageId="0" pageNumber="79">Melanotus communis</emphasis>
</taxonomicName>
