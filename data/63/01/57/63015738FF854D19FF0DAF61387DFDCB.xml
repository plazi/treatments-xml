<taxonomicName authorityName="Motschulsky" authorityYear="1860" box="[690,877,394,417]" class="Insecta" family="Curculionidae" genus="Stephanocleonus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="449" phylum="Arthropoda" rank="genus">
