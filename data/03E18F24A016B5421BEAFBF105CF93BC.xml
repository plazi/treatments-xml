<emphasis id="B93CE220A015B5431C85F87103EC96A9" bold="true" box="[1217,1231,1940,1957]" pageId="10" pageNumber="215">D</emphasis>
, left lateral view; 
<emphasis id="B93CE220A015B5431D20F871025396A9" bold="true" box="[1380,1392,1940,1957]" pageId="10" pageNumber="215">E</emphasis>
, dorsal view; 
<emphasis id="B93CE220A015B54318F1F84E07E396B0" bold="true" box="[181,192,1963,1980]" pageId="10" pageNumber="215">F</emphasis>
, 
