<normalizedToken id="4F01FA7BDF8FADA4B5C928624C2ACFF3" originalValue="cáprea" pageId="null" pageNumber="675">caprea</normalizedToken>
<authorityName id="D2923FB238D946ACE39B7D216B73BE78" pageId="null" pageNumber="675">L.</authorityName>
