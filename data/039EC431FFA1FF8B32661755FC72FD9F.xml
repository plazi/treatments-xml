<taxonomicName id="4C370EA4FFA1FF943299141EFD8BF99A" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[503,569,1567,1590]" class="Aves" family="Paradisaeidae" genus="Cicinnurus" kingdom="Animalia" order="Passeriformes" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="regius">
