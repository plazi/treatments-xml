<taxonomicName id="4C5C9BE6FF931053FADEF8F3FA4C3705" box="[1386,1445,1884,1908]" class="Pinopsida" family="Pinaceae" genus="Abies" kingdom="Plantae" order="Pinales" pageId="11" pageNumber="75" phylum="Tracheophyta" rank="genus">
<emphasis id="B9283C77FF931053FADEF8F3FA4C3705" box="[1386,1445,1884,1908]" italics="true" pageId="11" pageNumber="75">Abies</emphasis>
</taxonomicName>
