<taxonomicName id="4C5C9BE6FF931053FA8BF895FEC13705" authority="Butler" authorityName="Butler" authorityYear="1958" class="Insecta" family="Tortricidae" genus="Archippus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="species" species="pulchra">
<emphasis id="B9283C77FF931053FA8BF895FF323705" italics="true" pageId="11" pageNumber="75">Archippus pulchra</emphasis>
Butler
</taxonomicName>
, 
<taxonomicName id="4C5C9BE6FF931053FE82F8F2FE043705" authority="Butler" authorityName="Butler" authorityYear="1958" box="[310,493,1884,1908]" class="Insecta" family="Tortricidae" genus="Archippus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="species" species="similes">
<emphasis id="B9283C77FF931053FE82F8F2FE493705" box="[310,416,1884,1908]" italics="true" pageId="11" pageNumber="75">A. similes</emphasis>
Butler
</taxonomicName>
, 
<taxonomicName id="4C5C9BE6FF931053FE4EF8F3FC883705" authority="Hubner" authorityName="Hubner" box="[506,865,1884,1908]" class="Insecta" family="Tortricidae" genus="Choristoneura" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="species" species="diversana">
<emphasis id="B9283C77FF931053FE4EF8F3FCE13705" box="[506,776,1884,1908]" italics="true" pageId="11" pageNumber="75">Choristoneura diversana</emphasis>
Hübner
</taxonomicName>
and 
<taxonomicName id="4C5C9BE6FF931053FC2EF8F2FB083705" authority="(Issiki)" baseAuthorityName="Issiki" box="[922,1249,1884,1908]" class="Insecta" family="Tortricidae" genus="Lozotaenia" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="75" phylum="Arthropoda" rank="species" species="coniferana">
