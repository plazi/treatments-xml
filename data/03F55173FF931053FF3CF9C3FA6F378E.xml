<taxonomicName id="4C5C9BE6FF931053FF3CF82FFF2C37E6" box="[136,197,1920,1943]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="11" pageNumber="75" phylum="Tracheophyta" rank="genus">
