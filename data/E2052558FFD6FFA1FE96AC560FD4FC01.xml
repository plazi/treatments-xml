<collectingCountry id="12BBD4DEFFD6FFA1FD92ACC60E56FC81" box="[561,639,744,768]" name="Bhutan" pageId="11" pageNumber="644">Bhutan</collectingCountry>
, 
<collectingCountry id="12BBD4DEFFD6FFA1FD2AACC60E96FC81" box="[649,703,744,768]" name="India" pageId="11" pageNumber="644">India</collectingCountry>
