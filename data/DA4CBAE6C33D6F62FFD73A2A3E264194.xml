<taxonomicName id="D0CD04EB0B0BB0A62C96DB5A7451F56C" LSID="http://zoobank.org/CC9D5DE7-61D7-420C-91F4-D30A3E158A6D" authority="Kirkaldy, 1901" authorityName="Kirkaldy" authorityYear="1901" class="Insecta" family="Alaudidae" genus="Rhamphocoris" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhamphocoris guizhouensis" order="Passeriformes" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="guizhouensis">Rhamphocoris guizhouensis</taxonomicName>
