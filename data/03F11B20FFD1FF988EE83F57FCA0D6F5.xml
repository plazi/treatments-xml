<collectingRegion id="499C64D4FFCEFF988F183A8BFDBAD6F5" box="[359,616,1197,1223]" country="South Africa" name="Mpumalanga" pageId="16" pageNumber="277">Mpumalanga Province</collectingRegion>
, 
<collectingCountry id="F34FEAA6FFCEFF988C0B3A8BFCD1D6F5" box="[628,771,1197,1223]" name="South Africa" pageId="16" pageNumber="277">South Africa</collectingCountry>
(
