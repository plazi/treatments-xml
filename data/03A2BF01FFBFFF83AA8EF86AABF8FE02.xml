<quantity id="4CF3A3F2FFBEFF83A963FE81AA61FEE7" box="[890,1029,359,380]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" metricValueMax="3.4" metricValueMin="3.2" pageId="11" pageNumber="551" unit="mm" value="3.3" valueMax="3.4" valueMin="3.2">3.20–3.40 mm</quantity>
