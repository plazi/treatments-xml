<taxonomicName id="4C0B7594FFBEFF83AEC5FE62AB04FE02" authority="Cognato &amp; Smith &amp; Beaver, 2020" authorityName="Cognato &amp; Smith &amp; Beaver" authorityYear="2020" box="[1244,1376,388,409]" class="Insecta" family="Curculionidae" genus="Tricosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="551" phylum="Arthropoda" rank="species" species="indochinensis" status="sp. n.">
