<quantity id="4CF3A3F2FFBEFF83A965FECAAA6CFED9" box="[892,1032,300,322]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" metricValueMax="3.1" metricValueMin="2.7" pageId="11" pageNumber="551" unit="mm" value="2.9" valueMax="3.1" valueMin="2.7">2.70–3.10 mm</quantity>
