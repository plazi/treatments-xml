<taxonomicName id="3EA04D828B53FFE76377F9A0FE23756D" authority="Hernandes, 2020" authorityName="Hernandes" authorityYear="2020" box="[295,494,1581,1604]" class="Arachnida" family="Gabuciniidae" genus="Aetacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="accipiter" status="sp. nov.">
