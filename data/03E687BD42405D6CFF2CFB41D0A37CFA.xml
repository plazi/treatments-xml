<typeStatus id="54F4880942405D6FFF7EF963D0247BEC" box="[197,258,1671,1697]" pageId="13" pageNumber="14">Type</typeStatus>
locality.
</emphasis>
<collectingCountry id="F358763B42405D6FFEC9F96CD3247BEF" box="[370,514,1672,1698]" name="Guadeloupe" pageId="13" pageNumber="14">Guadeloupe</collectingCountry>
, 
<quantity id="4CB79B4E42405D6FFDB5F96CD3957BEF" box="[526,691,1672,1698]" metricMagnitude="3" metricUnit="m" metricValue="1.507" metricValueMax="1.607" metricValueMin="1.4069999999999998" pageId="13" pageNumber="14" unit="m" value="1507.0" valueMax="1607.0" valueMin="1407.0">1407–1607 m</quantity>
