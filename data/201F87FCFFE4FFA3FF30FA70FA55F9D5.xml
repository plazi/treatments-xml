<number id="1E7E24CDFFE4FFA1FCB6F81AFC8CF830" box="[785,837,2002,2027]" pageId="5" pageNumber="276" value="1.06">1.06</number>
; XI = 
<number id="1E7E24CDFFE4FFA1FC2BF81AFC09F830" box="[908,960,2002,2027]" pageId="5" pageNumber="276" value="1.46">1.46</number>
