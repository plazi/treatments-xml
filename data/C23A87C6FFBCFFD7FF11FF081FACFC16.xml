<collectionCode id="2C82AE15FFBCFFD7FEA8FEF91D79FE7E" LSID="urn:lsid:biocol.org:col:34871" box="[302,383,361,385]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="15" pageNumber="16">USNM</collectionCode>
