<materialsCitation id="3B2E4919F3267F4114BFA6FBFD06FB1D" ID-GBIF-Occurrence="1701951658" box="[151,757,1160,1186]" location="Southern" pageId="34" pageNumber="235" specimenCount="1" stateProvince="Southern">
