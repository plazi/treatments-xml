<taxonomicName id="19A64D6F22601B68FCC7F8E2FC0E147F" authorityName="Travassos" authorityYear="1929" box="[794,962,1880,1905]" class="Secernentea" family="Thelastomatidae" kingdom="Animalia" order="Oxyurida" pageId="4" pageNumber="91" phylum="Nematoda" rank="family">Thelastomatidae</taxonomicName>
