(Hampson, 
<number id="EA102491611B440FC7705E71FE83FCDF" box="[276,332,816,840]" pageId="2" pageNumber="290" value="1900.0">1900</number>
)
</taxonomicName>
(
<taxonomicName id="9BD84D35611B440FC7085E71FE14FCDF" box="[364,475,816,840]" class="Insecta" family="Arctiidae" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="290" phylum="Arthropoda" rank="family">Arctiidae</taxonomicName>
