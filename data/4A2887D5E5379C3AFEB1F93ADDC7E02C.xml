<number id="744924E4E5299C39FB4EFD52DC40E73C" box="[1271,1322,764,790]" pageId="17" pageNumber="18" value="0.95">0.95</number>
long, 
<number id="744924E4E5299C39FEF6FC8AD8E8E714" box="[335,386,804,830]" pageId="17" pageNumber="18" value="0.8">0.80</number>
