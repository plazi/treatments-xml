<collectingCountry id="E5C9F2D88033FF90E987C9B31C3709CF" box="[1260,1386,1946,1971]" name="Philippines" pageId="55" pageNumber="56">Philippines</collectingCountry>
and 
<collectingCountry id="E5C9F2D88033FF90EDFCC996182F09AB" box="[151,370,1982,2007]" name="Papua New Guinea" pageId="55" pageNumber="56">Papua New Guinea</collectingCountry>
