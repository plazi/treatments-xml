<quantity id="5A261FAD8033FF90EF91C9971A1209AA" box="[762,847,1982,2007]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="5.0" metricValueMin="0.0" pageId="55" pageNumber="56" unit="m" value="25.0" valueMax="50.0" valueMin="0.0">0–50 m</quantity>
