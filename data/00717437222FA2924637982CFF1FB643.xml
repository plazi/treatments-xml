<taxonomicName id="4DF6ECB11AB6B6F3CD0194F0D06189E2" class="Insecta" family="Formicidae" genus="Myrmica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmica rugulososcabrinodis" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="rugulososcabrinodis">Myrmica rugulososcabrinodis</taxonomicName>
