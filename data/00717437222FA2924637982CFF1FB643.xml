<taxonomicName id="F2FAC71A7374250F59E45C7E3D3EE281" class="Insecta" family="Formicidae" genus="Myrmica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmica rugulososcabrinodis" order="Hymenoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="rugulososcabrinodis">Myrmica rugulososcabrinodis</taxonomicName>
