<taxonomicName id="4C504D56A64D8A09FAA641537D96F87B" authority="Chapman, 1917" authorityName="Chapman" authorityYear="1917" class="Insecta" family="Lycaenidae" genus="Plebejus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="591" phylum="Arthropoda" rank="subSpecies" species="idas" status="syn.n." subSpecies="sareptensis">
<emphasis id="B924EAC7A64D8A09FAA641537D5BF87B" italics="true" pageId="2" pageNumber="591">P. idas sareptensis</emphasis>
Chapman, 1917
</taxonomicName>
, 
<taxonomicNameLabel id="A21757BCA64D8A09FE3B41717E47F87B" box="[463,530,1919,1943]" pageId="2" pageNumber="591">
