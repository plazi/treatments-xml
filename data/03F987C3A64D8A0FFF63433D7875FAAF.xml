<taxonomicName id="4C504D56A64D8A09FC1D4153797BF898" authority="idas (Linnaeus, 1761)" authorityName="idas (Linnaeus" authorityYear="1761" box="[1001,1326,1884,1908]" class="Insecta" family="Lycaenidae" genus="Plebejus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="591" phylum="Arthropoda" rank="subSpecies" species="idas" subSpecies="idas">
<emphasis id="B924EAC7A64D8A09FC1D4153783CF898" box="[1001,1129,1884,1908]" italics="true" pageId="2" pageNumber="591">P. idas idas</emphasis>
