<taxonomicName id="4C504D56A64D8A09FB2741347D11F898" authority="Chapman, 1917" authorityName="Chapman" authorityYear="1917" class="Insecta" family="Lycaenidae" genus="Plebejus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="591" phylum="Arthropoda" rank="subSpecies" species="idas" subSpecies="sareptensis">
