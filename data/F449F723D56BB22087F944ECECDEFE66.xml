<specimenCount id="6AE68DBCD568B22085FB4536E924FAEF" box="[226,336,1363,1384]" pageId="74" pageNumber="75" type="female">four female</specimenCount>
specimens collected in the 
<location id="793F10EED568B22087444536E967FA04" LSID="urn:lsid:plazi:treatment:F449F723D56BB22087F944ECECDEFE66:793F10EED568B22087444536E967FA04" country="United States of America" name="Davis Mountains" pageId="74" pageNumber="75" stateProvince="Texas">Davis Mountains</location>
, 
<collectingRegion id="BE2488D7D568B2208404450BE921FA04" box="[285,341,1390,1411]" country="United States of America" name="Texas" pageId="74" pageNumber="75">Texas</collectingRegion>
, 
<collectingCountry id="04F706A5D568B2208478450BE9E7FA04" box="[353,403,1390,1411]" name="United States of America" pageId="74" pageNumber="75">USA</collectingCountry>
