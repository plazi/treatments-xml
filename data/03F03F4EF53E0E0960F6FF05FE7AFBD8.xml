<subSubSection id="C343DDD3F53E0E0960F6FE90FC65FEB8" box="[151,907,258,283]" pageId="5" pageNumber="556" type="diagnosis">
<paragraph id="8BE68E58F53E0E0960F6FE90FC65FEB8" blockId="5.[151,1437,258,1147]" box="[151,907,258,283]" pageId="5" pageNumber="556">
<emphasis id="B92D524AF53E0E0960F6FE90FEFFFEB8" bold="true" box="[151,273,258,283]" pageId="5" pageNumber="556">Diagnosis.</emphasis>
<collectingCountry id="F34ECEC8F53E0E096179FE90FEBEFEB8" box="[280,336,258,283]" name="India" pageId="5" pageNumber="556">Hind</collectingCountry>
