<subSubSection id="A08065459051FF99E0A1A14B14CBFACE" box="[151,450,1377,1403]" pageId="6" pageNumber="581" type="nomenclature">
<paragraph id="E82536CE9051FF99E0A1A14B14CBFACE" blockId="6.[151,450,1377,1403]" box="[151,450,1377,1403]" pageId="6" pageNumber="581">
<heading id="B36D81A29051FF99E0A1A14B14CBFACE" bold="true" box="[151,450,1377,1403]" fontSize="11" level="1" pageId="6" pageNumber="581" reason="1">
<taxonomicName id="2F9A4D4D9051FF99E0A1A14B14CBFACE" authority="Crosnier, 1986" authorityName="Crosnier" authorityYear="1986" box="[151,450,1377,1403]" class="Malacostraca" family="Pandalidae" genus="Plesionika" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="581" phylum="Arthropoda" rank="species" species="carsini">
