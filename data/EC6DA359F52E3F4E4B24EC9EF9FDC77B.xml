<taxonomicName id="A3C469CCF52F3F4E49E9EDA3FB89C39D" box="[905,1133,715,757]" class="Magnoliopsida" family="Lecythidaceae" genus="Barringtonia" kingdom="Plantae" order="Ericales" pageId="35" pageNumber="46" phylum="Tracheophyta" rank="genus">
