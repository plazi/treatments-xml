<taxonomicName id="8D934D53FF88FFE3FEDEFBBE1E0CFBB8" box="[344,522,1070,1095]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="59" pageNumber="60" phylum="Porifera" rank="species" species="schmidti">
<emphasis id="78E7EAC2FF88FFE3FEDEFBBE1E0CFBB8" box="[344,522,1070,1095]" italics="true" pageId="59" pageNumber="60">Agelas schmidti</emphasis>
</taxonomicName>
; 
<quantity id="8D6B9B35FF88FFE3FD9FFBBE1E7DFBB9" box="[537,635,1070,1095]" metricMagnitude="1" metricUnit="m" metricValue="2.15" metricValueMax="2.5" metricValueMin="1.8" pageId="59" pageNumber="60" unit="m" value="21.5" valueMax="25.0" valueMin="18.0">18–25 m</quantity>
depth.
</paragraph>
</subSubSection>
<subSubSection id="0289655BFF88FFE3FF41FBC218A3FB94" box="[199,1189,1106,1131]" pageId="59" pageNumber="60" type="distribution">
<paragraph id="4A2C36D0FF88FFE3FF41FBC218A3FB94" blockId="59.[151,1437,602,1131]" box="[199,1189,1106,1131]" pageId="59" pageNumber="60">
<emphasis id="78E7EAC2FF88FFE3FF41FBC21D66FB94" bold="true" box="[199,352,1106,1131]" pageId="59" pageNumber="60">Distribution.</emphasis>
