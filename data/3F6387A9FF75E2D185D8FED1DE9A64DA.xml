<geoCoordinate id="D2FE5078FF75E2D186A7FE41D9316482" box="[1000,1087,402,427]" direction="south" orientation="latitude" pageId="152" pageNumber="153" precision="555" value="-49.15">49.15ºS</geoCoordinate>
