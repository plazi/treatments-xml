<number id="3D94AE59FFC05E7DFEF1FF26FEA1FF2E" box="[263,320,188,212]" pageId="1" value="1834.0">1834</number>
)
</taxonomicName>
[
<taxonomicName id="4C5CC7FDFFC05E7DFEAFFF26FE59FF2F" box="[345,440,188,213]" class="Magnoliopsida" family="Amaranthaceae" genus="Halimus" kingdom="Animalia" order="Caryophyllales" pageId="1" phylum="Tracheophyta" rank="genus">
<emphasis id="B928606CFFC05E7DFEAFFF26FE59FF2F" box="[345,440,188,213]" italics="true" pageId="1">Halimus</emphasis>
