<number id="3D94AE59FFC05E7DFAADFF26FA75FF2F" box="[1371,1428,188,213]" pageId="1" value="1865.0">1865</number>
)
</taxonomicName>
[
<taxonomicName id="4C5CC7FDFFC05E7DFF56FF45FF1EFF02" box="[160,255,223,248]" class="Magnoliopsida" family="Amaranthaceae" genus="Halimus" kingdom="Animalia" order="Caryophyllales" pageId="1" phylum="Tracheophyta" rank="genus">
<emphasis id="B928606CFFC05E7DFF56FF45FF1EFF02" box="[160,255,223,248]" italics="true" pageId="1">Halimus</emphasis>
