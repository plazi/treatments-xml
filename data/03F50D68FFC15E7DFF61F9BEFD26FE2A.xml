]; 
<taxonomicName id="4C5CC7FDFFC05E7DFE26FF27FD14FF2F" authority="Latreille, 1825" authorityName="Latreille" authorityYear="1825" box="[464,757,188,213]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="1" phylum="Arthropoda" rank="species" species="aurita">
<emphasis id="B928606CFFC05E7DFE26FF27FDD7FF2E" box="[464,566,189,212]" italics="true" pageId="1">N. aurita</emphasis>
(Latreille, 
<number id="3D94AE59FFC05E7DFD42FF26FD0CFF2F" box="[692,749,188,213]" pageId="1" value="1825.0">1825</number>
)
</taxonomicName>
[
<taxonomicName id="4C5CC7FDFFC05E7DFCF0FF27FCD9FF2E" box="[774,824,189,212]" class="Malacostraca" family="Epialtidae" genus="Pisa" kingdom="Animalia" order="Decapoda" pageId="1" phylum="Arthropoda" rank="genus">
<emphasis id="B928606CFFC05E7DFCF0FF27FCD9FF2E" box="[774,824,189,212]" italics="true" pageId="1">Pisa</emphasis>
</taxonomicName>
] (
<typeStatus id="54E702DCFFC05E7DFCA6FF26FC63FF2E" box="[848,898,188,212]" pageId="1">type</typeStatus>
species by monotypy); 
<taxonomicName id="4C5CC7FDFFC05E7DFB78FF27FA7DFF2F" authority="Hess, 1865" authorityName="Hess" authorityYear="1865" box="[1166,1436,188,213]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="1" phylum="Arthropoda" rank="species" species="spinosa">
<emphasis id="B928606CFFC05E7DFB78FF27FAE7FF2E" box="[1166,1286,189,212]" italics="true" pageId="1">N. spinosa</emphasis>
