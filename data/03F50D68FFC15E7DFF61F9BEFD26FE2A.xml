]; 
<taxonomicName id="4C5CC7FDFFC05E7DFEE1FF7BFDC1FF02" authority="Dana, 1851" authorityName="Dana" authorityYear="1851" box="[279,544,223,248]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="1" phylum="Arthropoda" rank="species" species="tumida">
