<taxonomicName id="469C4D41FFECFFDBFEF95DA3FCA7FCC5" authority="(Xue, Song &amp; Hong, 2006)" baseAuthorityName="Xue, Song &amp; Hong" baseAuthorityYear="2006" box="[272,788,750,775]" class="Arachnida" family="Phytoptidae" genus="Nalepella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="fabris">
