<taxonomicName id="469C4D41FFECFFDBFB6F5C7AFAB4FC8D" authorityName="Huang" authorityYear="1996" box="[1158,1287,823,846]" class="Arachnida" family="Phytoptidae" genus="Pentaporca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
<emphasis id="B3E8EAD0FFECFFDBFB6F5C7AFAB4FC8D" box="[1158,1287,823,846]" italics="true" pageId="12" pageNumber="13">Pentaporca</emphasis>
</taxonomicName>
, 
<taxonomicName id="469C4D41FFECFFDBFAF35C7AFE08FCB1" authority="Huang, 1996" authorityName="Huang" authorityYear="1996" class="Arachnida" family="Phytoptidae" genus="Pentaporca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="taiwanensis">
<emphasis id="B3E8EAD0FFECFFDBFAF35C7AFEA8FCB1" italics="true" pageId="12" pageNumber="13">Pentaporca taiwanensis</emphasis>
Huang, 1996
</taxonomicName>
[from 
<taxonomicName id="469C4D41FFECFFDBFDF95C16FCB5FCB1" authority="Pritz" authorityName="Pritz" box="[528,774,858,883]" class="Pinopsida" family="Pinaceae" genus="Tsuga" kingdom="Plantae" order="Pinales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="species" species="chinensis">
<emphasis id="B3E8EAD0FFECFFDBFDF95C16FD76FCB0" box="[528,709,859,883]" italics="true" pageId="12" pageNumber="13">Tsuga chinensis</emphasis>
Pritz
</taxonomicName>
(
<taxonomicName id="469C4D41FFECFFDBFCF15C17FC31FCB1" box="[792,898,858,882]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="12" pageNumber="13" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
)] and 
<taxonomicName id="469C4D41FFECFFDBFC275C16FF68FC55" authority="(Xue, Song &amp; Hong, 2006)" baseAuthorityName="Xue, Song &amp; Hong" baseAuthorityYear="2006" class="Arachnida" family="Phytoptidae" genus="Pentaporca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="fabris">
<emphasis id="B3E8EAD0FFECFFDBFC275C16FB2FFCB0" box="[974,1180,859,883]" italics="true" pageId="12" pageNumber="13">Pentaporca fabris</emphasis>
(
<bibRefCitation id="E50D4B33FFECFFDBFB465C17FF60FC55" author="Xue, Song &amp; Hong" firstAuthor="Xue" pageId="12" pageNumber="13" pagination="31 - 45" refId="ref52836" refString="Song, Z. - W., Xue, X. - F. &amp; Hong, X. - Y. (2006) A review of Chinese Phyllocoptes Nalepa (Acari: Eriophyoidea: Eriophyidae: Phyllocoptinae) with descriptions of five new species. Zootaxa, 1167, 31 - 45." type="journal article" year="2006">Xue, Song &amp; Hong, 2006</bibRefCitation>
