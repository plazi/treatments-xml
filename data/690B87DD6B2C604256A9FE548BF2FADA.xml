<taxonomicName id="26A24D486B2C60425459FC9D8BDBFC86" box="[615,760,846,871]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="12" pageNumber="173" phylum="Arthropoda" rank="species" species="seminuda">
<emphasis id="D3D6EAD96B2C60425459FC9D8BDBFC86" box="[615,760,846,871]" italics="true" pageId="12" pageNumber="173">M. seminuda</emphasis>
