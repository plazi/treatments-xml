<taxonomicName id="4850CD7D5FBFD2E912CAA77E97DA1A94" authorityName="García &amp; Ahumada-C." authorityYear="2022" baseAuthorityName="Gonzalez-Sponga" baseAuthorityYear="1987" class="Arachnida" family="Agoristenidae" genus="Barinas" kingdom="Animalia" lsidName="Barinas virginis" order="Opiliones" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="virginis">
