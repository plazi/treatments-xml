<taxonomicName id="82834B8FF5CD61B6E89595567896896C" LSID="http://zoobank.org/C176D270-253E-4B06-9AA5-A3B969A5E332" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus pseudosumbawensis" order="Coleoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="pseudosumbawensis">Trigonopterus pseudosumbawensis Riedel</taxonomicName>
