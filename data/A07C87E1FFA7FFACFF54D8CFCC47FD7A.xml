<materialsCitation id="98BD3CAAFFA7FFA8FB11D97FC975FA0D" ID-GBIF-Occurrence="1803517701" collectingDate="1982-07-03" country="Russia" county="Chaun DiStrict" location="Pucheveem Stream" municipality="river Chaun" pageId="1" pageNumber="250" specimenCount="1" stateProvince="Magadan Province">
