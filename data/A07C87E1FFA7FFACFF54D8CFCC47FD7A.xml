<materialsCitation id="98BD3CAAFFA7FFA8FF54D95ACCACFAB5" ID-GBIF-Occurrence="1803517702" collectingDate="1981-08-16" country="Russia" county="Anadyr DiStrict" location="Anadyr River" municipality="Settlement Markovo" pageId="1" pageNumber="250" specimenCount="1" stateProvince="Magadan Province">
