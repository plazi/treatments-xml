<collectingDate id="4C2FE9DFFFA7FFA8FE43D97ECA52FAB5" box="[384,568,1318,1343]" pageId="1" pageNumber="250" value="1981-08-16">
<date id="5C6B1037FFA7FFA8FE43D97ECA52FAB5" box="[384,568,1318,1343]" pageId="1" pageNumber="250" value="1981-08-16">16 AuguSt 1981</date>
</collectingDate>
