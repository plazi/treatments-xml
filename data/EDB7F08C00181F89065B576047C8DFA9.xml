<taxonomicName id="4AE295179CE2199338728B2B3B0775FD" class="Arachnida" family="Phalangiidae" genus="Zachaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zachaeus anatolicus" order="Opiliones" pageId="1" pageNumber="16" phylum="Arthropoda" rank="species" species="anatolicus">Zachaeus anatolicus</taxonomicName>
