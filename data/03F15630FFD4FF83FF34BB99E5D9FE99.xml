<taxonomicName id="4C589CA5FFD4FF82FDF6BBD4E299F8FD" authority="Fowler, 1934" authorityName="Fowler" authorityYear="1934" box="[597,959,1770,1795]" class="Actinopterygii" family="Paralepididae" genus="Paralepis" kingdom="Animalia" order="Aulopiformes" pageId="8" pageNumber="122" phylum="Chordata" rank="species" species="philippinus">
