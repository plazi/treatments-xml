<taxonomicName id="4C2410E6FF9BFE3A05B3FB26FECB4A7D" authorityName="Deflandre, 1973:" authorityYear="1150" box="[134,306,1271,1297]" class="Polycystinea" family="Hexalonchidae" genus="Hollandosphaera" higherTaxonomySource="GBIF" kingdom="Chromista" order="Entactinaria" pageId="15" pageNumber="418" phylum="Retaria" rank="genus">
