<quantity id="4CBEEEA1F31B7F7C10AAA701FB7DFA38" box="[1154,1166,1394,1415]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" pageId="31" pageNumber="232" unit="in" value="3.0">3</quantity>
</figureCitation>
