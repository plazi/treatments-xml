<caption id="DF531481C7681808FF214E56FDC2F945" httpUri="https://zenodo.org/record/2623926/files/figure.png" pageId="18" pageNumber="515" startId="18.[151,250,1558,1580]" targetBox="[157,1428,196,1533]" targetPageId="18">
<paragraph id="8B934409C7681808FF214E56FDC2F945" blockId="18.[151,1436,1558,1672]" pageId="18" pageNumber="515">
<emphasis id="B958981BC7681808FF214E56FE87F9E1" bold="true" box="[151,283,1558,1580]" pageId="18" pageNumber="515">FIGURE 14</emphasis>
