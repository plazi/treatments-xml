<taxonomicName id="4C2C3F8AC7681808FDAE4E56FD7FF9E1" authorityName="Zhang &amp; Li &amp; Yin" authorityYear="2019" box="[536,739,1558,1580]" class="Insecta" family="Staphylinidae" genus="Labomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="515" phylum="Arthropoda" rank="species" species="maolan">
