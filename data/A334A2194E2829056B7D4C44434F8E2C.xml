<taxonomicName id="70190F1F6CC991BD0541F3EF0E8D4C4A" LSID="http://zoobank.org/B0620ADD-D4B0-418E-8545-CD6A9416E712" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus ejaculatorius" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="ejaculatorius">Trigonopterus ejaculatorius Riedel</taxonomicName>
