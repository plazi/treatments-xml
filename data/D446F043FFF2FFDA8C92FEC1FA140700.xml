</taxonomicName>
, 
<taxonomicName id="9BEF3AD6FFF2FFDA8D43FCC6FD630381" authority="Hoernes &amp; Auinger, 1880" authorityName="Hoernes &amp; Auinger" authorityYear="1880" box="[326,746,871,898]" class="Gastropoda" family="Mitridae" genus="Mitra" kingdom="Animalia" order="Neogastropoda" pageId="7" pageNumber="8" phylum="Mollusca" rank="species" species="sturi">
<emphasis id="6E9B9D47FFF2FFDA8D43FCC6FE490381" box="[326,448,872,897]" italics="true" pageId="7" pageNumber="8">Mitra sturi</emphasis>
