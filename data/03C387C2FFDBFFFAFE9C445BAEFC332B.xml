<number id="3DA224F3FFDBFFFAFDBE466EAF14332B" box="[554,569,809,835]" pageId="34" pageNumber="35" value="8.0">8</number>
and 
<number id="3DA224F3FFDBFFFAFDE6466EAFBD332B" box="[626,656,809,835]" pageId="34" pageNumber="35" value="24.0">24</number>
