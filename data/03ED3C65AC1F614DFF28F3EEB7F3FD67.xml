(
<taxonomicName id="4C44F6F0AC1F614DFF20F113B71DFD49" box="[159,313,618,644]" class="Magnoliopsida" family="Rhamnaceae" kingdom="Plantae" order="Rosales" pageId="6" pageNumber="39" phylum="Tracheophyta" rank="family">Rhamnaceae</taxonomicName>
