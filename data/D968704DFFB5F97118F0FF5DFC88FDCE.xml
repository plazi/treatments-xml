<location id="541E9780FFB5F9701A08FDF6FD77FD3F" LSID="urn:lsid:plazi:treatment:D968704DFFB5F97118F0FF5DFC88FDCE:541E9780FFB5F9701A08FDF6FD77FD3F" box="[599,754,621,642]" country="Canada" name="North America" pageId="4" pageNumber="31" stateProvince="British Columbia">North America</location>
