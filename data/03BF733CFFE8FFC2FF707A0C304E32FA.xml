<bibRefCitation id="EF87BFDBFFEFFFC2FD9E7E8F363232FA" author="ICZN" box="[633,779,404,428]" pageId="20" pageNumber="21" refString="ICZN (1999) International Code of Zoological Nomenclature, 4 th ed. International Trust for Zoological Nomenclature, London, xxix + 306 pp." type="book" year="1999">
<collectionCode id="ED075AEFFFEFFFC2FD9E7E8F378332FA" box="[633,698,404,428]" pageId="20" pageNumber="21">ICZN</collectionCode>
(
<number id="3DDED00DFFEFFFC2FD2E7E8F363A32FA" box="[713,771,404,428]" pageId="20" pageNumber="21" value="1999.0">1999</number>
)
</bibRefCitation>
by 
<bibRefCitation id="EF87BFDBFFEFFFC2FCD27E8F314F32FA" author="Gourcy-Droitaumont" box="[821,1142,404,428]" pageId="20" pageNumber="21" refString="Gourcy-Droitaumont, [H. F. G.] von (1829) Einige wichtige Beobachtungen uber seltene Vogel. Isis, 22, col. 724 - 736." type="journal article" year="1829">
Gourcy-Droitaumont (
<number id="3DDED00DFFEFFFC2FBD67E8F315432FA" box="[1073,1133,404,428]" pageId="20" pageNumber="21" value="1829.0">1829</number>
)
</bibRefCitation>
