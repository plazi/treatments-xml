(
<collectionCode id="1BD3AE74FF99F3765E3DF913FB29F973" box="[1144,1227,1757,1783]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="3" pageNumber="32" type="Museum">FMNH</collectionCode>
<collectionCode id="1BD3AE74FF99F3765E96F913FB19F973" box="[1235,1275,1757,1783]" pageId="3" pageNumber="32">KV</collectionCode>
