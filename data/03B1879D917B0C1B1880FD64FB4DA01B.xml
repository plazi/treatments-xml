<taxonomicName id="4C184D08917B0C1B18E1FD64FD3EA4C0" authority="Jairajpuri, 1969" authorityName="Jairajpuri" authorityYear="1969" box="[246,643,659,685]" class="Adenophorea" family="Mylonchulidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mononchida" pageId="28" pageNumber="29" phylum="Nematoda" rank="family">Mylonchulidae Jairajpuri, 1969</taxonomicName>
