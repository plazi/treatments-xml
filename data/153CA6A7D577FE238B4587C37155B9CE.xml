<taxonomicName id="C8ED6754FEAD6A7CEA02A98FF32EC7FA" LSID="http://zoobank.org/CC4D9926-B36D-449E-AFB9-9FBD4CBD5F2E" authority="Chen &amp; Ren" class="Insecta" family="Coccinellidae" genus="Scymnus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scymnus (Orthoscymnus) crispatus" order="Coleoptera" pageId="9" pageNumber="100" phylum="Arthropoda" rank="species" species="crispatus" subGenus="Orthoscymnus">Scymnus (Orthoscymnus) crispatus Chen &amp; Ren</taxonomicName>
<taxonomicNameLabel id="416CEB2CC7DBA4DDD9C2F516D3082774" pageId="9" pageNumber="100">sp. n.</taxonomicNameLabel>
