<taxonomicName id="4C55C7F7C46EFF92A1F8AD17DE22217E" authority="(Bouche, 1844)" baseAuthorityName="Bouche" baseAuthorityYear="1844" box="[150,617,1578,1603]" class="Insecta" family="Coccidae" genus="Parthenolecanium" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="504" phylum="Arthropoda" rank="species" species="corni">
