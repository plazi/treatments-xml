<taxonomicName id="2F9A4D4D9053FF9BE0A1A328173FF8A8" authority="Chace, 1985" authorityName="Chace" authorityYear="1985" box="[151,566,1794,1821]" class="Malacostraca" family="Pandalidae" genus="Plesionika" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="579" phylum="Arthropoda" rank="species" species="acinacifer">
