<taxonomicName id="142B4680DB67A8EDD36F61084D85B53F" class="Insecta" family="Osmylidae" genus="Osmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Osmylus langii" order="Neuroptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="langii">Osmylus langii</taxonomicName>
