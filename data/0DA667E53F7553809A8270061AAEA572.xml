<subSubSection id="51D504793E1E429672D0D3F9E5CC0B87" pageId="1" pageNumber="44" type="reference_group">
<paragraph id="2F18DC4A7BC521AC8A78FBE8C68A11C3" pageId="1" pageNumber="44">
<taxonomicName id="211EAEE3D7F96FA89FFAD9B707C7184F" class="Insecta" family="Osmylidae" genus="Thyridosmylus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thyridosmylus" order="Neuroptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="genus">Thyridosmylus</taxonomicName>
