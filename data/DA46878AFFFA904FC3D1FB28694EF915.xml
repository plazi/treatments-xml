<taxonomicName id="95EF4D1FFFFA904FC1D8FB286FCBFB43" authority="Stal, 1860" authorityName="Stal" authorityYear="1860" box="[670,1157,1224,1251]" class="Insecta" family="Reduviidae" genus="Brontostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" phylum="Arthropoda" rank="species" species="rubrovenosum">
