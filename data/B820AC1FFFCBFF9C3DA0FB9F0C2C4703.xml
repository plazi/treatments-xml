, and 
<taxonomicName id="F789668AFFCBFF9E3FB6F8400C8547F7" authority="Lee &amp; Beenen, 2017" authorityName="Lee &amp; Beenen" authorityYear="2017" box="[641,742,1987,2011]" class="Insecta" family="Chrysomelidae" genus="Oides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="wangi" status="sp. nov.">
