<quantity id="4CE4EAD84733FFA1E017FB74A4459BDF" box="[514,587,1185,1209]" metricMagnitude="-2" metricUnit="m" metricValue="1.6" pageId="39" pageNumber="869" unit="mm" value="16.0">16 mm</quantity>
