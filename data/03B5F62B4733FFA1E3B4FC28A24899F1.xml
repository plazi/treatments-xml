<quantity id="4CE4EAD84733FFA1E36FFB14A7A09BBF" box="[378,430,1217,1241]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="39" pageNumber="869" unit="m" value="20.0">20 m</quantity>
