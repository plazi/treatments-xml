<quantity id="4CE4EAD84733FFA1E299FB34A6DA9B9F" box="[140,212,1249,1273]" metricMagnitude="-2" metricUnit="m" metricValue="2.5" pageId="39" pageNumber="869" unit="mm" value="25.0">25 mm</quantity>
