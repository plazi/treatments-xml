<taxonomicName id="F831EAFF36E18B51471C80DA616BF4C1" authority="Chevrolat, 1836" authorityName="Chevrolat" authorityYear="1836" class="Insecta" family="Chrysomelidae" genus="Atrachya" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Atrachya" order="Coleoptera" pageId="0" pageNumber="117" phylum="Arthropoda" rank="genus">
