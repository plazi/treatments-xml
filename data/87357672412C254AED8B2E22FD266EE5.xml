<taxonomicName id="C89CBCE7412C254AEC2E2E95FDC76FCD" authority="Gray, 1832" authorityName="Gray" authorityYear="1832" box="[306,544,1152,1175]" class="Reptilia" family="Diplodactylidae" genus="Diplodactylus" kingdom="Animalia" order="Squamata" pageId="3" pageNumber="36" phylum="Chordata" rank="species" species="vittatus">
