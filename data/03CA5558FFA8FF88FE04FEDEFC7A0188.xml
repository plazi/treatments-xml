<paragraph id="8BDCE44EFFAAFF88FF41FEECFC7A0188" blockId="11.[213,1169,320,409]" pageId="11" pageNumber="2933">
<taxonomicName id="4C639FCDFFAAFF88FF41FEECFE680148" box="[213,414,321,345]" class="Insecta" family="Aleyrodidae" genus="Eucarsia" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="2933" phylum="Arthropoda" rank="species" species="lineolata">
