<taxonomicName id="59CBAFA32320475AFB2BF809FF5EB685" authority="(Moschler, 1877)" authorityName="Moschler" authorityYear="1877" baseAuthorityName="Moschler" baseAuthorityYear="1877" class="Insecta" family="Hesperiidae" genus="Enosis" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="137" phylum="Arthropoda" rank="species" species="angularis">
