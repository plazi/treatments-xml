<taxonomicName id="77C50C83DCF1A5B2FED58CC2FD52FB47" authority="Breitfuss, 1896" authorityName="Breitfuss" authorityYear="1896" box="[293,668,1224,1246]" class="Calcarea" family="Amphoriscidae" genus="Amphoriscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leucosolenida" pageId="131" pageNumber="132" phylum="Porifera" rank="species" species="semoni">
<emphasis id="82B1AB12DCF1A5B2FED58CC2FE36FB47" box="[293,504,1224,1246]" italics="true" pageId="131" pageNumber="132">Amphoriscus semoni</emphasis>
