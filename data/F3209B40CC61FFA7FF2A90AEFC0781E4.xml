<subSubSection id="339379DDCC61FFA7FF7A9584FC0781E4" box="[199,994,1950,1975]" pageId="6" pageNumber="51" type="etymology">
<paragraph id="7B362A56CC61FFA7FF7A9584FC0781E4" blockId="6.[151,1437,798,1975]" box="[199,994,1950,1975]" pageId="6" pageNumber="51">
<emphasis id="49FDF644CC61FFA7FF7A9584FEA381E4" bold="true" box="[199,326,1950,1975]" pageId="6" pageNumber="51">Etymology</emphasis>
