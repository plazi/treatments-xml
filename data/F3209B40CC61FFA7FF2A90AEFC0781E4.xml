<emphasis id="49FDF644CC61FFA7FD929584FD5681E5" box="[559,691,1950,1974]" italics="true" pageId="6" pageNumber="51">
<taxonomicName id="BC8951D5CC61FFA7FD929584FD4A81E5" box="[559,687,1950,1974]" class="Actinopterygii" family="Aphyonidae" genus="Nybelinella" kingdom="Animalia" order="Ophidiiformes" pageId="6" pageNumber="51" phylum="Chordata" rank="species" species="brevianalis">brevianalis</taxonomicName>
