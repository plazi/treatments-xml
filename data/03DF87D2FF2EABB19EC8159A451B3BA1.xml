<collectionCode id="ED67AE01FF28ABB19FAE128544603A99" box="[497,573,672,698]" country="Canada" httpUri="http://grbio.org/cool/rzcb-tdp2" name="Canadian National Collection Insects" pageId="196" pageNumber="197">CNCI</collectionCode>
