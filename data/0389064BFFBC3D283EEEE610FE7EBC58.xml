sp. cf. 
<taxonomicName id="4C20CCDEFFBC3D283D75E440FC47BC05" box="[780,906,1796,1821]" class="Nodosariata" family="Polymorphinidae" genus="Guttulina" higherTaxonomySource="CoL" kingdom="Chromista" order="Lagenida" pageId="70" pageNumber="71" phylum="Foraminifera" rank="species" species="bartschi">
<emphasis id="B9546B4FFFBC3D283D75E440FCD0BC04" box="[780,797,1797,1820]" italics="true" pageId="70" pageNumber="71">G</emphasis>
