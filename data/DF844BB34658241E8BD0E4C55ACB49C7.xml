<taxonomicName id="06325383784D16E391987DAEA24EFDC0" class="Magnoliopsida" family="Fagaceae" genus="Quercus" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Plantae" order="Fagales" pageId="25" pageNumber="424" phylum="Tracheophyta" rank="species" species="libanotis">Q. libanotis</taxonomicName>
