<materialsCitation id="3B5D9564FFCCD561FF25F9BEFCDDFEEA" collectorName="M. Pavageau. One &amp; C. Callou" country="Colombia" lastPageId="29" lastPageNumber="30" location="Geoffroy St." pageId="28" pageNumber="29" specimenCount="1" typeStatus="syntype">
