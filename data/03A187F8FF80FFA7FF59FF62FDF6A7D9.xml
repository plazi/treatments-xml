<number id="3DC024C9FF80FFA7FF34F97FFE81A689" box="[250,284,1674,1700]" pageId="2" pageNumber="16" value="12.0">12</number>
’ E
</geoCoordinate>
), 
<date id="FFB6102EFF80FFA7FE9DF97FFE69A689" box="[339,500,1674,1700]" pageId="2" pageNumber="16" value="2006-07-17">
<number id="3DC024C9FF80FFA7FE9DF97FFEEEA689" box="[339,371,1674,1700]" pageId="2" pageNumber="16" value="17.0">17</number>
. VII. 
<number id="3DC024C9FF80FFA7FE78F97FFE69A689" box="[438,500,1674,1700]" pageId="2" pageNumber="16" value="2006.0">2006</number>
