<taxonomicName id="03E44D172B49FF8C3E8E2A15CA5E8268" authority="Pall." authorityName="Pall." box="[1180,1338,1804,1831]" class="Magnoliopsida" family="Amaranthaceae" genus="Salsola" kingdom="Plantae" order="Caryophyllales" pageId="50" pageNumber="51" phylum="Tracheophyta" rank="species" species="rigida">
