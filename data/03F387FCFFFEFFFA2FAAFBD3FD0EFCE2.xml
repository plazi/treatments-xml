(
<taxonomicName id="4C5A4D69FFFFFFFA2C16FA3FF886FA22" box="[358,468,1473,1496]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="43" pageNumber="44" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
