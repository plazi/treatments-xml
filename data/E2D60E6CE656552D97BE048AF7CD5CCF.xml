<emphasis id="3D8CC6F2FB85F25418594B726714952A" italics="true" pageId="4" pageNumber="103">V. rzedowskiana</emphasis>
</taxonomicName>
, 
<taxonomicName id="7CD632D27CE49554483D0885136C6A82" genus="V." lsidName="V. secundifolia" pageId="4" pageNumber="103" rank="species" species="secundifolia">
<emphasis id="279441D0CC5C3504EF80AAE7D1446072" italics="true" pageId="4" pageNumber="103">V. secundifolia</emphasis>
</taxonomicName>
and 
<taxonomicName id="1E22B99E332481314AB434CBBFC0DD3F" genus="V." lsidName="V. teloloapanensis" pageId="4" pageNumber="103" rank="species" species="teloloapanensis">
<emphasis id="780FFB9667048BA186D18F04AE9EB8F1" italics="true" pageId="4" pageNumber="103">V. teloloapanensis</emphasis>
