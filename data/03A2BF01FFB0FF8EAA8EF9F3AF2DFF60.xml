<quantity id="4CF3A3F2FFB0FF8DA87CF922ACA9F945" box="[613,717,1732,1758]" metricMagnitude="-3" metricUnit="m" metricValue="2.02" pageId="5" pageNumber="545" unit="mm" value="2.02">2.02 mm</quantity>
