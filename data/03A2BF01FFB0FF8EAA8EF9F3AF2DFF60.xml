: 
<quantity id="4CF3A3F2FFB0FF8DAB03F925AFDAF945" box="[282,446,1731,1758]" metricMagnitude="-3" metricUnit="m" metricValue="2.025" metricValueMax="2.15" metricValueMin="1.9" pageId="5" pageNumber="545" unit="mm" value="2.025" valueMax="2.15" valueMin="1.9">1.90–2.15 mm</quantity>
