<collectingRegion id="2A5EF82C9059FF91E151A5D414A1FDA2" box="[359,424,510,535]" country="Maldives" name="Dhekunu" pageId="14" pageNumber="589">South</collectingRegion>
<collectingCountry id="908D765E9059FF91E199A5D414FBFDA2" box="[431,498,510,535]" name="China" pageId="14" pageNumber="589">China</collectingCountry>
Sea, 
<collectingCountry id="908D765E9059FF91E206A5D417A7FDA2" box="[560,686,510,535]" name="Philippines" pageId="14" pageNumber="589">Philippines</collectingCountry>
and 
<collectingCountry id="908D765E9059FF91E2D2A5D4165DFDA2" box="[740,852,510,535]" name="Indonesia" pageId="14" pageNumber="589">Indonesia</collectingCountry>
; at depths of 
<quantity id="2F629B2B9059FF91E3DFA5D4117EFDA3" box="[1001,1143,510,535]" metricMagnitude="2" metricUnit="m" metricValue="6.75" metricValueMax="12.5" metricValueMin="1.0" pageId="14" pageNumber="589" unit="m" value="675.0" valueMax="1250.0" valueMin="100.0">100–1250 m</quantity>
(
<bibRefCitation id="8C0B4B3F9059FF91E4B3A5D411F9FDA2" author="Li" box="[1157,1264,510,535]" pageId="14" pageNumber="589" refString="Li, X. (2006 a) Additional Pandalid shrimps from the South China Sea (Crustacea: Decapoda: Caridea), with description of a new species. Raffles Bulletin of Zoology, 54, 361 - 372." type="journal article" year="2006" yearSuffix="a">Li 2006a</bibRefCitation>
).
</paragraph>
