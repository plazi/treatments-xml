<taxonomicName id="4C534D7FFFF3FF985AE10167FBC37212" authority="Schedl, 1957" authorityName="Schedl" authorityYear="1957" box="[1022,1120,1855,1881]" class="Insecta" family="Curculionidae" genus="Ozopemon" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="242" phylum="Arthropoda" rank="species" species="laevis">
