<taxonomicName id="4C2B4D3B0766FFBFFF19FA76FB32F9F2" authority=", Peters, P. Z. S. 1866, p. 426" authorityName="Peters, P. Z. S." authorityPageNumber="426" authorityYear="1866" box="[246,1182,1534,1570]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="4" pageNumber="104" phylum="Chordata" rank="species" species="coelophyllus">
