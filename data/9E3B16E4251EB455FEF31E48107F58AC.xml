<paragraph id="F7AE3F93D5CDFF27AF46860F00BED2C0" pageNumber="688">
Etymology L 
<taxonomicName id="07767D6B654B109D76225AEAAA1B5861" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33734" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole fabricator (Smith)" lsidName-HNS="Pheidole fabricator (Smith)" order="Hymenoptera" pageNumber="688" phylum="Arthropoda" rank="species" species="fabricator">fabricator</taxonomicName>
