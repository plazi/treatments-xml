<paragraph id="E7300193BC36044F171144718C11D325" pageId="12" pageNumber="99">
<taxonomicName id="FA4356710FB383E1FA3D315C0B5E5397" authority="Sasaji, 1967" authorityName="Sasaji" authorityYear="1967" class="Insecta" family="Coccinellidae" genus="Shirozuella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Shirozuella parenthesis" order="Coleoptera" pageId="12" pageNumber="99" phylum="Arthropoda" rank="species" species="parenthesis">Shirozuella parenthesis</taxonomicName>
