x 
<number id="5D57F84FB87BB572FD768E6E679D2E10" box="[695,732,1887,1911]" pageId="7" value="1.1">1.1</number>
; (
<figureCitation id="73A4F6EDB87BB572FD308E6E66742E10" box="[753,821,1887,1911]" captionStart="FIGURE 4" captionStartId="9.[151,249,1855,1878]" captionTargetBox="[163,1429,206,1810]" captionTargetId="figure@9.[151,1436,193,1834]" captionTargetPageId="9" captionText="FIGURE 4. Phrudoneura abbreviata Meuffels &amp; Grootaert; a. male habitus, left lateral. P. collessi sp. nov.; b. male habitus, left lateral. P. popondetta sp. nov.; c. male habitus, right lateral; d. male antenna, right lateral; e. female habitus, right lateral; f. female antenna, right lateral." httpUri="https://zenodo.org/record/218312/files/figure.png" pageId="7">
