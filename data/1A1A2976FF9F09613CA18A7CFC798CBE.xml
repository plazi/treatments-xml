<taxonomicName id="55B3E3E3FF9F09613ED58ABFFC7D8CBE" authority="Signoret, 1861" authorityName="Signoret" authorityYear="1861" box="[678,929,1673,1700]" class="Insecta" family="Issidae" genus="Dracela" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="249" phylum="Arthropoda" rank="genus">
