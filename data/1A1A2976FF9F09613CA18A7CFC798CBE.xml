<taxonomicName id="55B3E3E3FF9F09613F6F8D0AFCB38B4E" authorityName="Fennah" authorityYear="1947" box="[796,879,316,340]" class="Insecta" family="Issidae" genus="Oronoqua" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="249" phylum="Arthropoda" rank="species" species="deina">
