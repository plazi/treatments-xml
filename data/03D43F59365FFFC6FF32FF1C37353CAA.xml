<quantity id="4C8523AA365FFFC6FEE4F98A348F3D5A" box="[321,459,1551,1577]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" metricValueMax="2.6" metricValueMin="2.2" pageId="50" pageNumber="51" unit="mm" value="2.4" valueMax="2.6" valueMin="2.2">2.2–2.6 mm</quantity>
