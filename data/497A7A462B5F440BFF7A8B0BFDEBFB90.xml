<date id="B56DED902B5F440BFD188B9DFCC1FD08" box="[720,844,742,764]" pageId="19" pageNumber="178" value="1995-04-20" valueMax="1995-04-26" valueMin="1995-04-20">20-26.iv.1995</date>
