<taxonomicName id="06D3B0D32B5F440BFCD78BD4FC1AFD30" authorityName="HUEMER &amp; KARSHOLT" authorityYear="2001" box="[799,919,686,708]" class="Insecta" family="Gelechiidae" genus="Mirificarma" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="178" phylum="Arthropoda" rank="species" species="minimella">M. minimella</taxonomicName>
