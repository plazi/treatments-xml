taxon was described from 
<location id="785D603B5E126165FDC2DEF0FD351DF9" LSID="urn:lsid:plazi:treatment:F52B87F65E126165FF74DE0EFE811CD8:785D603B5E126165FDC2DEF0FD351DF9" box="[523,676,1201,1223]" country="China" county="East Palaearctic" municipality="Investigated" name="Eastern Tomb" pageId="36" pageNumber="169">Eastern Tomb</location>
/ 
<collectingCountry id="059576705E126165FD72DEF0FC911DF9" box="[699,768,1201,1223]" name="China" pageId="36" pageNumber="169">China</collectingCountry>
. 
<collectingMunicipality id="9D59AC9A5E126165FF5EDE90FEB01DD9" box="[151,289,1233,1255]" pageId="36" pageNumber="169">Investigated</collectingMunicipality>
were 
<specimenCount id="6B84FD695E126165FEAEDE90FE4D1DD9" box="[359,476,1233,1255]" pageId="36" pageNumber="169" type="worker">six worker</specimenCount>
<typeStatus id="A23988425E126165FE2CDE90FDD81DD8" box="[485,585,1233,1254]" pageId="36" pageNumber="169">syntypes</typeStatus>
