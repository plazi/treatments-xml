<collectionCode id="1B93AE255E126165FD59DE90FD571DD8" box="[656,710,1233,1254]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="36" pageNumber="169" type="Museum">MCZ</collectionCode>
