<geoCoordinate id="EE235022FF99FFABFB23FC12FACE9A8C" box="[1268,1377,946,972]" direction="west" orientation="longitude" pageId="14" pageNumber="15" precision="555" value="-69.87">
<number id="3DDF24C2FF99FFABFB23FC12FA969A8C" box="[1268,1337,946,972]" pageId="14" pageNumber="15" value="69.87">69.87</number>
°W
</geoCoordinate>
] 
<number id="3DDF24C2FF99FFABFAADFC12FA269A8C" box="[1402,1417,946,972]" pageId="14" pageNumber="15" value="1.0">1</number>
Ψ 
<date id="FFA91025FF99FFABFF40FC7AFE889AB4" box="[151,295,986,1012]" pageId="14" pageNumber="15" value="1986-08-29">
<number id="3DDF24C2FF99FFABFF40FC7AFF199AB4" box="[151,182,986,1012]" pageId="14" pageNumber="15" value="29.0">29</number>
.viii. 
<number id="3DDF24C2FF99FFABFF3FFC7AFE889AB4" box="[232,295,986,1012]" pageId="14" pageNumber="15" value="1986.0">1986</number>
</date>
, mixed pine &amp; hardwood litter, R.E. Nelson (
<collectionCode id="ED06AE20FF99FFABFCECFC7AFCD29AB4" box="[827,893,986,1012]" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="14" pageNumber="15">CNC</collectionCode>
).
