<taxonomicName id="9C8B4D30FFD6FFA26AAD6F87FD5413DC" authorityName="Boulard" authorityYear="1968" box="[697,762,1206,1232]" class="Insecta" family="Membracidae" genus="Hamma" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="100" phylum="Arthropoda" rank="species" species="heimi">
<emphasis id="69FFEAA1FFD6FFA26AAD6F87FD5413DC" box="[697,762,1206,1232]" italics="true" pageId="11" pageNumber="100">heimi</emphasis>
</taxonomicName>
and 
<taxonomicName id="9C8B4D30FFD6FFA26B206F87FC2713C3" authority="Durante &amp; Loudit &amp; Susini, 2021" authorityName="Durante &amp; Loudit &amp; Susini" authorityYear="2021" box="[820,905,1206,1231]" class="Insecta" family="Membracidae" genus="Hamma" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="100" phylum="Arthropoda" rank="species" species="nigrum" status="sp. nov.">
<emphasis id="69FFEAA1FFD6FFA26B206F87FC2713C3" box="[820,905,1206,1231]" italics="true" pageId="11" pageNumber="100">nigrum</emphasis>
</taxonomicName>
<taxonomicNameLabel id="72CC57DAFFD6FFA26B846F87FC4413DC" box="[912,1002,1206,1232]" pageId="11" pageNumber="100" rank="species">sp. nov.</taxonomicNameLabel>
