<elevation id="004DD1A1A26FFFF4FD86F8C4FE9FF8F0" box="[151,349,1826,1851]" metricMagnitude="3" metricUnit="m" metricValue="1.7205" metricValueMax="1.841" metricValueMin="1.6" pageId="19" pageNumber="320" unit="m" value="1720.5" valueMax="1841.0" valueMin="1600.0">
