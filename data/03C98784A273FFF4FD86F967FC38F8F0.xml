<collectingCountry id="F3777602A26FFFF4FFBBF8C4FD3DF8F0" box="[682,767,1826,1851]" name="Mexico" pageId="19" pageNumber="320">Mexico</collectingCountry>
and 
<collectingRegion id="49A4F870A26FFFF4FE27F8C5FC48F8F0" box="[822,906,1827,1851]" country="Mexico" name="Oaxaca" pageId="19" pageNumber="320">Oaxaca</collectingRegion>
(
