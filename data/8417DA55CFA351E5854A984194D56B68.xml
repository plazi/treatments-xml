<taxonomicName id="A64A103AF22E8266D830C82361F9CD7F" class="Insecta" genus="P." kingdom="Animalia" lsidName="P. mnemosyne" order="Coleoptera" pageId="0" pageNumber="193" phylum="Arthropoda" rank="species" species="mnemosyne">
