<emphasis id="B94FEA9F732AAB784385F985FE85F9F5" bold="true" box="[199,273,1621,1646]" pageId="95" pageNumber="96">Notes.</emphasis>
<taxonomicName id="4C3B4D0E732AAB78425AF985FDB0F9F5" authorityName="Robertson" authorityYear="1897" box="[280,548,1621,1646]" class="Insecta" family="Halictidae" genus="Sphecodes" kingdom="Animalia" order="Hymenoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="pycnanthemi">
