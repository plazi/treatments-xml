<paragraph id="F88097C1CDCDBCB41440A12473DA7A91" pageId="0" pageNumber="25">
<taxonomicName id="369F70EE3A82AE1BF20B10016DDA3CA4" authorityName="Pereira, Lehmann &amp; Reis" authorityYear="2016" class="Actinopterygii" family="Loricariidae" genus="Pareiorhaphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pareiorhaphis vetula" order="Siluriformes" pageId="0" pageNumber="25" phylum="Chordata" rank="species" species="vetula">
