<taxonomicName id="4C4D4D06A1470474BA7D637EFC096CE9" authority="Matschie, 1894" authorityName="Matschie" authorityYear="1894" box="[299,927,880,906]" class="Mammalia" family="Chrysochloridae" genus="Chrysochloris" kingdom="Animalia" order="Afrosoricida" pageId="6" pageNumber="7" phylum="Chordata" rank="subSpecies" species="stuhlmanni" subSpecies="stuhlmanni">
