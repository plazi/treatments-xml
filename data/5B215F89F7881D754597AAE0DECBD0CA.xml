Galium 
<normalizedToken id="6245F07A6E6A886A16131F25643EC465" originalValue="aristátum" pageId="null" pageNumber="304">aristatum</normalizedToken>
<authorityName id="D646D560E4A74D96238559C6FB07420D" pageId="null" pageNumber="304">L.</authorityName>
