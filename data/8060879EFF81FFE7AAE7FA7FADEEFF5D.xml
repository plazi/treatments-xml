<taxonomicName id="CFC94D0BFF81FFE4AF7BF8E6AD12F8FD" box="[1291,1376,1876,1898]" class="Insecta" family="Psychodidae" genus="Hoffeinsodes" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="109" phylum="Arthropoda" rank="species" species="bifida" status="sp. n.">
