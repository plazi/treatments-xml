<subSubSection id="B7608A1DEC915CFE30FBD089319EF1BD" pageId="0" pageNumber="73" type="type species">
<paragraph id="034AD98F827537D21429F69B202CD5EC" pageId="0" pageNumber="73">Type species.</paragraph>
<paragraph id="DB5520FFF6AADF7385BF906CD3F2854F" pageId="0" pageNumber="73">
<taxonomicName id="F61C090C1BDA601C0AAD50EEBC43CCA6" class="Insecta" family="Mantidae" genus="Hondurantemna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hondurantemna chespiritoi" order="Mantodea" pageId="0" pageNumber="73" phylum="Arthropoda" rank="species" species="chespiritoi">Hondurantemna chespiritoi</taxonomicName>
