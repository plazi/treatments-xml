<taxonomicName id="96A76BC3FFE0FFE7AA0EFB517BC860BB" baseAuthorityName="ZETTERSTEDT" baseAuthorityYear="1838" box="[871,969,1161,1182]" class="Insecta" family="Halictidae" genus="Lasioglossum" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="570" phylum="Arthropoda" rank="species" species="rufitarse">
