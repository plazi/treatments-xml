<taxonomicName id="4C4C4D583C7DFF8A0317F8E5BFCF5479" authorityName="Gray" authorityYear="1867" box="[285,465,1876,1901]" class="Demospongiae" family="Halichondriidae" kingdom="Animalia" order="Suberitida" pageId="3" pageNumber="295" phylum="Porifera" rank="family">Halichondriidae</taxonomicName>
