<emphasis id="B93BEACC4110AD55FC9837ECB2F5A3CE" box="[863,1004,1220,1241]" italics="true" pageId="71" pageNumber="668">E. crispatoria</emphasis>
</taxonomicName>
(L.), 
<taxonomicName id="4C4F4D5D4110AD55FBE037ECB031A3E0" baseAuthorityName="Fabricius" baseAuthorityYear="1793" class="Insecta" family="Ichneumonidae" genus="Eutanyacra" kingdom="Animalia" order="Hymenoptera" pageId="71" pageNumber="668" phylum="Arthropoda" rank="species" species="glaucatoria">
<emphasis id="B93BEACC4110AD55FBE037ECB031A3E0" italics="true" pageId="71" pageNumber="668">E. glaucatoria</emphasis>
</taxonomicName>
(F.) and 
<taxonomicName id="4C4F4D5D4110AD55FE4237CAB356A3E1" authority="(SCHRANK)" baseAuthorityName="SCHRANK" box="[389,591,1249,1271]" class="Insecta" family="Ichneumonidae" genus="Eutanyacra" kingdom="Animalia" order="Hymenoptera" pageId="71" pageNumber="668" phylum="Arthropoda" rank="species" species="picta">
