<taxonomicName id="E10C73D71AFB7DC71A69C36597D3537F" authorityName="Chevrolat" authorityYear="1839" class="Insecta" family="Carabidae" genus="Ega" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="783" pageNumber="784" phylum="Arthropoda" rank="species" species="sallei">
