<taxonomicName id="4FC4E690387BAE03FD806E99FC25FD79" authority="(Skilton)" baseAuthorityName="Skilton" baseAuthorityYear="1849" box="[628,962,673,699]" class="Amphibia" family="Salamandridae" genus="Taricha" kingdom="Animalia" order="Caudata" pageId="16" pageNumber="17" phylum="Chordata" rank="species" species="granulosa">
