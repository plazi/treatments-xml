<paragraph id="8B888D405A2EFF819FF8011AE69F0AF3" blockId="3.[189,1398,1365,1462]" pageId="3" pageNumber="4">
<taxonomicName id="4C37F6C35A2EFF819FF8011AE4C90A2A" authority="Groeninckx &amp; Janssens &amp; Smets &amp; Verstraete, 2017" authorityName="Groeninckx &amp; Janssens &amp; Smets &amp; Verstraete" authorityYear="2017" box="[189,446,1365,1391]" class="Magnoliopsida" family="Rubiaceae" genus="Astiella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Gentianales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="antongilensis" status="sp. nov.">
