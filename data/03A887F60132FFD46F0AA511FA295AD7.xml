<subSubSection id="C31B656B013DFFD56F3AA313FD555E66" pageId="8" pageNumber="9" type="materials_examined">
<paragraph id="8BBE36E0013DFFD56F3AA313FBC25C86" blockId="8.[264,1323,1357,1903]" pageId="8" pageNumber="9">
<emphasis id="B975EAF2013DFFD56F3AA313F9DD5C6E" box="[312,419,1358,1383]" italics="true" pageId="8" pageNumber="9">
<typeStatus id="54BA8842013DFFD56F3AA313F9DD5C6E" box="[312,419,1358,1383]" pageId="8" pageNumber="9">Holotype</typeStatus>
