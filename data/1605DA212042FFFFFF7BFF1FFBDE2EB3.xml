</emphasis>
<taxonomicName id="59AC10B42042FFFFFED5FF57FCD02C8F" authority="Song and Chai, 1990" authorityName="Song and Chai" authorityYear="1990" box="[313,846,222,246]" class="Arachnida" family="Thomisidae" genus="Misumenops" kingdom="Animalia" order="Araneae" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="xiushanensis">
