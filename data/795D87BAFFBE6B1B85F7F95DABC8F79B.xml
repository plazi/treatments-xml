<taxonomicName id="36F44D2FFFBD6B1B87EBFAD9ABC8FADB" authority="Zidek &amp; Pokorny, 2010" authorityName="Zidek &amp; Pokorny" authorityYear="2010" box="[411,759,1347,1371]" class="Insecta" family="Scarabaeidae" genus="Synapsis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="417" phylum="Arthropoda" rank="species" species="horaki">
