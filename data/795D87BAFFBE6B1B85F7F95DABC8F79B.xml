). .......................................... 
<emphasis id="C380EABEFFBE6B1882EBF79BACA2F79A" bold="true" box="[1179,1437,2049,2074]" pageId="9" pageNumber="416">
<taxonomicName id="36F44D2FFFBE6B1882EBF79BAC7CF799" authority="Bui &amp; Bonkowski, 2018" authorityName="Bui &amp; Bonkowski" authorityYear="2018" box="[1179,1347,2049,2073]" class="Insecta" family="Scarabaeidae" genus="Synapsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="416" phylum="Arthropoda" rank="species" species="puluongensis" status="sp. nov.">
