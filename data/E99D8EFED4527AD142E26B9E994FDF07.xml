<taxonomicName id="A4D6558257900762E69E5BF5F499E249" class="Insecta" family="Carabidae" genus="Geocharidius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Geocharidius zullinii" order="Coleoptera" pageId="12" pageNumber="73" phylum="Arthropoda" rank="species" species="zullinii">Geocharidius zullinii</taxonomicName>
