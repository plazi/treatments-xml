<location id="8EE4CFE7FFE8FFEAAD1A583A1A80F891" LSID="urn:lsid:plazi:treatment:0392282AFFE8FFEBAEC15A0D1AD7FECA:8EE4CFE7FFE8FFEAAD1A583A1A80F891" box="[844,945,1794,1819]" country="United States of America" name="Beaufort" pageId="14" pageNumber="15" stateProvince="North Carolina">Beaufort</location>
, depth 
<quantity id="4CC334D9FFE8FFEAAA57583A1D57F890" box="[1025,1126,1794,1819]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="2.6" metricValueMin="2.4" pageId="14" pageNumber="15" unit="m" value="25.0" valueMax="26.0" valueMin="24.0">
<elevation id="00167E0FFFE8FFEAAA57583A1D57F890" box="[1025,1126,1794,1819]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="2.6" metricValueMin="2.4" pageId="14" pageNumber="15" unit="m" value="25.0" valueMax="26.0" valueMin="24.0">24–26 m</elevation>
</quantity>
, on a crab (
<bibRefCitation id="EFAAE4CDFFE8FFEAAAB1583B1C4EF891" box="[1255,1407,1795,1819]" pageId="14" pageNumber="15" refId="ref74719" refString="Fraser, C. M. (1912 c) Some hydroids of Beaufort, North Carolina. Bulletin of the United States Bureau of Fisheries, 30, 337 - 387. [25 July 1912: noted in Contents of the bulletin]" type="journal article">Fraser 1912c</bibRefCitation>
).
</materialsCitation>
