<taxonomicName id="4C744D0CFF933B2CFBF6FA1CFB38894D" authorityName="Khatri &amp; Webb" authorityYear="2010" box="[1049,1197,1523,1549]" class="Insecta" family="Cicadellidae" genus="Stirellus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="191" phylum="Arthropoda" rank="species" species="lahorensis">
