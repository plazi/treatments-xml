<collectingCountry id="F34A76659D6BFFDCFD17FBC23239FB95" box="[701,770,1068,1092]" name="Brazil" pageId="23" pageNumber="674">Brazil</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFF02FBA0302FFBB7" box="[168,276,1102,1126]" name="Colombia" pageId="23" pageNumber="674">Colombia</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE8BFBA030A5FBB7" box="[289,414,1102,1126]" name="Costa Rica" pageId="23" pageNumber="674">Costa Rica</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE01FBA03332FBB7" box="[427,521,1102,1126]" name="Curaçao" pageId="23" pageNumber="674">Curaçao</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFDBDFBA03348FBB7" box="[535,627,1102,1126]" name="Ecuador" pageId="23" pageNumber="674">Ecuador</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD2AFBA03238FBB7" box="[640,771,1102,1126]" name="El Salvador" pageId="23" pageNumber="674">El Salvador</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFF02FB81301AFB56" box="[168,289,1135,1159]" name="Guatemala" pageId="23" pageNumber="674">Guatemala</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE86FB8130B8FB56" box="[300,387,1135,1159]" name="Guyana" pageId="23" pageNumber="674">Guyana</collectingCountry>
, French Guiana, 
<collectingCountry id="F34A76659D6BFFDCFD95FB813390FB56" box="[575,683,1135,1159]" name="Honduras" pageId="23" pageNumber="674">Honduras</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFD1CFB8131E8FB78" name="Nicaragua" pageId="23" pageNumber="674">Nicaragua</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFF77FB7F300DFB79" box="[221,310,1169,1192]" name="Panama" pageId="23" pageNumber="674">Panama</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFEEBFB7F304DFB79" box="[321,374,1169,1192]" name="Peru" pageId="23" pageNumber="674">Peru</collectingCountry>
, 
<collectingCountry id="F34A76659D6BFFDCFE2AFB7E30D7FB79" box="[384,492,1168,1192]" name="Suriname" pageId="23" pageNumber="674">Suriname</collectingCountry>
