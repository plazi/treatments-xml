<taxonomicName id="D5894D54FFEB95403919FB5C54E5FB69" authority="Dupérré &amp; Tapia, 2020" authorityName="Dupérré &amp; Tapia" authorityYear="2020" box="[545,691,1156,1182]" class="Arachnida" family="Mysmenidae" genus="Mysmenopsis" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="salazarae" status="n. sp.">
<emphasis id="20FDEAC5FFEB95403919FB5C54E5FB69" box="[545,691,1156,1182]" italics="true" pageId="63" pageNumber="64">M. salazarae</emphasis>
</taxonomicName>
<emphasis id="20FDEAC5FFEB95403981FB5D54A1FB69" bold="true" box="[697,759,1156,1182]" pageId="63" pageNumber="64">
<taxonomicNameLabel id="3BCE57BEFFEB95403981FB5D54A1FB69" box="[697,759,1156,1182]" pageId="63" pageNumber="64" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="D5894D54FFEB95403814FB5C5597FB69" authority="Dupérré &amp; Tapia, 2020" authorityName="Dupérré &amp; Tapia" authorityYear="2020" box="[812,961,1156,1182]" class="Arachnida" family="Mysmenidae" genus="Mysmenopsis" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="bartolozzi" status="n. sp.">
