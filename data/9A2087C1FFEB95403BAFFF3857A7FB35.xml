with 
<taxonomicName id="D5894D54FFEB95403E01FB5C53CAFB69" authority="Dupérré &amp; Tapia, 2020" authorityName="Dupérré &amp; Tapia" authorityYear="2020" box="[1337,1436,1157,1182]" class="Arachnida" family="Mysmenidae" genus="Mysmenopsis" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="penai" status="n. sp.">
