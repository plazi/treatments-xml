</taxonomicName>
<emphasis id="20FDEAC5FFEB95403BAFFB715680FB35" bold="true" box="[151,214,1192,1218]" pageId="63" pageNumber="64">
<taxonomicNameLabel id="3BCE57BEFFEB95403BAFFB715680FB35" box="[151,214,1192,1218]" pageId="63" pageNumber="64" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="D5894D54FFEB95403A35FB7057FDFB35" authority="Dupérré &amp; Tapia, 2020" authorityName="Dupérré &amp; Tapia" authorityYear="2020" box="[269,427,1193,1218]" class="Arachnida" family="Mysmenidae" genus="Mysmenopsis" kingdom="Animalia" order="Araneae" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="amazonica" status="n. sp.">
