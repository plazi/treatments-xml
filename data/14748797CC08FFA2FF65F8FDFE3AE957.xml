<taxonomicName id="5BDD4D02CC09FFADFEA8FA2EFD76E8A4" authority="Yang, 1987" authorityName="Yang" authorityYear="1987" box="[346,686,1407,1429]" class="Insecta" family="Hemerobiidae" genus="Micromus" kingdom="Animalia" order="Neuroptera" pageId="7" pageNumber="134" phylum="Arthropoda" rank="species" species="pallidius">
