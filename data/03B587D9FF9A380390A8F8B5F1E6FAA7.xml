<quantity id="4CE49B2AFF85380393A6FBF5F097FBC7" box="[921,1010,1096,1122]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="5.0" metricValueMin="0.0" pageId="16" pageNumber="47" unit="m" value="25.0" valueMax="50.0" valueMin="0.0">0–50 m</quantity>
