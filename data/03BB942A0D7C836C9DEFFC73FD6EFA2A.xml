<taxonomicName id="4C125EBF0D7E836C9C43FB9FFDC5FBCC" authority="Stauffer, Bowers, Kellogg &amp; McKaye, 1997" authorityName="Stauffer, Bowers, Kellogg &amp; McKaye" authorityYear="1997" box="[315,526,1052,1074]" class="Teleostei" family="Cichlidae" genus="Metriaclima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="glaucos">
