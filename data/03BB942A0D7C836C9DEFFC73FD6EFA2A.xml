<caption id="DF6D75B40D7E836C9DEFFB9FFDF3FB8E" httpUri="https://zenodo.org/record/204918/files/figure.png" pageId="10" pageNumber="11" targetBox="[276,1311,195,1030]" targetPageId="10">
