<number id="616D248AFFAFFF9EFB9EFB4FF884F08C" box="[1136,1261,1183,1207]" pageId="7" pageNumber="40" value="6253.5" valueMax="6267.0" valueMin="6240.0">6240-6267</number>
; 
<collectionCode id="B1B4AE68FFAFFF9EFB12FB4FF95EF08C" box="[1276,1335,1183,1207]" httpUri="http://grbio.org/cool/23qr-v2gj" name="Instituto Biologico de Sao Paulo" pageId="7" pageNumber="40">IBSP</collectionCode>
<date id="A31B106DFFAFFF9EFAAFFB4FF9F1F08C" box="[1345,1432,1183,1207]" pageId="7" pageNumber="40" value="57-04-16">160457</date>
, 
<date id="A31B106DFFAFFF9EFF79FB14FC84F0E7" box="[151,237,1220,1244]" pageId="7" pageNumber="40" value="60-04-16">160460</date>
- 
<date id="A31B106DFFAFFF9EFF1AFB14FD23F0E7" box="[244,330,1220,1244]" pageId="7" pageNumber="40" value="62-04-16">160462</date>
, 
<date id="A31B106DFFAFFF9EFEB8FB14FDC4F0E7" box="[342,429,1220,1244]" pageId="7" pageNumber="40" value="75-04-16">160475</date>
, 
<date id="A31B106DFFAFFF9EFE56FB14FE79F0E7" box="[440,528,1220,1244]" pageId="7" pageNumber="40" value="76-04-16">
<number id="616D248AFFAFFF9EFE56FB14FE79F0E7" box="[440,528,1220,1244]" pageId="7" pageNumber="40" value="160476.0">160476</number>
