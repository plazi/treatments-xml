<heading id="D0A41579BA38FFFDF8EEF9B28A1AF9B4" bold="true" box="[151,544,1543,1569]" fontSize="11" level="1" pageId="12" pageNumber="513" reason="1">
<emphasis id="B9277E07BA38FFFDF8EEF9B28A1AF9B4" bold="true" box="[151,544,1543,1569]" pageId="12" pageNumber="513">
<taxonomicName id="4C53D996BA38FFFDF8EEF9B28980F9B4" box="[151,442,1543,1569]" class="Insecta" family="Hybotidae" genus="Oropezella" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="513" phylum="Arthropoda" rank="species" species="costaricensis" status="sp. nov.">
<emphasis id="B9277E07BA38FFFDF8EEF9B28980F9B4" bold="true" box="[151,442,1543,1569]" italics="true" pageId="12" pageNumber="513">Oropezella costaricensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A214C37CBA38FFFDF9BBF9B28A1AF9B4" box="[450,544,1543,1569]" pageId="12" pageNumber="513" rank="species">sp. nov.</taxonomicNameLabel>
