</taxonomicName>
, 
<taxonomicName id="4C1E4D19FFD9CD30FF4CEAC1B1AEEB82" box="[151,221,692,715]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" kingdom="Plantae" order="Fagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">
<emphasis id="B96AEA88FFD9CD30FF4CEAC1B1AEEB82" box="[151,221,692,715]" italics="true" pageId="14" pageNumber="15">Fagus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C1E4D19FFD9CD30FF30EAC6B020EB83" box="[235,339,691,714]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" kingdom="Plantae" order="Fagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">
