<number id="8171B367FFA0FFDFFB87FBB9FBD10FAE" box="[1098,1114,1126,1152]" pageId="8" pageNumber="9" value="5.0">5</number>
­ 
<number id="8171B367FFA0FFDFFBA9FBB9FBFF0FAE" box="[1124,1140,1126,1152]" pageId="8" pageNumber="9" value="5.0">5</number>
