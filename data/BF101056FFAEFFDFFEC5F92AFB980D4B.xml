­ 
<number id="8171B367FFA0FFDFFB33FBB9FA850FAE" box="[1278,1294,1126,1152]" pageId="8" pageNumber="9" value="2.0">2</number>
­ 
<number id="8171B367FFA0FFDFFADAFBB9FAAC0FAE" box="[1303,1319,1126,1152]" pageId="8" pageNumber="9" value="2.0">2</number>
