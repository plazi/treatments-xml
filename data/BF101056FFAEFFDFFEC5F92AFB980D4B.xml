<emphasis id="05CD7D52FFA0FFDFFD8BFE30FDD80926" box="[582,595,495,520]" italics="true" pageId="8" pageNumber="9">c</emphasis>
<number id="8171B367FFA0FFDFFD9EFE25FDD40921" box="[595,607,506,527]" pageId="8" pageNumber="9" value="2.0">2</number>
