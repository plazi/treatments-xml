<emphasis id="05CD7D52FFA0FFDFFB2CFE1DFB7B0AF5" box="[1249,1264,450,475]" italics="true" pageId="8" pageNumber="9">h</emphasis>
<number id="8171B367FFA0FFDFFB22FE12FB710ACE" box="[1263,1274,461,480]" pageId="8" pageNumber="9" value="1.0">1</number>
