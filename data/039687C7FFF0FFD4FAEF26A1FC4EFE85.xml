<taxonomicName id="4C3F4D52FFF1FFD3F9DE25F6FBB2F8B3" box="[934,1048,1789,1812]" class="Gymnolaemata" family="Candidae" genus="Amastigia" kingdom="Animalia" order="Cheilostomatida" pageId="35" pageNumber="36" phylum="Bryozoa" rank="species" species="avicularia">avicularia</taxonomicName>
