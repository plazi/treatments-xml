<taxonomicName id="8FEE4D41FF71FF5AFB5FFC2C6023C8E2" authorityName="Owen" authorityYear="1848" box="[1231,1376,907,933]" class="Mammalia" kingdom="Animalia" order="Artiodactyla" pageId="152" pageNumber="153" phylum="Chordata" rank="order">Artiodactyla</taxonomicName>
