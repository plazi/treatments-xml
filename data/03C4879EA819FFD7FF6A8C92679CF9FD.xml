<taxonomicName id="4C6D4D0BA819FFD7FB218DEA6702FA25" authorityName="Bitter" authorityYear="1919" class="Magnoliopsida" family="Solanaceae" genus="Lycianthes" kingdom="Plantae" order="Solanales" pageId="4" pageNumber="43" phylum="Tracheophyta" rank="variety" species="multiflora" variety="plicitomentosa">
