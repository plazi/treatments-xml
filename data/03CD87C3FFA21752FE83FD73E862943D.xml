urn:lsid:zoobank.org:act: 
<uuid id="FFC20C00FFA21750FDBDFD1AED729335" box="[633,1132,748,772]" pageId="9" pageNumber="1997">1886E0D3-2A50-45F4-AF96-8AE4D79A809F</uuid>
