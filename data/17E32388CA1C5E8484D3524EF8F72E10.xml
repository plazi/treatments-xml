<taxonomicName id="B51DFA7D57C27E03C2B243F7A5DDB4C8" class="Magnoliopsida" family="Euphorbiaceae" genus="Croton" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Croton hovarum" order="Malpighiales" pageId="64" pageNumber="65" phylum="Tracheophyta" rank="species" species="hovarum">
