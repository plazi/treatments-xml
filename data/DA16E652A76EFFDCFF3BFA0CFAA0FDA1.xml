<taxonomicName id="95BF2CC7A76FFFDFFE22F862FDF5F816" authorityName="Day" authorityYear="1963" box="[398,537,1985,2007]" class="Polychaeta" family="Syllidae" genus="Anguillosyllis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="17" pageNumber="214" phylum="Annelida" rank="genus">
