<taxonomicName id="4C347CC0161EFFF1B43AE58A695DCBB8" authorityName="Mayr" authorityYear="1862" box="[260,516,1709,1732]" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="324" phylum="Arthropoda" rank="species" species="acvapimensis">
