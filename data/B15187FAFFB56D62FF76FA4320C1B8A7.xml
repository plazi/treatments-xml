<subSubSection id="71E26567FFB56D62FCBBF97120C1B8A7" lastPageId="26" lastPageNumber="27" pageId="25" pageNumber="26" type="materials_examined">
<paragraph id="394736ECFFB56D62FCBBF97120C1B8A7" blockId="25.[837,1474,1771,1887]" lastBlockId="26.[113,751,183,452]" lastPageId="26" lastPageNumber="27" pageId="25" pageNumber="26">
<emphasis id="0B8CEAFEFFB56D61FCBBF9712585BE60" box="[837,1199,1771,1795]" italics="true" pageId="25" pageNumber="26">Additional Material Examined.</emphasis>
D.R. 
<collectingCountry id="41EF767CFFB56D61FAE2F97124A2BE60" box="[1308,1416,1771,1795]" name="Democratic Republic of the Congo" pageId="25" pageNumber="26">
<collectingRegion id="FB3CF80EFFB56D61FAE2F97124A2BE60" box="[1308,1416,1771,1795]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="25" pageNumber="26">CONGO</collectingRegion>
</collectingCountry>
: Bas 
<collectingCountry id="41EF767CFFB56D61FCBBF89022BEBE41" box="[837,916,1802,1826]" name="Democratic Republic of the Congo" pageId="25" pageNumber="26">
<collectingRegion id="FB3CF80EFFB56D61FCBBF89022BEBE41" box="[837,916,1802,1826]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="25" pageNumber="26">Congo</collectingRegion>
