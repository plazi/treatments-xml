<collectionCode id="B9F872F4FF99FFC0FF1BFD9B8A68FD99" box="[159,263,592,618]" country="Brazil" httpUri="http://grbio.org/cool/6yx0-3nm7" name="Museu de Zoologia da Universidade de Sao Paulo" pageId="44" pageNumber="45">MZUSP</collectionCode>
