<quantity id="0761EDE9FF90FFD0FC26F9C7FC11EBCF" box="[959,1015,1655,1676]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="67" pageNumber="238" unit="mm" value="1.0">1 mm</quantity>
