<number id="E04024F7FFD8FF8FFCBEFCB86B084571" box="[835,866,794,818]" pageId="19" pageNumber="520" value="32.0">32</number>
; (
<number id="E04024F7FFD8FF8FFC8AFCB86BEF4571" box="[887,901,794,818]" pageId="19" pageNumber="520" value="3.0">3</number>
) PV = 
<number id="E04024F7FFD8FF8FFC28FCB86B9E4571" box="[981,1012,794,818]" pageId="19" pageNumber="520" value="73.0">73</number>
; (
<number id="E04024F7FFD8FF8FFBF5FCB86C7D4571" box="[1032,1047,794,818]" pageId="19" pageNumber="520" value="4.0">4</number>
) dorsal scales non-striated without apical pits; (
<number id="E04024F7FFD8FF8FFED5FC9F695C4515" box="[296,310,829,854]" pageId="19" pageNumber="520" value="5.0">5</number>
