<number id="E04024F7FFD8FF8FFB24FC9C6C824515" box="[1241,1256,830,854]" pageId="19" pageNumber="520" value="7.0">7</number>
) anterior loreal single; (
<number id="E04024F7FFD8FF8FFF0FFCC0696A4539" box="[242,256,866,890]" pageId="19" pageNumber="520" value="8.0">8</number>
) three preoculars; (
<number id="E04024F7FFD8FF8FFE26FCC069804539" box="[475,490,866,890]" pageId="19" pageNumber="520" value="9.0">9</number>
) and 
<number id="E04024F7FFD8FF8FFDD7FCC06A2C4539" box="[554,582,866,890]" pageId="19" pageNumber="520" value="14.0">14</number>
Toe IV SDL.
