<taxonomicName id="64EF4D359E28FFE0FD287684FB9E14FC" authority="Lawrence, 1864: 107" authorityName="Lawrence" authorityPageNumber="107" authorityYear="1864" box="[667,1128,669,689]" class="Aves" family="Icteridae" genus="Cassicus" kingdom="Animalia" order="Passeriformes" pageId="44" pageNumber="44" phylum="Chordata" rank="species" species="vitellinus">
