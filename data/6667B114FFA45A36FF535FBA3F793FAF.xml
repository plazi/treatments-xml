<collectingCountry id="96D94092FFA45A36FE1E5EF638483FD7" box="[474,575,1921,1947]" name="Slovakia" pageId="5" pageNumber="233">Slovakia</collectingCountry>
, 
<collectingCountry id="96D94092FFA45A36FD8E5EF538DA3FD0" box="[586,685,1922,1948]" name="Hungary" pageId="5" pageNumber="233">Hungary</collectingCountry>
, 
<collectingCountry id="96D94092FFA45A36FD7C5EF639623FD7" box="[696,789,1921,1947]" name="Ukraine" pageId="5" pageNumber="233">Ukraine</collectingCountry>
