<collectingCountry id="59F38CE259657133FDD239EBAB84FA41" box="[620,724,1482,1506]" name="Romania" pageId="16" pageNumber="317">Romania</collectingCountry>
, 
<collectingCountry id="59F38CE259657133FD5F39EBAA62FA41" box="[737,818,1482,1506]" name="Turkey" pageId="16" pageNumber="317">Turkey</collectingCountry>
