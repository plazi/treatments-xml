<paragraph id="4310FA7F8B1B8BB47A682870157D3938" pageId="null" pageNumber="590">
<pageBreakToken id="685F93020D2D43D6695AB7BD01C2CAC9" pageId="null" pageNumber="590">Artengruppe</pageBreakToken>
des 
<taxonomicName id="44D5E29671EC5AE4D1733A7A722E8311" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="murorum">
