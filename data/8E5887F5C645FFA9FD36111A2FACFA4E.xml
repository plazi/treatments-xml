<emphasis id="3485EAF1C645FFA8FECF114C2AB6F9EE" box="[353,494,1638,1661]" italics="true" pageId="8" pageNumber="1254">
<typeStatus id="D94A8841C645FFA8FECF114C2AC1F9EE" box="[353,409,1638,1661]" pageId="8" pageNumber="1254">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="C1F14D60C645FFA8FE53114C28F2F9EE" authority="(Titova, 1975)" baseAuthorityName="Titova" baseAuthorityYear="1975" box="[509,938,1636,1661]" class="Chilopoda" family="Mecistocephalidae" genus="Agnostrup" kingdom="Animalia" order="Geophilomorpha" pageId="8" pageNumber="1254" phylum="Arthropoda" rank="species" species="striganovae">
