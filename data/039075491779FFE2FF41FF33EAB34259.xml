(
<number id="3DF1D6781779FFE2FB86FB9BEB50410D" box="[1104,1136,1087,1111]" pageId="2" pageNumber="440" value="24.0">24</number>
), 
<number id="3DF1D6781779FFE2FB52FB9AEB80410D" box="[1156,1184,1086,1111]" pageId="2" pageNumber="440" value="68.0">68</number>
(
<number id="3DF1D6781779FFE2FB66FB9BEBEE410D" box="[1200,1230,1087,1111]" pageId="2" pageNumber="440" value="17.0">17</number>
), 
<number id="3DF1D6781779FFE2FB32FB9AEA20410D" box="[1252,1280,1086,1111]" pageId="2" pageNumber="440" value="51.0">51</number>
(
<number id="3DF1D6781779FFE2FAC6FB9BEA0E410D" box="[1296,1326,1087,1111]" pageId="2" pageNumber="440" value="17.0">17</number>
), 
<number id="3DF1D6781779FFE2FA95FB9AEA7F410D" box="[1347,1375,1086,1111]" pageId="2" pageNumber="440" value="52.0">52</number>
(
