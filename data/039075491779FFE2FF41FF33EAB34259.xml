<number id="3DF1D6781779FFE2FAE7FC76EA7E46B1" box="[1329,1374,978,1003]" pageId="2" pageNumber="440" value="160.0">160</number>
; eyes length 
<number id="3DF1D6781779FFE2FF30FC52EE254155" box="[230,261,1014,1039]" pageId="2" pageNumber="440" value="60.0">60</number>
, width 
<number id="3DF1D6781779FFE2FE8AFC53EE5B4155" box="[348,379,1015,1039]" pageId="2" pageNumber="440" value="49.0">49</number>
