<taxonomicName id="4C693CB49D299E34FE46BB995055FD63" authorityName="Mesomphaliini" baseAuthorityName="Spaeth" baseAuthorityYear="1911" box="[500,622,663,686]" class="Insecta" family="Chrysomelidae" genus="Stolas" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="703" phylum="Arthropoda" rank="species" species="hermanni">
