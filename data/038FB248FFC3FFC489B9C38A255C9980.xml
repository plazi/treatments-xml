<taxonomicName id="4C2678DDFFC3FFC4898BC38A23C49BC3" authority="(Creplin, 1846) Anderson &amp; Wong, 1981" authorityName="(Creplin, 1846) Anderson &amp; Wong" authorityYear="1981" box="[165,910,1433,1459]" class="Chromadorea" family="Acuariidae" genus="Paracuaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="114" pageNumber="115" phylum="Nematoda" rank="species" species="adunca">
