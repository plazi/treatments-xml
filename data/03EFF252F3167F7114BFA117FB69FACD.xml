<collectingCountry id="F35103D4F3167F711549A6B9FE4BFB5C" box="[353,440,1226,1251]" name="Angola" pageId="18" pageNumber="219">Angola</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F7115E2A6B9FDD1FB5C" box="[458,546,1226,1251]" name="Zambia" pageId="18" pageNumber="219">Zambia</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F71161CA6B9FD5CFB5C" box="[564,687,1226,1251]" name="Zimbabwe" pageId="18" pageNumber="219">Zimbabwe</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F7116E8A6B9FCEDFB5C" box="[704,798,1226,1251]" name="Rwanda" pageId="18" pageNumber="219">Rwanda</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F711718A6B9FC7DFB5C" box="[816,910,1226,1251]" name="Burundi" pageId="18" pageNumber="219">Burundi</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F711788A6B9FC0BFB5C" box="[928,1016,1226,1251]" name="Malawi" pageId="18" pageNumber="219">Malawi</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F711022A6B9FB81FB5C" box="[1034,1138,1226,1251]" name="Tanzania" pageId="18" pageNumber="219">Tanzania</collectingCountry>
, 
<collectingCountry id="F35103D4F3167F7110ACA6B9FAEBFB5C" box="[1156,1304,1226,1251]" name="Mozambique" pageId="18" pageNumber="219">Mozambique</collectingCountry>
and 
<collectingCountry id="F35103D4F3167F711173A6B9FF17FAB8" name="South Africa" pageId="18" pageNumber="219">
<collectingRegion id="49828DA6F3167F711173A6B9FA6FFB5C" box="[1371,1436,1226,1251]" country="Tanzania" name="Kusini Unguja" pageId="18" pageNumber="219">South</collectingRegion>
Africa
</collectingCountry>
