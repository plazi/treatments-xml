<taxonomicName id="4C4638C7F3167F71159AA72FFDDFFACC" baseAuthorityName="Quedenfeldt" baseAuthorityYear="1883" box="[434,556,1370,1395]" class="Insecta" family="Carabidae" genus="Trichodela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="nubifera">
