<taxonomicName id="4C4638C7F3167F71102DA72FFB79FACC" baseAuthorityName="W. Horn" baseAuthorityYear="1907" box="[1029,1162,1370,1395]" class="Insecta" family="Carabidae" genus="Lophyra" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="wellmani">
