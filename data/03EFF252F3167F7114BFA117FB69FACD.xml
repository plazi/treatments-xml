<taxonomicName id="4C4638C7F3167F7110BBA744FAD5FAF0" baseAuthorityName="Putzeys" baseAuthorityYear="1880" box="[1171,1318,1334,1359]" class="Insecta" family="Carabidae" genus="Progonochaetus" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="planicollis">
