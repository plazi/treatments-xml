</taxonomicName>
, 
<taxonomicName id="4C4638C7F3167F711612A72FFD5DFACC" baseAuthorityName="Putzeys" baseAuthorityYear="1880" box="[570,686,1370,1395]" class="Insecta" family="Carabidae" genus="Epitrichodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="villosus">
