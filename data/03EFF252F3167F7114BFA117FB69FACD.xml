<taxonomicName id="4C4638C7F3167F711695A72FFCA0FACC" baseAuthorityName="Guerin-Meneville" baseAuthorityYear="1849" box="[701,851,1370,1395]" class="Insecta" family="Carabidae" genus="Lophyra" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="saraliensis">
