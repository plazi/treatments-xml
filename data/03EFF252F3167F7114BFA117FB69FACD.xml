<taxonomicName id="4C4638C7F3167F7114BFA744FED7FAF1" authorityName="Muller" authorityYear="1940" box="[151,292,1335,1358]" class="Insecta" family="Carabidae" genus="Stereostoma" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="guineense">
