<paragraph id="AE71522D759C682A17DEC3861A7915B5" pageId="4" pageNumber="425">
<taxonomicName id="0624E3C4419CBF2706C70BEE06E0E0C8" authority="Rasnitsyn &amp; Jarzembowski, 2000" authorityName="Rasnitsyn &amp; Jarzembowski" authorityYear="2000" class="Insecta" family="Maimetshidae" genus="Andyrossia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Andyrossia" order="Hymenoptera" pageId="4" pageNumber="425" phylum="Arthropoda" rank="genus">Andyrossia</taxonomicName>
