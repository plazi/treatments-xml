<taxonomicName id="4C1D4D7D5548FF84FCFCFB32FC215392" baseAuthorityName="Rosa" baseAuthorityYear="1890" box="[869,948,1175,1199]" class="Clitellata" family="Megascolecidae" genus="Amynthas" kingdom="Animalia" order="Opisthopora" pageId="20" pageNumber="1248" phylum="Annelida" rank="species" species="bournei">
