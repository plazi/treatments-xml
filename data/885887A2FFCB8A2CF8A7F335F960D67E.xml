<collectionCode id="66E0AE71FFCB8A2DF29BF20BF2FDD4B2" box="[3415,3448,3498,3537]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="4" pageNumber="283">R</collectionCode>
