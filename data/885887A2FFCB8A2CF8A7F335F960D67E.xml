<collectingCountry id="78E67624FFCB8A2DF4E1F20BF3ACD4B2" box="[2861,3113,3498,3537]" name="Mozambique" pageId="4" pageNumber="283">Mozambique</collectingCountry>
. 
<collectingMunicipality id="E02AACCEFFCB8A2DF388F20BF281D4B2" box="[3140,3332,3498,3537]" pageId="4" pageNumber="283">Restricted</collectingMunicipality>
