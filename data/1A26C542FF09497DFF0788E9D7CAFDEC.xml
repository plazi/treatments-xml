<taxonomicName id="558F0FD7FF09497DFF0788E9D5C7FA34" authority="(White)" baseAuthorityName="White" baseAuthorityYear="1855" box="[151,472,1454,1475]" class="Insecta" family="Cerambycidae" genus="Megacyllene" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="744" phylum="Arthropoda" rank="species" species="antennata">
