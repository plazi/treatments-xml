<emphasis id="B928131DC50276705DAAE0BB55C4FD9B" bold="true" box="[208,254,495,516]" pageId="8" pageNumber="1761">
<collectingRegion id="499801EDC50276705DAAE0BB55C4FD9B" box="[208,254,495,516]" country="Turkey" name="Kars" pageId="8" pageNumber="1761">Kars</collectingRegion>
</emphasis>
