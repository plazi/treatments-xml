<quantity id="4CA462EAC50276705CBBE0BA573FFD9B" box="[449,517,494,516]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="8" pageNumber="1761" unit="m" value="1500.0">1500 m</quantity>
