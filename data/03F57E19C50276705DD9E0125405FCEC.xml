<emphasis id="B928131DC50276705C52E0825447FE73" bold="true" box="[296,381,470,492]" pageId="8" pageNumber="1761">
<collectingRegion id="499801EDC50276705C52E0825447FE73" box="[296,381,470,492]" country="Turkey" name="Erzincan" pageId="8" pageNumber="1761">Erzincan</collectingRegion>
</emphasis>
