<quantity id="4CA462EAC50276705FC9E08357C0FE73" box="[691,762,471,492]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="8" pageNumber="1761" unit="m" value="1300.0">1300 m</quantity>
