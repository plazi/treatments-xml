<subSubSection box="[833,1174,1095,1129]" pageId="44" pageNumber="83" type="nomenclature">
<paragraph blockId="44.[833,1178,1090,1129]" box="[833,1174,1095,1129]" pageId="44" pageNumber="83">
<emphasis bold="true" box="[833,1018,1095,1129]" pageId="44" pageNumber="83">
<taxonomicName box="[833,1011,1095,1129]" class="Insecta" family="Mydidae" genus="Eumydas" kingdom="Animalia" order="Diptera" pageId="44" pageNumber="83" phylum="Arthropoda" rank="genus" status="gen. n.">Eumydas</taxonomicName>
,
</emphasis>
<taxonomicNameLabel box="[1044,1174,1096,1128]" pageId="44" pageNumber="83" rank="genus">gen. n.</taxonomicNameLabel>
</paragraph>
