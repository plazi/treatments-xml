<collectingCountry box="[811,875,1554,1577]" name="Japan" pageId="13" pageNumber="434">Japan</collectingCountry>
, approximately 
<quantity box="[1055,1153,1554,1578]" metricMagnitude="1" metricUnit="m" metricValue="2.75" metricValueMax="3.5" metricValueMin="2.0" pageId="13" pageNumber="434" unit="m" value="27.5" valueMax="35.0" valueMin="20.0">20–35 m</quantity>
