<taxonomicName class="Magnoliopsida" family="Juglandaceae" genus="Juglans" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Juglans" order="Fagales" pageId="7" pageNumber="48" phylum="Tracheophyta" rank="genus">Juglans</taxonomicName>
