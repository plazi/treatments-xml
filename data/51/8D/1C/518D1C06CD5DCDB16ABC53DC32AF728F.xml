<taxonomicName authorityName="J.Gaertner" authorityYear="1791" class="Magnoliopsida" family="Asteraceae" genus="Gazania" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Asterales" pageId="9" pageNumber="38" phylum="Magnoliophyta" rank="genus">
