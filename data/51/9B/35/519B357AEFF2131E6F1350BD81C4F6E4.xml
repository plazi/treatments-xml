<taxonomicName authority="Bourguignat, 1884" authorityName="Bourguignat" authorityYear="1884" class="Gastropoda" family="Melanopsidae" genus="Fagotia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Fagotia" order="Sorbeoconcha" pageId="6" pageNumber="7" phylum="Mollusca" rank="genus">
