<date box="[1188,1422,751,793]" pageId="7" pageNumber="72" value="1936-05-06">May 6, 1936</date>
;
</materialsCitation>
<materialsCitation ID-GBIF-Occurrence="3331287302" collectedfrom="Messerschmidia (Tournefortia)" collectingDate="1936-05-17" collectorName="R. L. Usinger" country="Guam" location="Tarague" pageId="7" pageNumber="72" specimenCount="2">
