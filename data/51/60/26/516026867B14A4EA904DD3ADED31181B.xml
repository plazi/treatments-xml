<taxonomicName class="Liliopsida" family="Poaceae" genus="Fargesia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fargesia" order="Poales" pageId="2" pageNumber="83" phylum="Tracheophyta" rank="genus">
<emphasis italics="true" pageId="2" pageNumber="83">Fargesia</emphasis>
</taxonomicName>
, 
<taxonomicName class="Liliopsida" family="Poaceae" genus="Phragmites" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Phragmites" order="Poales" pageId="2" pageNumber="83" phylum="Tracheophyta" rank="genus">
