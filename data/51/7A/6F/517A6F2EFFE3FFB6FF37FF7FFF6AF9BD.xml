<caption ID-Table-UUID="8DAC8EB0FFE6FFB6FF37FF0AFAA1FF5E" box="[151,1398,151,175]" httpUri="http://table.plazi.org/id/8DAC8EB0FFE6FFB6FF37FF0AFAA1FF5E" pageId="4" pageNumber="59" targetBox="[159,1266,207,785]" targetIsTable="true" targetPageId="4">
<paragraph blockId="4.[151,1398,151,175]" box="[151,1398,151,175]" pageId="4" pageNumber="59">
