<subSubSection pageId="1" pageNumber="42" type="etymology">
<paragraph blockId="1.[151,1437,1370,2036]" pageId="1" pageNumber="42">
<emphasis bold="true" box="[198,343,1969,1995]" pageId="1" pageNumber="42">Etymology:</emphasis>
<taxonomicName box="[350,436,1970,1995]" class="Insecta" family="Hydroptilidae" genus="Ochrotrichia" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="43" phylum="Arthropoda" rank="species" species="avicula" status="new species">
