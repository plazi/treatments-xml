<emphasis italics="true" pageId="0" pageNumber="17">Araeopteron</emphasis>
</taxonomicName>
spp. 
<emphasis bold="true" pageId="0" pageNumber="17">7</emphasis>
<taxonomicName class="Insecta" genus="A." kingdom="Animalia" lsidName="A. dawai" order="Lepidoptera" pageId="0" pageNumber="17" phylum="Arthropoda" rank="species" species="dawai">
<emphasis italics="true" pageId="0" pageNumber="17">A. dawai</emphasis>
</taxonomicName>
sp. nov. holotype, gen. prep. no. hhl-4010-1 
<emphasis bold="true" pageId="0" pageNumber="17">8</emphasis>
