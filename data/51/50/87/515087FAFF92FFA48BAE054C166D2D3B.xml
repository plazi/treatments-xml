<taxonomicName authority="Skalický, 2019" authorityName="Skalický" authorityYear="2019" box="[965,1058,622,644]" class="Insecta" family="Heteroceridae" genus="Heterocerus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1423" phylum="Arthropoda" rank="species" species="havai" status="nov.sp.">
<emphasis box="[965,983,623,644]" italics="true" pageId="2" pageNumber="1423">H</emphasis>
. 
<emphasis box="[1002,1058,622,643]" italics="true" pageId="2" pageNumber="1423">havai</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[151,225,652,673]" pageId="2" pageNumber="1423" rank="species">nov.sp.</taxonomicNameLabel>
(see Figs 15,16, 40, 47, 71, 72, 73, 114, 116 and 
<quantity box="[728,793,651,673]" metricMagnitude="0" metricUnit="m" metricValue="2.9718" pageId="2" pageNumber="1423" unit="in" value="117.0">117 in</quantity>
