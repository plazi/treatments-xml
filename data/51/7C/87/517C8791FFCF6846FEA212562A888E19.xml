<paragraph pageId="6" pageNumber="67">
<table box="[264,1309,374,1616]" gridcols="4" gridrows="11" pageId="6" pageNumber="67">
<tr box="[264,1309,374,433]" gridrow="0" pageId="6" pageNumber="67">
<th box="[264,519,374,433]" gridcol="0" gridrow="0" pageId="6" pageNumber="67">Character</th>
<th box="[539,784,374,433]" gridcol="1" gridrow="0" pageId="6" pageNumber="67">
<taxonomicName box="[539,663,375,398]" class="Insecta" family="Carabidae" genus="Halocoryza" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="67" phylum="Arthropoda" rank="genus">
<emphasis box="[539,663,375,398]" italics="true" pageId="6" pageNumber="67">Halocoryza</emphasis>
</taxonomicName>
.
</th>
<emphasis italics="true" pageId="6" pageNumber="67">
<th box="[804,1049,374,433]" gridcol="2" gridrow="0" pageId="6" pageNumber="67">
<emphasis italics="true" pageId="6" pageNumber="67">
<taxonomicName box="[804,1009,375,398]" class="Insecta" family="Carabidae" genus="Schizogenius" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="67" phylum="Arthropoda" rank="species" species="sensu">
<taxonomicName box="[804,943,375,398]" class="Insecta" family="Carabidae" genus="Schizogenius" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="67" phylum="Arthropoda" rank="genus" sensu="lato">Schizogenius</taxonomicName>
<taxonomicNameLabel box="[950,1009,375,398]" pageId="6" pageNumber="67" sensu="lato">sensu</taxonomicNameLabel>
</taxonomicName>
lato.
