, 
<emphasis box="[204,239,515,538]" italics="true" pageId="16" pageNumber="387">pv’</emphasis>
, 
<emphasis box="[253,295,515,538]" italics="true" pageId="16" pageNumber="387">pv’’</emphasis>
, 
<emphasis box="[309,323,516,539]" italics="true" pageId="16" pageNumber="387">u</emphasis>
