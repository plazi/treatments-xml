<emphasis box="[1357,1374,751,775]" italics="true" pageId="27" pageNumber="398">ϕ</emphasis>
<emphasis box="[1374,1382,765,779]" italics="true" pageId="27" pageNumber="398">1</emphasis>
6, 
<emphasis box="[1411,1428,751,775]" italics="true" pageId="27" pageNumber="398">ϕ</emphasis>
<emphasis box="[1428,1436,765,779]" italics="true" pageId="27" pageNumber="398">2</emphasis>
