<collectingCounty box="[947,1067,1650,1674]" pageId="132" pageNumber="134">Chaetome</collectingCounty>
of &quot;complete 
<typeStatus box="[1232,1287,1651,1675]" pageId="132" pageNumber="134">type</typeStatus>
