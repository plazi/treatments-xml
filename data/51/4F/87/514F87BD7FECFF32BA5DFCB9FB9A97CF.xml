&gt; 
<emphasis box="[1018,1036,1335,1359]" italics="true" pageId="132" pageNumber="134">le</emphasis>
&gt; 
<emphasis box="[1052,1074,1336,1359]" italics="true" pageId="132" pageNumber="134">ro</emphasis>
. 
<location LSID="urn:lsid:plazi:treatment:514F87BD7FECFF32BA5DFCB9FB9A97CF:DC3960707FECFF32BD2BFAA1FB6A951C" box="[1100,1231,1335,1359]" county="Chaetome" municipality="Ventral region" name="Notogaster" pageId="132" pageNumber="134">Notogaster</location>
with very short (
<emphasis box="[835,846,1371,1394]" italics="true" pageId="132" pageNumber="134">c</emphasis>
<subScript attach="left" box="[846,855,1379,1396]" fontSize="7" pageId="132" pageNumber="134">1</subScript>
&lt;1/4 
<emphasis box="[915,926,1371,1394]" italics="true" pageId="132" pageNumber="134">c</emphasis>
<subScript attach="left" box="[926,935,1379,1396]" fontSize="7" pageId="132" pageNumber="134">1</subScript>
- 
<emphasis box="[945,958,1370,1394]" italics="true" pageId="132" pageNumber="134">d</emphasis>
<subScript attach="left" box="[959,968,1379,1396]" fontSize="7" pageId="132" pageNumber="134">1</subScript>
), fine, spiniform, setae; setae 
<emphasis box="[1343,1354,1371,1394]" italics="true" pageId="132" pageNumber="134">c</emphasis>
<subScript attach="left" box="[1354,1363,1379,1396]" fontSize="7" pageId="132" pageNumber="134">1</subScript>
situated at anterior margin, setae 
<emphasis box="[1168,1179,1406,1429]" italics="true" pageId="132" pageNumber="134">c</emphasis>
<subScript attach="left" box="[1179,1188,1415,1432]" fontSize="7" pageId="132" pageNumber="134">3</subScript>
