<taxonomicName authority="Jenkin, 1908" authorityName="Jenkin" authorityYear="1908" box="[222,514,837,864]" class="Calcarea" family="Achramorphidae" genus="Achramorpha" kingdom="Animalia" order="Leucosolenida" pageId="5" pageNumber="206" phylum="Porifera" rank="species" species="grandinis">
<emphasis box="[222,360,837,864]" italics="true" pageId="5" pageNumber="206">A. grandinis</emphasis>
<bibRefCitation author="Jenkin, C. F." box="[368,514,837,863]" pageId="5" pageNumber="206" pagination="1 - 49" refId="ref27105" refString="Jenkin, C. F. (1908) Porifera. III. Calcarea. National Antarctic Expedition, 1901 - 1904, Natural History, 4, Zoology, 1 - 49, pls. XXVII-XXXVIII." type="journal article" year="1908">Jenkin, 1908</bibRefCitation>
</taxonomicName>
; 
<taxonomicName authority="Jenkin, 1908" authorityName="Jenkin" authorityYear="1908" box="[526,782,837,864]" class="Calcarea" family="Achramorphidae" genus="Achramorpha" kingdom="Animalia" order="Leucosolenida" pageId="5" pageNumber="206" phylum="Porifera" rank="species" species="nivalis">
<emphasis box="[526,632,837,864]" italics="true" pageId="5" pageNumber="206">A. nivalis</emphasis>
<bibRefCitation author="Jenkin, C. F." box="[639,782,837,863]" pageId="5" pageNumber="206" pagination="1 - 49" refId="ref27105" refString="Jenkin, C. F. (1908) Porifera. III. Calcarea. National Antarctic Expedition, 1901 - 1904, Natural History, 4, Zoology, 1 - 49, pls. XXVII-XXXVIII." type="journal article" year="1908">Jenkin, 1908</bibRefCitation>
