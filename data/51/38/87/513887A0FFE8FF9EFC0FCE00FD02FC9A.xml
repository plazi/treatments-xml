<paragraph blockId="1.[115,753,193,243]" pageId="1" pageNumber="282">
<typeStatus box="[115,163,193,214]" pageId="1" pageNumber="282">Type</typeStatus>
species. – 
<taxonomicName authority="Thorell, 1889" authorityName="Thorell" authorityYear="1889" box="[281,629,194,215]" class="Arachnida" family="Charontidae" genus="Charon" kingdom="Animalia" order="Amblypygi" pageId="1" pageNumber="282" phylum="Arthropoda" rank="species" species="cavernicola">
