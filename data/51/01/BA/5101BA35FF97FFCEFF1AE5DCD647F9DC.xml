<subSubSection pageId="55" pageNumber="56" type="etymology">
<paragraph blockId="55.[151,1437,1055,1620]" pageId="55" pageNumber="56">
<emphasis bold="true" box="[199,326,1560,1583]" pageId="55" pageNumber="56">Etymology</emphasis>
