<collectingCountry id="53FE76098915FFA7FE4F8D0DFE4BA4E3" box="[405,484,1402,1427]" name="Somalia" pageId="8" pageNumber="257">Somali</collectingCountry>
province, 
<collectingCountry id="53FE76098915FFA7FDBE8D0DFD6CA4E3" box="[612,707,1402,1427]" name="Ethiopia" pageId="8" pageNumber="257">Ethiopia</collectingCountry>
