. 
<taxonomicName id="2CBF4D7FFF94FFA8E37F7ADDF1D9F8C6" authority="(Stimpson, 1860)" baseAuthorityName="Stimpson" baseAuthorityYear="1860" box="[298,718,1778,1803]" class="Malacostraca" family="Upogebiidae" genus="Upogebia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="266" phylum="Arthropoda" rank="species" species="carinicauda">
