<quantity id="4CEF9B25FF48FF6C9CFCFA1DFE88360F" box="[1153,1225,1519,1546]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="217" pageNumber="218" unit="m" value="700.0">700 m</quantity>
