<quantity id="4CEF9B25FF48FF6C9D5FFA39FFDD35E3" box="[1314,1436,1483,1510]" metricMagnitude="2" metricUnit="m" metricValue="8.025" metricValueMax="16.0" metricValueMin="0.05" pageId="217" pageNumber="218" unit="m" value="802.5" valueMax="1600.0" valueMin="5.0">5–1,600 m</quantity>
