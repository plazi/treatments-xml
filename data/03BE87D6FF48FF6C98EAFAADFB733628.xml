<quantity id="4CEF9B25FF48FF6C9897FA71FB6E359B" box="[234,303,1411,1438]" metricMagnitude="-1" metricUnit="m" metricValue="5.3" pageId="217" pageNumber="218" unit="cm" value="53.0">53 cm</quantity>
