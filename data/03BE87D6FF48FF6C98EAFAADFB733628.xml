<quantity id="4CEF9B25FF48FF6C9AD6FA71F8B1359B" box="[683,752,1411,1438]" metricMagnitude="-1" metricUnit="m" metricValue="6.0" pageId="217" pageNumber="218" unit="cm" value="60.0">60 cm</quantity>
