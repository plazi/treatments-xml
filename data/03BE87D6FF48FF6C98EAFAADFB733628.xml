<quantity id="4CEF9B25FF48FF6C9AD6FA71F8B1359B" box="[683,752,1411,1438]" metricMagnitude="-1" metricUnit="m" metricValue="6.0" pageId="217" pageNumber="218" unit="cm" value="60.0">60 cm</quantity>
(
<quantity id="4CEF9B25FF48FF6C9A82FA71F97B359B" box="[767,826,1411,1438]" metricMagnitude="-1" metricUnit="m" metricValue="6.096" pageId="217" pageNumber="218" unit="in" value="24.0">24 in</quantity>
) TL (
<bibRefCitation id="EF864B31FF48FF6C9B05FA71FE10359B" author="Kramer, D. E. &amp; Barss, W. H. &amp; Paust, B. C. &amp; Bracken, B. E." box="[888,1105,1411,1438]" pageId="217" pageNumber="218" refId="ref292593" refString="Kramer, D. E., Barss, W. H., Paust, B. C. &amp; Bracken, B. E. (1995) Guide to northeast Pacific flatfishes: families Bothidae, Cynoglossidae, and Pleuronectidae. University of Alaska Sea Grant, Fairbanks, Marine Advisory Bulletin, 47." type="book" year="1995">
