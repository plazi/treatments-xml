and 
<taxonomicName id="4C794D17FF8BC65673A9593C4354FE59" class="Magnoliopsida" family="Euphorbiaceae" kingdom="Plantae" order="Malpighiales" pageId="39" pageNumber="39" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
