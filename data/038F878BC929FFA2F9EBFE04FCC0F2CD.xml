); from 
<quantity id="4CDE9B78C928FFA2F8F3FF65FE4FF2CD" box="[399,470,188,215]" metricMagnitude="2" metricUnit="m" metricValue="1.6" pageId="59" pageNumber="60" unit="m" value="160.0">160 m</quantity>
to 
<quantity id="4CDE9B78C928FFA2F886FF64FDD7F2CD" box="[506,590,189,215]" metricMagnitude="3" metricUnit="m" metricValue="1.17" pageId="59" pageNumber="60" unit="m" value="1170.0">1170 m</quantity>
depth (
<bibRefCitation id="EFB74B6CC928FFA2FBDDFF65FCD4F2CD" author="Opresko, D. M." box="[673,845,188,215]" pageId="59" pageNumber="60" pagination="109" refId="ref49084" refString="Opresko, D. M. (2006) Revision of theAntipatharia (Cnidaria: Anthozoa). Part V. Establishment of a new family, Stylopathidae. Zoologische Mededelingen, 80 (4), 109." type="journal article" year="2006">Opresko, 2006</bibRefCitation>
).
</paragraph>
</subSubSection>
