<quantity id="4CED0B8C993DFFEA1D43F889FAFBF80B" box="[1302,1386,1900,1926]" metricMagnitude="-2" metricUnit="m" metricValue="2.7178" pageId="16" pageNumber="17" unit="in" value="1.07">1.07 in</quantity>
