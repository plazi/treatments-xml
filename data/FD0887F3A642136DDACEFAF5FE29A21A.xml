<quantity id="B2599B00A644136DD880FF6FFCEAA3B6" box="[729,802,153,179]" metricMagnitude="4" metricUnit="m" metricValue="7.8" pageId="4" pageNumber="361" unit="km" value="78.0">78 km</quantity>
