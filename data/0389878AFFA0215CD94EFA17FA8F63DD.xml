<taxonomicName id="4C204D1FFFA0215CDB76F998FD646442" authorityName="Bergey et al." authorityYear="1923" baseAuthorityName="Orla-Jensen" baseAuthorityYear="1919" box="[577,715,1549,1573]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="45" pageNumber="2827" phylum="Firmicutes" rank="species" species="plantarum">
