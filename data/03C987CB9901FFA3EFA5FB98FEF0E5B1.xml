<taxonomicName id="4C604D5E990EFFA3EC4FFA3FFC0EE6F1" baseAuthorityName="Meyrick" baseAuthorityYear="1908" box="[811,931,1477,1499]" class="Insecta" family="Pterophoridae" genus="Hellinsia" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="chlorias">
