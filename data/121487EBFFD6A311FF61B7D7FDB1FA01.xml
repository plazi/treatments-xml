<quantity id="5D459B18FFD6A311FDDCB7A0FD67FEEB" box="[554,660,233,256]" metricMagnitude="-1" metricUnit="m" metricValue="5.75" metricValueMax="6.0" metricValueMin="5.5" pageId="6" pageNumber="57" unit="cm" value="57.5" valueMax="60.0" valueMin="55.0">55−60 cm</quantity>
