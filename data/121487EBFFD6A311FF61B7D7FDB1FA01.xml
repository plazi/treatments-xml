<quantity id="5D459B18FFD6A311FB08B640FABFFECB" box="[1278,1356,265,288]" metricMagnitude="-2" metricUnit="m" metricValue="7.0" metricValueMax="8.0" metricValueMin="6.0" pageId="6" pageNumber="57" unit="cm" value="7.0" valueMax="8.0" valueMin="6.0">6–8 cm</quantity>
