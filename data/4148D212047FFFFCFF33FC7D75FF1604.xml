<taxonomicName id="0EE11887047FFFFCFB14FA1C738115DC" baseAuthorityName="Verrill" baseAuthorityYear="1882" box="[1200,1427,1489,1515]" class="Ophiuroidea" family="Ophiuridae" genus="Ophiura" kingdom="Animalia" order="Ophiurida" pageId="47" pageNumber="48" phylum="Echinodermata" rank="species" species="aurantiaca">
<emphasis id="FB95BF16047FFFFCFB14FA1C738115DC" box="[1200,1427,1489,1515]" italics="true" pageId="47" pageNumber="48">Ophiura aurantiaca</emphasis>
</taxonomicName>
