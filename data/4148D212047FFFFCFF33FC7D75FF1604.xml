<taxonomicName id="0EE11887047FFFFCFCCEFA1C726F15DC" baseAuthorityName="Verrill" baseAuthorityYear="1882" box="[874,1149,1489,1515]" class="Ophiuroidea" family="Ophiuridae" genus="Ophiopleura" kingdom="Animalia" order="Ophiurida" pageId="47" pageNumber="48" phylum="Echinodermata" rank="species" species="aurantiaca">
<emphasis id="FB95BF16047FFFFCFCCEFA1C726F15DC" box="[874,1149,1489,1515]" italics="true" pageId="47" pageNumber="48">Ophiopleura aurantiaca</emphasis>
</taxonomicName>
