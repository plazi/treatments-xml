<collectingRegion id="0B25ADE6047FFFFCFEF0FA3877B11638" box="[340,419,1525,1551]" country="Portugal" name="Acores" pageId="47" pageNumber="48">Azores</collectingRegion>
waters (
<emphasis id="FB95BF16047FFFFCFDA2FA3874881638" box="[518,666,1525,1551]" italics="true" pageId="47" pageNumber="48">Hirondelle II</emphasis>
, sta 3150: 
<geoCoordinate id="ACD505C3047FFFFCFCBCFA3875691638" box="[792,891,1525,1551]" degrees="38" direction="north" minutes="01" orientation="latitude" pageId="47" pageNumber="48" precision="925" value="38.016666">38°01’N</geoCoordinate>
, 
<geoCoordinate id="ACD505C3047FFFFCFC2CFA3975E21638" box="[904,1008,1524,1551]" degrees="25" direction="west" minutes="21" orientation="longitude" pageId="47" pageNumber="48" precision="925" value="-25.35">25°21’W</geoCoordinate>
