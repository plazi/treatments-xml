<taxonomicName id="4C7E55A4FFB9AE0DC39EFD57FCFDA80F" authorityName="Rondani" authorityYear="1856" box="[732,864,862,887]" class="Insecta" family="Agromyzidae" genus="Domomyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="genus">
