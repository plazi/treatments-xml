<taxonomicName id="4C7E55A4FFB9AE0DC584FD57FA06A80F" authorityName="Rondani" authorityYear="1856" box="[1222,1435,862,887]" class="Insecta" family="Agromyzidae" genus="Domomyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="cincta">
