<taxonomicName id="0FEDA0D3FFBA6701FB8200B389ECFCAE" authority="Bown &amp; Schankler, 1982" authorityName="Bown &amp; Schankler" authorityYear="1982" class="Insecta" family="Odonata" genus="Eolestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="2" pageNumber="140" phylum="Arthropoda" rank="genus">
