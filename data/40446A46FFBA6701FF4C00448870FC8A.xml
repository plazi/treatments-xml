<taxonomicName id="0FEDA0D3FFBA6701FBE0009F8873FC8A" authority="Holroyd, Bown &amp; Schankler, 2004" authorityName="Holroyd, Bown &amp; Schankler" authorityYear="2004" class="Mammalia" family="Creotarsidae" genus="Auroralestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Erinaceomorpha" pageId="2" pageNumber="140" phylum="Chordata" rank="genus">
