from &quot;Alaska: Juneau. Mts. near Mendenhall Glacier, 
<geoCoordinate id="BDD2FABDFFD6FF8696BDF5C9FE3118EC" box="[404,502,1485,1514]" direction="north" orientation="latitude" pageId="15" pageNumber="42" precision="925" value="58.483334">
