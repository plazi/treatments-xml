o 
<number id="6E2E8E5DFFD6FF869511F5C9FD9018EC" box="[568,599,1488,1514]" pageId="15" pageNumber="42" value="40.0">40</number>
'W
</geoCoordinate>
, 
<date id="AC58BABAFFD6FF8695ADF5C9FCDD18EC" box="[644,794,1488,1514]" pageId="15" pageNumber="42" value="1980-07-13">
