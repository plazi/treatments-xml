<taxonomicName id="06E3A04E134F446657A7701FBA836C75" authorityName="Linnavuori" authorityYear="1978" class="Insecta" family="Cicadellidae" genus="Signoretia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Hemiptera" pageId="13" pageNumber="316" phylum="Arthropoda" rank="species" species="errans">Signoretia errans</taxonomicName>
