<taxonomicName id="D99B4D6FFFE1FF8173FAFC6BFDEEFC04" authority="Ferragut &amp; Escudero, 1997" authorityName="Ferragut &amp; Escudero" authorityYear="1997" class="Arachnida" family="Phytoseiidae" genus="Euseius" kingdom="Animalia" order="Mesostigmata" pageId="8" pageNumber="109" phylum="Arthropoda" rank="species" species="scutalis">
