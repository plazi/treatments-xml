<taxonomicName id="4C334D10FF8CFFCBB1F1A27121FFFB91" authority="Allegro, Giachino &amp; Sciaky, 2008" authorityName="Allegro, Giachino &amp; Sciaky" authorityYear="2008" box="[751,1254,1084,1109]" class="Insecta" family="Carabidae" genus="Oxytrechus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="145" phylum="Arthropoda" rank="species" species="onorei">
