<quantity id="4C9BDEBB9B4D6078FBFBF9FF5296F70B" box="[1069,1162,1624,1651]" metricMagnitude="-3" metricUnit="m" metricValue="6.3" pageId="16" pageNumber="65" unit="mm" value="6.3">6.3 mm</quantity>
