<quantity id="4C9BDEBB9B4D6078FCE7F9FF5592F70B" box="[817,910,1624,1651]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" pageId="16" pageNumber="65" unit="mm" value="5.1">5.1 mm</quantity>
