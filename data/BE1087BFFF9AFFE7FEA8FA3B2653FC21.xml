<taxonomicName id="F1B94D2AFF9BFFE4FE36FBAD2738FB7E" box="[406,599,1210,1233]" class="Insecta" family="Achilidae" genus="Anabunda" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="minuta">
