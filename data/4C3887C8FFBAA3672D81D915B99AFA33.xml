<paragraph id="C42E36DEFFBAA3672CB9D802BF7BFAB3" blockId="8.[151,1127,1357,1508]" box="[151,1073,1357,1381]" pageId="8" pageNumber="287">
<emphasis id="F6E5EACCFFBAA3672CB9D802BA0EFAB3" bold="true" box="[151,324,1357,1380]" pageId="8" pageNumber="287">Current status.</emphasis>
<taxonomicName id="03914D5DFFBAA3672D65D801B800FAB3" authority="Kuwert, 1891" authorityName="Kuwert" authorityYear="1891" box="[331,842,1357,1381]" class="Insecta" family="Passalidae" genus="Veturius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="287" phylum="Arthropoda" rank="species" species="tuberculifrons" subGenus="Veturius">
<emphasis id="F6E5EACCFFBAA3672D65D801BAE8FAB2" box="[331,418,1358,1381]" italics="true" pageId="8" pageNumber="287">Veturius</emphasis>
(
<emphasis id="F6E5EACCFFBAA3672D9CD801B943FAB2" box="[434,521,1358,1381]" italics="true" pageId="8" pageNumber="287">Veturius</emphasis>
) 
<emphasis id="F6E5EACCFFBAA3672E37D802B9E4FAB2" box="[537,686,1357,1381]" italics="true" pageId="8" pageNumber="287">tuberculifrons</emphasis>
<bibRefCitation id="A0004B2FFFBAA3672E9AD802B800FAB3" author="KUWERT A. F." box="[692,842,1357,1380]" pageId="8" pageNumber="287" pagination="161 - 192" refId="ref8439" refString="KUWERT A. F. 1891: Systematische Uebersicht der Passaliden-Arten und Gattungen. Deutsche Entomologische Zeitschrift 1891: 161 - 192." type="journal article" year="1891">Kuwert, 1891</bibRefCitation>
