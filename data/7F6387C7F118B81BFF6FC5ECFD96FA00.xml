and anteriorly with 
<number id="410224F6F118B81BFB63C049FB3CFBB8" box="[1179,1225,1044,1068]" pageId="1" pageNumber="2" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
; III, 
<number id="410224F6F118B81BFB05C049FAB0FBB8" box="[1277,1349,1044,1068]" pageId="1" pageNumber="2" value="18.5" valueMax="19.0" valueMin="18.0">18–19</number>
; IV, 
<number id="410224F6F118B81BFA80C049FA62FBB8" box="[1400,1431,1044,1068]" pageId="1" pageNumber="2" value="24.0">24</number>
; V, 
<number id="410224F6F118B81BFF4EC065FEF5FBC4" box="[182,256,1080,1104]" pageId="1" pageNumber="2" value="26.5" valueMax="27.0" valueMin="26.0">26–27</number>
