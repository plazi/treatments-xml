<taxonomicName id="30CA4D52F118B81BFBD0C064FB38FBC4" box="[1064,1229,1080,1104]" class="Insecta" family="Menoponidae" genus="Myrsidea" kingdom="Animalia" order="Phthiraptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="ochrolaemi">
