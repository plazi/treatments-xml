<quantity id="30329B34F118B81BFE1AC0B1FDDCFA90" box="[482,553,1260,1284]" metricMagnitude="4" metricUnit="m" metricValue="2.0" pageId="1" pageNumber="2" unit="km" value="20.0">20 km</quantity>
