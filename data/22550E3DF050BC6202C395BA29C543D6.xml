<taxonomicName id="21859275DF6DABB5F22E797F6CF226EE" class="Bryopsida" family="FAMILIA" genus="Hypnum" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Hypnum" order="ORDO" pageId="22" pageNumber="73" phylum="Bryophyta" rank="genus">Hypnum</taxonomicName>
