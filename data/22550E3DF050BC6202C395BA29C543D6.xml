<taxonomicName id="EE59E6EAD295B2113080F7EEC434E2E6" class="Lecanoromycetes" family="FAMILIA" genus="Fellhanera" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Fellhanera duplex" order="ORDO" pageId="22" pageNumber="73" phylum="Ascomycota" rank="species" species="duplex">
<pageBreakToken id="79D8B22D4309DBC9F5144E6A12864258" pageId="22" pageNumber="73" start="start">Fellhanera</pageBreakToken>
