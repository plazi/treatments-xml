<taxonomicName id="B4971FD7FFAEFFF9FB1969E4FAF9F87D" baseAuthorityName="Fabricius" baseAuthorityYear="1781" box="[1187,1305,1958,1982]" class="Insecta" family="Reduviidae" genus="Rhynocoris" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="244" phylum="Arthropoda" rank="species" species="nitidulus">
