<collectionCode id="ED5CAE40A1560465B93E611EFD3E6E49" box="[616,680,272,298]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="23" pageNumber="24">ZMB</collectionCode>
