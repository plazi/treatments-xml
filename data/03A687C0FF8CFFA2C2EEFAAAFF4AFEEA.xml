<quantity id="4CF79B33FF8CFFA5C302FA66FE24FA3F" box="[379,434,1471,1491]" metricMagnitude="2" metricUnit="m" metricValue="7.55" pageId="99" pageNumber="254" unit="m" value="755.0">755m</quantity>
, 8642/4, 
<date id="FFB11016FF8CFFA5C07CFA66FDF8FA3F" box="[517,622,1471,1491]" pageId="99" pageNumber="254" value="2005-05-30">30.05.2005</date>
