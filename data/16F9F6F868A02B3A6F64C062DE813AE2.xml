<taxonomicName id="5A82F5E545687CB8E94CF90D59F760D9" authority="Bourguignat, 1884" authorityName="Bourguignat" authorityYear="1884" class="Gastropoda" family="Melanopsidae" genus="Fagotia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Fagotia" order="Sorbeoconcha" pageId="150" pageNumber="151" phylum="Mollusca" rank="genus">
