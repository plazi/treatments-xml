<taxonomicName id="4C0B4D2AFF5A38039835FE40FCB55DCF" box="[572,807,480,505]" class="Arachnida" family="Diptilomiopidae" genus="Neocatarhinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="42" phylum="Arthropoda" rank="species" species="dendrocalamus" status="sp. nov.">
