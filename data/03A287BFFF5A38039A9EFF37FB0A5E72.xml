<taxonomicName id="4C0B4D2AFF5A38039E9FFE40FAAC5DCF" box="[1174,1342,480,505]" class="Arachnida" family="Diptilomiopidae" genus="Neocatarhinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="42" phylum="Arthropoda" rank="species" species="bambusae">
