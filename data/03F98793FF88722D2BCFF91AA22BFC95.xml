<collectingCountry id="F3477615FF89722D28C0FF40A45BFF15" box="[920,1105,152,178]" name="Pitcairn" pageId="29" pageNumber="30">Pitcairn Islands</collectingCountry>
(
<bibRefCitation id="EFC14B74FF89722D2F3BFF40A4F6FF15" author="Preece" box="[1123,1276,152,178]" pageId="29" pageNumber="56" refString="Preece, R. C. (1995) The composition and relationships of the marine molluscan fauna of the Pitcairn Islands. Biological Journal of the Linnean Society, 56, 339 - 358." type="journal article" year="1995">
Preece 
<number id="3D9824A2FF89722D2FE6FF40A4F6FF15" box="[1214,1276,152,178]" pageId="29" pageNumber="30" value="1995.0">1995</number>
</bibRefCitation>
; 
<bibRefCitation id="EFC14B74FF89722D2E53FF40A121FF7D" author="Edmunds" pageId="29" pageNumber="54" refString="Edmunds, M. &amp; Preece, R. (1996) A collection of opisthobranch mollusks from the Pitcairn Islands. Journal of Conchology, 35, 407 - 420." type="journal article" year="1996">
