<taxonomicName id="4C5459F4485BFFACFDE2FC165A286C92" authorityName="Schülke" authorityYear="2016" box="[563,781,903,924]" class="Insecta" family="Staphylinidae" genus="Carphacis" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="667" phylum="Arthropoda" rank="species" species="rufipennis">
