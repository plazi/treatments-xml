<taxonomicName id="4C3DFFA0FFEF4D11E449ED19FC91FD9D" authorityName="Gordon &amp; Hanley" authorityYear="2014" box="[579,775,515,539]" class="Insecta" family="Glaresidae" genus="Glaresis" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="39" phylum="Arthropoda" rank="species" species="paramendica">
