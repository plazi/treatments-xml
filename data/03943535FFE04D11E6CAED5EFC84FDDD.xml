<collectingRegion id="49F94AC1FFEF4D11E4F0ED3EFCCCFDBD" box="[762,858,547,571]" country="United States of America" name="Arizona" pageId="40" pageNumber="39">Arizona</collectingRegion>
species, 
<taxonomicName id="4C3DFFA0FFEF4D11E5C0ED39FB25FDBD" authority="Warner" authorityName="Warner. AA" authorityYear="1995" box="[970,1203,547,571]" class="Insecta" family="Glaresidae" genus="Glaresis" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="39" phylum="Arthropoda" rank="species" species="gordoni">
<emphasis id="B9495831FFEF4D11E5C0ED39FC49FDBD" box="[970,991,548,571]" italics="true" pageId="40" pageNumber="39">G</emphasis>
