<taxonomicName id="4C45C185FF8EFFA1FF53F8F9FE69D1D0" authority="Davie &amp; Ng, 2007" authorityName="Davie &amp; Ng" authorityYear="2007" box="[151,506,1898,1924]" class="Malacostraca" family="Sesarmidae" genus="Karstarma" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="21" phylum="Arthropoda" rank="genus">
