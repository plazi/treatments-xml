<taxonomicName id="4C595BECFFC0FF9C46B4F1B3FD1F423E" authority="(Fabricius)" baseAuthorityName="Fabricius" baseAuthorityYear="1787" box="[497,759,488,512]" class="Insecta" family="Asilidae" genus="Eccritosia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="barbata">
