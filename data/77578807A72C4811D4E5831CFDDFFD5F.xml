<taxonomicName id="38FE4292A72F4811D65483F9FEE5FDFE" authority="Botosaneanu &amp; Moubayed, 1985" authorityName="Botosaneanu &amp; Moubayed" authorityYear="1985" class="Insecta" family="Hydroptilidae" genus="Hydroptila" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="247" phylum="Arthropoda" rank="species" species="fonsorontina">
