</taxonomicName>
, and 
<taxonomicName id="38FE4292A72F4811D4DF8019FD13FDFF" authority="Malicky, 1998" authorityName="Malicky" authorityYear="1998" box="[386,698,541,565]" class="Insecta" family="Hydroptilidae" genus="Hydroptila" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="247" phylum="Arthropoda" rank="species" species="ernstreichli">
