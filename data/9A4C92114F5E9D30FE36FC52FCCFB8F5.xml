<elevation id="99C8C4344F5E9D30FEBBFABCFEA5BB02" box="[274,363,1354,1378]" metricMagnitude="3" metricUnit="m" metricValue="4.755" pageId="14" pageNumber="1059" unit="m" value="4755.0">4755 m</elevation>
