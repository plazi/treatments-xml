<taxonomicName id="4C194D4CA602E42E3276F9B0FB4B1D12" box="[1089,1205,1567,1593]" class="Magnoliopsida" family="Tiliaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="32" pageNumber="381" phylum="Tracheophyta" rank="family">Tiliaceae</taxonomicName>
)
</emphasis>
