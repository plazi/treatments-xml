; OH, 
<quantity id="4C9B0A9BF278474D1119F82FFC9AFC85" box="[599,771,822,846]" metricMagnitude="-3" metricUnit="m" metricValue="9.605" metricValueMax="14.64" metricValueMin="4.57" pageId="4" pageNumber="297" unit="mm" value="9.605" valueMax="14.64" valueMin="4.57">4.57–14.64 mm</quantity>
; OT, 
<quantity id="4C9B0A9BF278474D13F7F84FFEC0FCA5" box="[185,345,854,878]" metricMagnitude="-3" metricUnit="m" metricValue="3.52" metricValueMax="5.68" metricValueMin="1.36" pageId="4" pageNumber="297" unit="mm" value="3.52" valueMax="5.68" valueMin="1.36">1.36–5.68 mm</quantity>
; OsL, 
<quantity id="4C9B0A9BF278474D12ECF84FFDC9FCA5" box="[418,592,854,878]" metricMagnitude="-3" metricUnit="m" metricValue="6.305" metricValueMax="10.01" metricValueMin="2.6" pageId="4" pageNumber="297" unit="mm" value="6.305" valueMax="10.01" valueMin="2.6">2.60–10.01 mm</quantity>
