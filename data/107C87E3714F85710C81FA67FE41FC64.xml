<taxonomicName id="5FD54D76714E85770D56FA22FE1CF8C4" authority="Cui &amp; Xue &amp; Jiang, 2018" authorityName="Cui &amp; Xue &amp; Jiang" authorityYear="2018" box="[320,443,1802,1824]" class="Insecta" family="Geometridae" genus="Aquilargilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus" status="gen. nov.">
