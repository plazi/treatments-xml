<number id="9310B22DFFEEFFFF88A6FB1CFF57FAFE" box="[222,237,1267,1291]" pageId="9" pageNumber="324" value="2.0">2</number>
</figureCitation>
c, 
<number id="9310B22DFFEEFFFF897FFB1DFEAFFAFE" box="[263,277,1266,1291]" pageId="9" pageNumber="324" value="3.0">3</number>
b, e, f, 
<number id="9310B22DFFEEFFFF891BFB1DFECBFAFE" box="[355,369,1266,1291]" pageId="9" pageNumber="324" value="5.0">5</number>
a, b, 
<number id="9310B22DFFEEFFFF89DFFB1DFE0FFAFE" box="[423,437,1266,1291]" pageId="9" pageNumber="324" value="6.0">6</number>
