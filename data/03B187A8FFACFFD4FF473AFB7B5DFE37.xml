<typeStatus id="54A3881CFFACFFD4FF263C767F8AFACB" box="[140,195,1317,1340]" pageId="42">TYPE</typeStatus>
SPECIES
</collectionCode>
: 
<taxonomicName id="4C184D3DFFACFFD4FE843C767E85FACA" authority="Grimaldi, 2018" authorityName="Grimaldi" authorityYear="2018" box="[302,460,1317,1341]" class="Insecta" family="Platypezidae" genus="Canadopeza" kingdom="Animalia" order="Diptera" pageId="42" phylum="Arthropoda" rank="species" species="biacrosticha" status="new species">
<emphasis id="B96CEAACFFACFFD4FE843C767E85FACA" box="[302,460,1317,1341]" italics="true" pageId="42">C. biacrosticha</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25F57D7FFACFFD4FE793C767D1EFACA" box="[467,599,1317,1341]" pageId="42" rank="species">new species</taxonomicNameLabel>
