<mcCollectionCode id="5E5D537EFFFAFF87331EFC4F3DC3FBEC" box="[580,645,1016,1040]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33074" lsid="urn:lsid:biocol.org:col:33074" name="China Agricultural University" pageId="10" pageNumber="11">CAU</mcCollectionCode>
