<number id="3DBB248CFFEEFF82FB76FEF73AB01C6C" box="[1209,1259,294,319]" pageId="6" pageNumber="187" value="0.9">0.90</number>
/ 
<number id="3DBB248CFFEEFF82FB3BFEF73B7F1C6C" box="[1268,1316,294,319]" pageId="6" pageNumber="187" value="0.61">0.61</number>
