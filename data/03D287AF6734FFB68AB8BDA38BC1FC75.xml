<taxonomicName id="4C7B4D3A6734FFB68AB8BDA38835FF35" box="[151,320,151,177]" class="Insecta" family="Pentatomidae" genus="Pentamyrmex" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="596" phylum="Arthropoda" rank="genus" status="new genus">Pentamyrmex</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A23C57D06734FFB68B63BDAC88B8FF36" box="[332,461,152,178]" pageId="1" pageNumber="596" rank="genus">new genus</taxonomicNameLabel>
