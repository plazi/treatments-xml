<taxonomicName id="2DCD9BDB7D488A17E900FCE9FD0DFCDA" authority="Krauss, 1890" authorityName="Krauss" authorityYear="1890" box="[291,705,870,892]" class="Insecta" family="Phaneropteridae" genus="Eurycorypha" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="aequatorialis">
