<taxonomicName id="2DCD9BDB7D488A17EAB8FC90FB7CFC9C" authority="Brunner von Wattenwyl 1878" authorityName="Brunner von Wattenwyl" authorityYear="1878" box="[667,1200,799,826]" class="Insecta" family="Phaneropteridae" genus="Eurycorypha" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="securifera">
