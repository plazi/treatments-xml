<emphasis id="B94BB114BF20C22CCC7AB742E5431DE1" bold="true" pageId="47" pageNumber="200">
<taxonomicName id="4C3F1685BF20C22CCC7AB742E6A81DFF" authority="E.M.FRIIS, P.R.CRANE et K.R.PEDERSEN" authorityName="E. M. FRIIS, P. R. CRANE et K. R. PEDERSEN" authorityYear="2019" class="Amphibia" family="Rhacophoridae" genus="Burgeria" kingdom="Animalia" order="Anura" pageId="47" pageNumber="200" phylum="Chordata" rank="genus" status="gen. nov.">
