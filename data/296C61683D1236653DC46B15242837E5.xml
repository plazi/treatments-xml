<taxonomicName id="66C5ABFD3D1236643FAD6AA02408359F" authority="Krober (1928)" authorityName="Krober" authorityYear="1928" box="[766,1038,1922,1945]" class="Insecta" family="Sphaeroceridae" genus="Leptocera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="43" phylum="Arthropoda" rank="genus">
<emphasis id="93B10C6C3D1236643FAD6AA0236C359F" box="[766,874,1922,1945]" italics="true" pageId="2" pageNumber="43">Leptocera</emphasis>
<bibRefCitation id="C554AD8F3D1236643E216AA02408359F" author="Krober" box="[882,1038,1922,1945]" pageId="2" pageNumber="66" refString="Krober, O. (1928) Neue und wenig bekannte Dipteren aus den Familien Omphralidae, Conopidae und Therevidae. Konowia, 7, 113 - 134." type="journal article" year="1928">
Kröber (
<number id="170DC2593D1236643E9E6AA02400359F" box="[973,1030,1922,1945]" pageId="2" pageNumber="43" value="1928.0">1928</number>
)
