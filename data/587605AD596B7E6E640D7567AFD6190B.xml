<taxonomicName id="396299CC7F66B4DBC54D04DF87B4707F" class="Insecta" family="Noctuidae" genus="Aseptis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aseptis pseudolichena" order="Lepidoptera" pageId="14" pageNumber="71" phylum="Arthropoda" rank="species" species="pseudolichena">
