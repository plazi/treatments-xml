<quantity id="931438E46E11FFC408154CF4FB26FB66" box="[1056,1218,1164,1188]" metricMagnitude="-3" metricUnit="m" metricValue="3.8499999999999996" metricValueMax="3.9" metricValueMin="3.8" pageId="6" pageNumber="53" unit="mm" value="3.8499999999999996" valueMax="3.9" valueMin="3.8">3.80–3.90 mm</quantity>
in 
