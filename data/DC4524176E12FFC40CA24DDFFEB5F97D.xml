(
<quantity id="931438E46E11FFC40DBD4CD7FDC8FB05" box="[392,556,1199,1223]" metricMagnitude="-3" metricUnit="m" metricValue="2.06" metricValueMax="2.18" metricValueMin="1.94" pageId="6" pageNumber="53" unit="mm" value="2.06" valueMax="2.18" valueMin="1.94">1.94–2.18 mm</quantity>
in 
<number id="E22487266E11FFC40E7B4CC8FD9CFB0A" box="[590,632,1200,1224]" pageId="6" pageNumber="53" value="33.0">33</number>
, 
