elongate, parallel-sided, convex; length, 
<quantity id="C8739B6BFF806C5E6D02F97CFC70F949" box="[798,901,1678,1702]" metricMagnitude="-3" metricUnit="m" metricValue="2.12" pageId="10" pageNumber="9" unit="mm" value="2.12">2.12mm</quantity>
