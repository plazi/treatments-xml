<taxonomicName id="AF741AAB5F9368FB9914A58B616CED9A" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="456" phylum="Tracheophyta" rank="species" species="solstitialis">
