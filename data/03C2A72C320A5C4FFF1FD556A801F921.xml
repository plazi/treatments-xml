<taxonomicName id="4C6B6DB9320A5C4FFD0BD5D6AFE8FB61" authority="" box="[724,928,1250,1274]" class="Magnoliopsida" family="Sapindaceae" genus="Litchi" kingdom="Plantae" order="Sapindales" pageId="18" pageNumber="17" phylum="Tracheophyta" rank="subGenus" subGenus="German">Litchi (German)</taxonomicName>
