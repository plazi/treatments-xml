<taxonomicName id="01548D02B449425B500B23B070121F22" authority="Walckenaer, 1805" authorityName="Walckenaer" authorityYear="1805" class="Arachnida" family="Araneidae" genus="Epeira" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epeira hamiltoni" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="hamiltoni">Epeira hamiltoni</taxonomicName>
