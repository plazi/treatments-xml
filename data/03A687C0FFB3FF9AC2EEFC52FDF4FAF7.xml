<quantity id="4CF79B33FFB3FF9AC086FC65FCA3FC3C" box="[767,821,956,976]" metricMagnitude="2" metricUnit="m" metricValue="7.15" pageId="92" pageNumber="247" unit="m" value="715.0">715m</quantity>
