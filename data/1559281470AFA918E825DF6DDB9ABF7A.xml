(1823
</bibRefCitation>
: pl. 7, fig. 7) as &quot; 
<taxonomicName id="BFC9E5BEBB164AECC8B28A4C958C4780" baseAuthorityName="Olivier" baseAuthorityYear="1801" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis buccinoidea" order="Pteropoda" pageId="256" pageNumber="257" phylum="Mollusca" rank="species" species="buccinoidea">
<emphasis id="E34D8CFFCD6C95F679C3613F038F69DE" italics="true" pageId="256" pageNumber="257">Melanopsis buccinoidea</emphasis>
</taxonomicName>
var. 
<normalizedToken id="23469515125955F28A7DA5030C34F586" originalValue="γ">γ</normalizedToken>
) 
<taxonomicName id="C3D28B0E9876C346E5CDA15E13C71B08" authorityName="Pallary" authorityYear="1916" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="antiqua" order="Pteropoda" pageId="256" pageNumber="257" phylum="Mollusca" rank="species" species="antiqua">antiqua</taxonomicName>
; 
<taxonomicName id="43206928FB2B8B52F22DB11394913D48" authorityName="Doncieux" authorityYear="1908" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="elongata" order="Pteropoda" pageId="256" pageNumber="257" phylum="Mollusca" rank="species" species="elongata">
