Genus 
<taxonomicName id="79703B3647CE86C95C7879B948BF7105" LSID="1BDAB814-1028-5CFC-88F9-D77836860820" authority="Szepligeti, 1900" authorityName="Szepligeti" authorityYear="1900" class="Insecta" family="Braconidae" genus="Campyloneurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Campyloneurus" order="Hymenoptera" pageId="0" pageNumber="21" phylum="Arthropoda" rank="genus">
