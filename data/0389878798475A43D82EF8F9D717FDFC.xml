<paragraph id="8B9F369198475A43DADCF85AD717FDFC" blockId="91.[189,657,2001,2027]" lastBlockId="93.[189,1399,267,541]" lastPageId="93" lastPageNumber="94" pageId="91" pageNumber="92">
<subSubSection id="C33A651A98475A45DADCF85AD084F80B" box="[189,657,2001,2027]" pageId="91" pageNumber="92" type="reference_group">
<treatmentCitationGroup id="AB3011BF98475A45DADCF85AD084F80B" box="[189,657,2001,2027]" pageId="91" pageNumber="92">
<treatmentCitation id="0A81108098475A45DADCF85AD09BF80B" author="Setzer H. W." box="[189,654,2001,2027]" page="260" pageId="91" pageNumber="92" year="1971">
