<taxonomicName id="4C154D1FE86BFFC97A31FD86FEC7FDC1" authorityName="Ansari" authorityYear="1968" class="Insecta" family="Philopteridae" genus="Sturnidoecus" kingdom="Animalia" order="Phthiraptera" pageId="41" pageNumber="442" phylum="Arthropoda" rank="species" species="zahrae">
