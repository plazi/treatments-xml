, 
<collectorName id="26CF5376FFA415687D76FA95FC38FA5B" box="[810,927,1404,1424]" pageId="9" pageNumber="616">P. Pacholátko</collectorName>
leg., 
<date id="FF841060FFA415687D8EFA94FBE7FA5B" box="[978,1088,1404,1423]" pageId="9" pageNumber="616" value="2005-05-05" valueMax="2005-05-24" valueMin="2005-05-05">
<collectingDate id="EFC0E988FFA415687D8EFA94FBE7FA5B" box="[978,1088,1404,1423]" pageId="9" pageNumber="616" value="2005-05-05" valueMax="2005-05-24" valueMin="2005-05-05">5.–24.v.2005</collectingDate>
