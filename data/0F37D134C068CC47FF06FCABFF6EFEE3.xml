<date id="F32046E2C068CC46FC21FEC4F97AF93B" box="[944,1062,1728,1754]" pageId="2" pageNumber="3" value="1391-06-20">20061391</date>
–A 
<date id="F32046E2C068CC46FBDBFEC4F99EF93B" box="[1098,1218,1728,1754]" pageId="2" pageNumber="3" value="1407-06-20">20061407</date>
