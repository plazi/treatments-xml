<number id="0E687725FFE0FFAB39B6FADDFF27A704" box="[151,182,1324,1350]" pageId="25" pageNumber="126" value="21.0">21</number>
. 
<taxonomicName id="7FA01E81FFE0FFAB39E2FADDFC9FA704" authority="Saylor, 1937" authorityName="Saylor" authorityYear="1937" box="[195,782,1324,1350]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="126" phylum="Arthropoda" rank="species" species="yucana" subGenus="Phyllophaga">
