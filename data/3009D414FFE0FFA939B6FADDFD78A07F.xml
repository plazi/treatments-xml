<taxonomicName id="7FA01E81FFE3FFA8387AF8C2FDBCA50B" box="[347,557,1843,1865]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="yucana">
<emphasis id="8AD4B910FFE3FFA8387AF8C2FDBCA50B" box="[347,557,1843,1865]" italics="true" pageId="26" pageNumber="127">Phyllophaga yucana</emphasis>
