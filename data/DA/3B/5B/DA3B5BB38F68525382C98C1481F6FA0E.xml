<emphasis italics="true" pageId="48" pageNumber="49">Xylopia aurantiiodora</emphasis>
</taxonomicName>
, 
<taxonomicName family="Annonaceae" genus="X." kingdom="Plantae" lsidName="X. congolensis" order="Magnoliales" pageId="48" pageNumber="49" rank="species" species="congolensis">
