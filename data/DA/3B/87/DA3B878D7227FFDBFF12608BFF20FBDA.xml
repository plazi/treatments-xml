<quantity box="[1106,1245,829,854]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" metricValueMax="3.6" metricValueMin="3.2" pageId="30" pageNumber="31" unit="mm" value="3.4" valueMax="3.6" valueMin="3.2">3.2–3.6 mm</quantity>
; wingspan about 
<quantity box="[151,289,865,890]" metricMagnitude="-3" metricUnit="m" metricValue="7.25" metricValueMax="7.6" metricValueMin="6.9" pageId="30" pageNumber="31" unit="mm" value="7.25" valueMax="7.6" valueMin="6.9">6.9–7.6 mm</quantity>
