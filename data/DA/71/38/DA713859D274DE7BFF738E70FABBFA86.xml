<subSubSection pageId="21" pageNumber="22" type="materials_examined">
<paragraph blockId="21.[151,1437,1450,2015]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[151,329,1450,1475]" pageId="21" pageNumber="22">
<typeStatus box="[151,210,1451,1475]" pageId="21" pageNumber="22">Type</typeStatus>
material:
</emphasis>
<typeStatus box="[338,445,1450,1474]" pageId="21" pageNumber="22">Holotype</typeStatus>
