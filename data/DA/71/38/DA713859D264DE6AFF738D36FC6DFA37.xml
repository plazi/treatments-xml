<subSubSection pageId="7" pageNumber="8" type="distribution">
<paragraph blockId="7.[151,1437,151,1508]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[199,352,655,680]" pageId="7" pageNumber="8">Distribution.</emphasis>
Tropical Africa, 
<collectingCountry box="[553,690,655,679]" name="Madagascar" pageId="7" pageNumber="8">Madagascar</collectingCountry>
, the 
<collectingCountry box="[747,850,655,679]" name="Comoros" pageId="7" pageNumber="8">Comoros</collectingCountry>
islands, 
<collectingCountry box="[954,1012,655,679]" name="India" pageId="7" pageNumber="8">India</collectingCountry>
