<paragraph blockId="46.[408,730,1239,1296]" box="[408,730,1271,1296]" pageId="46" pageNumber="1057">
(®gures 2c, 
<quantity box="[539,561,1271,1295]" metricMagnitude="3" metricUnit="kg" metricValue="3.0" pageId="46" pageNumber="1057" unit="t" value="3.0">3t</quantity>
