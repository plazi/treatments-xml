<taxonomicName class="Pinopsida" family="Pinaceae" genus="Pinus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Pinales" pageId="3" pageNumber="58" phylum="Coniferophyta" rank="species" species="resinosa">
