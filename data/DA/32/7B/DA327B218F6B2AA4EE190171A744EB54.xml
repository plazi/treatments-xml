<taxonomicName authorityName="Kimsey" authorityYear="2011" class="Hexapoda" family="Tiphiidae" genus="Mesa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="8" pageNumber="53" phylum="Arthropoda" rank="genus">
