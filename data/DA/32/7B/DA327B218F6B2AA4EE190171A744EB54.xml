</taxonomicName>
<emphasis bold="true" pageId="8" pageNumber="53">5</emphasis>
<taxonomicName authorityName="Kimsey" authorityYear="2011" class="Hexapoda" family="Tiphiidae" genus="Meria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="8" pageNumber="53" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="8" pageNumber="53">Meria</emphasis>
</taxonomicName>
<emphasis bold="true" pageId="8" pageNumber="53">6</emphasis>
<taxonomicName authorityName="Guiglia" authorityYear="1959" class="Insecta" family="Tiphiidae" genus="Myzinella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="8" pageNumber="53" phylum="Arthropoda" rank="genus">
