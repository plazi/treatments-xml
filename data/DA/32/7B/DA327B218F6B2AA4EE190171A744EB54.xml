</taxonomicName>
<emphasis bold="true" pageId="8" pageNumber="53">3</emphasis>
<taxonomicName authorityName="Kimsey" authorityYear="2011" class="Hexapoda" family="Tiphiidae" genus="Tiphia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="8" pageNumber="53" phylum="Arthropoda" rank="genus">
