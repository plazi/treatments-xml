<taxonomicName authorityName="Lamarck" authorityYear="1818" box="[281,336,1411,1433]" class="Polychaeta" family="Syllidae" genus="Syllis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="15" pageNumber="212" phylum="Annelida" rank="genus">
