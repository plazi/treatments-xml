<subSubSection pageId="0" pageNumber="1" type="new records">
<paragraph pageId="0" pageNumber="1">New records.</paragraph>
<paragraph pageId="0" pageNumber="1">
Israel • 1 spcm; Palmachim; 
<geoCoordinate degrees="31.9292" direction="north" orientation="latitude" precision="5" value="31.9292">31.9292°N</geoCoordinate>
