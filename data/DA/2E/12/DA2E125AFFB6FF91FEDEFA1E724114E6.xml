<collectingRegion box="[633,836,188,213]" country="China" name="Yunnan" pageId="2" pageNumber="117">Yunnan Province</collectingRegion>
, 
<collectingCountry box="[849,918,188,213]" name="China" pageId="2" pageNumber="117">China</collectingCountry>
(
<figureCitation box="[935,1008,188,213]" captionStart="FIGURE 2" captionStartId="2.[151,244,1081,1102]" captionTargetBox="[264,1301,309,1048]" captionTargetId="figure-324@2.[264,1323,303,1056]" captionTargetPageId="2" captionText="FIGURE 2. Distribution map of Coelogyne pianmaensis in Yunnan, China." figureDoi="http://doi.org/10.5281/zenodo.5132032" httpUri="https://zenodo.org/record/5132032/files/figure.png" pageId="2" pageNumber="117">Fig. 2</figureCitation>
