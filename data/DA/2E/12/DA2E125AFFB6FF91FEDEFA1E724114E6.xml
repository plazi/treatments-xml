<quantity box="[992,1161,223,248]" metricMagnitude="3" metricUnit="m" metricValue="2.8649999999999998" metricValueMax="3.13" metricValueMin="2.6" pageId="2" pageNumber="117" unit="m" value="2865.0" valueMax="3130.0" valueMin="2600.0">
