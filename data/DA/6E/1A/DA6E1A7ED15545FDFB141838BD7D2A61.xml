<subSubSection pageId="0" pageNumber="63" type="materials_examined">
<paragraph pageId="0" pageNumber="63">Type.</paragraph>
<paragraph pageId="0" pageNumber="63">
CHINA. Yunnan: Qujing City, Luliang county, 
<geoCoordinate direction="north" orientation="latitude" precision="15" value="25.090279">25°05'25&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="15" value="103.945">103°56'42&quot;E</geoCoordinate>
