(
<figureCitation box="[159,273,330,355]" captionStart="FIGURES 7 – 12" captionStartId="4.[151,264,1938,1961]" captionTargetBox="[188,1393,999,1904]" captionTargetId="figure@4.[188,1398,993,1915]" captionTargetPageId="4" captionText="FIGURES 7 – 12. Sympycnus ferganicus sp. nov., male holotype. 7, habitus; 8, head, dorsolateral view; 9, wing; 10, hind tarsus, dorsolateral view; 11, hypopygium, left lateral view; 12, hypopygium, ventral view. Abbreviations: apv lobe = apicoventral epandrial lobe, cer = cercus, ep = epandrium, hyp = hypandrium, ph = phallus, pgt = postgonite, sur = surstylus." httpUri="https://zenodo.org/record/814625/files/figure.png" pageId="3" pageNumber="534">Figs 7–12</figureCitation>
)
