<taxonomicName class="Insecta" family="Staphylinidae" genus="Zyras" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zyras" order="Coleoptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="genus">Zyras</taxonomicName>
