<caption httpUri="https://zenodo.org/record/583877/files/figure.png" pageId="5" pageNumber="256" targetBox="[173,1410,589,1372]" targetPageId="5">
<paragraph blockId="5.[151,1436,1397,1450]" pageId="5" pageNumber="256">
<emphasis bold="true" box="[151,275,1397,1419]" pageId="5" pageNumber="256">FIGURE 9.</emphasis>
<taxonomicName box="[281,579,1397,1418]" class="Insecta" family="Corduliidae" genus="Navicordulia" kingdom="Animalia" order="Odonata" pageId="5" pageNumber="256" phylum="Arthropoda" rank="species" species="tumucurakensis" status="sp. nov.">
<emphasis box="[281,579,1397,1418]" italics="true" pageId="5" pageNumber="256">Navicordulia tumucurakensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[586,666,1397,1418]" pageId="5" pageNumber="256" rank="species">
