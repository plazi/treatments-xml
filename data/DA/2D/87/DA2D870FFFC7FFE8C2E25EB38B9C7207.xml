<number box="[1055,1107,1200,1224]" pageId="6" pageNumber="66" value="0.1">0.10</number>
; AME-ALE 
<number box="[1255,1306,1200,1224]" pageId="6" pageNumber="66" value="0.2">0.20</number>
, PME-PLE 
<number box="[151,202,1236,1260]" pageId="6" pageNumber="66" value="0.22">0.22</number>
