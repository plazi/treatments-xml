<quantity id="4CEF9B25FFEDFFC99BD7FEF1FE463118" box="[938,1031,259,285]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="124" pageNumber="125" unit="m" value="1100.0">1,100 m</quantity>
(
