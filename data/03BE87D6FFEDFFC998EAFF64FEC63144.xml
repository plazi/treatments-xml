<quantity id="4CEF9B25FFEDFFC99CF3FF2DFEB630FC" box="[1166,1271,223,249]" metricMagnitude="2" metricUnit="m" metricValue="2.11" metricValueMax="4.1899999999999995" metricValueMin="0.03" pageId="124" pageNumber="125" unit="m" value="211.0" valueMax="419.0" valueMin="3.0">3–419 m</quantity>
