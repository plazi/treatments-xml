<taxonomicName id="46813EECFFE57E29AAAD218AFD5107C7" authority="Heiss &amp; B, 2015" authorityName="Heiss &amp; B" authorityYear="2015" box="[468,714,1474,1495]" class="Insecta" family="Thaumastocoridae" genus="Thaumastotingis" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="534" phylum="Arthropoda" rank="species" species="areolatus" status="nov.gen., nov.sp.">
