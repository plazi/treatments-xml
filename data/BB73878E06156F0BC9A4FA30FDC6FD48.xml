<collectorName id="9E2F534E06136F0BCB1EFEB2FDE9FED0" box="[557,622,259,285]" pageId="4" pageNumber="395">Three</collectorName>
<typeStatus id="EC61883A06136F0BCB45FEB2FD66FED0" box="[630,737,259,285]" pageId="4" pageNumber="395">paratypes</typeStatus>
of the same 
<typeStatus id="EC61883A06136F0BCA43FEB2FC55FED0" box="[880,978,259,285]" pageId="4" pageNumber="395">holotype</typeStatus>
