<subSubSection id="CEF4308FEB533066A0F6CB5638FED9E0" pageId="2" pageNumber="301" type="reference_group">
<paragraph id="E82A18C1A35FE6751D379F3340690A9B" pageId="2" pageNumber="301">
<taxonomicName id="CFED4101830EFB9C03CD890B28F54EF3" class="Insecta" family="Ptychopteridae" genus="Ptychoptera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ptychoptera deleta" order="Diptera" pageId="2" pageNumber="301" phylum="Arthropoda" rank="species" species="deleta">Ptychoptera deleta</taxonomicName>
