(&quot; 
<taxonomicName id="9D47ABF3CD66FFC81D3318178F1F88A6" authority="Handlirsch, 1909" authorityName="Handlirsch" authorityYear="1909" class="Insecta" family="Ptychopteridae" genus="Ptychopterula" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ptychopterula" order="Diptera" pageId="2" pageNumber="301" phylum="Arthropoda" rank="genus">Ptychopterula</taxonomicName>
