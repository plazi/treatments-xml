(RO) and 
<collectingRegion id="0A10F85AC821442ADDA7FF08603E29BF" box="[643,809,223,248]" country="France" name="Guyane" pageId="6" pageNumber="457">
<collectingCountry id="B0C37628C821442ADDA7FF08603E29BF" box="[643,809,223,248]" name="French Guiana" pageId="6" pageNumber="457">French Guiana</collectingCountry>
