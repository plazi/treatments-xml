<taxonomicName id="4C7522ACFF8BCC4CA6CABF6FFEAAF9F3" authority="Hedwig" authorityName="Hedwig" authorityYear="1957" class="Insecta" family="Ichneumonidae" genus="Ophion" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="180" phylum="Arthropoda" rank="species" species="fossulatus">
