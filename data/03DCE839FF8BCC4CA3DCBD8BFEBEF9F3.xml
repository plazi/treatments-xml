<taxonomicName id="4C7522ACFF8BCC4CA126BC83FD30F9A8" box="[621,758,1523,1550]" class="Insecta" family="Ichneumonidae" genus="Ophion" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="180" phylum="Arthropoda" rank="species" species="parvulus">
