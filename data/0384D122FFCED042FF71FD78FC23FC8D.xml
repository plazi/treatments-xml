<heading id="D0DAD758FFCED041FF71FD78FE18FD23" bold="true" box="[151,491,738,764]" fontSize="11" level="1" pageId="1" pageNumber="60" reason="1">
<treatmentCitationGroup id="AB3D471AFFCED041FF71FD78FE18FD23" box="[151,491,738,764]" pageId="1" pageNumber="60">
<taxonomicName id="4C2D1BB7FFCED041FF71FD78FE18FD23" authority="Gill, 1862: 236" authorityName="Gill" authorityPageNumber="236" authorityYear="1862" box="[151,491,738,764]" class="Actinopterygii" family="Serranidae" genus="Acanthistius" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="60" phylum="Chordata" rank="genus">
