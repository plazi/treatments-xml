<taxonomicName id="2A09175BC4EDD0D2B4D0860234369492" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2414" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus Mayr" order="Hymenoptera" pageNumber="14" phylum="Arthropoda" rank="genus">CAMPONOTUS</taxonomicName>
