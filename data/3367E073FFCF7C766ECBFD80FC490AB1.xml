species:
</emphasis>
<taxonomicName id="7CCE2AE6FFCF7C766F6FFD38FD190D99" authority="Karsch, 1896" authorityName="Karsch" authorityYear="1896" box="[307,661,660,684]" class="Insecta" family="Phaneropteridae" genus="Plangia" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="198" phylum="Arthropoda" rank="species" species="laminifera">
<emphasis id="89BA8D77FFCF7C766F6FFD38FE710D99" box="[307,509,660,684]" italics="true" pageId="1" pageNumber="198">Plangia laminifera</emphasis>
<bibRefCitation id="DF5F2C94FFCF7C766C58FD39FD190D99" author="Karsch" box="[516,661,661,684]" pageId="1" pageNumber="198" refString="Karsch, F. (1896) Neue Orthopteren aus dem tropischen Afrika. Stettiner entomologische Zeitung, 57, 242 - 359. http: // www. biodiversitylibrary. org / item / 36013  page / 250 / mode / 1 up." type="journal article" year="1896">
Karsch, 
<number id="0D064342FFCF7C766C01FD39FD190D99" box="[605,661,661,684]" pageId="1" pageNumber="198" value="1896.0">1896</number>
</bibRefCitation>
