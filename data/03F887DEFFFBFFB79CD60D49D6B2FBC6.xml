<paragraph id="8BEE36C8FFF8FFB79C860D9ED53FFCEE" blockId="14.[151,1436,151,1040]" pageId="14" pageNumber="15">
<emphasis id="B925EADAFFF8FFB79C860D9ED602FD26" bold="true" box="[199,315,727,752]" pageId="14" pageNumber="15">Remarks.</emphasis>
<taxonomicName id="4C514D4BFFF8FFB79D020D9ED526FD26" box="[323,543,727,752]" class="Hydrozoa" family="Bougainvilliidae" genus="Pachycordyle" kingdom="Animalia" order="Anthoathecata" pageId="14" pageNumber="15" phylum="Cnidaria" rank="species" species="navis">
<emphasis id="B925EADAFFF8FFB79D020D9ED526FD26" box="[323,543,727,752]" italics="true" pageId="14" pageNumber="15">Pachycordyle navis</emphasis>
</taxonomicName>
