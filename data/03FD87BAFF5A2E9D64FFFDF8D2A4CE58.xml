<taxonomicName id="4C544D2FFF5A2E9D6551FD73D0DEC8D4" authority="Gerken, 2010" authorityName="Gerken" authorityYear="2010" box="[313,657,714,739]" class="Malacostraca" family="Lampropidae" genus="Watlingia" kingdom="Animalia" order="Cumacea" pageId="182" pageNumber="183" phylum="Arthropoda" rank="species" species="cassis">
