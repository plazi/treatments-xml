<quantity id="4CCE9B62FFBCFF89E58CFC0AFDF3FE83" box="[435,573,979,1003]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" metricValueMax="9.1" metricValueMin="8.9" pageId="7" pageNumber="120" unit="mm" value="9.0" valueMax="9.1" valueMin="8.9">8.9–9.1 mm</quantity>
, EL 8.0 mm, WH 
<quantity id="4CCE9B62FFBCFF89E729FC0AFCA1FE83" box="[790,879,979,1003]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="7" pageNumber="120" unit="mm" value="2.2">2.2 mm</quantity>
, PL 
<quantity id="4CCE9B62FFBCFF89E798FC0AFBFCFE83" box="[935,1074,979,1003]" metricMagnitude="-3" metricUnit="m" metricValue="1.25" metricValueMax="1.3" metricValueMin="1.2" pageId="7" pageNumber="120" unit="mm" value="1.25" valueMax="1.3" valueMin="1.2">1.2–1.3 mm</quantity>
, PW 
<quantity id="4CCE9B62FFBCFF89E04DFC0AFB33FE83" box="[1138,1277,979,1003]" metricMagnitude="-3" metricUnit="m" metricValue="1.85" metricValueMax="1.9" metricValueMin="1.8" pageId="7" pageNumber="120" unit="mm" value="1.85" valueMax="1.9" valueMin="1.8">1.8–1.9 mm</quantity>
, Ediam 
<quantity id="4CCE9B62FFBCFF89E162FC0BFF33F967" metricMagnitude="-4" metricUnit="m" metricValue="5.6" metricValueMax="6.0" metricValueMin="5.2" pageId="7" pageNumber="120" unit="mm" value="0.56" valueMax="0.6" valueMin="0.52">0.52– 0.60 mm</quantity>
, Edist 
<quantity id="4CCE9B62FFBCFF89E576FC2FFE21F967" box="[329,495,1014,1039]" metricMagnitude="-4" metricUnit="m" metricValue="6.3" metricValueMax="6.4" metricValueMin="6.2" pageId="7" pageNumber="120" unit="mm" value="0.63" valueMax="0.64" valueMin="0.62">0.62–0.64 mm</quantity>
.
</paragraph>
