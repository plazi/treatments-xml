<typeStatus id="54A48856156C2A2ED4C9FEF04B35FE8E" box="[431,512,264,284]" pageId="9" pageNumber="736">Holotype</typeStatus>
, 
<collectingRegion id="49DBF816156C2A2ED74AFEF04B51FE8E" box="[556,612,264,284]" country="Malaysia" name="Sabah" pageId="9" pageNumber="736">Sabah</collectingRegion>
, Danum Valley, B.R.L., f.i.t., 
<date id="FFA11034156C2A2ED6E4FEF04ACEFE8E" box="[898,1019,264,284]" pageId="9" pageNumber="736" value="2007-02-14" valueMax="2007-02-16" valueMin="2007-02-14">14-16.II.2007</date>
