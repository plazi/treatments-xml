<taxonomicName id="A2CD4D3710782B1E80CC8A0E21003A41" authority="(Hopkins)" baseAuthorityName="Hopkins" baseAuthorityYear="1915" box="[198,600,549,576]" class="Insecta" family="Curculionidae" genus="Hypothenemus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="179" phylum="Arthropoda" rank="species" species="brunneus">
<emphasis id="57B9EAA610782B1E80CC8A0E22833A3E" box="[198,475,549,575]" italics="true" pageId="8" pageNumber="179">Hypothenemus brunneus</emphasis>
