<taxonomicName id="4C208CE56166FFDFFDCFC4A336D1968E" box="[610,743,1153,1178]" class="Insecta" family="Thripidae" genus="Sericothrips" kingdom="Animalia" order="Thysanoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="basilaris">
