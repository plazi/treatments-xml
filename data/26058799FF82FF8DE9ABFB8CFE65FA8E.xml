<taxonomicName id="69AC4D0CFF82FF8DEDAFFB37FED0FB6A" authority="Peckham &amp; Peckham, 1885" authorityName="Peckham &amp; Peckham" authorityYear="1885" class="Arachnida" family="Salticidae" genus="Titanattus" kingdom="Animalia" order="Araneae" pageId="29" pageNumber="430" phylum="Arthropoda" rank="genus">
