Genre 
<taxonomicName id="FC42B6B9486760803A2DEBDF3682E1CA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2431" class="Insecta" family="Formicidae" genus="Cerapachys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cerapachys Smith" order="Hymenoptera" pageNumber="243" phylum="Arthropoda" rank="genus">CERAPACHYS</taxonomicName>
