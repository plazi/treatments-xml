<collectingCountry id="FFF0763D7B2CFFA0FDD5FDC1A46DFD61" box="[608,795,620,645]" name="Solomon Islands" pageId="29" pageNumber="268">Solomon Islands</collectingCountry>
in 
<quantity id="401F9B487B2CFFA0FC8AFDC1A4B7FD60" box="[831,961,620,644]" metricMagnitude="2" metricUnit="m" metricValue="3.82" metricValueMax="3.8299999999999996" metricValueMin="3.81" pageId="29" pageNumber="268" unit="m" value="382.0" valueMax="383.0" valueMin="381.0">
381– 
<specimenCount id="91E1FD247B2CFFA0FCC2FDC1A4B7FD60" box="[887,961,620,644]" pageId="29" pageNumber="268" type="generic">383 m</specimenCount>
</quantity>
