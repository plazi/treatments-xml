<taxonomicName id="40E74D2E7B2DFFA1FD25F898A479F8AF" box="[656,783,1845,1867]" class="Gastropoda" family="Turridae" genus="Vaughanites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="28" pageNumber="267" phylum="Mollusca" rank="genus">Vaughanites</taxonomicName>
