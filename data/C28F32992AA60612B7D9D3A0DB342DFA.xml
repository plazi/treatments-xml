spp. 13-14 
<taxonomicName id="61C25A0403ACC77E8F9DED050868C4E8" class="Insecta" family="Bolboceratidae" genus="Bolbochromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bolbochromus minutus" order="Coleoptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="minutus">Bolbochromus minutus</taxonomicName>
sp. n. 15-16 
<taxonomicName id="6AF386834A8EED8F038952DA285CDBBE" class="Insecta" family="Bolboceratidae" genus="Bolbochromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bolbochromus nomurai" order="Coleoptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="nomurai">Bolbochromus nomurai</taxonomicName>
sp. n. 17-18 
<taxonomicName id="006FA82D55DEC63B710C476E3839ECC4" class="Insecta" family="Bolboceratidae" genus="Bolbochromus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bolbochromus malayensis" order="Coleoptera" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="malayensis">Bolbochromus malayensis</taxonomicName>
