<quantity id="4C9988D1FFB2DD4B34C7FC5BFF42FC42" box="[151,240,952,978]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="99" pageNumber="100" unit="m" value="1000.0">1000 m</quantity>
