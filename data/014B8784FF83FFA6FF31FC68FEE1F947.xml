<collectingCountry id="F1F57602FF82FFA1FD30FD07FD79FD71" box="[653,754,667,689]" name="Guatemala" pageId="3" pageNumber="32">Guatemala</collectingCountry>
(
<date id="FD5C1052FF82FFA1FD43FD07FCC6FD71" box="[766,845,667,689]" pageId="3" pageNumber="32" value="2007-05">V. 2007</date>
