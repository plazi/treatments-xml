<taxonomicName id="4C6D4D5EEEE13B85FEFFF8BDFD9EF84C" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[314,603,1907,1933]" class="Maxillopoda" family="Notodelphyidae" genus="Thoracodelphys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="432" pageNumber="433" phylum="Arthropoda" rank="species" species="depressa" status="sp. nov.">
