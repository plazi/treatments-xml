<taxonomicName id="4C5D70FDFFD2FF8CFAE050F645B9FC63" box="[1355,1440,969,994]" class="Magnoliopsida" family="Fabaceae" genus="Retama" kingdom="Plantae" order="Fabales" pageId="9" pageNumber="8" phylum="Tracheophyta" rank="genus">
