<paragraph id="8BBE6815D23A0274FC95FE0B548D3115" blockId="53.[828,1423,509,535]" box="[828,1423,509,535]" pageId="53" pageNumber="2835">
Basonym: 
<taxonomicName id="4C011396D23A0274FC02FE0B543C3109" authority="Tohno et al. 2013" authorityName="Tohno" authorityYear="2013" box="[939,1342,511,535]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="53" pageNumber="2835" phylum="Firmicutes" rank="species" species="oryzae">
