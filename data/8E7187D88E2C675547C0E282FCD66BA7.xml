<typeStatus id="D963886C8E2C675547C0E34AFF2E6878" box="[151,200,1338,1359]" pageId="19" pageNumber="20">Type</typeStatus>
species: 
<taxonomicName id="C1D84D4D8E2C67554671E349FD066878" authority="A. E. Verrill, 1872" authorityName="A. E. Verrill" authorityYear="1872" box="[294,736,1337,1359]" class="Gastropoda" family="Raphitomidae" genus="Pleurotomella" kingdom="Animalia" order="Neogastropoda" pageId="19" pageNumber="20" phylum="Mollusca" rank="species" species="packardii">
<emphasis id="34ACEADC8E2C67554671E349FDFA6878" box="[294,540,1337,1359]" italics="true" pageId="19" pageNumber="20">Pleurotomella packardii</emphasis>
<bibRefCitation id="62494B3F8E2C67554574E34AFD066878" author="Verrill" box="[547,736,1337,1359]" pageId="19" pageNumber="20" refString="Verrill, A. E. (1872) Brief contributions to zoology, from the Museum of Yale College. No. XXIII. Results of recent dredging expeditions on the coast of New England. American Journal of Science and Arts, series (3) 5, 1 - 16." type="journal article" year="1872">
A. E. Verrill, 
<number id="B01024E98E2C675545FCE34AFD066878" box="[683,736,1338,1359]" pageId="19" pageNumber="20" value="1872.0">1872</number>
