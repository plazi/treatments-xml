<geoCoordinate id="17D5502B248EBDD7FAA6D9E61E57A4BA" box="[1286,1384,1956,1979]" degrees="74" direction="west" minutes="47" orientation="longitude" pageId="144" pageNumber="145" precision="925" value="-74.78333">74°47’W</geoCoordinate>
