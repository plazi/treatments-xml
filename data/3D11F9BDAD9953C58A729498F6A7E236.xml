<taxonomicName id="30A9AF4B2A6F4684945CD0EA672151C8" authorityName="A. Massal" authorityYear="1855" class="Eurotiomycetes" family="Verrucariaceae" genus="Tichothecium" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Tichothecium erraticum" order="Verrucariales" pageId="0" pageNumber="43" phylum="Ascomycota" rank="species" species="erraticum">
