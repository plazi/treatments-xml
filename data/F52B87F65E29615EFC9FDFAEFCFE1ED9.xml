<taxonomicName id="BA824D635E29615EFCF9DC50FA891F18" authority="FOREL, 1874" authorityName="FOREL" authorityYear="1874" box="[816,1304,1553,1574]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="31" pageNumber="164" phylum="Arthropoda" rank="variety" species="rufa" variety="rufopratensis">
