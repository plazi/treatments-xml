, 
<typeStatus id="00525493FFECFFB5FC2EFBF38F7FFBA1" box="[938,1040,1080,1106]" pageId="89" pageNumber="90">holotype</typeStatus>
male, 
<date id="AB57CCF1FFECFFB5FBE5FBF38FBCFBA1" box="[1121,1235,1080,1106]" pageId="89" pageNumber="90" value="1998-06-03">
