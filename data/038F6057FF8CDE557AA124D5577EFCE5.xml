<taxonomicName id="4C26AAC2FF8CDE557AC9264457E0FD8D" baseAuthorityName="Molina" baseAuthorityYear="1788" box="[677,988,576,603]" class="Maxillopoda" family="Balanidae" genus="Austromegabalanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sessilia" pageId="13" pageNumber="98" phylum="Arthropoda" rank="species" species="psittacus">
