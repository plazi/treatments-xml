<taxonomicName id="59BA4D515C0EFFEEB62D4B68308AD2B6" authority="Keyserling, 1891" authorityName="Keyserling" authorityYear="1891" box="[298,644,1650,1675]" class="Arachnida" family="Ctenidae" genus="Ctenus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="540" phylum="Arthropoda" rank="species" species="longipes">
<emphasis id="ACCEEAC05C0EFFEEB62D4B6833C2D2B7" box="[298,460,1650,1674]" italics="true" pageId="1" pageNumber="540">Ctenus longipes</emphasis>
<bibRefCitation id="FA2B4B235C0EFFEEB6D44B69308AD2B6" author="Keyserling, E." box="[467,644,1650,1675]" pageId="1" pageNumber="540" pagination="1 - 278" refId="ref8454" refString="Keyserling, E. (1891) Die Spinnen Amerikas. Brasilianische Spinnen. Nurnberg 3, 1 - 278." type="journal article" year="1891">Keyserling, 1891</bibRefCitation>
