<taxonomicName id="4BC04D451F796359C210C0C235859DF0" baseAuthorityName="Kolenati" baseAuthorityYear="1845" box="[328,669,944,967]" class="Insecta" family="Rhopalidae" genus="Maccevethus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="71" phylum="Arthropoda" rank="subSpecies" species="errans" subSpecies="caucasicus">
<emphasis id="BEB4EAD41F796359C210C0C235859DF0" box="[328,669,944,967]" italics="true" pageId="4" pageNumber="71">Maccevethus errans caucasicus</emphasis>
</taxonomicName>
