<taxonomicName id="4BC04D451F796359C1D4C0FD341D9D90" box="[652,773,911,935]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="71" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
