<subSubSection id="C3202CBBFFD3FFD17CA88A7CFE58FD59" pageId="45" pageNumber="46" type="etymology">
<paragraph id="8B857F30FFD3FFD17CA88A7CFE58FD59" blockId="45.[151,1436,152,700]" pageId="45" pageNumber="46">
<emphasis id="B94EA322FFD3FFD17CA88A7CFEB7FD72" bold="true" box="[196,342,637,663]" pageId="45" pageNumber="46">Etymology:</emphasis>
