<subSubSection id="7EBD653E897B265386471385E8974B39" pageId="3" pageNumber="295" type="description">
<paragraph id="361836B5897B265386471385EECC4CAE" blockId="3.[843,1483,657,798]" pageId="3" pageNumber="295">
1976 
<taxonomicName id="F1A74D36897B2653869A1385EFC54CCC" authority="ETTINGSH." authorityName="ETTINGSH." box="[918,1218,657,678]" class="Magnoliopsida" family="Magnoliaceae" genus="Liriodendron" kingdom="Plantae" order="Magnoliales" pageId="3" pageNumber="295" phylum="Tracheophyta" rank="species" species="haueri">
