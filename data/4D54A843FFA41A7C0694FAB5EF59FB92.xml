<emphasis id="F789C547FFA51A7C0217FD0DEABBFD55" box="[1044,1275,674,699]" italics="true" pageId="29" pageNumber="30">
<taxonomicName id="02FD62D6FFA51A7C0217FD0DEAB7FD55" box="[1044,1271,674,699]" class="Insecta" family="Staphylinidae" genus="Acylophorus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="trigonocephalus">A. trigonocephalus</taxonomicName>
