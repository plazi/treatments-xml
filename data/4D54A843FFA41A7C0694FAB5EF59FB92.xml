<taxonomicName id="02FD62D6FFA51A7C044EFD0DEC9BFD55" box="[589,731,674,699]" class="Insecta" family="Staphylinidae" genus="Acylophorus" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="leonensis">
