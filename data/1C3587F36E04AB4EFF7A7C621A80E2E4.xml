<taxonomicName id="539C4D666E02AB4EFD597A101C26E7B1" authority="Tong &amp; Li, 2008" authorityName="Tong &amp; Li" authorityYear="2008" box="[692,1083,376,402]" class="Arachnida" family="Telemidae" genus="Telema" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="oculata">
