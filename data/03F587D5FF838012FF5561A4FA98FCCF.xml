<paragraph id="8BE336C3FF838012FF5561A4FAC9FEE3" blockId="0.[218,1104,252,318]" pageId="0" pageNumber="124">
<heading id="D0AB81AFFF838012FF5561A4FAC9FEE3" centered="true" fontSize="36" level="2" pageId="0" pageNumber="124" reason="5">
<emphasis id="B928EAD1FF838012FF5561A4F855FEC8" bold="true" box="[218,366,253,281]" pageId="0" pageNumber="124">THE GENUS</emphasis>
<taxonomicName id="4C5C4D40FF838012FEF661A5FB96FEC8" authority="LECONTE" authorityName="LECONTE" box="[377,685,252,281]" class="Insecta" family="Dryophthoridae" genus="Rhodobaenus" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="124" phylum="Arthropoda" rank="genus">
<emphasis id="B928EAD1FF838012FEF661A5FB13FEC9" bold="true" box="[377,552,252,281]" italics="true" pageId="0" pageNumber="124">RHODOBAENUS</emphasis>
<emphasis id="B928EAD1FF838012FDBC61A4FB96FEC8" bold="true" box="[563,685,253,281]" pageId="0" pageNumber="124">LECONTE</emphasis>
</taxonomicName>
<emphasis id="B928EAD1FF838012FD3761A4FBB8FEE3" bold="true" pageId="0" pageNumber="124">
(
<taxonomicName id="4C5C4D40FF838012FD4D61A4FA52FEC8" authorityName="Linnaeus" authorityYear="1758" box="[706,873,253,281]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Coleoptera" pageId="0" pageNumber="124" phylum="Tracheophyta" rank="order">COLEOPTERA</taxonomicName>
: 
<taxonomicName id="4C5C4D40FF838012FCF261A4FD7DFEC8" authorityName="Latreille" authorityYear="1802" box="[893,1094,253,281]" class="Insecta" family="Curculionidae" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="124" phylum="Arthropoda" rank="family">CURCULIONIDAE</taxonomicName>
: 
<taxonomicName id="4C5C4D40FF838012FEB5607BFB22FEE3" authorityName="Schoenherr" authorityYear="1825" box="[314,537,290,318]" class="Insecta" family="Dryophthoridae" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="124" phylum="Arthropoda" rank="subFamily" subFamily="Dryophthorinae">DRYOPHTHORINAE</taxonomicName>
) IN THE
</emphasis>
<emphasis id="B928EAD1FF838012FD01607BFAC9FEE3" bold="true" box="[654,1010,290,318]" pageId="0" pageNumber="124">
<collectingCountry id="F34B7653FF838012FD01607BFAC9FEE3" box="[654,1010,290,318]" name="United States of America" pageId="0" pageNumber="124">
UNITED STATES OF 
<emphasis id="B928EAD1FF838012FCF4607BFAC9FEE3" bold="true" box="[891,1010,290,318]" pageId="0" pageNumber="124">AMERICA</emphasis>
</collectingCountry>
</emphasis>
