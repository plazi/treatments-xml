<taxonomicName id="4C5FF0E9FC5CFFD5F01BFB7CFD41FB7A" authorityName="ULMER" authorityYear="1951" box="[521,676,1161,1183]" class="Insecta" family="Hydroptilidae" genus="Orthotrichia" kingdom="Animalia" order="Trichoptera" pageId="29" pageNumber="1038" phylum="Arthropoda" rank="species" species="maeandrica">
