<taxonomicName id="6EA64D598368C94A67B7FB70C222FB7F" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[1014,1389,1194,1221]" class="Gymnolaemata" family="Bugulidae" genus="Bugula" kingdom="Animalia" order="Cheilostomatida" pageId="5" pageNumber="552" phylum="Bryozoa" rank="species" species="neritina">
