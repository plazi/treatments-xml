<taxonomicName id="0DF221965ACD467F47BF4C33AAE5B077" class="Arachnida" genus="P." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="P. sinuosa" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="sinuosa">
<emphasis id="EE3BB35353B36E94DB77BDABC1DACF72" italics="true" pageId="0" pageNumber="1">P. sinuosa</emphasis>
