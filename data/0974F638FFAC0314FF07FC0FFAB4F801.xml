<collectingCountry id="F9CA07BEFFAC0314FDA7F8CCFD3CF8FA" box="[567,658,1818,1844]" name="Taiwan" pageId="4" pageNumber="49">Taiwan</collectingCountry>
), 
<collectingCountry id="F9CA07BEFFAC0314FD35F8CCFD46F8FA" box="[677,744,1818,1844]" name="Japan" pageId="4" pageNumber="49">Japan</collectingCountry>
