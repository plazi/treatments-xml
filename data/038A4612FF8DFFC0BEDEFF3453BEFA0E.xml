<taxonomicName id="4C238C87FF8DFFC0BEDEFE365047FE0F" authority=": Vasseur 1967" authorityName=": Vasseur" authorityYear="1967" box="[151,569,405,427]" class="Ascidiacea" family="Styelidae" genus="Polyandrocarpa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stolidobranchia" pageId="5" pageNumber="296" phylum="Chordata" rank="species" species="anguinea">
