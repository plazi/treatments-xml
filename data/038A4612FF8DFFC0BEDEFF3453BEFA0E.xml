<taxonomicName id="4C238C87FF8DFFC0BD3AFAEA5654FAC5" authorityName="Michaelsen" authorityYear="1904" box="[883,1066,1353,1377]" class="Ascidiacea" family="Styelidae" genus="Polyandrocarpa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stolidobranchia" pageId="5" pageNumber="296" phylum="Chordata" rank="genus">
