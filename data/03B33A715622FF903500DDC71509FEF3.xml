) and 
<taxonomicName id="4C1AF0E45622FF9134FBDA8715EBF971" authority="Sowerby, 1862" authorityName="Sowerby" authorityYear="1862" class="Coccolithophyceae" family="Isochrysidaceae" genus="Scintilla" kingdom="Chromista" order="Isochrysidales" pageId="22" pageNumber="3196" phylum="Haptophyta" rank="species" species="pisum">
<emphasis id="B96E57755622FF9134FBDA8714BFF970" italics="true" pageId="22" pageNumber="3196">Scintilla pisum</emphasis>
<bibRefCitation id="EF8BF6965622FF9137E4DAA715EBF971" author="Sowerby" box="[229,398,1708,1732]" firstAuthor="Sowerby" pageId="22" pageNumber="3196" pagination="173 - 182" refId="ref20770" refString="SOWERBY, G. B., 1862, Thesaurus Conchyliorum, Vol. 3, Part 21, Galeomma (London: Sowerby) pp. 173 - 182." type="book chapter" year="1862">Sowerby, 1862</bibRefCitation>
</taxonomicName>
(
<figureCitation id="132197E25622FF9136A1DAA7166BF971" box="[416,526,1708,1732]" captionStart="FIGS" captionStartId="26.[154,170,1548,1570]" captionTargetBox="[162,1103,178,1504]" captionTargetId="figure-0@26.[154,1111,170,1510]" captionTargetPageId="26" captionText="FIGS. 81â€“88. (81) Scintilla lynchi, NMW.Z.2000.086.00011 [8.98 mm]. (82) Scintilla vitrea, NMW.Z.2000.086.00004 [7.37 mm]. (83) Scintilla larcombei, NMW.Z.2001.061.00028 [3.29 mm]. (84) Scintilla anomala, NMW.Z.2000.086.00007 [9.2mm]. (85) Scintilla pisum, NMW.Z.2001.061.00025 [8.5 mm]. (86) Amphilepida aurantia, NMW.Z.1999.073.00004 [10.1 mm]. (87) Scintilla sp. A, NMW.Z.2000.086.00012 [2.62 mm]. (88) Scintilla sp. B, NMW.Z.2000.086.00013 [14.91 mm]. All external (MontageTM)." pageId="22" pageNumber="3196">figures 85</figureCitation>
, 
