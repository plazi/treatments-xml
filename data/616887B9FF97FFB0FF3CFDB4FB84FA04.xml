<paragraph id="E97E36AFFF97FFB0FF3CFDB4FD21FD67" blockId="10.[151,1436,617,1506]" box="[151,687,617,642]" pageId="10" pageNumber="51">
<heading id="B23681C3FF97FFB0FF3CFDB4FD21FD67" bold="true" box="[151,687,617,642]" fontSize="10" level="2" pageId="10" pageNumber="51" reason="2">
