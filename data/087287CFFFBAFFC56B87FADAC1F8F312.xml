<subSubSection id="C8C16552FFBEFFC66BD7FD07C621F0FE" box="[199,1088,727,752]" pageId="5" pageNumber="372" type="etymology">
<paragraph id="806436D9FFBEFFC66BD7FD07C621F0FE" blockId="5.[151,1437,151,933]" box="[199,1088,727,752]" pageId="5" pageNumber="372">
<emphasis id="B2AFEACBFFBEFFC66BD7FD07C32FF0FE" bold="true" box="[199,334,727,752]" pageId="5" pageNumber="372">Etymology.</emphasis>
