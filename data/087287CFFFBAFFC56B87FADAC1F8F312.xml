<taxonomicName id="47DB4D5AFFBFFFC76A70F8ACC05EF59C" box="[352,575,1916,1938]" class="Insecta" family="Reduviidae" genus="Henricohahnia" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="4" pageNumber="371" phylum="Arthropoda" rank="species" species="parva" status="sp. nov.">
