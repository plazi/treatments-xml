<taxonomicName id="04FF1D802B4C3F5F6E1D632FE0D3DDEF" LSID="http://zoobank.org/CE644BB3-52C6-43E6-AAC7-06BC248177F7" authority="Keifer, 1939" authorityName="Keifer" authorityYear="1939" class="Arachnida" family="Diptilomiopidae" genus="Rhyncaphytoptus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhyncaphytoptus fuyuniensis" order="Trombidiformes" pageId="4" pageNumber="101" phylum="Arthropoda" rank="species" species="fuyuniensis">Rhyncaphytoptus fuyuniensis</taxonomicName>
<taxonomicNameLabel id="A31B0BAB9178240DD2832065FBE79294" pageId="4" pageNumber="101">sp. n.</taxonomicNameLabel>
