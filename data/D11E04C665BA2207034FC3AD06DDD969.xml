<taxonomicName id="20168394F27DB0110ECF2B521DB94CD3" authority="Keifer, 1939" authorityName="Keifer" authorityYear="1939" class="Arachnida" family="Diptilomiopidae" genus="Rhyncaphytoptus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhyncaphytoptus" order="Trombidiformes" pageId="6" pageNumber="103" phylum="Arthropoda" rank="genus">Rhyncaphytoptus</taxonomicName>
