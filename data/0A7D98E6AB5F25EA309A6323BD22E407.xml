<taxonomicName id="3EE5F00272DAAAB1F3E85399F2A2078A" class="Magnoliopsida" family="Brassicaceae" genus="Lepidium" higherTaxonomySource="CoL" kingdom="Plantae" order="Brassicales" pageId="1" pageNumber="550" phylum="Tracheophyta" rank="species" species="virginicum">L. virginicum</taxonomicName>
<normalizedToken id="CFFF9E137F07A492E96EFD3F1D1DD261" originalValue="dürften">duerften</normalizedToken>
