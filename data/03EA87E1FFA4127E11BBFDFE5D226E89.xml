<location id="8E9C602CFFA4127E1220FCEF5BAE6EDF" LSID="urn:lsid:plazi:treatment:03EA87E1FFA4127E11BBFDFE5D226E89:8E9C602CFFA4127E1220FCEF5BAE6EDF" box="[780,1021,882,908]" country="Italy" county="Belluno province" municipality="Mantua province" name="Bosco della Fontana" pageId="21" stateProvince="Lombardia">
Bosco 
<collectorName id="26B65321FFA4127E1272FCEF5BAE6EDF" box="[862,1021,882,908]" pageId="21">della Fontana</collectorName>
</location>
