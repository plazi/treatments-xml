<taxonomicName id="4C5E4D5C2E15FFF2D6F62D7E6EE4C637" authority="Nutt." authorityName="Nutt." box="[679,964,1827,1851]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="species" species="parviflorus">
