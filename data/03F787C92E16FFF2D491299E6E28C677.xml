</taxonomicName>
(
<taxonomicName id="4C5E4D5C2E15FFF2D6CE2D5E6E3BC617" box="[671,795,1795,1819]" class="Magnoliopsida" family="Ericaceae" kingdom="Plantae" order="Ericales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="family">Ericaceae</taxonomicName>
), 
<taxonomicName id="4C5E4D5C2E15FFF2D7612D596E92C617" box="[816,946,1796,1819]" class="Magnoliopsida" family="Cornaceae" genus="Cornus" kingdom="Plantae" order="Cornales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis id="B92AEACD2E15FFF2D7612D596EA8C617" box="[816,904,1796,1819]" italics="true" pageId="46" pageNumber="45">Cornus</emphasis>
sp.
</taxonomicName>
(
<taxonomicName id="4C5E4D5C2E15FFF2D7922D5E6968C617" box="[963,1096,1795,1819]" class="Magnoliopsida" family="Cornaceae" kingdom="Plantae" order="Cornales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="family">Cornaceae</taxonomicName>
), 
<taxonomicName id="4C5E4D5C2E15FFF2D00D2D5E6880C617" authority="(Sm.) DC." authorityName="DC." baseAuthorityName="Sm." box="[1116,1440,1795,1819]" class="Magnoliopsida" family="Berberidaceae" genus="Achlys" kingdom="Plantae" order="Ranunculales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="species" species="triphylla">
<emphasis id="B92AEACD2E15FFF2D00D2D5E683FC617" box="[1116,1311,1795,1819]" italics="true" pageId="46" pageNumber="45">Achlys triphylla</emphasis>
