<taxonomicName id="4C5E4D5C2E15FFF2D6602D1E6F84C657" authorityName="de Jussieu" authorityYear="1789" box="[561,676,1859,1883]" class="Liliopsida" family="Liliaceae" kingdom="Plantae" order="Liliales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="family">Liliaceae</taxonomicName>
