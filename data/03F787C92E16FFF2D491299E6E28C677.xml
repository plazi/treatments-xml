</taxonomicName>
(
<taxonomicName id="4C5E4D5C2E15FFF2D7832D7E6965C637" box="[978,1093,1827,1851]" class="Magnoliopsida" family="Rosaceae" kingdom="Plantae" order="Rosales" pageId="46" pageNumber="45" phylum="Tracheophyta" rank="family">Rosaceae</taxonomicName>
