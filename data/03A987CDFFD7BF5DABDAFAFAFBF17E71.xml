<quantity id="4CF89B3EFFD7BF5DAAD4F9F5F9927E71" box="[444,489,1566,1586]" metricMagnitude="3" metricUnit="m" metricValue="5.0" pageId="38" pageNumber="501" unit="km" value="5.0">5 km</quantity>
