<collectingCountry id="B2A87ED5FF8E3B1BFC68AE21FBE6721D" box="[969,1034,333,357]" name="Brazil" pageId="7" pageNumber="377">Brazil</collectingCountry>
(K. 
<bibRefCitation id="AE2E43B4FF8E3B1BFB9AAE21FB54721D" author="Viets, K." box="[1083,1208,333,357]" pageId="7" pageNumber="377" pagination="161 - 247" refId="ref8570" refString="Viets, K. 1954 b. Wassermilben aus dem Amazonasgebiet (Hydrachnellae, Acari). (Systematsche und okologische Untersuchungen). Bearbeitung der Sammlungen Dr. R. Braun, und Dr. H. Sioli, Belem. Schweiz. Zschr. Hydrol., 16: 78 - 151, 16: 161 - 247. doi: 10.1007 / BF 02483636" type="journal article" year="1954">Viets 1954b</bibRefCitation>
) and 
<collectingCountry id="B2A87ED5FF8E3B1BFB56AE21FA81721D" box="[1271,1389,333,357]" name="Argentina" pageId="7" pageNumber="377">Argentina</collectingCountry>
(
<bibRefCitation id="AE2E43B4FF8E3B1BFADBAE21FD7672F0" author="Rosso de Ferradas, B." pageId="7" pageNumber="377" pagination="23 - 34" refId="ref8316" refString="Rosso de Ferradas, B. 2006 b. Arrenuridos de la cuenca del Rio Parana, Argentina (Acari: Parasitengona: Hydrachnidia). Rev. Soc. Ent. Argent., 65: 23 - 34." type="journal article" year="2006">Rosso de Ferradás 2006b</bibRefCitation>
, as 
<taxonomicName id="0DBF45C6FF8E3B1BFD1BAE1CFCA772F0" authorityName="Rosso de Ferradas" authorityYear="2006" box="[698,843,368,392]" class="Arachnida" family="Arrenuridae" genus="Arrenurus" kingdom="Animalia" order="Trombidiformes" pageId="7" pageNumber="377" phylum="Arthropoda" rank="species" species="gladiiferus">
<emphasis id="F8CBE257FF8E3B1BFD1BAE1CFCA772F0" box="[698,843,368,392]" italics="true" pageId="7" pageNumber="377">A. gladiiferus</emphasis>
</taxonomicName>
).
</paragraph>
