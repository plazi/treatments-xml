<paragraph id="B55D368AE1576B4D6BA1FBBBAA37FBE5" blockId="10.[151,1292,1076,1098]" box="[151,1292,1076,1098]" pageId="10" pageNumber="461">
<treatmentCitationGroup id="95F211A4E1576B4D6BA1FBBBAA37FBE5" box="[151,1292,1076,1098]" pageId="10" pageNumber="461">
<taxonomicName id="72E24D09E1576B4D6BA1FBBBAD59FBE5" authority="Drake &amp; Hoberlandt, 1950: 1" authorityName="Drake &amp; Hoberlandt" authorityPageNumber="1" authorityYear="1950" box="[151,610,1076,1098]" class="Insecta" family="Saldidae" genus="Saldula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="461" phylum="Arthropoda" rank="species" species="stoneri">
<emphasis id="8796EA98E1576B4D6BA1FBBBAE0AFBE5" box="[151,305,1076,1098]" italics="true" pageId="10" pageNumber="461">Saldula stoneri</emphasis>
