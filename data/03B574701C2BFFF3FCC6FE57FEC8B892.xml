<collectingCounty id="62C2BDEA1C29FFF3FE95FEB5FE70BF47" box="[377,508,343,369]" pageId="6" pageNumber="67">Revêtement</collectingCounty>
des soies de 
<typeStatus id="54A77BC41C29FFF3FF9DFE95FF2CBFA7" box="[113,160,375,401]" pageId="6" pageNumber="67">type</typeStatus>
