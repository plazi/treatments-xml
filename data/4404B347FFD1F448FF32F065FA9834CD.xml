<taxonomicName id="0BAD79D2FFD1F449FF32F065FD3B3466" authority="Ax, 1951" authorityName="Ax" authorityYear="1951" box="[151,654,1831,1858]" class="Rhabditophora" family="Macrostomidae" genus="Macrostomum" kingdom="Animalia" order="Macrostomida" pageId="2" pageNumber="579" phylum="Platyhelminthes" rank="species" species="rubrocinctum">
