<quantity id="4CEF9B25FFF2FFD69AC1FD7DF94132AF" box="[700,768,655,682]" metricMagnitude="-1" metricUnit="m" metricValue="4.1" pageId="99" pageNumber="100" unit="cm" value="41.0">41 cm</quantity>
