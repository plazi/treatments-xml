<quantity id="4CEF9B25FFF2FFD69B04FC95FE4C3387" box="[889,1037,871,898]" metricMagnitude="3" metricUnit="m" metricValue="1.06" metricValueMax="2.0" metricValueMin="0.12" pageId="99" pageNumber="100" unit="m" value="1060.0" valueMax="2000.0" valueMin="120.0">120–2,000 m</quantity>
