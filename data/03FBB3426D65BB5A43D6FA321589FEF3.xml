<keyLead id="30A3A1616D65BB5B43D6F9C91589F938" pageId="12" pageNumber="333">
<number id="3D9A10736D65BB5B43D6F9C910B6F963" box="[151,163,1663,1684]" pageId="12" pageNumber="333" value="3.0">3</number>
