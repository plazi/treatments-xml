<taxonomicName id="4C71E0ABBD2A110F3D27FDF2FE06FD8C" box="[354,464,513,534]" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="67" pageNumber="68" phylum="Tracheophyta" rank="genus">
