<taxonomicName id="4C7000F1FFF91737D091FE554BF98955" authority="Han &amp; Zhang, 1982: 210" authorityName="Han &amp; Zhang" authorityPageNumber="210" authorityYear="1982" box="[151,678,495,520]" class="Insecta" family="Thripidae" genus="Frankliniella" kingdom="Animalia" order="Thysanoptera" pageId="11" pageNumber="152" phylum="Arthropoda" rank="species" species="zizaniophila">
