<taxonomicName id="CAA721BCFF9FFFD9EE9BFAF691770046" authority="(Gunther)" baseAuthorityName="Gunther" baseAuthorityYear="1866" box="[151,518,1368,1395]" class="Actinopterygii" family="Belonidae" genus="Strongylura" kingdom="Animalia" order="Beloniformes" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="krefftii">
