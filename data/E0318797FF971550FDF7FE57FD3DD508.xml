<quantity id="AF609B64FF971553FDF7FD2DFD36D5B6" box="[625,730,669,696]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="9" pageNumber="10" unit="mm" value="5.9">5.90 mm</quantity>
