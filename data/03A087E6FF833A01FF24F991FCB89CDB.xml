<taxonomicName id="4C094D73FF873A01FC02FF48FC1498F1" box="[945,1020,226,252]" class="Insecta" family="Chironomidae" genus="Corynoneurella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="length">length</taxonomicName>
