<taxonomicName id="669612C1241ABC54FD62FB7741A479C8" authorityName="Zayas" authorityYear="1988" box="[644,727,1235,1256]" class="Insecta" family="Buprestidae" genus="Isophaenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="335" phylum="Arthropoda" rank="species" species="israeli">
