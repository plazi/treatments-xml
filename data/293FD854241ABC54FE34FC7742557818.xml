</taxonomicName>
(
<collectionCode id="C787F187241ABC54FE44FB4942847823" box="[418,503,1261,1283]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="2" pageNumber="335">AMNH</collectionCode>
