<taxonomicName id="669612C1241ABC54FEC9FB4942E17822" authorityName="Cazier" authorityYear="1952" box="[303,402,1261,1282]" class="Insecta" family="Buprestidae" genus="Xenorhipis" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="335" phylum="Arthropoda" rank="species" species="vauriei">
