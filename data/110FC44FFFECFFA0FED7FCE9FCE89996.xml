</emphasis>
<taxonomicName id="5EA60EDAFFECFFA3FEEAFCA9FE0D9F99" authority="Ng &amp; Mar &amp; Shih, 2020" authorityName="Ng &amp; Mar &amp; Shih" authorityYear="2020" box="[271,483,893,917]" class="Malacostraca" family="Potamidae" genus="Indawmon" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="530" phylum="Arthropoda" rank="species" species="thanayei" status="new species">
