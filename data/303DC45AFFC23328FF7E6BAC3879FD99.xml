<taxonomicNameLabel id="91D31425FFC2332FFEF96BAC3BB0FA4B" box="[309,385,1452,1473]" pageId="11" pageNumber="1618" rank="species">nov.sp.</taxonomicNameLabel>
(
<taxonomicName id="7F940ECFFFC2332FFE426BAC3BC4FA48" authorityName="Ulmer" authorityYear="1903" box="[398,501,1452,1474]" class="Insecta" family="Goeridae" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="1618" phylum="Arthropoda" rank="family">Goeridae</taxonomicName>
