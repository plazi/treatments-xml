<paragraph id="0D4E73A7F3FFA74B62D2EA3022EC0688" pageId="4" pageNumber="15">
Genus 
<taxonomicName id="4BAB76CB52B8E02D9A00754130CA40C6" LSID="http://zoobank.org/D0F10EB4-C1F6-440C-A6B0-6723D1BD0506" authority="Wang &amp; Liu" class="Insecta" family="Tettigoniidae" genus="Athaumaspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Athaumaspis" order="Orthoptera" pageId="4" pageNumber="15" phylum="Arthropoda" rank="genus">Athaumaspis Wang &amp; Liu</taxonomicName>
