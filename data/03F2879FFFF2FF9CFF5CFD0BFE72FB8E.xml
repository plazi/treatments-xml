<taxonomicName id="4C5B4D0AFFF2FF9CFE88FD04FDF7FD2E" authority="Breddin, 1898" authorityName="Breddin" authorityYear="1898" box="[273,539,663,688]" class="Insecta" family="Veliidae" genus="Paravelia" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="184" phylum="Arthropoda" rank="genus">
