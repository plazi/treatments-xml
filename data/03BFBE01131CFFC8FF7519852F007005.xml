<typeStatus id="54ADB1B5131DFFC8FF241F432F41723B" box="[198,259,338,364]" pageId="1" pageNumber="19">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="4C167494131DFFC8FE921F432CFD723C" box="[368,703,338,363]" class="Arachnida" family="Eriophyidae" genus="Juxtacolopodacus" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="19" phylum="Arthropoda" rank="species" species="phalakros" status="n. sp.">
<emphasis id="B962D305131DFFC8FE921F432CFD723C" box="[368,703,338,363]" italics="true" pageId="1" pageNumber="19">Juxtacolopodacus phalakros</emphasis>
</taxonomicName>
