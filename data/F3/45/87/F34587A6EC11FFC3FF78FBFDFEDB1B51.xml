, 
<collectingCountry box="[1178,1271,1664,1688]" name="Portugal" pageId="7" pageNumber="338">Portugal</collectingCountry>
, 
<collectingCountry box="[1280,1377,1664,1687]" name="Morocco" pageId="7" pageNumber="338">Morocco</collectingCountry>
, the
</paragraph>
<paragraph blockId="7.[816,1421,1663,1720]" box="[816,1365,1696,1720]" pageId="7" pageNumber="338">
<collectingRegion box="[816,1119,1696,1720]" isEnumeration="true" pageId="7" pageNumber="338">Canary Islands and Madeira</collectingRegion>
