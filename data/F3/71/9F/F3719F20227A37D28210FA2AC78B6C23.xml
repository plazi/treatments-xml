<taxonomicName class="Insecta" family="Curculionidae" genus="Anisus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anisus (Andrusowia)" order="Coleoptera" pageId="68" pageNumber="99" phylum="Arthropoda" rank="subGenus" subGenus="Andrusowia">Anisus (Andrusowia)</taxonomicName>
[sic] 
<taxonomicName lsidName="kolesnikovi" pageId="68" pageNumber="99" rank="species" species="kolesnikovi">kolesnikovi</taxonomicName>
