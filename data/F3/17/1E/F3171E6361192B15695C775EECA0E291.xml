<normalizedToken originalValue="египетскій">egipetskіi</normalizedToken>
; 
<normalizedToken originalValue="пойманъ">poiman</normalizedToken>
12 
<normalizedToken originalValue="мая">maya</normalizedToken>
<normalizedToken originalValue="въ">v</normalizedToken>
