<emphasis bold="true" box="[198,320,267,293]" pageId="7" pageNumber="8">Remarks.</emphasis>
<bibRefCitation box="[332,561,268,294]" pageId="7" pageNumber="15" refString="O'Loughlin, P. M. (2002) Report on selected species of BANZARE and ANARE Holothuroidea, with reviews of Meseres Ludwig and Heterocucumis Panning (Echinodermata). Memoirs of Museum Victoria, 59 (2), 297 - 325." type="journal article">
O’Loughlin (
<number box="[491,553,268,294]" pageId="7" pageNumber="8" value="2002.0">2002</number>
)
</bibRefCitation>
reviewed 
<taxonomicName authority="Vaney, 1908" authorityName="Vaney" authorityYear="1908" box="[694,1224,268,294]" class="Holothuroidea" family="Psolidae" genus="Psolidium" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="coatsi" subGenus="Cucumaria">
<emphasis box="[694,1059,268,293]" italics="true" pageId="7" pageNumber="8">Psolidium (Cucumaria) coatsi</emphasis>
<bibRefCitation author="Vaney" box="[1072,1224,268,294]" pageId="7" pageNumber="16" refString="Vaney, C. (1908) Les Holothuries de l'Expedition Antarctique Nationale Ecossaise. Transactions of the Royal Society of Edinburgh, 46, 405 - 441, 5 pls." type="journal article" year="1908">
Vaney, 
<number box="[1163,1224,268,294]" pageId="7" pageNumber="8" value="1908.0">1908</number>
</bibRefCitation>
</taxonomicName>
, erected the new genus 
<taxonomicName box="[231,348,306,331]" class="Holothuroidea" family="Cucumariidae" genus="Psolicrux" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="genus">
<emphasis box="[231,348,306,331]" italics="true" pageId="7" pageNumber="8">Psolicrux</emphasis>
</taxonomicName>
for the species, and judged that 
<taxonomicName authority="Vaney, 1908" authorityName="Vaney" authorityYear="1908" box="[752,1185,306,332]" class="Holothuroidea" family="Cucumariidae" genus="Cucumaria" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="conspicua">
<emphasis box="[752,1022,306,331]" italics="true" pageId="7" pageNumber="8">Cucumaria conspicua</emphasis>
<bibRefCitation author="Vaney" box="[1030,1185,306,332]" pageId="7" pageNumber="16" refString="Vaney, C. (1908) Les Holothuries de l'Expedition Antarctique Nationale Ecossaise. Transactions of the Royal Society of Edinburgh, 46, 405 - 441, 5 pls." type="journal article" year="1908">
Vaney, 
<number box="[1121,1185,306,332]" pageId="7" pageNumber="8" value="1908.0">1908</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Ekman, 1927" authorityName="Ekman" authorityYear="1927" class="Holothuroidea" family="Psolidae" genus="Psolidium" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="navicula">
<emphasis box="[1198,1436,306,331]" italics="true" pageId="7" pageNumber="8">Psolidium navicula</emphasis>
<bibRefCitation author="Ekman" box="[151,321,345,371]" pageId="7" pageNumber="14" refString="Ekman, S. (1927) Holothurien der deutschen Sudpolar-Expedition 1901 - 1903 aus der Ostantarktis und von den Kerguelen. Deutsche Sudpolar-Expedition, 19 (Zoology 11), 359 - 419." type="journal article" year="1927">
Ekman, 
<number box="[258,321,345,371]" pageId="7" pageNumber="8" value="1927.0">1927</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName authority="Ludwig &amp; Heding, 1935" authorityName="Ludwig &amp; Heding" authorityYear="1935" box="[385,981,345,371]" class="Holothuroidea" family="Psolidae" genus="Psolidium" kingdom="Animalia" order="Dendrochirotida" pageId="7" pageNumber="8" phylum="Echinodermata" rank="species" species="bistriatum">
<emphasis box="[385,657,345,370]" italics="true" pageId="7" pageNumber="8">Psolidium bistriatum</emphasis>
<bibRefCitation author="Ludwig" box="[666,981,345,371]" pageId="7" pageNumber="15" refString="Ludwig, H. &amp; Heding, S. G. (1935) Die holothurien der Deutschen Tiefsee-Expedition. 1. Fusslose und dendrochirote formen. Wissenschaftliche Ergeb-nisse der Deutschen Tiefsee-Expedition auf dem dampfer Valdivia 1898 - 1899, 24, 123 - 214." type="journal article" year="1935">
Ludwig &amp; Heding, 
<number box="[918,981,345,371]" pageId="7" pageNumber="8" value="1935.0">1935</number>
</bibRefCitation>
</taxonomicName>
