<taxonomicName class="Holothuroidea" family="Phyllophoridae" genus="Thyone" kingdom="Animalia" order="Dendrochirotida" pageId="9" pageNumber="10" phylum="Echinodermata" rank="species" species="scotiae">
<emphasis italics="true" pageId="9" pageNumber="10">T. scotiae</emphasis>
</taxonomicName>
