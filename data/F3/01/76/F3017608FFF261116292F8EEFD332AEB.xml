species: 
<taxonomicName authority="Fabricius, 1805" authorityName="Fabricius" authorityYear="1805" box="[806,1128,1862,1885]" class="Insecta" family="Tephritidae" genus="Dacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="123" phylum="Arthropoda" rank="species" species="armatus">
