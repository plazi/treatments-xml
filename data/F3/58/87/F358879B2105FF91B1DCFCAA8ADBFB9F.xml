<taxonomicName baseAuthorityName="EPPELSHEIM" baseAuthorityYear="1884" class="Insecta" family="Staphylinidae" genus="Lobrathium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="193" phylum="Arthropoda" rank="species" species="lederi">
