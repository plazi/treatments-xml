<heading bold="true" box="[151,619,1325,1351]" fontSize="11" level="1" pageId="31" pageNumber="332" reason="1">
<taxonomicName authority="Karsch, 1896" authorityName="Karsch" authorityYear="1896" box="[151,619,1325,1351]" class="Insecta" family="Hesperiidae" genus="Artitropa" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="332" phylum="Arthropoda" rank="subSpecies" species="erinnys" subSpecies="ehlersi">
<emphasis bold="true" box="[151,619,1325,1351]" pageId="31" pageNumber="332">
<emphasis bold="true" box="[151,447,1325,1351]" italics="true" pageId="31" pageNumber="332">Artitropa erinnys ehlersi</emphasis>
<bibRefCitation author="Karsch" box="[454,619,1325,1351]" firstAuthor="Karsch" pageId="31" pageNumber="332" pagination="372" refId="ref31462" refString="Karsch, F. (1896) Aethiopische hesperiiden. Entomologische Nachrichten, 22, 372 - 378." type="journal article" year="1896">
Karsch, 
<number box="[560,619,1325,1351]" pageId="31" pageNumber="332" value="1896.0">1896</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
</heading>
</paragraph>
