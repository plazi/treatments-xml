<collectingCountry box="[359,426,943,968]" name="China" pageId="6" pageNumber="81">China</collectingCountry>
(
<collectingRegion box="[441,547,943,968]" country="China" name="Zhejiang" pageId="6" pageNumber="81">Zhejiang</collectingRegion>
