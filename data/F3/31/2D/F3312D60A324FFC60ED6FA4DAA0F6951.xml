<collectingCountry box="[361,428,299,323]" name="China" pageId="11" pageNumber="556">China</collectingCountry>
(Heilongjiang, Henan, Jilin, Liaoning), 
<collectingCountry box="[878,950,300,323]" name="South Korea" pageId="11" pageNumber="556">Korea</collectingCountry>
, 
<collectingCountry box="[960,1027,300,323]" name="Japan" pageId="11" pageNumber="556">Japan</collectingCountry>
, 
<collectingCountry box="[1038,1119,299,323]" name="Turkey" pageId="11" pageNumber="556">Turkey</collectingCountry>
, 
<collectingCountry box="[1130,1207,299,323]" name="Russia" pageId="11" pageNumber="556">Russia</collectingCountry>
, Europe.
</paragraph>
</subSubSection>
