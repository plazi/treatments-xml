<number box="[483,539,583,607]" pageId="24" pageNumber="175" value="7.0" valueMax="11.0" valueMin="3.0">3–11</number>
<quantity box="[546,647,583,607]" metricMagnitude="-4" metricUnit="m" metricValue="4.3" pageId="24" pageNumber="175" unit="mm" value="0.43">0.43 mm</quantity>
