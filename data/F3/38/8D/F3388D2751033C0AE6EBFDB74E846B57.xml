<subSubSection box="[199,1260,870,895]" pageId="47" pageNumber="198" type="etymology">
<paragraph blockId="47.[151,1437,654,1039]" box="[199,1260,870,895]" pageId="47" pageNumber="198">
<emphasis bold="true" box="[199,326,870,895]" pageId="47" pageNumber="198">Etymology</emphasis>
