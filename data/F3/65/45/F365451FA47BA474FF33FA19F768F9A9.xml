</taxonomicName>
, 
<taxonomicName baseAuthorityName="Marsham" baseAuthorityYear="1802" box="[906,1050,1499,1523]" class="Insecta" family="Staphylinidae" genus="Oxytelus" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="laqueatus">
<emphasis box="[906,1050,1499,1523]" italics="true" pageId="54" pageNumber="55">O. laqueatus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Schubert" authorityYear="1906" box="[1064,1193,1499,1523]" class="Insecta" family="Staphylinidae" genus="Oxytelus" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="robustus">
