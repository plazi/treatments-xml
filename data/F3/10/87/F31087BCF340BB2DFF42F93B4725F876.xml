<paragraph blockId="12.[151,911,1698,1725]" box="[151,911,1698,1725]" pageId="12" pageNumber="463">
<heading bold="true" box="[151,911,1698,1725]" fontSize="11" level="1" pageId="12" pageNumber="463" reason="1">
<emphasis bold="true" box="[151,911,1698,1725]" pageId="12" pageNumber="463">
<taxonomicName authority="Cornel &amp; Mayi" authorityName="Cornel &amp; Mayi &amp; Kowo &amp; Foncha &amp; Andongma &amp; Anong &amp; Elad &amp; Djomo &amp; Tchuinkam &amp; Brisco &amp; Sehgal" authorityYear="2020" box="[151,832,1698,1725]" class="Insecta" family="Culicidae" genus="Culex" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="463" phylum="Arthropoda" rank="species" species="pseudosubaequalis" status="sp. n." subGenus="Culiciomyia">
<emphasis bold="true" box="[151,223,1698,1725]" italics="true" pageId="12" pageNumber="463">Culex</emphasis>
(
<emphasis bold="true" box="[240,387,1698,1725]" italics="true" pageId="12" pageNumber="463">Culiciomyia</emphasis>
) 
<emphasis bold="true" box="[404,632,1698,1725]" italics="true" pageId="12" pageNumber="463">pseudosubaequalis</emphasis>
Cornel &amp; Mayi
</taxonomicName>
, 
<taxonomicNameLabel box="[845,911,1699,1725]" pageId="12" pageNumber="463" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
