<typeStatus box="[151,210,1336,1360]" pageId="1" pageNumber="82">Type</typeStatus>
material. 
<typeStatus box="[337,446,1336,1360]" pageId="1" pageNumber="82">Holotype</typeStatus>
</emphasis>
: 
<number box="[463,484,1336,1360]" pageId="1" pageNumber="82" value="3.0">3</number>
, 
<emphasis bold="true" box="[500,613,1335,1360]" pageId="1" pageNumber="82">
