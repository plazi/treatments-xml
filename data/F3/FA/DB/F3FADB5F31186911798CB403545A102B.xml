<subSubSection pageId="127" pageNumber="128" type="type horizon">
<paragraph pageId="127" pageNumber="128">Type horizon.</paragraph>
<paragraph pageId="127" pageNumber="128">Middle-late Cernikian, late Pliocene-early Pleistocene.</paragraph>
</subSubSection>
<subSubSection pageId="127" pageNumber="128" type="type locality">
<paragraph pageId="127" pageNumber="128">Type locality.</paragraph>
<paragraph pageId="127" pageNumber="128">
<normalizedToken originalValue="“Sibinj”">&quot;Sibinj&quot;</normalizedToken>
, Croatia.
</paragraph>
