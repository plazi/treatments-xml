<paragraph id="8BEA36B8992C447C07D2FB407854FB1C" blockId="21.[151,1437,1153,2045]" box="[151,838,1153,1181]" pageId="21" pageNumber="22">
<emphasis id="B921EAAA992C447C07D2FB407A3BFB1D" bold="true" box="[151,297,1153,1180]" pageId="21" pageNumber="22">Description.</emphasis>
<typeStatus id="54EE881A992C447C0675FB437A8EFB1D" box="[304,412,1154,1180]" pageId="21" pageNumber="22">Holotype</typeStatus>
