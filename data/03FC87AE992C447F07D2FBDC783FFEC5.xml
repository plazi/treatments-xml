<quantity id="4CAD9B5D992C447C0501FB40798FFB1C" box="[580,669,1153,1181]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="21" pageNumber="22" unit="mm" value="4.5">4.5 mm</quantity>
. LFW 
<quantity id="4CAD9B5D992C447C05ACFB437850FB1C" box="[745,834,1154,1181]" metricMagnitude="-3" metricUnit="m" metricValue="4.3" pageId="21" pageNumber="22" unit="mm" value="4.3">4.3 mm</quantity>
