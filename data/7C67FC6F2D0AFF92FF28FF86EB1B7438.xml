<typeStatus id="2B75F3DB2D0AFF91FF28FE39EF94706E" box="[151,204,1861,1884]" pageId="57" pageNumber="58">Type</typeStatus>
genus: 
<taxonomicName id="33CE36FA2D0AFF91FEA1FE39EE22706E" authorityName="Fibiger" authorityYear="2010" box="[286,378,1861,1884]" class="Insecta" family="Erebidae" genus="Obscura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="C6BA916B2D0AFF91FEA1FE39EE22706E" box="[286,378,1861,1884]" italics="true" pageId="57" pageNumber="58">Obscura</emphasis>
</taxonomicName>
, 
<emphasis id="C6BA916B2D0AFF91FE37FE38EEA5706E" bold="true" box="[392,509,1860,1884]" pageId="57" pageNumber="58">
<taxonomicNameLabel id="DD892C102D0AFF91FE37FE38EEA5706E" box="[392,509,1860,1884]" pageId="57" pageNumber="58" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
