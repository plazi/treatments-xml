<heading id="C86281D00063FF90FF1DFD97FD5EFD21" bold="true" box="[151,762,621,648]" fontSize="11" level="1" pageId="114" pageNumber="115" reason="1">
<emphasis id="A1E1EAAE0063FF90FF1DFD97FD5EFD21" bold="true" box="[151,762,621,648]" pageId="114" pageNumber="115">
Genus 
<taxonomicName id="54954D3F0063FF90FF64FD97FD5EFD21" authority="Burmeister &amp; Schaum, 1840" authorityName="Burmeister &amp; Schaum" authorityYear="1840" box="[238,762,621,648]" class="Insecta" family="Cetoniidae" genus="Clastocnemis" kingdom="Animalia" order="Coleoptera" pageId="114" pageNumber="115" phylum="Arthropoda" rank="genus">
<emphasis id="A1E1EAAE0063FF90FF64FD97FE2BFD2E" bold="true" box="[238,399,621,648]" italics="true" pageId="114" pageNumber="115">Clastocnemis</emphasis>
