<taxonomicName id="54E699BEFFCF9429334885C3BD60F957" authorityName="Jordal" authorityYear="2021" box="[848,1105,1701,1725]" class="Insecta" family="Curculionidae" genus="Dendrochilus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="592" phylum="Arthropoda" rank="species" species="udzungwae">
<emphasis id="A1923E2FFFCF9429334885C3BD60F957" box="[848,1105,1701,1725]" italics="true" pageId="5" pageNumber="592">Dendrochilus udzungwae</emphasis>
</taxonomicName>
(14, 16) and 
<taxonomicName id="54E699BEFFCF942934C185C3BCADF957" authorityName="Jordal" authorityYear="2021" box="[1241,1436,1701,1725]" class="Insecta" family="Curculionidae" genus="Dendrochilus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="592" phylum="Arthropoda" rank="species" species="tener">
