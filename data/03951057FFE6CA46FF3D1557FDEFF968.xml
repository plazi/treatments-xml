<paragraph id="8B83A141FFE6CA46FF3D15AFFB6BFA6C" blockId="47.[151,1436,1373,1722]" box="[151,1279,1445,1470]" pageId="47" pageNumber="48">
<taxonomicName id="4C3CDAC2FFE6CA46FF3D15AFFCCFFA6C" authority=": Takaoka &amp; Davies, 1995: 115" authorityName=": Takaoka &amp; Davies" authorityPageNumber="115" authorityYear="1995" box="[151,859,1445,1470]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="hirtinervis" subGenus="Simulium">
