<taxonomicName id="4C65C0BC5A43AA596F38DB24FBE71CE9" authority="Gale, 2020" authorityName="Gale" authorityYear="2020" box="[1049,1097,1897,1921]" class="Magnoliopsida" family="Myricaceae" genus="Gale" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="140" phylum="Tracheophyta" rank="genus" status="gen. nov.">Gale</taxonomicName>
