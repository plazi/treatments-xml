<quantity id="F3069B12053FFFE01E40FF05FB8AFF22" box="[1063,1119,186,212]" metricMagnitude="1" metricUnit="m" metricValue="5.11048" pageId="15" pageNumber="144" unit="in" value="2012.0">2012</quantity>
</taxonomicName>
