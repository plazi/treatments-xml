<taxonomicName id="F3FE4D74053FFFE018C2FF05FB8AFF22" authority="Gorochov, 2012" authorityName="Gorochov" authorityYear="2012" box="[677,1119,186,212]" class="Liliopsida" family="Araceae" genus="Rhaphidophora" kingdom="Plantae" order="Alismatales" pageId="15" pageNumber="144" phylum="Tracheophyta" rank="species" species="incilis">
