<subSubSection id="7CE4657C053FFFE01AA0FF05FB81FF0E" pageId="15" pageNumber="144" type="discussion">
<paragraph id="344136F7053FFFE01AA0FF05FB81FF0E" blockId="15.[151,1436,150,285]" pageId="15" pageNumber="144">
<emphasis id="068AEAE5053FFFE01AA0FF05FE99FF22" bold="true" box="[199,332,186,212]" pageId="15" pageNumber="144">Discussion.</emphasis>
