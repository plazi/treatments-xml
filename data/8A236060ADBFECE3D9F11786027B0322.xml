60. 
<taxonomicName id="C4214D72D4C05721AC776DA89065611A" LSID="http://zoobank.org/C8F1FF61-9818-478E-ACF1-8E40A5A37698" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus pseudoflorensis" order="Coleoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="pseudoflorensis">Trigonopterus pseudoflorensis Riedel</taxonomicName>
