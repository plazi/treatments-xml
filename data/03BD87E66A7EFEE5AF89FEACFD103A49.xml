<quantity id="4CEC9B156A41FEE5AEC8FEADFDF83BAD" box="[470,550,1735,1761]" metricMagnitude="-3" metricUnit="m" metricValue="8.4" pageId="288" pageNumber="289" unit="mm" value="8.4">8.4mm</quantity>
, length of elytra: 
<quantity id="4CEC9B156A41FEE5ADF5FEACFCAA3BAD" box="[747,884,1734,1761]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" metricValueMax="6.4" metricValueMin="5.8" pageId="288" pageNumber="289" unit="mm" value="6.1" valueMax="6.4" valueMin="5.8">5.8–6.4 mm</quantity>
, width: 
<quantity id="4CEC9B156A41FEE5ACD0FEACFB863BAD" box="[974,1112,1734,1761]" metricMagnitude="-3" metricUnit="m" metricValue="4.949999999999999" metricValueMax="5.3" metricValueMin="4.6" pageId="288" pageNumber="289" unit="mm" value="4.949999999999999" valueMax="5.3" valueMin="4.6">4.6–5.3 mm</quantity>
