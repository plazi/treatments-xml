<taxonomicName id="4C284D20FF91FFFFFDAFFB18FD47FD19" authorityName="Cavalcante &amp; Dumas &amp; Nessimian" authorityYear="2018" box="[579,683,620,644]" class="Insecta" family="Calamoceratidae" genus="Phylloicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="436" phylum="Arthropoda" rank="species" species="mirabilis">mirabilis</taxonomicName>
