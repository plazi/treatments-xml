<taxonomicName id="4C271B8EFFE5BC25FEEAA0A145B9E9FB" authorityName="Tamanini" authorityYear="1946" box="[344,467,1934,1958]" class="Insecta" family="Veliidae" genus="Velia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="145" phylum="Arthropoda" rank="species" species="africana">
