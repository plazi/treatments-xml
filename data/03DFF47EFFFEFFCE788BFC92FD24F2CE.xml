<emphasis id="B902997AFFFEFFCC7903FB23FE8EF546" box="[287,312,1267,1293]" italics="true" pageId="13" pageNumber="226">z5</emphasis>
<emphasis id="B902997AFFFEFFCC7923FB22FEFBF547" bold="true" box="[319,333,1266,1292]" pageId="13" pageNumber="226">8</emphasis>
(7–9), 
<emphasis id="B902997AFFFEFFCC7983FB23FE0AF546" box="[415,444,1267,1293]" italics="true" pageId="13" pageNumber="226">Z4</emphasis>
<emphasis id="B902997AFFFEFFCC79DFFB22FE69F547" bold="true" box="[451,479,1266,1292]" pageId="13" pageNumber="226">46</emphasis>
(45–48), 
<emphasis id="B902997AFFFEFFCC7A51FB23FDDDF546" box="[589,619,1267,1293]" italics="true" pageId="13" pageNumber="226">Z5</emphasis>
<emphasis id="B902997AFFFEFFCC7A6EFB22FD38F547" bold="true" box="[626,654,1266,1292]" pageId="13" pageNumber="226">59</emphasis>
(58–62), 
<emphasis id="B902997AFFFEFFCC7AE7FB23FCA2F546" box="[763,788,1267,1293]" italics="true" pageId="13" pageNumber="226">s4</emphasis>
<emphasis id="B902997AFFFEFFCC7B07FB22FC81F547" bold="true" box="[795,823,1266,1292]" pageId="13" pageNumber="226">68</emphasis>
(67–69), 
<emphasis id="B902997AFFFEFFCC7BB9FB22FC08F547" box="[933,958,1266,1292]" italics="true" pageId="13" pageNumber="226">s6</emphasis>
<emphasis id="B902997AFFFEFFCC7BD9FB22FC57F547" bold="true" box="[965,993,1266,1292]" pageId="13" pageNumber="226">69</emphasis>
(67–71), 
