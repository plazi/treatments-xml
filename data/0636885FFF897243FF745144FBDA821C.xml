<materialsCitation id="3EF73314FF897243FF245254FCCA8349" ID-GBIF-Occurrence="1913870670" box="[199,839,1631,1656]" country="Philippines" location="Philippines" pageId="56" pageNumber="57" specimenCount="1" typeStatus="holotype">
