<taxonomicName id="2FA94D47FFAF48325132FBA8FE651BA2" box="[291,454,1481,1502]" class="Insecta" family="Mycetophilidae" genus="Mycomya" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="143" phylum="Arthropoda" rank="species" species="irena" status="sp. n.">
<emphasis id="DADDEAD6FFAF48325132FBA8FE651BA2" box="[291,454,1481,1502]" italics="true" pageId="14" pageNumber="143">
Mycomya 
<taxonomicName id="2FA94D47FFAF48325180FBA8FE651BA2" box="[401,454,1481,1502]" pageId="14" pageNumber="143" rank="species" species="irena" status="sp. n.">irena</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="DADDEAD6FFAF483251C0FBA9FDAC1BA1" bold="true" box="[465,527,1480,1501]" pageId="14" pageNumber="143">
<taxonomicNameLabel id="C1EE57ADFFAF483251C0FBA9FDAC1BA1" box="[465,527,1480,1501]" pageId="14" pageNumber="143" rank="species">sp. n.</taxonomicNameLabel>
