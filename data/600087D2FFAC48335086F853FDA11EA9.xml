, Kambaiti, 
<quantity id="2F519B21FFAC483153D5F8FCFBBD18C9" box="[964,1054,1693,1717]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="13" pageNumber="142" unit="m" value="2000.0">2000 m</quantity>
, 
<date id="9C171004FFAC4831543DF8FCFB1718C9" box="[1068,1204,1693,1717]" pageId="13" pageNumber="142" value="1934-06-04">
<number id="5E6124E3FFAC4831543DF8FCFB9E18C9" box="[1068,1085,1693,1717]" pageId="13" pageNumber="142" value="4.0">4</number>
