<geoCoordinate id="1E3965924740AB43E3DE4C3C07D2EA5F" degrees="26" direction="east" minutes="45" orientation="longitude" precision="15" seconds="22" value="26.756111">26°45'22&quot;E</geoCoordinate>
], 14.xi.2006, G. Osella leg., 1♂ (BAQ); CP [ECape], Umtiza, East London Coast Reserves, East London, 
<geoCoordinate id="7A8E90625E0FE530A1BFEF108E374EE0" degrees="33" direction="south" minutes="02" orientation="latitude" precision="925" value="-33.033333">33°02'S</geoCoordinate>
