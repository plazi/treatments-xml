<geoCoordinate id="3D909BDC6342CEEEF5677E38991A73BD" degrees="26" direction="east" minutes="36" orientation="longitude" precision="15" seconds="24" value="26.606667">26°36'24&quot;E</geoCoordinate>
], 500 m, 13.xi.2006, G. Osella leg., 1♂ (BAQ); Eastern Cape, W Bisho, km 35 da Peddie s.s.n. 2, 280 m, 33°16'69&quot;S, 26°48'95&quot;E [
<geoCoordinate id="03E75195EBA9925290D05536A109C83E" degrees="33" direction="south" minutes="14" orientation="latitude" precision="15" seconds="24" value="-33.24">33°14'24&quot;S</geoCoordinate>
