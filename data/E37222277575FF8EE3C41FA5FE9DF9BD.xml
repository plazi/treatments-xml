<quantity id="AC233ED47575FF8EE1281851FD5FFAC5" box="[635,717,1296,1319]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="1" pageNumber="288" unit="mm" value="0.9">0.9 mm</quantity>
