<taxonomicName id="E6CC0DB1C57B1510FCFCBFD1FC49FA68" baseAuthorityName="Leprieur" baseAuthorityYear="1876" box="[831,943,1436,1460]" class="Insecta" family="Dytiscidae" genus="Deronectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="fairmairei">
<emphasis id="13B8AA20C57B1510FCFCBFD1FC49FA68" box="[831,943,1436,1460]" italics="true" pageId="24" pageNumber="25">fairmairei</emphasis>
</taxonomicName>
-grοup, 
<taxonomicName id="E6CC0DB1C57B1510FBC9BFD0FB62FA69" baseAuthorityName="Fairmaire" baseAuthorityYear="1858" box="[1034,1156,1436,1461]" class="Insecta" family="Dytiscidae" genus="Deronectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="lareynii">
