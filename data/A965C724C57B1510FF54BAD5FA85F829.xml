<taxonomicName id="E6CC0DB1C57B1510FB2BBFA8FA9BFA21" authorityName="Fery &amp; Fresneda" authorityYear="1988" box="[1256,1405,1508,1533]" class="Insecta" family="Dytiscidae" genus="Deronectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="algibensis">
<emphasis id="13B8AA20C57B1510FB2BBFA8FA9BFA21" box="[1256,1405,1508,1533]" italics="true" pageId="24" pageNumber="25">D. algibensis</emphasis>
