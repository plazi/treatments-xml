<taxonomicName id="07F9B7A5B5D4861B046B51B524F902D2" authority="St Laurent, 2016" authorityName="St Laurent" authorityYear="2016" class="Insecta" family="Mimallonidae" genus="Cunicumara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cunicumara anae" order="Lepidoptera" pageId="26" pageNumber="83" phylum="Arthropoda" rank="species" species="anae">Cunicumara anae</taxonomicName>
