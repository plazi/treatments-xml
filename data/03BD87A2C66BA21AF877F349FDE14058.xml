<taxonomicName id="4C144D37C66BA219F414F112F3B04DE9" authorityName="Temminck" authorityYear="1834" box="[3052,3167,3720,3759]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="29" pageNumber="256" phylum="Chordata" rank="species" species="bicolor">
