<taxonomicName id="109A4D2CFF966055FB16291B2F09FDE6" box="[1226,1328,576,601]" class="Insecta" family="Eriococcidae" genus="Kuwanina" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="parva">
