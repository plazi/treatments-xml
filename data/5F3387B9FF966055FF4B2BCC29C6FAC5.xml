</taxonomicName>
(
<collectionCode id="B18BAE6AFF966055FA9D291B2FADFDE5" LSID="urn:lsid:biocol.org:col:34070" box="[1345,1428,576,602]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34070" name="New Zealand Arthropod Collection" pageId="9" pageNumber="10">NZAC</collectionCode>
