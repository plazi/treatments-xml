<typeStatus id="54CF887CFF97731B1C5CFCA6FB159459" box="[1134,1216,845,869]" pageId="46" pageNumber="323">syntype</typeStatus>
series as 
<specimenCount id="9D72FD57FF97731B1D10FCA6FA5B9459" box="[1314,1422,845,869]" pageId="46" pageNumber="323" type="male">two males</specimenCount>
and 
<specimenCount id="9D72FD57FF97731B1B74FC81FC1F94BE" box="[838,970,874,898]" pageId="46" pageNumber="323" type="female">five females</specimenCount>
