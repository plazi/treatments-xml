<collectionCode id="ED65AE1BFF97731B1C77FCF9FBA59416" box="[1093,1136,786,810]" country="Australia" name="Australian Museum" pageId="46" pageNumber="323" type="Museum">AM</collectionCode>
<typeStatus id="54CF887CFF97731B1C47FCF8FB129417" box="[1141,1223,787,811]" pageId="46" pageNumber="323">syntype</typeStatus>
