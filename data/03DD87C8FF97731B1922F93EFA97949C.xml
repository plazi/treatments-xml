<collectingCounty id="62AA4E52FF97731B1CD8FD1EFAB39431" box="[1258,1382,757,781]" pageId="46" pageNumber="323">Mary River</collectingCounty>
in 
<date id="FFCA101EFF97731B1DB8FD1EFCA89416" pageId="46" pageNumber="323" value="1895-05">
<collectingDate id="EF8EE9F6FF97731B1DB8FD1EFCA89416" pageId="46" pageNumber="323" value="1895-05">May 1895</collectingDate>
</date>
