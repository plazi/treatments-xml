<collectionCode id="ED65AE1BFF97731B1D1DFC80FA5494BE" box="[1327,1409,875,898]" pageId="46" pageNumber="323">BMNH</collectionCode>
, 
<collectionCode id="ED65AE1BFF97731B1DBEFC80FA6394BE" box="[1420,1462,875,898]" country="0" name="University of Montana Museum" pageId="46" pageNumber="323">MV</collectionCode>
