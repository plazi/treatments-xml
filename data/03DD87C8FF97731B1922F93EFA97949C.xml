<subSubSection id="C36E6555FF97731B1B75FD94FA97949C" pageId="46" pageNumber="323" type="discussion">
<paragraph id="8BCB36DEFF97731B1B75FD94FA97949C" blockId="46.[837,1468,639,928]" pageId="46" pageNumber="323">
<emphasis id="B900EACCFF97731B1B75FD94FC1495AB" bold="true" box="[839,961,639,663]" pageId="46" pageNumber="323">Comments</emphasis>
. 
<materialsCitation id="3B1C3C83FF97731B1BFCFD6BFAEB949C" collectingDate="1895-05" collectionCode="AM, BMNH, MV, NHMO, RMNH" collectorName="Wigg &amp; Bachmann &amp; The &amp; Wiig" county="Mary River" location="Type" municipality="Collett" pageId="46" pageNumber="323" specimenCount="6" specimenCount-adult="1" specimenCount-female="1" specimenCount-juvenile="1" specimenCount-male="2" typeStatus="syntype">
