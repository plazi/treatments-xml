<specimenCount id="9D72FD57FF97731B1B56FD76FC169589" box="[868,963,669,693]" pageId="46" pageNumber="323" type="male">one male</specimenCount>
, 
<specimenCount id="9D72FD57FF97731B1BFEFD76FBF99589" box="[972,1068,669,693]" pageId="46" pageNumber="323" type="adult">five adult</specimenCount>
