<location id="8EAB6005FF97731B1DB6FD76FA6E9589" LSID="urn:lsid:plazi:treatment:03DD87C8FF97731B1922F93EFA97949C:8EAB6005FF97731B1DB6FD76FA6E9589" box="[1412,1467,669,693]" county="Mary River" municipality="Collett" name="Skull" pageId="46" pageNumber="323">Skull</location>
