<collectionCode id="ED65AE1BFF97731B1B75FC63FC4D949C" box="[839,920,904,928]" country="Norway" httpUri="http://grbio.org/cool/2knt-7f1r" name="Natural History Museum, University of Oslo" pageId="46" pageNumber="323" type="Museum">NHMO</collectionCode>
and 
<collectionCode id="ED65AE1BFF97731B1BF9FC63FBCE94A3" box="[971,1051,904,927]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="46" pageNumber="323" type="Museum">RMNH</collectionCode>
