; 
<quantity id="4CA59B109D61FFD6FA8BFB3F345DFB39" box="[1313,1382,1233,1257]" metricMagnitude="2" metricUnit="m" metricValue="1.43" pageId="29" pageNumber="680" unit="m" value="143.0">143 m</quantity>
; 
