<caption ID-DOI="http://doi.org/10.5281/zenodo.3897032" ID-Zenodo-Dep="3897032" box="[151,1311,1908,1933]" httpUri="https://zenodo.org/record/3897032/files/figure.png" pageId="212" pageNumber="213" startId="212.[151,250,1908,1933]" targetBox="[155,1436,604,1875]" targetPageId="212">
<paragraph blockId="212.[151,1311,1908,1933]" box="[151,1311,1908,1933]" pageId="212" pageNumber="213">
