<caption ID-DOI="http://doi.org/10.5281/zenodo.3897126" ID-Zenodo-Dep="3897126" httpUri="https://zenodo.org/record/3897126/files/figure.png" pageId="288" pageNumber="289" startId="288.[151,250,1877,1902]" targetBox="[151,1436,193,1852]" targetPageId="288">
<paragraph blockId="288.[151,1436,1877,1938]" pageId="288" pageNumber="289">
<emphasis bold="true" box="[151,301,1877,1902]" pageId="288" pageNumber="289">FIGURE 152.</emphasis>
<taxonomicName authorityName="New" authorityYear="1985" box="[307,529,1878,1902]" class="Insecta" family="Myrmeleontidae" genus="Riekoleon" kingdom="Animalia" order="Neuroptera" pageId="288" pageNumber="289" phylum="Arthropoda" rank="species" species="convergens">
<emphasis box="[307,529,1878,1902]" italics="true" pageId="288" pageNumber="289">Riekoleon convergens</emphasis>
</taxonomicName>
: 
