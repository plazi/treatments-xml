<specimenCount box="[391,412,1413,1439]" pageId="36" pageNumber="37" type="male">♂</specimenCount>
, 
<collectingRegion box="[429,635,1413,1439]" country="Australia" name="Western Australia" pageId="36" pageNumber="37">Western Australia</collectingRegion>
, Kimberley District, May, Mjoberg (labeled 
<taxonomicName authorityName="Esben-Petersen" authorityYear="1918" class="Insecta" family="Myrmeleontidae" genus="Anomaloplectron" kingdom="Animalia" order="Neuroptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="lineatipenne">
<emphasis italics="true" pageId="36" pageNumber="37">Anomaloplectron lineatipenne</emphasis>
</taxonomicName>
