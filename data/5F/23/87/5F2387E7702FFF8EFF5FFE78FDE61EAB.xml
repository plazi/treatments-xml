<subSubSection box="[151,1253,580,605]" pageId="44" pageNumber="45" type="type_taxon">
<paragraph blockId="44.[151,1253,580,605]" box="[151,1253,580,605]" pageId="44" pageNumber="45">
<emphasis bold="true" box="[151,291,580,605]" pageId="44" pageNumber="45">
<typeStatus box="[151,204,581,605]" pageId="44" pageNumber="45">Type</typeStatus>
species.
</emphasis>
<taxonomicName authorityName="New" authorityYear="1985" box="[298,535,581,605]" class="Insecta" family="Myrmeleontidae" genus="Glenoleon" kingdom="Animalia" order="Neuroptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="nigristriatus">
