, 
<taxonomicName authorityName="New" authorityYear="1985" class="Insecta" family="Myrmeleontidae" genus="Parvoleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="71" pageNumber="72">Parvoleon</emphasis>
</taxonomicName>
, and 
<taxonomicName authorityName="Miller &amp; Stange" authorityYear="2012" box="[252,341,1372,1398]" class="Insecta" family="Myrmeleontidae" genus="Speleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[252,341,1372,1398]" italics="true" pageId="71" pageNumber="72">Speleon</emphasis>
</taxonomicName>
) (
<bibRefCitation author="Stange, L. A." box="[364,509,1372,1398]" pageId="71" pageNumber="72" pagination="1 - 565" refId="ref218011" refString="Stange, L. A. (2004) A systematic catalog, bibliography and classification of the world antlions (Insecta: Neuroptera: Myrmeleontidae). Memoirs of the American Entomological Institute, 74, [i-iv] + 1 - 565." type="journal article" year="2004">Stange 2004</bibRefCitation>
; 
<bibRefCitation author="Miller, R. B. &amp; Stange, L. A." box="[519,767,1372,1398]" pageId="71" pageNumber="72" pagination="1 - 65" refId="ref217418" refString="Miller, R. B. &amp; Stange, L. A. (2012) The cave mouth antlions of Australia (Neuroptera: Myrmeleontidae). Insecta Mundi, 250, 1 - 65." type="journal article" year="2012">Miller &amp; Stange 2012</bibRefCitation>
), which are not treated in this revision; and six that form the monophyletic 
<taxonomicName authorityName="Gerstaecker-Stange" authorityYear="1888" box="[313,437,1408,1434]" class="Insecta" family="Myrmeleontidae" genus="Periclystus" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[313,437,1408,1434]" italics="true" pageId="71" pageNumber="72">Periclystus</emphasis>
</taxonomicName>
genus group (
<taxonomicName authorityName="Esben-Petersen" authorityYear="1917" box="[601,830,1408,1434]" class="Insecta" family="Myrmeleontidae" genus="Austrogymnocnemia" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[601,830,1408,1434]" italics="true" pageId="71" pageNumber="72">Austrogymnocnemia</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Banks" authorityYear="1913" box="[845,962,1408,1434]" class="Insecta" family="Myrmeleontidae" genus="Glenoleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[845,962,1408,1434]" italics="true" pageId="71" pageNumber="72">Glenoleon</emphasis>
</taxonomicName>
, 
<taxonomicName authority="Machado &amp; Oswald, 2020" authorityName="Machado &amp; Oswald" authorityYear="2020" box="[977,1145,1408,1434]" class="Insecta" family="Myrmeleontidae" genus="Megagonoleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis box="[977,1145,1408,1434]" italics="true" pageId="71" pageNumber="72">Megagonoleon</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1153,1259,1408,1434]" pageId="71" pageNumber="72">
<taxonomicNameLabel box="[1153,1259,1408,1434]" pageId="71" pageNumber="72" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authority="Machado &amp; Oswald, 2020" authorityName="Machado &amp; Oswald" authorityYear="2020" box="[1274,1380,1408,1434]" class="Insecta" family="Myrmeleontidae" genus="Minyleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis box="[1274,1380,1408,1434]" italics="true" pageId="71" pageNumber="72">Minyleon</emphasis>
</taxonomicName>
<emphasis bold="true" pageId="71" pageNumber="72">
<taxonomicNameLabel pageId="71" pageNumber="72" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName authorityName="Gerstaecker-Stange" authorityYear="1888" box="[214,338,1444,1470]" class="Insecta" family="Myrmeleontidae" genus="Periclystus" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[214,338,1444,1470]" italics="true" pageId="71" pageNumber="72">Periclystus</emphasis>
</taxonomicName>
, and 
<taxonomicName authorityName="New" authorityYear="1985" box="[399,511,1444,1470]" class="Insecta" family="Myrmeleontidae" genus="Riekoleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[399,511,1444,1470]" italics="true" pageId="71" pageNumber="72">Riekoleon</emphasis>
</taxonomicName>
), which are treated here. The former 
<taxonomicName authorityName="Stange" authorityYear="1976" box="[929,1061,1444,1470]" class="Insecta" family="Myrmeleontidae" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="subTribe" subTribe="Periclystina">Periclystina</taxonomicName>
genera 
<taxonomicName authorityName="Esben-Petersen" authorityYear="1917" box="[1149,1275,1444,1470]" class="Insecta" family="Myrmeleontidae" genus="Ceratoleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[1149,1275,1444,1470]" italics="true" pageId="71" pageNumber="72">Ceratoleon</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Esben-Petersen" authorityYear="1923" box="[1329,1436,1444,1470]" class="Insecta" family="Myrmeleontidae" genus="Platyleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[1329,1436,1444,1470]" italics="true" pageId="71" pageNumber="72">Platyleon</emphasis>
</taxonomicName>
are synonymized here with 
<taxonomicName authorityName="Esben-Petersen" authorityYear="1917" box="[461,690,1480,1506]" class="Insecta" family="Myrmeleontidae" genus="Austrogymnocnemia" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[461,690,1480,1506]" italics="true" pageId="71" pageNumber="72">Austrogymnocnemia</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Banks" authorityYear="1913" box="[744,861,1480,1506]" class="Insecta" family="Myrmeleontidae" genus="Glenoleon" kingdom="Animalia" order="Neuroptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="genus">
<emphasis box="[744,861,1480,1506]" italics="true" pageId="71" pageNumber="72">Glenoleon</emphasis>
</taxonomicName>
respectively.
