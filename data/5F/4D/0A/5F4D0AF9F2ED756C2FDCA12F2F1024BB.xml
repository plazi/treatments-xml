<paragraph pageId="44" pageNumber="129">
<taxonomicName class="Insecta" family="Staphylinidae" genus="Paragoniusa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paragoniusa myrmicae" order="Coleoptera" pageId="44" pageNumber="129" phylum="Arthropoda" rank="species" species="myrmicae">
