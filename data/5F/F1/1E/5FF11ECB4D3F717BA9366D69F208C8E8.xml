<taxonomicName LSID="http://zoobank.org/2AFEC3A5-8B40-483E-BD13-CFFFE13E0351" class="Arachnida" family="Sparassidae" genus="Diminutella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diminutella" order="Araneae" pageId="3" pageNumber="16" phylum="Arthropoda" rank="genus">Diminutella</taxonomicName>
