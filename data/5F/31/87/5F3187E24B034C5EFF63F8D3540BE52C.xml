(
<emphasis box="[493,664,1827,1851]" italics="true" pageId="15" pageNumber="378">Tropidoscaphula</emphasis>
) 
<emphasis box="[680,857,1827,1851]" italics="true" pageId="15" pageNumber="378">megalodacnoides</emphasis>
</taxonomicName>
<emphasis bold="true" box="[866,948,1827,1851]" pageId="15" pageNumber="378">
<taxonomicNameLabel box="[866,948,1827,1851]" pageId="15" pageNumber="378" rank="species">sp. nov.</taxonomicNameLabel>
