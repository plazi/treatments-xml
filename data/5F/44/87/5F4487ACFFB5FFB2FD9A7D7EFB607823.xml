<taxonomicName authorityName="Kröger &amp; Pohle" authorityYear="2021" box="[875,1139,1886,1912]" class="Cephalopoda" family="Rioceratidae" genus="Svalbardoceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tetrabranchia" pageId="26" pageNumber="27" phylum="Mollusca" rank="species" species="sterna">
