<taxonomicName authority="Flower, 1964" authorityName="Flower" authorityYear="1964" box="[351,618,300,326]" class="Cephalopoda" family="Baltoceratidae" genus="Rioceras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthocerida" pageId="29" pageNumber="30" phylum="Mollusca" rank="genus">
