<taxonomicName authorityName="Matsumura" authorityYear="1927" box="[508,671,802,827]" class="Insecta" family="Xiphydriidae" genus="Xiphydria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="535" phylum="Arthropoda" rank="species" species="ogasawarai">
<emphasis box="[508,671,802,827]" italics="true" pageId="4" pageNumber="535">X. ogasawarai</emphasis>
</taxonomicName>
while the “ 
<typeStatus box="[798,948,801,827]" pageId="4" pageNumber="535">allolectotype</typeStatus>
