<emphasis bold="true" box="[151,760,1567,1591]" pageId="0" pageNumber="595">
<taxonomicName authority="(Marcuzzi, 1985)" authorityName="Faúndez &amp; Rider &amp; Carvajal" authorityYear="2014" baseAuthorityName="Marcuzzi" baseAuthorityYear="1985" box="[151,663,1567,1591]" class="Insecta" family="Tenebrionidae" genus="Allotriocochabambia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="595" phylum="Arthropoda" rank="species" species="kulzeri" status="n.comb.">
