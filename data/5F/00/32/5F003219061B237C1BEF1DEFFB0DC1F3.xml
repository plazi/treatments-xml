<taxonomicName authorityName="Jakovlev" authorityYear="1884" box="[1128,1230,666,687]" class="Insecta" family="Miridae" genus="Brachycoleus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="106" phylum="Arthropoda" rank="species" species="lineellus">
