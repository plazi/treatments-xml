<emphasis box="[332,572,1968,1992]" italics="true" pageId="4" pageNumber="524">
<taxonomicName authorityName="Lacordaire" authorityYear="1868" box="[332,568,1968,1992]" class="Insecta" family="Cerambycidae" genus="Nothopleurus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="524" phylum="Arthropoda" rank="species" species="ebeninus">Nothopleurus ebeninus</taxonomicName>
