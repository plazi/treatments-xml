<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[318,408,1421,1447]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="9" pageNumber="10">MNHN</collectionCode>
