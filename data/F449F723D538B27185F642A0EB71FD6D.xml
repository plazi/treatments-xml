<taxonomicName id="BBE03DB6D539B271871B442FEA09FBD8" box="[514,637,1098,1119]" class="Insecta" family="Formicidae" genus="Atta" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="mexicana">
