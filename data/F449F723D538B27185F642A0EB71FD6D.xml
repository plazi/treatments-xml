<taxonomicName id="BBE03DB6D539B271858E444AE93BFBC3" box="[151,335,1071,1092]" class="Magnoliopsida" family="Asteraceae" genus="Helianthus" kingdom="Plantae" order="Asterales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="annus">
