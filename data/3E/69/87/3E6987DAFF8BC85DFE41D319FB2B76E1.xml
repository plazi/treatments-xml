(Lambayeque; 
<location LSID="urn:lsid:plazi:treatment:3E6987DAFF8BC85DFE41D319FB2B76E1:B31F6017FF8BC85DFC2BD282FC3D73B4" box="[917,1014,896,922]" country="Colombia" county="Based" municipality="Andean" name="Pacheco" pageId="22" stateProvince="Cochabamba">Pacheco</location>
