; see 
<location LSID="urn:lsid:plazi:treatment:3E6987DAFF8BC85DFE41D319FB2B76E1:B31F6017FF8BC85DFDFDD2A7FD667391" box="[579,685,933,959]" country="Colombia" county="Based" municipality="Andean" name="Remarks" pageId="22" stateProvince="Cochabamba">Remarks</location>
