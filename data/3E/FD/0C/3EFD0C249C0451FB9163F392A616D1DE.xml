<emphasis italics="true" pageId="0" pageNumber="1">Michelia rubriflora</emphasis>
</taxonomicName>
and 
<taxonomicName genus="M." lsidName="M. mediocris" pageId="0" pageNumber="1" rank="species" species="mediocris">
<emphasis italics="true" pageId="0" pageNumber="1">M. mediocris</emphasis>
