<quantity box="[1115,1245,1242,1268]" metricMagnitude="2" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="6" pageNumber="46" unit="m" value="400.0" valueMax="500.0" valueMin="300.0">300–500m</quantity>
, 
<number box="[1257,1321,1242,1268]" pageId="6" pageNumber="46" value="2000.0">2000</number>
. 
<date box="[1325,1381,1242,1268]" pageId="6" pageNumber="46" value="1928-05">
V. 
