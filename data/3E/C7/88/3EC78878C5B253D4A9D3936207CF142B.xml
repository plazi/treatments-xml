<taxonomicName authority="Viswajyothi &amp; Konstantinov, 2020" authorityName="Viswajyothi &amp; Konstantinov" authorityYear="2020" class="Insecta" family="Chrysomelidae" genus="Blakealtica" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Blakealtica fusca" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="fusca">
