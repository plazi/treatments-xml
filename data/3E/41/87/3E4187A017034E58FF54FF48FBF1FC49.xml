.
</emphasis>
<specimenCount box="[521,542,259,282]" pageId="80" pageNumber="81" type="female">♀</specimenCount>
, “ 
<collectingRegion box="[570,682,258,283]" country="Australia" name="Tasmania" pageId="80" pageNumber="81">Tasmania</collectingRegion>
, 
<collectorName box="[698,821,258,283]" pageId="80" pageNumber="81">A. Simson</collectorName>
