1. 
<normalizedToken originalValue="Blütenstiele" pageId="null" pageNumber="700">Bluetenstiele</normalizedToken>
<normalizedToken originalValue="kürzer" pageId="null" pageNumber="700">kuerzer</normalizedToken>
als die 
<normalizedToken originalValue="Blüten" pageId="null" pageNumber="700">Blueten</normalizedToken>
; 
<normalizedToken originalValue="Früchte" pageId="null" pageNumber="700">Fruechte</normalizedToken>
kahl, fast sitzend
</td>
</tr>
<tr pageId="null" pageNumber="700">
