<paragraph blockId="187.[151,1074,227,283]" pageId="187" pageNumber="644">
<taxonomicName authorityName="Makranczy" authorityYear="2014" box="[151,234,228,251]" class="Magnoliopsida" family="Melastomataceae" genus="Ochthephilus" kingdom="Plantae" order="Myrtales" pageId="187" pageNumber="644" phylum="Tracheophyta" rank="species" species="ritae">
