of 
<emphasis box="[113,339,850,876]" italics="true" pageId="30" pageNumber="259">
<taxonomicName authorityName="Bourne" authorityYear="1922" box="[113,335,850,876]" class="Malacostraca" family="Raninidae" genus="Notosceles" kingdom="Animalia" order="Decapoda" pageId="30" pageNumber="259" phylum="Arthropoda" rank="species" species="chimmonis">Notosceles chimmonis</taxonomicName>
.
