<subSubSection box="[197,969,378,404]" pageId="28" pageNumber="29" type="biology_ecology">
<paragraph blockId="28.[151,1436,152,1023]" box="[197,969,378,404]" pageId="28" pageNumber="29">
<emphasis bold="true" box="[197,302,378,404]" pageId="28" pageNumber="29">Ecology.</emphasis>
