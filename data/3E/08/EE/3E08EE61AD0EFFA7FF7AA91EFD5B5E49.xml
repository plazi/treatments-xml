<collectingCountry box="[365,527,1196,1222]" name="New Zealand" pageId="5" pageNumber="6">New Zealand</collectingCountry>
: Giggenbach submarine volcano, Kermadec Volcanic Arc; 
<specimenCount box="[1220,1301,1196,1222]" pageId="5" pageNumber="6" type="generic">
<quantity box="[1220,1297,1196,1222]" metricMagnitude="2" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="6" unit="m" value="160.0">160 m</quantity>
