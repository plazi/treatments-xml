<subSubSection pageId="5" pageNumber="6" type="etymology">
<paragraph blockId="5.[151,1436,1771,1991]" pageId="5" pageNumber="6">
<emphasis bold="true" box="[198,339,1926,1952]" pageId="5" pageNumber="6">Etymology.</emphasis>
