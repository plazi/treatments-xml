<subSubSection box="[197,1294,1079,1106]" pageId="9" pageNumber="10" type="biology_ecology">
<paragraph blockId="9.[151,1436,152,1492]" box="[197,1294,1079,1106]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[197,302,1079,1105]" pageId="9" pageNumber="10">Ecology.</emphasis>
<taxonomicName authorityName="Kilgallen" authorityYear="2009" box="[309,405,1080,1105]" class="Malacostraca" family="Endevouridae" genus="Ensayara" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="ursus">
