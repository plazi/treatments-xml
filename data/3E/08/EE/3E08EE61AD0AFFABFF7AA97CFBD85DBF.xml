<taxonomicName authorityName="Kilgallen" authorityYear="2009" box="[555,625,1927,1952]" class="Malacostraca" family="Endevouridae" genus="Ensayara" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="ursus">
