</date>
, 
<collectingMethod box="[496,650,188,213]" pageId="15" pageNumber="331">Malaise traps</collectingMethod>
; 
<collectorName box="[661,764,188,213]" pageId="15" pageNumber="331">I.F. Melo</collectorName>
