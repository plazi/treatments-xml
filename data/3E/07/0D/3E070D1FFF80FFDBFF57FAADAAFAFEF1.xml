<location LSID="urn:lsid:plazi:treatment:3E070D1FFF80FFDBFF57FAADAAFAFEF1:B371EAD2FF81FFDBFF07FF6EAF30FF01" box="[199,303,151,176]" country="Brazil" county="Usina Bela Vista" municipality="Pontal" name="Material" pageId="15" pageNumber="331">Material</location>
examined.
</emphasis>
<typeStatus box="[446,550,151,176]" pageId="15" pageNumber="331">Holotype</typeStatus>
(DCBU-34590); female, 
<collectingCountry box="[849,920,151,176]" name="Brazil" pageId="15" pageNumber="331">Brazil</collectingCountry>
, SP, 
<collectingMunicipality box="[981,1056,151,176]" pageId="15" pageNumber="331">Pontal</collectingMunicipality>
, 
<collectingCounty box="[1070,1268,151,176]" pageId="15" pageNumber="331">Usina Bela Vista</collectingCounty>
, semidecidous mesophilic forest, 
<date box="[358,485,188,212]" pageId="15" pageNumber="331" value="2010-04-14">
