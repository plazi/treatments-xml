<taxonomicName class="Insecta" family="Gelechiidae" genus="Phthorimaea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phthorimaea operculella" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="operculella">Phthorimaea operculella</taxonomicName>
