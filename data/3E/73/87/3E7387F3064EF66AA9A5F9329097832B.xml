<geoCoordinate box="[988,1172,300,327]" degrees="09" direction="north" minutes="20" orientation="latitude" pageId="31" pageNumber="252" precision="1" seconds="0.24" value="9.3334">09° 20’ 0.24”N</geoCoordinate>
, 
<geoCoordinate box="[1183,1358,300,327]" degrees="82" direction="west" minutes="13" orientation="longitude" pageId="31" pageNumber="252" precision="1" seconds="6.84" value="-82.21857">82°13’6.84”W</geoCoordinate>
), from the canals of 
<taxonomicName box="[306,444,338,363]" class="Demospongiae" family="Halichondriidae" genus="Hymeniacidon" kingdom="Animalia" order="Suberitida" pageId="31" pageNumber="252" phylum="Porifera" rank="species" species="caerulea">
