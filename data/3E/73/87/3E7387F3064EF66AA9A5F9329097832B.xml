<collectingCountry box="[856,958,264,290]" name="Curaçao" pageId="31" pageNumber="252">Curaçao</collectingCountry>
, CL 
<quantity box="[1017,1111,264,290]" metricMagnitude="-3" metricUnit="m" metricValue="4.6" pageId="31" pageNumber="252" unit="mm" value="4.6">4.6 mm</quantity>
