<taxonomicName authorityName="Walker" authorityYear="1874" box="[725,864,244,269]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="51" pageNumber="1165" phylum="Arthropoda" rank="species" species="semiluteus">
<emphasis box="[725,864,244,269]" italics="true" pageId="51" pageNumber="1165">B. semiluteus</emphasis>
</taxonomicName>
with 
<taxonomicName authority="(Wesmael, 1838)" baseAuthorityName="Wesmael" baseAuthorityYear="1838" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="51" pageNumber="1165" phylum="Arthropoda" rank="species" species="piger" subGenus="Pigeria">
<emphasis box="[925,1001,244,269]" italics="true" pageId="51" pageNumber="1165">Bracon</emphasis>
(
<emphasis box="[1013,1087,244,269]" italics="true" pageId="51" pageNumber="1165">Pigeria</emphasis>
) 
<emphasis box="[1100,1155,244,269]" italics="true" pageId="51" pageNumber="1165">piger</emphasis>
(
<bibRefCitation author="Wesmael C" box="[166,338,279,304]" pageId="51" pageNumber="1165" pagination="1 - 166" refId="ref25252" refString="Wesmael C. 1838. Monographie des Braconides de Belgique. 4. Nouveaux Mem Acad R Sci et Belleslettres Bruxelles. 11: 1 - 166." type="journal article" year="1838">Wesmael, 1838</bibRefCitation>
)
</taxonomicName>
, but 
<bibRefCitation author="Papp J" box="[406,542,279,304]" pageId="51" pageNumber="1165" pagination="1 - 154" refId="ref23457" refString="Papp J. 2012. A revision of the Bracon Fabricius species in Wesmael's collection deposited in Brussels (Hymenoptera: Braconidae: Braconinae). Eur J Taxonomy. 21: 1 - 154. doi: 10.5852 / ejt. 2012.21." type="journal article" year="2012">Papp (2012)</bibRefCitation>
