</taxonomicName>
(= 
<taxonomicName authority="(Walker, 1874))" authorityName="" baseAuthorityName="Walker" baseAuthorityYear="1874" box="[787,1122,1329,1352]" class="Insecta" family="Ichneumonidae" genus="Hyposoter" kingdom="Animalia" order="Hymenoptera" pageId="76" pageNumber="1190" phylum="Arthropoda" rank="species" species="ventralis">
