(
<bibRefCitation author="Walker F" box="[639,798,1013,1038]" pageId="74" pageNumber="1188" pagination="301 - 310" refId="ref25043" refString="Walker F. 1874. Descriptions of some Japanese Hymenoptera. Cistula Entomol. 1: 301 - 310." type="journal article" year="1874">Walker, 1874</bibRefCitation>
)
</taxonomicName>
(
<taxonomicName authorityName="Nees von Esenbeck" authorityYear="1811" box="[831,960,1013,1038]" class="Insecta" family="Braconidae" kingdom="Animalia" order="Hymenoptera" pageId="74" pageNumber="1188" phylum="Arthropoda" rank="family">Braconidae</taxonomicName>
: 
<taxonomicName authorityName="Forster" authorityYear="1862" box="[983,1152,1013,1038]" class="Insecta" family="Braconidae" kingdom="Animalia" order="Hymenoptera" pageId="74" pageNumber="1188" phylum="Arthropoda" rank="subFamily" subFamily="Microgastrinae">Microgastrinae</taxonomicName>
: 
<taxonomicName authorityName="Mason" authorityYear="1981" box="[160,291,1048,1073]" class="Insecta" family="Braconidae" kingdom="Animalia" order="Hymenoptera" pageId="74" pageNumber="1188" phylum="Arthropoda" rank="tribe" tribe="Microplitini">Microplitini</taxonomicName>
).
</paragraph>
