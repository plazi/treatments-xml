</taxonomicName>
(= 
<taxonomicName authority="(Gravenhorst, 1820))" authorityName="" baseAuthorityName="Gravenhorst" baseAuthorityYear="1820" class="Insecta" family="Ichneumonidae" genus="Perilissus" kingdom="Animalia" order="Hymenoptera" pageId="72" pageNumber="1186" phylum="Arthropoda" rank="species" species="rufoniger">
