<subSubSection id="92A826F8E807FFCAFF6AF8D12EEFFE42" lastPageId="7" lastPageNumber="6" pageId="6" pageNumber="5" type="type_taxon">
<paragraph id="DA0D7573E807FFCBFF6AF8D1297BF85E" blockId="6.[192,820,1904,1929]" box="[192,820,1904,1929]" pageId="6" pageNumber="5">
<emphasis id="E8C6A961E807FFCBFF6AF8D12B38F85F" bold="true" box="[192,375,1904,1928]" pageId="6" pageNumber="5">
<typeStatus id="0509CBD1E807FFCBFF6AF8D12B4BF85F" box="[192,260,1904,1928]" pageId="6" pageNumber="5">Type</typeStatus>
