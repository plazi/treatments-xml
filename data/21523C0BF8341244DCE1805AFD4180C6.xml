<treatmentCitationGroup id="89EBAA33F8341244DCE1805AFC9F81FB" box="[151,856,1654,1680]" pageId="5" pageNumber="50">
<taxonomicName id="6EFBF69EF8341244DCE1805AFC9F81FB" authority="Mound, Tree &amp; Goldarazena, 2010: 64" authorityName="Mound, Tree &amp; Goldarazena" authorityPageNumber="64" authorityYear="2010" box="[151,856,1654,1680]" class="Insecta" family="Thripidae" genus="Scolothrips" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="50" phylum="Arthropoda" rank="species" species="ochoa">
<emphasis id="9B8F510FF8341244DCE1805AFC9F81FB" bold="true" box="[151,856,1654,1680]" pageId="5" pageNumber="50">
<emphasis id="9B8F510FF8341244DCE1805AFEB681FB" bold="true" box="[151,369,1654,1680]" italics="true" pageId="5" pageNumber="50">Scolothrips ochoa</emphasis>
<treatmentCitation id="285AAB0CF8341244DD01805AFC9F81FB" ID-DOI="http://doi.org/10.5281/zenodo.3507748" ID-Zenodo-Dep="3507748" author="Mound" box="[375,856,1654,1680]" httpUri="http://treatment.plazi.org/id/039B0A15FFE0FFAFB7F1FC0770E2D5CE" page="64" pageId="5" pageNumber="50" year="2010">
<bibRefCitation id="CD6AF0ECF8341244DD01805AFCE881FB" author="Mound" box="[375,815,1654,1680]" pageId="5" pageNumber="50" refString="Mound, L. A., Tree, D. J. &amp; Goldarazena, A. (2010) A new species of predatory Scolothrips (Thysanoptera, Thripidae) feeding on Raoiella mites (Tenuipalpidae) in Australia. Zootaxa, 2620, 63 - 68." type="journal article" year="2010">
Mound, Tree &amp; Goldarazena, 
<number id="1F339F3AF8341244DE86805AFCE881FB" box="[752,815,1654,1680]" pageId="5" pageNumber="50" value="2010.0">2010</number>
</bibRefCitation>
