<taxonomicName id="F5EA0AA4FF8C1B185B795862E126F8B1" box="[516,638,1809,1833]" class="Insecta" family="Hydrophilidae" genus="Oocyclus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="66" phylum="Arthropoda" rank="species" species="fikaceki">
