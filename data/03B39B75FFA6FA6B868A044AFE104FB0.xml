<quantity id="4CE28786FFA7FA6B858807C0FBDE4D30" box="[917,1089,232,258]" metricMagnitude="-2" metricUnit="m" metricValue="1.425" metricValueMax="1.49" metricValueMin="1.3599999999999999" pageId="3" pageNumber="4" unit="mm" value="14.25" valueMax="14.9" valueMin="13.6">13.6–14.9 mm</quantity>
(n = 
<number id="3DD23844FFA7FA6B829A07C0FB074D30" box="[1159,1176,232,258]" pageId="3" pageNumber="4" value="2.0">2</number>
