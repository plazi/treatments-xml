<taxonomicName id="AB04F34E5AE94BF48AEA97162E103BEB" authorityName="Korschefsky" authorityYear="1931" class="Insecta" family="Coccinellidae" genus="Nephus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nephus ornatulus" order="Coleoptera" pageId="0" pageNumber="35" phylum="Arthropoda" rank="species" species="ornatulus">
<emphasis id="6E92C8A5A9C62BE074FA6125C34B07B3" italics="true" pageId="0" pageNumber="35">Nephus ornatulus</emphasis>
