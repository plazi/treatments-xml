<taxonomicName id="27CA4D35FEA3FEC55CB7FEC6FB4E601C" box="[1193,1277,331,357]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="366" pageNumber="367" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
