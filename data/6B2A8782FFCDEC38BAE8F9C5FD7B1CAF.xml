<taxonomicName id="24834D17FFCDEC38BAE8F9C5FDC71D33" authority="Opresko, 2005" authorityName="Opresko" authorityYear="2005" box="[151,520,1615,1642]" class="Anthozoa" family="Schizopathidae" genus="Umbellapathes" kingdom="Animalia" order="Antipatharia" pageId="10" pageNumber="553" phylum="Cnidaria" rank="genus">
