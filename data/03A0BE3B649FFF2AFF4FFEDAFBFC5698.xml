<taxonomicName id="4C0974AE649FFF2AFF36FEDAFDC8560C" authority="Gervais, 1855" authorityName="Gervais" authorityYear="1855" box="[238,521,332,358]" class="Mammalia" family="Delphinidae" genus="Tursiops" kingdom="Animalia" order="Cetacea" pageId="149" pageNumber="150" phylum="Chordata" rank="genus">
