<subSubSection id="C3260796FF5EFF36DF36FB25FE3DA689" box="[93,484,1193,1216]" pageId="212" pageNumber="212" type="nomenclature">
<paragraph id="8B83541DFF5EFF36DF36FB25FE3DA689" blockId="212.[93,484,1193,1216]" box="[93,484,1193,1216]" pageId="212" pageNumber="212">
<heading id="D0CBE371FF5EFF36DF36FB25FE3DA689" box="[93,484,1193,1216]" fontSize="9" level="2" pageId="212" pageNumber="212" reason="6">
<emphasis id="B948880FFF5EFF36DF36FB25FE3DA689" bold="true" box="[93,484,1193,1216]" pageId="212" pageNumber="212">
<taxonomicName id="4C3C2F9EFF5EFF36DF36FB25FE9AA6F6" box="[93,323,1193,1216]" class="Insecta" family="Miridae" genus="Slaterocoris" kingdom="Animalia" order="Hemiptera" pageId="212" pageNumber="212" phylum="Arthropoda" rank="species" species="flavipes">
<emphasis id="B948880FFF5EFF36DF36FB25FE9AA6F6" bold="true" box="[93,323,1193,1216]" italics="true" pageId="212" pageNumber="212">Slaterocoris flavipes</emphasis>
</taxonomicName>
