<typeStatus id="D351880D366C280700B5FB16FDD134A0" box="[485,586,1165,1189]" pageId="5" pageNumber="423">Paratype</typeStatus>
, 
<number id="BA222488366C2807030AFB17FDF334A1" box="[602,616,1164,1188]" pageId="5" pageNumber="423" value="1.0">1</number>
♀, 
<collectingCountry id="74FD763F366C280703C0FB17FD4E34A0" box="[656,725,1164,1189]" name="China" pageId="5" pageNumber="423">China</collectingCountry>
, Yunnan, Jinping, Mengla, Wendang, Alt. 
<quantity id="CB129B4A366C2807059FFB17FA8934A0" box="[1231,1298,1164,1189]" metricMagnitude="2" metricUnit="m" metricValue="8.5" pageId="5" pageNumber="423" unit="m" value="850.0">850m</quantity>
