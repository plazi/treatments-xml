<specimenCount id="08ABFD1C0E4BFFE9FEC2A944FEA4FD89" box="[331,345,596,612]" pageId="112" pageNumber="253" type="female">♀</specimenCount>
, here designated, 
<collectionCode id="78BCAE500E4BFFE9FE71A943FDCCFD8E" box="[504,561,595,611]" country="0" httpUri="http://grbio.org/cool/1d5a-4efs" name="Natural History Museum Bucharest" pageId="112" pageNumber="253">NHMB</collectionCode>
<taxonomicName id="D9AD4D160E4BFFE9FDB2A943FF05FD97" authority="♀ Walter Aurig" authorityName="♀ Walter Aurig" authorityYear="1947" class="Arachnida" family="Hygrobatidae" genus="Megapus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="112" pageNumber="253" phylum="Arthropoda" rank="species" species="primitivus">
