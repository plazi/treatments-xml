<taxonomicName id="1E8C4D00FFBE8535DDEBF8CCFA85DB50" authorityName="Dujardin" authorityYear="1845" box="[1245,1371,1797,1823]" class="Trematoda" family="Opecoelidae" genus="Podocotyle" kingdom="Animalia" order="Plagiorchiida" pageId="15" pageNumber="522" phylum="Platyhelminthes" rank="genus">
<emphasis id="EBF8EA91FFBE8535DDEBF8CCFA85DB50" box="[1245,1371,1797,1823]" italics="true" pageId="15" pageNumber="522">Podocotyle</emphasis>
