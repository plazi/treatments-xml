<taxonomicName id="D64B62AF05B158CBD7CE83EFF8CADD49" class="Diplopoda" family="Haplodesmidae" genus="Prosopodesmus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Prosopodesmus panporus" order="Polydesmida" pageId="9" pageNumber="42" phylum="Arthropoda" rank="species" species="panporus">Prosopodesmus panporus</taxonomicName>
