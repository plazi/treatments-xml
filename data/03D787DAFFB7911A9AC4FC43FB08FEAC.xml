<subSubSection id="C3646547FFB7911A9A94F8ADFC2EFF31" lastPageId="3" lastPageNumber="328" pageId="2" pageNumber="327" type="materials_examined">
<paragraph id="8BC136CCFFB7911B9A94F8ADFEE2F865" blockId="2.[151,1437,1004,1964]" pageId="2" pageNumber="327">
<emphasis id="B90AEADEFFB7911B9A94F8ADFE7EF841" bold="true" box="[199,400,1903,1928]" pageId="2" pageNumber="327">Material studied</emphasis>
. 
<typeStatus id="54C5886EFFB7911B9BF1F8ADFDE4F841" box="[418,522,1903,1928]" pageId="2" pageNumber="327">Holotype</typeStatus>
female, 
