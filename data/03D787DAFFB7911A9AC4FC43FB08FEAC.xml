<number id="3DB624EBFFB7911B99CEF8C6FC08F8D4" box="[925,998,1796,1821]" pageId="2" pageNumber="327" value="60.55">60, 55</number>
, 
<number id="3DB624EBFFB7911B99A2F8C6FBD4F8D5" box="[1009,1082,1796,1821]" pageId="2" pageNumber="327" value="55.4">55, 40</number>
