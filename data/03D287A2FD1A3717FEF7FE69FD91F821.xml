''N
</geoCoordinate>
, 
<geoCoordinate id="EE4F5073FD1A3717FFF8FA61FDA2FA29" box="[408,574,1492,1516]" direction="east" orientation="longitude" pageId="19" pageNumber="20" precision="1" value="14.0369835">
<number id="3DB32493FD1A3717FFF8FA61FE28FA29" box="[408,436,1492,1516]" pageId="19" pageNumber="20" value="14.0">14</number>
°02' 
<number id="3DB32493FD1A3717FFBEFA61FDBEFA29" box="[478,546,1492,1516]" pageId="19" pageNumber="20" value="13.14">13.14</number>
''E
