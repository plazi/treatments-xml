1889. 
<taxonomicName id="4C7071ED9476FFEAFC9627C6BB43F44B" authority=", Topsent" authorityName="Topsent" box="[790,1378,2950,2978]" class="Demospongiae" family="Tedaniidae" genus="Tedania" kingdom="Animalia" order="Poecilosclerida" pageId="35" pageNumber="456" phylum="Porifera" rank="species" species="leptoderma">
