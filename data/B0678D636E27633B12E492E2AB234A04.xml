<paragraph id="7890CB60BDAA157E999ABFA7659CBA79" pageId="null" pageNumber="274">
<taxonomicName id="CFDE8E91D07DA0EF54CCA495204C15AE" authority="L." class="Magnoliopsida" family="Plantaginaceae" genus="Plantago" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="274" phylum="Tracheophyta" rank="species" species="lanceolata">
