<emphasis id="B971CE2FFF9CFFBD2DD9115AFCCBF9E9" bold="true" box="[731,829,1580,1601]" pageId="33" pageNumber="41">Remarks.</emphasis>
<taxonomicName id="4C0569BEFF9CFFBD2C44115AFBE2F9E9" authorityName="Casey" authorityYear="1914" box="[838,1044,1580,1602]" class="Insecta" family="Carabidae" genus="Selenophorus" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="41" phylum="Arthropoda" rank="species" species="otiosus">
