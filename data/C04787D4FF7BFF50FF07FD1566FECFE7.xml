<taxonomicName id="8FEE4D41FF7BFF50FE9BFBC46764CF3B" baseAuthorityName="Sulzer" baseAuthorityYear="1776" box="[267,551,1123,1148]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="marginatus">
