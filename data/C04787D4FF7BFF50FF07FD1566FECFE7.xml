<taxonomicName id="8FEE4D41FF7BFF50FAF8FBC464C4CFE7" baseAuthorityName="Sulzer" baseAuthorityYear="1776" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="marginatus">
