<collectingCountry box="[211,292,159,181]" name="Vietnam" pageId="12" pageNumber="221">Vietnam</collectingCountry>
in 
<collectingRegion box="[323,442,159,181]" country="Vietnam" name="Quang Ngai" pageId="12" pageNumber="221">Quang Ngai</collectingRegion>
