<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147239" authority="" authorityName="" class="Insecta" family="Formicidae" genus="Sphaerocrema" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Sphaerocrema Santschi" lsidName-HNS="Sphaerocrema Santschi" order="Hymenoptera" phylum="Arthropoda" rank="genus">Sphaerocrema</taxonomicName>
