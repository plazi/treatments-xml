<taxonomicName baseAuthorityName="Uchida" baseAuthorityYear="1928" class="Insecta" family="Ichneumonidae" genus="Enicospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enicospilus shinkanus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="shinkanus">
