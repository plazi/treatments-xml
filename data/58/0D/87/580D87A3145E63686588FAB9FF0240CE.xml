<subSubSection box="[146,898,1362,1384]" pageId="1" pageNumber="560" type="materials_examined">
<paragraph blockId="1.[146,898,1362,1384]" box="[146,898,1362,1384]" pageId="1" pageNumber="560">
<typeStatus box="[146,198,1362,1384]" pageId="1" pageNumber="560">Type</typeStatus>
species: 
<taxonomicName authority="López-Guerrero, 1921" authorityName="López-Guerrero" authorityYear="1921" box="[289,542,1362,1383]" class="Insecta" family="Buprestidae" genus="Camerunadora" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="560" phylum="Arthropoda" rank="species" species="bifasciata" status="new species">
