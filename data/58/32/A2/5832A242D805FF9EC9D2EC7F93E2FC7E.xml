<taxonomicName baseAuthorityName="FABRICIUS" baseAuthorityYear="1787" box="[896,1085,860,881]" class="Insecta" family="Cerambycidae" genus="Ceresium" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1583" phylum="Arthropoda" rank="species" species="unicolor">
