<collectingCountry box="[465,557,889,910]" name="Comoros" pageId="2" pageNumber="1583">Comoros</collectingCountry>
(
<collectingRegion box="[581,758,889,911]" country="Comoros" name="Grande Comore" pageId="2" pageNumber="1583">Grande Comore</collectingRegion>
