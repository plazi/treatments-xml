<collectingRegion box="[875,967,889,911]" country="Comoros" name="Anjouan" pageId="2" pageNumber="1583">Anjouan</collectingRegion>
), 
<collectingCountry box="[994,1081,890,911]" name="Mayotte" pageId="2" pageNumber="1583">Mayotte</collectingCountry>
, 
<collectingCountry box="[178,303,919,941]" name="Madagascar" pageId="2" pageNumber="1583">Madagascar</collectingCountry>
, 
<collectingCountry box="[313,401,919,941]" name="Réunion" pageId="2" pageNumber="1583">Réunion</collectingCountry>
