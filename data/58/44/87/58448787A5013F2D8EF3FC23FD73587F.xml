: 
<taxonomicName authorityName="Illiger" authorityYear="1811" box="[534,675,1442,1467]" class="Aves" family="Columbidae" kingdom="Animalia" order="Columbiformes" pageId="25" pageNumber="26" phylum="Chordata" rank="family">Columbidae</taxonomicName>
