); Palaearctic (
<collectingCountry box="[1342,1432,1755,1780]" name="Finland" pageId="31" pageNumber="32">Finland</collectingCountry>
, 
<collectingCountry box="[151,256,1792,1816]" name="Germany" pageId="31" pageNumber="32">Germany</collectingCountry>
, Macedonia, 
<collectingCountry box="[416,528,1791,1816]" name="Mongolia" pageId="31" pageNumber="32">Mongolia</collectingCountry>
, 
<collectingCountry box="[544,625,1791,1816]" name="Poland" pageId="31" pageNumber="32">Poland</collectingCountry>
, 
<collectingCountry box="[641,744,1791,1816]" name="Slovakia" pageId="31" pageNumber="32">Slovakia</collectingCountry>
), Panamanian (
<collectingCountry box="[924,1019,1792,1816]" name="Panama" pageId="31" pageNumber="32">Panama</collectingCountry>
), Sino-Japanese (
<collectingCountry box="[1223,1294,1791,1816]" name="China" pageId="31" pageNumber="32">China</collectingCountry>
