<collectingCountry box="[1311,1376,1792,1816]" name="Japan" pageId="31" pageNumber="32">Japan</collectingCountry>
) and Afrotropical (
<collectingCountry box="[304,395,1828,1852]" name="Guyana" pageId="31" pageNumber="32">Guyana</collectingCountry>
, 
<collectingCountry box="[406,510,1827,1852]" name="Tanzania" pageId="31" pageNumber="32">Tanzania</collectingCountry>
, 
<collectingCountry box="[521,596,1828,1852]" name="Kenya" pageId="31" pageNumber="32">Kenya</collectingCountry>
