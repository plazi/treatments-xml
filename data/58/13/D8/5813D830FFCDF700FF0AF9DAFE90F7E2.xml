<caption ID-DOI="http://doi.org/10.5281/zenodo.6388735" ID-Zenodo-Dep="6388735" httpUri="https://zenodo.org/record/6388735/files/figure.png" pageId="6" pageNumber="159" startId="6.[136,229,2029,2051]" targetBox="[412,1225,354,2005]" targetPageId="6">
<paragraph blockId="6.[136,1451,2029,2087]" pageId="6" pageNumber="159">
