and 
<taxonomicName id="81B409E0E339FFE7ECCAD82DFD22BB48" authority="(Caradja, 1920)" baseAuthorityName="Caradja" baseAuthorityYear="1920" box="[444,759,843,868]" class="Insecta" family="Oecophoridae" genus="Exaeretia" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="207" phylum="Arthropoda" rank="species" species="nebulosella">
