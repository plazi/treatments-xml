<emphasis id="B9104654FFF967540FFEFED8FEDD8E18" bold="true" box="[197,357,270,296]" pageId="17" pageNumber="18">Distribution.</emphasis>
<collectingCountry id="F373DAD6FFF967540E56FED9FDDD8E19" box="[365,613,271,297]" name="Dominican Republic" pageId="17" pageNumber="18">Dominican Republic</collectingCountry>
: 
<collectingRegion id="49A054A4FFF967540D4AFED9FD0A8E19" box="[625,690,271,297]" country="Dominican Republic" name="Azua" pageId="17" pageNumber="18">Azua</collectingRegion>
, 
<collectingRegion id="49A054A4FFF967540D84FED9FC8A8E19" box="[703,818,271,297]" country="Dominican Republic" name="Barahona" pageId="17" pageNumber="18">Barahona</collectingRegion>
, 
<collectingRegion id="49A054A4FFF967540C05FED9FC548E19" box="[830,1004,271,297]" country="Dominican Republic" name="Independencia" pageId="17" pageNumber="18">Independencia</collectingRegion>
, 
<collectingRegion id="49A054A4FFF967540CC3FED9FBCF8E19" box="[1016,1143,271,297]" country="Dominican Republic" name="Pedernales" pageId="17" pageNumber="18">Pedernales</collectingRegion>
