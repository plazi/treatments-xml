<taxonomicName baseAuthorityName="Uvarov" baseAuthorityYear="1916" box="[880,975,583,609]" class="Insecta" family="Tettigoniidae" genus="Uvarovistia" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="54" phylum="Arthropoda" rank="species" species="zebra">
<emphasis box="[880,975,583,609]" italics="true" pageId="9" pageNumber="54">U. zebra</emphasis>
</taxonomicName>
in 
<collectingRegion box="[1010,1054,583,609]" country="Turkey" name="Van" pageId="9" pageNumber="54">Van</collectingRegion>
and 
<collectingRegion box="[1107,1197,583,609]" country="Turkey" name="Hakkari" pageId="9" pageNumber="54">Hakkari</collectingRegion>
