<quantity box="[994,1085,978,1005]" metricMagnitude="3" metricUnit="m" metricValue="2.035" pageId="4" pageNumber="49" unit="m" value="2035.0">2035 m</quantity>
, 
<date box="[1101,1240,979,1005]" pageId="4" pageNumber="49" value="2018-07-13">13.VII.2018</date>
, 
<specimenCount box="[1256,1292,979,1005]" pageId="4" pageNumber="49" type="male">1♂</specimenCount>
, (leg. M. S. Taylan &amp; Ş. Beyter); Şemdinli, near the Seyid Taha Türbesi, 37°17´569´´N, 44°31´147´´E, 
<quantity box="[1168,1255,1015,1041]" metricMagnitude="3" metricUnit="m" metricValue="1.384" pageId="4" pageNumber="49" unit="m" value="1384.0">1384 m</quantity>
