<taxonomicName authority="fauna" authorityName="Fauna" box="[666,856,403,429]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Orthoptera" pageId="5" pageNumber="68" phylum="Tracheophyta" rank="order">Orthoptera fauna</taxonomicName>
