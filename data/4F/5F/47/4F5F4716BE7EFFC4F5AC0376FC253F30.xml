<taxonomicName box="[1178,1315,1581,1605]" class="Insecta" family="Chrysomelidae" genus="Sermyloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="59" phylum="Arthropoda" rank="genus">
