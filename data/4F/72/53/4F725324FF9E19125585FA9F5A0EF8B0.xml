<subSubSection pageId="32" pageNumber="31" type="etymology">
<paragraph blockId="32.[192,1440,1730,1786]" pageId="32" pageNumber="31">
<emphasis bold="true" box="[192,344,1730,1754]" pageId="32" pageNumber="31">Etymology.</emphasis>
