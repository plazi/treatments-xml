Hispaniola, 
<collectingCountry box="[700,804,1864,1888]" name="Jamaica" pageId="14" pageNumber="13">Jamaica</collectingCountry>
, 
<collectingCountry box="[815,959,1864,1888]" name="Puerto Rico" pageId="14" pageNumber="13">Puerto Rico</collectingCountry>
