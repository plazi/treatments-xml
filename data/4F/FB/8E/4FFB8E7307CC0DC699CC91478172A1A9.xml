<taxonomicName class="Arachnida" family="Anapidae" genus="Comaroma" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="82" phylum="Arthropoda" rank="genus">Comaroma</taxonomicName>
