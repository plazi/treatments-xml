<normalizedToken originalValue="Über" pageId="1" pageNumber="82">Uebergangsform</normalizedToken>
von 
<taxonomicName class="Arachnida" family="Theridiidae" genus="Pholcomma" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="82" phylum="Arthropoda" rank="genus">Pholcomma</taxonomicName>
