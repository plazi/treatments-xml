</taxonomicName>
(
<typeStatus box="[472,517,1067,1091]" pageId="1" pageNumber="390">type</typeStatus>
species: 
<taxonomicName authority="Thorell, 1891" authorityName="Thorell" authorityYear="1891" box="[614,943,1067,1091]" class="Arachnida" family="Salticidae" genus="Zeuxippus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="390" phylum="Arthropoda" rank="species" species="histrio">
