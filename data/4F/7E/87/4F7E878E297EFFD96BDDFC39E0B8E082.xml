</taxonomicName>
<emphasis bold="true" box="[750,845,620,645]" pageId="22" pageNumber="323">
<taxonomicNameLabel box="[750,845,620,645]" pageId="22" pageNumber="323" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
differs from 
<taxonomicName box="[1007,1113,620,644]" class="Insecta" family="Pipunculidae" genus="Elmohardyia" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="valida">
<emphasis box="[1007,1113,620,644]" italics="true" pageId="22" pageNumber="323">E. valida</emphasis>
</taxonomicName>
