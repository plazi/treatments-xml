<paragraph blockId="5.[264,1325,284,1750]" box="[312,715,924,950]" pageId="5" pageNumber="6">
<emphasis box="[312,455,924,949]" italics="true" pageId="5" pageNumber="6">Distribution</emphasis>
: 
<collectingCountry box="[478,561,924,950]" name="India" pageId="5" pageNumber="6">INDIA</collectingCountry>
: 
<collectingRegion box="[591,711,924,950]" country="India" name="Rajasthan" pageId="5" pageNumber="6">Rajasthan</collectingRegion>
