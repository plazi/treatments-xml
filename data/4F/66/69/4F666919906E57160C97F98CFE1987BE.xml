. 
<taxonomicName box="[355,665,1979,2001]" class="Insecta" family="Phoridae" genus="Chaetocnemistoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="formosa">
