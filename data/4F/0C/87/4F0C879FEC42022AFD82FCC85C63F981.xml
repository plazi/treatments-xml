<taxonomicName authority="Drury, 1770: 107" authorityName="Drury" authorityPageNumber="107" authorityYear="1770" box="[306,626,1349,1374]" class="Insecta" family="Cimicidae" genus="Cimex" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="picta">
