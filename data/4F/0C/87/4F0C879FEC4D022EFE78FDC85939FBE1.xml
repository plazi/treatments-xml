, and 
<taxonomicName authority="(Breddin, 1901)" authorityName="Brailovsky" authorityYear="2009" baseAuthorityName="Breddin" baseAuthorityYear="1901" box="[698,988,1083,1108]" class="Insecta" family="Coreidae" genus="Phthiarella" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="71" phylum="Arthropoda" rank="species" species="sponsa" status="comb. nov.">
<emphasis box="[698,813,1085,1108]" italics="true" pageId="12" pageNumber="71">Ph. sponsa</emphasis>
