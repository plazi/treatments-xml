</taxonomicName>
, 
<taxonomicName box="[261,376,190,215]" class="Magnoliopsida" family="Malvaceae" genus="Tilia" kingdom="Plantae" order="Malvales" pageId="21" pageNumber="22" phylum="Tracheophyta" rank="species" species="taquetii">
