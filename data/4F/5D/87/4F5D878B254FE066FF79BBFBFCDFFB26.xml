<taxonomicName box="[665,774,909,935]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
: 
<taxonomicName box="[785,1042,909,935]" class="Magnoliopsida" family="Fabaceae" genus="Caragana" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="arborescens">
<emphasis box="[785,1042,909,935]" italics="true" pageId="14" pageNumber="15">Caragana arborescens</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1057,1156,909,935]" class="Magnoliopsida" family="Fabaceae" genus="Caragana" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="frutex">
<emphasis box="[1057,1156,909,935]" italics="true" pageId="14" pageNumber="15">C. frutex</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1171,1267,909,935]" class="Magnoliopsida" family="Fabaceae" genus="Caragana" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="boisii">
<emphasis box="[1171,1267,909,935]" italics="true" pageId="14" pageNumber="15">C. boisii</emphasis>
</taxonomicName>
, 
<taxonomicName authority="(Kirichenko et al. 2016)" authorityName="Kirichenko" authorityYear="2016" baseAuthorityName="Kirichenko" baseAuthorityYear="2016" class="Magnoliopsida" family="Fabaceae" genus="Medicago" kingdom="Plantae" order="Fabales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="sativa">
