<taxonomicName box="[730,886,1881,1908]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="barbinerve">A. barbinerve</taxonomicName>
, 
<taxonomicName box="[896,1045,1882,1908]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="japonicum">A. japonicum</taxonomicName>
</emphasis>
, 
<taxonomicName box="[1058,1201,1881,1908]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="32" pageNumber="33" phylum="Tracheophyta" rank="species" species="palmatum">
