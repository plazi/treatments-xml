<taxonomicName baseAuthorityName="Kumata" baseAuthorityYear="1963" box="[718,814,1773,1799]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="ulmi">
<emphasis box="[718,814,1773,1799]" italics="true" pageId="31" pageNumber="32">Ph. ulmi</emphasis>
</taxonomicName>
and 
<taxonomicName box="[869,993,1773,1799]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="31" pageNumber="32" phylum="Tracheophyta" rank="species" species="pumila">
<emphasis box="[869,993,1773,1799]" italics="true" pageId="31" pageNumber="32">Ph. pumila</emphasis>
</taxonomicName>
