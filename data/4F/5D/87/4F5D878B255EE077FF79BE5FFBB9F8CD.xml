<emphasis box="[479,562,1737,1763]" italics="true" pageId="31" pageNumber="32">
<taxonomicName baseAuthorityName="Kumata" baseAuthorityYear="1963" box="[479,558,1737,1763]" class="Insecta" family="Gracillariidae" genus="Phyllonorycter" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="ulmi">P. ulmi</taxonomicName>
