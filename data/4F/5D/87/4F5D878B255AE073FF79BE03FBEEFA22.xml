<taxonomicName box="[671,772,1342,1367]" class="Magnoliopsida" family="Caprifoliaceae" genus="Lonicera" kingdom="Plantae" order="Dipsacales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="genus">
<emphasis box="[671,772,1342,1367]" italics="true" pageId="27" pageNumber="28">Lonicera</emphasis>
</taxonomicName>
: 
<taxonomicName authority="Turcz. ex Freyn" authorityName="Freyn" box="[789,1079,1341,1367]" class="Magnoliopsida" family="Caprifoliaceae" genus="Lonicera" kingdom="Plantae" order="Dipsacales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="edulis">
<emphasis box="[789,887,1341,1367]" italics="true" pageId="27" pageNumber="28">L. edulis</emphasis>
Turcz. ex Freyn
</taxonomicName>
, 
<taxonomicName authority="(Rupr.) Maxim." authorityName="Maxim." baseAuthorityName="Rupr." box="[1092,1401,1341,1367]" class="Magnoliopsida" family="Caprifoliaceae" genus="Lonicera" kingdom="Plantae" order="Dipsacales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="maackii">
<emphasis box="[1092,1213,1341,1367]" italics="true" pageId="27" pageNumber="28">L. maackii</emphasis>
(Rupr.) Maxim.
</taxonomicName>
, 
<taxonomicName authority="Batalin" authorityName="Batalin" authorityYear="1892" class="Magnoliopsida" family="Caprifoliaceae" genus="Lonicera" kingdom="Plantae" order="Dipsacales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="praeflorens">
