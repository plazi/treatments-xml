<emphasis box="[798,967,1593,1619]" italics="true" pageId="15" pageNumber="16">Betula pendula</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Sukaczev" authorityYear="1911" box="[981,1137,1593,1619]" class="Magnoliopsida" family="Betulaceae" genus="Betula" kingdom="Plantae" order="Fagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="platyphylla">
