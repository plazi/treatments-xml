<paragraph blockId="9.[151,1437,151,645]" box="[199,947,511,536]" pageId="9">
<emphasis bold="true" box="[199,352,511,536]" pageId="9">Distribution.</emphasis>
<collectingRegion box="[359,476,511,536]" country="United States of America" name="California" pageId="9">California</collectingRegion>
