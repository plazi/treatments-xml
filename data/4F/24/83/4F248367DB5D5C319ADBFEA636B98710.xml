<number box="[411,427,296,320]" pageId="3" pageNumber="101" value="3.0">3</number>
</figureCitation>
). Corniculi short and horn-like; hypostomal seta 
<emphasis box="[982,1010,296,320]" italics="true" pageId="3" pageNumber="101">
h 
<number box="[996,1010,296,320]" pageId="3" pageNumber="101" value="3.0">3</number>
</emphasis>
longest (
<number box="[1114,1190,295,320]" pageId="3" pageNumber="101" value="48.5" valueMax="52.0" valueMin="45.0">45–52</number>
), 
<emphasis box="[1210,1238,296,320]" italics="true" pageId="3" pageNumber="101">
h 
<number box="[1224,1238,296,320]" pageId="3" pageNumber="101" value="2.0">2</number>
</emphasis>
shortest (
<number box="[1349,1423,296,320]" pageId="3" pageNumber="101" value="12.0" valueMax="14.0" valueMin="10.0">10–14</number>
), and 
<emphasis box="[198,226,332,356]" italics="true" pageId="3" pageNumber="101">
h 
<number box="[214,226,332,356]" pageId="3" pageNumber="101" value="1.0">
