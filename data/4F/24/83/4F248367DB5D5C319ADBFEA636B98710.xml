<paragraph blockId="2.[151,1437,1422,2023]" lastBlockId="3.[151,1436,151,1040]" lastPageId="3" lastPageNumber="101" pageId="2" pageNumber="100">
<emphasis box="[199,268,1675,1698]" italics="true" pageId="2" pageNumber="100">Venter</emphasis>
(
<figureCitation box="[285,354,1674,1699]" captionStart="FIGURES 1 – 2" captionStartId="1.[151,264,1900,1923]" captionTargetBox="[163,1418,772,1869]" captionTargetId="figure@1.[151,1436,767,1878]" captionTargetPageId="1" captionText="FIGURES 1 – 2. Pachyseius destitutus sp. nov., female. 1. Dorsal shield; 2. Ventral idiosoma." httpUri="https://zenodo.org/record/236433/files/figure.png" pageId="2" pageNumber="100">
Fig. 
<number box="[337,354,1675,1699]" pageId="2" pageNumber="100" value="2.0">2</number>
</figureCitation>
