(
<taxonomicName box="[958,1107,1461,1485]" class="Chilopoda" family="Scolopendridae" genus="Digitipes" kingdom="Animalia" order="Scolopendromorpha" pageId="3" pageNumber="102" phylum="Arthropoda" rank="species" species="barnabasi">
<emphasis box="[958,1107,1461,1485]" italics="true" pageId="3" pageNumber="102">D. barnabasi</emphasis>
</taxonomicName>
of Joshi and Karanth, 
<number box="[1364,1424,1461,1485]" pageId="3" pageNumber="102" value="2012.0">2012</number>
