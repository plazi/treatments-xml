r. 
<taxonomicName id="7430BC0F67529FF610E294687EDD399E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181941" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Pheidole sculptior Forel" order="Hymenoptera" pageNumber="414" phylum="Arthropoda" rank="species" species="sculptior">Pheidole sculptior</taxonomicName>
