<taxonomicName box="[879,965,488,514]" class="Insecta" family="Aleyrodidae" genus="Dialeurodes" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="citri">
<emphasis box="[879,900,488,513]" italics="true" pageId="26" pageNumber="27">D</emphasis>
. 
<emphasis box="[916,965,488,513]" italics="true" pageId="26" pageNumber="27">citri</emphasis>
</taxonomicName>
and right section for 
<taxonomicName box="[1223,1395,488,513]" class="Insecta" family="Aleyrodidae" genus="Aleurocanthus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
<emphasis box="[1223,1395,488,513]" italics="true" pageId="26" pageNumber="27">Aleurocanthus</emphasis>
