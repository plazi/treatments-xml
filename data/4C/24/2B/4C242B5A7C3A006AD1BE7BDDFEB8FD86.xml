<bibRefCitation author="Ashmead" box="[368,556,450,476]" pageId="26" pageNumber="27" refString="Ashmead, W. M. (1885) The orange Aleurodes (Aleurodes citri n. sp.). Florida Dispatch, 2, 704." type="journal article" year="1885">
Ashmead, 
<number box="[496,556,450,476]" pageId="26" pageNumber="27" value="1885.0">1885</number>
</bibRefCitation>
)
</taxonomicName>
and 
<taxonomicName box="[623,793,450,475]" class="Insecta" family="Aleyrodidae" genus="Aleurocanthus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">
