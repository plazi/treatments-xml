, 
<number box="[223,275,764,788]" pageId="6" pageNumber="349" value="0.86">0.86</number>
, 
<number box="[285,337,764,788]" pageId="6" pageNumber="349" value="0.63">0.63</number>
, 
<number box="[348,401,764,788]" pageId="6" pageNumber="349" value="0.51">0.51</number>
); IV 
<number box="[457,506,764,788]" pageId="6" pageNumber="349" value="3.68">3.68</number>
(
<number box="[521,573,764,788]" pageId="6" pageNumber="349" value="1.05">1.05</number>
, 
<number box="[584,635,764,788]" pageId="6" pageNumber="349" value="1.18">1.18</number>
, 
<number box="[647,699,764,788]" pageId="6" pageNumber="349" value="0.86">0.86</number>
, 
