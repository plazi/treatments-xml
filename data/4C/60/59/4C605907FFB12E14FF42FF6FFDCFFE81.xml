<number box="[778,828,727,751]" pageId="6" pageNumber="349" value="1.11">1.11</number>
, 
<number box="[842,894,727,751]" pageId="6" pageNumber="349" value="0.56">0.56</number>
, 
<number box="[907,961,727,751]" pageId="6" pageNumber="349" value="0.54">0.54</number>
); II 
