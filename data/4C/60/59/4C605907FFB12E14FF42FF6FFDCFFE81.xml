<number box="[545,558,620,644]" pageId="6" pageNumber="349" value="6.0">6</number>
A–B
</figureCitation>
) total length 
<number box="[761,814,620,644]" pageId="6" pageNumber="349" value="2.34">2.34</number>
. Prosoma 
<number box="[930,979,620,644]" pageId="6" pageNumber="349" value="1.67">1.67</number>
long, 
<number box="[1049,1098,620,644]" pageId="6" pageNumber="349" value="0.81">0.81</number>
wide; opisthosoma 
<number box="[1323,1372,620,644]" pageId="6" pageNumber="349" value="1.29">1.29</number>
long, 
<number box="[151,200,655,679]" pageId="6" pageNumber="349" value="0.94">0.94</number>
wide. Eye sizes and interdistances: AME 
<number box="[668,720,655,679]" pageId="6" pageNumber="349" value="0.05">0.05</number>
, ALE 
<number box="[791,843,655,679]" pageId="6" pageNumber="349" value="0.07">0.07</number>
, PME 
<number box="[917,969,655,679]" pageId="6" pageNumber="349" value="0.07">0.07</number>
, PLE 
<number box="[1035,1087,655,679]" pageId="6" pageNumber="349" value="0.09">0.09</number>
; AME–AME 
<number box="[1243,1295,655,679]" pageId="6" pageNumber="349" value="0.04">0.04</number>
, AME–ALE 
<number box="[151,203,692,716]" pageId="6" pageNumber="349" value="0.02">0.02</number>
, PME–PME 
<number box="[359,411,692,716]" pageId="6" pageNumber="349" value="0.06">0.06</number>
, PME–PLE 
<number box="[559,611,692,716]" pageId="6" pageNumber="349" value="0.05">0.05</number>
, ALE–PLE 
<number box="[757,810,692,716]" pageId="6" pageNumber="349" value="0.03">0.03</number>
. MOA 
<number box="[901,950,692,716]" pageId="6" pageNumber="349" value="0.18">0.18</number>
long, front width 
<number box="[1172,1224,692,716]" pageId="6" pageNumber="349" value="0.13">0.13</number>
, back width 
<number box="[1379,1432,692,716]" pageId="6" pageNumber="349" value="0.21">0.21</number>
. Clypeus height 
<number box="[331,384,727,751]" pageId="6" pageNumber="349" value="0.12">0.12</number>
. Leg measurements: I 
<number box="[645,694,727,751]" pageId="6" pageNumber="349" value="3.15">3.15</number>
(
<number box="[711,764,727,751]" pageId="6" pageNumber="349" value="0.94">0.94</number>
