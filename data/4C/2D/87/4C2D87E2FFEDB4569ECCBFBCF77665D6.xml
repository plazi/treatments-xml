<emphasis italics="true" pageId="7" pageNumber="7">
<taxonomicName class="Insecta" family="Mordellidae" genus="Glipidiomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="7" phylum="Arthropoda" rank="species" species="atraterga">G. atraterga</taxonomicName>
.
</emphasis>
<emphasis bold="true" box="[370,393,761,780]" pageId="7" pageNumber="7">12</emphasis>
) 
<emphasis box="[409,527,761,780]" italics="true" pageId="7" pageNumber="7">
<taxonomicName authorityName="Lu &amp; Fan" authorityYear="2000" box="[409,523,761,780]" class="Insecta" family="Mordellidae" genus="Glipidiomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="7" phylum="Arthropoda" rank="species" species="rufiterga">G. rufiterga</taxonomicName>
