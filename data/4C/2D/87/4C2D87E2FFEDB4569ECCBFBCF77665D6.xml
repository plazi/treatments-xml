<emphasis box="[573,711,761,780]" italics="true" pageId="7" pageNumber="7">
<taxonomicName authorityName="Ermisch" authorityYear="1968" box="[573,706,761,780]" class="Insecta" family="Mordellidae" genus="Glipidiomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="7" phylum="Arthropoda" rank="species" species="curticauda">G. curticauda</taxonomicName>
.
</emphasis>
<emphasis bold="true" box="[718,741,761,780]" pageId="7" pageNumber="7">14</emphasis>
) 
<emphasis box="[756,965,761,780]" italics="true" pageId="7" pageNumber="7">
<taxonomicName authorityName="Ermisch" authorityYear="1968" box="[756,962,761,780]" class="Insecta" family="Mordellidae" genus="Glipidiomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="7" phylum="Arthropoda" rank="species" species="rufobrunneipennis">G. rufobrunneipennis</taxonomicName>
.
</emphasis>
<emphasis bold="true" box="[972,995,761,780]" pageId="7" pageNumber="7">15</emphasis>
) 
<emphasis italics="true" pageId="7" pageNumber="7">
<taxonomicName authorityName="Ermisch" authorityYear="1968" class="Insecta" family="Mordellidae" genus="Glipidiomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="7" phylum="Arthropoda" rank="species" species="kuatunensis">G. kuatunensis</taxonomicName>
