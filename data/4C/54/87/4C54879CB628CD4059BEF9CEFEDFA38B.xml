, 
<collectingCountry box="[1199,1324,1895,1922]" name="Costa Rica" pageId="117" pageNumber="118">Costa Rica</collectingCountry>
, 
<collectingCountry box="[1337,1432,1895,1921]" name="Ecuador" pageId="117" pageNumber="118">Ecuador</collectingCountry>
