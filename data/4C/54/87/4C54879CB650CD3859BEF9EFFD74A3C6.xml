<collectingCountry box="[777,891,1865,1891]" name="Argentina" pageId="13" pageNumber="14">Argentina</collectingCountry>
, 
<collectingCountry box="[904,989,1865,1891]" name="Bolivia" pageId="13" pageNumber="14">Bolivia</collectingCountry>
, 
<collectingCountry box="[1002,1073,1865,1891]" name="Brazil" pageId="13" pageNumber="14">Brazil</collectingCountry>
, and 
<collectingCountry box="[1135,1186,1865,1891]" name="Peru" pageId="13" pageNumber="14">Peru</collectingCountry>
(
<bibRefCitation author="Metcalf, Z. P." box="[1203,1372,1865,1891]" pageId="13" pageNumber="14" pagination="1 - 585" refId="ref106666" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
