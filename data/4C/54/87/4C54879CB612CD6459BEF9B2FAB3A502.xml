<emphasis box="[199,323,1809,1835]" italics="true" pageId="80" pageNumber="81">C. lichiana</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Sanborn" authorityYear="2011" box="[378,503,1809,1835]" class="Insecta" family="Cicadidae" genus="Carineta" kingdom="Animalia" order="Hemiptera" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="modesta">
<emphasis box="[378,503,1809,1835]" italics="true" pageId="80" pageNumber="81">C. modesta</emphasis>
