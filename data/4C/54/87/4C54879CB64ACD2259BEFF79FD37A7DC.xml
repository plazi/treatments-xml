, 
<collectingCountry box="[1044,1129,827,853]" name="Bolivia" pageId="23" pageNumber="24">Bolivia</collectingCountry>
, 
<collectingCountry box="[1144,1215,827,853]" name="Brazil" pageId="23" pageNumber="24">Brazil</collectingCountry>
, 
<collectingCountry box="[1230,1343,827,853]" name="Colombia" pageId="23" pageNumber="24">Colombia</collectingCountry>
, French Guiana, 
<collectingCountry box="[250,340,863,889]" name="Guyana" pageId="23" pageNumber="24">Guyana</collectingCountry>
, 
<collectingCountry box="[354,459,863,889]" name="Paraguay" pageId="23" pageNumber="24">Paraguay</collectingCountry>
, 
<collectingCountry box="[474,528,863,889]" name="Peru" pageId="23" pageNumber="24">Peru</collectingCountry>
, 
<collectingCountry box="[543,653,863,889]" name="Suriname" pageId="23" pageNumber="24">Suriname</collectingCountry>
, 
<collectingCountry box="[668,767,863,889]" name="Uruguay" pageId="23" pageNumber="24">Uruguay</collectingCountry>
, and 
<collectingCountry box="[832,947,863,889]" name="Venezuela" pageId="23" pageNumber="24">Venezuela</collectingCountry>
(
<bibRefCitation author="Metcalf, Z. P." box="[965,1136,863,889]" pageId="23" pageNumber="24" pagination="1 - 585" refId="ref106666" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
; 
<bibRefCitation author="Sanborn, A. F." box="[1151,1326,863,889]" pageId="23" pageNumber="24" pagination="377 - 418" refId="ref107675" refString="Sanborn, A. F. (2011 a) Checklist of the cicadas of French Guiana including new records and the description of nine new species (Insecta, Hemiptera, Cicadoidea, Cicadidae). Zoosystema, 33, 377 - 418. https: // doi. org / 10.5252 / z 2011 n 3 a 6" type="journal article" year="2011">Sanborn 2011a</bibRefCitation>
, b; 2013; 2016b, c; 2019b; 2020a; 
<bibRefCitation author="Sanborn, A. F. &amp; Heath, M. S." box="[430,694,899,925]" pageId="23" pageNumber="24" pagination="1 - 94" refId="ref108909" refString="Sanborn, A. F. &amp; Heath, M. S. (2014) The cicadas of Argentina with new records, a new genus and fifteen new species (Hemiptera: Cicadoidea: Cicadidae). Zootaxa, 3883 (1), 1 - 94. https: // doi. org / 10.11646 / zootaxa. 3883.1.1" type="journal article" year="2014">Sanborn &amp; Heath 2014</bibRefCitation>
).
</paragraph>
</subSubSection>
