<collectingCountry box="[1217,1331,1896,1922]" name="Argentina" pageId="49" pageNumber="50">Argentina</collectingCountry>
, 
<collectingCountry box="[1347,1432,1896,1922]" name="Bolivia" pageId="49" pageNumber="50">Bolivia</collectingCountry>
, 
<collectingCountry box="[151,222,1932,1958]" name="Brazil" pageId="49" pageNumber="50">Brazil</collectingCountry>
, 
<collectingCountry box="[234,347,1932,1958]" name="Colombia" pageId="49" pageNumber="50">Colombia</collectingCountry>
, 
<collectingCountry box="[360,455,1932,1958]" name="Ecuador" pageId="49" pageNumber="50">Ecuador</collectingCountry>
, 
<collectingCountry box="[467,636,1932,1958]" name="French Guiana" pageId="49" pageNumber="50">French Guiana</collectingCountry>
, 
<collectingCountry box="[648,740,1932,1958]" name="Panama" pageId="49" pageNumber="50">Panama</collectingCountry>
, 
<collectingCountry box="[751,805,1932,1958]" name="Peru" pageId="49" pageNumber="50">Peru</collectingCountry>
, 
<collectingCountry box="[817,927,1932,1958]" name="Suriname" pageId="49" pageNumber="50">Suriname</collectingCountry>
, 
<collectingCountry box="[939,1157,1932,1958]" name="Trinidad and Tobago" pageId="49" pageNumber="50">Trinidad &amp; Tobago</collectingCountry>
, and 
<collectingCountry box="[1217,1332,1932,1958]" name="Venezuela" pageId="49" pageNumber="50">Venezuela</collectingCountry>
(
<bibRefCitation author="Metcalf, Z. P." pageId="49" pageNumber="50" pagination="1 - 492" refId="ref106756" refString="Metcalf, Z. P. (1963 c) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i-vi + 1 - 492." type="journal article" year="1963">Metcalf 1963c</bibRefCitation>
