<collectingCountry box="[691,804,1007,1033]" name="Colombia" pageId="76" pageNumber="77">Colombia</collectingCountry>
, 
<collectingCountry box="[819,946,1007,1033]" name="Costa Rica" pageId="76" pageNumber="77">Costa Rica</collectingCountry>
, 
<collectingCountry box="[961,1056,1007,1033]" name="Ecuador" pageId="76" pageNumber="77">Ecuador</collectingCountry>
, 
<collectingCountry box="[1070,1194,1007,1033]" name="Guatemala" pageId="76" pageNumber="77">Guatemala</collectingCountry>
, 
<collectingCountry box="[1208,1326,1007,1033]" name="Nicaragua" pageId="76" pageNumber="77">Nicaragua</collectingCountry>
, 
<collectingCountry box="[1341,1433,1007,1033]" name="Panama" pageId="76" pageNumber="77">Panamá</collectingCountry>
, and 
<collectingCountry box="[199,250,1043,1069]" name="Peru" pageId="76" pageNumber="77">Peru</collectingCountry>
(
<bibRefCitation author="Metcalf, Z. P." box="[266,435,1043,1069]" pageId="76" pageNumber="77" pagination="1 - 492" refId="ref106756" refString="Metcalf, Z. P. (1963 c) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i-vi + 1 - 492." type="journal article" year="1963">Metcalf 1963c</bibRefCitation>
