<collectingCountry box="[1318,1432,1582,1608]" name="Argentina" pageId="25" pageNumber="26">Argentina</collectingCountry>
, 
<collectingCountry box="[151,236,1618,1644]" name="Bolivia" pageId="25" pageNumber="26">Bolivia</collectingCountry>
, 
<collectingCountry box="[247,318,1618,1644]" name="Brazil" pageId="25" pageNumber="26">Brazil</collectingCountry>
, 
<collectingCountry box="[329,442,1618,1644]" name="Colombia" pageId="25" pageNumber="26">Colombia</collectingCountry>
, 
<collectingCountry box="[453,577,1618,1644]" name="Costa Rica" pageId="25" pageNumber="26">Costa Rica</collectingCountry>
, 
<collectingCountry box="[588,683,1618,1644]" name="Ecuador" pageId="25" pageNumber="26">Ecuador</collectingCountry>
, French Guiana, 
<collectingCountry box="[872,962,1618,1644]" name="Guyana" pageId="25" pageNumber="26">Guyana</collectingCountry>
, 
<collectingCountry box="[972,1090,1618,1644]" name="Nicaragua" pageId="25" pageNumber="26">Nicaragua</collectingCountry>
, 
<collectingCountry box="[1101,1193,1619,1645]" name="Panama" pageId="25" pageNumber="26">Panama</collectingCountry>
, 
<collectingCountry box="[1203,1308,1619,1645]" name="Paraguay" pageId="25" pageNumber="26">Paraguay</collectingCountry>
, 
<collectingCountry box="[1319,1373,1619,1645]" name="Peru" pageId="25" pageNumber="26">Peru</collectingCountry>
