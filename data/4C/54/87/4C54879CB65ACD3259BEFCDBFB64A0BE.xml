<collectingCountry box="[892,1002,1145,1171]" name="Colombia" pageId="7" pageNumber="8">Colombia</collectingCountry>
and 
<collectingCountry box="[1057,1150,1145,1171]" name="Ecuador" pageId="7" pageNumber="8">Ecuador</collectingCountry>
(
