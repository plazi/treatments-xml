<taxonomicName authorityName="Miranda" authorityYear="2017" box="[619,844,1492,1517]" class="Insecta" family="Syrphidae" genus="Hybobathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="luciane">
