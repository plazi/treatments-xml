<taxonomicName authorityName="Dobson" authorityYear="1872" box="[980,1119,948,977]" class="Mammalia" family="Vespertilionidae" genus="Murina" isUncertain="true" kingdom="Animalia" order="Chiroptera" pageId="156" pageNumber="917" phylum="Chordata" rank="species" species="cyclotis">cf. cyclotis</taxonomicName>
(from 
<collectingCountry box="[1239,1318,948,977]" name="India" pageId="156" pageNumber="917">India</collectingCountry>
), 
<taxonomicName authorityName="Dobson" authorityYear="1872" box="[745,885,991,1016]" class="Mammalia" family="Vespertilionidae" genus="Murina" kingdom="Animalia" order="Chiroptera" pageId="156" pageNumber="917" phylum="Chordata" rank="species" species="cyclotis">M. cyclotis</taxonomicName>
