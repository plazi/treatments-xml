<materialsCitation pageId="203" pageNumber="965">
about 
<quantity box="[841,926,905,934]" metricMagnitude="4" metricUnit="m" metricValue="3.0" pageId="203" pageNumber="965" unit="km" value="30.0">30 km</quantity>
S of Pokhara town, 
<quantity box="[1209,1277,905,934]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="203" pageNumber="965" unit="km" value="4.0">4 km</quantity>
E of Syangja, 
<quantity box="[877,979,940,973]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="203" pageNumber="965" unit="m" value="1300.0">1300 m</quantity>
, Syangja District, 
