<materialsCitation pageId="56" pageNumber="814">
“Boentok, Barito 
<collectingRegion box="[1126,1198,439,468]" country="Nigeria" name="Rivers" pageId="56" pageNumber="814">River</collectingRegion>
[
<collectingRegion country="Indonesia" name="Kalimantan Tengah" pageId="56" pageNumber="814">Central Kalimantan</collectingRegion>
