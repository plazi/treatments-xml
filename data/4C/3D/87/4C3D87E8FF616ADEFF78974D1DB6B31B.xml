, 
<materialsCitation pageId="154" pageNumber="915">
“near Nam Pan in the Annamite Mountains, 
<collectingRegion box="[1005,1326,528,553]" country="Laos" name="Bolikhamxai" pageId="154" pageNumber="915">Bolikhamxai Province</collectingRegion>
, 
<collectingCountry box="[743,806,558,591]" name="Laos" pageId="154" pageNumber="915">Laos</collectingCountry>
(
<geoCoordinate box="[830,946,558,591]" degrees="18" direction="north" minutes="28" orientation="latitude" pageId="154" pageNumber="915" precision="55555" value="28.0">18°28°N</geoCoordinate>
, 
<geoCoordinate box="[966,1082,558,591]" degrees="105" direction="east" minutes="05" orientation="longitude" pageId="154" pageNumber="915" precision="925" value="105.083336">105°05’E</geoCoordinate>
,alt. ~ 
<quantity box="[1174,1282,558,591]" metricMagnitude="3" metricUnit="m" metricValue="1.3" pageId="154" pageNumber="915" unit="m" value="1300.0">1300 m</quantity>
