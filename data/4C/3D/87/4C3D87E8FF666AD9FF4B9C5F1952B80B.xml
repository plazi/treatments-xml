<subSubSection pageId="157" pageNumber="918" type="discussion">
<paragraph blockId="157.[796,1390,2695,3117]" pageId="157" pageNumber="918">
<taxonomicName authorityName="Gray" authorityYear="1842" box="[799,1021,2891,2920]" class="Mammalia" family="Vespertilionidae" genus="Murina" kingdom="Animalia" order="Chiroptera" pageId="157" pageNumber="918" phylum="Chordata" rank="genus">Murina liboensis</taxonomicName>
