, N 
<collectingCountry box="[1928,2057,3420,3445]" name="Thailand" pageId="149" pageNumber="910">Thailand</collectingCountry>
, N 
<collectingCountry box="[2107,2173,3420,3445]" name="Laos" pageId="149" pageNumber="910">Laos</collectingCountry>
, N 
<collectingCountry box="[2221,2343,3420,3445]" name="Vietnam" pageId="149" pageNumber="910">Vietnam</collectingCountry>
, and E 
