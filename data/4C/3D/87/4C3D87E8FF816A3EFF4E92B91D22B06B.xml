<taxonomicName baseAuthorityName="Peters" baseAuthorityYear="1870" box="[865,1016,1531,1564]" class="Liliopsida" family="Poaceae" genus="Lasiurus" kingdom="Plantae" order="Poales" pageId="122" pageNumber="880" phylum="Tracheophyta" rank="species" species="egregius">L. egregius</taxonomicName>
. 
<taxonomicName authorityName="Handley" authorityYear="1960" box="[1048,1305,1531,1564]" class="Liliopsida" family="Poaceae" genus="Lasiurus" kingdom="Plantae" order="Poales" pageId="122" pageNumber="880" phylum="Tracheophyta" rank="species" species="castaneus">Lasiurus castaneus</taxonomicName>
