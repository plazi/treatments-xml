, 
<materialsCitation box="[953,1379,1456,1485]" pageId="122" pageNumber="880">
“Tacarcuna Village, 
<quantity box="[1258,1379,1456,1485]" metricMagnitude="-1" metricUnit="m" metricValue="9.7536" pageId="122" pageNumber="880" unit="ft" value="3.2">3,200 ft.</quantity>
</materialsCitation>
</paragraph>
<paragraph blockId="122.[790,1384,1412,1840]" pageId="122" pageNumber="880">
[= 
<quantity box="[830,923,1496,1525]" metricMagnitude="2" metricUnit="m" metricValue="9.75" pageId="122" pageNumber="880" unit="m" value="975.0">975 m</quantity>
], Rio Pucro, 
