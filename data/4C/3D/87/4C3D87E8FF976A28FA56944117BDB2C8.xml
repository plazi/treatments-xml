<subSubSection pageId="108" pageNumber="866" type="reference_group">
<paragraph blockId="108.[2062,2657,709,1131]" pageId="108" pageNumber="866">
<emphasis bold="true" box="[2062,2217,709,738]" pageId="108" pageNumber="866">Taxonomy.</emphasis>
<taxonomicName authority="Duli" authorityName="Duli" baseAuthorityName="J. Fischer" baseAuthorityYear="1829" class="Mammalia" family="Vespertilionidae" genus="Plecotus" kingdom="Animalia" order="Chiroptera" pageId="108" pageNumber="866" phylum="Chordata" rank="species" species="austriacus">Plecotus austriacus kolombatovici Duli¢, 1980</taxonomicName>
, 
<materialsCitation box="[2239,2570,748,777]" pageId="108" pageNumber="866">
Korcula Island, 
<collectingCountry box="[2462,2566,748,777]" name="Croatia" pageId="108" pageNumber="866">Croatia</collectingCountry>
