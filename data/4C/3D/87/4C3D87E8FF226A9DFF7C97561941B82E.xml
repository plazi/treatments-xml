<caption ID-DOI="http://doi.org/10.5281/zenodo.6399003" ID-Zenodo-Dep="6399003" httpUri="https://zenodo.org/record/6399003/files/figure.png" inLine="true" pageId="217" pageNumber="979" targetBox="[123,714,453,866]" targetPageId="217">
