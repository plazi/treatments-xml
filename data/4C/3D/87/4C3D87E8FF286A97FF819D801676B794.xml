<collectingRegion box="[2131,2178,330,359]" country="South Sudan" name="Lakes" pageId="211" pageNumber="973">See</collectingRegion>
<taxonomicName authorityName="Hollister" authorityYear="1912" box="[2195,2315,330,359]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="211" pageNumber="974" phylum="Chordata" rank="species" species="petax">M. petax</taxonomicName>
