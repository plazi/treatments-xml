<taxonomicName authorityName="Temminck" authorityYear="1840" box="[2311,2488,286,319]" class="Mammalia" family="Vespertilionidae" genus="Vespertilio" kingdom="Animalia" order="Chiroptera" pageId="211" pageNumber="973" phylum="Chordata" rank="species" species="macrodactylus">macrodactylus</taxonomicName>
