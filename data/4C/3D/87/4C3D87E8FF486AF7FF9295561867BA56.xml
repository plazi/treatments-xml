<collectingCountry box="[918,997,1111,1144]" name="Brazil" pageId="179" pageNumber="941">Brazil</collectingCountry>
to NE 
<collectingCountry box="[1106,1249,1111,1144]" name="Argentina" pageId="179" pageNumber="941">Argentina</collectingCountry>
and 
<collectingCountry box="[724,846,1155,1184]" name="Uruguay" pageId="179" pageNumber="941">Uruguay</collectingCountry>
