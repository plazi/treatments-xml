<taxonomicName authority="Kaup, 1829" box="[917,1002,1188,1221]" class="Mammalia" family="Vespertilionidae" genus="Myotis" kingdom="Animalia" order="Chiroptera" pageId="215" pageNumber="977" phylum="Chordata" rank="genus">Myotis</taxonomicName>
; 
<taxonomicName authorityName="Kuhl" authorityYear="1817" box="[1048,1200,1188,1221]" class="Mammalia" family="Vespertilionidae" genus="Vespertilio" kingdom="Animalia" order="Chiroptera" pageId="215" pageNumber="977" phylum="Chordata" rank="species" species="daubentonii">daubentonii</taxonomicName>
