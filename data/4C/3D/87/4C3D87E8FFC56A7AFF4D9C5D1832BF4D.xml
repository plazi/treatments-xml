<taxonomicName box="[2273,2430,801,834]" class="Insecta" family="Scarabaeidae" genus="Coleoptera" kingdom="Animalia" order="Coleoptera" pageId="62" pageNumber="820" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
, 
<taxonomicName authority=", Dictyoptera" authorityName="Dictyoptera" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="820" phylum="Arthropoda" rank="order">Hymenoptera, Dictyoptera</taxonomicName>
(particularly mantids), and 
<taxonomicName box="[2007,2183,841,874]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="62" pageNumber="820" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
