<taxonomicName box="[485,599,833,855]" class="Insecta" family="Coreidae" genus="Anisoscelis" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="205" phylum="Arthropoda" rank="genus">
<emphasis box="[485,599,833,855]" italics="true" pageId="10" pageNumber="205">Anisoscelis</emphasis>
</taxonomicName>
(
<taxonomicName authority="Osuna, 1977" authorityName="Osuna" authorityYear="1977" box="[614,663,834,855]" class="Insecta" family="Coreidae" genus="Bitta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="205" phylum="Arthropoda" rank="genus">
<emphasis box="[614,663,834,855]" italics="true" pageId="10" pageNumber="205">Bitta</emphasis>
</taxonomicName>
) spp., in two views. 13–14, 
<taxonomicName authority="Westwood." authorityName="Westwood." box="[951,1287,833,855]" class="Insecta" family="Coreidae" genus="Anisoscelis" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="205" phylum="Arthropoda" rank="species" species="hymenipherus" subGenus="Bitta">
<emphasis box="[951,966,834,855]" italics="true" pageId="10" pageNumber="205">A</emphasis>
