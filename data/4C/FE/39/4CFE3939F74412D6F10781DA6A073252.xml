<paragraph pageId="null" pageNumber="274">
<pageBreakToken pageId="null" pageNumber="274" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName authority="L." class="Liliopsida" family="Poaceae" genus="Phleum" kingdom="Plantae" order="Poales" pageId="null" pageNumber="274" phylum="Tracheophyta" rank="species" species="pratense">
