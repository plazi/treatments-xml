<subSubSection box="[151,576,1504,1531]" pageId="43" pageNumber="44" type="nomenclature">
<paragraph blockId="43.[151,576,1504,1531]" box="[151,576,1504,1531]" pageId="43" pageNumber="44">
<heading bold="true" box="[151,576,1504,1531]" fontSize="11" level="1" pageId="43" pageNumber="44" reason="1">
<emphasis bold="true" box="[151,576,1504,1531]" pageId="43" pageNumber="44">
Genus 
<taxonomicName authority="Karanovic &amp; Hancock, 2009" authorityName="Karanovic &amp; Hancock" authorityYear="2009" box="[238,460,1505,1531]" class="Hexanauplia" family="Ameiridae" genus="Eduardonitocrella" kingdom="Animalia" order="Harpacticoida" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[238,460,1505,1531]" italics="true" pageId="43" pageNumber="44">Eduardonitocrella</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[467,576,1504,1530]" pageId="43" pageNumber="44" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
