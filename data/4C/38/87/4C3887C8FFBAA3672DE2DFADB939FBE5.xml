<taxonomicName authority="Bates, 1886" authorityName="Bates" authorityYear="1886" box="[649,1082,955,978]" class="Insecta" family="Passalidae" genus="Veturius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="287" phylum="Arthropoda" rank="species" species="cirratus" subGenus="Ouayana">
<emphasis box="[649,736,955,978]" italics="true" pageId="8" pageNumber="287">Veturius</emphasis>
(
<emphasis box="[751,849,955,978]" italics="true" pageId="8" pageNumber="287">Ouayana</emphasis>
) 
<emphasis box="[864,948,955,978]" italics="true" pageId="8" pageNumber="287">cirratus</emphasis>
Bates, 1886
</taxonomicName>
