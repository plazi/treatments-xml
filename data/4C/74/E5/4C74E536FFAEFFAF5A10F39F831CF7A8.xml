<taxonomicName authority="Song &amp; Mendes &amp; Huang, 2012" authorityName="Song &amp; Mendes &amp; Huang" authorityYear="2012" box="[223,558,2033,2055]" class="Insecta" family="Meinertellidae" genus="Machilontus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeognatha" pageId="12" pageNumber="325" phylum="Arthropoda" rank="species" species="lii" status="new subgenus, new species" subGenus="Protumidolontus">
