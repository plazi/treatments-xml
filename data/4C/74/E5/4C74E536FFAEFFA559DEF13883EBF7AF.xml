<taxonomicName authority="Song &amp; Mendes &amp; Huang, 2012" authorityName="Song &amp; Mendes &amp; Huang" authorityYear="2012" box="[280,611,603,625]" class="Insecta" family="Meinertellidae" genus="Machilontus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeognatha" pageId="7" pageNumber="320" phylum="Arthropoda" rank="species" species="lii" status="new species" subGenus="Protumidolontus">
