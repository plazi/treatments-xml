<typeStatus box="[144,196,1380,1401]" pageId="20" pageNumber="93">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Koch, 1956" authorityName="Koch" authorityYear="1956" box="[295,651,1379,1401]" class="Insecta" family="Tenebrionidae" genus="Upembarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="93" phylum="Arthropoda" rank="species" species="saegeri">
