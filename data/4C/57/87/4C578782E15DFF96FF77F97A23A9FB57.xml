</date>
(
<number box="[968,1050,190,216]" pageId="10" pageNumber="30" value="71.0" valueMax="75.0" valueMin="67.0">67–75</number>
), setae 
<emphasis bold="true" box="[1145,1174,190,216]" pageId="10" pageNumber="30">ve</emphasis>
<number box="[1182,1214,190,216]" pageId="10" pageNumber="30" value="37.0">37</number>
(
<number box="[1224,1307,190,216]" pageId="10" pageNumber="30" value="37.5" valueMax="40.0" valueMin="35.0">35–40</number>
), setae 
<emphasis bold="true" box="[1402,1437,190,216]" pageId="10" pageNumber="30">sci</emphasis>
<number box="[151,183,229,255]" pageId="10" pageNumber="30" value="22.0">22</number>
(
<number box="[191,269,229,255]" pageId="10" pageNumber="30" value="22.5" valueMax="25.0" valueMin="20.0">20–25</number>
), sensillae 
<emphasis bold="true" box="[403,441,229,255]" pageId="10" pageNumber="30">sce</emphasis>
<number box="[449,482,229,255]" pageId="10" pageNumber="30" value="75.0">75</number>
, 
<emphasis bold="true" box="[495,522,229,255]" pageId="10" pageNumber="30">
c 
<number box="[509,522,229,255]" pageId="10" pageNumber="30" value="1.0">1</number>
</emphasis>
<number box="[531,562,229,255]" pageId="10" pageNumber="30" value="27.0">27</number>
(
<number box="[571,649,229,255]" pageId="10" pageNumber="30" value="27.5" valueMax="30.0" valueMin="25.0">25–30</number>
), 
<emphasis bold="true" box="[671,699,229,255]" pageId="10" pageNumber="30">
c 
<number box="[684,699,229,255]" pageId="10" pageNumber="30" value="2.0">2</number>
</emphasis>
<number box="[708,739,229,255]" pageId="10" pageNumber="30" value="16.0">16</number>
(
<number box="[748,825,229,255]" pageId="10" pageNumber="30" value="16.0" valueMax="17.0" valueMin="15.0">15–17</number>
), 
<emphasis bold="true" box="[848,879,229,255]" pageId="10" pageNumber="30">
d 
<number box="[866,879,229,255]" pageId="10" pageNumber="30" value="1.0">1</number>
</emphasis>
<number box="[888,918,229,255]" pageId="10" pageNumber="30" value="25.0">25</number>
(
<number box="[926,1004,229,255]" pageId="10" pageNumber="30" value="24.5" valueMax="27.0" valueMin="22.0">22–27</number>
) and 
<geoCoordinate box="[1072,1139,229,255]" direction="east" orientation="longitude" pageId="10" pageNumber="30" precision="925" value="1.4166666">
<emphasis bold="true" box="[1072,1100,229,255]" pageId="10" pageNumber="30">
e 
<number box="[1086,1100,229,255]" pageId="10" pageNumber="30" value="1.0">1</number>
</emphasis>
<number box="[1108,1139,229,255]" pageId="10" pageNumber="30" value="25.0">25</number>
</geoCoordinate>
(
<number box="[1147,1227,229,255]" pageId="10" pageNumber="30" value="25.0" valueMax="30.0" valueMin="20.0">20–30</number>
). Posteriad to the dorsal idiosomal shield one pair of tiny platelets occur carring setae 
<emphasis bold="true" box="[975,999,267,293]" pageId="10" pageNumber="30">
f 
<number box="[987,999,267,293]" pageId="10" pageNumber="30" value="1.0">1</number>
</emphasis>
<number box="[1008,1038,268,294]" pageId="10" pageNumber="30" value="27.0">27</number>
and 
<emphasis bold="true" box="[1097,1122,267,293]" pageId="10" pageNumber="30">
f 
<number box="[1108,1122,267,293]" pageId="10" pageNumber="30" value="2.0">2</number>
</emphasis>
<number box="[1130,1157,268,294]" pageId="10" pageNumber="30" value="11.0">11</number>
(
<number box="[1166,1244,268,294]" pageId="10" pageNumber="30" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
) each. Setae 
<emphasis bold="true" box="[1405,1436,267,293]" pageId="10" pageNumber="30">
h 
<number box="[1423,1436,267,293]" pageId="10" pageNumber="30" value="1.0">1</number>
