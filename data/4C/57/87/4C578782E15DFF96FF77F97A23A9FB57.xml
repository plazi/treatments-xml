<number box="[680,710,1880,1906]" pageId="11" pageNumber="31" value="86.0">86</number>
(
<number box="[719,797,1880,1906]" pageId="11" pageNumber="31" value="86.0" valueMax="92.0" valueMin="80.0">80–92</number>
) long with 
<number box="[939,954,1880,1906]" pageId="11" pageNumber="31" value="4.0">4</number>
pairs of 
<emphasis bold="true" box="[1064,1096,1880,1906]" pageId="11" pageNumber="31">hg</emphasis>
-setae [
<emphasis bold="true" box="[1186,1233,1880,1906]" pageId="11" pageNumber="31">
hg 
<number box="[1220,1233,1880,1906]" pageId="11" pageNumber="31" value="1.0">1</number>
</emphasis>
<number box="[1241,1274,1880,1906]" pageId="11" pageNumber="31" value="10.0">10</number>
, 
<emphasis bold="true" box="[1288,1335,1880,1906]" pageId="11" pageNumber="31">
hg 
<number box="[1320,1335,1880,1906]" pageId="11" pageNumber="31" value="2.0">2</number>
</emphasis>
<number box="[1343,1376,1880,1906]" pageId="11" pageNumber="31" value="10.0">10</number>
, 
<emphasis bold="true" box="[1389,1436,1880,1906]" pageId="11" pageNumber="31">
hg 
<number box="[1422,1436,1880,1906]" pageId="11" pageNumber="31" value="3.0">3</number>
