<emphasis bold="true" box="[330,360,306,332]" pageId="10" pageNumber="30">
h 
<number box="[346,360,306,332]" pageId="10" pageNumber="30" value="2.0">2</number>
</emphasis>
<number box="[368,383,306,332]" pageId="10" pageNumber="30" value="9.0">9</number>
