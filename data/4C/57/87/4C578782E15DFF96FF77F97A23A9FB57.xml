<emphasis bold="true" box="[337,383,1919,1945]" pageId="11" pageNumber="31">
hg 
<number box="[369,383,1919,1945]" pageId="11" pageNumber="31" value="4.0">4</number>
</emphasis>
<number box="[392,424,1919,1945]" pageId="11" pageNumber="31" value="10.0">10</number>
