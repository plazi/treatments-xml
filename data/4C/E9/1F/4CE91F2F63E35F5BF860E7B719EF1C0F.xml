<geoCoordinate degrees="19.25" direction="south" orientation="latitude" precision="555" value="-19.25">19.25S</geoCoordinate>
, 
<geoCoordinate degrees="18.05" direction="east" orientation="longitude" precision="555" value="18.05">18.05E</geoCoordinate>
