<geoCoordinate degrees="19.17" direction="south" orientation="latitude" precision="555" value="-19.17">19.17S</geoCoordinate>
, 
<geoCoordinate degrees="27.43" direction="east" orientation="longitude" precision="555" value="27.43">27.43E</geoCoordinate>
