<geoCoordinate degrees="18.55" direction="south" orientation="latitude" precision="555" value="-18.55">18.55S</geoCoordinate>
, 
<geoCoordinate degrees="27.45" direction="east" orientation="longitude" precision="555" value="27.45">27.45E</geoCoordinate>
