<quantity box="[1254,1313,585,611]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="9" pageNumber="290" unit="km" value="4.0">4 km</quantity>
SSE Messaure
</locationDeviation>
, small-area, swampy, herb-rich spruce forest, 
<collectingDate box="[752,983,620,647]" pageId="9" pageNumber="290" value="2016-08-24" valueMax="2016-08-25" valueMin="2016-08-24">
<date box="[752,983,620,647]" pageId="9" pageNumber="290" value="2016-08-24" valueMax="2016-08-25" valueMin="2016-08-24">24–25 August 2016</date>
</collectingDate>
, aspirator, 
<collectorName box="[1113,1324,621,647]" pageId="9" pageNumber="290">M. &amp; C. Jaschhof</collectorName>
(spn. no. 
<mcCollectionCode box="[151,207,657,683]" pageId="9" pageNumber="290">CEC</mcCollectionCode>
<quantity box="[207,291,657,683]" metricMagnitude="1" metricUnit="m" metricValue="4.3688" pageId="9" pageNumber="290" unit="in" value="1720.0">1720 in</quantity>
<mcCollectionCode box="[297,377,657,683]" country="Sweden" httpUri="http://grbio.org/cool/rsbi-vwmq" name="Swedish Museum of Natural History, Entomology Collections" pageId="9" pageNumber="290">NHRS</mcCollectionCode>
