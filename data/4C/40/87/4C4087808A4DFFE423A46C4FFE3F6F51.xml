<collectingCountry box="[1351,1432,1809,1835]" name="Poland" pageId="15" pageNumber="296">Poland</collectingCountry>
, 
<collectingCountry box="[151,238,1845,1871]" name="Estonia" pageId="15" pageNumber="296">Estonia</collectingCountry>
