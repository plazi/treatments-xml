<taxonomicName id="89EBD881A410FFE4FAECDB2EFA39F8BD" box="[1313,1436,1806,1827]" class="Insecta" family="Staphylinidae" genus="Lomechusoides" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="69" phylum="Arthropoda" rank="species" species="amurensis">
