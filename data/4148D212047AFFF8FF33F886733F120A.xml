<taxonomicName id="0EE11887047BFFF8FD84FDEE7516120A" authorityName="Koehler" authorityYear="1896" box="[544,772,547,573]" class="Ophiuroidea" family="Ophiuridae" genus="Ophiernus" kingdom="Animalia" order="Ophiurida" pageId="43" pageNumber="44" phylum="Echinodermata" rank="species" species="abyssalis">
<emphasis id="FB95BF16047BFFF8FD84FDEE7516120A" box="[544,772,547,573]" italics="true" pageId="43" pageNumber="44">Ophiernus abyssalis</emphasis>
</taxonomicName>
