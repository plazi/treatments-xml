<taxonomicName id="0EE11887047BFFF8FDC1FE16754711C2" authorityName="$ Lyman" authorityYear="1878" box="[613,853,475,501]" class="Ophiuroidea" family="Ophiuridae" genus="Ophiernus" kingdom="Animalia" order="Ophiurida" pageId="43" pageNumber="44" phylum="Echinodermata" rank="species" species="vallincola">
