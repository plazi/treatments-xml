<taxonomicName id="92964D54FFA5FF990BD4EE8CFDACFC1D" authority="Rosenschoeld, 1838" authorityName="Rosenschoeld" authorityYear="1838" box="[151,621,967,993]" class="Insecta" family="Curculionidae" genus="Rhinusa" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="37" phylum="Arthropoda" rank="species" species="comosa">
