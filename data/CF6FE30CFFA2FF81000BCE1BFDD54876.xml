<taxonomicName id="80C62999FFA6FF8E00BACC2DFB644D2E" authorityName="Vandel" authorityYear="1952" box="[982,1155,1819,1841]" class="Malacostraca" family="Styloniscidae" kingdom="Animalia" order="Isopoda" pageId="7" pageNumber="367" phylum="Arthropoda" rank="subFamily" subFamily="Styloniscinae">Styloniscinae</taxonomicName>
, Notoniscinae, and Kuscheloniscinae (
<bibRefCitation id="23572FEBFFA6FF8E076ACC0CFB5C4D4F" author="Strouhal H" box="[1030,1211,1850,1872]" pageId="7" pageNumber="367" pagination="185 - 244" refId="ref26756" refString="Strouhal H. 1961. Die Oniscoideen-Fauna der Juan Fernandez- Inseln (Crustacea, Isopoda terrestria). Annalen des naturhistorischen Museums in Wien 64: 185 - 244." type="journal article" year="1961">Strouhal, 1961</bibRefCitation>
). The new genus belongs to 
<taxonomicName id="80C62999FFA6FF8E00F6CC6FFBCC4D70" authorityName="Vandel" authorityYear="1952" box="[922,1067,1881,1903]" class="Malacostraca" family="Styloniscidae" kingdom="Animalia" order="Isopoda" pageId="7" pageNumber="367" phylum="Arthropoda" rank="subFamily" subFamily="Styloniscinae">Styloniscinae</taxonomicName>
, which also includes 
<taxonomicName id="80C62999FFA6FF8E0662CC6FFA614D71" authorityName="Dana" authorityYear="1853" box="[1294,1414,1881,1902]" class="Malacostraca" family="Styloniscidae" genus="Styloniscus" kingdom="Animalia" order="Isopoda" pageId="7" pageNumber="367" phylum="Arthropoda" rank="genus">
<emphasis id="75B28E08FFA6FF8E0662CC6FFA614D71" box="[1294,1414,1881,1902]" italics="true" pageId="7" pageNumber="367">Styloniscus</emphasis>
</taxonomicName>
, 
<taxonomicName id="80C62999FFA9FF8103C8CBF0FEA04AC4" authorityName="Graeve" authorityYear="1914" box="[164,327,198,219]" class="Malacostraca" family="Styloniscidae" genus="Cordioniscus" kingdom="Animalia" order="Isopoda" pageId="8" pageNumber="368" phylum="Arthropoda" rank="genus">
<emphasis id="75B28E08FFA9FF8103C8CBF0FEA04AC4" box="[164,327,198,219]" italics="true" pageId="8" pageNumber="368">Cordioniscus</emphasis>
</taxonomicName>
, 
<taxonomicName id="80C62999FFA9FF810211CBF0FDA24AC4" authorityName="Arcangeli" authorityYear="1930" box="[381,581,198,219]" class="Malacostraca" family="Styloniscidae" genus="Clavigeroniscus" kingdom="Animalia" order="Isopoda" pageId="8" pageNumber="368" phylum="Arthropoda" rank="genus">
<emphasis id="75B28E08FFA9FF810211CBF0FDA24AC4" box="[381,581,198,219]" italics="true" pageId="8" pageNumber="368">Clavigeroniscus</emphasis>
</taxonomicName>
, 
<taxonomicName id="80C62999FFA9FF810116CBF0FCE54AC4" authorityName="Vandel" authorityYear="1952" box="[634,770,198,219]" class="Malacostraca" family="Styloniscidae" genus="Indoniscus" kingdom="Animalia" order="Isopoda" pageId="8" pageNumber="368" phylum="Arthropoda" rank="genus">
<emphasis id="75B28E08FFA9FF810116CBF0FCE54AC4" box="[634,770,198,219]" italics="true" pageId="8" pageNumber="368">Indoniscus</emphasis>
</taxonomicName>
, 
<taxonomicName id="80C62999FFA9FF8103C8CBD2FE914AE6" authorityName="Dalens" authorityYear="1989" box="[164,374,228,249]" class="Malacostraca" family="Styloniscidae" genus="Thailandoniscus" kingdom="Animalia" order="Isopoda" pageId="8" pageNumber="368" phylum="Arthropoda" rank="genus">
<emphasis id="75B28E08FFA9FF8103C8CBD2FE914AE6" box="[164,374,228,249]" italics="true" pageId="8" pageNumber="368">Thailandoniscus</emphasis>
</taxonomicName>
, 
<taxonomicName id="80C62999FFA9FF8102FBCBD2FDD24AE6" authorityName="Taiti &amp; Xue" authorityYear="2012" box="[407,565,228,249]" class="Malacostraca" family="Styloniscidae" genus="Trogloniscus" kingdom="Animalia" order="Isopoda" pageId="8" pageNumber="368" phylum="Arthropoda" rank="genus">
<emphasis id="75B28E08FFA9FF8102FBCBD2FDD24AE6" box="[407,565,228,249]" italics="true" pageId="8" pageNumber="368">Trogloniscus</emphasis>
