<taxonomicName id="B7DF9BA7CF66FF93FCBAFD151A1279F8" box="[837,1063,661,686]" class="Insecta" family="Zopheridae" genus="Diodesma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="414" phylum="Arthropoda" rank="species" species="slipinskii" status="sp. nov.">
