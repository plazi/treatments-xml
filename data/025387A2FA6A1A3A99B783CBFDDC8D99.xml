<taxonomicName id="4DFA4D37FA6A1A3A98698207FCFF8D44" authority="Kiaerskou (1893: 86)" authorityName="Kiaerskou" authorityPageNumber="86" authorityYear="1893" box="[342,784,1346,1373]" class="Magnoliopsida" family="Myrtaceae" genus="Myrcia" kingdom="Plantae" order="Myrtales" pageId="14" pageNumber="83" phylum="Tracheophyta" rank="species" species="rabeniana">
