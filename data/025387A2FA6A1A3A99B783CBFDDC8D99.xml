<taxonomicName id="4DFA4D37FA6A1A3A99998222FDC08D99" authority="(Govaerts et al. 2020)" baseAuthorityName="Govaerts" baseAuthorityYear="2020" box="[166,559,1382,1408]" class="Magnoliopsida" family="Myrtaceae" genus="Myrcia" kingdom="Plantae" order="Myrtales" pageId="14" pageNumber="83" phylum="Tracheophyta" rank="species" species="venulosa">
