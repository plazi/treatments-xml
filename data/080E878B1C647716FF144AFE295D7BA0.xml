<subSubSection id="C8BD65161C64771BFF4649C32A087FCA" pageId="4" pageNumber="5" type="diagnosis">
<paragraph id="8018369D1C64771BFF4649C32A087FCA" blockId="4.[151,1437,1650,2016]" pageId="4" pageNumber="5">
<emphasis id="B2D3EA8F1C64771BFF4649C3291E7FE0" bold="true" box="[197,331,1725,1751]" pageId="4" pageNumber="5">Diagnosis:</emphasis>
