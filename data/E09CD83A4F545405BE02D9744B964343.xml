<taxonomicName id="923730067252B6668A8368D2485FDF11" authorityName="Zimmermann" authorityYear="1832" class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="598" pageNumber="599" phylum="Arthropoda" rank="subSpecies" species="quenseli" subSpecies="silvicola">
