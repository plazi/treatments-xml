<taxonomicName id="3CD84D6AFF8A9829B42CFE58FC30DCC9" authority="Biswas &amp; Majumder" authorityName="Biswas &amp; Majumder" box="[537,949,398,429]" class="Arachnida" family="Liocranidae" genus="Oedignatha" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="105" phylum="Arthropoda" rank="species" species="shillongensis" status="sp. nov.">
