<number id="924BFC33FFD9FFD98CC9362FFD59AE5E" box="[710,738,1051,1072]" pageId="9" pageNumber="262" value="12.0">12</number>
; Monné, 
<number id="924BFC33FFD9FFD98D4E362EFCCCAE5E" box="[833,887,1050,1072]" pageId="9" pageNumber="262" value="2005.0">2005</number>
