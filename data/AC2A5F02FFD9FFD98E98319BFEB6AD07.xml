<taxonomicName id="E3839597FFD9FFD98E98362EFDE1AE5E" authority="Melzer, 1932: 226" authorityName="Melzer" authorityPageNumber="226" authorityYear="1932" box="[151,602,1050,1072]" class="Insecta" family="Cerambycidae" genus="Trypanidius" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="262" phylum="Arthropoda" rank="species" species="albosignatus">
<emphasis id="16F73206FFD9FFD98E98362EFE23AE5E" box="[151,408,1050,1072]" italics="true" pageId="9" pageNumber="262">Trypanidius albosignatus</emphasis>
Melzer, 
<number id="924BFC33FFD9FFD98FFE362FFD93AE5E" box="[497,552,1051,1072]" pageId="9" pageNumber="262" value="1932.0">1932</number>
