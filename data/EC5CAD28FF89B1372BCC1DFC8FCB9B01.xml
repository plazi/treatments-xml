<number id="D23D0E19FF89B13629CB1CF988989DC1" box="[656,719,1962,1983]" pageId="8" pageNumber="279" value="23.5" valueMax="25.0" valueMin="22.0">22–25</number>
; vertebrae 
<number id="D23D0E19FF89B13628631CF989079DC1" box="[824,848,1962,1983]" pageId="8" pageNumber="279" value="24.0">24</number>
.............................................................................................. 
