<taxonomicName id="E6431241F70DE26E12E464A7A23386A6" baseAuthorityName="Letzner" baseAuthorityYear="1851" class="Hexapoda" family="Carabidae" genus="Platynus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="772" pageNumber="773" phylum="Arthropoda" rank="species" species="marginatus">
