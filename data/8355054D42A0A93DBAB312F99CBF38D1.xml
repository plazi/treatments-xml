<taxonomicName id="BB41E6EE989F65E7CF45B8D04D5A004B" LSID="MB826798" authority="C. M. Tian &amp; N. Jiang" class="Sordariomycetes" family="Erythrogloeaceae" genus="Dendrostoma" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Dendrostoma chinense" order="Diaporthales" pageId="10" pageNumber="77" phylum="Ascomycota" rank="species" species="chinense">Dendrostoma chinense C.M. Tian &amp; N. Jiang</taxonomicName>
<taxonomicNameLabel id="ED270D68927374F942CFF594868086D3" pageId="10" pageNumber="77">sp. nov.</taxonomicNameLabel>
