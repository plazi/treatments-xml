<taxonomicName id="4C764D47EB7E7F723C45FAC1FDEA3678" authority="Aitlinger, 2013" authorityName="Aitlinger" authorityYear="2013" box="[284,558,1331,1351]" class="Arachnida" family="Trombidiidae" genus="Sicilitrombium" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="1999" phylum="Arthropoda" rank="species" species="albanesianum" status="nov.sp.">
