<quantity id="4CE156C17026B405B9D8FA2157000BE9" box="[491,592,1465,1487]" metricMagnitude="2" metricUnit="m" metricValue="3.95" metricValueMax="7.5" metricValueMin="0.4" pageId="6" pageNumber="1943" unit="m" value="395.0" valueMax="750.0" valueMin="40.0">
