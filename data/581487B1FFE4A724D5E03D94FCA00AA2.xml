<number id="66752480FFE4A724D4FD3E43FE2E0A54" box="[394,453,1620,1646]" pageId="19" pageNumber="20" value="1831.0">1831</number>
or 
<number id="66752480FFE4A724D49B3E43FDC20A54" box="[492,553,1620,1646]" pageId="19" pageNumber="20" value="1832.0">1832</number>
