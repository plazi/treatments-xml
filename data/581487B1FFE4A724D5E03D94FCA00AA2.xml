</bibRefCitation>
; 
<bibRefCitation id="B42C4B56FFE4A724D41F3E3DFDF30A7E" author="Evenhuis" box="[360,536,1578,1604]" pageId="19" pageNumber="32" refString="Evenhuis, N. L. (1997) Litteratura   Taxonomica   Dipterorum (1758 - 1930). Backhuys Publishers, Leiden, 872 pp." type="book chapter" year="1997">
Evenhuis 
<number id="66752480FFE4A724D4AB3E3DFDF30A7E" box="[476,536,1578,1604]" pageId="19" pageNumber="20" value="1997.0">1997</number>
</bibRefCitation>
