<collectingRegion id="49A93CB9FF9CFFC6B451FB36FB1C04C3" box="[1058,1154,1171,1194]" country="China" name="Guangxi" pageId="1" pageNumber="238">Guangxi</collectingRegion>
, 
<collectingRegion id="49A93CB9FF9CFFC6B4FEFB36FB4804C3" box="[1165,1238,1171,1194]" country="China" name="Fujian" pageId="1" pageNumber="238">Fujian</collectingRegion>
