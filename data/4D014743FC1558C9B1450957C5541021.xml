<taxonomicName id="F135B379BF83164834ACF2E0E14FA065" authorityName="Zhu &amp; Zhong" authorityYear="2020" class="Arachnida" family="Sparassidae" genus="Thelcticopis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thelcticopis dahanensis" order="Araneae" pageId="0" pageNumber="105" phylum="Arthropoda" rank="species" species="dahanensis">
