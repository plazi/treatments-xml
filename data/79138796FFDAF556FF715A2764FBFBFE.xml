<collectionCode id="97ABAE45FFDAF556FAE25AB7615CFCDE" box="[1284,1390,765,791]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="5" pageNumber="156" type="Museum">NHMUK</collectionCode>
