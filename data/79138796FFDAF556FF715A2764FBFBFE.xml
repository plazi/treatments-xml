<collectionCode id="97ABAE45FFDAF556FD085B0F6706FC96" box="[750,820,837,863]" pageId="5" pageNumber="156">TYPE</collectionCode>
<collectionCode id="97ABAE45FFDAF556FCDB5B0F67B0FC96" box="[829,898,837,863]" pageId="5" pageNumber="156">HYM</collectionCode>
. [printed] 17B.930 [handwritten]\ 
<taxonomicName id="36BA4D03FFDAF556FAEF5B0F655BFC4A" authority="Smith." authorityName="Smith." class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="156" phylum="Arthropoda" rank="species" species="insularis">
<emphasis id="C3CEEA92FFDAF556FAEF5B0F6524FC4A" italics="true" pageId="5" pageNumber="156">Centris insularis</emphasis>
Smith.
</taxonomicName>
