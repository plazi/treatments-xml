<collectionCode id="97ABAE45FFDAF556FB705B236132FC4A" box="[1174,1280,873,899]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="5" pageNumber="156" type="Museum">NHMUK</collectionCode>
010812619 in the 
<collectionCode id="97ABAE45FFDAF556FEEA5BC76544FC6E" box="[268,374,909,935]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="5" pageNumber="156" type="Museum">NHMUK</collectionCode>
