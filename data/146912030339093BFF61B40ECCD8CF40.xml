<heading id="C73714790339093BFF61B40ECAB0CC44" box="[172,753,1116,1140]" centered="true" fontSize="9" level="2" pageId="29" pageNumber="56" reason="2">
<taxonomicName id="5BC0D8960339093BFF61B40ECA59CC43" authority="Pollet, 2005" authorityName="Pollet" authorityYear="2005" box="[172,536,1116,1140]" class="Insecta" family="Dolichopodidae" genus="Australachalcus" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="56" phylum="Arthropoda" rank="species" species="cummingi" status="SP. NOV.">AUSTRALACHALCUS CUMMINGI</taxonomicName>
<emphasis id="AEB47F070339093BFDEDB432CAC5CC44" bold="true" box="[544,644,1116,1140]" pageId="29" pageNumber="56">
