<heading id="D0F181A9C43BB54CFC1A8718FAB1BD05" box="[911,1334,1047,1073]" centered="true" fontSize="36" level="2" pageId="4" pageNumber="73" reason="5">
<taxonomicName id="4C064D46C43BB54CFC1A8718FAB1BD05" authority="Horsfield, 1823" authorityName="Horsfield" authorityYear="1823" box="[911,1334,1047,1073]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="4" pageNumber="73" phylum="Chordata" rank="species" species="affinis">
<emphasis id="B972EAD7C43BB54CFC1A8718FBF2BD05" box="[911,1141,1047,1073]" italics="true" pageId="4" pageNumber="73">Rhinolophus affinis</emphasis>
