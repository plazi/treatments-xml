AC, AL, AM, AP, BA, CE, DF, ES, GO, MA, MG, MS, MT, PA, PB, PE, PI, 
<collectingRegion box="[597,632,1422,1445]" country="Brazil" name="Para" pageId="6" pageNumber="975">PR</collectingRegion>
