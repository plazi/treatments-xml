<collectionCode LSID="urn:lsid:biocol.org:col:34991" box="[603,694,789,813]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="8" pageNumber="9">HNHM</collectionCode>
