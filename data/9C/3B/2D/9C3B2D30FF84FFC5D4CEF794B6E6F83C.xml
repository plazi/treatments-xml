. Female (
<collectionCode LSID="urn:lsid:biocol.org:col:34991" box="[606,697,258,282]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34991" name="Hungarian Natural History Museum (Termeszettudomanyi Muzeum)" pageId="16" pageNumber="17">HNHM</collectionCode>
), 
<collectingCountry box="[717,815,258,282]" name="Vietnam" pageId="16" pageNumber="17">Vietnam</collectingCountry>
, Tam Dao, submontane foggy forest, litter sample, altitude 
<emphasis box="[206,233,296,319]" italics="true" pageId="16" pageNumber="17">ca</emphasis>
<quantity box="[239,326,294,318]" metricMagnitude="3" metricUnit="m" metricValue="1.2" pageId="16" pageNumber="17" unit="m" value="1200.0">1200 m</quantity>
, 
<date box="[338,529,294,318]" pageId="16" pageNumber="17" value="1988-10-16">
<number box="[338,366,294,318]" pageId="16" pageNumber="17" value="16.0">16</number>
October 
<number box="[470,529,294,318]" pageId="16" pageNumber="17" value="1988.0">1988</number>
</date>
, J. Starý coll. 
<typeStatus box="[687,790,296,319]" pageId="16" pageNumber="17">
