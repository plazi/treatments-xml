in 
<collectingCountry box="[1233,1291,1423,1449]" name="India" pageId="4" pageNumber="60">India</collectingCountry>
viz., 
<taxonomicName authorityName="Eaton" authorityYear="1881" class="Insecta" family="Leptophlebiidae" genus="Choroterpes" kingdom="Animalia" order="Ephemeroptera" pageId="4" pageNumber="60" phylum="Arthropoda" rank="genus">
