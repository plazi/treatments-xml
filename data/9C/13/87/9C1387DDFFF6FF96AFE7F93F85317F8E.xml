<subSubSection box="[199,850,599,626]" pageId="6" pageNumber="61" type="etymology">
<paragraph blockId="6.[151,1437,152,700]" box="[199,850,599,626]" pageId="6" pageNumber="61">
<emphasis bold="true" box="[199,340,599,625]" pageId="6" pageNumber="61">Etymology.</emphasis>
