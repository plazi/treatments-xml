; body with wing 
<number box="[905,957,487,513]" pageId="6" pageNumber="61" value="311.0">311</number>
.0, Ƥ 
<number box="[1014,1068,488,514]" pageId="6" pageNumber="61" value="11.5">11.5</number>
; pronotum 
<number box="[1202,1238,487,513]" pageId="6" pageNumber="61" value="31.0">31</number>
.0, Ƥ 
<number box="[1297,1336,488,514]" pageId="6" pageNumber="61" value="1.1">1.1</number>
