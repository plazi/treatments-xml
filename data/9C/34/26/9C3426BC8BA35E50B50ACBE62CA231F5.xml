<emphasis bold="true" pageId="0" pageNumber="183">5</emphasis>
<taxonomicName baseAuthorityName="Warncke" baseAuthorityYear="1972" class="Insecta" family="Andrenidae" genus="Avpanurgus" kingdom="Animalia" lsidName="Avpanurgus flavofasciatus" order="Hymenoptera" pageId="0" pageNumber="183" phylum="Arthropoda" rank="species" species="flavofasciatus">
