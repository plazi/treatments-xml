<taxonomicName authority="Veenakumari, 2014" authorityName="Veenakumari" authorityYear="2014" box="[1078,1377,459,483]" class="Insecta" family="Scelionidae" genus="Phanuromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="357" phylum="Arthropoda" rank="species" species="jarawa">
