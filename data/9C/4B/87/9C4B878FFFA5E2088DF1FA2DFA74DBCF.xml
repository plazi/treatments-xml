<subSubSection box="[199,1391,1087,1112]" pageId="9" pageNumber="382" type="etymology">
<paragraph blockId="9.[151,1437,151,1365]" box="[199,1391,1087,1112]" pageId="9" pageNumber="382">
<emphasis bold="true" box="[199,326,1087,1112]" pageId="9" pageNumber="382">Etymology</emphasis>
