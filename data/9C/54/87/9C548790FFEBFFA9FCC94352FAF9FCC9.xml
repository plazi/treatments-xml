<collectingCountry box="[1362,1439,723,742]" name="Ecuador" pageId="28" pageNumber="29">Ecuador</collectingCountry>
, 
<quantity box="[818,941,752,772]" metricMagnitude="2" metricUnit="m" metricValue="7.75" metricValueMax="13.0" metricValueMin="2.5" pageId="28" pageNumber="29" unit="m" value="775.0" valueMax="1300.0" valueMin="250.0">250–1,300 m</quantity>
