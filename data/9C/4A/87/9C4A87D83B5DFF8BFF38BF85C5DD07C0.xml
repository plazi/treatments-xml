to New 
<collectingCountry box="[465,546,1461,1487]" name="Guinea-Bissau" pageId="3" pageNumber="126">Guinea</collectingCountry>
and 
<collectingCountry box="[598,701,1461,1487]" name="Australia" pageId="3" pageNumber="126">Australia</collectingCountry>
