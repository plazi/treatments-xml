<taxonomicName id="EEF98AD69A27FFDDFED5FDF4FE4CFD27" box="[379,454,618,643]" class="Insecta" family="Coenagrionidae" genus="Argia" kingdom="Animalia" order="Odonata" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="haberi">
