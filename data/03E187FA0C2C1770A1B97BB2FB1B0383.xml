<quantity id="4CB09B090C2C1772A33C7ADDFDFA0196" box="[530,619,1971,1995]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="1" unit="mm" value="0.7">0.7 mm</quantity>
