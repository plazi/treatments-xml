<collectionCode id="ED7AAE07873BAA2442BBFE92DFBFAB5F" box="[1190,1247,352,378]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" lsid="urn:lsid:biocol.org:col:34700" name="California Academy of Sciences" pageId="32" pageNumber="33" type="Museum">CAS</collectionCode>
