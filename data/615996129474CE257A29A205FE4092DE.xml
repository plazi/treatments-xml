) and 
<taxonomicName id="2EF05C879475CE257C0BA504FB61923D" baseAuthorityName="Cocco" baseAuthorityYear="1838" box="[1092,1245,338,364]" class="Actinopterygii" family="Myctophidae" genus="Lobianchia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myctophiformes" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="gemellarii">
