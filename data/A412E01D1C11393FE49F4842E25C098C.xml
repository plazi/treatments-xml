<materialsCitation id="9CD35B561C11393CE49F48FCE26B0E87" ID-GBIF-Occurrence="2605309626" pageId="21" pageNumber="148">
<typeStatus id="F300EFA91C11393CE49F48FCE0030EE7" box="[178,268,1411,1432]" pageId="21" pageNumber="148">Lectotype</typeStatus>
: 
<specimenCount id="3ABD9A821C11393CE53B48FCE0280EE7" box="[278,295,1411,1432]" pageId="21" pageNumber="148" type="female">♀</specimenCount>
. 
<collectingCountry id="54AC119B1C11393CE51E4801E0720EE8" box="[307,381,1406,1431]" name="Peru" pageId="21" pageNumber="148">PÉROU</collectingCountry>
, Paita et Tallazo de Paita (Reg. Desetique, bord de la mer) (
<quantity id="EB43FCEE1C11393CE7B448FDE2EA0EE7" box="[921,997,1410,1432]" metricMagnitude="1" metricUnit="m" metricValue="5.5" metricValueMax="8.0" metricValueMin="3.0" pageId="21" pageNumber="148" unit="m" value="55.0" valueMax="80.0" valueMin="30.0">30-80 m</quantity>
