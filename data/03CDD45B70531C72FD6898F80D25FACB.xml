<taxonomicName id="4C641ECE70531C72FC8B98F80BA3FF3F" authorityName="Thomson" authorityYear="1894" box="[864,990,151,178]" class="Malacostraca" family="Anaspididae" genus="Anaspides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anaspidacea" pageId="4" pageNumber="317" phylum="Arthropoda" rank="genus">
<emphasis id="B910B95F70531C72FC8B98F80BA3FF3F" bold="true" box="[864,990,151,178]" italics="true" pageId="4" pageNumber="317">Anaspides</emphasis>
