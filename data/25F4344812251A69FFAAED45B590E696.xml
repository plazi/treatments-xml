<subSubSection id="58F78D4BABBDA057A501C27523B2107C" pageId="3" pageNumber="4" type="type series">
<paragraph id="F44A834CB3A05E1057977715EC424DF7" pageId="3" pageNumber="4">Type series.</paragraph>
<paragraph id="B1A226679212D3359B9E2ACAA99541FA" pageId="3" pageNumber="4">
