<geoCoordinate id="5BB333852DD6A0D131C7AE5C77C3A634" direction="north" orientation="latitude" precision="1" value="36.8774">36°52.644'N</geoCoordinate>
, 
<geoCoordinate id="E4183712F596ACF8ECB5CC96714A8115" direction="east" orientation="longitude" precision="1" value="128.2964">128°17.784'E</geoCoordinate>
