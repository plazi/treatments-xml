<taxonomicName id="6FDCA042D407423D32943042E350AB09" authority="L." class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="651" phylum="Tracheophyta" rank="species" species="nigra">
