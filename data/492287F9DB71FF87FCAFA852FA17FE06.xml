<quantity id="06739B0ADB71FF99FB5CAB6BFB40F8C8" box="[1161,1243,1828,1851]" metricMagnitude="-3" metricUnit="m" metricValue="8.8" pageId="14" pageNumber="543" unit="mm" value="8.8">8.8 mm</quantity>
(J 
<specimenCount id="D78DFD66DB71FF99FB2DAB6EFA97F8C9" box="[1272,1292,1825,1850]" pageId="14" pageNumber="543" type="female">♀</specimenCount>
