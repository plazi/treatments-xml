<taxonomicName id="4C122BFAFFECFFA40449FBBC5562A729" class="Magnoliopsida" family="Bignoniaceae" genus="Tabebuia" kingdom="Plantae" order="Lamiales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="species" species="radiata">
