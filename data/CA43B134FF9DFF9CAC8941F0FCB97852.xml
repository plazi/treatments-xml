<taxonomicName id="85EA7BA1FF9DFF9CAC8941F0FEEB7EC3" authorityName="Banks" authorityYear="1906" box="[178,306,651,673]" class="Insecta" family="Apataniidae" genus="Moropsyche" kingdom="Animalia" order="Trichoptera" pageId="21" pageNumber="1284" phylum="Arthropoda" rank="genus">Moropsyche</taxonomicName>
