<taxonomicName id="4C5246A7F80CFFA1FCFEFED5FC0F4A7C" authorityName="Heyden" authorityYear="1870" box="[777,930,332,357]" class="Insecta" family="Leiodidae" genus="Catops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="vandalitiae">
