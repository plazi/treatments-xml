<taxonomicName id="4C2A07EFFF80FFFAD5B1F907301FF915" baseAuthorityName="Hayward" baseAuthorityYear="1947" box="[694,800,1718,1744]" class="Insecta" family="Hesperiidae" genus="Astraptes" kingdom="Animalia" order="Lepidoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="genus">
