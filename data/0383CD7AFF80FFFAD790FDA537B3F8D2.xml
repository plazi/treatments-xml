<taxonomicName id="4C2A07EFFF80FFFAD458F90737B2F90A" authority="(Cramer, 1777)" authorityName="Cramer" authorityYear="1777" baseAuthorityName="Cramer" baseAuthorityYear="1777" box="[863,1165,1717,1744]" class="Insecta" family="Hesperiidae" genus="Astraptes" kingdom="Animalia" order="Lepidoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="anaphus">
