<quantity id="4C879B28E35D0C11BFEA8DA6FA8599BC" box="[1220,1388,1976,2001]" metricMagnitude="-3" metricUnit="m" metricValue="2.35" metricValueMax="2.45" metricValueMin="2.25" pageId="6" pageNumber="444" unit="mm" value="2.35" valueMax="2.45" valueMin="2.25">2.25–2.45 mm</quantity>
