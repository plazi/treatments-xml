<quantity id="4C879B28E35D0C11B9F38DA6FCD199BC" box="[733,824,1976,2000]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="6" pageNumber="444" unit="mm" value="1.4">1.4 mm</quantity>
