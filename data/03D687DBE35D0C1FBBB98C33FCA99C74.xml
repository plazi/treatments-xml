<quantity id="4C879B28E35D0C11B8548DA6FC0D99BC" box="[890,996,1976,2001]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" pageId="6" pageNumber="444" unit="mm" value="0.85">0.85 mm</quantity>
