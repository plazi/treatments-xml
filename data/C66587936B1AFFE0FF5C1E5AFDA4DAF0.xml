<taxonomicName id="89CC4D066B1AFFE0FEE31EFCFD1EDBAD" authority="Dworakowska, 1970" authorityName="Dworakowska" authorityYear="1970" box="[296,670,1742,1765]" class="Insecta" family="Cicadellidae" genus="Zorka" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="83" phylum="Arthropoda" rank="species" species="ariadnae">
