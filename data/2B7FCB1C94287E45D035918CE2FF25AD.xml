2010 
<taxonomicName id="64D6018994287E42D0D49291E4342C0C" authority="(J. DE C. SOWERBY)" baseAuthorityName="J. DE C. SOWERBY" baseAuthorityYear="1839" box="[916,1371,2043,2066]" class="Lingulata" family="Trematidae" genus="Schizocrania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lingulida" pageId="3" pageNumber="228" phylum="Brachiopoda" rank="species" species="striata">
<emphasis id="91A2A61894287E42D0D49291E51C2C0C" box="[916,1139,2043,2066]" italics="true" pageId="3" pageNumber="228">Schizocrania striata</emphasis>
