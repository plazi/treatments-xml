<collectingCountry id="9EE3767CFFC9FF90FED24074FEB6FE89" box="[266,334,316,339]" name="Brazil" pageId="3" pageNumber="206">Brazil</collectingCountry>
, 
<collectingCountry id="9EE3767CFFC9FF90FE804074FE01FE89" box="[344,505,316,339]" name="French Guiana" pageId="3" pageNumber="206">French Guiana</collectingCountry>
, and 
<collectingCountry id="9EE3767CFFC9FF90FDE94074FD9AFE89" box="[561,610,316,339]" name="Peru" pageId="3" pageNumber="206">Peru</collectingCountry>
(
<figureCitation id="7ECF2A69FFC9FF90FDA84074FD38FE89" box="[624,704,316,339]" captionStart="Fig" captionStartId="11.[113,144,917,936]" captionTargetBox="[113,1088,219,899]" captionTargetId="figure-229@11.[113,1088,218,901]" captionTargetPageId="11" captionText="Fig. 36. Map showing distribution of the three Pseudodira species." figureDoi="http://doi.org/10.5281/zenodo.5372928" httpUri="https://zenodo.org/record/5372928/files/figure.png" pageId="3" pageNumber="206">Fig. 36</figureCitation>
).
</paragraph>
