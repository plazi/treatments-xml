, 
<collectingCountry id="5DFD8C8AF17CAD77977C16D5F5291AA4" box="[805,890,735,760]" name="Taiwan" pageId="8" pageNumber="309">Taiwan</collectingCountry>
, 
<collectingCountry id="5DFD8C8AF17CAD7797D616D5F2431AA4" box="[911,1040,735,760]" name="Philippines" pageId="8" pageNumber="309">Philippines</collectingCountry>
, 
<collectingCountry id="5DFD8C8AF17CAD77907C16D5F2C61AA4" box="[1061,1173,735,760]" name="Indonesia" pageId="8" pageNumber="309">Indonesia</collectingCountry>
, 
<collectingCountry id="5DFD8C8AF17CAD7790F016EAF3CB1AA4" box="[1193,1432,735,760]" name="Papua New Guinea" pageId="8" pageNumber="309">Papua New Guinea</collectingCountry>
, 
<collectingCountry id="5DFD8C8AF17CAD7794CE170EF74B1B41" box="[151,280,772,797]" name="Micronesia" pageId="8" pageNumber="309">Micronesia</collectingCountry>
, 
