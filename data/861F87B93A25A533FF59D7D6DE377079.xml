<paragraph id="0E0936AF3A25A533FF39D0C4DD007059" blockId="9.[151,760,1092,1659]" pageId="9" pageNumber="304">
<emphasis id="3CC2EABD3A25A533FF39D0C4DD457299" bold="true" box="[151,271,1156,1179]" pageId="9" pageNumber="304">Comment.</emphasis>
<taxonomicName id="C9B64D2C3A25A533FEBAD0C4DDB17299" authorityName="Horvath" authorityYear="1907" box="[276,507,1156,1179]" class="Insecta" family="Anthocoridae" genus="Temnostethus" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="304" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis id="3CC2EABD3A25A533FEBAD0C4DDB17299" box="[276,507,1156,1179]" italics="true" pageId="9" pageNumber="304">Temnostethus gracilis</emphasis>
</taxonomicName>
