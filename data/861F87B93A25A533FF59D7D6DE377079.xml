<taxonomicName id="C9B64D2C3A25A533FE2FD144DD8F7319" authorityName="P.Miller" authorityYear="1754" box="[385,453,1284,1307]" class="Magnoliopsida" family="Rosaceae" genus="Malus" kingdom="Plantae" order="Rosales" pageId="9" pageNumber="304" phylum="Tracheophyta" rank="genus">
<emphasis id="3CC2EABD3A25A533FE2FD144DD8F7319" box="[385,453,1284,1307]" italics="true" pageId="9" pageNumber="304">Malus</emphasis>
</taxonomicName>
, 
<taxonomicName id="C9B64D2C3A25A533FE78D144DE5F7319" box="[470,533,1284,1307]" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" kingdom="Plantae" order="Rosales" pageId="9" pageNumber="304" phylum="Tracheophyta" rank="genus">
