<quantity id="AD099B01FFA1FF899AA1FB86994E4A63" box="[459,548,1098,1124]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="6" pageNumber="7" unit="mm" value="3.3">3.3 mm</quantity>
(
<quantity id="AD099B01FFA1FF899959FB8699E44A63" box="[563,654,1098,1124]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="6" pageNumber="7" unit="mm" value="3.1">3.1 mm</quantity>
in 
<typeStatus id="B54A8846FFA1FF8999DEFB85984A4A63" box="[692,800,1097,1124]" pageId="6" pageNumber="7">holotype</typeStatus>
