<subSubSection id="54D1EAC7FF86FFAA9CF329FEFC6D5847" pageId="1" pageNumber="64" type="diagnosis">
<paragraph id="1C74B94CFF86FFAA9CF329FEFC6D5847" blockId="1.[151,1436,1437,1606]" pageId="1" pageNumber="64">
<emphasis id="2EBF655EFF86FFAA9CF329FEFEFB5B98" bold="true" box="[197,319,1509,1534]" pageId="1" pageNumber="64">Diagnosis.</emphasis>
