<number id="861124F7810F8976FCA3281525C0B202" box="[849,903,324,350]" pageId="15" pageNumber="46" value="0.17">0.17</number>
, PME–PLE 
<number id="861124F7810F8976FBEC28152212B202" box="[1054,1109,324,350]" pageId="15" pageNumber="46" value="0.33">0.33</number>
