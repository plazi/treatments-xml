<number id="861124F7810F8976FD3E284D2545B26A" box="[716,770,284,310]" pageId="15" pageNumber="46" value="0.1">0.10</number>
, PME 
<number id="861124F7810F8976FCA2284D25C1B26A" box="[848,902,284,310]" pageId="15" pageNumber="46" value="0.12">0.12</number>
, PLE 
