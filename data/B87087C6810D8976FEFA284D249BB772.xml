<number id="861124F7810F8976FB40281522A2B202" box="[1202,1253,324,350]" pageId="15" pageNumber="46" value="0.18">0.18</number>
long, front width 
<number id="861124F7810F8976FE6F283D2794B2DA" box="[413,467,364,390]" pageId="15" pageNumber="46" value="0.31">0.31</number>
