(
<bibRefCitation id="23244B53FFEB4533FEB4F890FD88FE31" box="[340,530,1831,1856]" pageId="17" pageNumber="468" refId="ref12451" refString="THOmPSON, H. (1948) Pelagic tunicates of Australia. COmmONWEAlTH COUNCIl FOR SCIENTIFIC AND INDUSTRIAl RESEARCH, MElbOURNE, 196 PP." type="book">Thompson 1948</bibRefCitation>
), Province of Izu (
<bibRefCitation id="23244B53FFEB4533FD03F890FC1EFE31" box="[739,900,1831,1856]" pageId="17" pageNumber="468" refId="ref12476" refString="TOkIOkA, T. (1937) NOTES ON SAlPAS AND DOlIOlUmS OCCURRING IN THE PACIFIC COAST OF MIDDlE JAPAN. The Zoological Society of Japan, 16 (3), 219 - 235." type="journal article">Tokioka 1937</bibRefCitation>
; 
<bibRefCitation id="23244B53FFEB4533FC71F890FC57FE31" box="[913,973,1831,1856]" pageId="17" pageNumber="468" refId="ref12512" refString="TOkIOkA, T. (1960) STUDIES ON THE DISTRIbUTION OF APPENDICUlARIANS AND SOmE THAlIACEANS OF THE NORTH PACIFIC, WITH SOmE mORPHOlOGICAl NOTES. Publications of the Seto Marine Biological Laboratory, 8 (2), 351 - 443." type="journal article">1960</bibRefCitation>
), European waters (Costello 
<emphasis id="75C1EAB0FFEB4533FAF3F89EFAD6FE31" box="[1299,1356,1831,1856]" italics="true" pageId="17" pageNumber="468">et al.</emphasis>
2001), Kaoping Trench, southwestern waters of 
<collectingCountry id="3FA27632FFEB4533FD82F8FBFD2EFE15" box="[610,692,1868,1892]" name="Taiwan" pageId="17" pageNumber="468">Taiwan</collectingCountry>
(
<bibRefCitation id="23244B53FFEB4533FD20F8FBFCE2FE14" box="[704,888,1868,1893]" pageId="17" pageNumber="468" refId="ref12300" refString="TEW, K. S. &amp; LO, W. T. (2005) DISTRIbUTION OF THAlIACEA IN SW TAIWAN COASTAl WATER IN 1997, WITH SPECIAl REFERENCE TO Doliolum denticulatum, Thalia democratica AND T. orientalis. Marine Ecology Progress Series, 292, 181 - 193." type="journal article">Tew &amp; Lo 2005</bibRefCitation>
), 
<collectingCountry id="3FA27632FFEB4533FC6BF8FBFC47FE15" box="[907,989,1868,1892]" name="Taiwan" pageId="17" pageNumber="468">Taiwan</collectingCountry>
