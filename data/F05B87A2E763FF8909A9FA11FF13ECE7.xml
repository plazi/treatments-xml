, and 
<taxonomicName id="BFF24D37E763FF8908ABF818F8C6ECE7" authority="Riley, 1885" authorityName="Riley" authorityYear="1885" box="[405,707,1952,1979]" class="Insecta" family="Noctuidae" genus="Anomis" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="texana">
<emphasis id="4A86EAA6E763FF8908ABF818F83EECE6" box="[405,571,1953,1979]" italics="true" pageId="8" pageNumber="209">Anomis texana</emphasis>
Riley, 1885
</taxonomicName>
(
<taxonomicName id="BFF24D37E763FF890BEFF819F95AECE6" box="[721,863,1952,1978]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
