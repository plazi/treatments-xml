<paragraph id="8BE859445419FFF780EAF877780CFF5C" blockId="0.[146,1431,1969,2024]" lastBlockId="1.[151,1437,152,271]" lastPageId="1" lastPageNumber="20" pageId="0" pageNumber="19">
<emphasis id="B92385565419FFF680EAF8777CD1F87B" italics="true" pageId="0" pageNumber="19">
Affinis 
<taxonomicName id="4C5722C75419FFF680A5F8777836F85B" box="[221,369,1969,1992]" class="Magnoliopsida" family="Gesneriaceae" genus="Primulina" kingdom="Plantae" order="Lamiales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="yungfuensi">P. yungfuensi</taxonomicName>
, a qua pedunculo 
<quantity id="4CAFF4A15419FFF68249F8777BCBF85B" box="[561,652,1969,1992]" metricMagnitude="-1" metricUnit="m" metricValue="1.15" metricValueMax="1.5" metricValueMin="0.8" pageId="0" pageNumber="19" unit="cm" value="11.5" valueMax="15.0" valueMin="8.0">8–15 cm</quantity>
longo (vs. 
<quantity id="4CAFF4A15419FFF68378F8777A23F85B" box="[768,868,1969,1992]" metricMagnitude="-2" metricUnit="m" metricValue="6.75" metricValueMax="9.0" metricValueMin="4.5" pageId="0" pageNumber="19" unit="cm" value="6.75" valueMax="9.0" valueMin="4.5">4.5–9 cm</quantity>
), calyce apice acuminato (vs. acuto), corolla 
<quantity id="4CAFF4A15419FFF6853FF87779A6F87B" metricMagnitude="-2" metricUnit="m" metricValue="3.15" metricValueMax="3.5" metricValueMin="2.8" pageId="0" pageNumber="19" unit="cm" value="3.15" valueMax="3.5" valueMin="2.8">2.8–3.5 cm</quantity>
longa (vs. 
<quantity id="4CAFF4A15419FFF68112F81778AFF87B" box="[362,488,2001,2024]" metricMagnitude="-2" metricUnit="m" metricValue="4.199999999999999" metricValueMax="4.6" metricValueMin="3.8" pageId="0" pageNumber="19" unit="cm" value="4.199999999999999" valueMax="4.6" valueMin="3.8">3.8–4.6 cm</quantity>
), tubo corollae anguste infundibulari, 
<quantity id="4CAFF4A15419FFF683DBF8177D5CF87B" box="[931,1051,2001,2024]" metricMagnitude="-2" metricUnit="m" metricValue="2.15" metricValueMax="2.5" metricValueMin="1.8" pageId="0" pageNumber="19" unit="cm" value="2.15" valueMax="2.5" valueMin="1.8">1.8-2.5 cm</quantity>
longo (vs. cupiformi, 
<quantity id="4CAFF4A15419FFF68567F8177CCDF87B" box="[1311,1418,2001,2024]" metricMagnitude="-2" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="0" pageNumber="19" unit="cm" value="2.75" valueMax="3.0" valueMin="2.5">2.5–3 cm</quantity>
),
</emphasis>
<emphasis id="B92385565418FFF780BFFF5E780CFF5C" italics="true" pageId="1" pageNumber="20">filamentis dense glanduloso-puberulis (vs. glabris), stigmate oblongo (vs. lineari); floret Sept. usque ad Nov. distinguitur.</emphasis>
</paragraph>
</subSubSection>
