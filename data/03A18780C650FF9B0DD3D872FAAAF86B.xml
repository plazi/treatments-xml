, and 
<taxonomicName id="4C084D15C650FF9B0FFEDE49FCCCF8DE" authorityName="Richards" authorityYear="1961" box="[698,844,1876,1900]" class="Insecta" family="Sphaeroceridae" genus="Archiborborus" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="chaetosus">
