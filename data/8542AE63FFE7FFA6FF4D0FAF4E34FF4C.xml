<collectingCountry id="75FC5FE5FFE4FFA6FEBD0FE74BD5FF4C" box="[359,421,223,247]" name="Israel" pageId="10" pageNumber="83">Israel</collectingCountry>
(Smit 
<number id="BB230D52FFE4FFA6FE2B0FE7485CFF4C" box="[497,556,223,247]" pageId="10" pageNumber="83" value="1997.0">1997</number>
), 
<collectingCountry id="75FC5FE5FFE4FFA6FD9A0FD8481DFF4C" box="[576,621,224,247]" name="Iran" pageId="10" pageNumber="83">Iran</collectingCountry>
