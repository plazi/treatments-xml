</taxonomicName>
(= 
<taxonomicName id="5CBE4D5FFF957528FBCFF8CCFB7AF87C" box="[1031,1227,1899,1922]" class="Insecta" family="Notodontidae" genus="Notodonta" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="591" phylum="Arthropoda" rank="subSpecies" species="ziczac" subSpecies="gigantea">
