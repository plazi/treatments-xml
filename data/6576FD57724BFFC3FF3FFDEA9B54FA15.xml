<taxonomicName id="2ADF37C2724BFFC3FE47FA5D9DFFFA15" box="[495,698,1501,1525]" class="Insecta" family="Haglidae" genus="Prosopogryllacris" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="439" phylum="Arthropoda" rank="genus">
