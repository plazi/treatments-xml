<taxonomicName id="72ECEFC544E211BC3E05EC8F163981F3" authority="Hedicke, 1912" authorityName="Hedicke" authorityYear="1912" class="Insecta" family="Liopteridae" genus="Tessmannella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tessmannella kiplingi" order="Hymenoptera" pageId="139" pageNumber="140" phylum="Arthropoda" rank="species" species="kiplingi">Tessmannella kiplingi</taxonomicName>
