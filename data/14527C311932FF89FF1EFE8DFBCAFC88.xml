<quantity id="5B0360C21932FF89FDE0FD9EFD67FD97" box="[588,652,577,599]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="4" pageNumber="333" unit="m" value="500.0">500 m</quantity>
