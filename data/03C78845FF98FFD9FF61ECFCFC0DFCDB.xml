(
<collectingRegion id="49AAF7B1FF9DFFD9FCEFEBF5FC38FD03" box="[793,960,693,719]" country="Brazil" name="Espirito Santo" pageId="22" pageNumber="23">Espírito Santo</collectingRegion>
