(Plate 
<number id="866F7119FFC1FF90FDEB7080FDA902E0" box="[622,639,1690,1716]" pageId="14" pageNumber="15" value="2.0">2</number>
, f; 
<figureCitation id="A89C7FBBFFC1FF90FD207080FD2902E0" box="[677,767,1690,1716]" captionStart="FIGURE 18" captionStartId="40.[151,256,1684,1708]" captionTargetBox="[162,1426,204,1650]" captionTargetId="figure@40.[151,1436,194,1661]" captionTargetPageId="40" captionText="FIGURE 18. Psammotettix emarginata Singh, a, head and thorax dorsal view; b, face; c, forewing; d, apex of female abdomen, ventral view; e, second valvulae; f, first valvulae; g-k, male terminalia, g, genital capsule lateral view; h, i, anal tube, ventral and lateral view respectively; j, k, genital capsule ventral and dorsal view respectively." httpUri="https://zenodo.org/record/193676/files/figure.png" pageId="14" pageNumber="15">
Figs 
<number id="866F7119FFC1FF90FD5A7080FD2902E0" box="[735,767,1690,1716]" pageId="14" pageNumber="15" value="18.19">18</number>
</figureCitation>
, 
<figureCitation id="A89C7FBBFFC1FF90FC8E7080FCFF02E0" box="[779,809,1690,1716]" captionStart="FIGURE 19" captionStartId="41.[151,260,1779,1803]" captionTargetBox="[192,1394,194,1751]" captionTargetId="figure@41.[178,1409,194,1755]" captionTargetPageId="41" captionText="FIGURE 19. Psammotettix species, a, b, P. provincialis Ribaut, aedeagus ventral and left lateral view; c – i, P. emarginata Sing, c, d, aedeagus ventral and left lateral view; e, f, same; g, valve, subgenital plate, style and connective, dorsal view; h, i, style and style apex respectively." httpUri="https://zenodo.org/record/193677/files/figure.png" pageId="14" pageNumber="15">19</figureCitation>
)
</paragraph>
</subSubSection>
