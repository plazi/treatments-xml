<taxonomicName id="4C290E8049014F63CA7BFD6BFBA498CF" authority="M. Bieb." authorityName="M. Bieb." box="[830,1076,734,761]" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="diluta">
<emphasis id="B95DA91149014F63CA7BFD6BFC5A98CF" box="[830,970,734,761]" italics="true" pageId="7" pageNumber="8">Carex diluta</emphasis>
M. Bieb.
</taxonomicName>
× 
<taxonomicName id="4C290E8049014F63CD17FD6BFB5198CF" box="[1106,1217,734,761]" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="distans">
