<date id="FFD710314560FFACFDE9F8B8FD7F4F24" box="[578,738,1820,1845]" pageId="23" pageNumber="524" value="2006-05-16">
<number id="3DA124D64560FFACFDE9F8B8FDC24F24" box="[578,607,1820,1845]" pageId="23" pageNumber="524" value="16.0">16</number>
May 
<number id="3DA124D64560FFACFD0CF8B8FD7F4F24" box="[679,738,1820,1845]" pageId="23" pageNumber="524" value="2006.0">2006</number>
</date>
