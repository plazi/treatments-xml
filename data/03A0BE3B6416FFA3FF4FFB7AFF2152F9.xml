<taxonomicName id="4C0974AE6416FFA3FF2EFB7AFD99526C" authority="G. Fischer, 1814" authorityName="G. Fischer" authorityYear="1814" box="[246,600,1260,1286]" class="Mammalia" family="Talpidae" kingdom="Animalia" order="Soricomorpha" pageId="28" pageNumber="29" phylum="Chordata" rank="family">TALPIDAE G. Fischer, 1814</taxonomicName>
