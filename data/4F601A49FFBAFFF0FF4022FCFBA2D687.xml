<subSubSection id="8FD3F8D4FFBAFFF3FF402369FE7AD530" pageId="13" pageNumber="54" type="materials_examined">
<paragraph id="C776AB5FFFBAFFF3FF402369FE7AD530" blockId="13.[151,1437,258,498]" pageId="13" pageNumber="54">
<emphasis id="F5BD774DFFBAFFF3FF402369FE48D515" bold="true" box="[151,384,258,283]" pageId="13" pageNumber="54">Material examined.</emphasis>
<typeStatus id="187215FDFFBAFFF3FE502369FE3CD514" box="[391,500,258,282]" pageId="13" pageNumber="54">Holotype</typeStatus>
: 
<number id="7101B978FFBAFFF3FE2A2368FDDAD515" box="[509,530,259,283]" pageId="13" pageNumber="54" value="3.0">3</number>
, 
<collectingCountry id="BFDEEBCFFFBAFFF3FDF72369FDAED514" box="[544,614,258,282]" name="China" pageId="13" pageNumber="54">China</collectingCountry>
, Yunnan province, Lijiang, 
<date id="B3778D9FFFBAFFF3FC4C2369FBF5D514" box="[923,1085,258,282]" pageId="13" pageNumber="54" value="2000-08-11">
