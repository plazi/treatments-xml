<collectingCountry id="F342EB8FFFEEF756FE7E490D08B3F974" box="[444,506,1647,1671]" name="Peru" pageId="49" pageNumber="48">Perú</collectingCountry>
, 
<collectingCountry id="F342EB8FFFEEF756FDC6490D0B12F974" box="[516,603,1647,1671]" name="Bolivia" pageId="49" pageNumber="48">Bolivia</collectingCountry>
