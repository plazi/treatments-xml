<taxonomicName id="4C55D09CFFEFF757FEAB4A110BF5FA7B" authorityName="Sternberg" authorityYear="1910" box="[361,700,1395,1416]" class="Insecta" family="Dynastidae" genus="Strategus" kingdom="Animalia" order="Coleoptera" pageId="48" pageNumber="47" phylum="Arthropoda" rank="subSpecies" species="surinamensis" subSpecies="hirtus">
