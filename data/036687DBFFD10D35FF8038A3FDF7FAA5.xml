<number id="3D0724EAFFD20D35FEC13C0DFE3AFC94" box="[470,486,827,851]" pageId="18" pageNumber="466" value="8.0">8</number>
; OOL: 
<number id="3D0724EAFFD20D35FD2B3C0DFD90FC94" box="[572,588,827,851]" pageId="18" pageNumber="466" value="8.0">8</number>
