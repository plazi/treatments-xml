<taxonomicName id="4C4C4D0DFFF3FFC83655FAB8CD46FAB8" class="Demospongiae" family="Halichondriidae" genus="Hymeniacidon" kingdom="Animalia" order="Halichondrida" pageId="12" pageNumber="13" phylum="Porifera" rank="species" species="caerulea">
<emphasis id="B938EA9CFFF3FFC83655FAB8CD46FAB8" italics="true" pageId="12" pageNumber="13">Hymeniacidon caerulea</emphasis>
</taxonomicName>
, but not in 
<taxonomicName id="4C4C4D0DFFF3FFC832F4FADFCE99FAB8" box="[496,699,1373,1397]" class="Demospongiae" family="Agelasidae" genus="Agelas" kingdom="Animalia" order="Agelasida" pageId="12" pageNumber="13" phylum="Porifera" rank="species" species="clathrodes">
<emphasis id="B938EA9CFFF3FFC832F4FADFCE99FAB8" box="[496,699,1373,1397]" italics="true" pageId="12" pageNumber="13">Agelas clathrodes</emphasis>
</taxonomicName>
