<taxonomicName id="4C4C4D0DFFF3FFC83140FAB8CEB2FA9C" box="[580,656,1337,1361]" class="Malacostraca" family="Alpheidae" genus="Synalpheus" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="thele">
<emphasis id="B938EA9CFFF3FFC83140FAB8CEB2FA9C" box="[580,656,1337,1361]" italics="true" pageId="12" pageNumber="13">S thele</emphasis>
</taxonomicName>
