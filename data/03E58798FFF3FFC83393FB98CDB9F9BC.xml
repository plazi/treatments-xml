<taxonomicName id="4C4C4D0DFFF3FFC8372AFABBC93BFA9C" box="[1070,1305,1337,1361]" class="Demospongiae" family="Spongiidae" genus="Hyattella" kingdom="Animalia" order="Dictyoceratida" pageId="12" pageNumber="13" phylum="Porifera" rank="species" species="intestinalis">
