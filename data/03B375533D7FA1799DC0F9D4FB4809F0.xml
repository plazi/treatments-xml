</emphasis>
<taxonomicName id="4C1ABFC63D7FA1799C4EF91DFE0909B8" authorityName="Hatch" authorityYear="1927" box="[281,442,1690,1714]" class="Insecta" family="Leiodidae" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="subFamily" subFamily="Catopocerinae">Catopocerinae</taxonomicName>
