<paragraph id="8BABBD1EFF90FFD4FF1BFE1FFE43FE7C" blockId="1.[151,399,405,432]" box="[151,399,405,432]" pageId="1" pageNumber="422">
<heading id="D0E30A72FF90FFD4FF1BFE1FFE43FE7C" bold="true" box="[151,399,405,432]" fontSize="11" level="1" pageId="1" pageNumber="422" reason="1">
<emphasis id="B960610CFF90FFD4FF1BFE1FFE43FE7C" bold="true" box="[151,399,405,432]" pageId="1" pageNumber="422">
<taxonomicName id="4C14C69DFF90FFD4FF1BFE1FFEF1FE7C" authority="Mound &amp; Wells, 2020" authorityName="Mound &amp; Wells" authorityYear="2020" box="[151,317,405,432]" class="Insecta" family="Phlaeothripidae" genus="Drypetothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="422" phylum="Arthropoda" rank="genus" status="gen.n.">
<emphasis id="B960610CFF90FFD4FF1BFE1FFEF1FE7C" bold="true" box="[151,317,405,432]" italics="true" pageId="1" pageNumber="422">Drypetothrips</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A253DC77FF90FFD4FEC9FE1CFE43FE7C" box="[325,399,406,432]" pageId="1" pageNumber="422" rank="genus">gen.n.</taxonomicNameLabel>
