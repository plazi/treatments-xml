<collectingCountry id="68D876452603000BAF13B05DFC5EEC80" box="[782,932,618,642]" name="Mozambique" pageId="3" pageNumber="174">Mozambique</collectingCountry>
(23ss47¾ 
<collectionCode id="76DEAE102603000BA81EB05DFBE9EC80" box="[1027,1043,618,642]" country="Sweden" lsid="urn:lsid:biocol.org:col:15668" name="Department of Botany, Swedish Museum of Natural History" pageId="3" pageNumber="174" type="Herbarium">S</collectionCode>
, 35ss22¾ 
<collectionCode id="76DEAE102603000BACBEB0BDFF4FECA3" box="[163,181,650,673]" country="United Kingdom" lsid="urn:lsid:biocol.org:col:15670" name="Royal Botanic Garden Edinburgh" pageId="3" pageNumber="174" type="Herbarium">E</collectionCode>
