<collectionCode id="76DEAE102603000BACE1B0BDFEF0ECA3" box="[252,266,650,673]" country="South Africa" lsid="urn:lsid:biocol.org:col:14647" name="University of the Witwatersrand" pageId="3" pageNumber="174" type="Herbarium">J</collectionCode>
. Marshall, 
<date id="647110152603000BAD92B0BDFDBBECA0" box="[399,577,650,674]" pageId="3" pageNumber="174" value="1997-12">
<collectingDate id="7435E9FD2603000BAD92B0BDFDBBECA0" box="[399,577,650,674]" pageId="3" pageNumber="174" value="1997-12">December 1997</collectingDate>
