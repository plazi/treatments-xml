(n= 
<number id="3050585FFFD15866FB49FB0BFA97FB1C" box="[1277,1290,1178,1202]" pageId="16" pageNumber="17" value="3.0">3</number>
, µ= 
<number id="3050585FFFD15866FA83FB0BFAC6FB1C" box="[1335,1371,1178,1202]" pageId="16" pageNumber="17" value="1.2">1.2</number>
± 
<number id="3050585FFFD15866FADEFB0BFA12FB1C" box="[1386,1423,1178,1202]" pageId="16" pageNumber="17" value="0.5">0.5</number>
), lumen diameter from 
<quantity id="4160E79DFFD15866FE3EFB2FFE7DFB78" box="[394,480,1214,1238]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="16" pageNumber="17" unit="mm" value="0.3">0.3 mm</quantity>
to 
<quantity id="4160E79DFFD15866FDB7FB2FFDC4FB78" box="[515,601,1214,1238]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="16" pageNumber="17" unit="mm" value="1.3">1.3 mm</quantity>
(n= 
<number id="3050585FFFD15866FD3EFB2FFD0AFB78" box="[650,663,1214,1238]" pageId="16" pageNumber="17" value="3.0">3</number>
, µ= 
<number id="3050585FFFD15866FD77FB2FFD75FB78" box="[707,744,1214,1238]" pageId="16" pageNumber="17" value="0.8">0.8</number>
