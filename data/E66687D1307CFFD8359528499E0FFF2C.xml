</emphasis>
<taxonomicName id="A9CF4D44307CFFD93442289C994EF918" authority="Keifer, 1966 a: 1 - 2" authorityName="Keifer" authorityYear="1966" box="[320,867,1724,1750]" class="Arachnida" family="Eriophyidae" genus="Echinacrus" kingdom="Animalia" order="Trombidiformes" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="rhamnicroceae">
