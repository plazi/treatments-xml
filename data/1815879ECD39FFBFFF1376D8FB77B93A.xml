species: 
<taxonomicName id="57BC4D0BCD39FFBCFEA2777CFC67BF89" authority="Vercammen-Grandjean and Taufflieb, 1959" authorityName="Vercammen-Grandjean and Taufflieb" authorityYear="1959" box="[294,1010,1910,1933]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="galliardi">
<emphasis id="A2C8EA9ACD39FFBCFEA2777CFDA6BF88" box="[294,563,1911,1933]" italics="true" pageId="69" pageNumber="70">Leptotrombidium galliardi</emphasis>
