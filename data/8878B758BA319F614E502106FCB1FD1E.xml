<taxonomicName id="C7D17DCDBA319F6F4D9B240EFC44FBDD" authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[456,957,1027,1054]" class="Bivalvia" family="Cardiidae" genus="Acanthocardia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="71" pageNumber="72" phylum="Mollusca" rank="species" species="echinata">
