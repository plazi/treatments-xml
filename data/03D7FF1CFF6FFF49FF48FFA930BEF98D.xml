records 
<taxonomicName id="4C7E3589FF6FFF49FDE6FD6A32E9F9EC" authorityName="Mannerheim" authorityYear="1829" box="[622,917,1540,1564]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="164" pageNumber="163" phylum="Arthropoda" rank="species" species="atricapilla">
