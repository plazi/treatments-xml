<taxonomicName id="4C7E3589FF6FFF49FCC1FED23512FA24" authorityName="Mannerheim" authorityYear="1829" box="[841,1134,1468,1492]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="164" pageNumber="163" phylum="Arthropoda" rank="species" species="atricapilla">
