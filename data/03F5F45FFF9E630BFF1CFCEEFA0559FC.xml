<typeStatus id="54E7FBEBFF9E630CFF1CFCB4FF5458C2" box="[192,249,889,915]" pageId="3" pageNumber="2">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4C5C3ECAFF9E630CFE83FCB4FDDD58C2" authority="Muona, 2022" authorityName="Muona" authorityYear="2022" box="[351,624,889,915]" class="Insecta" family="Eucnemidae" genus="Neusiokia" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="species" species="appalachiensis" status="new species">
<emphasis id="B928995BFF9E630CFE83FCB4FDDD58C2" box="[351,624,889,915]" italics="true" pageId="3" pageNumber="2">Neusiokia appalachiensis</emphasis>
</taxonomicName>
