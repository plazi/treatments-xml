(4); 
<taxonomicName id="4C6669EF2E59FF91FF7DF807FEE74884" authority="Jałoszyński, 2018" authorityName="Jałoszyński" authorityYear="2018" box="[220,345,1959,1981]" class="Insecta" family="Staphylinidae" genus="Paraneseuthia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="269" phylum="Arthropoda" rank="species" species="ehimensis" status="sp. n.">
