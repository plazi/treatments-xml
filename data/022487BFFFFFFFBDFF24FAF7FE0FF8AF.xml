<taxonomicName id="4D8D4D2AFFFFFFBDFF24FAF7FDD7FAD2" authority="Bartsch, 1986" authorityName="Bartsch" authorityYear="1986" box="[151,609,1302,1329]" class="Arachnida" family="Halacaridae" genus="Scaptognathus" kingdom="Animalia" order="Trombidiformes" pageId="34" pageNumber="235" phylum="Arthropoda" rank="species" species="tereninus">
<emphasis id="B8F9EABBFFFFFFBDFF24FAF7FDD7FAD2" bold="true" box="[151,609,1302,1329]" pageId="34" pageNumber="235">
<emphasis id="B8F9EABBFFFFFFBDFF24FAF7FE00FAD2" bold="true" box="[151,438,1302,1329]" italics="true" pageId="34" pageNumber="235">Scaptognathus tereninus</emphasis>
