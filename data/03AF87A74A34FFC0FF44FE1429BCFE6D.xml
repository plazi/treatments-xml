<quantity id="4CFE9B544A34FFC0FF7BFD1128B0FB55" box="[168,209,762,786]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="7" pageNumber="494" unit="in" value="5.0">5 in</quantity>
