<taxonomicName id="5BD639BEFA14FFDB31B592A4B3B4FA1A" authorityName="RODRIGUES &amp; MACHADO PELLEGRINO &amp; DIXO &amp; VERDADE &amp; PAVAN &amp; SUZART ARGOLO &amp; SITES" authorityYear="2007" box="[178,440,1491,1512]" class="Reptilia" family="Gymnophthalmidae" genus="Alexandresaurus" kingdom="Animalia" order="Squamata" pageId="15" pageNumber="16" phylum="Chordata" rank="species" species="camacan">
<emphasis id="AEA29E2FFA14FFDB31B592A4B3B4FA1A" box="[178,440,1491,1512]" italics="true" pageId="15" pageNumber="16">Alexandresaurus camacan</emphasis>
