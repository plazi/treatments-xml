<taxonomicName id="72CA8F8A9E71C54AA119FF4D57414982" box="[639,796,152,176]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="402" phylum="Arthropoda" rank="species" species="jiulingense">
