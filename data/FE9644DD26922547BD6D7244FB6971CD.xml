6. 
<taxonomicName id="3E9AD02D9219A5B456DBE552BCF332E5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:139502" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" lsidName="Ponera mutabilis Smith" lsidName-HNS="Ponera mutabilis Smith" order="Hymenoptera" pageNumber="45" phylum="Arthropoda" rank="species" species="mutabilis">Ponera mutabilis</taxonomicName>
