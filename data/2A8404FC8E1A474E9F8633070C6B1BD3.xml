<taxonomicName id="4A6123ED44D0C7CD2618C4E7ACF0978A" authorityName="H. Rob. &amp; B. Kahn" authorityYear="1986" baseAuthorityName="Sond." class="Dictyochophyceae" family="Asteraceae" genus="Distephanus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Distephanus anisochaetoides" order="Asterales" pageId="13" pageNumber="62" phylum="Ochrophyta" rank="species" species="anisochaetoides">
