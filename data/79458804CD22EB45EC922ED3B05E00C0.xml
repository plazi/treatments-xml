<taxonomicName id="352337BD8FBB62CCC85A3CB84BB166A5" class="Insecta" family="Cerambycidae" genus="Neostenus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neostenus" order="Coleoptera" pageId="207" pageNumber="208" phylum="Arthropoda" rank="genus">Neostenus</taxonomicName>
