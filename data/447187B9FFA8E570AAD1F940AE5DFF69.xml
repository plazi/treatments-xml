<subSubSection id="84C26524FFA8E573AAD1F8F0A9C9F89C" box="[151,1021,1835,1862]" pageId="52" pageNumber="53" type="diagnosis">
<paragraph id="CC6736AFFFA8E573AAD1F8F0A9C9F89C" blockId="52.[151,1437,1835,2042]" box="[151,1021,1835,1862]" pageId="52" pageNumber="53">
