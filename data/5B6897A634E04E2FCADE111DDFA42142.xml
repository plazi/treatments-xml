<paragraph id="3A7B6A84C9BA6897BEF7ECF55F89320E" pageId="767" pageNumber="768">
<taxonomicName id="1E4E65A98534FFE04AE2358EAF439CBD" authorityName="Casey" authorityYear="1920" class="Hexapoda" family="Carabidae" genus="Agonum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="767" pageNumber="768" phylum="Arthropoda" rank="species" species="lacustre">Agonum lacustre</taxonomicName>
