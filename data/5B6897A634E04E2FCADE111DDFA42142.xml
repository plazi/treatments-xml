<taxonomicName id="CF882B479E0D6BE3FC889753E8E9F884" baseAuthorityName="LeConte" baseAuthorityYear="1854" class="Hexapoda" family="Carabidae" genus="Agonum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="767" pageNumber="768" phylum="Arthropoda" rank="species" species="metallescens">
