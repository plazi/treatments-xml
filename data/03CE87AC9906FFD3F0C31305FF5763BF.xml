<taxonomicName id="4C674D399906FFD3F0BA1302FE7D601F" authority="Cook" authorityName="Cook" box="[238,451,1355,1382]" class="Diplopoda" family="Cleidogonidae" genus="Cleidogona" kingdom="Animalia" order="Chordeumatida" pageId="2" pageNumber="89" phylum="Arthropoda" rank="genus">
<emphasis id="B913EAA89906FFD3F0BA1302FEC7601C" bold="true" box="[238,377,1356,1382]" italics="true" pageId="2" pageNumber="89">Cleidogona</emphasis>
