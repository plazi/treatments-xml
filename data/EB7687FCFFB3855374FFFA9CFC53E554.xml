<taxonomicName id="A4DF4D69FFBC8553740CFA67FC25E7CA" authorityName="Joseph &amp; Framenau" authorityYear="2012" baseAuthorityName="BERLAND" baseAuthorityYear="1924" box="[826,1023,1500,1522]" class="Arachnida" family="Araneidae" genus="Plebs" kingdom="Animalia" order="Araneae" pageId="41" pageNumber="320" phylum="Arthropoda" rank="species" species="neocaledonicus">
