<bibRefCitation id="074E4B1BFFBC85537485FA04FBB9E7EF" author="Berland L" box="[947,1123,1470,1492]" pageId="41" pageNumber="320" pagination="121 - 190" refId="ref35622" refString="Berland L. 1938. Araignees des Nouvelles-Hebrides. Annales de la Societe Entomologique de France 107: 121 - 190." type="journal article" year="1938">Berland (1938)</bibRefCitation>
misidentified 
<taxonomicName id="A4DF4D69FFBC85537224FA04FAA0E7E8" authorityName="Joseph &amp; Framenau" authorityYear="2012" box="[1298,1402,1470,1491]" class="Arachnida" family="Araneidae" genus="Plebs" kingdom="Animalia" order="Araneae" pageId="41" pageNumber="320" phylum="Arthropoda" rank="species" species="opacus">
