<quantity id="70009B36D342E001F4E6FD98C92AC752" box="[1196,1281,548,573]" metricMagnitude="3" metricUnit="m" metricValue="2.544" pageId="5" pageNumber="81" unit="m" value="2544.0">2544 m</quantity>
