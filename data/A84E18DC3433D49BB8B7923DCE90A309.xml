de 
<normalizedToken id="617060A9ED51CC65C076267C0C7B69EA" originalValue="Saül">Sauel</normalizedToken>
, point de vue, 
<geoCoordinate id="5EC16ECE044BF2656F2261E9478CA8F2" direction="north" orientation="latitude" precision="15" value="3.0227778">3°1'22&quot;N</geoCoordinate>
