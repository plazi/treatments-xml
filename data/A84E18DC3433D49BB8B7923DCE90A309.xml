<geoCoordinate id="D8B197255328F2C82F96D48C7AEF6C4E" direction="west" orientation="longitude" precision="15" value="-53.209446">53°12'34&quot;W</geoCoordinate>
, 
<normalizedToken id="DDEA902D66156B234202F4AB4CB45620" originalValue="piège">piege</normalizedToken>
vitre, 31 Nov 2010, SEAG&quot; / &quot;Caterino/Tishechkin Exosternini Voucher EXO-01295&quot; / &quot;HOLOTYPE 
<taxonomicName id="E14F06A1E55722B2AF074E9ED51AD8BC" class="Insecta" family="Histeridae" genus="Mecistostethus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mecistostethus seagorum" order="Coleoptera" pageId="4" pageNumber="67" phylum="Arthropoda" rank="species" species="seagorum">Mecistostethus seagorum</taxonomicName>
