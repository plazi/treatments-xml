<taxonomicName id="A71B5EAC3E072087ABAA9B7A82906DD5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142378" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole (Elasmopheidole) laticrista Santschi" lsidName-HNS="Pheidole (Elasmopheidole) laticrista Santschi" order="Hymenoptera" pageNumber="134" phylum="Arthropoda" rank="species" species="laticrista" subGenus="Elasmopheidole">Pheidole (Elasmopheidole) laticrista</taxonomicName>
