<taxonomicName id="06F04D3AFFF1332BFDD9FEA85E05A359" authority="Bagdasarian" authorityName="Bagdasarian" authorityYear="1976" box="[627,860,284,310]" class="Arachnida" family="Diptilomiopidae" genus="Trimeroptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="rubi">
