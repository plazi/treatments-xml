<taxonomicName id="06F04D3AFFF1332BFD99FEF05EE2A331" authority="Xue &amp; Hong" authorityName="Xue &amp; Song &amp; Hong" authorityYear="2006" box="[563,955,324,350]" class="Arachnida" family="Diptilomiopidae" genus="Diptacus" kingdom="Animalia" order="Prostigmata" pageId="3" pageNumber="60" phylum="Arthropoda" rank="species" species="luanchuanensis">
