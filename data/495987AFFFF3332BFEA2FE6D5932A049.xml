<taxonomicName id="06F04D3AFFF0332BFB2EF88E5C3CA359" authority="(Pursh.)" authorityName="Pursh." baseAuthorityName="Pursh." class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" lastPageId="3" lastPageNumber="60" order="Aquifoliales" pageId="2" pageNumber="59" phylum="Tracheophyta" rank="species" species="laevigata">
