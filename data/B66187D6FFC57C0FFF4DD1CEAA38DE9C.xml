by 
<bibRefCitation id="5A594B31FFC57C0FFE51D598AD0CDE9D" author="Sakai" box="[395,541,1230,1255]" pageId="3" pageNumber="232" refString="Sakai, K. (2011) Axioidea of the world and a reconsideration of the Callianassoidea (Decapoda, Thalassinidea, Callianassida). Crustaceana Monographs, 13, 1 - 616." type="journal article" year="2011">
Sakai (
<number id="880024E7FFC57C0FFE03D599AD04DE9D" box="[473,533,1231,1255]" pageId="3" pageNumber="232" value="2011.0">2011</number>
)
</bibRefCitation>
. 
<bibRefCitation id="5A594B31FFC57C0FFDF2D598ADE8DE9D" author="Dworschak" box="[552,761,1230,1255]" pageId="3" pageNumber="232" refString="Dworschak, P. C. (2014) The Axiidea (Crustacea, Decapoda) of Cocos (Keeling) and Christmas Islands, with description of a new species of Eucalliax Manning &amp; Felder, 1991. Raffles Bulletin of Zoology Supplement, 30, 230 - 245." type="journal article" year="2014">
Dworschak (
<number id="880024E7FFC57C0FFD6CD599ADE0DE9D" box="[694,753,1231,1255]" pageId="3" pageNumber="232" value="2014.0">2014</number>
)
</bibRefCitation>
synonymised 
<taxonomicName id="F9C84D43FFC57C0FFC41D599AB83DE9D" authority="Sakai, 2011" authorityName="Sakai" authorityYear="2011" box="[923,1170,1230,1255]" class="Malacostraca" family="Callianideidae" genus="Callianidea" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="232" phylum="Arthropoda" rank="species" species="ryukyu">
