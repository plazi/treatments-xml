<taxonomicName id="F9C84D43FFC57C0FFA58D5FDAE70DE9D" class="Malacostraca" family="Callianideidae" genus="Callianidea" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="232" phylum="Arthropoda" rank="species" species="madagascariensis">
