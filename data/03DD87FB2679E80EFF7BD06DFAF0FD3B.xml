], III (
<date id="FFCA102D267AE80EFC45D496FBB5FF7B" box="[937,1077,151,176]" pageId="7" pageNumber="578" value="2015-01-01" valueMax="2015-03-02" valueMin="2015-01-01">
<number id="3DBC24CA267AE80EFC45D496FC52FF7B" box="[937,978,151,176]" pageId="7" pageNumber="578" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
