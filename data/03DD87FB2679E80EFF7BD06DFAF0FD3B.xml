– 
<number id="3DBC24CA267AE80EFE65D496FE33FF7B" box="[393,435,151,176]" pageId="7" pageNumber="578" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
– 
<number id="3DBC24CA267AE80EFE28D496FE61FF7B" box="[452,481,151,176]" pageId="7" pageNumber="578" value="20.0">20</number>
</date>
) [
<number id="3DBC24CA267AE80EFE14D499FDA2FF7B" box="[504,546,152,176]" pageId="7" pageNumber="578" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
<number id="3DBC24CA267AE80EFDD8D499FDC3FF7B" box="[564,579,152,176]" pageId="7" pageNumber="578" value="2.0">2</number>
], II (
<date id="FFCA102D267AE80EFD95D496FC87FF7B" box="[633,775,151,176]" pageId="7" pageNumber="578" value="2015-03-01" valueMax="2015-04-04" valueMin="2015-03-01">
<number id="3DBC24CA267AE80EFD95D496FD20FF7B" box="[633,672,151,176]" pageId="7" pageNumber="578" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
– 
<number id="3DBC24CA267AE80EFD5DD496FD5CFF7B" box="[689,732,151,176]" pageId="7" pageNumber="578" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
– 
<number id="3DBC24CA267AE80EFD01D496FC87FF7B" box="[749,775,151,176]" pageId="7" pageNumber="578" value="15.0">15</number>
</date>
) [
<number id="3DBC24CA267AE80EFCF2D499FCA8FF7B" box="[798,808,152,176]" pageId="7" pageNumber="578" value="1.0">1</number>
