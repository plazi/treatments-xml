– 
<number id="3DBC24CA267AE80EFBF6D496FBB5FF7B" box="[1050,1077,151,176]" pageId="7" pageNumber="578" value="15.0">15</number>
</date>
) [
<number id="3DBC24CA267AE80EFBA2D499FBD8FF7B" box="[1102,1112,152,176]" pageId="7" pageNumber="578" value="1.0">1</number>
– 
<number id="3DBC24CA267AE80EFB87D499FBF5FF7B" box="[1131,1141,152,176]" pageId="7" pageNumber="578" value="1.0">1</number>
–0], IV (
<date id="FFCA102D267AE80EFB36D496FAE7FF7B" box="[1242,1383,151,176]" pageId="7" pageNumber="578" value="2012-02-01" valueMax="2012-03-02" valueMin="2012-02-01">
<number id="3DBC24CA267AE80EFB36D496FA80FF7B" box="[1242,1280,151,176]" pageId="7" pageNumber="578" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
– 
