<date id="8B1F66ACB840E57E61CCF8256053F887" box="[682,831,1981,2008]" pageId="98" pageNumber="99" value="1962-06-25">25 June 1962</date>
(
<collectionCode id="99B0D8A9B840E57E6028F82660EEF887" box="[846,898,1982,2008]" pageId="98" pageNumber="99">RJL</collectionCode>
). Recorded from 
<collectingRegion id="3D658E8EB840E57E6725F82667DDF887" box="[1091,1201,1982,2008]" country="United States of America" name="Wyoming" pageId="98" pageNumber="99">Wyoming</collectingRegion>
