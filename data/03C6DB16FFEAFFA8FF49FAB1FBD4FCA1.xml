<taxonomicName id="4C6F1183FFE9FFA8FE1DFE2AFD39FE7C" box="[452,693,390,416]" class="Magnoliopsida" family="Boraginaceae" genus="Symphytum" kingdom="Plantae" order="Boraginales" pageId="2" pageNumber="21" phylum="Tracheophyta" rank="species" species="officinale">
<emphasis id="B91BB612FFE9FFA8FE1DFE2AFD39FE7C" box="[452,693,390,416]" italics="true" pageId="2" pageNumber="21">Symphytum officinale</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6F1183FFE9FFA8FD1AFE2AFC31FE7C" box="[707,957,390,416]" class="Magnoliopsida" family="Boraginaceae" genus="Pulmonaria" kingdom="Plantae" order="Boraginales" pageId="2" pageNumber="21" phylum="Tracheophyta" rank="species" species="officinalis">
<emphasis id="B91BB612FFE9FFA8FD1AFE2AFC31FE7C" box="[707,957,390,416]" italics="true" pageId="2" pageNumber="21">Pulmonaria officinalis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C6F1183FFE9FFA8FC15FE2AFF70FE1E" class="Magnoliopsida" family="Boraginaceae" genus="Lithospermum" kingdom="Plantae" order="Boraginales" pageId="2" pageNumber="21" phylum="Tracheophyta" rank="species" species="officinale">
