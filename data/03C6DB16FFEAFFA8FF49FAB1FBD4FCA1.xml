<taxonomicName id="4C6F1183FFE9FFA8FDD6FE05FDFAFE1E" box="[527,630,425,450]" class="Magnoliopsida" family="Boraginaceae" genus="Myosotis" kingdom="Plantae" order="Boraginales" pageId="2" pageNumber="21" phylum="Tracheophyta" rank="genus">
