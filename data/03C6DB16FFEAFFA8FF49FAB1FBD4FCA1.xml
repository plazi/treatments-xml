<quantity id="4C97C7E5FFEAFFABFDCDFA09FD00FA1C" box="[532,652,1445,1472]" metricMagnitude="-2" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="1" pageNumber="20" unit="mm" value="16.5" valueMax="18.0" valueMin="15.0">15–18 mm</quantity>
