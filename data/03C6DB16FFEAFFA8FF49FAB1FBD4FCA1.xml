<emphasis id="B91BB612FFE9FFA8FD7AFE8CFBEBFEE6" box="[675,1127,288,314]" italics="true" pageId="2" pageNumber="21">
<taxonomicName id="4C6F1183FFE9FFA8FD7AFE8CFCBCFEE6" box="[675,816,288,314]" class="Magnoliopsida" family="Boraginaceae" genus="Pulmonaria" kingdom="Plantae" order="Boraginales" pageId="2" pageNumber="21" phylum="Tracheophyta" rank="genus">Pulmonaria</taxonomicName>
, 
<taxonomicName id="4C6F1183FFE9FFA8FC9AFE8CFC61FEE6" box="[835,1005,288,314]" class="Magnoliopsida" family="Boraginaceae" genus="Lithospermum" kingdom="Plantae" order="Boraginales" pageId="2" pageNumber="21" phylum="Tracheophyta" rank="genus">Lithospermum</taxonomicName>
, 
