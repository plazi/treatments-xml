<taxonomicName id="4C7E3589FF6FFF49FC5CF870347EFCC6" authorityName="H. W. Bates" authorityYear="1888" box="[980,1282,798,822]" class="Insecta" family="Dynastidae" genus="Cyclocephala" kingdom="Animalia" order="Coleoptera" pageId="164" pageNumber="163" phylum="Arthropoda" rank="species" species="amblyopsis">
<emphasis id="B90A9218FF6FFF49FC5CF870347EFCC6" box="[980,1282,798,822]" italics="true" pageId="164" pageNumber="163">Cyclocephala amblyopsis</emphasis>
</taxonomicName>
from 
<collectingCountry id="F3690E9AFF6FFF49FAC1F8703062FCA6" name="French Guiana" pageId="164" pageNumber="163">French Guiana</collectingCountry>
