<taxonomicName id="7F8A7B9D4340F1FFE15DC84C81245E54" class="Eudicots" family="Asteraceae" genus="Arctium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Arctium" order="Asterales" pageId="0" pageNumber="1" phylum="Magnoliophyta" rank="genus">
<emphasis id="B67CA747BA2CD517E0BC2FC0AB52F472" italics="true" pageId="0" pageNumber="1">Arctium</emphasis>
</taxonomicName>
, 
<taxonomicName id="D3F492EAEDBE7C60148E0D63D0AA749B" class="Eudicots" family="Asteraceae" genus="Carduus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Carduus" order="Asterales" pageId="0" pageNumber="1" phylum="Magnoliophyta" rank="genus">
<emphasis id="79B2B08604512547C72277A4B70BAE0E" italics="true" pageId="0" pageNumber="1">Carduus</emphasis>
