<taxonomicName id="4543FFD13347965A2AE60907A10049A5" authorityName="Jager" authorityYear="1999" class="Arachnida" family="Sparassidae" genus="Sinopoda" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinopoda" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
<emphasis id="30D35DCF913538D6D4C6BC038CCB0C04" italics="true" pageId="0" pageNumber="1">Sinopoda</emphasis>
</taxonomicName>
: 
<emphasis id="3A27004941F59DBE153153C57800377C" bold="true" pageId="0" pageNumber="1">1</emphasis>
<taxonomicName id="673AB9CACD53CCA1ED00925D254545F4" genus="S." lsidName="S. hongruii" pageId="0" pageNumber="1" rank="species" species="hongruii">
<emphasis id="650B4ADF1A7C20ECAD2B7614570BCC85" italics="true" pageId="0" pageNumber="1">S. hongruii</emphasis>
</taxonomicName>
sp. nov. (Anhui, China) 
