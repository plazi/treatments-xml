<taxonomicName id="4C504D708C4DFFCBBE167AB6FAE9CFB8" authority="Fahri &amp; Amaliah &amp; Annawaty &amp; Nguyen, 2017" authorityName="Fahri &amp; Amaliah &amp; Annawaty &amp; Nguyen" authorityYear="2017" box="[1067,1285,189,210]" class="Clitellata" family="Megascolecidae" genus="Polypheretima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="4" pageNumber="563" phylum="Annelida" rank="species" species="sahlani" status="new species">
<emphasis id="B924EAE18C4DFFCBBE167AB6FAE9CFB8" box="[1067,1285,189,210]" italics="true" pageId="4" pageNumber="563">Polypheretima sahlani</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A217579A8C4DFFCBBF2F7AB5FA66CFB9" box="[1298,1418,189,211]" pageId="4" pageNumber="563" rank="species">new species</taxonomicNameLabel>
.
</paragraph>
