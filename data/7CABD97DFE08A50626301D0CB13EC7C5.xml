<location id="B1A477F68A63E89371BB632EE18D30D3" LSID="urn:lsid:plazi:treatment:7CABD97DFE08A50626301D0CB13EC7C5:B1A477F68A63E89371BB632EE18D30D3" country="Germany" elevation="0 m" latitude="53.7820" longLatPrecision="600" longitude="7.9380" name="Vogelschutzgebiet-Ost" stateProvince="Wangerooge">Vogelschutzgebiet-Ost</location>
