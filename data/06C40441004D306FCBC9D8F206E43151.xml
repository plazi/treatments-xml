[[ Genus 
<taxonomicName id="8103456EC27A2D3D9CAFF7D300848689" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24785" class="Insecta" family="Formicidae" genus="Myrmelachista" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Myrmelachista Roger" order="Hymenoptera" pageNumber="49" phylum="Arthropoda" rank="genus">Myrmelachista</taxonomicName>
