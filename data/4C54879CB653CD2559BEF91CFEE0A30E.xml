series collected near 
<location id="C1226051B64DCD255D63F8F1FAD5A36A" LSID="urn:lsid:plazi:treatment:4C54879CB653CD2559BEF91CFEE0A30E:C1226051B64DCD255D63F8F1FAD5A36A" box="[1098,1312,1809,1835]" country="Peru" name="Puerto Maldonado" pageId="16" pageNumber="17" stateProvince="Madre de Dios">Puerto Maldonado</location>
, 
<collectingRegion id="0639F868B64DCD255C07F8F1FF3AA30E" country="Peru" name="Madre de Dios" pageId="16" pageNumber="17">Madre de Dios</collectingRegion>
, 
<collectingCountry id="BCEA761AB64DCD2559F3F8D5FEE4A30E" box="[218,273,1845,1871]" name="Peru" pageId="16" pageNumber="17">Peru</collectingCountry>
</materialsCitation>
.
</paragraph>
</subSubSection>
