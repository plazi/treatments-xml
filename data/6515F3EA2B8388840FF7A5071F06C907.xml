<taxonomicName id="A4F1C9B31E587A7BF1C8A7AAB354E0F5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:37245" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Tetramorium syriacum Emery" order="Hymenoptera" pageNumber="35" phylum="Arthropoda" rank="species" species="syriacum">T. syriacum</taxonomicName>
