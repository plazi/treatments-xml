<taxonomicName id="3EDFD1FC3D65D1F29A413E25CE4BA947" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:25179" class="Insecta" family="Formicidae" genus="Amblyopone" kingdom="Animalia" lsidName-HNS="Amblyopone ferruginea Smith" order="Hymenoptera" pageId="110" pageNumber="110" phylum="Arthropoda" rank="species" species="ferruginea">Amblyopone ferruginea</taxonomicName>
