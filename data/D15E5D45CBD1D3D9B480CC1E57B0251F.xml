<taxonomicName id="EF5469EA07B6C8A11F8887DD7084CCB2" LSID="http://zoobank.org/C7493C8C-E242-4EC9-BD39-9BC4C4C579F9" authority="Davis &amp; Engel" authorityName="Davis &amp; Engel" authorityYear="2014" class="Insecta" family="Nemonychidae" genus="Burmonyx" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Burmonyx" order="Coleoptera" pageId="1" pageNumber="128" phylum="Arthropoda" rank="genus">Burmonyx Davis &amp; Engel</taxonomicName>
