<taxonomicName id="FB9D4F884BFE4BB18BB7C3D855C9CFB1" authority="Konow, 1902" authorityName="Konow" authorityYear="1902" class="Insecta" family="Magnoliaceae" genus="Cladiucha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cladiucha insolita" order="Hymenoptera" pageId="3" pageNumber="176" phylum="Arthropoda" rank="species" species="insolita">
