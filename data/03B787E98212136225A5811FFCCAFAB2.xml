<taxonomicName id="4C1E4D7C8212136225A581F0FE83FA97" authority="Moradmand, 2013" authorityName="Moradmand" authorityYear="2013" box="[151,305,1267,1291]" class="Arachnida" family="Sparassidae" genus="Eusparassus" kingdom="Animalia" order="Araneae" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="borakalalo" status="spec. nov.">
