<taxonomicName id="4C38F3EDFFE9FFCDFEABBFAC630EF864" baseAuthorityName="Rambur" baseAuthorityYear="1842" box="[294,516,1947,1971]" class="Insecta" family="Libellulidae" genus="Agrionoptera" kingdom="Animalia" order="Odonata" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="insignis">
