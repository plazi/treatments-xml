<taxonomicName id="9DECD1996E04205F775E61DD41329E79" class="Reptilia" family="Pythonidae" genus="Aspidites" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aspidites melanocephalus subsp. davieii" order="Squamata" pageId="8" pageNumber="37" phylum="Chordata" rank="subSpecies" species="melanocephalus" subSpecies="davieii">Aspidites melanocephalus davieii</taxonomicName>
