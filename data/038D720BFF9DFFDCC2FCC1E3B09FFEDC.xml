<collectingCountry id="F333838DFF9EFFDCC30CC438B3D2FEDC" box="[359,427,258,284]" name="Brazil" pageId="26" pageNumber="27">Brazil</collectingCountry>
(
<collectingRegion id="49E00DFFFF9EFFDCC3D0C438B019FEDD" box="[443,608,258,285]" country="Brazil" name="Rio de Janeiro" pageId="26" pageNumber="27">Rio de Janeiro</collectingRegion>
) (
<figureCitation id="131FDF98FF9EFFDCC01CC438B0A0FEDC" box="[631,729,258,284]" captionStart="FIGURE 184" captionStartId="35.[151,250,1449,1473]" captionTargetBox="[173,1414,545,1408]" captionTargetId="figure-171@35.[151,1436,525,1425]" captionTargetPageId="35" captionText="FIGURE 184. Occurrence records of the species of Stysiana, Luridocimex and Pedinonotus." figureDoi="http://doi.org/10.5281/zenodo.4897450" httpUri="https://zenodo.org/record/4897450/files/figure.png" pageId="26" pageNumber="27">Fig. 184</figureCitation>
).
</paragraph>
