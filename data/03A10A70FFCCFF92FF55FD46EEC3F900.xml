, 
<collectingCountry id="F31FFBF6FFCCFF92FEEEF937EDAEF903" box="[300,405,1732,1758]" name="Germany" pageId="23" pageNumber="265">Germany</collectingCountry>
, 
<collectingCountry id="F31FFBF6FFCCFF92FE63F937EDC7F903" box="[417,508,1732,1758]" name="Norway" pageId="23" pageNumber="265">Norway</collectingCountry>
, 
<collectingCountry id="F31FFBF6FFCCFF92FDC5F930EE5BF900" box="[519,608,1731,1757]" name="Sweden" pageId="23" pageNumber="265">Sweden</collectingCountry>
and 
<collectingCountry id="F31FFBF6FFCCFF92FD54F930EECFF900" box="[662,756,1731,1757]" name="Ukraine" pageId="23" pageNumber="265">Ukraine</collectingCountry>
