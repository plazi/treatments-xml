<taxonomicName id="4C5CC7FDFFC25E7FFAE3F88BFA8FF8D2" box="[1301,1390,1809,1832]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="aries">
