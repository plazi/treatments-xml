<taxonomicName id="4C5CC7FDFFC25E7FFE60F8AFFE13F8B6" box="[406,498,1845,1868]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="aries">
<emphasis id="B928606CFFC25E7FFE60F8AFFE13F8B6" box="[406,498,1845,1868]" italics="true" pageId="3">N. aries</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C5CC7FDFFC25E7FFDDBF8AFFD46F8B6" box="[557,679,1845,1868]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="spinosa">
<emphasis id="B928606CFFC25E7FFDDBF8AFFD46F8B6" box="[557,679,1845,1868]" italics="true" pageId="3">N. spinosa</emphasis>
</taxonomicName>
