<taxonomicName id="4C5CC7FDFFC25E7FFC34F977FB90F8FF" box="[962,1137,1772,1797]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="atlantica" status="n. sp.">
