<taxonomicName id="4C5CC7FDFFC25E7FFAB5F8C3FA7DF88A" box="[1347,1436,1881,1904]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="aries">
<emphasis id="B928606CFFC25E7FFAB5F8C3FA7DF88A" box="[1347,1436,1881,1904]" italics="true" pageId="3">N. aries</emphasis>
</taxonomicName>
and straight and directed outward and downward in 
<taxonomicName id="4C5CC7FDFFC25E7FFD28F8E7FCB5F86E" box="[734,852,1917,1940]" class="Malacostraca" family="Majidae" genus="Naxia" kingdom="Animalia" order="Decapoda" pageId="3" phylum="Arthropoda" rank="species" species="spinosa">
<emphasis id="B928606CFFC25E7FFD28F8E7FCB5F86E" box="[734,852,1917,1940]" italics="true" pageId="3">N. spinosa</emphasis>
</taxonomicName>
