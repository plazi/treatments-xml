<paragraph id="86725D304BD0E365FC28DC76244CE834" pageId="15" pageNumber="34">
<taxonomicName id="B9BD9262266B5562A2C1904E2A4E3F45" authorityName="Mabille" authorityYear="1887" class="Gastropoda" family="Pupinidae" genus="Hybocystis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Architaenioglossa" pageId="15" pageNumber="34" phylum="Mollusca" rank="species" species="rochebruni">Hybocystis rochebruni</taxonomicName>
