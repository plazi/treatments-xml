<paragraph id="3024732CED48DAD88C2D15369A703D35" pageId="15" pageNumber="34">
<taxonomicName id="057D87BE7B3F0F66E1A45807C62E738A" baseAuthorityName="Mabille" baseAuthorityYear="1887" class="Gastropoda" family="Pupinidae" genus="Pollicaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Architaenioglossa" pageId="15" pageNumber="34" phylum="Mollusca" rank="species" species="rochebruni">Pollicaria rochebruni</taxonomicName>
