<subSubSection id="0891108EFFBE2402CBD8A5E7719E229A" box="[199,1273,1195,1221]" pageId="117" pageNumber="118" type="etymology">
<paragraph id="40344305FFBE2402CBD8A5E7719E229A" blockId="117.[151,1437,151,1221]" box="[199,1273,1195,1221]" pageId="117" pageNumber="118">
<emphasis id="72FF9F17FFBE2402CBD8A5E77429229A" bold="true" box="[199,334,1195,1221]" pageId="117" pageNumber="118">Etymology.</emphasis>
