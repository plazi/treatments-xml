<heading id="D0C0070D4BD7D8C3FCF8357AB3C0F9F4" box="[847,1166,1572,1595]" centered="true" fontSize="9" level="2" pageId="131" pageNumber="132" reason="2">
<taxonomicName id="4C37CBE24BD7D8C3FCF8357AB3C0F9F4" authority="Bangs, 1900" authorityName="Bangs" authorityYear="1900" box="[847,1166,1572,1595]" class="Mammalia" family="Cricetidae" genus="Oligoryzomys" kingdom="Animalia" order="Rodentia" pageId="131" pageNumber="132" phylum="Chordata" rank="genus">
