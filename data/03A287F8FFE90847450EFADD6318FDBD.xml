<taxonomicName id="4C0B4D6DFFE90846450EFA096516FA7B" authority="Smith, 1899" authorityName="Smith" authorityYear="1899" box="[264,608,1433,1457]" class="Insecta" family="Erebidae" genus="Heteranassa" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Lepidoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="minor" status="NEW SYNONYM">
<emphasis id="B97FEAFCFFE90846450EFA0966A3FA7A" box="[264,469,1434,1457]" italics="true" pageId="6" pageNumber="7">Heteranassa minor</emphasis>
<bibRefCitation id="EF9A4B1FFFE9084645DAFA0A6516FA7B" author="Smith" box="[476,608,1433,1456]" pageId="6" pageNumber="43" refString="Smith, J. B. (1899) Some new species of Hadena. Canadian Entomologist, 31, 257 - 265." type="journal article" year="1899">
Smith, 
<number id="3DC324C9FFE90846462FFA0A6516FA7B" box="[553,608,1433,1456]" pageId="6" pageNumber="7" value="1899.0">1899</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicNameLabel id="A24C5787FFE90846466DFA0A6440FA7B" box="[619,822,1433,1456]" pageId="6" pageNumber="7">
<collectionCode id="ED1AAE2BFFE90846466DFA0A65DEFA7B" box="[619,680,1433,1456]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="6" pageNumber="7">NEW</collectionCode>
