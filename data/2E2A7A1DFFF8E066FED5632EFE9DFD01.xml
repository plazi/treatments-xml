<taxonomicName id="6183B088FFF9E066FCDD6316FC98FD8A" box="[768,849,564,589]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="raedae">
