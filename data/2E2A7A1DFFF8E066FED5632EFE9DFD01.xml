<taxonomicName id="6183B088FFF9E066FD1D63A6FCD8FD5A" box="[704,785,644,669]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="raedae">
