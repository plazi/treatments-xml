<taxonomicName id="03F11A0A74411530DA2783D83ACE9EB8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33815" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole kohli Mayr" order="Hymenoptera" pageNumber="131" phylum="Arthropoda" rank="species" species="kohli">Pheidole kohli Mayr</taxonomicName>
