<quantity id="4CBD9DF3FFA04640FDC0FD32AECE1A90" box="[590,677,657,683]" metricMagnitude="-3" metricUnit="m" metricValue="2.9" pageId="52" pageNumber="53" unit="mm" value="2.9">2.9 mm</quantity>
