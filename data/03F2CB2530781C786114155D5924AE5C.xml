<number id="3D93681430781C78638915DD5F90A8ED" box="[917,927,974,991]" pageId="3" pageNumber="4" value="2.0">2</number>
+ 
<number id="3D93681430781C7863B615DD5FBCA8ED" box="[938,947,974,991]" pageId="3" pageNumber="4" value="3.0">3</number>
