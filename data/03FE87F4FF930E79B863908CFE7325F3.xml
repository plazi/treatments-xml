<taxonomicName id="4C574D61FF940E78BAEC92AEFB2121C0" authorityName="Locker" authorityYear="2006" box="[903,1173,1686,1709]" class="Insecta" family="Cixiidae" genus="Ozoliarus" kingdom="Animalia" order="Hemiptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="nourlangiensis">
