<quantity id="1366BBC6FFEABA71FE14B483F513D504" box="[427,517,1023,1047]" metricMagnitude="3" metricUnit="m" metricValue="2.92" pageId="16" pageNumber="17" unit="m" value="2920.0">2920 m</quantity>
