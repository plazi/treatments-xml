<collectionCode id="B28F8EE6FFEABA71FAFFB4A1F28ED2E7" LSID="urn:lsid:biocol.org:col:34992" box="[1344,1432,989,1012]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="16" pageNumber="17">RMNH</collectionCode>
