<quantity id="4CCFC383FFACA172FF66DA1910C5FEE2" box="[151,250,296,320]" metricMagnitude="1" metricUnit="m" metricValue="1.6" metricValueMax="1.8" metricValueMin="1.4" pageId="18" pageNumber="169" unit="m" value="16.0" valueMax="18.0" valueMin="14.0">14â€“18 m</quantity>
(AH 
