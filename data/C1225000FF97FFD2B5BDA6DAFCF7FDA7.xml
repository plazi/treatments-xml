</emphasis>
<taxonomicName id="8E8B9A95FF97FFD3B413A691FCAEF9B9" authority="Nakanishi 1978" authorityName="Nakanishi" authorityYear="1978" box="[313,774,1517,1542]" class="Insecta" family="Ichneumonidae" genus="Eurytyloides" kingdom="Animalia" order="Hymenoptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="kusigematii">
