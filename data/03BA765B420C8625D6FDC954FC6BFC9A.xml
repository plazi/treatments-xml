<taxonomicName id="4C13BCCE420C8626D43BCC0CFC81FB9B" box="[628,818,1088,1109]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" kingdom="Plantae" order="Fagales" pageId="1" pageNumber="564" phylum="Tracheophyta" rank="species" species="betulus">
