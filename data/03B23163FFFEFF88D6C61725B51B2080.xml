</emphasis>
of 
<taxonomicName id="4C1BFBF6FFFEFF88D5C51662B52E20AC" baseAuthorityName="Mateu" baseAuthorityYear="1953" box="[916,1372,1917,1943]" class="Insecta" family="Carabidae" genus="Laemostenus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="161" phylum="Arthropoda" rank="species" species="cazorlensis" subGenus="Antisphodrus">
