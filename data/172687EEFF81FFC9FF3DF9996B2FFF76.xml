<taxonomicName id="588F4D7BFF8EFFC9FC8FFF256DECFF79" box="[805,965,224,248]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="10" pageNumber="57" phylum="Platyhelminthes" rank="species" species="elizabethae">
<emphasis id="ADFBEAEAFF8EFFC9FC8FFF256D11FF79" box="[805,824,225,248]" italics="true" pageId="10" pageNumber="57">N</emphasis>
