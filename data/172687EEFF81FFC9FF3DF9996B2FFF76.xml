<taxonomicName id="588F4D7BFF8EFFC9FBECFF256A9CFF79" box="[1094,1205,224,248]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="10" pageNumber="57" phylum="Platyhelminthes" rank="species" species="fentoni">
