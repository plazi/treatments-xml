<taxonomicNameLabel id="B6C85791FF8CFFCBFAB5F8026B72F85C" box="[1311,1371,1990,2013]" pageId="8" pageNumber="55" rank="species">n. sp.</taxonomicNameLabel>
(with a length of 
<number id="294724DFFF8EFFC9FE89FF536FA0FF2E" box="[291,393,151,175]" pageId="10" pageNumber="57" value="294.5" valueMax="301.0" valueMin="288.0">288–301</number>
). 
<taxonomicName id="588F4D7BFF8EFFC9FE0BFF5C6CB9FF31" box="[417,656,152,176]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="10" pageNumber="57" phylum="Platyhelminthes" rank="species" species="grossi" status="n. sp.">
