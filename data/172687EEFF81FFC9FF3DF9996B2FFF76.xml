<taxonomicName id="588F4D7BFF8EFFC9FBF0FF5D6AD7FF31" box="[1114,1278,152,176]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="10" pageNumber="57" phylum="Platyhelminthes" rank="species" species="elizabethae">
