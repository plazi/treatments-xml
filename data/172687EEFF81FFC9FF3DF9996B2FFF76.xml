<taxonomicName id="588F4D7BFF8CFFCBFB1FF8026B30F85C" box="[1205,1305,1990,2013]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="8" pageNumber="55" phylum="Platyhelminthes" rank="species" species="grossi" status="n. sp.">
<emphasis id="ADFBEAEAFF8CFFCBFB1FF8026B30F85C" box="[1205,1305,1990,2013]" italics="true" pageId="8" pageNumber="55">N. grossi</emphasis>
