<taxonomicName id="588F4D7BFF8CFFCBFCCDF8666DFEF838" box="[871,983,1953,1977]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="8" pageNumber="55" phylum="Platyhelminthes" rank="species" species="fentoni">
