<taxonomicName id="588F4D7BFF8CFFCBFD26F8666D07F838" box="[652,814,1953,1977]" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="8" pageNumber="55" phylum="Platyhelminthes" rank="species" species="elizabethae">
