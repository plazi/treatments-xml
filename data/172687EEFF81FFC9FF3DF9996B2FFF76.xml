<taxonomicName id="588F4D7BFF8CFFCBFACFF8666FA6F85C" class="Trematoda" family="Polystomatidae" genus="Neopolystoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polyopisthocotylea" pageId="8" pageNumber="55" phylum="Platyhelminthes" rank="species" species="elizabethae">
<emphasis id="ADFBEAEAFF8CFFCBFACFF8666FA6F85C" italics="true" pageId="8" pageNumber="55">Neopolystoma elizabethae</emphasis>
