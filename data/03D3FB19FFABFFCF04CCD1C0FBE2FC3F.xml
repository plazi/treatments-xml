<quantity id="4C82E7EAFFACFFCF0118D092FA48FE77" box="[1347,1431,432,458]" metricMagnitude="3" metricUnit="m" metricValue="1.05" pageId="5" pageNumber="68" unit="m" value="1050.0">1050m</quantity>
