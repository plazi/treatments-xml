<paragraph id="CEA2DF412297BD57B75C66635BD3C3A8" pageNumber="656">
<taxonomicName id="7F129BEEA0A21AA0A8B2344A2F4AD1DA" class="Insecta" family="Formicidae" genus="Strumigenys" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageNumber="656" phylum="Arthropoda" rank="species" species="rogeri-group">rogeri-group</taxonomicName>
</paragraph>
