<emphasis id="D1CDEA8FF802A556FF01FCE1FE8EFC20" box="[185,257,870,896]" inLineHeading="true" pageId="17" pageNumber="48" reason="1">Other</emphasis>
material.
</emphasis>
<specimenCount id="F5BFFD14F802A556FE3CFCE0FE56FC21" box="[388,473,871,897]" pageId="17" pageNumber="48" type="male">1 male</specimenCount>
, details as for 
<typeStatus id="3C02883FF802A556FD36FCE0FD7EFC21" box="[654,753,871,897]" pageId="17" pageNumber="48">holotype</typeStatus>
but 
<geoCoordinate id="868D505AF802A556FC89FCE1FC3CFC21" box="[817,947,870,897]" degrees="43" direction="south" minutes="23" orientation="latitude" pageId="17" pageNumber="48" precision="15" seconds="55" value="-43.398613">43°23'55&quot;S</geoCoordinate>
<geoCoordinate id="868D505AF802A556FC78FCE1FBDBFC21" box="[960,1108,870,897]" degrees="147" direction="east" minutes="17" orientation="longitude" pageId="17" pageNumber="48" precision="15" seconds="59" value="147.29973">147°17'59&quot;E</geoCoordinate>
(
<accessionNumber id="FCEAAB7EF802A556FF2CFC0DFE94FC04" box="[148,283,906,932]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/EM242947" pageId="17" pageNumber="48">EM242947</accessionNumber>
) ± 
<quantity id="24419B78F802A556FE82FC0DFEF8FC04" box="[314,375,906,933]" metricMagnitude="1" metricUnit="m" metricValue="2.5" pageId="17" pageNumber="48" unit="m" value="25.0">25 m</quantity>
, 
<quantity id="24419B78F802A556FE3BFC0DFE46FC04" box="[387,457,906,932]" metricMagnitude="2" metricUnit="m" metricValue="3.6" pageId="17" pageNumber="48" unit="m" value="360.0">360m</quantity>
, 
