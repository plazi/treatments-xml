<taxonomicName id="5FD8DAD70FF35C08E2AA3975ACF831A6" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" baseAuthorityName="Kriebel" baseAuthorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia grayumii" order="Myrtales" pageId="181" pageNumber="182" phylum="Tracheophyta" rank="species" species="grayumii" status="comb. nov.">
