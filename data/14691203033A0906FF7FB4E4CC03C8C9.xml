<taxonomicName id="5BC0D896033A0938FF43B54CC9BFCD03" authority="(Van Duzee)" baseAuthorityName="Van Duzee" baseAuthorityYear="1930" box="[142,510,1310,1331]" class="Insecta" family="Dolichopodidae" genus="Enlinia" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="57" phylum="Arthropoda" rank="species" species="edwardsae">
