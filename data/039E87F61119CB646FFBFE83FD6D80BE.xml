<elevation id="001AD1D3111ECB656FFBF87BFF5182AA" box="[151,225,1940,1964]" metricMagnitude="2" metricUnit="m" metricValue="1.5" pageId="44" pageNumber="45" unit="m" value="150.0">150 m</elevation>
