<taxonomicName id="4C634D02FFEBFFDF8ED9E88FFC9B1915" authorityName="Gingerich" authorityYear="1993" class="Mammalia" family="Anaptomorphidae" genus="Teilhardina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Primates" pageId="2" pageNumber="3" phylum="Chordata" rank="species" species="brandti">
