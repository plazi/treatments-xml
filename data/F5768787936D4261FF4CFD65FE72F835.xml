<specimenCount id="6BD9FD18936D4261FDDAFCD5FD9BFC52" box="[513,584,891,917]" pageId="15" pageNumber="16" type="generic">
<quantity id="BA279B74936D4261FDDAFCD5FD97FC52" box="[513,580,891,917]" metricMagnitude="1" metricUnit="m" metricValue="1.4" pageId="15" pageNumber="16" unit="m" value="14.0">14 m</quantity>
