<collectionCode id="ED27579BCB29FFCEF36DF6F34110FF37" LSID="urn:lsid:biocol.org:col:34992" box="[1041,1129,189,212]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="4" pageNumber="557">RMNH</collectionCode>
