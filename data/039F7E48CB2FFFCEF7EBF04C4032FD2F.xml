<taxonomicName id="4C36B4DDCB2FFFC8F46EF0BB46F2F8ED" box="[786,907,1781,1806]" class="Insecta" family="Cetoniidae" genus="Platysodes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="555" phylum="Arthropoda" rank="genus">
