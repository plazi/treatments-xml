<taxonomicName id="4C5AA7FBFFCCE03C56EFFE709648A6BC" box="[702,818,440,464]" class="Insecta" family="Ichneumonidae" genus="Jezarotes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="425" phylum="Arthropoda" rank="species" species="apicalis">
