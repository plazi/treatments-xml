<collectionCode id="D4F1E6FFFF8BFF83FCF3FC755E4CFBD2" box="[827,884,1023,1047]" httpUri="http://grbio.org/cool/vmfx-g997" name="Zoological Institute, Russian Academy of Sciences" pageId="4" pageNumber="544">ZISP</collectionCode>
