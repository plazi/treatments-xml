<paragraph id="0D76491EFFBCFFB5B8E58EEBFC1CFEBB" blockId="7.[149,652,1713,1737]" lastBlockId="7.[714,1247,253,330]" pageId="7" pageNumber="624">
<geoCoordinate id="68FD2FD9FFBCFFB5B8E58EEBFEC8F938" box="[149,268,1713,1737]" degrees="144" direction="west" minutes="22.8" orientation="longitude" pageId="7" pageNumber="624" precision="92" value="-144.38">144°22.8’W</geoCoordinate>
, 
<collectionCode id="6BD8D1DBFFBCFFB5B9668EEBFE97F938" box="[278,339,1713,1737]" pageId="7" pageNumber="624">RAPA</collectionCode>
