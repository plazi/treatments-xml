<taxonomicName id="E0B484B2AD085F62FF0BE04638E83FDD" authority="Belokobylskij, 1988" authorityName="Belokobylskij" authorityYear="1988" box="[189,716,639,665]" class="Insecta" family="Braconidae" genus="Rhaconotus" kingdom="Animalia" order="Hymenoptera" pageId="113" pageNumber="114" phylum="Arthropoda" rank="species" species="hexatermus">
