<quantity id="4CF79B33FFC3FFEAC1E9FA3EFC47FA17" box="[912,977,1511,1531]" metricMagnitude="3" metricUnit="m" metricValue="2.23" pageId="44" pageNumber="199" unit="m" value="2230.0">2230m</quantity>
