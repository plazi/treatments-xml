<paragraph id="8BB036D6FFC3FFEAC2EEFA15FDD1F9FA" blockId="44.[151,1059,1484,1558]" pageId="44" pageNumber="199">
<collectingRegion id="49CBF834FFC3FFEAC2EEFA15FF7DFA0C" box="[151,235,1484,1504]" country="Austria" name="Salzburg" pageId="44" pageNumber="199">Salzburg</collectingRegion>
