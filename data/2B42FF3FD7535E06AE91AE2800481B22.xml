<taxonomicName id="BB177C0B218598785BDDA042262DA305" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. taliangensis" order="Caudata" pageId="0" pageNumber="83" rank="species" species="taliangensis">
<emphasis id="17E80F95AA9D9BDC237E1C991236C153" italics="true" pageId="0" pageNumber="83">T. taliangensis</emphasis>
</taxonomicName>
); from 
<taxonomicName id="A991A2A645F169F7092E74CE464F6813" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. kweichowensis" order="Caudata" pageId="0" pageNumber="83" rank="species" species="kweichowensis">
