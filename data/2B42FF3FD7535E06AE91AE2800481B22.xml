<taxonomicName id="D654A342F3E98E1E39652AE447F0F910" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. taliangensis" order="Caudata" pageId="0" pageNumber="83" rank="species" species="taliangensis">
