<taxonomicName id="4342D77BE249A9BB65B0ABD79CC6AFEA" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. kweichowensis" order="Caudata" pageId="0" pageNumber="83" rank="species" species="kweichowensis">
<emphasis id="2AABCD5A9481B178E5D779DF8DCFFE49" italics="true" pageId="0" pageNumber="83">T. kweichowensis</emphasis>
</taxonomicName>
and 
<taxonomicName id="4F93634FA021020BCFC38C941833B827" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. pseudoverrucosus" order="Caudata" pageId="0" pageNumber="83" rank="species" species="pseudoverrucosus">
<emphasis id="8819D17EBC2CA19819464FEAD9F83453" italics="true" pageId="0" pageNumber="83">T. pseudoverrucosus</emphasis>
