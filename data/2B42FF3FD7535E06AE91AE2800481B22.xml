, and 
<taxonomicName id="7D92AAFEC3311A6A1008FB62005C8C42" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. shanjing" order="Caudata" pageId="0" pageNumber="83" rank="species" species="shanjing">
