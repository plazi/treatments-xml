); from 
<taxonomicName id="20AF05F8F172A9BFA638889BCF51B6EA" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. kachinorum" order="Caudata" pageId="0" pageNumber="83" rank="species" species="kachinorum">
<emphasis id="E2C1C9C8A01EFA6769507A865AF42A81" italics="true" pageId="0" pageNumber="83">T. kachinorum</emphasis>
</taxonomicName>
, 
<taxonomicName id="C07E6B0B8981923121E402CF0A04F93A" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. pulcherrimus" order="Caudata" pageId="0" pageNumber="83" rank="species" species="pulcherrimus">
<emphasis id="D7194A447FE5BAB339CB72C7CD3A3D20" italics="true" pageId="0" pageNumber="83">T. pulcherrimus</emphasis>
