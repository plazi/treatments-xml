<taxonomicName id="9A7922E2072B2151A85FAB7EDFE35C72" family="Marantaceae" genus="T." kingdom="Animalia" lsidName="T. himalayanus" order="Caudata" pageId="0" pageNumber="83" rank="species" species="himalayanus">
