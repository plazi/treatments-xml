<paragraph id="5E79894CFFD78104FF0DF9F25D42F90D" blockId="6.[151,1436,1433,1889]" pageId="6" pageNumber="428">
<taxonomicName id="99C6F2CFFFD78104FF0DF9F25E1BF9BA" authority="Weise, 1960" authorityName="Weise" authorityYear="1960" box="[199,618,1613,1638]" class="Insecta" family="Lucanidae" genus="Platycerus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="428" phylum="Arthropoda" rank="species" species="primigenius">
<emphasis id="6CB2555EFFD78104FF0DF9F25DA1F9B9" box="[199,464,1613,1637]" italics="true" pageId="6" pageNumber="428">Platycerus primigenius</emphasis>
<bibRefCitation id="3A57F4BDFFD78104FE16F9F25E1BF9BA" author="Weise, E." box="[476,618,1613,1638]" firstAuthor="Weise" journalOrPublisher="Entomologische Blatter" pageId="6" pageNumber="428" pagination="133 - 149" part="56" refId="ref10244" refString="Weise, E. (1960) Die palaarktischen Arten der Gattung Platycerus Fourcr. (Col., Lucanidae). Entomologische Blatter, 56, 133 - 149." title="Die palaarktischen Arten der Gattung Platycerus Fourcr. (Col., Lucanidae)" type="journal article" year="1960">
