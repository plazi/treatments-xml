<emphasis id="B908EADEF428FFB992CEFEEE9F8F7017" bold="true" box="[440,509,256,276]" pageId="28" pageNumber="305">
<collectingCountry id="F36B765CF428FFB992CEFEEE9F8F7017" box="[440,509,256,276]" name="Turkey" pageId="28" pageNumber="305">Turkey</collectingCountry>
</emphasis>
