<taxonomicName id="75B54D4C9701FFC2FEBA2A9FB89CA3AD" authorityName="Carl" authorityYear="1932" box="[306,504,1494,1521]" class="Diplopoda" family="Paradoxosomatidae" genus="Grammorhabdus" kingdom="Animalia" order="Polydesmida" pageId="20" pageNumber="505" phylum="Arthropoda" rank="genus">
<emphasis id="80C1EADD9701FFC2FEBA2A9FB89CA3AD" bold="true" box="[306,504,1494,1521]" italics="true" pageId="20" pageNumber="505">Grammorhabdus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="9BF257A69701FFC2FD8E2A9EBBF3A3AC" box="[518,663,1494,1521]" pageId="20" pageNumber="505">stat. revalid</taxonomicNameLabel>
