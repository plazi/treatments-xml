<quantity id="6C089B54AB72FFB7FD6BFDF5FD605437" box="[640,700,565,585]" metricMagnitude="2" metricUnit="m" metricValue="8.5" pageId="5" pageNumber="1264" unit="m" value="850.0">850 m</quantity>
