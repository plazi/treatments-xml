<taxonomicName id="6CF04D32AB72FFB7FD0FFDB0FCA354FB" box="[740,895,624,645]" class="Insecta" family="Tenthredinidae" genus="Pristiphora" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="1264" phylum="Arthropoda" rank="species" species="undetermined">
