<treatmentCitationGroup id="09D9A504FFA0FF95F287AD54A070A55B" box="[151,544,1577,1599]" pageId="8" pageNumber="9">
<taxonomicName id="EEC9F9A9FFA0FF95F287AD54A070A55B" authority="Lea, 1899: 226" authorityName="Lea" authorityPageNumber="226" authorityYear="1899" box="[151,544,1577,1599]" class="Insecta" family="Curculionidae" genus="Melanterius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="antennalis">
