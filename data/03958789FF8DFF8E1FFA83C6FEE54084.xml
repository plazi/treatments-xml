<taxonomicName id="4C3C4D1CFF8BFF8E1CE580DEFB9C41A4" box="[904,1037,189,212]" class="Insecta" family="Fanniidae" genus="Fannia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="immutica">
