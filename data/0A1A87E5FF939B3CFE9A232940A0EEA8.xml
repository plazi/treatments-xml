</taxonomicName>
resembles 
<taxonomicName id="45B34D70FF939B3CFE5E24F440E9EF8F" authorityName="Fukuoka &amp; Murano" authorityYear="2005" baseAuthorityName="Liu and Wang" baseAuthorityYear="1983" box="[490,662,1243,1267]" class="Malacostraca" family="Mysidae" genus="Orientomysis" kingdom="Animalia" order="Mysida" pageId="37" pageNumber="692" phylum="Arthropoda" rank="species" species="meridionalis">
