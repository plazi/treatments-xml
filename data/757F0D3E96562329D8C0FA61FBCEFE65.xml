<geoCoordinate id="98E2DAEF96552329DB2DFE71FC1CFE65" box="[890,951,477,503]" degrees="21" direction="north" orientation="latitude" pageId="43" pageNumber="44" precision="55555" value="21.0">21°N</geoCoordinate>
