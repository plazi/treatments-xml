<emphasis id="B951EAD1FF94FFC55C2C5FD4FDF6FD7C" bold="true" box="[264,616,684,710]" pageId="5" pageNumber="6">
<emphasis id="B951EAD1FF94FFC55C2C5FD4FD9DFD7C" bold="true" box="[264,515,684,710]" italics="true" pageId="5" pageNumber="6">
<taxonomicName id="4C254D40FF94FFC55C2C5FD4FE62FD7C" box="[264,508,684,710]" class="Insecta" family="Mydidae" genus="Hessemydas" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="parkeri" status="sp. nov.">Hessemydas parkeri</taxonomicName>
,
</emphasis>
<taxonomicNameLabel id="A26257AAFF94FFC55F2E5FD4FDF6FD7C" box="[522,616,684,710]" pageId="5" pageNumber="6" rank="species">sp. nov.</taxonomicNameLabel>
