<taxonomicName id="4C1A33ECFF89425CFD25FDB5066DE9F2" box="[735,888,621,644]" class="Malacostraca" family="Galatheidae" genus="Galathea" kingdom="Animalia" order="Decapoda" pageId="122" pageNumber="123" phylum="Arthropoda" rank="species" species="raventosae">
<emphasis id="B96E947DFF89425CFD25FDB5066DE9F2" box="[735,888,621,644]" italics="true" pageId="122" pageNumber="123">G. raventosae</emphasis>
</taxonomicName>
were 
