<taxonomicName id="E7D44D1EFFC6FFCB0BA8F918F92BF899" box="[151,333,1769,1793]" class="Lingulata" family="Acrotretidae" genus="Cyrtonotreta" kingdom="Animalia" order="Acrotretida" pageId="48" pageNumber="49" phylum="Brachiopoda" rank="species" species="undetermined">
<emphasis id="12A0EA8FFFC6FFCB0BA8F918F941F899" box="[151,295,1769,1793]" italics="true" pageId="48" pageNumber="49">Cyrtonotreta</emphasis>
