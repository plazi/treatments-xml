Material: 
<specimenCount id="A587FD18D558FFE02122019439C8211B" box="[320,401,1524,1549]" pageId="28" pageNumber="285" type="male">
<number id="054924B6D558FFE0212201943917211B" box="[320,334,1524,1549]" pageId="28" pageNumber="285" value="1.0">1</number>
male
</specimenCount>
,
</emphasis>
<collectingCountry id="CB967601D558FFE021FD01953A50211B" box="[415,521,1525,1549]" name="Samoa" pageId="28" pageNumber="285">SAMOA</collectingCountry>
: Savaii, Asau 
<quantity id="74799B74D558FFE022C801953B50211B" box="[682,777,1525,1549]" metricMagnitude="2" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="28" pageNumber="285" unit="m" value="150.0" valueMax="300.0" valueMin="0.0">0–300m</quantity>
, 
<date id="C73F1051D558FFE0237701943B35211B" box="[789,876,1524,1549]" pageId="28" pageNumber="285" value="1969-09">
ix. 
<number id="054924B6D558FFE0235201943B35211B" box="[816,876,1524,1549]" pageId="28" pageNumber="285" value="1969.0">1969</number>
