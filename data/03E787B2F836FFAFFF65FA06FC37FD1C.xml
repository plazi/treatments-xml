<typeStatus id="54F58806F836FFAFFF5AFA4FFF01FA28" box="[144,196,1512,1533]" pageId="12" pageNumber="519">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C4E4D27F836FFAFFEE3FA4FFD66FA28" authority="Loew, 1857" authorityName="Loew" authorityYear="1857" box="[297,675,1511,1533]" class="Insecta" family="Syrphidae" genus="Paragus" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="519" phylum="Arthropoda" rank="species" species="longiventris">
