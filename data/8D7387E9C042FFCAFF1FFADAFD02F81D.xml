<taxonomicName id="C2DA4D7CC04BFFCAFEA1F9CBFD43F993" authority="Liu &amp; Chen, 2010" authorityName="Liu &amp; Chen" authorityYear="2010" box="[297,710,1636,1659]" class="Arachnida" family="Linyphiidae" genus="Neriene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="poculiforma">
