<number id="4F0A5664FF8EFFCF8B63F8E6796162EA" box="[1028,1081,1897,1920]" pageId="5" pageNumber="6" value="1986.0">1986</number>
</taxonomicName>
<number id="4F0A5664FF8EFFCF8B27F8E6790562EA" box="[1088,1117,1897,1920]" pageId="5" pageNumber="6" value="14.0">14</number>
