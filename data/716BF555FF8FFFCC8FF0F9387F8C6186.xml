<number id="4F0A5664FF8EFFCF8C78FF327E7565BD" box="[799,813,189,215]" pageId="5" pageNumber="6" value="3.0">3</number>
&gt; 
<number id="4F0A5664FF8EFFCF8C58FF327E1765BD" box="[831,847,189,215]" pageId="5" pageNumber="6" value="4.0">4</number>
