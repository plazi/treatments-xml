<taxonomicName id="697ACB0A962E098A1E2F82E6C2F221D6" class="Magnoliopsida" family="Amaranthaceae" genus="Chenopodium" genus.bestMatchDistance="780" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Plantae" order="Caryophyllales" pageId="26" pageNumber="30" phylum="Tracheophyta" rank="species" species="album">C. album</taxonomicName>
