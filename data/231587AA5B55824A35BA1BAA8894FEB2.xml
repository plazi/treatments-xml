<collectingRegion id="6978F85E5B54824A35EF1F288AC2FEB2" box="[194,445,352,378]" country="Canada" name="Prince Edward Island" pageId="25" pageNumber="26">Prince Edward Island</collectingRegion>
found boring in 
<taxonomicName id="6CBC4D3F5B54824A37AE1F28881EFEB2" authority="Sarg." authorityName="Sarg." box="[643,865,352,378]" class="Pinopsida" family="Pinaceae" genus="Picea" kingdom="Plantae" order="Pinales" pageId="25" pageNumber="26" phylum="Tracheophyta" rank="species" species="rubens">
<emphasis id="99C8EAAE5B54824A37AE1F288862FEB1" box="[643,797,352,377]" italics="true" pageId="25" pageNumber="26">Picea rubens</emphasis>
Sarg.
</taxonomicName>
(
<taxonomicName id="6CBC4D3F5B54824A365D1F2888A0FEB2" box="[880,991,352,378]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="25" pageNumber="26" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
