<paragraph id="7F0936B4FFB4FF9D6DCD928DFE745631" blockId="19.[151,1437,151,2021]" box="[199,388,1780,1805]" pageId="19" pageNumber="220">
<taxonomicName id="B8B64D37FFB4FF9D6DCD928DFE745631" baseAuthorityName="Weymer" baseAuthorityYear="1895" box="[199,388,1780,1805]" class="Insecta" family="Pieridae" genus="Enantia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="species" species="clarissa">
