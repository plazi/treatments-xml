Known only from 
<typeStatus id="54F938A35B4E11177C30D829C2657833" box="[575,623,1568,1592]" pageId="48" pageNumber="149">type</typeStatus>
locality at 
<taxonomicName id="4C42FD825B4E11177CFCD816C34A7833" box="[755,832,1567,1592]" class="Insecta" family="Braconidae" genus="Andesipolis" kingdom="Animalia" order="Hymenoptera" pageId="48" pageNumber="143" phylum="Arthropoda" rank="species" species="itatiaia" status="sp. n.">Itatiaia</taxonomicName>
National Park, 
<collectingRegion id="498648E35B4E11177DF9D816C4957833" box="[1014,1183,1567,1592]" country="Brazil" name="Rio de Janeiro" pageId="48" pageNumber="149">Rio de Janeiro</collectingRegion>
, 
<collectingCountry id="F355C6915B4E11177AA3D816C4F97833" box="[1196,1267,1567,1592]" name="Brazil" pageId="48" pageNumber="149">Brazil</collectingCountry>
, at 
<quantity id="4CBA2BE45B4E11177B11D816C576783C" box="[1310,1404,1567,1592]" metricMagnitude="3" metricUnit="m" metricValue="2.255" pageId="48" pageNumber="149" unit="m" value="2255.0">2,255 m</quantity>
