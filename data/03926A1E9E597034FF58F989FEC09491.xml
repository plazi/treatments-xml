<subSubSection id="C32188839E597034FF58F904FA0B9449" lastPageId="14" lastPageNumber="15" pageId="13" pageNumber="14" type="reference_group">
<paragraph id="8B84DB089E597037FF58F904FD0693BA" blockId="13.[151,653,1730,1753]" box="[151,653,1730,1753]" pageId="13" pageNumber="14">
<treatmentCitationGroup id="AB2BFC269E597037FF58F904FD0693BA" box="[151,653,1730,1753]" pageId="13" pageNumber="14">
<treatmentCitation id="0A9AFD199E597037FF58F904FD0293BA" author="Makarchenko, E. A." box="[151,649,1730,1753]" page="87" pageId="13" pageNumber="14" year="1994">
<taxonomicName id="4C3BA08B9E597037FF58F904FD0293BA" authority="Makarchenko, 1994: 87" authorityName="Makarchenko. A" authorityPageNumber="87" authorityYear="1994" box="[151,649,1730,1753]" class="Insecta" family="Chironomidae" genus="Boreoheptagyia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="unica">
<emphasis id="B94F071A9E597037FF58F904FE0993BA" box="[151,386,1730,1753]" italics="true" pageId="13" pageNumber="14">Boreoheptagyia unica</emphasis>
<bibRefCitation id="EFAAA6F99E597037FE46F904FD0293BA" author="Makarchenko, E. A." box="[393,649,1730,1753]" pageId="13" pageNumber="14" pagination="87 - 90" refId="ref8941" refString="Makarchenko, E. A. (1994) Chironomids of the subfamily Diamesinae (Diptera, Chironomidae) from Japan. III. Boreoheptagyia unica sp. n. Bulletin of the National Science Museum, Tokyo, Series A, 20 (2), 87 - 90." type="journal article" year="1994">Makarchenko, 1994: 87</bibRefCitation>
