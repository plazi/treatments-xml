<taxonomicName id="4C3044C2FFDEFFEF8A3FFB14FF95BFC0" authority="Dworakowska, 1974" authorityName="Dworakowska" authorityYear="1974" box="[300,674,1211,1235]" class="Insecta" family="Cicadellidae" genus="Nlunga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="332" phylum="Arthropoda" rank="species" species="reeneni">
