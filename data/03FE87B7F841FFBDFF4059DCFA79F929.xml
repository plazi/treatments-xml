<taxonomicName id="4C574D22F841FFBEFE4A59DCFCEAFAE5" authority="Targioni Tozzetti, 1884" authorityName="Targioni Tozzetti" authorityYear="1884" box="[413,783,1266,1293]" class="Insecta" family="Kerriidae" genus="Kerria" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="65" phylum="Arthropoda" rank="genus">
