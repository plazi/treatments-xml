<heading id="1B1581B0CC19FFBAE8DD06D00E4DF3E2" bold="true" box="[151,338,1687,1713]" fontSize="11" level="1" pageId="15" pageNumber="16" reason="1">
<emphasis id="7296EACECC19FFBAE8DD06D00E4DF3E2" bold="true" box="[151,338,1687,1713]" pageId="15" pageNumber="16">
<taxonomicName id="87E24D5FCC19FFBAE8DD06D00E37F3E2" box="[151,296,1687,1713]" class="Hydrozoa" family="Hydractiniidae" genus="Hydractinia" kingdom="Animalia" order="Anthoathecata" pageId="15" pageNumber="16" phylum="Cnidaria" rank="genus">
<emphasis id="7296EACECC19FFBAE8DD06D00E37F3E2" bold="true" box="[151,296,1687,1713]" italics="true" pageId="15" pageNumber="16">Hydractinia</emphasis>
