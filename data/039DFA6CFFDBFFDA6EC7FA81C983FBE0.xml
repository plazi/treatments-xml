<collectingCountry id="F3230BEAFFDBFFDB6FB0F8C7CA49F88D" box="[511,615,1883,1909]" name="Denmark" pageId="4" pageNumber="554">Denmark</collectingCountry>
Hill, Ipswich, 
<collectingRegion id="49F08598FFDBFFDB6EC7F81CC921F862" box="[136,271,1920,1946]" country="Australia" name="Queensland" pageId="4" pageNumber="554">Queensland</collectingRegion>
, 
<collectingCountry id="F3230BEAFFDBFFDB6F6FF81CC9A4F862" box="[288,394,1920,1946]" name="Australia" pageId="4" pageNumber="554">Australia</collectingCountry>
