<taxonomicName id="37E270D1E22E3141FF08FA40FE3B608B" authority="(Kirby, 1808)" baseAuthorityName="Kirby" baseAuthorityYear="1808" box="[185,484,1416,1442]" class="Insecta" family="Apionidae" genus="Holotrichapion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="423" phylum="Arthropoda" rank="species" species="ononis">
