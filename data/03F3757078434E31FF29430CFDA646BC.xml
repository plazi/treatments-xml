<taxonomicName id="4C5ABFE578414E30FDFA43FBFCE843B7" baseAuthorityName="Galileo" baseAuthorityYear="1987" box="[580,799,1970,1991]" class="Insecta" family="Cerambycidae" genus="Biribellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="136" phylum="Arthropoda" rank="species" species="huedepohli">
