<taxonomicName id="B4DA0FD85532FFCB8C52F2EEFDC99F30" authorityName="Davis &amp; Gentili-Poole &amp; Mitter" authorityYear="2008" baseAuthorityName="Schaus" baseAuthorityYear="1905" box="[311,551,1271,1290]" class="Insecta" family="Cossidae" genus="Magulacra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="248" phylum="Arthropoda" rank="species" species="niveogrisea">
