<taxonomicName id="4C294D52FFA7FF8EB80056C862C810AF" authorityName="Rao" authorityYear="1981" box="[592,717,1407,1432]" class="Gastrotricha (awaiting allocation)" family="Thaumastodermatidae" genus="Tetranchyroderma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macrodasyida" pageId="8" pageNumber="503" phylum="Gastrotricha" rank="species" species="littoralis">
