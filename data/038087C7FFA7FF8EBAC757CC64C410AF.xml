<taxonomicName id="4C294D52FFA7FF8EB8A056C8636310AF" authorityName="Rao" authorityYear="1981" box="[752,870,1407,1432]" class="Gastrotricha (awaiting allocation)" family="Thaumastodermatidae" genus="Tetranchyroderma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Macrodasyida" pageId="8" pageNumber="503" phylum="Gastrotricha" rank="species" species="littorale">
