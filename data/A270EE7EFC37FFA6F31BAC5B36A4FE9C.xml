<paragraph id="2A665F68FC37FFA1F181AA4036A0F886" blockId="75.[189,1399,1808,1870]" pageId="75" pageNumber="76">
<taxonomicName id="EDD924EBFC37FFA1F181AA403532F8E2" authorityName="Johansson &amp; Cederberg" authorityYear="2019" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[189,354,1808,1834]" class="Insecta" family="Ichneumonidae" genus="Ophion" kingdom="Animalia" order="Hymenoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="luteus">
