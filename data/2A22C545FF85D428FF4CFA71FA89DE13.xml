<paragraph id="A2347453FF85D42BFF4CF9C5FDA4DF6D" blockId="1.[151,609,1618,1640]" box="[151,609,1618,1640]" pageId="1" pageNumber="58">
<taxonomicName id="658B0FD0FF85D42BFF4CF9C5FDEDDF6D" authority="Kuoh, 1992: 269" authorityName="Kuoh" authorityPageNumber="269" authorityYear="1992" box="[151,552,1618,1640]" class="Insecta" family="Cicadellidae" genus="Macropsis" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="species" species="flavovirens">
