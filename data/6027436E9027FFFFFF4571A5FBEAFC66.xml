<specimenCount id="FE8839F19024FFFFFAFB70A0FABCFD21" box="[1321,1398,655,681]" pageId="7" pageNumber="411" type="male">1 male</specimenCount>
on 
<taxonomicName id="2F8E89FB9024FFFFFF45709CFE4EFD45" box="[151,388,691,717]" class="Magnoliopsida" family="Elaeagnaceae" genus="Elaeagnus" kingdom="Plantae" order="Rosales" pageId="7" pageNumber="411" phylum="Tracheophyta" rank="species" species="umbellata">
