<quantity id="4CB87F9A437AC978271C27C3FE5BF850" box="[366,455,2009,2033]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="29" pageNumber="30" unit="m" value="2000.0">2000 m</quantity>
