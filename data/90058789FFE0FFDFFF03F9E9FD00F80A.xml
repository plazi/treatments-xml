<taxonomicName id="DFAC4D1CFFE0FFD9FF53F95FFE37F945" authority="L. R. Parra" authorityName="L. R. Parra" box="[199,424,1699,1722]" class="Liliopsida" family="Eriocaulaceae" genus="Comanthera" kingdom="Plantae" order="Poales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="giuliettiae">giuliettiae L.R.Parra</taxonomicName>
