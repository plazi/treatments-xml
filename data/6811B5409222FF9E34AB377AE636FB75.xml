(
<figureCitation id="788318D39222FF9F371C375AE693FA05" box="[566,681,1391,1415]" captionStart="Figure 2" captionStartId="13.[212,279,1554,1576]" captionTargetBox="[212,1172,386,1524]" captionTargetId="figure-96@13.[212,1172,379,1524]" captionTargetPageId="13" captionText="Figure 2. Dihammaphora species. (A) Dihammaphora perforata Klug, female (?). (B) Dihammaphora ibirajarai Mermudes, female holotype. (C) Dihammaphora nitidicollis Bates, male (?) holotype. (D) Dihammaphora ruficollis Chevrolat, male. (E) Dihammaphora mineira sp. nov., male (?) holotype. (F) Dihammaphora chontalensis Bates, female holotype. (C) and (F) reproduced from slides." figureDoi="http://doi.org/10.5281/zenodo.4652158" httpUri="https://zenodo.org/record/4652158/files/figure.png" pageId="14" pageNumber="882">Figure 2B</figureCitation>
) 
<taxonomicName id="27B87FD59222FF9F35BA37A5E6F6FA25" authority="Mermudes, 1998: 98" authorityName="Mermudes" authorityPageNumber="98" authorityYear="1998" box="[144,716,1423,1448]" class="Insecta" family="Cerambycidae" genus="Dihammaphora" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="882" phylum="Arthropoda" rank="species" species="ibirajarai">
