<paragraph id="2232AAFC244274F6D82EBDA25B8DA8E0" pageId="24" pageNumber="49">
<taxonomicName id="545D7ABF257C3549A5CAA332EB727124" class="Sordariomycetes" family="Pestalotiopsidaceae" genus="Pestalotiopsis" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Pestalotiopsis microspora" order="Amphisphaeriales" pageId="24" pageNumber="49" phylum="Ascomycota" rank="species" species="microspora">Pestalotiopsis microspora</taxonomicName>
