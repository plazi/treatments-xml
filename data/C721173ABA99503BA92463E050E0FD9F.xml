<paragraph id="6E4EC9351BB62ABCEDC4D6741BCEB2D7" pageId="0" pageNumber="66115">
<taxonomicName id="6C192503690A288107F5750CF3ABFDDD" class="Insecta" family="Palaeontinidae" genus="Aphrophoridae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aphrophoridae" order="Hemiptera" pageId="0" pageNumber="66115" phylum="Arthropoda" rank="genus">Aphrophoridae</taxonomicName>
sp. (
<taxonomicName id="06F5111DE5D51920592BF6B47B61E05B" authorityName="Petch" authorityYear="1931" class="Sordariomycetes" family="Ophiocordycipitaceae" genus="Ophiocordyceps" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="" order="Hemiptera" pageId="0" pageNumber="66115" phylum="Ascomycota" rank="order">Hemiptera</taxonomicName>
