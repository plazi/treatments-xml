<taxonomicName id="B344E416395B9F49236264E155617839" LSID="http://zoobank.org/7604DDB6-3057-4550-98C5-63E622442CC8" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus mellosus" order="Hymenoptera" pageId="113" pageNumber="114" phylum="Arthropoda" rank="species" species="mellosus">Heterospilus mellosus Marsh</taxonomicName>
