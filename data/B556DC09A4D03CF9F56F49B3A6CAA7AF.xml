<taxonomicName id="5DE5D23EFE775345CED6B968D2D7A96D" class="Arachnida" family="Ceratozetidae" genus="Ceratozetes" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageNumber="386" phylum="Arthropoda" rank="species" species="maximus">Ceratozetes maximus</taxonomicName>
