: 
<taxonomicName id="4C208FBCFFC5FF9EFF49F8FAFCB49800" authority="Bonaparte, 1856" authorityName="Bonaparte" authorityYear="1856" box="[227,878,1820,1846]" class="Aves" family="Columbidae" genus="Streptopelia" kingdom="Animalia" order="Columbiformes" pageId="2" pageNumber="17" phylum="Chordata" rank="subSpecies" species="senegalensis" subSpecies="ermanni">
<emphasis id="B954282DFFC5FF9EFF49F8FAFD5C9803" box="[227,646,1820,1845]" italics="true" pageId="2" pageNumber="17">Streptopelia senegalensis ermanni</emphasis>
(Bonaparte, 
<number id="3DE8E618FFC5FF9EFC8DF8FAFCBF9800" box="[807,869,1820,1846]" pageId="2" pageNumber="17" value="1856.0">1856</number>
)
</taxonomicName>
(e.g. 
<bibRefCitation id="EFB189CEFFC5FF9EFC1FF8FAFBA19800" author="Dickinson" box="[949,1147,1820,1846]" pageId="2" pageNumber="23" refString="Dickinson, E. C. (Ed., 2003) The Howard and Moore Complete checklist of the birds of the world. 3 rd rev. ed. Christopher Helm, London, 1039 pp." type="book" year="2003">
Dickinson 
<number id="3DE8E618FFC5FF9EFB91F8FAFBA19800" box="[1083,1147,1820,1846]" pageId="2" pageNumber="17" value="2003.0">2003</number>
</bibRefCitation>
, based on a revision by 
<bibRefCitation id="EFB189CEFFC5FF9EFF3DF8A4FEF5986A" author="Hartert" box="[151,303,1858,1884]" pageId="2" pageNumber="24" refString="Hartert, E. (1916) Notes on pigeons. Novitates Zoologicae, 23, 78 - 88." type="journal article" year="1916">
Hartert 
<number id="3DE8E618FFC5FF9EFF5BF8A4FEF5986A" box="[241,303,1858,1884]" pageId="2" pageNumber="17" value="1916.0">1916</number>
</bibRefCitation>
; see also 
<bibRefCitation id="EFB189CEFFC5FF9EFE36F8A4FDEF986A" author="Hartert" box="[412,565,1858,1884]" pageId="2" pageNumber="24" refString="Hartert, E. (1920) Die Vogel der palaarktischen Fauna. Vol. 3 (Heft 11 - 12). R. Friedlander &amp; Sohn, Berlin, pp. 1345 - 1600." type="journal article" year="1920">
Hartert 
<number id="3DE8E618FFC5FF9EFE5CF8A4FDEF986A" box="[502,565,1858,1884]" pageId="2" pageNumber="17" value="1920.0">1920</number>
</bibRefCitation>
: 
<number id="3DE8E618FFC5FF9EFDEAF8A4FDA6986A" box="[576,636,1858,1884]" pageId="2" pageNumber="17" value="1495.0">1495</number>
) or 
<taxonomicName id="4C208FBCFFC5FF9EFD00F8A5FEDF98B5" authority="(Gmelin, 1789) (Gibbs et al. 2001)" authorityName="(Gmelin, 1789) (Gibbs et al." authorityYear="2001" class="Aves" family="Columbidae" genus="Streptopelia" kingdom="Animalia" order="Columbiformes" pageId="2" pageNumber="17" phylum="Chordata" rank="subSpecies" species="senegalensis" subSpecies="cambayensis">
<emphasis id="B954282DFFC5FF9EFD00F8A5FBB4986A" box="[682,1134,1859,1884]" italics="true" pageId="2" pageNumber="17">Streptopelia senegalensis cambayensis</emphasis>
(Gmelin, 
<number id="3DE8E618FFC5FF9EFB4FF8A4FAFB986A" box="[1253,1313,1858,1884]" pageId="2" pageNumber="17" value="1789.0">1789</number>
) (
<bibRefCitation id="EFB189CEFFC5FF9EFA92F8A4FF2198B5" author="Gibbs" pageId="2" pageNumber="23" refString="Gibbs, D., Barnes, E. &amp; Cox, J. (2001) Pigeons and doves. A guide to the pigeons and doves of the world. Yale University Press, New Haven, 615 pp." type="book" year="2001">
Gibbs 
<emphasis id="B954282DFFC5FF9EFA2DF8A5FF7498B4" italics="true" pageId="2" pageNumber="17">et al</emphasis>
. 
<number id="3DE8E618FFC5FF9EFF16F88FFF2198B5" box="[188,251,1897,1923]" pageId="2" pageNumber="17" value="2001.0">2001</number>
</bibRefCitation>
)
</taxonomicName>
.
</paragraph>
