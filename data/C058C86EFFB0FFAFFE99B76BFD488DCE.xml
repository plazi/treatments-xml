<figureCitation id="D0CA65FDFFB0FFA9FE80B748FEB58A89" box="[273,354,879,905]" captionStart="FIGURE 1" captionStartId="3.[264,367,1679,1703]" captionTargetBox="[328,1279,324,1631]" captionTargetId="figure@3.[273,1325,302,1664]" captionTargetPageId="3" captionText="FIGURE 1. Libiocoris heissi sp. nov. Ψ, holotype. Habitus. Scale bar = 0.695 mm." httpUri="https://zenodo.org/record/174896/files/figure.png" pageId="2" pageNumber="41">Figs. 1</figureCitation>
, 
<figureCitation id="D0CA65FDFFB0FFA9FEE1B748FE4A8A89" box="[368,413,879,905]" captionStart="FIGURES 3 – 12. 3 – 7" captionStartId="7.[264,382,1769,1793]" captionTargetBox="[304,1278,319,1734]" captionTargetId="figure@7.[268,1319,262,1760]" captionTargetPageId="7" captionText="FIGURES 3 – 12. 3 – 7, Libiocoris heissi sp. nov.; 8 – 12, Libiocoris sinensis sp. nov. 3, 4, 8, 9, Head, antennae partly removed; 5, 10, right antenna; 6, 11, apex of right fore tibia with comb and tarsus; 7, 12, abdominal segments V – VIII. 5, 10, dorsal view; 3, 6, 8, 11, lateral view; 4, 7, 9, 12, ventral view. Scale bar of 3 – 5, 7 – 10, 12 = 0.5 mm; of 6, 11 = 0.25 mm." httpUri="https://zenodo.org/record/174898/files/figure.png" pageId="2" pageNumber="41">
3– 
<number id="FE396B5FFFB0FFA9FE1FB748FE4A8A89" box="[398,413,879,905]" pageId="2" pageNumber="41" value="7.0">7</number>
</figureCitation>
)
