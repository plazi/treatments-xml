<taxonomicName id="D9DD4D77FFC7FFC4FECD9CCDFCC4FD3D" authority="Fallen" authorityName="Fallen" box="[279,468,709,736]" class="Insecta" family="Ephydridae" genus="Ephydra" kingdom="Animalia" order="Diptera" pageId="87" pageNumber="88" phylum="Arthropoda" rank="genus">
