<taxonomicName id="568C5F8AFFF5E85471DEF9C8FD979B24" authority="Vinogradova, 1970" authorityName="Vinogradova" authorityYear="1970" box="[236,581,1554,1573]" class="Ascidiacea" family="Pyuridae" genus="Culeolus" kingdom="Animalia" order="Pleurogona" pageId="28" pageNumber="334" phylum="Chordata" rank="species" species="robustus">
