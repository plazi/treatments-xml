<taxonomicName id="0AD002F9AA7AF37BFDE9FAF1FD498DBB" box="[557,742,1397,1422]" class="Insecta" family="Stratiomyidae" genus="Sargus" kingdom="Animalia" order="Diptera" pageId="15" phylum="Arthropoda" rank="species" species="lateritius">
