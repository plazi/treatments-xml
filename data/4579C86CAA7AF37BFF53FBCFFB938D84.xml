<materialsCitation id="7DB87327AA7AF37BFF53FAF1FB938D84" ID-GBIF-Occurrence="1500371019" collectionCode="MZUN" country="Madagascar" location="Madagascar" pageId="15" specimenCount="1" typeStatus="holotype">
