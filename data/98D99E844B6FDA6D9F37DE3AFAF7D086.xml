<taxonomicName id="8D28F48E532DA2E26E46CECECF8508E5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31703" class="Insecta" family="Formicidae" genus="Monomorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Monomorium salomonis (Linnaeus)" order="Hymenoptera" pageNumber="137" phylum="Arthropoda" rank="species" species="salomonis">Monomorium salomonis (Linnaeus)</taxonomicName>
