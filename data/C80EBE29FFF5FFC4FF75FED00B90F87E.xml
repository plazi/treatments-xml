<treatmentCitationGroup id="60B72811FFF5FFC4FF75FED00D47FF4C" box="[151,597,151,178]" pageId="50" pageNumber="51">
<taxonomicName id="87A774BCFFF5FFC4FF75FED00D47FF4C" authority="Forcart, 1953: 379" authorityName="Forcart" authorityPageNumber="379" authorityYear="1953" box="[151,597,151,178]" class="Reptilia" family="Colubridae" genus="Stegonotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="50" pageNumber="51" phylum="Chordata" rank="species" species="sutteri">
