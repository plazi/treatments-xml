<specimenCode id="1001A744FFF5FFC4FEA0FA590EF1FBC8" box="[322,483,1054,1078]" collectionCode="NMBA" country="Switzerland" httpUri="http://grbio.org/cool/js1h-4156" name="Naturhistorisches Museum, Basel" pageId="50" pageNumber="51">NMBA 14872</specimenCode>
(
