<quantity id="890413999514FF862241FECBFE44E13E" box="[359,459,297,323]" metricMagnitude="-4" metricUnit="m" metricValue="8.4" pageId="8" pageNumber="359" unit="mm" value="0.84">0.84 mm</quantity>
