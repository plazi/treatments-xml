<collectionCode id="28ED26B99514FF862128FF7BFDE3E0CE" box="[526,620,153,179]" country="Austria" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="8" pageNumber="359" type="Museum">NHMW</collectionCode>
