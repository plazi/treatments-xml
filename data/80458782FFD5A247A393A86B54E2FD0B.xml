<taxonomicName id="CFEC4D17FFD5A258A213A86B5743F89C" authority="Karny, 1912" authorityName="Karny" authorityYear="1912" box="[279,616,1854,1880]" class="Insecta" family="Tettigoniidae" genus="Pseudonicsara" kingdom="Animalia" order="Orthoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
