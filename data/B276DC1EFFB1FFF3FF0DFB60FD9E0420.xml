<quantity id="FD27C0EDFFB1FFF5FB86F8D8FB770205" box="[1052,1189,1832,1857]" metricMagnitude="-3" metricUnit="m" metricValue="8.7" metricValueMax="8.9" metricValueMin="8.5" pageId="19" pageNumber="60" unit="mm" value="8.7" valueMax="8.9" valueMin="8.5">8.5–8.9 mm</quantity>
