<taxonomicNameLabel id="08C257E4FFB0FF86322A937F1121FBF4" box="[574,637,1052,1076]" pageId="9" pageNumber="392" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="E6854D0EFFB0FF86329E937F11B5FBF4" box="[650,745,1052,1076]" class="Insecta" family="Arctiidae" genus="Afrasura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="392" phylum="Arthropoda" rank="species" species="violacea">
<emphasis id="13F1EA9FFFB0FF86329E937F11B5FBF4" box="[650,745,1052,1076]" italics="true" pageId="9" pageNumber="392">violacea</emphasis>
</taxonomicName>
, 
