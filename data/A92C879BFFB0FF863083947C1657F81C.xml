<taxonomicNameLabel id="08C257E4FFB0FF863344937F10D3FBF4" box="[848,911,1052,1076]" pageId="9" pageNumber="392" rank="species">
<emphasis id="13F1EA9FFFB0FF863344937F10D3FBF4" bold="true" box="[848,911,1052,1076]" pageId="9" pageNumber="392">sp. n.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="E6854D0EFFB0FF863388937F171DFBF4" box="[924,1089,1052,1076]" class="Insecta" family="Arctiidae" genus="Afrasura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="392" phylum="Arthropoda" rank="species" species="submarmorata">
<emphasis id="13F1EA9FFFB0FF863388937F171DFBF4" box="[924,1089,1052,1076]" italics="true" pageId="9" pageNumber="392">submarmorata</emphasis>
