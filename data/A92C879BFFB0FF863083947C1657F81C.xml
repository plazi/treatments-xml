<taxonomicName id="E6854D0EFFB0FF863254932311D7FB98" box="[576,651,1088,1112]" class="Insecta" family="Arctiidae" genus="Afrasura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="392" phylum="Arthropoda" rank="species" species="duplex" status="sp. n.">
