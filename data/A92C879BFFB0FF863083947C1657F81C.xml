<taxonomicName id="E6854D0EFFB0FF863313932210D9FB98" box="[775,901,1089,1112]" class="Insecta" family="Arctiidae" genus="Afrasura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="392" phylum="Arthropoda" rank="species" species="amaniensis">
