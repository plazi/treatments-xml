<taxonomicName id="4C7C82D9D430C371FBA0F8A6FB38F8E8" authorityName="Klautau &amp; Valentine" authorityYear="2003" box="[1049,1225,1821,1842]" class="Calcarea" family="Clathrinidae" genus="Clathrina" kingdom="Animalia" order="Clathrinida" pageId="44" pageNumber="45" phylum="Porifera" rank="species" species="sinusarabica">
