<specimenCount id="0B827EA6FF81FF90FE94EE32FE74F880" box="[368,398,1833,1854]" pageId="11" pageNumber="242" type="female">1 ♀</specimenCount>
, 
<date id="693A93EFFF81FF90FE41EE32FDF2F883" box="[421,520,1834,1854]" pageId="11" pageNumber="242" value="1991-05-25">25-V-1991</date>
; 3 ♁, 
<date id="693A93EFFF81FF90FDB6EE32FD3AF883" box="[594,704,1834,1854]" pageId="11" pageNumber="242" value="1991-06-16">16-VI-1991</date>
. Marais de Yafélé, env. de Kéoulenta, 
<quantity id="DA7C18CAFF81FF90FBAFEFE9FB72F8B8" box="[1099,1160,1777,1797]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="11" pageNumber="242" unit="m" value="500.0">500 m</quantity>
