<quantity id="DA7C18CAFF81FF90FB91EE16FB49F89F" box="[1141,1203,1806,1826]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="11" pageNumber="242" unit="m" value="500.0">500 m</quantity>
