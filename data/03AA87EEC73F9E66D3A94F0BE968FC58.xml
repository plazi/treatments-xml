<taxonomicName id="4C034D7BC7399E66D11E4B3FE95EFC84" authorityName="Distant" authorityYear="1881" box="[544,631,837,863]" class="Insecta" family="Pentatomidae" genus="Edessa" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="136" phylum="Arthropoda" rank="species" species="stalii">
<emphasis id="B977EAEAC7399E66D11E4B3FE95EFC84" box="[544,631,837,863]" italics="true" pageId="5" pageNumber="136">E. stalii</emphasis>
