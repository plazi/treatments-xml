</subSubSection>
<subSubSection id="CAFA41A4DF456008FF1AC43BDA47F9E0" box="[159,824,1602,1627]" pageId="31" pageNumber="2485" type="reference_group">
<paragraph id="825F122FDF456008FF1AC43BDA47F9E0" blockId="31.[159,831,1539,1627]" box="[159,824,1602,1627]" pageId="31" pageNumber="2485">
<taxonomicName id="45E069ACDF456008FF1AC43BDB46F9E0" authority="Kott, 2001: 130" authorityName="Kott" authorityPageNumber="130" authorityYear="2001" box="[159,569,1602,1627]" class="Ascidiacea" family="Didemnidae" genus="Polysyncraton" kingdom="Animalia" order="Aplousobranchia" pageId="31" pageNumber="2485" phylum="Chordata" rank="species" species="rica">
