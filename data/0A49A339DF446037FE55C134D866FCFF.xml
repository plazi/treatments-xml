<taxonomicName id="45E069ACDF456008FF51C770D8C3FAA7" authorityName="Kott" authorityYear="2001" box="[212,444,1288,1309]" class="Ascidiacea" family="Didemnidae" genus="Polysyncraton" kingdom="Animalia" order="Aplousobranchia" pageId="31" pageNumber="2485" phylum="Chordata" rank="species" species="tegetum">
