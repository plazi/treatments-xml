<taxonomicName id="50FAC0CCD977FFF8FE8CFC37FE2DFC33" box="[321,444,928,952]" class="Insecta" family="Leuctridae" genus="Leuctra" kingdom="Animalia" order="Plecoptera" pageId="15" pageNumber="31" phylum="Arthropoda" rank="species" species="alabama">
