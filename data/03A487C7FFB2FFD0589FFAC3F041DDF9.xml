<taxonomicName id="4C0D4D52FFB2FFD05AAFF962F5A2DE4F" authorityName="Gros" authorityYear="1845" class="Arachnida" family="Scutacaridae" genus="Scutacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="8" pageNumber="384" phylum="Arthropoda" rank="genus">
<emphasis id="B979EAC3FFB2FFD05AAFF962F5A2DE4F" italics="true" pageId="8" pageNumber="384">Scutacarus</emphasis>
</taxonomicName>
