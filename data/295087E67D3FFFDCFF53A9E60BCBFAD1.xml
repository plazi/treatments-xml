<quantity id="66019B157D3FFFDCFE38AEEB08D9FB49" box="[508,654,1240,1266]" metricMagnitude="2" metricUnit="m" metricValue="6.675" metricValueMax="11.3" metricValueMin="2.05" pageId="2" pageNumber="3" unit="m" value="667.5" valueMax="1130.0" valueMin="205.0">205–1130 m</quantity>
