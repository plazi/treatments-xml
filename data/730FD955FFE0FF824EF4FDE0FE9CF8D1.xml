<typeStatus id="241DD6E1FFE0FF824C46FD28FDB9FD54" box="[549,599,655,676]" pageId="10" pageNumber="32">Type</typeStatus>
species 
<taxonomicName id="3CA613C0FFE0FF824CCEFD28FC6DFD55" authority="C.L.Koch, 1836" authorityName="C.L.Koch" authorityYear="1836" box="[685,899,655,677]" class="Arachnida" family="Laelapidae" genus="Laelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="32" phylum="Arthropoda" rank="species" species="meridionalis">
