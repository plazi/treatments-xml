</taxonomicName>
(
<taxonomicName id="4C384D34AB053C57C0FDFB42EE09FB12" box="[1165,1287,1180,1206]" class="Magnoliopsida" family="Myrtaceae" kingdom="Plantae" order="Myrtales" pageId="7" pageNumber="50" phylum="Tracheophyta" rank="family">Myrtaceae</taxonomicName>
) in 
<collectingRegion id="49FCF855AB053C57C15FFB43EBC1FB7E" country="Australia" name="New South Wales" pageId="7" pageNumber="50">New South Wales</collectingRegion>
, 
<collectingCountry id="F32F7627AB053C57C4AAFB1EEA4AFB7E" box="[218,324,1216,1242]" name="Australia" pageId="7" pageNumber="50">Australia</collectingCountry>
. Morphologically, 
<taxonomicName id="4C384D34AB053C57C66AFB1EE82DFB7E" authorityName="Y. Gao, Gentekaki E &amp; Monkai" authorityYear="2021" box="[538,803,1216,1242]" class="Dothideomycetes" family="Dothideaceae" genus="Dothidea" kingdom="Fungi" order="Dothideales" pageId="7" pageNumber="50" phylum="Ascomycota" rank="species" species="kunmingensis">
<emphasis id="B94CEAA5AB053C57C66AFB1EE82DFB7E" box="[538,803,1216,1242]" italics="true" pageId="7" pageNumber="50">Dothidea kunmingensis</emphasis>
</taxonomicName>
