<taxonomicName id="93980B9D16C2D0A4B41DC45E56027364" authorityName="Schaeffer" authorityYear="1910" class="Hexapoda" family="Carabidae" genus="Pterostichus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="526" pageNumber="527" phylum="Arthropoda" rank="species" species="fuchsi">Pterostichus fuchsi</taxonomicName>
