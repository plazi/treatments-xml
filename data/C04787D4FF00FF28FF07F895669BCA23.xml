<taxonomicName id="8FEE4D41FF00FF2BFF57F89567F1CC0A" authority="Warburton, 1912" authorityName="Warburton" authorityYear="1912" box="[199,690,1842,1869]" class="Arachnida" family="Ixodidae" genus="Rhipicephalus" kingdom="Animalia" order="Ixodida" pageId="233" pageNumber="234" phylum="Arthropoda" rank="species" species="punctatus">
