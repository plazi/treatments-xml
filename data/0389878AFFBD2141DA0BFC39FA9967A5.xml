Basonym: 
<taxonomicName id="4C204D1FFFBD2141DA9DFBFFFAD866E6" authority="Oberg et al. 2016" authorityName="Oberg" authorityYear="2016" box="[938,1399,1129,1153]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="48" pageNumber="2830" phylum="Firmicutes" rank="species" species="wasatchensis">
