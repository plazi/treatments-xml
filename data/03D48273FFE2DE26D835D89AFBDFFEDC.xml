<emphasis id="B909EF77FFE7DE26DA80DC18FBCFFEB4" bold="true" box="[957,1059,283,309]" pageId="4" pageNumber="5">n. comb</emphasis>
.
</taxonomicNameLabel>
, 
<taxonomicName id="4C7D48E6FFE7DE26DD00DC1FFAC6FEB4" authority="Buffington, 2004" authorityName="Buffington" authorityYear="2004" box="[1085,1322,284,309]" pageId="4" pageNumber="5" status="n. sp.">
<emphasis id="B909EF77FFE7DE26DD00DC1FFAC6FEB4" box="[1085,1322,284,309]" italics="true" pageId="4" pageNumber="5">Preseucoela heratyi</emphasis>
</taxonomicName>
Buffington, 
<emphasis id="B909EF77FFE7DE26D8AADC40FE35FEDC" bold="true" box="[407,473,323,349]" pageId="4" pageNumber="5">
<taxonomicNameLabel id="A23A520CFFE7DE26D8AADC40FE35FEDC" box="[407,473,323,349]" pageId="4" pageNumber="5" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C7D48E6FFE7DE26DB2FDC47FC09FEDF" authority="Buffington" authorityName="Buffington" authorityYear="2004" box="[530,997,324,350]" class="Insecta" family="Figitidae" genus="Preseucoela" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="imallshookupis" status="n. sp.">
<emphasis id="B909EF77FFE7DE26DB2FDC47FCB6FEDC" box="[530,858,324,349]" italics="true" pageId="4" pageNumber="5">Preseucoela imallshookupis</emphasis>
Buffington
