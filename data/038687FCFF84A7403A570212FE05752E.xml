<quantity id="4CD79B0FFF86A740391E0512FE55752F" box="[364,431,786,811]" metricMagnitude="2" metricUnit="m" metricValue="3.51" pageId="18" pageNumber="822" unit="m" value="351.0">351 m</quantity>
