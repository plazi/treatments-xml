<taxonomicName id="7E8B4D65B7130E0E899CE15FFC584F19" authority="Budde-Lund, 1904" authorityName="Budde-Lund" authorityYear="1904" box="[660,1018,864,891]" class="Malacostraca" family="Detonidae" kingdom="Animalia" order="Isopoda" pageId="18" pageNumber="19" phylum="Arthropoda" rank="family">
