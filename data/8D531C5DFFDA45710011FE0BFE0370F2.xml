<taxonomicName id="C2FAD6C8FFDB45720356F9E4FDE972EE" authority="Papp, 2013" authorityName="Papp" authorityYear="2013" box="[336,602,1656,1678]" class="Insecta" family="Braconidae" genus="Mirax" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="118" phylum="Arthropoda" rank="species" species="ruptus" status="sp. n." subGenus="Centistidea">
