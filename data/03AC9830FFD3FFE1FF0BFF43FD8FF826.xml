<number id="3DCD3B01FFD3FFE1FDFEF96DFD3DF906" box="[610,659,1721,1745]" pageId="9" pageNumber="10" value="7.7">7.70</number>
(
<number id="3DCD3B01FFD3FFE1FD3EF96DFD79F906" box="[674,727,1721,1745]" pageId="9" pageNumber="10" value="2.45">2.45</number>
, 
<number id="3DCD3B01FFD3FFE1FD7EF96DFCB8F906" box="[738,790,1721,1745]" pageId="9" pageNumber="10" value="2.7">2.70</number>
, 
<number id="3DCD3B01FFD3FFE1FCBDF96DFCFBF906" box="[801,853,1721,1745]" pageId="9" pageNumber="10" value="1.8">1.80</number>
, 
<number id="3DCD3B01FFD3FFE1FCFCF96DFC38F906" box="[864,918,1721,1745]" pageId="9" pageNumber="10" value="0.75">0.75</number>
