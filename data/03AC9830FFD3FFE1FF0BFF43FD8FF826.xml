<number id="3DCD3B01FFD3FFE1FBD1F96DFB2FF906" box="[1101,1153,1721,1745]" pageId="9" pageNumber="10" value="2.6">2.60</number>
, 
<number id="3DCD3B01FFD3FFE1FB10F96DFB6EF906" box="[1164,1216,1721,1745]" pageId="9" pageNumber="10" value="1.9">1.90</number>
