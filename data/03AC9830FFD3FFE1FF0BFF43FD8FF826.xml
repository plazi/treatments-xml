<number id="3DCD3B01FFD3FFE1FC51F96DFC50F906" box="[973,1022,1721,1745]" pageId="9" pageNumber="10" value="7.75">7.75</number>
(
<number id="3DCD3B01FFD3FFE1FB91F96DFBECF906" box="[1037,1090,1721,1745]" pageId="9" pageNumber="10" value="2.5">2.50</number>
