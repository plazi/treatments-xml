<quantity id="AD2569D83860FFB23A702938FEB0D470" box="[257,342,532,556]" metricMagnitude="1" metricUnit="m" metricValue="3.65" metricValueMax="7.3" metricValueMin="0.0" pageId="19" pageNumber="20" unit="m" value="36.5" valueMax="73.0" valueMin="0.0">0–73 m</quantity>
