<geoCoordinate id="7E7859A7CBFBE0467B323D2467166DD5" direction="north" orientation="latitude" precision="925" value="10.433333">10°26'N</geoCoordinate>
, 
<geoCoordinate id="8E998856935B8753FA7532414A2AE1E2" direction="west" orientation="longitude" precision="925" value="-84.01667">84°01'W</geoCoordinate>
