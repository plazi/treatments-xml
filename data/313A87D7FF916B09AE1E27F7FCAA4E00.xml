<taxonomicName id="7E934D42FF916B0BAE1E27F7FD414D9E" authority="Lee and Patterson" authorityName="Lee and Patterson" authorityYear="2000" box="[291,743,710,734]" class="Imbricatea" family="Thaumatomonadidae" genus="Petalomonas" higherTaxonomySource="GBIF" kingdom="Chromista" order="Thaumatomonadida" pageId="37" pageNumber="518" phylum="Cercozoa" rank="species" species="intortus" status="n. sp.">
<emphasis id="8BE7EAD3FF916B0BAE1E27F7FDAB4D9E" bold="true" box="[291,525,710,734]" italics="true" pageId="37" pageNumber="518">Petalomonas intortus</emphasis>
Lee and Patterson
</taxonomicName>
, 
<taxonomicNameLabel id="90D457A8FF916B0BADC527F6FC914D9E" box="[760,823,711,734]" pageId="37" pageNumber="518" rank="species">n. sp.</taxonomicNameLabel>
