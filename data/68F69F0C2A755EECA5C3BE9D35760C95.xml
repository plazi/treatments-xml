<paragraph id="8B7205F1DB43350F48C0D89C0828872D" lastPageId="19" lastPageNumber="312" pageId="18" pageNumber="311">
<emphasis id="D293E7E8DA2FA6FEAF9A9F3A02128BCD" bold="true" pageId="18" pageNumber="311">New Brunswick, Albert Co.</emphasis>
, Caledonia Gorge P.N.A., 
<geoCoordinate id="966773BA3853443D32201B19B69B4EBC" degrees="45.8176" direction="north" orientation="latitude" precision="5" value="45.8176">45.8176°N</geoCoordinate>
