<quantity id="956A9B7E7232FFCEFF59608BFEE2FE38" box="[220,305,440,464]" metricMagnitude="3" metricUnit="m" metricValue="4.0" pageId="11" pageNumber="12" unit="m" value="4000.0">4000 m</quantity>
