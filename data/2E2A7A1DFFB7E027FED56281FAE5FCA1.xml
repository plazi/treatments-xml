<taxonomicName id="6183B088FFB8E027FD9163DEFC85FCD2" box="[588,844,764,789]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="staerae">
