<taxonomicName id="4C014D455F06FFCDFE2DEEADFCAF4938" authority="De Meyer, 1993" authorityName="De Meyer" authorityYear="1993" box="[388,768,1518,1545]" class="Insecta" family="Pipunculidae" genus="Tomosvaryella" kingdom="Animalia" order="Diptera" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="transvaalensis">
