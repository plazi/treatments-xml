<taxonomicName id="4C3E468FFFA90557FEEFFA05D55CFA76" authorityName="Assing" authorityYear="2016" box="[357,562,1508,1529]" class="Insecta" family="Staphylinidae" genus="Oxypoda" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="123" phylum="Arthropoda" rank="species" species="subplicata">
