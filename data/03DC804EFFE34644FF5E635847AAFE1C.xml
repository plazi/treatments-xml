<number id="3DBD237FFFE34645FE6261AC43C8F907" box="[427,510,1716,1737]" pageId="8" pageNumber="263" value="51.5" valueMax="54.0" valueMin="49.0">49 to 54</number>
; 
<number id="3DBD237FFFE34645FDC061AC4023F907" box="[521,533,1716,1737]" pageId="8" pageNumber="263" value="5.0">5</number>
