<paragraph id="8BCA3158FFE34645FF5E62B447AAF9D7" blockId="8.[151,1436,1158,2030]" pageId="8" pageNumber="263">
<keyLead id="3084926DFFE34645FF5E62B447AAF9D7" pageId="8" pageNumber="263">
<number id="3DBD237FFFE34645FF5E62B44319FA0F" box="[151,303,1452,1473]" pageId="8" pageNumber="263" value="29.740000000000002" valueMax="55.0" valueMin="4.48">4. 48 to 55</number>
