<number id="3DBD237FFFE34645FAA5611C47AAF9D7" box="[1388,1436,1540,1561]" pageId="8" pageNumber="263" value="1938.0">1938</number>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="8BCA3158FFE34645FF5E613947AAF965" blockId="8.[151,1436,1158,2030]" pageId="8" pageNumber="263">
<keyLead id="3084926DFFE34645FF5E613947AAF965" pageId="8" pageNumber="263">
