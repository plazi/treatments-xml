<taxonomicName id="4C2F0DB1E27FFFA1228FC573FE31A21F" authority="Roback, 1952" authorityName="Roback" authorityYear="1952" box="[151,469,258,280]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="168" phylum="Arthropoda" rank="species" species="piva">
