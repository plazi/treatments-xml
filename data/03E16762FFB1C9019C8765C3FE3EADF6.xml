<paragraph id="8BF7D674FFB1C9019C8765B6FDB8AD63" blockId="45.[151,1436,482,695]" pageId="45" pageNumber="46">
Bajmak 
<date id="FFF6F0B4FFB1C9019CEA65B6FE75AEBD" box="[250,431,482,508]" pageId="45" pageNumber="46" value="1998-06-17" valueMax="1998-06-18" valueMin="1998-06-17">17–18.VI.1998</date>
; Moskovo 
<date id="FFF6F0B4FFB1C9019E2365B6FD3DAEBD" box="[563,743,482,508]" pageId="45" pageNumber="46" value="1998-06-18" valueMax="1998-06-19" valueMin="1998-06-18">18–19.VI.1998</date>
, 
<date id="FFF6F0B4FFB1C9019EE465B6FC5CAEBD" box="[756,902,482,508]" pageId="45" pageNumber="46" value="1998-07-11">11.VII.1998</date>
; Verbljushka 
<date id="FFF6F0B4FFB1C901983765B6FB23AEBD" box="[1063,1273,482,508]" pageId="45" pageNumber="46" value="1998-05-30" valueMax="1998-06-02" valueMin="1998-05-30">30.V–02.VI.1998</date>
