<taxonomicName id="66664946965C5B3505C0C8E2EBE5C7AE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:192726" class="Insecta" family="Formicidae" genus="Prenolepis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Prenolepis vividula (Nylander)" lsidName-HNS="Prenolepis vividula (Nylander)" order="Hymenoptera" pageNumber="81" phylum="Arthropoda" rank="species" species="vividula">P. vividula Nyl</taxonomicName>
