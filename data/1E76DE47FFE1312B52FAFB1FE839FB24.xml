<subSubSection id="DEC53CDAFFE1312C52FAFAADEEB4FA47" box="[192,679,1402,1427]" pageId="11" pageNumber="10" type="etymology">
<paragraph id="96606F51FFE1312C52FAFAADEEB4FA47" blockId="11.[192,679,1402,1427]" box="[192,679,1402,1427]" pageId="11" pageNumber="10">
<emphasis id="A4ABB343FFE1312C52FAFAADED48FA46" bold="true" box="[192,347,1402,1426]" pageId="11" pageNumber="10">Etymology.</emphasis>
