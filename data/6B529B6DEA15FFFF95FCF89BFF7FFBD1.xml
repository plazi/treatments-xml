<quantity id="2403879EEA15FFFD9030FA3BFA58FA51" box="[1325,1414,1498,1523]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="1" pageNumber="2" unit="mm" value="1.3">1.3 mm</quantity>
; maximum width of the pronotum 
<quantity id="2403879EEA15FFFD9189FA1BFACDF9B1" box="[1172,1299,1530,1555]" metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="9.0" metricValueMin="8.0" pageId="1" pageNumber="2" unit="mm" value="0.85" valueMax="0.9" valueMin="0.8">0.8-0.9 mm</quantity>
