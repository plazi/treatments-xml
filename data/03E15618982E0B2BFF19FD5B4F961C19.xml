<taxonomicName id="4C489C8D982E0B2BFF19FCDB4FA61979" authority="Pacheco 1964: 75" authorityName="Pacheco" authorityPageNumber="75" authorityYear="1964" box="[192,741,802,826]" class="Insecta" family="Heteroceridae" genus="Neoheterocerus" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="20" phylum="Arthropoda" rank="species" species="sandersoni">
