<taxonomicName id="BAF84D70FFB6FF81FEFB9C2525747F10" authorityName=": Schwertner &amp; Grazia" authorityYear="2007" box="[346,518,1584,1608]" class="Insecta" family="Pentatomidae" genus="Chinavia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="448" phylum="Arthropoda" rank="species" species="pengue">
