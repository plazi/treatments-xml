9. 
<taxonomicName id="4C5E4D2AFF91FF8B4BBFFBF87A61FBF1" authority="Liebherr" authorityName="Liebherr" authorityYear="2017" box="[732,1063,1073,1094]" class="Insecta" family="Carabidae" genus="Mecyclothorax" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="695" phylum="Arthropoda" rank="species" species="gressitti" status="new species">
