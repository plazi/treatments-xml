<taxonomicName id="3874BA8767857C09CEE613DB0FE86FD2" authority="L." class="Liliopsida" family="Poaceae" genus="Lolium" kingdom="Plantae" order="Poales" pageId="null" pageNumber="373" phylum="Tracheophyta" rank="species" species="temulentum">
