<normalizedToken id="CB39C754E40375BA924A92155E42A4FC" originalValue="temuléntum" pageId="null" pageNumber="373">temulentum</normalizedToken>
<authorityName id="BF6D984D1803E19BE03DAF0D501265FA" pageId="null" pageNumber="373">L.</authorityName>
