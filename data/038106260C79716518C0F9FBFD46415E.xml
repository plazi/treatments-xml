<subSubSection id="C332E4BB0C79716718C0F9FBFDC64421" box="[151,550,1579,1606]" pageId="16" pageNumber="59" type="nomenclature">
<paragraph id="8B97B7300C79716718C0F9FBFDC64421" blockId="16.[151,550,1579,1606]" box="[151,550,1579,1606]" pageId="16" pageNumber="59">
<heading id="D0DF005C0C79716718C0F9FBFDC64421" bold="true" box="[151,550,1579,1606]" fontSize="11" level="1" pageId="16" pageNumber="59" reason="1">
<emphasis id="B95C6B220C79716718C0F9FBFDC64421" bold="true" box="[151,550,1579,1606]" pageId="16" pageNumber="59">
Genus 
<taxonomicName id="4C28CCB30C79716718B9F9FCFE2C4421" authority="Mitov &amp; Perkovsky &amp; Dunlop, 2021" authorityName="Mitov &amp; Perkovsky &amp; Dunlop" authorityYear="2021" box="[238,460,1579,1606]" class="Arachnida" family="Nemastomatidae" genus="Parahistricostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="16" pageNumber="59" phylum="Arthropoda" rank="genus" status="gen. n.">
<emphasis id="B95C6B220C79716718B9F9FCFE2C4421" bold="true" box="[238,460,1579,1606]" italics="true" pageId="16" pageNumber="59">Parahistricostoma</emphasis>
</taxonomicName>
