<number id="3DBBA4126513286FD53DFAE2FBC08B47" box="[1034,1151,1336,1361]" pageId="41" pageNumber="42" value="2.013025E7">20130250</number>
); Kantang, Trang Prov. (
<collectionCode id="ED622EF06513286FD1A8FA86FF458B62" LSID="urn:lsid:biocol.org:col:34992" box="[159,250,1372,1396]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="41" pageNumber="42">RMNH</collectionCode>
). 
<collectingCountry id="F364F6A56513286FD025FA81FEC28B62" box="[274,381,1371,1396]" name="Malaysia" pageId="41" pageNumber="42">Malaysia</collectingCountry>
