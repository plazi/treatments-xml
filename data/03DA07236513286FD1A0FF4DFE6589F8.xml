1887.3.10.24; 
<collectionCode id="ED622EF06513286FD478FACFFA2F8B3B" box="[1359,1424,1301,1325]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="41" pageNumber="42">ZMB</collectionCode>
). 
<collectingCountry id="F364F6A56513286FD1A0FAE2FF408B47" box="[151,255,1336,1361]" name="Thailand" pageId="41" pageNumber="42">Thailand</collectingCountry>
: Ranong (
<collectionCode id="ED622EF06513286FD0B7FAE2FE6C8B46" LSID="urn:lsid:biocol.org:col:34871" box="[384,467,1336,1360]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="41" pageNumber="42">USNM</collectionCode>
<number id="3DBBA4126513286FD0D4FAE2FD838B47" box="[483,572,1336,1361]" pageId="41" pageNumber="42" value="777649.0">777649</number>
