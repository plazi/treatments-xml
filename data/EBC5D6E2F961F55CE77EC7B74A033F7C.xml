<taxonomicName id="43A4A9CF5AEAC8ACC1DA72E4E43C35C1" authority="C.L.Koch, 1836" authorityName="C.L.Koch" authorityYear="1836" class="Arachnida" family="Ameroseiidae" genus="Laelaps" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Laelaps ampullula" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="ampullula">Laelaps ampullula</taxonomicName>
