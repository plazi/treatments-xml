<paragraph id="E67830A7439F48520BA2B3024B10A347" pageId="0" pageNumber="1">
<taxonomicName id="B65932FF9A75DB5A4773C7FB7780209E" authority="Vitzthum, 1942" authorityName="Vitzthum" authorityYear="1942" class="Arachnida" family="Ameroseiidae" genus="Neocypholaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Neocypholaelaps" order="Mesostigmata" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">Neocypholaelaps</taxonomicName>
