and 
<taxonomicName id="1E9D4D0E1D62392FFDDAFA27FD4DFA31" box="[515,668,1412,1435]" class="Insecta" family="Geometridae" genus="Prasinocyma" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="stictimargo">
