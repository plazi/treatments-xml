</bibRefCitation>
</taxonomicName>
from 
<taxonomicName id="4C7BA1B7FF800F4D5FA5FEE9FEAFFECE" class="Aves" family="Laridae" genus="Sterna" kingdom="Animalia" order="Charadriiformes" pageId="11" pageNumber="62" phylum="Chordata" rank="species" species="nilotica">
