<taxonomicName id="4C654D27981D0069BF3E69C83EF1FE0D" baseAuthorityName="Chen &amp; Toda" baseAuthorityYear="1998" box="[1032,1171,385,408]" class="Insecta" family="Drosophilidae" genus="Phortica" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="28" phylum="Arthropoda" rank="species" species="biprotrusa">
<emphasis id="B911EAB6981D0069BF3E69C83E7AFE0D" box="[1032,1048,385,408]" italics="true" pageId="5" pageNumber="28">P</emphasis>
. 
<emphasis id="B911EAB6981D0069BF1269C83EF1FE0D" box="[1060,1171,385,408]" italics="true" pageId="5" pageNumber="28">biprotrusa</emphasis>
</taxonomicName>
