<taxonomicName id="B45048898DE053CF222B106DE88AC3ED" authorityName="Nupponen" authorityYear="2022" class="Insecta" family="Scythrididae" genus="Scythris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scythris sanfranciscoensis" order="Lepidoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="species" species="sanfranciscoensis">
