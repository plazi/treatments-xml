in 
<taxonomicName id="E3F2F96A52C1B881E43C7433DE50DFF3" authorityName="Nupponen 1 &amp; Sihvonen" authorityYear="2022" class="Insecta" family="Scythrididae" genus="Scythris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scythris" order="Lepidoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="genus">
