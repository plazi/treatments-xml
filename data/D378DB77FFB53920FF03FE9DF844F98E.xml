</geoCoordinate>
, 
<geoCoordinate id="3EE50CA6FFB53920FDCDFB25FE9EFB33" box="[601,716,1216,1245]" direction="east" orientation="longitude" pageId="1" pageNumber="62" precision="925" value="130.41667">
<number id="ED197846FFB53920FDCDFB25FEDAFB33" box="[601,648,1219,1245]" pageId="1" pageNumber="62" value="130.0">130</number>
o 
<number id="ED197846FFB53920FD05FB25FEE1FB33" box="[657,691,1219,1245]" pageId="1" pageNumber="62" value="25.0">25</number>
'E
</geoCoordinate>
, 
<quantity id="9C29C784FFB53920FD4FFB25FF3AFB33" box="[731,872,1219,1245]" metricMagnitude="2" metricUnit="m" metricValue="2.7" metricValueMax="2.75" metricValueMin="2.65" pageId="1" pageNumber="62" unit="m" value="270.0" valueMax="275.0" valueMin="265.0">265–275 m</quantity>
depth). They have 
<number id="ED197846FFB53920FBF4FB25F82FFB33" box="[1120,1149,1219,1245]" pageId="1" pageNumber="62" value="19.0">19</number>
/ 
<number id="ED197846FFB53920FB13FB25F8F5FB33" box="[1159,1191,1219,1245]" pageId="1" pageNumber="62" value="78.0">78</number>
/ 
<number id="ED197846FFB53920FB25FB25F88DFB33" box="[1201,1247,1219,1245]" pageId="1" pageNumber="62" value="157.0">157</number>
and 
<number id="ED197846FFB53920FA8AFB25F96CFB33" box="[1310,1342,1219,1245]" pageId="1" pageNumber="62" value="20.0">20</number>
/ 
<number id="ED197846FFB53920FAD3FB25F934FB33" box="[1351,1382,1219,1245]" pageId="1" pageNumber="62" value="78.0">78</number>
/ 
