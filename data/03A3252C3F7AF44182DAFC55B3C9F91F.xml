<number id="3DC2861D3F7AF4418032FB24B4E2FB2D" box="[639,695,1164,1189]" pageId="6" pageNumber="499" value="1952.0">1952</number>
(Holarctic)
</taxonomicName>
; 
<taxonomicName id="4C0AEFB93F7AF441811FFB25B366FB2D" authority="Balogh et Mahunka, 1965" authorityName="Balogh et Mahunka" authorityYear="1965" box="[850,1331,1164,1189]" class="Arachnida" family="Tegoribatidae" genus="Lepidozetes" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="499" phylum="Arthropoda" rank="species" species="dashidorzsi">
