, 
<taxonomicName id="DCC998B88D78FFB0B359F9DDAF50AD90" authority="Bryk, 1953: 214" authorityName="Bryk" authorityPageNumber="214" authorityYear="1953" class="Insecta" family="Erebidae" genus="Agaraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="422" phylum="Arthropoda" rank="species" species="nigrotuberculata">
