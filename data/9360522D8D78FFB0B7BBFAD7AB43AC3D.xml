<taxonomicName id="DCC998B88D78FFB0B6A6F915AC32ADF5" box="[394,618,1724,1749]" class="Insecta" family="Erebidae" genus="Agaraea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="422" phylum="Arthropoda" rank="species" species="nigrotuberculata">
