<location id="1E16B5E08D78FFB0B5C1F93EAD38AD8F" LSID="urn:lsid:plazi:treatment:9360522D8D78FFB0B7BBFAD7AB43AC3D:1E16B5E08D78FFB0B5C1F93EAD38AD8F" box="[749,864,1687,1712]" country="Brazil" county="For" municipality="Despite" name="Rio Purus" pageId="1" pageNumber="422" stateProvince="Amazonas">Rio Purus</location>
