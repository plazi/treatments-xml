<taxonomicName id="4C0FD3ECC216FFECFD59FF92FBF712C0" authority="McIntosh, 1885" authorityName="McIntosh" authorityYear="1885" box="[726,1107,732,758]" class="Polychaeta" family="Polynoidae" genus="Harmothoe" kingdom="Animalia" order="Phyllodocida" pageId="53" pageNumber="54" phylum="Annelida" rank="species" species="benthaliana">
<emphasis id="B97B747DC216FFECFD59FF92FC2F12C3" box="[726,907,732,757]" italics="true" pageId="53" pageNumber="54">H. benthaliana</emphasis>
