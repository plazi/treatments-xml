<td id="0F5B5C04326F8BA57F7D7E198A494BBC" colspan="1" pageId="9" pageNumber="10" rowspan="1">53.0</td>
<td id="70E0BFBE457606F5A4369F71E69ABC4D" colspan="1" pageId="9" pageNumber="10" rowspan="1">52.5</td>
