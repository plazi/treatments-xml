<paragraph id="04B990313FFBA49A41D661E1128CB7A7" pageId="0" pageNumber="23">
Genus 
<taxonomicName id="A556FB70C9F8CC8B55A733BFB2B37DE6" LSID="98A1F3ED-994A-5804-A61D-6FCCB2ABC0E8" authority="Kim &amp; Lee, 2020" authorityName="Kim &amp; Lee" authorityYear="2020" class="Hexanauplia" family="Nannopodidae" genus="Concilicoxa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Concilicoxa" order="Harpacticoida" pageId="0" pageNumber="23" phylum="Arthropoda" rank="genus" status="gen. nov.">Concilicoxa</taxonomicName>
