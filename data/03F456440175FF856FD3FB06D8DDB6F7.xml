<collectingCountry id="F34AA7C2016BFF856E34FDDDD98AB699" box="[368,438,630,656]" name="China" pageId="80" pageNumber="81">China</collectingCountry>
(
<collectingRegion id="499929B0016BFF856E82FDDDDA2BB699" box="[454,535,630,656]" country="China" name="Fujian" pageId="80" pageNumber="81">Fujian</collectingRegion>
), 
<collectingCountry id="F34AA7C2016BFF856D6FFDDDDAAEB699" box="[555,658,630,656]" name="Vietnam" pageId="80" pageNumber="81">Vietnam</collectingCountry>
