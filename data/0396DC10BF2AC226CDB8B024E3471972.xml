<paragraph id="8B806D06BF2AC226CDB8B024E55F1A1B" blockId="37.[224,626,1553,1610]" pageId="37" pageNumber="190">
<heading id="D0C8DA6ABF2AC226CDB8B024E55F1A1B" centered="true" fontSize="8" level="2" pageId="37" pageNumber="190" reason="2">
<taxonomicName id="4C3F1685BF2AC226CDB8B024E55F1A1B" authority="E. M. FRIIS, P. R. CRANE et K. R. PEDERSEN, 2019" authorityName="E. M. FRIIS, P. R. CRANE et K. R. PEDERSEN" authorityYear="2019" class="Lycopodiopsida" family="Selaginellaceae" genus="Serialis" kingdom="Plantae" order="Selaginellales" pageId="37" pageNumber="190" phylum="Tracheophyta" rank="species" species="parva">
