<emphasis id="A1CD0B29FFCEBA4CFF7A71B5F21BFC1F" bold="true" box="[199,315,925,949]" pageId="6" pageNumber="65">Remarks.</emphasis>
<taxonomicName id="54B9ACB8FFCEBA4CFEF971B5F13CFC1F" box="[324,540,925,949]" class="Insecta" family="Tanyderidae" genus="Macrochile" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="baltica">
