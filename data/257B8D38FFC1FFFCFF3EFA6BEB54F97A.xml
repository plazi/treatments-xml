<taxonomicName id="6AD247ADFFC1FFFCFEBBF9DFE8EAF9CE" authorityName="Engleman" authorityYear="1983" box="[274,497,1555,1581]" class="Insecta" family="Pentatomidae" genus="Antiteuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="graziae">
