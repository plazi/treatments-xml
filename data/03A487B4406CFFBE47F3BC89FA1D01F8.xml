<taxonomicName id="4C0D4D21406CFFBE4546B9BDFC9103A5" box="[541,774,1674,1697]" class="Insecta" family="Mantispidae" genus="Haematomantispa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="277" phylum="Arthropoda" rank="species" species="sp.">
