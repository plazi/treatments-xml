<taxonomicName id="4C2C4D0C4206FFD098F178C2AD6DFD64" authorityName="Richardson" authorityYear="1914" box="[900,1027,701,722]" class="Malacostraca" family="Scleropactidae" genus="Sphaeroniscus" kingdom="Animalia" order="Isopoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="frontalis">
<emphasis id="B958EA9D4206FFD098F178C2AD6DFD64" box="[900,1027,701,722]" italics="true" pageId="58" pageNumber="59">S. frontalis</emphasis>
</taxonomicName>
as 
<taxonomicName id="4C2C4D0C4206FFD09F5878C2AD96FD64" authorityName="Gerstacker" authorityYear="1854" box="[1069,1272,701,722]" class="Malacostraca" family="Scleropactidae" genus="Sphaeroniscus" kingdom="Animalia" order="Isopoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="flavomaculatus">
<emphasis id="B958EA9D4206FFD09F5878C2AD96FD64" box="[1069,1272,701,722]" italics="true" pageId="58" pageNumber="59">S. flavomaculatus</emphasis>
</taxonomicName>
