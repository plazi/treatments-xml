<taxonomicName id="4C2C4D0C4206FFD09E70783DAD6BFDC0" authorityName="Gerstacker" authorityYear="1854" class="Malacostraca" family="Scleropactidae" genus="Sphaeroniscus" kingdom="Animalia" order="Isopoda" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="flavomaculatus">
<emphasis id="B958EA9D4206FFD09E70783DAD6BFDC0" italics="true" pageId="58" pageNumber="59">Sphaeroniscus flavomaculatus</emphasis>
</taxonomicName>
