<taxonomicName id="CC884D7CFF18FF51FCCA6085FBF9FD3E" authorityName="Bush" authorityYear="2017" box="[809,1072,644,669]" class="Insecta" family="Philopteridae" genus="Indoceoplanetes" kingdom="Animalia" order="Phthiraptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="loboccupatrix" subGenus="Capnodella">
<emphasis id="39FCEAEDFF18FF51FCCA6085FC89FD3F" box="[809,832,645,668]" italics="true" pageId="146" pageNumber="147">In</emphasis>
. (
<emphasis id="39FCEAEDFF18FF51FCB86085FC4BFD3F" box="[859,898,645,668]" italics="true" pageId="146" pageNumber="147">Cp.</emphasis>
) 
<emphasis id="39FCEAEDFF18FF51FC766084FBF9FD3E" box="[917,1072,644,669]" italics="true" pageId="146" pageNumber="147">loboccupatrix</emphasis>
</taxonomicName>
; sternal plates III–VI and male subgenital plate translucent in 
<taxonomicName id="CC884D7CFF18FF51FE0960A9FD22FD63" authorityName="Bush" authorityYear="2017" box="[490,747,679,704]" class="Insecta" family="Philopteridae" genus="Indoceoplanetes" kingdom="Animalia" order="Phthiraptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="laurocorythes" subGenus="Capnodella">
