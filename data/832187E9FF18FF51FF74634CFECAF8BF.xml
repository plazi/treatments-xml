<taxonomicName id="CC884D7CFF18FF51FAD4603DFEFAFDDB" authorityName="Bush" authorityYear="2017" class="Insecta" family="Philopteridae" genus="Indoceoplanetes" kingdom="Animalia" order="Phthiraptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="loboccupatrix" subGenus="Capnodella">
