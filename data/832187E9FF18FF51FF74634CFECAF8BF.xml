<taxonomicName id="CC884D7CFF18FF51FBEA6061FAC4FDDB" authorityName="Bush" authorityYear="2017" box="[1033,1293,607,632]" class="Insecta" family="Philopteridae" genus="Indoceoplanetes" kingdom="Animalia" order="Phthiraptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="species" species="laurocorythes" subGenus="Capnodella">
