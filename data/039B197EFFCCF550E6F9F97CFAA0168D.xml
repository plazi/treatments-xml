<quantity id="4CCA058DFFCBF550E518FD77FC61168D" box="[886,1016,692,716]" metricMagnitude="2" metricUnit="m" metricValue="6.645" metricValueMax="12.8" metricValueMin="0.49" pageId="20" pageNumber="21" unit="m" value="664.5" valueMax="1280.0" valueMin="49.0">49–1280 m</quantity>
