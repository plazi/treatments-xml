<paragraph id="37238EBE5B45B3801DBFCD219A24E56E" pageId="0" pageNumber="113">
Genus 
<taxonomicName id="855C3AD5DBC59734DC4A27E1966587A4" LSID="95E5D21B-2A5D-5474-861C-5DFFBEAD1E4A" authority="Liu, 2020" authorityName="Liu" authorityYear="2020" class="Insecta" family="Phoridae" genus="Sinogodavaria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sinogodavaria" order="Diptera" pageId="0" pageNumber="113" phylum="Arthropoda" rank="genus" status="gen. nov.">Sinogodavaria</taxonomicName>
