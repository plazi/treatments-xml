<collectingCountry id="F316760FFF1C99EEF067FA2EFE62FA6A" box="[359,426,1413,1439]" name="China" pageId="144" pageNumber="145">China</collectingCountry>
(
<collectingRegion id="49C5F87DFF1C99EEF0B9FA2EFDE8FA55" box="[441,544,1413,1440]" country="China" name="Guangxi" pageId="144" pageNumber="145">Guangxi</collectingRegion>
