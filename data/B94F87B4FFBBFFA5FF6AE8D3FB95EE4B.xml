<emphasis id="0392EAB0FFB8FFA3FD91EDF3FD04ECE1" box="[627,766,974,995]" italics="true" pageId="2" pageNumber="422">T. yunnanense</emphasis>
</taxonomicName>
<emphasis id="0392EAB0FFB8FFA3FCE4EDF3FCAFECE1" bold="true" box="[774,853,974,995]" pageId="2" pageNumber="422">
<taxonomicNameLabel id="18A157CBFFB8FFA3FCE4EDF3FCAFECE1" box="[774,853,974,995]" pageId="2" pageNumber="422" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype. 2. 
<taxonomicName id="F6E64D21FFB8FFA3FC03EDF3FBA3ECE1" baseAuthorityName="Sinev" baseAuthorityYear="1986" box="[993,1113,974,995]" class="Insecta" family="Cossidae" genus="Holcoceroides" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="422" phylum="Arthropoda" rank="species" species="scythrella">
<emphasis id="0392EAB0FFB8FFA3FC03EDF3FBA3ECE1" box="[993,1113,974,995]" italics="true" pageId="2" pageNumber="422">T. scythrella</emphasis>
</taxonomicName>
. 3. 
<taxonomicName id="F6E64D21FFB8FFA3FB63EDF3FAC1ECE1" authority="Teng &amp; Wang, 2018" authorityName="Teng &amp; Wang" authorityYear="2018" box="[1153,1339,974,995]" class="Insecta" family="Blastobasidae" genus="Tecmerium" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="422" phylum="Arthropoda" rank="species" species="rectimarginatum" status="sp. nov.">
<emphasis id="0392EAB0FFB8FFA3FB63EDF3FAC1ECE1" box="[1153,1339,974,995]" italics="true" pageId="2" pageNumber="422">T. rectimarginatum</emphasis>
</taxonomicName>
<emphasis id="0392EAB0FFB8FFA3FAA1EDF3FA68ECE1" bold="true" box="[1347,1426,974,995]" pageId="2" pageNumber="422">
<taxonomicNameLabel id="18A157CBFFB8FFA3FAA1EDF3FA68ECE1" box="[1347,1426,974,995]" pageId="2" pageNumber="422" rank="species">sp. nov.</taxonomicNameLabel>
