<quantity id="4CEC96DBFF8FFF97D5B2BAD2FE842DE5" box="[235,322,1154,1178]" metricMagnitude="3" metricUnit="m" metricValue="7.2" pageId="16" pageNumber="15" unit="km" value="7.2">7.2 km</quantity>
