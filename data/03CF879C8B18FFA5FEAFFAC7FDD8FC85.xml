<taxonomicName id="4C664D098B16FFA5FE14FDE7FD50FDCE" authorityName="Sun" authorityYear="2003" box="[435,649,604,629]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="88" pageNumber="89" phylum="Arthropoda" rank="species" species="japanensis">
