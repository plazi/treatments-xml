<taxonomicName id="4C414D1DFFF5137D3EB3FB205BF1FB26" authority="Mordukhovich &amp; Zograf &amp; Saulenko &amp; Fadeeva, 2020" authorityName="Mordukhovich &amp; Zograf &amp; Saulenko &amp; Fadeeva" authorityYear="2020" box="[238,424,1161,1188]" class="Enoplea" family="Oncholaimidae" genus="Oloncholaimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="2" pageNumber="558" phylum="Nematoda" rank="genus" status="gen. nov.">
<emphasis id="B935EA8CFFF5137D3EB3FB205BF1FB26" bold="true" box="[238,424,1161,1188]" italics="true" pageId="2" pageNumber="558">Oloncholaimus</emphasis>
