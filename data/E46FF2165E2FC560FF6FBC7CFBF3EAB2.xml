<taxonomicName id="ABC638835E2FC560FDD4BBCDFD0CE99D" authorityName="Ballantyne &amp; Lambkin" authorityYear="2013" box="[556,730,1246,1270]" class="Insecta" family="Lampyridae" genus="Australoluciola" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
