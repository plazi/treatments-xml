<location id="8EC8C6E8FFE1560DFB49CB1FFABEFDDA" LSID="urn:lsid:plazi:treatment:03BE2125FFE1560FFCC7C86FFE88FEDE:8EC8C6E8FFE1560DFB49CB1FFABEFDDA" box="[1188,1349,518,544]" country="Myanmar" county="Male" municipality="Antennae" name="Palaeontology" pageId="1" pageNumber="332" stateProvince="Kachin">Palaeontology</location>
