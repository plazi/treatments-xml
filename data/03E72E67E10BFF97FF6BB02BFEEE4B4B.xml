<quantity id="4CB63294E10BFF88FC7AB3BEFC364C8C" box="[902,1006,690,715]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="15" pageNumber="206" unit="mm" value="0.55">0.55 mm</quantity>
