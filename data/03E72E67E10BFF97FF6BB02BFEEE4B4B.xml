<subSubSection id="C354CCFAE10BFF97FF3BB3BEFC144CEE" lastPageId="16" lastPageNumber="207" pageId="15" pageNumber="206" type="description">
<paragraph id="8BF19F71E10BFF88FF3BB3BEFB3E4CA8" blockId="15.[151,1437,402,2011]" pageId="15" pageNumber="206">
<emphasis id="B93A4363E10BFF88FF3BB3BEFE894C8D" bold="true" box="[199,337,690,715]" pageId="15" pageNumber="206">Description</emphasis>
. Size: 
<typeStatus id="54F521D3E10BFF88FE63B3BEFDD44C8D" box="[415,524,690,715]" pageId="15" pageNumber="206">Holotype</typeStatus>
