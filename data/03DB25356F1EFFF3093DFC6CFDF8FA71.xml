<taxonomicName id="4C72EFA06F1EFFF3093DFC6CFC07FC3A" authorityName="Marshall" authorityYear="2019" baseAuthorityName="Hennig" baseAuthorityYear="1935" box="[636,951,940,967]" class="Insecta" family="Micropezidae" genus="Mesoconius" kingdom="Animalia" order="Diptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="speciesGroup" species="oblitus" speciesGroup="oblitus">
