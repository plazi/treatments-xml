<paragraph id="F2DD530C1CA6532F01D8BF196108AC57" pageId="2" pageNumber="117">
<taxonomicName id="5F3F960F31F45B28E416D68E672ABAB9" authority="Tanaka, 2016" authorityName="Tanaka" authorityYear="2016" class="Insecta" family="Pseudococcidae" genus="Ishigakicoccus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ishigakicoccus shimadai" order="Hemiptera" pageId="2" pageNumber="117" phylum="Arthropoda" rank="species" species="shimadai">Ishigakicoccus shimadai</taxonomicName>
