<paragraph id="3B45D14382D2701C0D963674007D5382" pageId="1" pageNumber="42">
<taxonomicName id="B1A82AA98A3A9224B91249B81C51B701" class="Insecta" family="Aphididae" genus="Aleurodaphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aleurodaphis blumae" order="Hemiptera" pageId="1" pageNumber="42" phylum="Arthropoda" rank="species" species="blumae">Aleurodaphis blumae</taxonomicName>
