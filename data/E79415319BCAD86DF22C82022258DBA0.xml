<taxonomicName id="3730F55F29E394AADDD8DD3C4E735C13" LSID="urn:lsid:zoobank.org:act:4C4BD7CE-586B-4142-BA7A-CC22284E094B" authority="López-Estrada &amp; Zaldívar-Riverón" authorityName="Lopez-Estrada &amp; Zaldivar-Riveron" authorityYear="2012" class="Insecta" family="Braconidae" genus="Notiospathius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Notiospathius venezuelae" order="Hymenoptera" pageId="14" pageNumber="51" phylum="Arthropoda" rank="species" species="venezuelae" status="sp. n.">
