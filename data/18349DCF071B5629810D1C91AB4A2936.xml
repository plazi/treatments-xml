<typeStatus id="EC25EAE366DE4C7A7360DA94B25C3464">holotype</typeStatus>
, ventral view 
<emphasis id="A044F18F5DA9606C8D39F14BF978B5B2" bold="true" pageId="0" pageNumber="127">C</emphasis>
<typeStatus id="A93EEF63620BA634189F60CE85DB3AA2">paratype</typeStatus>
, dorsal view 
<emphasis id="25D8E94ABA68B78E5DA7E0ADA57B5A2E" bold="true" pageId="0" pageNumber="127">D</emphasis>
