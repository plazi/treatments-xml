<collectingRegion id="34212CBE234EFFD6FDEBFC74AA39FBD3" box="[532,639,1015,1040]" country="Argentina" name="Mendoza" pageId="5" pageNumber="46">Mendoza</collectingRegion>
, 
<location id="F33AB487234EFFD6FD74FC74AA89FBD3" LSID="urn:lsid:plazi:treatment:7E4C534A2348FFD6FF68FA24AD11F993:F33AB487234EFFD6FD74FC74AA89FBD3" box="[651,719,1015,1040]" county="Miller" municipality="Gimpel" name="Lujan" pageId="5" pageNumber="46" stateProvince="Mendoza">Luján</location>
, 
<location id="F33AB487234EFFD6FD23FC7BABCCFBD3" LSID="urn:lsid:plazi:treatment:7E4C534A2348FFD6FF68FA24AD11F993:F33AB487234EFFD6FD23FC7BABCCFBD3" box="[732,906,1015,1040]" county="Miller" municipality="Gimpel" name="Los Papagallos" pageId="5" pageNumber="46" stateProvince="Mendoza">Los Papagallos</location>
