<typeStatus id="295E5CFE234EFFD6FC1DFC7BAC54FBD3" box="[994,1042,1016,1040]" pageId="5" pageNumber="46">type</typeStatus>
locality of 
<taxonomicName id="31E599DF234EFFD6FB69FC7AAD0DFBD3" box="[1174,1355,1016,1040]" class="Insecta" family="Eriococcidae" genus="Eriococcus" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="46" phylum="Arthropoda" rank="species" species="diversispinus">
<emphasis id="C4913E4E234EFFD6FB69FC7AAD0DFBD3" box="[1174,1355,1016,1040]" italics="true" pageId="5" pageNumber="46">E. diversispinus</emphasis>
</taxonomicName>
