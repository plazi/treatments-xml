<subSubSection id="4EC5C062E68CBD8C06BEFA76F2144940" pageId="28" pageNumber="61" type="reference_group">
<paragraph id="8234505E9D575BC60CCA4B439107891D" pageId="28" pageNumber="61">
<taxonomicName id="3D06783C63F77935C324649859A1ECBC" authority="" authorityName="" class="Gastropoda" family="Triphoridae" genus="Triforis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Triforis tricincta" order="Caenogastropoda" pageId="28" pageNumber="61" phylum="Mollusca" rank="species" species="tricincta">
<pageBreakToken id="C9034AB9CC9D73237FCDC896AB65A4FE" pageId="28" pageNumber="61" start="start">Triforis</pageBreakToken>
