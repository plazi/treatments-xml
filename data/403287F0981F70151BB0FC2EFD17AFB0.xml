Genus 
<taxonomicName id="0F9B4D65981F70151BC9FC2EFE90A8C0" authorityName="Last, Naylor &amp; Manjaji-Matsumoto" authorityYear="2016" box="[238,363,994,1021]" class="Elasmobranchii" family="Dasyatidae" genus="Pateobatis" kingdom="Animalia" order="Myliobatiformes" pageId="8" pageNumber="219" phylum="Chordata" rank="genus">
