= 
<accessionNumber id="5A9519D3FFCA2529438BFDA7FAD524B9" box="[1164,1299,628,652]" httpUri="http://www.ncbi.nlm.nih.gov/protein/LMG31724" pageId="17" pageNumber="18">LMG 31724</accessionNumber>
<superScript id="B2B32978FFCA25294214FDA0FADB24B4" attach="none" box="[1299,1309,627,641]" fontSize="6" pageId="17" pageNumber="18">T</superScript>
