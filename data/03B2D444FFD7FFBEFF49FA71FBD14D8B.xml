<collectingCountry id="F30C25C2FFD7FFBDFCD5F9C6FCF94909" box="[779,885,1553,1578]" name="Malaysia" pageId="1" pageNumber="184">Malaysia</collectingCountry>
, [Borneo], 
<collectingCountry id="F30C25C2FFD7FFBDFC21F9C6FBEE4909" box="[1023,1122,1553,1578]" name="Malaysia" pageId="1" pageNumber="184">Sarawak</collectingCountry>
, route Kuching-Serian, nr. Kampong Kuap (ca. 
<quantity id="4CE3C8B7FFD7FFBDFE5EF9E1FE4B496D" box="[384,455,1589,1614]" metricMagnitude="4" metricUnit="m" metricValue="1.8" pageId="1" pageNumber="184" unit="km" value="18.0">18 km</quantity>
