<paragraph id="06852FDB6CA1713204EBB2084ADC830F" pageId="0" pageNumber="323">
<taxonomicName id="D9C269A7A19B0BECC560D37E43F86247" authorityName="Kononova" authorityYear="2001" class="Insecta" family="Scelionidae" genus="Gryon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gryon elegans" order="Hymenoptera" pageId="0" pageNumber="323" phylum="Arthropoda" rank="species" species="elegans">Gryon elegans</taxonomicName>
