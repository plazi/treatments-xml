<taxonomicName id="F3B2A2815C1F663DFD0D358FFC52D3FF" box="[698,905,985,1009]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="laeviceps">
