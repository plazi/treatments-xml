<subSubSection id="C330654AFFCFFFECFF7FFE063E39FE01" box="[199,1314,439,464]" pageId="11" pageNumber="84" type="etymology">
<paragraph id="8B9536C1FFCFFFECFF7FFE063E39FE01" blockId="11.[151,1437,151,464]" box="[199,1314,439,464]" pageId="11" pageNumber="84">
<emphasis id="B95EEAD3FFCFFFECFF7FFE063A5DFE01" bold="true" box="[199,326,439,464]" pageId="11" pageNumber="84">Etymology</emphasis>
