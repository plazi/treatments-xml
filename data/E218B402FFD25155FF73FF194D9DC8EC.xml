<quantity id="AD49A8F1FFD25155FE56FE0C4DFAC89C" box="[434,508,386,412]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="33" pageNumber="34" unit="m" value="200.0">200 m</quantity>
