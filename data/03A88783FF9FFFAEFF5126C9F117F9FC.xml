<typeStatus id="54BA8837FF9FFFAEFD382103F643F9FC" box="[766,814,1558,1582]" pageId="2" pageNumber="279">type</typeStatus>
locality, 
<collectingRegion id="49C5F877FF9FFFAEFC532103F6B6F9FF" box="[917,987,1558,1581]" country="Cambodia" name="Pursat" pageId="2" pageNumber="279">Pursat</collectingRegion>
