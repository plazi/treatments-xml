<emphasis id="A4A4EABA5C6369067100FDF3FDCB6E71" box="[534,631,582,601]" italics="true" pageId="25" pageNumber="9">
<taxonomicName id="51D04D2B5C6369067100FDF3FDCE6E71" box="[534,626,582,601]" class="Actinopterygii" family="Trichomycteridae" genus="Pygidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Siluriformes" pageId="25" pageNumber="9" phylum="Chordata" rank="genus">Pygidium</taxonomicName>
