<taxonomicName id="918B188AC524FFD9FBA9FD3CFB01FD27" box="[1069,1221,724,749]" class="Insecta" family="Gracillariidae" genus="Porphyrosela" kingdom="Animalia" order="Lepidoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="genus">
