<paragraph id="8BAE36BDFFD9FFF1FD1AFB60FB66FA92" blockId="13.[676,1209,1153,1388]" pageId="13" pageNumber="152">
Genus 
<taxonomicName id="4C114D3EFFD9FFF1FD57FB63FB61FB62" authority="Poyato-Ariza &amp; Wenz, 2000" authorityName="Poyato-Ariza &amp; Wenz" authorityYear="2000" box="[767,1195,1153,1180]" class="Actinopterygii" family="Pycnodontiformes (awaiting allocation)" genus="Stenamara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="13" pageNumber="152" phylum="Chordata" rank="genus">
