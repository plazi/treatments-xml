, 
<taxonomicName id="BEEE2CBCAD712533303C271EC3284E5F" class="Actinopterygii" family="Cichlidae" genus="Rocio" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="56" phylum="Chordata" rank="species" species="ocotal">R. ocotal</taxonomicName>
, 
<taxonomicName id="6E0C16294E2D8FD1CD18EAF537A21A8E" class="Actinopterygii" family="Cichlidae" genus="Rocio" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="56" phylum="Chordata" rank="species" species="gemmata">R. gemmata</taxonomicName>
