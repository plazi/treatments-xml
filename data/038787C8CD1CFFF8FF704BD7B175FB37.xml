<subSubSection id="C3346555CD1FFFF8FF204DFDB6DFFF7F" box="[199,1437,151,176]" pageId="22" pageNumber="23" type="distribution">
<paragraph id="8B9136DECD1FFFF8FF204DFDB6DFFF7F" blockId="22.[151,1437,151,1255]" box="[199,1437,151,176]" pageId="22" pageNumber="23">
<emphasis id="B95AEACCCD1FFFF8FF204DFDB222FF60" bold="true" box="[199,352,151,176]" pageId="22" pageNumber="23">Distribution.</emphasis>
Off 
<collectingCountry id="F339764ECD1FFFF8FE744DFDB113FF7F" box="[403,593,151,175]" name="Solomon Islands" pageId="22" pageNumber="23">Solomon Islands</collectingCountry>
, 
<quantity id="4CD69B3BCD1FFFF8FDBB4DFDB1A8FF7F" box="[604,746,151,175]" metricMagnitude="2" metricUnit="m" metricValue="7.2" metricValueMax="11.35" metricValueMin="3.05" pageId="22" pageNumber="23" unit="m" value="720.0" valueMax="1135.0" valueMin="305.0">305–1135 m</quantity>
