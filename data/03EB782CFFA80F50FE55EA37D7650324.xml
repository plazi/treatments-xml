, Kalimantan, 
<collectingRegion id="498607D8FFA90F50FE05E9F6D6490304" box="[511,634,507,531]" country="Malaysia" name="Sabah" pageId="13" pageNumber="624">Sabah state</collectingRegion>
. 
<taxonomicName id="4C42B2B9FFA90F50FD79E9F1D7AB0304" authority="rubricata" box="[643,920,508,531]" class="Insecta" family="Largidae" genus="Iphita" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="624" phylum="Arthropoda" rank="subSpecies" species="rubricata" subSpecies="rubricata">
