<taxonomicName id="4C4E4D4EFFC6052DFC32C30536A23763" class="Insecta" family="Heleomyzidae" genus="Heleomyza" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="2082" phylum="Arthropoda" rank="species" species="borealis">
