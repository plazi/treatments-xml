<taxonomicName id="0CC74D20BE7C555AFEBAFA63FDA54ECD" box="[330,545,1506,1531]" class="Actinopterygii" family="Lethrinidae" genus="Lethrinus" kingdom="Animalia" order="Perciformes" pageId="26" pageNumber="177" phylum="Chordata" rank="species" species="mahsena">
