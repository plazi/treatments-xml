<quantity id="61159B50FFC60206FE8EFAB0FE483D5F" box="[306,384,1354,1376]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="7" pageNumber="76" unit="mm" value="0.5">0.5 mm</quantity>
