<quantity id="007448CC4403FFC4A9C9FC400774FC1A" box="[994,1085,978,1005]" metricMagnitude="3" metricUnit="m" metricValue="2.035" pageId="4" pageNumber="49" unit="m" value="2035.0">2035 m</quantity>
