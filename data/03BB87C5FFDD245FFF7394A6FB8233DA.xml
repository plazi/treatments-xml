<subSubSection id="C3086558FFDD245FFF7394A6FCCE36AB" box="[151,840,655,682]" pageId="1" pageNumber="362" type="nomenclature">
<paragraph id="8BAD36D3FFDD245FFF7394A6FCCE36AB" blockId="1.[151,840,655,682]" box="[151,840,655,682]" pageId="1" pageNumber="362">
<heading id="D0E581BFFFDD245FFF7394A6FCCE36AB" bold="true" box="[151,840,655,682]" fontSize="11" level="1" pageId="1" pageNumber="362" reason="1">
<emphasis id="B966EAC1FFDD245FFF7394A6FCCE36AB" bold="true" box="[151,840,655,682]" pageId="1" pageNumber="362">
<taxonomicName id="4C124D50FFDD245FFF7394A6FE2536A8" box="[151,419,655,681]" class="Insecta" family="Mutillidae" genus="Dasymutilla" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="362" phylum="Arthropoda" rank="species" species="paradoxa">
<emphasis id="B966EAC1FFDD245FFF7394A6FE2536A8" bold="true" box="[151,419,655,681]" italics="true" pageId="1" pageNumber="362">Dasymutilla paradoxa</emphasis>
</taxonomicName>
