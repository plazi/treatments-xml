<materialsCitation id="3B6B3C98285BFFCCF7A0FAB0FEA3B54B" ID-GBIF-Occurrence="1503739224" country="Brazil" location="Sao Paulo" pageId="27" pageNumber="28" specimenCount="1" stateProvince="Sao Paulo">
