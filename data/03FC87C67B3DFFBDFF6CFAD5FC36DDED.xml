<collectingRegion id="4991F8327B3DFFA2FEE4F934FE3CDEED" box="[328,450,1763,1787]" country="United States of America" name="New York" pageId="15" pageNumber="14">New York</collectingRegion>
<collectingMunicipality id="6B8EACAA7B3DFFA2FE6BF933FE01DEED" box="[455,511,1763,1787]" pageId="15" pageNumber="14">City</collectingMunicipality>
, 
<collectingRegion id="4991F8327B3DFFA2FDA5F934FD7DDEED" box="[521,643,1763,1787]" country="United States of America" name="New York" pageId="15" pageNumber="14">New York</collectingRegion>
<location id="8E8A600B7B3DFFA2FD25F933FD18DEED" LSID="urn:lsid:plazi:treatment:03FC87C67B3DFFBDFF6CFAD5FC36DDED:8E8A600B7B3DFFA2FD25F933FD18DEED" box="[649,742,1763,1787]" municipality="City" name="County" pageId="15" pageNumber="14" stateProvince="New York">County</location>
