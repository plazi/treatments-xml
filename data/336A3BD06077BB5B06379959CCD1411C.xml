<taxonomicName id="95584F8D264790380B5F1CF42578346A" authority="L." class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="479" phylum="Tracheophyta" rank="species" species="uliginosum">
