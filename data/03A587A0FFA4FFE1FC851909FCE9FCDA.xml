, frontal; 
<emphasis id="B978EAA4FFA2FFE3FF4C1F88FEAEFC61" bold="true" box="[251,267,992,1009]" pageId="4" pageNumber="27">Ic</emphasis>
, intercalaire; 
<emphasis id="B978EAA4FFA2FFE3FE361F88FE0DFC61" bold="true" box="[385,424,992,1009]" pageId="4" pageNumber="27">Leth</emphasis>
, ethmoïde latéral; 
