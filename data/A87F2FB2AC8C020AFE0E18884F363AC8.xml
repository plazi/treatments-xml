<taxonomicName id="C962055789D3ED5CDB715CD73A9EAFEA" LSID="http://zoobank.org/284F6D53-474C-43C7-A723-A48E44F78D7F" authority="Klimaszewski &amp; Webster" class="Insecta" family="Staphylinidae" genus="Pleurotobia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pleurotobia brunswickensis" order="Coleoptera" pageId="69" pageNumber="154" phylum="Arthropoda" rank="species" species="brunswickensis">Pleurotobia brunswickensis Klimaszewski &amp; Webster</taxonomicName>
