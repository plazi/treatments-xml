<taxonomicName id="89B7B884FFA64C21FBDB367E36AFB665" box="[1036,1159,1809,1835]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="38" pageNumber="39" phylum="Tracheophyta" rank="genus">
