<subSubSection id="45E030B3FF8BFFAAECB67C046AB1FD7B" lastPageId="10" lastPageNumber="249" pageId="9" pageNumber="248" type="description">
<paragraph id="0D456338FF8BFFA9ECB67C046E3CFA48" blockId="9.[151,1436,1400,2007]" pageId="9" pageNumber="248">
<emphasis id="3F8EBF2AFF8BFFA9ECB67C046875FA25" bold="true" box="[199,545,1400,1425]" pageId="9" pageNumber="248">Description. Measurements.</emphasis>
