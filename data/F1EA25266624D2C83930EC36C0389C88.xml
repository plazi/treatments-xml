<paragraph id="9F27945B88AFC8DCB201D30470A2E446" pageId="2" pageNumber="57">
<taxonomicName id="9446157F205ACFD8C884B1A507BCEA6B" class="Insecta" family="Therevidae" genus="Actenomeros" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Actenomeros corniculaticaudus" order="Diptera" pageId="2" pageNumber="57" phylum="Arthropoda" rank="species" species="corniculaticaudus">Actenomeros corniculaticaudus</taxonomicName>
