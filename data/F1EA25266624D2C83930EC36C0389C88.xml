<taxonomicName id="51E791FD3C5453BBA39D7BEDECE7DB36" class="Insecta" family="Therevidae" genus="Actenomeros" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Actenomeros budawang" order="Diptera" pageId="2" pageNumber="57" phylum="Arthropoda" rank="species" species="budawang">Actenomeros budawang</taxonomicName>
