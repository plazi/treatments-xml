<paragraph id="004E36B4FFEC8A0AFBCEFC01FB55DD05" blockId="35.[196,1821,716,2601]" pageId="35" pageNumber="314">
<taxonomicName id="C7F14D37FFEC8A0AFBCEFC01FAF3DAA4" authorityName="Csorba" authorityYear="1997" box="[1026,1398,928,967]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="35" pageNumber="314" phylum="Chordata" rank="species" species="convexus">
