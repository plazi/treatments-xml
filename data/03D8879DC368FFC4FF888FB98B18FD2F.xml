<taxonomicName id="4C714D08C36BFFC5FF3A8A4388D6F983" box="[195,430,1640,1657]" class="Magnoliopsida" family="Apocynaceae" genus="Calyptranthera" kingdom="Plantae" order="Gentianales" pageId="2" pageNumber="35" phylum="Tracheophyta" rank="species" species="schatziana">
