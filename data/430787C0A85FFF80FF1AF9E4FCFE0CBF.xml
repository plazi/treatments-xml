<collectionCode id="ADBFAE13A85FFF80FD28FD45FCA30BBF" box="[701,798,656,677]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="71" pageNumber="71">AMNH</collectionCode>
