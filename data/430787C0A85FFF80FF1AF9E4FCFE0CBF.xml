<location id="CE71600DA85FFF80FCDFFDCFFCD00B35" LSID="urn:lsid:plazi:treatment:430787C0A85FFF80FF1AF9E4FCFE0CBF:CE71600DA85FFF80FCDFFDCFFCD00B35" box="[842,877,538,559]" country="Venezuela" county="Suapure" municipality="Those" name="He" pageId="71" pageNumber="71">He</location>
