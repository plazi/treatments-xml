<collectionCode id="ADBFAE13A85FFF80FD48FDCFFC830B35" box="[733,830,538,559]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="71" pageNumber="71">AMNH</collectionCode>
