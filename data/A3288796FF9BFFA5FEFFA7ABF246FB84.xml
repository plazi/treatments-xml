<paragraph id="2B3E3680FF9BFFA6FEFFA7ABF3D5F971" blockId="1.[264,695,1673,1699]" box="[264,695,1673,1699]" pageId="1" pageNumber="54">
<heading id="707681ECFF9BFFA6FEFFA7ABF3D5F971" bold="true" box="[264,695,1673,1699]" fontSize="11" level="1" pageId="1" pageNumber="54" reason="1">
<emphasis id="19F5EA92FF9BFFA6FEFFA7ABF3D5F971" bold="true" box="[264,695,1673,1699]" pageId="1" pageNumber="54">
Family 
<taxonomicName id="EC814D03FF9BFFA6FE90A7ABF3D5F971" authority="Bergstrom, 1914" authorityName="Bergstrom" authorityYear="1914" box="[359,695,1673,1699]" class="Polychaeta" family="Iospilidae" kingdom="Animalia" order="Phyllodocida" pageId="1" pageNumber="54" phylum="Annelida" rank="family">
Iospilidae Bergström, 
<number id="9D4924A7FF9BFFA6FD8BA7ABF3D5F971" box="[636,695,1673,1699]" pageId="1" pageNumber="54" value="1914.0">1914</number>
</taxonomicName>
</emphasis>
