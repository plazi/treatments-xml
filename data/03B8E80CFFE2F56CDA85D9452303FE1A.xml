<subSubSection id="C30B0A91FFEDF56CDAD5DC4925B0FE69" box="[199,1163,367,392]" pageId="24" pageNumber="525" type="etymology">
<paragraph id="8BAE591AFFEDF56CDAD5DC4925B0FE69" blockId="24.[151,1437,151,501]" box="[199,1163,367,392]" pageId="24" pageNumber="525">
<emphasis id="B9658508FFEDF56CDAD5DC49207DFE66" bold="true" box="[199,326,367,392]" pageId="24" pageNumber="525">Etymology</emphasis>
