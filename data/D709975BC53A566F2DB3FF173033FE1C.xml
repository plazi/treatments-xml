<collectingRegion id="9D64E8AFC53A566F2835FE3930D3FE21" box="[1297,1432,438,463]" country="Australia" name="Queensland" pageId="2" pageNumber="503">Queensland</collectingRegion>
, 
<taxonomicName id="98A05DCEC53A566F2DB3FE5437CEFE1D" authority="Keyserling, 1883" authorityName="Keyserling" authorityYear="1883" box="[151,645,474,499]" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="nigromaculatus">
<emphasis id="6DD4FA5FC53A566F2DB3FE5434E0FE1D" box="[151,427,475,499]" italics="true" pageId="2" pageNumber="503">Maratus nigromaculatus</emphasis>
(
<bibRefCitation id="3B315BBCC53A566F2C9EFE553736FE1D" author="Keyserling" box="[442,637,474,499]" pageId="2" pageNumber="503" refString="Keyserling, E. (1883) Die Arachniden Australiens. Nurnberg, 1, 1421 - 1489." type="journal article" year="1883">Keyserling, 1883</bibRefCitation>
)
</taxonomicName>
plus the new species 
<taxonomicName id="98A05DCEC53A566F2E5DFE543129FE1D" box="[889,1122,475,499]" class="Arachnida" family="Salticidae" genus="Maratus" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="kiwirrkurra">
<emphasis id="6DD4FA5FC53A566F2E5DFE543129FE1D" box="[889,1122,475,499]" italics="true" pageId="2" pageNumber="503">Maratus kiwirrkurra</emphasis>
