New 
<collectingRegion id="9D64E8AFC53A566F2EECFE393114FE21" box="[968,1119,438,463]" inferredBySuffix="true" pageId="2" pageNumber="503">
South 
<collectingCountry id="27B766DDC53A566F293CFE393114FE21" box="[1048,1119,438,463]" name="United Kingdom" pageId="2" pageNumber="503">Wales</collectingCountry>
