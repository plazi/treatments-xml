<taxonomicName id="84834D5B1C13FFCA02F1FB37FDE481EE" authority="Schmidt-Goebel, 1846" authorityName="Schmidt-Goebel" authorityYear="1846" box="[207,628,1196,1223]" class="Insecta" family="Carabidae" genus="Eustra" kingdom="Animalia" order="Coleoptera" pageId="27" pageNumber="574" phylum="Arthropoda" rank="species" species="plagiata">
