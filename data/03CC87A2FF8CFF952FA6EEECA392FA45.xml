<paragraph id="8BDA36B4FF8CFF952FA6EF4CA392FA45" blockId="10.[192,1441,1377,1498]" pageId="10" pageNumber="9">
<emphasis id="B911EAA6FF8CFF952FA6EF4CA18BFAE6" bold="true" box="[192,451,1377,1401]" pageId="10" pageNumber="9">Generic diagnosis.</emphasis>
<taxonomicName id="4C654D37FF8CFF952EAAEF4FA200FAE5" authorityName="LeConte" authorityYear="1876" box="[460,584,1378,1402]" class="Insecta" family="Anthribidae" genus="Allandrus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="9" phylum="Arthropoda" rank="genus">
