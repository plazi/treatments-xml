, 
<collectingRegion id="49F62991FFDC13769C02F9A7FC30F9CB" box="[867,932,1569,1594]" country="Ireland" name="Laois" pageId="62" pageNumber="63">Laois</collectingRegion>
, 
<collectingRegion id="49F62991FFDC13769CD7F9A7FB8BF9CB" box="[950,1055,1569,1594]" country="Ireland" name="Limerick" pageId="62" pageNumber="63">Limerick</collectingRegion>
