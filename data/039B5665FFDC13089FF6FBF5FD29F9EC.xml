<collectingRegion id="49F62991FFDC13769B53F9A4FBE2F9CB" box="[1074,1142,1570,1594]" country="Ireland" name="Mayo" pageId="62" pageNumber="63">Mayo</collectingRegion>
, 
<collectingRegion id="49F62991FFDC13769BE9F9A4FA8CF9C8" box="[1160,1304,1570,1593]" country="Ireland" name="Roscommon" pageId="62" pageNumber="63">Roscommon</collectingRegion>
