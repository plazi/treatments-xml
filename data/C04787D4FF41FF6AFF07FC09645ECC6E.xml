<taxonomicName id="8FEE4D41FF41FF6AFEF5F94C672DCC43" authorityName="Birula" authorityYear="1895" box="[357,622,1771,1797]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="168" pageNumber="169" phylum="Arthropoda" rank="species" species="inermis">
