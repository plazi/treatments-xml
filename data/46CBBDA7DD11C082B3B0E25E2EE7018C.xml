<taxonomicName id="616B2EAA2858043A5E5AB9E1C600EEB2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230741" authority="Andrei" authorityName="Andrei" class="Hexapoda" family="Formicidae" genus="Sima" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sima (Pachysima) latifrons Emery" lsidName-HNS="Sima (Pachysima) latifrons Emery" order="Hymenoptera" pageNumber="97" phylum="Arthropoda" rank="subGenus" subGenus="Pachysima">Pachysima</taxonomicName>
