<heading id="362181C7E75A2E5EFF64FDD4FD06FDA9" bold="true" box="[151,731,547,574]" fontSize="11" level="1" pageId="3" pageNumber="34" reason="1">
<emphasis id="5FA2EAB9E75A2E5EFF64FDD4FD06FDA9" bold="true" box="[151,731,547,574]" pageId="3" pageNumber="34">
<taxonomicName id="AAD64D28E75A2E5EFF64FDD4FDADFDA9" authority="Daccordi et Ge" authorityName="Daccordi et Ge" box="[151,624,547,574]" class="Insecta" family="Chrysomelidae" genus="Chrysolina" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="34" phylum="Arthropoda" rank="species" species="hohxilensis" status="sp. nov.">
<emphasis id="5FA2EAB9E75A2E5EFF64FDD4FE72FDAA" bold="true" box="[151,431,548,574]" italics="true" pageId="3" pageNumber="34">Chrysolina hohxilensis</emphasis>
Daccordi 
<emphasis id="5FA2EAB9E75A2E5EFDDDFDD4FD9EFDAA" bold="true" box="[558,579,548,574]" italics="true" pageId="3" pageNumber="34">et</emphasis>
Ge
</taxonomicName>
, 
<taxonomicNameLabel id="449157C2E75A2E5EFD8EFDD3FD06FDA9" box="[637,731,547,573]" pageId="3" pageNumber="34" rank="species">sp. nov.</taxonomicNameLabel>
