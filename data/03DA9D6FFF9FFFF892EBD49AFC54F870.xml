<taxonomicName id="4C7357FAFF9FFFF892EBD49AFDE7FB0E" authority="Lefebvre, 1842" authorityName="Lefebvre" authorityYear="1842" box="[151,550,1197,1223]" class="Insecta" family="Ascalaphidae" genus="Deleproctophylla" kingdom="Animalia" order="Neuroptera" pageId="13" pageNumber="300" phylum="Arthropoda" rank="genus">
