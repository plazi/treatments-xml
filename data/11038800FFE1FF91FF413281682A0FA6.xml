<collectingCountry id="E1BD7986FFE0FF91FE37346968560C86" box="[481,539,362,387]" name="India" pageId="15" pageNumber="16">India</collectingCountry>
and 
<collectingCountry id="E1BD7986FFE0FF91FD87346968F30C86" box="[593,702,362,387]" name="Sri Lanka" pageId="15" pageNumber="16">Sri Lanka</collectingCountry>
to 
<collectingCountry id="E1BD7986FFE0FF91FD343468696F0C87" box="[738,802,363,386]" name="Japan" pageId="15" pageNumber="16">Japan</collectingCountry>
and 
<collectingCountry id="E1BD7986FFE0FF91FC8E346969E70C86" box="[856,938,362,387]" name="Taiwan" pageId="15" pageNumber="16">Taiwan</collectingCountry>
