<quantity id="5A261FAD8122FE81ECDDC96E1B560922" box="[438,523,1863,1887]" metricMagnitude="0" metricUnit="m" metricValue="6.0" metricValueMax="10.0" metricValueMin="2.0" pageId="294" pageNumber="295" unit="m" value="6.0" valueMax="10.0" valueMin="2.0">2–10 m</quantity>
