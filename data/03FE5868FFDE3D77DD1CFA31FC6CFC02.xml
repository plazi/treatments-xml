</paragraph>
<paragraph id="8BE8E97EFFD93D77DFC3FCD7FB14FCBF" blockId="44.[189,1271,852,878]" box="[189,1271,852,878]" pageId="44" pageNumber="45">
<collectingCountry id="F340A9EEFFD93D77DFC3FCD7FEFBFCBF" box="[189,280,852,878]" name="Canada" pageId="44" pageNumber="45">Canada</collectingCountry>
, 
<collectingCountry id="F340A9EEFFD93D77DE5AFCD7FE67FCBF" box="[292,388,852,878]" name="Norway" pageId="44" pageNumber="45">Norway</collectingCountry>
, 
<collectingCountry id="F340A9EEFFD93D77DEF1FCD7FE0CFCBF" box="[399,495,852,878]" name="Sweden" pageId="44" pageNumber="45">Sweden</collectingCountry>
, northern 
<collectingCountry id="F340A9EEFFD93D77DD1BFCD7FD30FCBF" box="[613,723,852,878]" name="Germany" pageId="44" pageNumber="45">Germany</collectingCountry>
, 
