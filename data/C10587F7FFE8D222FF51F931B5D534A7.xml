<subSubSection id="01B6656AFFE8D225FF51F88DB3DE33CA" pageId="3" pageNumber="227" type="distribution">
<paragraph id="491336E1FFE8D225FF51F88DB4F03366" blockId="3.[151,1437,1770,2013]" box="[151,974,1878,1905]" pageId="3" pageNumber="227">
<emphasis id="7BD8EAF3FFE8D225FF51F88DB6173367" bold="true" box="[151,297,1878,1904]" pageId="3" pageNumber="227">Distribution</emphasis>
. Palaearctic and Oriental regions; 
<emphasis id="7BD8EAF3FFE8D225FD6BF88DB4F93367" bold="true" box="[685,967,1878,1904]" pageId="3" pageNumber="227">
first record for 
<collectingCountry id="31BB7671FFE8D225FCA3F88DB4F93367" box="[869,967,1878,1904]" name="Ukraine" pageId="3" pageNumber="227">Ukraine</collectingCountry>
</emphasis>
