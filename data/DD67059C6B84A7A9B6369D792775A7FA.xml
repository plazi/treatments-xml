<taxonomicName id="6F5D176A9133336D7750D0A1FA38E3AB" authorityName="Kunth" authorityYear="1843" class="Liliopsida" family="Asphodelaceae" genus="Trachyandra" higherTaxonomySource="GBIF,CoL" kingdom="Plantae" lsidName="" order="Asparagales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="genus">Trachyandra</taxonomicName>
