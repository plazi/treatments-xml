</taxonomicName>
from 
<collectingCountry id="F3527660FFA6883999BE1848FAF50FC4" box="[1174,1280,367,392]" name="Malaysia" pageId="35" pageNumber="336">Malaysia</collectingCountry>
, 
<taxonomicName id="4C454D73FFA68839983C1856FE630FE0" authority="Szymczakowski, 1972" authorityName="Szymczakowski" authorityYear="1972" class="Insecta" family="Leiodidae" genus="Ptomaphaginus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="336" phylum="Arthropoda" rank="species" species="assimilis">
<emphasis id="B931EAE2FFA68839983C1856FAD70FC4" box="[1300,1314,369,392]" italics="true" pageId="35" pageNumber="336">P</emphasis>
. 
<emphasis id="B931EAE2FFA6883998121848FA690FC4" box="[1338,1436,367,392]" italics="true" pageId="35" pageNumber="336">assimilis</emphasis>
