<emphasis id="BC87EAE22A66A42FFF5EFD63FF590938" bold="true" box="[151,207,766,790]" pageId="21" pageNumber="22">Type</emphasis>
:— 
<materialsCitation id="3E9B3CAD2A66A42FFF24FD63FB70091B" collectingDate="1953-02-04" collectorName="R. Ross." country="Tanzania" elevation="8" location="Lake Tanganyika" pageId="21" pageNumber="22" specimenCount="1" typeStatus="holotype">
<collectingCountry id="F6E476602A66A42FFF24FD63FEED093B" box="[237,379,766,789]" name="Tanzania" pageId="21" pageNumber="22">TANZANIA</collectingCountry>
. 
<location id="8B2C602B2A66A42FFE4CFD63FDD6093B" LSID="urn:lsid:plazi:treatment:065A87E62A66A42FFF5EFD2CFBA50C3E:8B2C602B2A66A42FFE4CFD63FDD6093B" box="[389,576,766,789]" country="Tanzania" name="Lake Tanganyika" pageId="21" pageNumber="22">Lake Tanganyika</location>
, tangled in trawl from 
<elevation id="05DED1C32A66A42FFCFCFD63FC35093B" box="[821,931,766,789]" metricMagnitude="0" metricUnit="m" metricValue="8.0" pageId="21" pageNumber="22" unit="m" value="8.0">
<quantity id="490B9B152A66A42FFCFCFD63FCC8093B" box="[821,862,766,789]" metricMagnitude="0" metricUnit="m" metricValue="8.0" pageId="21" pageNumber="22" unit="m" value="8.0">8 m</quantity>
depth
</elevation>
, ¼ mile offshore 
<locationDeviation id="9FFE2FD22A66A42FFB97FD63FA15093B" box="[1118,1411,766,789]" firstLocation="Kjiyi" pageId="21" pageNumber="22" secondLocation="Bangwe">between Kjiyi and Bangwe</locationDeviation>
, 
<date id="FA4D10302A66A42FFA47FD63FEF3091B" pageId="21" pageNumber="22" value="1953-02-04">
