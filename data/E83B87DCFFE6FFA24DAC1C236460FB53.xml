<taxonomicName id="A7924D49FFE6FFA24E701934651BFB88" authorityName="Remy" authorityYear="1954" box="[843,1065,1125,1151]" class="Pauropoda" family="Hansenauropodidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tetramerocerata" pageId="2" pageNumber="375" phylum="Arthropoda" rank="family">Hansenauropodidae</taxonomicName>
