); 
<taxonomicName id="4C329CF0FFD6137C9CCDFBFFFBB6FB63" baseAuthorityName="Halbert" baseAuthorityYear="1915" box="[940,1058,1145,1170]" class="Sphagnopsida" family="Sphagnaceae" genus="Sphagnum" kingdom="Plantae" order="Sphagnales" pageId="52" pageNumber="53" phylum="Bryophyta" rank="genus">
