<collectionCode id="F4A6A6889471FFF5FB08F970917CF8FF" box="[1253,1332,1770,1793]" country="Viet Nam" httpUri="http://grbio.org/cool/82c5-17ij" name="Zoological Museum, University of Hanoi" pageId="4" pageNumber="3">ZMUH</collectionCode>
