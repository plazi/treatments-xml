; south of 
<location id="976868969471FFF5FE82F8DC95FDF8A3" LSID="urn:lsid:plazi:treatment:1A1E8F5B9476FFF7FC45FAC1900BF8A4:976868969471FFF5FE82F8DC95FDF8A3" box="[367,437,1862,1885]" country="Mauritania" name="Aftout" pageId="4" pageNumber="3">Aftout</location>
, 
<date id="E609188D9471FFF5FE2FF8DC9608F8A3" box="[450,576,1862,1885]" pageId="4" pageNumber="3" value="1994-08-02">
