<heading id="D0D2C25CFF83FFA5FF3999E5FED5FB4D" bold="true" box="[151,332,1172,1198]" fontSize="11" level="1" pageId="44" pageNumber="45" reason="1">
<emphasis id="B951A922FF83FFA5FF3999E5FED5FB4D" bold="true" box="[151,332,1172,1198]" pageId="44" pageNumber="45">
<taxonomicName id="4C250EB3FF83FFA5FF3999E5FEBBFB4D" authorityName="Monke" authorityYear="1903" box="[151,290,1172,1198]" class="Trilobita" family="Liostracinidae" genus="Liostracina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Asaphida" pageId="44" pageNumber="45" phylum="Arthropoda" rank="genus">
<emphasis id="B951A922FF83FFA5FF3999E5FEBBFB4D" bold="true" box="[151,290,1172,1198]" italics="true" pageId="44" pageNumber="45">Liostracina</emphasis>
