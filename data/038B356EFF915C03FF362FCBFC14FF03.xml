<emphasis id="B956586AFF905C03FCAA2CB3FC14FF03" box="[779,984,195,220]" italics="true" pageId="5" pageNumber="43">
<taxonomicName id="4C22FFFBFF905C03FCAA2CB3FC18FF03" authorityName="Li &amp; Ren &amp; Shih" authorityYear="2008" box="[779,980,195,220]" class="Insecta" family="Bittacidae" genus="Jurahylobittacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mecoptera" pageId="5" pageNumber="43" phylum="Arthropoda" rank="genus">Jurahylobittacus</taxonomicName>
