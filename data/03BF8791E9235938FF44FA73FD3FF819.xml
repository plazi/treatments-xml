<taxonomicName id="4C164D04E9235938FCF7F8C7FB45F841" authority="Butler &amp; Druce, 1872" authorityName="A. Butler &amp; H. Druce" authorityYear="1872" box="[804,1206,1906,1933]" class="Insecta" family="Lycaenidae" genus="Emesis" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="478" phylum="Arthropoda" rank="species" species="furor">
