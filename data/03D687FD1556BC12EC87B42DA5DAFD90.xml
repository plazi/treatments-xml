<taxonomicName id="4C7F4D681556BC13EC87B42DA3BCFAA6" authority="Niu" authorityName="Niu &amp; Ascher &amp; Orr &amp; Griswold &amp; Zhu" authorityYear="2019" box="[151,682,1298,1325]" class="Insecta" family="Megachilidae" genus="Trachusa" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="261" phylum="Arthropoda" rank="species" species="pingdaensis" status="sp. nov." subGenus="Paraanthidium">
