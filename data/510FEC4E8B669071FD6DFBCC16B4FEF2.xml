<quantity id="1E5EF0BD8B669077FD66FA3E16C9FA5A" box="[654,722,1478,1504]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="34" pageNumber="35" unit="mm" value="1.0">1 mm</quantity>
