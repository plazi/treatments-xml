<taxonomicName id="4C0E4D68FFB0FFF8FD90FED1B3CC95D2" box="[512,676,376,401]" class="Insecta" family="Pteromalidae" genus="Macroglenes" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="gramineus">
