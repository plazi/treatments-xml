<taxonomicName id="2EA53B8EE003FFDBFD1324D6FEE4FC16" authority="Tran &amp; Zettel, 2021" authorityName="Tran &amp; Zettel" authorityYear="2021" class="Insecta" family="Nepidae" genus="Ranatra" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="57" phylum="Arthropoda" rank="species" species="pangantihoni" status="new species">
