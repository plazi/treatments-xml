<taxonomicName id="CC9E57816CC7D171D1571F535796726C" LSID="urn:lsid:zoobank.org:act:E3F95FC3-C247-41D3-90F9-47C7724EA7E3" authority="Huber &amp; Noyes" authorityName="Huber &amp; Noyes" authorityYear="2013" class="Insecta" family="Mymaridae" genus="Tinkerbella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tinkerbella" order="Hymenoptera" pageId="1" pageNumber="18" phylum="Arthropoda" rank="genus" status="gen. n.">Tinkerbella Huber &amp; Noyes</taxonomicName>
