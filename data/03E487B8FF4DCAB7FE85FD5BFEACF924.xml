<taxonomicName id="4C4D4D2DFF4DCAB7FE9AFA7FFDA5FA7F" authority="Hampson, 1901" authorityName="Hampson" authorityYear="1901" box="[272,565,1502,1529]" class="Insecta" family="Arctiidae" genus="Metacrisia" kingdom="Animalia" order="Lepidoptera" pageId="173" pageNumber="310" phylum="Arthropoda" rank="genus">
