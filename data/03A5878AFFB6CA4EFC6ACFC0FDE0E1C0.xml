<taxonomicName id="4C0C4D1FFFB7CA4EFC8FCF4BFB22E351" baseAuthorityName="Coe" baseAuthorityYear="1901" box="[875,1168,1050,1069]" class="Hoplonemertea" family="Cratenemertidae" genus="Nipponnemertes" kingdom="Animalia" order="Monostilifera" pageId="5" pageNumber="700" phylum="Nemertea" rank="species" species="bimaculata">
