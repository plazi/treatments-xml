<taxonomicName id="60B6789CA6D43E5658DDE70459D15F21" class="Insecta" family="Formicidae" genus="Trachymyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trachymyrmex smithi" order="Hymenoptera" pageId="5" pageNumber="78" phylum="Arthropoda" rank="species" species="smithi">Trachymyrmex smithi</taxonomicName>
