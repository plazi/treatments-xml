<quantity id="4CC59B7EFF3FE3266C509F1FFD0AF9C8" box="[631,720,1607,1631]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="59" pageNumber="60" unit="mm" value="1.7">1.7 mm</quantity>
, HL = 
