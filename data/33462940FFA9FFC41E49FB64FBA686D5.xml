<emphasis id="899B4444FFA9FFC5190BFB24FC308670" bold="true" box="[835,988,1273,1297]" pageId="0" pageNumber="327">
<typeStatus id="645426F4FFA9FFC5190BFB24FC918670" box="[835,893,1273,1297]" pageId="0" pageNumber="327">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="7CEFE3D5FFA9FFC519AEFB27FB418670" authority="Naruse &amp; Chung &amp; Tangah, 2015" authorityName="Naruse &amp; Chung &amp; Tangah" authorityYear="2015" box="[998,1197,1273,1297]" class="Malacostraca" family="Camptandriidae" genus="Exagorium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="327" phylum="Arthropoda" rank="species" species="fidelisi" status="new species">
