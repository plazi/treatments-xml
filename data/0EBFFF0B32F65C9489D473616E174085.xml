<emphasis id="AE4D5F1CBAD218797E30E23635BC66A6" bold="true" pageId="0" pageNumber="27">Figure 21.</emphasis>
<taxonomicName id="76EBDD53DD3ABACB325D026BFE92B579" authorityName="Fenyes" authorityYear="1921" class="Insecta" family="Staphylinidae" genus="Cyphea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyphea wallisi" order="Coleoptera" pageId="0" pageNumber="27" phylum="Arthropoda" rank="species" species="wallisi">
