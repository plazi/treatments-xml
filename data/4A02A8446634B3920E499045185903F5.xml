<taxonomicName id="E33C8C65DC385A1B0E2C47515FD7B6BE" LSID="http://zoobank.org/55057CB5-20C4-4AF8-B401-8306916B4773" authority="Fernandez-Triana &amp; Boudreault" class="Insecta" family="Braconidae" genus="Promicrogaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Promicrogaster kiralycastilloae" order="Hymenoptera" pageId="18" pageNumber="43" phylum="Arthropoda" rank="species" species="kiralycastilloae">Promicrogaster kiralycastilloae Fernandez-Triana &amp; Boudreault</taxonomicName>
