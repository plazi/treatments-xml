<emphasis id="B958EAAAFFE2FFAE34250160FE1DD396" italics="true" pageId="20" pageNumber="21">N. sphaerodactylodes</emphasis>
</taxonomicName>
; open circles are other locations for 
<taxonomicName id="4C2C4D3BFFE2FFAE32600185FBECD396" box="[855,1086,1678,1701]" class="Reptilia" family="Gekkonidae" genus="Nactus" kingdom="Animalia" order="Squamata" pageId="20" pageNumber="21" phylum="Chordata" rank="species" species="sphaerodactylodes">
<emphasis id="B958EAAAFFE2FFAE32600185FBECD396" box="[855,1086,1678,1701]" italics="true" pageId="20" pageNumber="21">
N. 
<taxonomicName id="4C2C4D3BFFE2FFAE32400185FBECD396" box="[887,1086,1678,1701]" pageId="20" pageNumber="21" rank="species" species="sphaerodactylodes">sphaerodactylodes</taxonomicName>
</emphasis>
</taxonomicName>
