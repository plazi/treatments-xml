<taxonomicName id="F5A9CB1568282B2EADDD3C1F38C78393" class="Lecanoromycetes" family="Graphidaceae" genus="Graphina" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Graphina taiwanensis" order="Ostropales" pageId="13" pageNumber="26" phylum="Ascomycota" rank="species" species="taiwanensis">Graphina taiwanensis</taxonomicName>
