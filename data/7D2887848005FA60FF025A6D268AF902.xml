<taxonomicName id="32814D118005FA60FDCD5D0F2749F9FF" baseAuthorityName="Mann" baseAuthorityYear="1864" box="[600,839,1548,1572]" class="Insecta" family="Pyralidae" genus="Episcythrastis" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="364" phylum="Arthropoda" rank="species" species="tabidella">Episcythrastis tabidella</taxonomicName>
