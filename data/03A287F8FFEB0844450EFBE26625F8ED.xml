<collectionCode id="ED1AAE2BFFEB08444684FB2F65C9FB18" box="[642,703,1212,1235]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="4" pageNumber="5">NEW</collectionCode>
