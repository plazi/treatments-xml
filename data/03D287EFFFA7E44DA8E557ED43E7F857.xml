<taxonomicName id="4C7B4D7AFFA7E44DA8E55539400BF88C" box="[151,264,1878,1901]" class="Insecta" family="Agromyzidae" genus="Agromyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
