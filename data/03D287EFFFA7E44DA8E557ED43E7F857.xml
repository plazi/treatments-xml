(as 
<taxonomicName id="4C7B4D7AFFA7E44DAA2655154277F873" authority="Zetterstedt" authorityName="Zetterstedt" authorityYear="1848" box="[596,884,1913,1938]" class="Insecta" family="Agromyzidae" genus="Agromyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="niveipennis">
