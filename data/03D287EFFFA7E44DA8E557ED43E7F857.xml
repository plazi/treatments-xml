<taxonomicName id="4C7B4D7AFFA7E44DA9FD553A433BF88F" box="[399,568,1877,1902]" class="Magnoliopsida" family="Poaceae" genus="Elymus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="species" species="hystrix">
<emphasis id="B90FEAEBFFA7E44DA9FD553A433BF88F" box="[399,568,1877,1902]" italics="true" pageId="7" pageNumber="8">Elymus hystrix</emphasis>
</taxonomicName>
