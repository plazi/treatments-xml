<taxonomicName id="4C7B4D7AFFA7E44DACDE5539441EF88C" box="[1196,1309,1878,1901]" class="Insecta" family="Agromyzidae" genus="Agromyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
<emphasis id="B90FEAEBFFA7E44DACDE5539441EF88C" box="[1196,1309,1878,1901]" italics="true" pageId="7" pageNumber="8">Agromyza</emphasis>
</taxonomicName>
species on 
<taxonomicName id="4C7B4D7AFFA7E44DA8E5551641E9F873" box="[151,234,1913,1938]" class="Magnoliopsida" family="Poaceae" genus="Elymus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="7" pageNumber="8" phylum="Tracheophyta" rank="genus">
