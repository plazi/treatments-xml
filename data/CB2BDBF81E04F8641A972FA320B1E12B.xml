<taxonomicName id="0A499DADDD1E487C3A6321CDFC5DC94C" class="Insecta" family="Noctuidae" genus="Gortyna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gortyna ximena" order="Lepidoptera" pageId="20" pageNumber="105" phylum="Arthropoda" rank="species" species="ximena">Gortyna ximena</taxonomicName>
