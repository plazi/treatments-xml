<taxonomicName id="27DFFD661DA828E8F5D037B4B4AA0420" authorityName="Wahlenb" authorityYear="1812" class="Ascidiacea" family="Polycitoridae" genus="Salix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Salix polaris" order="Aplousobranchia" pageId="0" pageNumber="17" phylum="Chordata" rank="species" species="polaris">
