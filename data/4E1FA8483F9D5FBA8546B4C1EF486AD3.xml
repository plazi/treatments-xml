<taxonomicName id="95276E53AFBFBDB299DDBA1096E7C78D" class="Magnoliopsida" family="Saxifragaceae" genus="Saxifraga" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Saxifraga oppositifolia" order="Saxifragales" pageId="0" pageNumber="17" phylum="Tracheophyta" rank="species" species="oppositifolia">
<emphasis id="FE477305549D0CAC8340F4EDA6710145" italics="true" pageId="0" pageNumber="17">Saxifraga oppositifolia</emphasis>
