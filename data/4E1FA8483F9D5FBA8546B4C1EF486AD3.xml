<taxonomicName id="E4FC2D797D3BBC13D360F9BCE9DDB06B" class="Bryopsida" family="Dicranaceae" genus="Dicranoweisia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dicranoweisia crispula" order="Dicranales" pageId="0" pageNumber="17" phylum="Bryophyta" rank="species" species="crispula">
<emphasis id="E47C1043CE5B5CE32861120597F7497E" italics="true" pageId="0" pageNumber="17">Dicranoweisia crispula</emphasis>
</taxonomicName>
, 
<taxonomicName id="DA9F84DB7050E2941D9FA9D6686B56E6" class="Lecanoromycetes" family="Parmeliaceae" genus="Parmelia" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Parmelia alpicola" order="Lecanorales" pageId="0" pageNumber="17" phylum="Ascomycota" rank="species" species="alpicola">
<emphasis id="8A13F2EF0B4941A21B1B2F0FCF6FAB13" italics="true" pageId="0" pageNumber="17">Parmelia alpicola</emphasis>
