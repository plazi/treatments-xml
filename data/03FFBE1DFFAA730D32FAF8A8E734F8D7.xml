<taxonomicNameLabel id="A2116E62FFAF730C366FF93CE746F947" box="[1026,1089,1670,1694]" pageId="92" pageNumber="93" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
; 
<emphasis id="B922D319FFAF730C363CF93CE76BF947" bold="true" box="[1105,1132,1670,1694]" pageId="92" pageNumber="93">B:</emphasis>
<taxonomicName id="4C567488FFAF730C3618F93DE60CF947" authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[1141,1291,1671,1694]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="trichordum" status="n. sp.">
<emphasis id="B922D319FFAF730C3618F93DE60CF947" box="[1141,1291,1671,1694]" italics="true" pageId="92" pageNumber="93">Z. trichordum</emphasis>
</taxonomicName>
<emphasis id="B922D319FFAF730C3779F93CE655F947" bold="true" box="[1300,1362,1670,1694]" pageId="92" pageNumber="93">
<taxonomicNameLabel id="A2116E62FFAF730C3779F93CE655F947" box="[1300,1362,1670,1694]" pageId="92" pageNumber="93" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
; 
<emphasis id="B922D319FFAF730C370EF93CE671F947" bold="true" box="[1379,1398,1670,1694]" pageId="92" pageNumber="93">C</emphasis>
: 
