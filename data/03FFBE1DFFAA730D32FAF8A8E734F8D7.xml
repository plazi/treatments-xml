<emphasis id="B922D319FFAF730C37EBF93DE22DF919" italics="true" pageId="92" pageNumber="93">Z. smaragdinum</emphasis>
</taxonomicName>
<emphasis id="B922D319FFAF730C3359F913E272F918" bold="true" box="[308,373,1705,1729]" pageId="92" pageNumber="93">
<taxonomicNameLabel id="A2116E62FFAF730C3359F913E272F918" box="[308,373,1705,1729]" pageId="92" pageNumber="93" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
; 
<emphasis id="B922D319FFAF730C33E5F913E29CF918" bold="true" box="[392,411,1705,1729]" pageId="92" pageNumber="93">D</emphasis>
: 
<taxonomicName id="4C567488FFAF730C33C0F913E133F919" authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[429,564,1705,1728]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="ambrense" status="n. sp.">
<emphasis id="B922D319FFAF730C33C0F913E133F919" box="[429,564,1705,1728]" italics="true" pageId="92" pageNumber="93">Z. ambrense</emphasis>
</taxonomicName>
