<caption id="FBF86618BD4D10419CC6F9F7FE3FDDA1" httpUri="https://zenodo.org/record/2673397/files/figure.png" pageId="2" pageNumber="174" startId="2.[151,250,1631,1656]" targetBox="[172,1418,215,1596]" targetPageId="2">
