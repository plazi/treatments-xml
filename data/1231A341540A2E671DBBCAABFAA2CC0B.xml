<collectingMunicipality id="7A43882D540B2E671C49CFDEFE2EC8EB" box="[357,401,188,213]" pageId="17" pageNumber="418">The</collectingMunicipality>
<typeStatus id="4523ACF5540B2E671CBBCFDEFE46C8EB" box="[407,505,188,213]" pageId="17" pageNumber="418">holotype</typeStatus>
