named 
<taxonomicName id="5D9869D4540B2E671F1ECFFAFCF6C88E" authorityName="Furtos" authorityYear="1936" box="[562,841,151,176]" class="Ostracoda" family="Cyprididae" genus="Herpetocypris" kingdom="Animalia" order="Podocopida" pageId="17" pageNumber="418" phylum="Arthropoda" rank="species" species="meridana">
