<taxonomicName id="4C6D4D5EEE7F3B1BFF1AFE54FE39FE74" authority="Sars, 1921" authorityName="Sars" authorityYear="1921" box="[223,508,410,437]" class="Maxillopoda" family="Notodelphyidae" genus="Doropygella" kingdom="Animalia" order="Cyclopoida" pageId="302" pageNumber="303" phylum="Arthropoda" rank="genus">
