<taxonomicName id="4C6D4D5EEE7F3B1BFEBFF9D1FF09F99D" authority="(Aurivillius, 1882)" baseAuthorityName="Aurivillius" baseAuthorityYear="1882" class="Maxillopoda" family="Notodelphyidae" genus="Doropygella" kingdom="Animalia" order="Cyclopoida" pageId="302" pageNumber="303" phylum="Arthropoda" rank="species" species="thorelli">
