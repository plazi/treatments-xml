</emphasis>
<taxonomicName id="4C27E3B77E6CFFE0FE81758ADC14F943" authority="Draudt, 1923" authorityName="Draudt" authorityYear="1923" box="[382,778,1696,1720]" class="Insecta" family="Hesperiidae" genus="Vorates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="30" pageNumber="29" phylum="Arthropoda" rank="species" species="auristriga">
