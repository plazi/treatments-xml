<subSubSection id="7C9121BF178BA9A498CFF8877A72F872" pageId="186" pageNumber="187" type="etymology">
<paragraph id="34347234178BA9A498CFF8877A72F872" blockId="186.[151,1436,988,1934]" pageId="186" pageNumber="187">
<emphasis id="06FFAE26178BA9A498CFF8877912F89A" bold="true" box="[197,337,1868,1894]" pageId="186" pageNumber="187">Etymology.</emphasis>
<taxonomicName id="F38B09B7178BA9A49953F88679FCF89A" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[345,447,1869,1894]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="186" pageNumber="187" phylum="Arthropoda" rank="species" species="anonima">
<emphasis id="06FFAE26178BA9A49953F88679FCF89A" box="[345,447,1869,1894]" italics="true" pageId="186" pageNumber="187">anonima</emphasis>
