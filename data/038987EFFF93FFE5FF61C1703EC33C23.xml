’S
</geoCoordinate>
, 
<geoCoordinate id="EE14503EFF90FFE5FB83C5F038EF3AEB" box="[1141,1269,741,765]" direction="east" orientation="longitude" pageId="10" pageNumber="11" precision="9" value="36.694668">
<number id="3DE824DEFF90FFE5FB83C5F038883AEB" box="[1141,1170,741,765]" pageId="10" pageNumber="11" value="36.0">36</number>
° 
<number id="3DE824DEFF90FFE5FB6DC5F038C53AEB" box="[1179,1247,741,765]" pageId="10" pageNumber="11" value="41.68">41.68</number>
’E
</geoCoordinate>
, 
<date id="FF9E1039FF90FFE5FAF2C5F0398D3AEB" box="[1284,1431,741,766]" pageId="10" pageNumber="11" value="2001-04-24">
<number id="3DE824DEFF90FFE5FAF2C5F039053AEB" box="[1284,1311,741,765]" pageId="10" pageNumber="11" value="24.0">24</number>
