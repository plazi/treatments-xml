<collectingCountry id="F37D88C5FFB7A8719AC2FEE9FBC7FE8D" box="[911,1046,258,283]" name="Georgia" pageId="7" pageNumber="458">
<emphasis id="B91E1447FFB7A8719AC2FEE9FBC7FE8D" bold="true" box="[911,1046,258,283]" pageId="7" pageNumber="458">GEORGIA</emphasis>
</collectingCountry>
