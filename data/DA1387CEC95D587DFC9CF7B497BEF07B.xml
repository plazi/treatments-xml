<taxonomicName id="95BA4D5BC95D5872FCDDF7C19496F7C6" authorityName="Thomas" authorityYear="1912" box="[833,926,1499,1522]" class="Mammalia" family="Didelphidae" genus="Glironia" kingdom="Animalia" order="Didelphimorphia" pageId="87" pageNumber="88" phylum="Chordata" rank="genus">
