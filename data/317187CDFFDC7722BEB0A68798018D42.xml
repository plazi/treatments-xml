<taxonomicName id="7ED84D58FFDC7722BD33A4E29DA28CF2" authority="Johnston, 1917" authorityName="Johnston" authorityYear="1917" box="[788,1228,1730,1755]" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="48" pageNumber="49" phylum="Platyhelminthes" rank="species" species="taxorchis">
