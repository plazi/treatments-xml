<taxonomicName id="7ED84D58FFDC7722BF9BA42E9B3D8C0E" box="[444,595,1550,1575]" class="Aves" family="Scolopacidae" kingdom="Animalia" order="Charadriiformes" pageId="48" pageNumber="49" phylum="Chordata" rank="family">Scolopacidae</taxonomicName>
