<taxonomicName id="7ED84D58FFDC7722BA53A4C698578D0A" authority="Stossich, 1902" authorityName="Stossich" authorityYear="1902" class="Trematoda" family="Cyclocoelidae" genus="Cyclocoelum" kingdom="Animalia" order="Plagiorchiida" pageId="48" pageNumber="49" phylum="Platyhelminthes" rank="species" species="tringae" subGenus="Haematotrephus">
