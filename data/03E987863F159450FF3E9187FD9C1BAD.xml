°09' 
<number id="3D8824B73F129450FE929419FED21B3D" box="[315,346,1863,1889]" pageId="4" pageNumber="31" value="37.0">37</number>
''N
</geoCoordinate>
, 
<geoCoordinate id="EE7450573F129450FE209419FD971B3D" box="[393,543,1863,1889]" direction="east" orientation="longitude" pageId="4" pageNumber="31" precision="15" value="100.05778">
<number id="3D8824B73F129450FE209419FE3F1B3D" box="[393,439,1863,1889]" pageId="4" pageNumber="31" value="100.0">100</number>
