<location id="8EF62CE60459150AFDCF4A2CFCAEFBEB" LSID="urn:lsid:plazi:treatment:0380CB2B0459150AFFB44D8CFC93FACB:8EF62CE60459150AFDCF4A2CFCAEFBEB" box="[748,837,1029,1055]" country="Italy" county="Adriatic Sea" municipality="Mediterranean Sea" name="Brocchi" pageId="80" pageNumber="81" stateProvince="Toscana">Brocchi</location>
