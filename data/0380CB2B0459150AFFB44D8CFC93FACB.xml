<collectingCountry id="F33E3AAD0459150AFDFE4A03FCAEFBB0" box="[733,837,1066,1092]" name="Morocco" pageId="80" pageNumber="81">Morocco</collectingCountry>
, the 
<collectingMunicipality id="6BF2E0470459150AFC5A4A00FBA5FBB7" box="[889,1102,1065,1091]" pageId="80" pageNumber="81">Mediterranean Sea</collectingMunicipality>
, including the 
<collectingCounty id="62F702B10459150AFBD14A00FA6AFBB7" box="[1266,1409,1065,1091]" pageId="80" pageNumber="81">Adriatic Sea</collectingCounty>
