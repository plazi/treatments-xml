<taxonomicName id="4C004D305E30FFFBFE703089FD1FFE76" authority="Kuhn" authorityName="Kuhn" authorityYear="1868" box="[488,642,383,407]" class="Polypodiopsida" family="Pteridaceae" genus="Pteris" kingdom="Plantae" order="Polypodiales" pageId="7" pageNumber="22" phylum="Tracheophyta" rank="species" species="mettenii">
