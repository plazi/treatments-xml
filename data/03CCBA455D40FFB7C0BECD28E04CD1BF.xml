<taxonomicName id="4C6570D05D40FFB7C440CD50E66ED162" authorityName="Boulenger" authorityYear="1894" box="[1129,1322,1124,1149]" class="Actinopterygii" family="Cyprinidae" genus="Rasbora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="5" phylum="Chordata" rank="species" species="calliura">
