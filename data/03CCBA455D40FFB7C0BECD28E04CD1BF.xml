<taxonomicName id="4C6570D05D40FFB7C288CD50E0EAD162" authorityName="Volz" authorityYear="1903" box="[673,942,1124,1149]" class="Actinopterygii" family="Cyprinidae" genus="Rasbora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="5" phylum="Chordata" rank="species" species="caudimaculata">
