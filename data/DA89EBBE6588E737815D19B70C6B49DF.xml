<taxonomicName id="170EA645704EB0133C0E026E0A3148F2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:31372" class="Insecta" family="Formicidae" genus="Metapone" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Metapone madagascarica Gregg" order="Hymenoptera" pageNumber="11" phylum="Arthropoda" rank="species" species="madagascarica">M. madagascarica</taxonomicName>
