, 
<taxonomicName id="4C0F51E5FF96FFD6DB43D4C7FD21F85B" authorityName="- Embolar" box="[613,758,1961,1987]" class="Arachnida" family="Arachnida (awaiting allocation)" genus="Cinetomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Arachnida (awaiting allocation)" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="nesophila">C. nesophila</taxonomicName>
