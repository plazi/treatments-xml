<taxonomicName class="Insecta" family="Cerambycidae" genus="Megaderus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megaderus" order="Coleoptera" pageId="210" pageNumber="211" phylum="Arthropoda" rank="genus">Megaderus</taxonomicName>
