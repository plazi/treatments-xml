<quantity box="[370,472,314,340]" metricMagnitude="-3" metricUnit="m" metricValue="3.04" pageId="27" pageNumber="28" unit="mm" value="3.04">3.04mm</quantity>
; HL 
<quantity box="[522,626,313,340]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="27" pageNumber="28" unit="mm" value="0.45">0.45 mm</quantity>
; HW 
<quantity box="[684,786,314,340]" metricMagnitude="-4" metricUnit="m" metricValue="7.7" pageId="27" pageNumber="28" unit="mm" value="0.77">0.77mm</quantity>
; PW 
<quantity box="[842,943,314,340]" metricMagnitude="-4" metricUnit="m" metricValue="8.2" pageId="27" pageNumber="28" unit="mm" value="0.82">0.82 mm</quantity>
; PL 
<quantity box="[995,1084,314,340]" metricMagnitude="-4" metricUnit="m" metricValue="7.0" pageId="27" pageNumber="28" unit="mm" value="0.7">0.7mm</quantity>
