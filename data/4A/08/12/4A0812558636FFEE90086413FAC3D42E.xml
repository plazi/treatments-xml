<subSubSection pageId="20" pageNumber="105" type="etymology">
<paragraph blockId="20.[839,1467,1861,2001]" pageId="20" pageNumber="105">
<emphasis bold="true" box="[839,958,1890,1914]" pageId="20" pageNumber="105">Etymology</emphasis>
