<emphasis box="[981,1148,1036,1062]" italics="true" pageId="176" pageNumber="177">
Islas 
<collectingCountry box="[1048,1148,1036,1062]" name="United Kingdom" pageId="176" pageNumber="177">Orcadas</collectingCountry>
</emphasis>
, cruise 575, Stn 54, 
<geoCoordinate box="[189,290,1072,1100]" degrees="57" direction="south" minutes="39" orientation="latitude" pageId="176" pageNumber="177" precision="925" value="-57.65">57°39ʹ S</geoCoordinate>
, 
<geoCoordinate box="[301,433,1072,1100]" degrees="26" direction="west" minutes="00.4" orientation="longitude" pageId="176" pageNumber="177" precision="92" value="-26.006666">26°00.4ʹ W</geoCoordinate>
[
<collectingCountry box="[445,734,1073,1100]" name="South Georgia and the South Sandwich Islands" pageId="176" pageNumber="177">South Sandwich Islands</collectingCountry>
], 
<quantity box="[751,912,1073,1100]" metricMagnitude="3" metricUnit="m" metricValue="2.4945" metricValueMax="2.6089999999999995" metricValueMin="2.38" pageId="176" pageNumber="177" unit="m" value="2494.5" valueMax="2609.0" valueMin="2380.0">2380–2609 m</quantity>
(
<bibRefCitation author="Holman &amp; Watling" box="[925,1215,1073,1099]" firstAuthor="Holman" pageId="176" pageNumber="177" pagination="29 - 53" refId="ref139761" refString="Holman H. &amp; Watling L. 1983. A revision of the Stilipedidae (Amphipoda). Crustaceana 44 (1): 29 - 53. https: // doi. org / 10.1163 / 156854083 x 00037" type="journal article" year="1983">Holman &amp; Watling 1983</bibRefCitation>
