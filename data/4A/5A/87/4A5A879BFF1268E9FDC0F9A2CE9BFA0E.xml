<geoCoordinate box="[862,1014,663,691]" degrees="60" direction="west" minutes="28.30" orientation="longitude" pageId="149" pageNumber="150" precision="9" value="-60.47167">60°28.30ʹ W</geoCoordinate>
, 
<quantity box="[1029,1167,665,691]" metricMagnitude="2" metricUnit="m" metricValue="1.9899999999999998" metricValueMax="2.02" metricValueMin="1.96" pageId="149" pageNumber="150" unit="m" value="199.0" valueMax="202.0" valueMin="196.0">196–202 m</quantity>
, Agassiz trawl and Rauschert dredge, 
<date box="[408,552,700,727]" pageId="149" pageNumber="150" value="2011-03-07">7 Mar. 2011</date>
, coll. C. Havermans and H. Robert (
<collectionCode box="[979,1070,700,726]" country="Belgium" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35271" lsid="urn:lsid:biocol.org:col:35271" name="Royal Belgian Institute of Natural Sciences" pageId="149" pageNumber="150" type="Museum">RBINS</collectionCode>
, 
<collectionCode box="[1082,1134,701,727]" country="United Kingdom" lsid="urn:lsid:biocol.org:col:13356" name="Inverness Museum and Art Gallery" pageId="149" pageNumber="150" type="Herbarium">INV</collectionCode>
. 122896).
