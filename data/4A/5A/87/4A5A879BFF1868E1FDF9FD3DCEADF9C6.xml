<subSubSection box="[189,1048,1107,1181]" pageId="158" pageNumber="159" type="distribution">
<paragraph blockId="158.[189,342,1107,1133]" box="[189,342,1107,1133]" pageId="158" pageNumber="159">
<heading bold="true" box="[189,342,1107,1133]" fontSize="11" level="3" pageId="158" pageNumber="159" reason="3">
<emphasis bold="true" box="[189,342,1107,1133]" pageId="158" pageNumber="159">Distribution</emphasis>
</heading>
</paragraph>
<paragraph blockId="158.[189,1048,1154,1181]" box="[189,1048,1154,1181]" pageId="158" pageNumber="159">
Boreal North Pacific, 
<collectingCountry box="[443,565,1154,1180]" name="Antarctica" pageId="158" pageNumber="159">Antarctica</collectingCountry>
(?), 
<quantity box="[620,755,1154,1181]" metricMagnitude="3" metricUnit="m" metricValue="1.302" metricValueMax="2.55" metricValueMin="0.054" pageId="158" pageNumber="159" unit="m" value="1302.0" valueMax="2550.0" valueMin="54.0">54–2550 m</quantity>
, possibly below 
<quantity box="[952,1044,1155,1181]" metricMagnitude="3" metricUnit="m" metricValue="3.0" pageId="158" pageNumber="159" unit="m" value="3000.0">3000 m</quantity>
.
</paragraph>
