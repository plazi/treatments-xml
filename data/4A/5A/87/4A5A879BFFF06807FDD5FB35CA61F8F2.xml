<taxonomicName authorityName="Chevreux" authorityYear="1912" box="[500,888,726,752]" class="Malacostraca" family="Epimeriidae" genus="Pseudepimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="120" pageNumber="121" phylum="Arthropoda" rank="species" species="grandirostris" subGenus="Pseudepimeria">
