<paragraph blockId="3.[151,1437,953,1880]" box="[199,1101,1349,1377]" pageId="3" pageNumber="404">
<emphasis bold="true" box="[199,382,1350,1376]" pageId="3" pageNumber="404">Measurements.</emphasis>
7¾ whorls, H= 
<quantity box="[556,659,1349,1377]" metricMagnitude="-2" metricUnit="m" metricValue="2.58" pageId="3" pageNumber="404" unit="mm" value="25.8">25.8 mm</quantity>
, D= 
<quantity box="[705,806,1349,1377]" metricMagnitude="-2" metricUnit="m" metricValue="2.57" pageId="3" pageNumber="404" unit="mm" value="25.7">25.7 mm</quantity>
