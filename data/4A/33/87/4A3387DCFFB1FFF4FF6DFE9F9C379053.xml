<subSubSection box="[199,1138,851,879]" pageId="15" pageNumber="416" type="description">
<paragraph blockId="15.[151,1437,491,1095]" box="[199,1138,851,879]" pageId="15" pageNumber="416">
<emphasis bold="true" box="[199,382,852,878]" pageId="15" pageNumber="416">Measurements.</emphasis>
