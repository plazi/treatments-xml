<quantity box="[268,383,1871,1892]" metricMagnitude="-1" metricUnit="m" metricValue="1.05" metricValueMax="1.5" metricValueMin="0.6" pageId="78" pageNumber="79" unit="mm" value="105.0" valueMax="150.0" valueMin="60.0">60–150 mm</quantity>
...................................................................................... 2
</keyLead>
</paragraph>
<paragraph blockId="78.[151,1436,1842,2010]" pageId="78" pageNumber="79">
<keyLead pageId="78" pageNumber="79">
