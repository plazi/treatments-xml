<taxonomicName authority="White, 1847" authorityName="White" authorityYear="1847" box="[1182,1436,251,272]" class="Malacostraca" family="Oxycephalidae" genus="Rhabdosoma" kingdom="Animalia" order="Amphipoda" pageId="69" pageNumber="70" phylum="Arthropoda" rank="genus">
