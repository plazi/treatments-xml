<emphasis bold="true" box="[703,759,1695,1717]" pageId="25" pageNumber="26">A–C.</emphasis>
<taxonomicName authority="Chopard" authorityName="Chopard" authorityYear="1922" box="[765,1076,1695,1717]" class="Insecta" family="Corydiidae" genus="Polyphaga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="thibetana">
