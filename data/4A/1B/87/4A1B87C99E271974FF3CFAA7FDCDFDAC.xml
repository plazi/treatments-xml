<collectingCountry box="[359,426,583,608]" name="China" pageId="54" pageNumber="55">China</collectingCountry>
(
<collectingRegion box="[440,529,584,608]" country="China" name="Yunnan" pageId="54" pageNumber="55">Yunnan</collectingRegion>
) (
