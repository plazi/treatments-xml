<collectingCountry box="[359,426,1303,1328]" name="China" pageId="49" pageNumber="50">China</collectingCountry>
(
<collectingRegion box="[440,529,1304,1328]" country="China" name="Yunnan" pageId="49" pageNumber="50">Yunnan</collectingRegion>
) (
