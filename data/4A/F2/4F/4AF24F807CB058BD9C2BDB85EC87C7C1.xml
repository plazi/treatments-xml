<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Insecta" family="Phylliidae" genus="Phyllium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phyllium celebicum" order="Orthoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="celebicum">
