<taxonomicName id="14924D3CFFC3FFDBFC87A0FC51460630" baseAuthorityName="Fabricius" baseAuthorityYear="1787" box="[793,878,1012,1031]" class="Insecta" family="Pentatomidae" genus="Antiteuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="55" pageNumber="220" phylum="Arthropoda" rank="species" species="mixtus">
