<caption id="87ED6637FFC3FFDBFC87A0D351C60631" ID-DOI="http://doi.org/10.5281/zenodo.3966154" ID-Zenodo-Dep="3966154" httpUri="https://zenodo.org/record/3966154/files/figure.png" pageId="55" pageNumber="220" startId="55.[793,825,987,1006]" targetBox="[790,1449,231,957]" targetPageId="55">
