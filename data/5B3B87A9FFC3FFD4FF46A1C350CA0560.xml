<taxonomicName id="14924D3CFFC3FFDBFAD5A0D357B601DB" baseAuthorityName="Fabricius" baseAuthorityYear="1787" box="[1355,1438,987,1006]" class="Insecta" family="Pentatomidae" genus="Antiteuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="55" pageNumber="220" phylum="Arthropoda" rank="species" species="mixtus">
