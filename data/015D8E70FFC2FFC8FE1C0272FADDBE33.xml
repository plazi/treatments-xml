<taxonomicName id="4EF444E5FFC1FFCEFDA204A1FC8ABDEA" authority="Stal, 1866" authorityName="Stal" authorityYear="1866" box="[581,806,1980,2007]" class="Insecta" family="Reduviidae" genus="Voconia" kingdom="Animalia" order="Hemiptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="genus">
