) and 
<collectingRegion id="805FF832FFDA8709FF770F3DFEC9FCAB" box="[201,310,367,393]" country="Brazil" name="Rondonia" pageId="6" pageNumber="441">Rondônia</collectingRegion>
(
<bibRefCitation id="260A4B21FFDA8709FEF80F3DFDD4FCAB" author="Firmino, V. C. &amp; Ribeiro, J. M. F. &amp; Dos Santos, E. M. &amp; Juen, L." box="[326,555,367,393]" pageId="6" pageNumber="441" pagination="446 - 450" refId="ref3999" refString="Firmino, V. C., Ribeiro, J. M. F., Dos Santos, E. M. &amp; Juen, L. (2019) First occurrence of Anacroneuria singularis Righi-Cavallaro &amp; Lecci, 2010 (Plecoptera: Perlidae) in Rondonia, western Amazonia, Brazil. Zootaxa, 4544 (3), 446 - 450. https: // doi. org / 10.11646 / zootaxa. 4544.3.10" type="journal article" year="2019">
Firmino 
<emphasis id="70EFEAC2FFDA8709FE150F3DFE21FCAB" box="[427,478,367,393]" italics="true" pageId="6" pageNumber="441">et al</emphasis>
. 2019
</bibRefCitation>
); and in the Cerrado in the states of 
<collectingRegion id="805FF832FFDA8709FC6E0F3DFB49FCAB" box="[976,1206,367,393]" country="Brazil" name="Mato Grosso do Sul" pageId="6" pageNumber="441">Mato Grosso do Sul</collectingRegion>
(
<bibRefCitation id="260A4B21FFDA8709FB790F3DFEE8FC8F" author="Righi-Cavallaro, K. O. &amp; Lecci, L. S." pageId="6" pageNumber="441" pagination="35 - 44" refId="ref5296" refString="Righi-Cavallaro, K. O. &amp; Lecci, L. S. (2010) Three new species of Anacroneuria (Plecoptera: Perlidae) from Centre-West and Southeast Brazil. Zootaxa, 2683 (1), 35 - 44. https: // doi. org / 10.11646 / zootaxa. 2683.1.3" type="journal article" year="2010">Righi-Cavallaro &amp; Lecci 2010</bibRefCitation>
), 
<collectingRegion id="805FF832FFDA8709FE940FC1FE40FC8F" box="[298,447,403,429]" country="Brazil" name="Minas Gerais" pageId="6" pageNumber="441">Minas Gerais</collectingRegion>
(
<bibRefCitation id="260A4B21FFDA8709FE730FC1FD37FC8F" author="Novaes, M. C. &amp; Bispo, P. C." box="[461,712,403,429]" pageId="6" pageNumber="441" pagination="433 - 442" refId="ref4682" refString="Novaes, M. C. &amp; Bispo, P. C. (2014) Plecoptera from Minas Gerais State, southeastern Brazil. Zootaxa, 3856 (3), 433 - 442. https: // doi. org / 10.11646 / zootaxa. 3856.3.8" type="journal article" year="2014">Novaes &amp; Bispo 2014</bibRefCitation>
