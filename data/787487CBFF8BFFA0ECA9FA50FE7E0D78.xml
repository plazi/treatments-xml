<taxonomicName id="37DD4D5EFF8BFFA1ECA9F9C7FDD50ABB" authority="Priesner, 1952: 878" authorityName="Priesner" authorityPageNumber="878" authorityYear="1952" box="[151,564,1639,1664]" class="Insecta" family="Thysanoptera (awaiting allocation)" genus="Fulgorothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="559" phylum="Arthropoda" rank="species" species="lesnei">
