<taxonomicName id="C9C60D183D00AFFED9E1EEB70949ED25" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142223" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole (Ceratopheidole) clydei Gregg" lsidName-HNS="Pheidole (Ceratopheidole) clydei Gregg" order="Hymenoptera" pageNumber="539" phylum="Arthropoda" rank="species" species="clydei" subGenus="Ceratopheidole">Pheidole (Ceratopheidole) clydei Gregg</taxonomicName>
