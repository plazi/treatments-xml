, (northeastern 
<collectingCountry id="F3357610FFCFFFD5192D56E5FAE1F81F" box="[1285,1394,1952,1978]" name="Australia" pageId="10" pageNumber="33">Australia</collectingCountry>
) in 
<quantity id="4CDA9B65FFCFFFD51CBF568DFE8FF847" box="[151,284,1992,2018]" metricMagnitude="2" metricUnit="m" metricValue="5.9799999999999995" metricValueMax="6.06" metricValueMin="5.9" pageId="10" pageNumber="33" unit="m" value="598.0" valueMax="606.0" valueMin="590.0">590–606 m</quantity>
depth (
<figureCitation id="13192A05FFCFFFD51D5C568DFE2AF847" box="[372,441,1992,2018]" captionStart="FIGURE 1" captionStartId="1.[151,255,1969,1993]" captionTargetBox="[469,1105,1483,1953]" captionTargetId="figure@1.[448,1139,1477,1960]" captionTargetPageId="1" captionText="FIGURE 1. Map showing the capture localities of the four new Parmaturus species: P. albimarginatus (white oval), P. albipenis (white oval), P. bigus (black oval) and P. lanatus (black star)." httpUri="https://zenodo.org/record/179872/files/figure.png" pageId="10" pageNumber="33">
