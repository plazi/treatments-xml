<number id="3DEA24A7FFCFFFD51FF456E5FB9FF81F" box="[988,1036,1952,1978]" pageId="10" pageNumber="33" value="154.0">154</number>
° 
<number id="3DEA24A7FFCFFFD5183F56E5FBA5F81F" box="[1047,1078,1952,1978]" pageId="10" pageNumber="33" value="21.0">21</number>
’E
