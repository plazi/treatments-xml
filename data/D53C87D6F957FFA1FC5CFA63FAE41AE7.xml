(Drury)
</taxonomicName>
and 
<taxonomicName id="9A954D43F956FFA1FE59F98FFD211D56" authority="(Closs)" baseAuthorityName="Closs" box="[405,670,1608,1630]" class="Insecta" family="Sphingidae" genus="Xylophanes" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="hannemanni">
