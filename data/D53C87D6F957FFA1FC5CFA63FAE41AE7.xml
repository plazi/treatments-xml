<taxonomicName id="9A954D43F956FFA1FDF3FA0BFD7D1EEB" baseAuthorityName="Pittaway" baseAuthorityYear="1993" box="[575,706,1485,1507]" class="Insecta" family="Sphingidae" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="family">Sphingidae</taxonomicName>
