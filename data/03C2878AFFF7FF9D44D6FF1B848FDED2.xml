<collectingCountry id="F37C760CFFF7FF9D4598FB318318DFF7" box="[473,523,1213,1234]" name="Peru" pageId="17" pageNumber="571">Peru</collectingCountry>
)......................................... 
<taxonomicName id="4C6B4D1FFFF7FF9D4044FB318488DFF7" authority="Goode &amp; Bean 1895" authorityName="Goode &amp; Bean" authorityYear="1895" box="[1029,1435,1213,1234]" class="Holocephali" family="Rhinochimaeridae" genus="Harriotta" kingdom="Animalia" order="Chimaeriformes" pageId="17" pageNumber="571" phylum="Chordata" rank="species" species="raleighana">
