<materialsCitation id="3B033CC1FFF7FF9D45E1FBCB8488DFB2" ID-GBIF-Occurrence="3382541307" collectionCode="BDL" country="Chile" location="Southern" pageId="17" pageNumber="571" specimenCount="1" stateProvince="California">
