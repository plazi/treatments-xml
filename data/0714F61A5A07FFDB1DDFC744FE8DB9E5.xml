<taxonomicName id="48BD3C8F5A06FFDC1D9DC0EBFB74BB9A" authorityName="Henn" authorityYear="1916" box="[913,1195,430,453]" class="Actinopterygii" family="Poeciliidae" genus="Phalloptychus" kingdom="Animalia" order="Cyprinodontiformes" pageId="3" pageNumber="376" phylum="Chordata" rank="species" species="eigenmanni">
