<taxonomicName id="4C304D343898FF442AA0FD22FBC2FD7E" box="[1001,1118,716,742]" class="Insecta" family="Ptiliidae" genus="Cissidium" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="138" pageNumber="139" phylum="Arthropoda" rank="genus">
