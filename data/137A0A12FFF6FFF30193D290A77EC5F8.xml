<quantity id="5C2B16E1FFF6FFF30523D7E9A248C46D" box="[1063,1123,1518,1543]" metricMagnitude="1" metricUnit="m" metricValue="5.3" pageId="9" pageNumber="129" unit="m" value="53.0">53 m</quantity>
