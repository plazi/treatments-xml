<subSubSection id="C35D657AFFCEFFB188BC79D8FE89C6C4" pageId="5" pageNumber="518" type="etymology">
<paragraph id="8BF836F1FFCEFFB188BC79D8FE89C6C4" blockId="5.[151,1437,1194,2013]" pageId="5" pageNumber="518">
<emphasis id="B933EAE3FFCEFFB188BC79D8FE8AC6E0" bold="true" box="[199,334,1447,1473]" pageId="5" pageNumber="518">Etymology.</emphasis>
