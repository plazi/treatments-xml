<quantity id="4C8A9B253B59E717FCF1FE30377BE198" box="[795,885,432,458]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="11" pageNumber="62" unit="mm" value="1.5">1.5 mm</quantity>
