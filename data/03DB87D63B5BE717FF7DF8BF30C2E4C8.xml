<quantity id="4C8A9B253B59E717FDBFFE3036A9E198" box="[597,679,432,458]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" pageId="11" pageNumber="62" unit="mm" value="18.0">18 mm</quantity>
