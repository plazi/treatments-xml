<taxonomicName id="7AFB4D65FFEEFFE484B7F805FA92F823" authority="Germar, 1817" authorityName="Germar" authorityYear="1817" box="[1021,1325,1973,1997]" class="Insecta" family="Curculionidae" genus="Anthonomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="17" phylum="Arthropoda" rank="genus">
