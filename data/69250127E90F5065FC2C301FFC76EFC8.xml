<collectingCountry id="999BF0A1E90E5065FF3A3318FF26E835" box="[164,234,1625,1647]" name="Brazil" pageId="45" pageNumber="74">Brazil</collectingCountry>
and 
<collectingCountry id="999BF0A1E90E5065FEB63318FE4BE835" box="[296,391,1625,1647]" name="Ecuador" pageId="45" pageNumber="74">Ecuador</collectingCountry>
