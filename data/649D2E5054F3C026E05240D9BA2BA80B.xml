<taxonomicName id="2A0E5E6D5B1B5664DE3898EABD2BD909" LSID="urn:lsid:zoobank.org:act:7728E3EC-1FCE-409B-BF16-1C6A84D372AE" authority="Aristov &amp; Storozhenko" class="Insecta" family="Permulidae" genus="Permula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Permula edemskii" order="Grylloblattodea" pageId="4" pageNumber="115" phylum="Arthropoda" rank="species" species="edemskii">Permula edemskii Aristov &amp; Storozhenko</taxonomicName>
