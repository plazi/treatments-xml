<taxonomicName id="05B34D6C661547672916FB5EC08EFA88" authority="Karny" authorityName="Karny" authorityYear="1915" box="[635,907,1266,1288]" class="Insecta" family="Phlaeothripidae" genus="Dolerothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="atavus">
