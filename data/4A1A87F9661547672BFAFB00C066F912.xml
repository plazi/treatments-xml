<taxonomicName id="05B34D6C661547672BFAFB5EC2E7FA88" authority="Priesner, 1952: 199" authorityName="Priesner" authorityPageNumber="199" authorityYear="1952" box="[151,482,1266,1288]" class="Insecta" family="Phlaeothripidae" genus="Thlibothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="genus">
