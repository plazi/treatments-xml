<taxonomicName id="ACF94D01B309B92CAF46458EFE02FB54" box="[281,470,1221,1243]" class="Malacostraca" family="Pilumnidae" genus="Leelumnus" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="63" phylum="Arthropoda" rank="species" species="radium">
