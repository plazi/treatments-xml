<typeStatus id="7207880D5350FFCBFF4D081D28D5FBEC" box="[151,201,1053,1077]" pageId="13" pageNumber="74">Type</typeStatus>
species: 
<taxonomicName id="6ABC4D2C5350FFCBFEFC081D2ABAFBEC" authority="Meyrick, 1913" authorityName="Meyrick" authorityYear="1913" box="[294,678,1052,1077]" class="Insecta" family="Lecithoceridae" genus="Lecithocera" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="74" phylum="Arthropoda" rank="species" species="picrodora">
