<taxonomicName id="A0AC495DDD25682067C29A241E0D03DB" authority="Blainville, 1824" authorityName="Blainville" authorityYear="1824" class="Gastropoda" family="Lymnaeidae" genus="Limnaea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Limnaea elmeteitensis" order="Hygrophila" pageId="3" pageNumber="134" phylum="Mollusca" rank="species" species="elmeteitensis">Limnaea elmeteitensis</taxonomicName>
