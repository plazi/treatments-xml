<taxonomicName id="6BAA74DB366C53BE36BD36CA4E9C1C1D" class="Hexapoda" family="Bostrichidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="1" pageNumber="12" phylum="Arthropoda" rank="family">Bostrichidae</taxonomicName>
) and 
<taxonomicName id="29DCE2FE87CFF48DD6682C70E6260B3E" class="Hexapoda" family="Cerambycidae" genus="Phoracantha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="1" pageNumber="12" phylum="Arthropoda" rank="genus">
<emphasis id="A3133D6D7B1C7D1E79F2EFC9F0EAF1C1" italics="true" pageId="1" pageNumber="12">Phoracantha</emphasis>
</taxonomicName>
