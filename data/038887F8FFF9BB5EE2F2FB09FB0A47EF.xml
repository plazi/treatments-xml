<paragraph id="8B9E36EEFFF9BB5DE3B3FA34FDE641B1" blockId="1.[160,616,1418,1478]" box="[160,616,1454,1478]" pageId="1" pageNumber="2278">
<taxonomicName id="4C214D6DFFF9BB5DE3B3FA34FE4F41B1" authority="Barnard, 1969" authorityName="Barnard" authorityYear="1969" box="[160,449,1454,1478]" class="Malacostraca" family="Hadziidae" genus="Dulzura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="2278" phylum="Arthropoda" rank="species" species="sal">
<emphasis id="B955EAFCFFF9BB5DE3B3FA34FE9241B1" box="[160,284,1454,1478]" italics="true" pageId="1" pageNumber="2278">Dulzura sal</emphasis>
<bibRefCitation id="EFB04B1FFFF9BB5DE237FA34FE4F41B1" author="Barnard JL" box="[292,449,1454,1478]" pageId="1" pageNumber="2278" pagination="1 - 230" refId="ref8205" refString="Barnard JL. 1969. Gammaridean Amphipoda of the rocky intertidal of California: Monterey Bay to La Jolla. Bull US Nat Mus. 258: 1 - 230." type="journal article" year="1969">Barnard, 1969</bibRefCitation>
</taxonomicName>
