, 
<taxonomicName id="4C384D40437E6B61FDF6FE6DFCD9FADB" authority="Hennipman, 1966" authorityName="Hennipman" authorityYear="1966" box="[593,797,1305,1330]" class="Polypodiopsida" family="Blechnaceae" genus="Pteridoblechnum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Pteridoblechnum</taxonomicName>
, 
<taxonomicName id="4C384D40437E6B61FC8EFE6DFC4BFADB" box="[809,911,1305,1330]" class="Polypodiopsida" family="Blechnaceae" genus="Sadleria" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Sadleria</taxonomicName>
, 
<taxonomicName id="4C384D40437E6B61FC3CFE6DFB27FADB" authority=", Stenochlaena" authorityName="Stenochlaena" box="[923,1251,1305,1330]" class="Polypodiopsida" family="Blechnaceae" genus="Salpichlaena" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Salpichlaena, Stenochlaena</taxonomicName>
, 
<taxonomicName id="4C384D40437E6B61FB49FE6DFA45FADB" box="[1262,1409,1305,1330]" class="Polypodiopsida" family="Blechnaceae" genus="Woodwardia" kingdom="Plantae" order="Polypodiales" pageId="9" pageNumber="16" phylum="Tracheophyta" rank="genus">Woodwardia</taxonomicName>
</emphasis>
