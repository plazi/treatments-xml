<paragraph id="5C9CB12152CC4211C28CB95F4C83B208" pageId="0" pageNumber="55">
<taxonomicName id="B38E777AE4ED864AF17A9261C29142F2" authority="Tong &amp; Li, 2021" authorityName="Tong &amp; Li" authorityYear="2021" class="Arachnida" family="Oonopidae" genus="Paramolotra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paramolotra pome" order="Araneae" pageId="0" pageNumber="55" phylum="Arthropoda" rank="species" species="pome">
