<taxonomicName id="A41F3AAF3479A6269A513E66E8F338E2" authority="Tong &amp; Li, 2021" authorityName="Tong &amp; Li" authorityYear="2021" class="Arachnida" family="Oonopidae" genus="Paramolotra" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paramolotra pome" order="Araneae" pageId="0" pageNumber="55" phylum="Arthropoda" rank="species" species="pome">
