<collectingRegion id="9654F83BA53B89168F8041388C45393A" box="[1008,1097,2057,2081]" country="Turkey" name="Hakkari" pageId="37" pageNumber="310">Hakkâri</collectingRegion>
, 
<collectingRegion id="9654F83BA53B8916882341388CB8393A" box="[1107,1204,2057,2081]" country="Turkey" name="Nevsehir" pageId="37" pageNumber="310">Nevşehir</collectingRegion>
