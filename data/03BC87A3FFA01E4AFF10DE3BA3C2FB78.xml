<taxonomicName id="4C154D36FFA01E4AFC12D97FA465FB87" authorityName="THOMSON 1894:" authorityYear="1809" box="[944,1086,1116,1137]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="49" pageNumber="1790" phylum="Arthropoda" rank="species" species="parvicornis">
