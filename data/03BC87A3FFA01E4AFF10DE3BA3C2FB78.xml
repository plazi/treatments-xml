<taxonomicName id="4C154D36FFA01E4AFCEAD95AA3C9FB78" authorityName="FABRICIUS" authorityYear="1804" box="[840,914,1145,1166]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="49" pageNumber="1790" phylum="Arthropoda" rank="genus">
