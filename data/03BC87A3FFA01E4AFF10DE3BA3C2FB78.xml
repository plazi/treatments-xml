<taxonomicName id="4C154D36FFA01E4AFD94D91DA295FBA2" authorityName="SZEPLIGETI" authorityYear="1901" box="[566,718,1086,1108]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="49" pageNumber="1790" phylum="Arthropoda" rank="species" species="carbonarius">
<emphasis id="B961EAA7FFA01E4AFD94D91DA295FBA2" box="[566,718,1086,1108]" italics="true" pageId="49" pageNumber="1790">B. carbonarius</emphasis>
</taxonomicName>
with 
<taxonomicName id="4C154D36FFA01E4AFCA8D91DA3C0FBA5" authorityName="THOMSON 1894:" authorityYear="1809" box="[778,923,1086,1107]" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="49" pageNumber="1790" phylum="Arthropoda" rank="species" species="parvicornis">
