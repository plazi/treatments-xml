<geoCoordinate id="3D8C5DAD5B12000AFD9CFA23FD767DED" box="[595,685,1461,1485]" degrees="2.35000" direction="east" orientation="longitude" pageId="30" pageNumber="266" precision="1" value="2.35">2.35000</geoCoordinate>
