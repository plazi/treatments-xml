<geoCoordinate id="3D8C5DAD5B12000AFE21FA23FD9C7DED" box="[494,583,1461,1485]" degrees="6.01667" direction="north" orientation="latitude" pageId="30" pageNumber="266" precision="1" value="6.01667">6.01667</geoCoordinate>
