<taxonomicNameLabel id="A24DF174FFD78C09FD7E71ADDDA2F9FF" box="[671,738,1515,1536]" pageId="3" pageNumber="1000" rank="species">nov.sp.</taxonomicNameLabel>
(
<emphasis id="B97E4C0FFFD78C09FD1171ACDC4EFA00" bold="true" box="[752,782,1514,1535]" pageId="3" pageNumber="1000">5-8</emphasis>
): (
<emphasis id="B97E4C0FFFD78C09FCCB71ACDC0DF9FF" bold="true" box="[810,845,1514,1536]" pageId="3" pageNumber="1000">1, 5</emphasis>
) habitus; (
<emphasis id="B97E4C0FFFD78C09FC5271ACDC96FA00" bold="true" box="[947,982,1514,1535]" pageId="3" pageNumber="1000">2, 6</emphasis>
) forebody; (
