<paragraph id="8B91E9653176BA18A9C7FA49792CF98E" blockId="10.[151,565,1483,1542]" box="[151,295,1518,1542]" pageId="10" pageNumber="24">
(Figs 
<number id="3DE6FB423176BA18A987FA497915F98E" box="[215,286,1518,1542]" pageId="10" pageNumber="24" value="14.0" valueMax="16.0" valueMin="12.0">12–16</number>
)
</paragraph>
