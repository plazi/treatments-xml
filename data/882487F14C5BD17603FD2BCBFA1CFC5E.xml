<treatmentCitationGroup id="209D11C94C5BD17903FD2C7BFB96FBFC" box="[151,1040,1051,1074]" pageId="23" pageNumber="24">
<taxonomicName id="C78D4D644C5BD17903FD2C7BFD52FBFC" authority="Gressitt &amp; Sedlacek, 1967: 486" authorityName="Gressitt &amp; Sedlacek" authorityPageNumber="486" authorityYear="1967" box="[151,724,1051,1074]" class="Insecta" family="Curculionidae" genus="Gymnopholus" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="toxopei">
<emphasis id="32F9EAF54C5BD17903FD2C7BFE07FBFC" box="[151,385,1051,1074]" italics="true" pageId="23" pageNumber="24">Gymnopholus toxopei</emphasis>
<treatmentCitation id="812C10F64C5BD17902ED2C7BFD52FBFC" author="Gressitt" box="[391,724,1051,1074]" page="486" pageId="23" pageNumber="24" year="1967">
