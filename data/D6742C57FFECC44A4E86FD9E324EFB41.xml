<taxonomicName id="99DDE6C2FFECC44A4C45FC9A32BFFC26" box="[596,923,872,896]" class="Insecta" family="Formicidae" genus="Bothriomyrmex" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="247" phylum="Arthropoda" rank="species" species="meridionalis">
