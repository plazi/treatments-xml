<taxonomicName id="4C6A4D67FF9AFFAFFF5B4C3D57AEFC94" authority="Nikolsky, 1907" authorityName="Nikolsky" authorityYear="1907" box="[151,470,799,824]" class="Reptilia" family="Gekkonidae" genus="Microgecko" kingdom="Animalia" order="Squamata" pageId="22" pageNumber="23" phylum="Chordata" rank="genus">
