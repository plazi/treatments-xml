<taxonomicName id="4B56F681EC059FD07B92468F95DCED14" authorityName="Gistel" authorityYear="1850" class="Insecta" family="Scarabaeidae" genus="Rutela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Rutela runica" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="runica">Rutela runica</taxonomicName>
