<taxonomicName id="4C70A5BCFFBBFF9EFF14FA03FE7B0BCB" box="[254,424,1471,1495]" class="Malacostraca" family="Pandalidae" genus="Chlorotocus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="30" pageNumber="475" phylum="Arthropoda" rank="genus">
<emphasis id="B904022DFFBBFF9EFF14FA03FE7B0BCB" box="[254,424,1471,1495]" italics="true" pageId="30" pageNumber="475">CHLOROTOCUS</emphasis>
