<number id="3DAF24F4FFA7E5360BDAFC55596A9B4B" box="[500,531,1846,1872]" pageId="33" pageNumber="34" value="39.0">39</number>
</figureCitation>
A; 
<number id="3DAF24F4FFA7E5360818FC55592E9B4B" box="[566,599,1846,1872]" pageId="33" pageNumber="34" value="40.0">40</number>
A,B; 
<number id="3DAF24F4FFA7E53608BDFC5559CB9B4B" box="[659,690,1846,1872]" pageId="33" pageNumber="34" value="41.0">41</number>
A,B; 
<number id="3DAF24F4FFA7E53608DEFC5558689B4B" box="[752,785,1846,1872]" pageId="33" pageNumber="34" value="42.0">42</number>
