alt., 
<date id="FF84101BFF8FFFAAFDF96413FD353115" box="[582,720,1578,1602]" pageId="16" pageNumber="321" value="2009-08-23">
<number id="3DF224FCFF8FFFAAFDF96413FD813115" box="[582,612,1578,1602]" pageId="16" pageNumber="321" value="23.0">23</number>
