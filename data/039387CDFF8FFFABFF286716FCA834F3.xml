<taxonomicName id="4C3A4D58FF8FFFAAFC75643EFBEB3149" box="[970,1038,1543,1566]" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="16" pageNumber="321" phylum="Tracheophyta" rank="genus">
