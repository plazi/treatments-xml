<paragraph id="8BD45242FFDFFFDBFF41FD81FA785F13" blockId="17.[159,1419,450,740]" box="[159,1418,564,586]" pageId="17" pageNumber="272">
<taxonomicName id="4C6B29C1FFDFFFDBFF41FD81FF155F13" box="[159,231,564,586]" class="Elasmobranchii" family="Scyliorhinidae" genus="Galeus" kingdom="Animalia" order="Carcharhiniformes" pageId="17" pageNumber="272" phylum="Chordata" rank="genus">
<emphasis id="B91F8E50FFDFFFDBFF41FD81FF155F13" box="[159,231,564,586]" italics="true" pageId="17" pageNumber="272">Galeus</emphasis>
