<quantity id="4CB60DDA9C50FFE4990297B33CBDEAA7" box="[597,663,1469,1491]" metricMagnitude="0" metricUnit="m" metricValue="2.5" metricValueMax="5.0" metricValueMin="0.0" pageId="2" unit="m" value="2.5" valueMax="5.0" valueMin="0.0">0–5 m</quantity>
