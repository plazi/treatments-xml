<taxonomicName id="65364F4142D9DAB567C1DDBEDBE9BCC8" authority="Simutnik, 2021" authorityName="Simutnik, gen. et" authorityYear="2021" class="Insecta" family="Chalcidoidea" genus="Sakhalinencyrtus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sakhalinencyrtus leleji" order="Hymenoptera" pageId="0" pageNumber="361" phylum="Arthropoda" rank="species" species="leleji">
