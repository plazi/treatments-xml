<materialsCitation id="3B437003CB79C248CEF6FC76FA9BC290" box="[151,1299,925,948]" collectionCode="UCDC" collectorName="M. S. Wasbauer &amp; J. E. Slansky" country="Mexico" location="Tamaulipas" pageId="101" pageNumber="102" specimenCount="1" stateProvince="Tamaulipas" typeStatus="holotype">
