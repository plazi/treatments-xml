<taxonomicName id="E9A10F81FFAFFF82CB01FD3FFC4939D9" authorityName="Shuckard" authorityYear="1840" box="[892,1022,642,664]" class="Insecta" family="Ampulicidae" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="975" phylum="Arthropoda" rank="subFamily" subFamily="Ampulicinae">Ampulicinae</taxonomicName>
