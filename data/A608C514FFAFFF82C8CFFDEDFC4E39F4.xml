<taxonomicName id="E9A10F81FFAFFF82C8CFFD22FE8339F4" authorityName="Shuckard" authorityYear="1840" box="[178,308,671,693]" class="Insecta" family="Ampulicidae" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="975" phylum="Arthropoda" rank="family">Ampulicidae</taxonomicName>
