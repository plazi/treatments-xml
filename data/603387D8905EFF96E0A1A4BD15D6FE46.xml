<taxonomicName id="2F9A4D4D905EFF96E0A1A4BD14DCFF07" authority="Chace, 1985" authorityName="Chace" authorityYear="1985" box="[151,469,151,178]" class="Malacostraca" family="Pandalidae" genus="Plesionika" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="584" phylum="Arthropoda" rank="species" species="intermedia">
