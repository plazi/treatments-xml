<quantity id="25769B72FF82FFDAFC86FDB8FC002612" box="[807,902,524,550]" metricMagnitude="2" metricUnit="m" metricValue="1.9899999999999998" pageId="7" pageNumber="8" unit="m" value="199.0">199 m</quantity>
