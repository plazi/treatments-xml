<quantity id="25769B72FF82FFDAFCAAFE08FCEF25E2" box="[779,873,444,470]" metricMagnitude="2" metricUnit="m" metricValue="6.76" pageId="7" pageNumber="8" unit="m" value="676.0">676 m</quantity>
long. Temporal setae 
<number id="544624B0FF82FFDAFBD4FE08FB1225E2" box="[1141,1172,444,470]" pageId="7" pageNumber="8" value="17.0">17</number>
