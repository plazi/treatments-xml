<quantity id="25769B72FF82FFDAFEEAFD80FE11267A" box="[331,407,564,590]" metricMagnitude="1" metricUnit="m" metricValue="7.5" pageId="7" pageNumber="8" unit="m" value="75.0">75 m</quantity>
