<taxonomicName id="EEE51FBE174EFFB9FD8DF8C2FD775567" box="[588,739,1856,1880]" class="Insecta" family="Aclopidae" genus="Gracilaclopus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="crepuscularis">
