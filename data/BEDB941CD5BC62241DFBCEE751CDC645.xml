<paragraph id="5D58C1F922ADD5947858E9C7D20B5D51" pageNumber="248">
<taxonomicName id="A10FD1AA3911B2FFA8D9618B7E7334F2" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145411" authority="Wheeler" authorityName="Wheeler" class="Hexapoda" family="Formicidae" genus="Viticicola" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Viticicola tessmanni var. castanea Wheeler" order="Hymenoptera" pageNumber="248" phylum="Arthropoda" rank="variety" species="tessmanni" variety="castanea">Viticicola tessmanni var. castanea Wheeler</taxonomicName>
