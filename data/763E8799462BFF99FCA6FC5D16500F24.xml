<taxonomicName id="39974D0C462AFF9FFE5CFDEA16250EEC" authority="Wenz, 2003" authorityName="Wenz" authorityYear="2003" box="[424,577,636,653]" class="Actinopterygii" family="Lepidotidae" genus="Lepidotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepisosteiformes" pageId="11" pageNumber="492" phylum="Chordata" rank="species" species="tanyrhis" status="n. sp.">
