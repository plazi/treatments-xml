<geoCoordinate id="EE6B1082FFF7FFB1FB50DDECFB20FB44" box="[1199,1274,1216,1240]" degrees="12.19" direction="south" orientation="latitude" pageId="18" pageNumber="293" precision="555" value="-12.19">12.19S</geoCoordinate>
