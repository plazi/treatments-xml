<taxonomicName id="4C704D5E5E21FFD51E6F5602FC52FEEE" authority="Thouars, Hist. Orchid." authorityName="Thouars, Hist. Orchid." class="Liliopsida" family="Orchidaceae" genus="Dryorkis" kingdom="Plantae" order="Asparagales" pageId="25" pageNumber="180" phylum="Tracheophyta" rank="species" species="antidris">
