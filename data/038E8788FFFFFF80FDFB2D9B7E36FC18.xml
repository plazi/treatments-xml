<taxonomicName id="4C274D1DFFF8FF80FF7C2E747D09FE56" authorityName="Vinarski &amp; Palatov &amp; Glöer" authorityYear="2014" box="[144,362,369,393]" class="Gastropoda" family="Hydrobiidae" genus="Pontohoratia" kingdom="Animalia" order="Littorinimorpha" pageId="12" pageNumber="2248" phylum="Mollusca" rank="species" species="smyri">
