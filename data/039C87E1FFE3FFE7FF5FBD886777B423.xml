<taxonomicName id="4C354D74FFE3FFE7FF26BD8864FDB6CF" authority="Stal, 1877" authorityName="Stal" authorityYear="1877" box="[238,466,224,252]" class="Insecta" family="Haglotettigoniidae" genus="Vescelia" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="106" phylum="Arthropoda" rank="genus">
<emphasis id="B941EAE5FFE3FFE7FF26BD88647FB6C8" bold="true" box="[238,336,225,252]" italics="true" pageId="5" pageNumber="106">Vescelia</emphasis>
