<taxonomicName id="4C764D6FFFF24C64FE5FED0B6D0DF8F7" authorityName="Bosquet" authorityYear="1852" box="[469,573,1857,1883]" class="Ostracoda" family="Cyprididae" genus="Cypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
