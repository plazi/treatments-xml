<taxonomicName id="4C4F4D0CFFDFFFA9F21DF95C2C0A7286" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,514,1697,1721]" class="Insecta" family="Derbidae" genus="Muiralevu" kingdom="Animalia" order="Hemiptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="telum">
