<taxonomicName id="4C705B88A7378D37FD00FA02FCAED7F3" authorityName="Stainton" authorityYear="1854" box="[641,769,1491,1515]" class="Insecta" family="Lyonetiidae" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="295" phylum="Arthropoda" rank="family">Lyonetiidae</taxonomicName>
) (
<figureCitation id="134B3C8EA7378D37FC96FA02FCF9D7F3" box="[791,854,1491,1515]" captionStart="FIGURE 4" captionStartId="4.[151,250,723,747]" captionTargetBox="[151,1435,181,699]" captionTargetId="figure-14@4.[151,1436,181,699]" captionTargetPageId="4" captionText="FIGURE 4. Lyonetiidae host- A. Pupal case, B. Adult moth." figureDoi="http://doi.org/10.5281/zenodo.5568744" httpUri="https://zenodo.org/record/5568744/files/figure.png" pageId="3" pageNumber="295">Fig. 4</figureCitation>
