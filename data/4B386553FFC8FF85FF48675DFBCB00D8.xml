<paragraph id="C32ED445FFC8FF8AFF4867B0FDF700F5" blockId="39.[151,580,367,441]" box="[151,545,419,441]" pageId="39" pageNumber="622">
<taxonomicName id="0491AFC6FFC8FF8AFF4867B0FDF700F5" authority="SCHMIEDEKNECHT, 1900" authorityName="SCHMIEDEKNECHT" authorityYear="1900" box="[151,545,419,441]" class="Insecta" family="Ichneumonidae" genus="Cryptus" kingdom="Animalia" order="Hymenoptera" pageId="39" pageNumber="622" phylum="Arthropoda" rank="species" species="hannibal">
