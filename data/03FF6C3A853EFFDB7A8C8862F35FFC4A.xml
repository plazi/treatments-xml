<heading id="D0A16A40853EFFDA7A8C8862F4D3FC04" bold="true" box="[151,669,886,912]" fontSize="11" level="1" pageId="14" pageNumber="15" reason="1">
<taxonomicName id="4C56A6AF853EFFDA7A8C8862F4D3FC04" authority="Tullberg, 1871" authorityName="Tullberg" authorityYear="1871" box="[151,669,886,912]" class="Entognatha" family="Neanuridae" genus="Pseudachorutes" kingdom="Animalia" order="Collembola" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="subcrassus">
