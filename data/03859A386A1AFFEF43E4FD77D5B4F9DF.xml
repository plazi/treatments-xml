<quantity id="4CD486CB6A1AFFEF4153FC94D559FCA0" box="[779,832,867,891]" metricMagnitude="1" metricUnit="m" metricValue="1.3" pageId="22" pageNumber="3041" unit="m" value="13.0">13 m</quantity>
