<taxonomicName id="4C5B4D79FF8AFF8FFC9EFB17FBEBFA84" box="[849,1064,1258,1284]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="46" pageNumber="127" phylum="Tracheophyta" rank="species" species="tremuloides">
