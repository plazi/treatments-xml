<taxonomicName id="4C5A2C9DFF92FD7FFE45C7A0244729BE" authority="(Dallas, 1851)" authorityName="Dallas" authorityYear="1851" baseAuthorityName="Dallas" baseAuthorityYear="1851" box="[396,776,392,417]" class="Insecta" family="Pentatomidae" genus="Dichelops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="32" phylum="Arthropoda" rank="species" species="melacanthus" subGenus="Diceraeus">Dichelops (Diceraeus) melacanthus</taxonomicName>
