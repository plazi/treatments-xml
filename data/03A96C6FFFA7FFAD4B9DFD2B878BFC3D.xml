<taxonomicName id="4C00A6FAFFA7FFAD4AEEFC53875CFC19" authorityName="Yang &amp; Li" authorityYear="1984" box="[484,623,943,968]" class="Insecta" family="Psyllidae" genus="Trisetipsylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis id="B974016BFFA7FFAD4AEEFC53875CFC19" box="[484,623,943,968]" italics="true" pageId="17" pageNumber="18">Trisetipsylla</emphasis>
</taxonomicName>
