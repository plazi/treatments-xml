<keyLead id="A82095A77916FFABFF03F978FA75F8B0" pageId="7" pageNumber="67">
<number id="A51924B57916FFABFF03F978FF4EF8F0" box="[151,167,1788,1811]" pageId="7" pageNumber="67" value="7.0">7</number>
