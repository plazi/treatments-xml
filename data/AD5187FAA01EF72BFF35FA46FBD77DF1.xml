<subSubSection id="6DE26567A01DF72BFF65FCE3FBBE7D2A" box="[199,1066,799,824]" pageId="22" pageNumber="23" type="etymology">
<paragraph id="254736ECA01DF72BFF65FCE3FBBE7D2A" blockId="22.[151,1437,151,1005]" box="[199,1066,799,824]" pageId="22" pageNumber="23">
