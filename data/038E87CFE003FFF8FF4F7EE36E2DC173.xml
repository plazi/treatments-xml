<emphasis id="B953EACBE003FFFFFF4F7EE36D4DC019" bold="true" box="[151,574,1769,1795]" pageId="3" pageNumber="201">
<taxonomicName id="4C274D5AE003FFFFFF4F7EE36EFDC019" authorityName="Borneo" baseAuthorityName="Gunther" baseAuthorityYear="1935" box="[151,398,1769,1795]" class="Insecta" family="Tetrigidae" genus="Tegotettix" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="201" phylum="Arthropoda" rank="species" species="cristiferus">
