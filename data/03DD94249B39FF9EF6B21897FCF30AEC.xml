<subSubSection id="C36E76B99B39FF9DF6E01B95FAE20EF9" box="[197,1314,1679,1706]" pageId="1" pageNumber="44" type="diagnosis">
<paragraph id="8BCB25329B39FF9DF6E01B95FAE20EF9" blockId="1.[151,1436,1599,2026]" box="[197,1314,1679,1706]" pageId="1" pageNumber="44">
<emphasis id="B900F9209B39FF9DF6E01B95FEFD0EFA" bold="true" box="[197,317,1679,1705]" pageId="1" pageNumber="44">Diagnosis</emphasis>
