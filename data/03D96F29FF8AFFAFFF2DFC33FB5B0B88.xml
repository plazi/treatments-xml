mentioned 
<specimenCount id="9D7615B6FF8AFFAFFA7EFEC0FC7C0FAD" pageId="47" pageNumber="492" type="generic">3 specimens</specimenCount>
of 
<taxonomicName id="4C70A5BCFF8AFFAFFC32FE26FB0A0FB2" authorityName="DE SAINT LAURENT" authorityYear="1984" box="[984,1241,410,431]" class="Malacostraca" family="Bythograeidae" genus="Cyanagraea" kingdom="Animalia" order="Decapoda" pageId="47" pageNumber="492" phylum="Arthropoda" rank="species" species="praedator">
<emphasis id="B904022DFF8AFFAFFC32FE26FB0A0FB2" box="[984,1241,410,431]" italics="true" pageId="47" pageNumber="492">Cyanagraea praedator</emphasis>
</taxonomicName>
