<paragraph id="8BCFDE3FFF8AFFAFFCB9FEC7FB0F0C11" blockId="47.[826,1442,195,524]" pageId="47" pageNumber="492">
<materialsCitation id="3B18D462FF8AFFAFFCB9FEC7FB0B0C11" collectionCode="SIO, EPR" pageId="47" pageNumber="492" specimenCount="3">
