<taxonomicName id="21AD4D4DFF975722FCC52B75FAEEFEB5" authority="Chemsak &amp; Linsley, 1984" authorityName="Chemsak &amp; Linsley" authorityYear="1984" box="[783,1305,336,358]" class="Insecta" family="Cerambycidae" genus="Noctileptura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="190" phylum="Arthropoda" rank="species" species="squamosa">
