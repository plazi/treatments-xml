<collectingCountry id="F32D7667FFD2CE28B074FA7CC258FA11" box="[551,619,1439,1462]" name="China" pageId="15" pageNumber="288">China</collectingCountry>
: 
<collectingRegion id="49FEF815FFD2CE28B021FA7CC280FA11" box="[626,691,1439,1462]" country="China" name="Hubei" pageId="15" pageNumber="288">Hubei</collectingRegion>
