<taxonomicName id="4C204D1FFF982164D9DFF9D3FDE6643A" authority="Jung et al. 2017" authorityName="Jung" authorityYear="2017" box="[232,585,1605,1629]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="21" pageNumber="2803" phylum="Firmicutes" rank="species" species="allii">
<emphasis id="B954EA8EFF982164D9DFF9D3FE30643A" box="[232,415,1605,1629]" italics="true" pageId="21" pageNumber="2803">Lactobacillus allii</emphasis>
Jung 
<emphasis id="B954EA8EFF982164D8EBF9D3FDA8643A" box="[476,519,1605,1629]" italics="true" pageId="21" pageNumber="2803">et al</emphasis>
