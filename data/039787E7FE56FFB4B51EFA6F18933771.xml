: Nova Friburgo, Rio de Janeiro, 
<collectingCountry id="F3297661FE5BFFB2B760F947188030EC" box="[886,962,1705,1731]" name="Brazil" pageId="10" pageNumber="47">Brazil</collectingCountry>
. The 
<typeStatus id="54858853FE5BFFB2B002F9471F3830EC" box="[1044,1146,1705,1731]" pageId="10" pageNumber="47">holotype</typeStatus>
and 
<typeStatus id="54858853FE5BFFB2B0ADF9471E6930EC" box="[1211,1323,1705,1731]" pageId="10" pageNumber="47">paratypes</typeStatus>
were collected at Três Picos (
<geoCoordinate id="EE0A5036FE5BFFB2B66AF93D186730C2" box="[636,805,1744,1773]" direction="south" orientation="latitude" pageId="10" pageNumber="47" precision="1" value="-22.336117">
<number id="3DF624D6FE5BFFB2B66AF93D19D930C2" box="[636,667,1747,1773]" pageId="10" pageNumber="47" value="22.0">22</number>
o 
<number id="3DF624D6FE5BFFB2B6B5F93D198130C2" box="[675,707,1747,1773]" pageId="10" pageNumber="47" value="20.0">20</number>
' 
<number id="3DF624D6FE5BFFB2B6D0F93D184C30C2" box="[710,782,1747,1773]" pageId="10" pageNumber="47" value="10.02">10.02</number>
&quot;S
</geoCoordinate>
, 
<geoCoordinate id="EE0A5036FE5BFFB2B72EF93D18AC30C2" box="[824,1006,1744,1773]" direction="west" orientation="longitude" pageId="10" pageNumber="47" precision="1" value="-42.71231">
<number id="3DF624D6FE5BFFB2B72EF93D181730C2" box="[824,853,1747,1773]" pageId="10" pageNumber="47" value="42.0">42</number>
o 
<number id="3DF624D6FE5BFFB2B74BF93D183F30C2" box="[861,893,1747,1773]" pageId="10" pageNumber="47" value="42.0">42</number>
' 
<number id="3DF624D6FE5BFFB2B796F93D188430C2" box="[896,966,1747,1773]" pageId="10" pageNumber="47" value="44.31">44.31</number>
&quot;W
</geoCoordinate>
