and 
<quantity id="4CC69B14FE5BFFB2B6B0F8A519A3314A" box="[678,737,1867,1893]" metricMagnitude="3" metricUnit="m" metricValue="1.7" pageId="10" pageNumber="47" unit="m" value="1700.0">1700</quantity>
</date>
m.
</paragraph>
