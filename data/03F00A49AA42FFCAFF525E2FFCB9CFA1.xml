<subSubSection id="C343E8D4AA42FFCAFF525E9CFF2DCD83" lastPageId="4" lastPageNumber="305" pageId="3" pageNumber="304" type="description">
<paragraph id="8BE6BB5FAA42FFCDFF525E9CFDD6CBE0" blockId="3.[151,1437,1436,2037]" pageId="3" pageNumber="304">
<emphasis id="B92D674DAA42FFCDFF525E9CFEEFC9F9" bold="true" box="[151,297,1436,1461]" pageId="3" pageNumber="304">Description.</emphasis>
FEMALE (
<typeStatus id="54E205FDAA42FFCDFE775E9CFDDFC9F9" box="[434,537,1436,1461]" pageId="3" pageNumber="304">holotype</typeStatus>
, 
<figureCitation id="1362A7DAAA42FFCDFDE35E9CFDB3C9F8" box="[550,629,1436,1461]" captionStart="FIGURE 1" captionStartId="4.[151,250,1975,1997]" captionTargetBox="[157,1430,1062,1943]" captionTargetId="figure@4.[151,1436,1060,1951]" captionTargetPageId="4" captionText="FIGURE 1. Trichorhynchiella myiarchus sp. nov., female. A—dorsal view; B—ventral view." httpUri="https://zenodo.org/record/230004/files/figure.png" pageId="3" pageNumber="304">
Figs. 
<number id="3D91A978AA42FFCDFDA25E9CFDB3C9F8" box="[615,629,1436,1460]" pageId="3" pageNumber="304" value="1.0">1</number>
</figureCitation>
and 
<figureCitation id="1362A7DAAA42FFCDFD6A5E9CFD06C9F8" box="[687,704,1436,1460]" captionStart="FIGURE 2" captionStartId="5.[151,250,1953,1975]" captionTargetBox="[172,1404,202,1922]" captionTargetId="figure@5.[172,1414,193,1932]" captionTargetPageId="5" captionText="FIGURE 2. Trichorhynchiella myiarchus sp. nov., details of female. A—palp in dorsal view; B leg I in ventral view; C—leg II in ventral view; D—apical segment of leg III in ventral view; E—leg IV in ventral view." httpUri="https://zenodo.org/record/230005/files/figure.png" pageId="3" pageNumber="304">
<number id="3D91A978AA42FFCDFD6A5E9CFD06C9F8" box="[687,704,1436,1460]" pageId="3" pageNumber="304" value="2.0">2</number>
</figureCitation>
). Body 
<number id="3D91A978AA42FFCDFCD85E9CFC81C9F9" box="[797,839,1436,1461]" pageId="3" pageNumber="304" value="680.0">680</number>
long (
<quantity id="4CA116BAAA42FFCDFC565E9CFC18C9F9" box="[915,990,1436,1461]" metricMagnitude="1" metricUnit="m" metricValue="1.6509999999999998" pageId="3" pageNumber="304" unit="in" value="650.0">650 in</quantity>
<number id="3D91A978AA42FFCDFC225E9CFC33C9F8" box="[999,1013,1436,1460]" pageId="3" pageNumber="304" value="2.0">2</number>
<typeStatus id="54E205FDAA42FFCDFC3B5E9CFBADC9F8" box="[1022,1131,1436,1460]" pageId="3" pageNumber="304">paratypes</typeStatus>
) and 
<number id="3D91A978AA42FFCDFB6B5E9CFB1EC9F9" box="[1198,1240,1436,1461]" pageId="3" pageNumber="304" value="610.0">610</number>
wide (
<number id="3D91A978AA42FFCDFAED5E9CFA57C9F9" box="[1320,1425,1436,1461]" pageId="3" pageNumber="304" value="592.5" valueMax="600.0" valueMin="585.0">585–600</number>
). Gnathosoma 
<number id="3D91A978AA42FFCDFEE95EBFFE90C994" box="[300,342,1471,1496]" pageId="3" pageNumber="304" value="125.0">125</number>
long (
<number id="3D91A978AA42FFCDFE5A5EBFFDC2C994" box="[415,516,1471,1496]" pageId="3" pageNumber="304" value="122.5" valueMax="125.0" valueMin="120.0">120–125</number>
) and 
<number id="3D91A978AA42FFCDFD865EC0FDABC994" box="[579,621,1472,1496]" pageId="3" pageNumber="304" value="170.0">170</number>
wide (
<number id="3D91A978AA42FFCDFD7E5EBFFCE4C994" box="[699,802,1471,1496]" pageId="3" pageNumber="304" value="167.5" valueMax="170.0" valueMin="165.0">165–170</number>
). Subcapitular setae 
<emphasis id="B92D674DAA42FFCDFBCE5EC1FBD9C994" box="[1035,1055,1473,1496]" italics="true" pageId="3" pageNumber="304">m</emphasis>
absent. Palp 
<number id="3D91A978AA42FFCDFB735EBFFB14C994" box="[1206,1234,1471,1496]" pageId="3" pageNumber="304" value="75.0">75</number>
long (
<number id="3D91A978AA42FFCDFADE5EBFFAA2C994" box="[1307,1380,1471,1496]" pageId="3" pageNumber="304" value="72.5" valueMax="75.0" valueMin="70.0">70–75</number>
) and 
<number id="3D91A978AA42FFCDFF525EE4FF75C9B1" box="[151,179,1508,1533]" pageId="3" pageNumber="304" value="50.0">50</number>
wide (
<number id="3D91A978AA42FFCDFF3A5EE4FE8CC9B1" box="[255,330,1508,1533]" pageId="3" pageNumber="304" value="55.0" valueMax="60.0" valueMin="50.0">50–60</number>
). Palpalae 
<emphasis id="B92D674DAA42FFCDFE075EE4FE27C9B1" box="[450,481,1508,1533]" italics="true" pageId="3" pageNumber="304">dF</emphasis>
<number id="3D91A978AA42FFCDFE2D5EE4FDC2C9B1" box="[488,516,1508,1533]" pageId="3" pageNumber="304" value="35.0">35</number>
long (
<number id="3D91A978AA42FFCDFD8E5EE4FD52C9B1" box="[587,660,1508,1533]" pageId="3" pageNumber="304" value="37.5" valueMax="40.0" valueMin="35.0">35–40</number>
) and 
<emphasis id="B92D674DAA42FFCDFD145EE4FD35C9B1" box="[721,755,1508,1533]" italics="true" pageId="3" pageNumber="304">dG</emphasis>
<number id="3D91A978AA42FFCDFD3F5EE4FCD0C9B0" box="[762,790,1508,1532]" pageId="3" pageNumber="304" value="30.0">30</number>
long (
<number id="3D91A978AA42FFCDFC985EE4FC6EC9B1" box="[861,936,1508,1533]" pageId="3" pageNumber="304" value="32.5" valueMax="35.0" valueMin="30.0">30–35</number>
), thickened and distinctly pectinate, palpalae 
<emphasis id="B92D674DAA42FFCDFF525D07FF04CA6C" box="[151,194,1543,1568]" italics="true" pageId="3" pageNumber="304">l”G</emphasis>
long 
<number id="3D91A978AA42FFCDFEC25D08FEE5CA6C" box="[263,291,1544,1568]" pageId="3" pageNumber="304" value="20.0">20</number>
(
<number id="3D91A978AA42FFCDFEF15D08FEBBCA6C" box="[308,381,1544,1568]" pageId="3" pageNumber="304" value="21.5" valueMax="23.0" valueMin="20.0">20–23</number>
) and smooth. Setae 
<emphasis id="B92D674DAA42FFCDFDAD5D09FD40CA6C" box="[616,646,1545,1568]" italics="true" pageId="3" pageNumber="304">vF</emphasis>
smooth, 
<number id="3D91A978AA42FFCDFD375D08FCC8CA6C" box="[754,782,1544,1568]" pageId="3" pageNumber="304" value="40.0">40</number>
long (
<number id="3D91A978AA42FFCDFC9F5D08FC60CA6C" box="[858,934,1544,1568]" pageId="3" pageNumber="304" value="39.0" valueMax="40.0" valueMin="38.0">38–40</number>
). Peritrematal branch about 
<number id="3D91A978AA42FFCDFB2F5D07FAC0CA6C" box="[1258,1286,1543,1568]" pageId="3" pageNumber="304" value="50.0">50</number>
long. Dorsal shield present, 
<number id="3D91A978AA42FFCDFE815D2CFEA8CA08" box="[324,366,1580,1604]" pageId="3" pageNumber="304" value="300.0">300</number>
long (
<number id="3D91A978AA42FFCDFE7F5D2CFDE6CA08" box="[442,544,1580,1604]" pageId="3" pageNumber="304" value="290.0" valueMax="300.0" valueMin="280.0">280–300</number>
) and 
<number id="3D91A978AA42FFCDFDA75D2CFD4ACA09" box="[610,652,1580,1605]" pageId="3" pageNumber="304" value="175.0">175</number>
wide (
<number id="3D91A978AA42FFCDFD1B5D2CFC83CA09" box="[734,837,1580,1605]" pageId="3" pageNumber="304" value="170.0" valueMax="175.0" valueMin="165.0">165–175</number>
). Dorsal idiosomal surface with 
<number id="3D91A978AA42FFCDFB7E5D2CFB0FCA08" box="[1211,1225,1580,1604]" pageId="3" pageNumber="304" value="2.0">2</number>
large lateral areas covered by oval granules; area immediately behind gnathosoma and posterior to dorsal shield covered by striations without granules. Setae 
<emphasis id="B92D674DAA42FFCDFE6A5D75FDFACAC0" box="[431,572,1653,1676]" italics="true" pageId="3" pageNumber="304">vi, ve, si, se</emphasis>
, and 
<emphasis id="B92D674DAA42FFCDFD445D74FD5ACAC0" box="[641,668,1652,1676]" italics="true" pageId="3" pageNumber="304">
c 
<number id="3D91A978AA42FFCDFD4B5D74FD5ACAC0" box="[654,668,1652,1676]" pageId="3" pageNumber="304" value="2.0">2</number>
</emphasis>
all serrate, 
<number id="3D91A978AA42FFCDFCEF5D74FCBBCAC0" box="[810,893,1652,1676]" pageId="3" pageNumber="304" value="90.0" valueMax="100.0" valueMin="80.0">80–100</number>
long. Setae 
<emphasis id="B92D674DAA42FFCDFBD15D74FBF6CAC1" box="[1044,1072,1652,1677]" italics="true" pageId="3" pageNumber="304">
h 
<number id="3D91A978AA42FFCDFBE75D74FBF6CAC1" box="[1058,1072,1652,1677]" pageId="3" pageNumber="304" value="2.0">2</number>
</emphasis>
<number id="3D91A978AA42FFCDFBFF5D74FB90CAC0" box="[1082,1110,1652,1676]" pageId="3" pageNumber="304" value="90.0">90</number>
long (
<number id="3D91A978AA42FFCDFB635D74FB3ACAC1" box="[1190,1276,1652,1677]" pageId="3" pageNumber="304" value="97.5" valueMax="105.0" valueMin="90.0">90–105</number>
) and smooth, located dorsally at bases of dorso-terminal lobes. Dorso- and ventro-terminal lobes of idiosoma poorly developed. Vulvar slit about 
<number id="3D91A978AA42FFCDFE9D5DBCFE44CA99" box="[344,386,1724,1749]" pageId="3" pageNumber="304" value="250.0">250</number>
long. Vulvar fold situated in median part of idiosoma almost equidistantly between insertions of legs II and III. Setae 
<emphasis id="B92D674DAA42FFCDFE655DE0FDC8CAB4" box="[416,526,1760,1784]" italics="true" pageId="3" pageNumber="304">
<number id="3D91A978AA42FFCDFE655DE0FE6BCAB4" box="[416,429,1760,1784]" pageId="3" pageNumber="304" value="1.0">1</number>
a, 
<number id="3D91A978AA42FFCDFE0F5DE0FE1ECAB4" box="[458,472,1760,1784]" pageId="3" pageNumber="304" value="1.0">1</number>
c, 
<number id="3D91A978AA42FFCDFE365DE0FDC8CAB4" box="[499,526,1760,1784]" pageId="3" pageNumber="304" value="36.5" valueMax="70.0" valueMin="3.0">3a</number>
</emphasis>
70– 
<number id="3D91A978AA42FFCDFD855DE0FD9ACAB4" box="[576,604,1760,1784]" pageId="3" pageNumber="304" value="40.0">40</number>
long, and 
<emphasis id="B92D674DAA42FFCDFD175DDFFD2BCAB4" box="[722,749,1759,1784]" italics="true" pageId="3" pageNumber="304">
h 
<number id="3D91A978AA42FFCDFD245DDFFD2BCAB4" box="[737,749,1759,1784]" pageId="3" pageNumber="304" value="1.0">1</number>
</emphasis>
<number id="3D91A978AA42FFCDFD305DDFFCFCCAB4" box="[757,826,1759,1784]" pageId="3" pageNumber="304" value="34.0" valueMax="35.0" valueMin="33.0">33–35</number>
long—all smooth. Legs I with 
<number id="3D91A978AA42FFCDFB5F5DE0FB6ECAB4" box="[1178,1192,1760,1784]" pageId="3" pageNumber="304" value="4.0">4</number>
articulated segments; legs II with 
<number id="3D91A978AA42FFCDFEE95C04FEFCCB51" box="[300,314,1796,1821]" pageId="3" pageNumber="304" value="5.0">5</number>
articulated segments. All tactile setae of legs I and II smooth, filiform, except 
<emphasis id="B92D674DAA42FFCDFB365C05FACCCB50" box="[1267,1290,1797,1820]" italics="true" pageId="3" pageNumber="304">p’</emphasis>
I and 
<emphasis id="B92D674DAA42FFCDFA915C05FAB7CB50" box="[1364,1393,1797,1820]" italics="true" pageId="3" pageNumber="304">p”</emphasis>
I–II eupathidia. Solenidia 
<emphasis id="B92D674DAA42FFCDFE495C28FE68CB0C" box="[396,430,1832,1856]" italics="true" pageId="3" pageNumber="304">
ω 
<date id="FFE79D9FAA42FFCDFE675C28FE68CB0C" box="[418,430,1832,1856]" pageId="3" pageNumber="304">
<number id="3D91A978AA42FFCDFE675C28FE68CB0C" box="[418,430,1832,1856]" pageId="3" pageNumber="304" value="1.0">1</number>
</date>
</emphasis>
I and 
<emphasis id="B92D674DAA42FFCDFE355C28FDD7CB0C" box="[496,529,1832,1856]" italics="true" pageId="3" pageNumber="304">
ω 
<date id="FFE79D9FAA42FFCDFDC05C28FDD7CB0C" box="[517,529,1832,1856]" pageId="3" pageNumber="304">
<number id="3D91A978AA42FFCDFDC05C28FDD7CB0C" box="[517,529,1832,1856]" pageId="3" pageNumber="304" value="1.0">1</number>
</date>
</emphasis>
II almost straight, subconical, 
<number id="3D91A978AA42FFCDFCAC5C28FC68CB0C" box="[873,942,1832,1856]" pageId="3" pageNumber="304" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
long. Number of setae on basal segment of legs I and femora-genua of legs II variable (
<number id="3D91A978AA42FFCDFD455C4CFD6DCB28" box="[640,683,1868,1892]" pageId="3" pageNumber="304" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
), genua I and II always bearing 
<number id="3D91A978AA42FFCDFBD55C4CFBFCCB28" box="[1040,1082,1868,1892]" pageId="3" pageNumber="304" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
setae. Basal segment of legs III with 
<number id="3D91A978AA42FFCDFF0A5C70FF3FCBC4" box="[207,249,1904,1928]" pageId="3" pageNumber="304" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
setae; apical segment about 
<number id="3D91A978AA42FFCDFDFF5C70FD90CBC4" box="[570,598,1904,1928]" pageId="3" pageNumber="304" value="40.0">40</number>
long and 
<number id="3D91A978AA42FFCDFD005C6FFD27CBC4" box="[709,737,1903,1928]" pageId="3" pageNumber="304" value="50.0">50</number>
wide, with 
<number id="3D91A978AA42FFCDFCA05C70FC48CBC4" box="[869,910,1904,1928]" pageId="3" pageNumber="304" value="8.0" valueMax="9.0" valueMin="7.0">7–9</number>
setae. Articulated segment of legs IV about 
<number id="3D91A978AA42FFCDFA455C6FFA5ACBC4" box="[1408,1436,1903,1928]" pageId="3" pageNumber="304" value="85.0">85</number>
long and 
<number id="3D91A978AA42FFCDFF3A5C94FEDDCBE1" box="[255,283,1940,1965]" pageId="3" pageNumber="304" value="65.0">65</number>
wide, with 
<number id="3D91A978AA42FFCDFE5A5C94FE0FCBE0" box="[415,457,1940,1964]" pageId="3" pageNumber="304" value="8.0" valueMax="9.0" valueMin="7.0">7–9</number>
setae.
</paragraph>
<paragraph id="8BE6BB5FAA42FFCAFF025CB9FF2DCD83" blockId="3.[151,1437,1436,2037]" lastBlockId="4.[151,1436,151,1005]" lastPageId="4" lastPageNumber="305" pageId="3" pageNumber="304">
MALE (
<number id="3D91A978AA42FFCDFEE05CB8FEF2CB9C" box="[293,308,1976,2000]" pageId="3" pageNumber="304" value="4.0">4</number>
<typeStatus id="54E205FDAA42FFCDFEFE5CB9FE6FCB9D" box="[315,425,1977,2001]" pageId="3" pageNumber="304">paratypes</typeStatus>
, 
<figureCitation id="1362A7DAAA42FFCDFE765CB8FE30CB9C" box="[435,502,1976,2001]" captionStart="FIGURE 3" captionStartId="6.[151,250,1161,1183]" captionTargetBox="[157,1434,198,1139]" captionTargetId="figure@6.[151,1436,193,1140]" captionTargetPageId="6" captionText="FIGURE 3. Trichorhynchiella myiarchus sp. nov., male. A—dorsal view; B—ventral view; C—palpalae in dorsal view. Scale bars: A, B = 100 µm; C = 50 µm." httpUri="https://zenodo.org/record/230006/files/figure.png" pageId="3" pageNumber="304">
Fig. 
<number id="3D91A978AA42FFCDFE235CB8FE30CB9C" box="[486,502,1976,2000]" pageId="3" pageNumber="304" value="3.0">3</number>
</figureCitation>
). Body 
<number id="3D91A978AA42FFCDFD8B5CB8FD76CB9D" box="[590,688,1976,2001]" pageId="3" pageNumber="304" value="397.5" valueMax="410.0" valueMin="385.0">385–410</number>
long and 
<number id="3D91A978AA42FFCDFCDB5CB8FC46CB9D" box="[798,896,1976,2001]" pageId="3" pageNumber="304" value="337.5" valueMax="350.0" valueMin="325.0">325–350</number>
wide. Gnathosoma 
<number id="3D91A978AA42FFCDFB9A5CB8FB06CB9C" box="[1119,1216,1976,2000]" pageId="3" pageNumber="304" value="115.0" valueMax="120.0" valueMin="110.0">110–120</number>
long and about 
<number id="3D91A978AA42FFCDFAB75CB8FA5ACB9D" box="[1394,1436,1976,2001]" pageId="3" pageNumber="304" value="145.0">145</number>
wide. Subapical setae 
<emphasis id="B92D674DAA42FFCDFE555CDDFE62CBB8" box="[400,420,2013,2036]" italics="true" pageId="3" pageNumber="304">m</emphasis>
and 
<emphasis id="B92D674DAA42FFCDFE1F5CDDFE2ECBB8" box="[474,488,2013,2036]" italics="true" pageId="3" pageNumber="304">n</emphasis>
present. Palpalae strongly thickened: 
<emphasis id="B92D674DAA42FFCDFC565CDCFC74CBB9" box="[915,946,2012,2037]" italics="true" pageId="3" pageNumber="304">dF</emphasis>
<number id="3D91A978AA42FFCDFC7C5CDCFC39CBB9" box="[953,1023,2012,2037]" pageId="3" pageNumber="304" value="42.5" valueMax="45.0" valueMin="40.0">40–45</number>
long, with 
<number id="3D91A978AA42FFCDFBBB5CDCFB6ECBB8" box="[1150,1192,2012,2036]" pageId="3" pageNumber="304" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
short projections; 
<emphasis id="B92D674DAA42FFCDFABF5CDCFA5ACBB9" box="[1402,1436,2012,2037]" italics="true" pageId="3" pageNumber="304">dG</emphasis>
<number id="3D91A978AA45FFCAFF525B97FF1BCCFC" box="[151,221,151,176]" pageId="4" pageNumber="305" value="37.5" valueMax="40.0" valueMin="35.0">35–40</number>
long, with large anteriorly directed projection located in median part and 
<number id="3D91A978AA45FFCAFBDE5B98FBEFCCFC" box="[1051,1065,152,176]" pageId="4" pageNumber="305" value="2.0">2</number>
small subapical projections; 
<emphasis id="B92D674DAA45FFCAFAB55B97FA5DCCFC" box="[1392,1435,151,176]" italics="true" pageId="4" pageNumber="305">l”G</emphasis>
<number id="3D91A978AA45FFCAFF525BBCFF1BCC99" box="[151,221,188,213]" pageId="4" pageNumber="305" value="27.5" valueMax="30.0" valueMin="25.0">25–30</number>
long with 
<number id="3D91A978AA45FFCAFEA15BBCFE48CC98" box="[356,398,188,212]" pageId="4" pageNumber="305" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
short projections. Setae 
<emphasis id="B92D674DAA45FFCAFD735BBDFD15CC98" box="[694,723,189,212]" italics="true" pageId="4" pageNumber="305">vF</emphasis>
<number id="3D91A978AA45FFCAFD1A5BBCFCE0CC99" box="[735,806,188,213]" pageId="4" pageNumber="305" value="60.0" valueMax="70.0" valueMin="50.0">50–70</number>
long, smooth. Idiosoma 
<number id="3D91A978AA45FFCAFB955BBCFB77CC99" box="[1104,1201,188,213]" pageId="4" pageNumber="305" value="277.5" valueMax="290.0" valueMin="265.0">265–290</number>
long. Dorsal shield trapezium-like, 
<number id="3D91A978AA45FFCAFE895BDFFE68CCB4" box="[332,430,223,248]" pageId="4" pageNumber="305" value="172.5" valueMax="175.0" valueMin="170.0">170–175</number>
long and 
<number id="3D91A978AA45FFCAFDE25BDFFD4FCCB4" box="[551,649,223,248]" pageId="4" pageNumber="305" value="242.5" valueMax="250.0" valueMin="235.0">235–250</number>
wide in basal part; its posterior margin slightly concave. Aedeagus 
<number id="3D91A978AA45FFCAFF525A04FF3ECD51" box="[151,248,260,285]" pageId="4" pageNumber="305" value="192.5" valueMax="200.0" valueMin="185.0">185–200</number>
long. Genital opening situated at anterior margin of dorsal shield. Three pairs of genital setae present, distance between setae 
<emphasis id="B92D674DAA45FFCAFE675A28FE7BCD0C" box="[418,445,296,320]" italics="true" pageId="4" pageNumber="305">
g 
<number id="3D91A978AA45FFCAFE745A28FE7BCD0C" box="[433,445,296,320]" pageId="4" pageNumber="305" value="1.0">1</number>
</emphasis>
and 
<emphasis id="B92D674DAA45FFCAFE3D5A28FDD2CD0C" box="[504,532,296,320]" italics="true" pageId="4" pageNumber="305">
g 
<number id="3D91A978AA45FFCAFDC35A28FDD2CD0C" box="[518,532,296,320]" pageId="4" pageNumber="305" value="2.0">2</number>
</emphasis>
<number id="3D91A978AA45FFCAFDD85A28FDA6CD0C" box="[541,608,296,320]" pageId="4" pageNumber="305" value="3.5" valueMax="4.0" valueMin="3.0">3 to 4</number>
times greater than between 
<emphasis id="B92D674DAA45FFCAFC615A28FC79CD0C" box="[932,959,296,320]" italics="true" pageId="4" pageNumber="305">
g 
<number id="3D91A978AA45FFCAFC775A28FC79CD0C" box="[946,959,296,320]" pageId="4" pageNumber="305" value="2.0">2</number>
</emphasis>
and 
<emphasis id="B92D674DAA45FFCAFC3F5A28FBD0CD0C" box="[1018,1046,296,320]" italics="true" pageId="4" pageNumber="305">
g 
<number id="3D91A978AA45FFCAFBCD5A28FBD0CD0C" box="[1032,1046,296,320]" pageId="4" pageNumber="305" value="3.0">3</number>
</emphasis>
. Setae 
<emphasis id="B92D674DAA45FFCAFBAE5A29FB37CD0C" box="[1131,1265,297,320]" italics="true" pageId="4" pageNumber="305">vi, ve, si, se</emphasis>
, and 
<emphasis id="B92D674DAA45FFCAFAF65A28FA8BCD0C" box="[1331,1357,296,320]" italics="true" pageId="4" pageNumber="305">
c 
<number id="3D91A978AA45FFCAFAFA5A28FA8BCD0C" box="[1343,1357,296,320]" pageId="4" pageNumber="305" value="2.0">2</number>
</emphasis>
<number id="3D91A978AA45FFCAFA935A27FA5ACD0C" box="[1366,1436,295,320]" pageId="4" pageNumber="305" value="82.5" valueMax="90.0" valueMin="75.0">75–90</number>
long, serrate, situated off dorsal shield. Dorsal striated cuticle of idiosoma without granules or tubercles. Setation legs I–II (solenidia in parentheses): tarsi 
<number id="3D91A978AA45FFCAFDA05A70FDB5CDC4" box="[613,627,368,392]" pageId="4" pageNumber="305" value="8.0">8</number>
(
<number id="3D91A978AA45FFCAFD415A70FD49CDC4" box="[644,655,368,392]" pageId="4" pageNumber="305" value="1.0">1</number>
)– 
<number id="3D91A978AA45FFCAFD6E5A70FD7DCDC4" box="[683,699,368,392]" pageId="4" pageNumber="305" value="7.0">7</number>
(
<number id="3D91A978AA45FFCAFD005A70FD16CDC4" box="[709,720,368,392]" pageId="4" pageNumber="305" value="1.0">1</number>
), tibiae 
<number id="3D91A978AA45FFCAFCE95A6FFCFCCDC4" box="[812,826,367,392]" pageId="4" pageNumber="305" value="5.0">5</number>
– 
<number id="3D91A978AA45FFCAFC8E5A6FFC9FCDC4" box="[843,857,367,392]" pageId="4" pageNumber="305" value="5.0">5</number>
, genua 
<number id="3D91A978AA45FFCAFC755A70FC7BCDC4" box="[944,957,368,392]" pageId="4" pageNumber="305" value="3.0">3</number>
– 
<number id="3D91A978AA45FFCAFC0B5A70FC1DCDC4" box="[974,987,368,392]" pageId="4" pageNumber="305" value="3.0">3</number>
, femorae 
<number id="3D91A978AA45FFCAFB895A70FB9FCDC4" box="[1100,1113,368,392]" pageId="4" pageNumber="305" value="3.0">3</number>
– 
<number id="3D91A978AA45FFCAFBAF5A70FBBFCDC4" box="[1130,1145,368,392]" pageId="4" pageNumber="305" value="2.0">2</number>
, trochanters 
<number id="3D91A978AA45FFCAFACC5A70FAD3CDC4" box="[1289,1301,368,392]" pageId="4" pageNumber="305" value="1.0">1</number>
– 
<number id="3D91A978AA45FFCAFAEF5A70FAF0CDC4" box="[1322,1334,368,392]" pageId="4" pageNumber="305" value="1.0">1</number>
. Legs III with 
<number id="3D91A978AA45FFCAFF155A94FF18CDE0" box="[208,222,404,428]" pageId="4" pageNumber="305" value="2.0">2</number>
articulated segments bearing 
<number id="3D91A978AA45FFCAFDEB5A94FD9ECDE0" box="[558,600,404,428]" pageId="4" pageNumber="305" value="0.5" valueMax="1.0" valueMin="0.0">0–1</number>
and 
<number id="3D91A978AA45FFCAFD4A5A94FD7FCDE1" box="[655,697,404,429]" pageId="4" pageNumber="305" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
setae, respectively. Legs IV with one articulated segment bearing 
<number id="3D91A978AA45FFCAFF525AB8FF63CD9C" box="[151,165,440,464]" pageId="4" pageNumber="305" value="2.0">2</number>
setae.
</paragraph>
</subSubSection>
