<quantity id="4CF3D7E5FFE9A256FC34FCFAFC5AFC98" box="[917,991,836,860]" metricMagnitude="2" metricUnit="m" metricValue="2.38" pageId="28" pageNumber="29" unit="m" value="238.0">238 m</quantity>
