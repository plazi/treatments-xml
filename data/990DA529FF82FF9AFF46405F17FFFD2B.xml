<taxonomicName id="D6A46FBCFF82FF9BFEA847E2140DFA34" box="[377,596,1469,1490]" class="Insecta" family="Dynastidae" genus="Tomarus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="479" phylum="Arthropoda" rank="species" species="cicatricosus">
