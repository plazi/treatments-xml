<collectingCountry id="E497CB1552534F451A5988289AAF633A" name="Réunion">REUNION</collectingCountry>
</normalizedToken>
. 
<normalizedToken id="4298E779B04229987FD163F06E9EB5DE" originalValue="Saint-Benoît">
<collectingCounty id="C7B4D12CB9D5CA2E92E69A2B340072D8">Saint-Benoit</collectingCounty>
