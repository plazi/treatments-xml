<taxonomicName id="4C6F4D0AD406C047B79D7ACBBB17FE7F" box="[642,812,418,443]" class="Insecta" family="Reduviidae" genus="Panstrongylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="mitarakaensis">
