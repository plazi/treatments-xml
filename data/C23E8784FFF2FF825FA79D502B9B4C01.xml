<taxonomicName id="8D974D11FFF2FF825A7D9C312BFC4C2A" authority="Jensen, 1952" authorityName="Jensen" authorityYear="1952" class="Insecta" family="Liviidae" genus="Katacephala" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="34" phylum="Arthropoda" rank="species" species="fasciata">
