<location id="8E8C6016FF90463D2EC0AD00D7032740" LSID="urn:lsid:plazi:treatment:03FA87DBFF91463A2D8DABE2D2C62737:8E8C6016FF90463D2EC0AD00D7032740" box="[964,1024,1369,1390]" country="Turkey" county="Epigyne" municipality="Palpal" name="Palpal" pageId="19" pageNumber="68">Palpal</location>
