<location id="8E8C6016FF90463D2E99AD19D0CD277B" LSID="urn:lsid:plazi:treatment:03FA87DBFF91463A2D8DABE2D2C62737:8E8C6016FF90463D2E99AD19D0CD277B" box="[925,974,1344,1365]" country="Turkey" county="Epigyne" municipality="Palpal" name="Male" pageId="19" pageNumber="68">Male</location>
chelicera, frontal view. (54, 55) 
<location id="8E8C6016FF90463D2C69AD00D2A12740" LSID="urn:lsid:plazi:treatment:03FA87DBFF91463A2D8DABE2D2C62737:8E8C6016FF90463D2C69AD00D2A12740" box="[365,418,1369,1390]" country="Turkey" county="Epigyne" municipality="Palpal" name="Right" pageId="19" pageNumber="68">Right</location>
