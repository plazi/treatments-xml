<taxonomicName id="4C3957B91F36FF839D3C3B4FFC1FFB89" authority="Linnaeus 1753: 183" authorityName="Linnaeus" authorityPageNumber="183" authorityYear="1753" box="[556,972,1038,1065]" class="Magnoliopsida" family="Rhamnaceae" genus="Rhamnus" kingdom="Plantae" order="Rosales" pageId="23" pageNumber="150" phylum="Tracheophyta" rank="species" species="alpina">
<emphasis id="B94DF0281F36FF839D3C3B4FFD30FB89" box="[556,739,1038,1064]" italics="true" pageId="23" pageNumber="150">Rhamnus alpina</emphasis>
<bibRefCitation id="EFA851CB1F36FF839DF93B4FFC1FFB89" author="Linnaeus, C." box="[745,972,1038,1065]" pageId="23" pageNumber="150" refId="ref28503" refString="Linnaeus, C. (1753) Species plantarum 1. Salvius, Holmiae [Stockholm], 560 pp." type="book" year="1753">Linnaeus 1753: 183</bibRefCitation>
