<paragraph id="3CFBF7B037DF53C5F54ABD00FD751506" pageId="1" pageNumber="130">
<taxonomicName id="F7D85234F97C0C3B8282013AD3AD9EE0" authority="Mahunka, 2001" authorityName="Mahunka" authorityYear="2001" class="Arachnida" family="Mycobatidae" genus="Schweizerzetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Oribatida" pageId="1" pageNumber="130" phylum="Arthropoda" rank="genus">Schweizerzetes</taxonomicName>
