’S
</geoCoordinate>
, 
<geoCoordinate id="CFEC505ADD70FF884840FE61FCEEFE11" box="[770,891,425,451]" direction="east" orientation="longitude" pageId="30" pageNumber="55" precision="925" value="106.78333">
<number id="1C1024BADD70FF884840FE61FCB8FE11" box="[770,813,425,451]" pageId="30" pageNumber="55" value="106.0">106</number>
º 
<number id="1C1024BADD70FF88487DFE61FCF4FE11" box="[831,865,425,451]" pageId="30" pageNumber="55" value="47.0">47</number>
’E
