<taxonomicName id="4C414D48CC5CFFCAFE98FA9834B0FA97" authority="Alvarado, 2020" authorityName="Alvarado" authorityYear="2020" box="[281,431,1355,1380]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="190" phylum="Arthropoda" rank="species" species="pisqa" status="sp. nov.">
