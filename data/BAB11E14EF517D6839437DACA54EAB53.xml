<taxonomicName id="8F912A58C138D14A612C069CB9800803" authority=", Oliv" authorityName=", Oliv" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="162" phylum="Arthropoda" rank="species" species="vagans">
