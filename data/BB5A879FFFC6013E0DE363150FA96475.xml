<paragraph id="334C3689FFC6013F0DE3609A0CF760F7" blockId="34.[151,1346,1459,1757]" box="[151,744,1735,1757]" pageId="34" pageNumber="35">
<taxonomicName id="F4F34D0AFFC6013F0DE3609A0F5F60F7" box="[151,320,1735,1757]" class="Gastropoda" family="Helicidae" genus="Erctella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="34" pageNumber="35" phylum="Mollusca" rank="species" species="insolida">
<emphasis id="0187EA9BFFC6013F0DE3609A0F5F60F7" box="[151,320,1735,1757]" italics="true" pageId="34" pageNumber="35">Erctella insolida</emphasis>
</taxonomicName>
— Liberto 
<emphasis id="0187EA9BFFC6013F0CC260950FF560F7" box="[438,490,1735,1757]" italics="true" pageId="34" pageNumber="35">et al.</emphasis>
<number id="853B24AEFFC6013F0C9B60950C3A60F7" box="[495,549,1736,1757]" pageId="34" pageNumber="35" value="2010.0">2010</number>
