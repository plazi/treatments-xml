<collectingCountry id="F33DEA8BFFCEFFFCFEE7FDB99491FD99" box="[366,494,552,578]" name="Costa Rica" pageId="9" pageNumber="66">Costa Rica</collectingCountry>
and 
<collectingCountry id="F33DEA8BFFCEFFFCFDAEFDB997FBFD99" box="[551,644,552,578]" name="Panama" pageId="9" pageNumber="66">Panama</collectingCountry>
(
<figureCitation id="1311B69EFFCEFFFCFD1AFDB99794FD99" box="[659,747,552,578]" captionStart="FIGURE 23" captionStartId="8.[151,255,1364,1388]" captionTargetBox="[384,1202,514,1340]" captionTargetId="figure@8.[384,1203,514,1340]" captionTargetPageId="8" captionText="FIGURE 23. Eucalandra, distribution; solid circle — E. luteosignata, open circle — E. boxi, asterisk — E. mexicana, plus — E. alas." httpUri="https://zenodo.org/record/182538/files/figure.png" pageId="9" pageNumber="66">
Fig. 
<number id="3DE2B83CFFCEFFFCFD43FDB99794FD99" box="[714,747,552,578]" pageId="9" pageNumber="66" value="23.0">23</number>
</figureCitation>
