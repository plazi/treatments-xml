<emphasis id="B940EA98BC336E0B6EAA743FBCA92827" bold="true" box="[151,944,1329,1355]" pageId="3" pageNumber="380">
<taxonomicName id="4C344D09BC336E0B6EAA743FBEFD2827" authority="Sormus" authorityName="Sormus" box="[151,484,1329,1355]" class="Insecta" family="Braconidae" genus="Hecabolus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="380" phylum="Arthropoda" rank="species" species="julianoi">
<emphasis id="B940EA98BC336E0B6EAA743FBE642827" bold="true" box="[151,381,1329,1355]" italics="true" pageId="3" pageNumber="380">Hecabolus julianoi</emphasis>
