in 
<collectionCode id="ED7CAE345D400C7BF3A440C5FD10FA67" box="[641,701,1444,1470]" country="Canada" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="35" pageNumber="36">CNC</collectionCode>
