<taxonomicName id="E7FF4D55FFD69B3CFBE6FE29019941EF" authorityName="Kraepelin" authorityYear="1906" box="[1101,1225,403,428]" class="Phylactolaemata" family="Plumatellidae" genus="Plumatella" kingdom="Animalia" order="Plumatellida" pageId="16" pageNumber="597" phylum="Bryozoa" rank="species" species="javanica">
