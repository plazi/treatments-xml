<geoCoordinate id="BCEA2973538438D00306EF4925F4D9AA" degrees="0.5748" direction="north" orientation="latitude" precision="5" value="0.5748">0.5748°N</geoCoordinate>
, 
<geoCoordinate id="F5C4D0C134C6D9240F9CEF9770AB6C3F" degrees="66.6869" direction="west" orientation="longitude" precision="5" value="-66.6869">66.6869°W</geoCoordinate>
