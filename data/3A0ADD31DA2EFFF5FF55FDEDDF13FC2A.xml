<taxonomicName id="75A317A4DA2EFFF5FD8AFC58D8C8FC76" authorityName=": Froeschner" authorityYear="1960" box="[584,685,934,959]" class="Insecta" family="Cydnidae" genus="Pangaeus" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="420" phylum="Arthropoda" rank="species" species="docilis">
