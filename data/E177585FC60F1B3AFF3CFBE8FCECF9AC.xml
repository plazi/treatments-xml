<collectingRegion id="AB1A27ABC60F1B3AFEA1FA1CFE7CFA8C" box="[266,505,1338,1361]" country="Iran" name="Hormozgan" pageId="9" pageNumber="10">Hormozgan Province</collectingRegion>
