<emphasis id="5BAA355BC60F1B3AFC15FA1FFB02FA8C" box="[958,1159,1337,1361]" italics="true" pageId="9" pageNumber="10">
<collectingRegion id="AB1A27ABC60F1B3AFC15FA1FFB02FA8C" box="[958,1159,1337,1361]" country="Iran" name="Bushehr" pageId="9" pageNumber="10">Bushehr Province</collectingRegion>
