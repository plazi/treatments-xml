<caption id="DF7CEB8DFF9CB66CFF0DF9D2FAF21C89" ID-DOI="http://doi.org/10.5281/zenodo.5120688" ID-Zenodo-Dep="5120688" box="[151,1285,1661,1686]" httpUri="https://zenodo.org/record/5120688/files/figure.png" pageId="10" pageNumber="411" startId="10.[151,250,1661,1685]" targetBox="[151,1435,525,1637]" targetPageId="10">
<paragraph id="8BBCBB05FF9CB66CFF0DF9D2FAF21C89" blockId="10.[151,1285,1661,1686]" box="[151,1285,1661,1686]" pageId="10" pageNumber="411">
