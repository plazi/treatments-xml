<quantity id="4CFB16E0FF9FB66FFD88F807FD981DDF" box="[530,623,1960,1987]" metricMagnitude="-1" metricUnit="m" metricValue="1.27" pageId="9" pageNumber="410" unit="cm" value="12.7">12.7 cm</quantity>
