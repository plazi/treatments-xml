<taxonomicName id="4C03C086FF9CB66CFE83F9D1FDAE1C8A" authority="Vervoort, 1993" authorityName="Vervoort" authorityYear="1993" box="[281,601,1662,1686]" class="Hydrozoa" family="Sertulariidae" genus="Gonaxia" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="411" phylum="Cnidaria" rank="species" species="errans">
