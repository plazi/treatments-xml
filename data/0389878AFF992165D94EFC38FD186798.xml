<taxonomicName id="4C204D1FFF992165DB38FBADFDC76634" box="[527,616,1083,1107]" class="Bacilli" family="Lactobacillaceae" genus="Apilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="20" pageNumber="2802" phylum="Firmicutes" rank="species" species="mellifera">mellifera</taxonomicName>
</emphasis>
).
</paragraph>
</subSubSection>
