<taxonomicName id="B4D663E65908FFA455ACFA1D3A9DB55B" box="[1018,1169,1516,1541]" class="Branchiopoda" family="Branchinectidae" genus="Branchinecta" kingdom="Animalia" order="Anostraca" pageId="3" pageNumber="252" phylum="Arthropoda" rank="genus">
