<normalizedToken id="7DB1B62ED19FF6D70303363B9D3FA0F8" originalValue="divariquées">divariquees</normalizedToken>
, 
<normalizedToken id="88BFEFB79686F1EEB54D4E716295AF47" originalValue="étalées">etalees</normalizedToken>
