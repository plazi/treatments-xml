<paragraph id="A36836F2FF95FFAFF5A6F920FC70F91D" blockId="56.[151,937,1725,1781]" box="[151,937,1757,1781]" pageId="56" pageNumber="57">
<typeStatus id="7C6C8850FF95FFAFF5A6F920FF15F91D" box="[151,204,1757,1780]" pageId="56" pageNumber="57">Type</typeStatus>
species: 
<taxonomicName id="64D74D71FF95FFAFF41CF923FC7CF91D" authority="Malicky, 1994: 75" authorityName="Malicky &amp; Chantaramongkol" authorityPageNumber="75" authorityYear="1994" box="[301,933,1757,1781]" class="Insecta" family="Calamoceratidae" genus="Anisocentropus" kingdom="Animalia" order="Trichoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="janus" subGenus="Anisomontropus">
<emphasis id="91A3EAE0FF95FFAFF41CF923FE0BF91C" box="[301,466,1758,1781]" italics="true" pageId="56" pageNumber="57">Anisocentropus</emphasis>
(
