<heading id="1E62215EFF93D80212D13A3BBC4AFDEF" bold="true" box="[151,836,547,574]" fontSize="11" level="1" pageId="5" pageNumber="6" reason="1">
<emphasis id="77E14A20FF93D80212D13A3BBC4AFDEF" bold="true" box="[151,836,547,574]" pageId="5" pageNumber="6">
<taxonomicName id="8295EDB1FF93D80212D13A3BBDD7FDEF" authority="Zhong, Yang &amp; Morse" authorityName="Zhong, Yang &amp; Morse" box="[151,729,547,574]" class="Insecta" family="Polycentropodidae" genus="Plectrocnemia" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="pectinata" status="sp. nov.">
<emphasis id="77E14A20FF93D80212D13A3BBEB4FDEC" bold="true" box="[151,442,548,574]" italics="true" pageId="5" pageNumber="6">Plectrocnemia pectinata</emphasis>
Zhong, Yang &amp; Morse
</taxonomicName>
, 
<taxonomicNameLabel id="6CD2F75BFF93D80210A13A3CBC4AFDEF" box="[743,836,547,573]" pageId="5" pageNumber="6" rank="species">sp. nov.</taxonomicNameLabel>
