<typeStatus id="54CD8866FF96FFE8FF373CED22B8FD7C" box="[151,201,711,732]" pageId="3" pageNumber="431">Type</typeStatus>
species: 
<taxonomicName id="4C764D47FF96FFE8FE863CED2032FD7C" authority="Druce, 1899" authorityName="Druce" authorityYear="1899" box="[294,579,710,732]" class="Insecta" family="Sesiidae" genus="Aegeria" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="431" phylum="Arthropoda" rank="species" species="pythes">
