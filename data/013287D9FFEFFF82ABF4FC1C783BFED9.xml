<paragraph id="892436CFFFEFFF83AAC4FBCC7B82FAF0" blockId="32.[213,1170,1053,1300]" pageId="32" pageNumber="2791">
<taxonomicName id="4E9B4D4CFFEFFF83AAC4FBCC7BB2FBD1" authorityName="Westwood" authorityYear="1834" box="[213,438,1053,1077]" class="Insecta" family="Veliidae" genus="Microvelia" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="2791" phylum="Arthropoda" rank="species" species="pulchella">
<emphasis id="BBEFEADDFFEFFF83AAC4FBCC7BB2FBD1" box="[213,438,1053,1077]" italics="true" pageId="32" pageNumber="2791">Microvelia pulchella</emphasis>
</taxonomicName>
