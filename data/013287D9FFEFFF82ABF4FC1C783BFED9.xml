<collectingCountry id="F18C765FFFEFFF83ABADFBEC7813FBB1" box="[444,535,1085,1109]" name="Canada" pageId="32" pageNumber="2791">Canada</collectingCountry>
, 
<collectingCountry id="F18C765FFFEFFF83A837FBEC785BFBB1" box="[550,607,1085,1109]" name="United States of America" pageId="32" pageNumber="2791">USA</collectingCountry>
(including 
<collectingRegion id="4B5FF82DFFEFFF83A8F4FBEC793CFBB1" box="[741,824,1085,1109]" country="United States of America" name="Alaska" pageId="32" pageNumber="2791">Alaska</collectingRegion>
), 
<collectingCountry id="F18C765FFFEFFF83A95EFBEC79A2FBB1" box="[847,934,1085,1109]" name="Mexico" pageId="32" pageNumber="2791">Mexico</collectingCountry>
, continental Central America, Antilles, 
<collectingCountry id="F18C765FFFEFFF83ABA2FB8C7823FB91" box="[435,551,1117,1141]" name="Colombia" pageId="32" pageNumber="2791">Colombia</collectingCountry>
, 
<collectingCountry id="F18C765FFFEFFF83A829FB8C78A9FB91" box="[568,685,1117,1141]" name="Venezuela" pageId="32" pageNumber="2791">Venezuela</collectingCountry>
, 
