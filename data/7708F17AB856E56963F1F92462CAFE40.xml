; 
<quantity id="3859ED89B856E568611CF82B61AEF892" box="[634,706,1971,1997]" metricMagnitude="4" metricUnit="m" metricValue="1.6" pageId="116" pageNumber="117" unit="km" value="16.0">16 km</quantity>
