<quantity id="4CB79B0FFFB9FF9A49B3FBD8FDF6FBE2" box="[439,584,1083,1107]" metricMagnitude="-2" metricUnit="m" metricValue="3.725" metricValueMax="3.9" metricValueMin="3.55" pageId="25" pageNumber="890" unit="mm" value="37.25" valueMax="39.0" valueMin="35.5">
35.5 
<emphasis id="B93BEAF8FFB9FF9A49E2FBD8FE4DFBE2" box="[486,499,1083,1107]" italics="true" pageId="25" pageNumber="890">–</emphasis>
39 mm
</quantity>
[mean (
<emphasis id="B93BEAF8FFB9FF9A4AB6FBD8FD7EFBE2" box="[690,704,1083,1107]" italics="true" pageId="25" pageNumber="890">n</emphasis>
= 5) 
<quantity id="4CB79B0FFFB9FF9A4B0AFBD8FC38FBE2" box="[782,902,1083,1107]" metricMagnitude="-2" metricUnit="m" metricValue="3.724" pageId="25" pageNumber="890" unit="mm" value="37.24">37.24 mm</quantity>
, SD = 0.777]; female 
<quantity id="4CB79B0FFFB9FF9A48C4FBBDFE85FBC7" box="[192,315,1117,1142]" metricMagnitude="-2" metricUnit="m" metricValue="4.4" metricValueMax="4.9" metricValueMin="3.9" pageId="25" pageNumber="890" unit="mm" value="44.0" valueMax="49.0" valueMin="39.0">
