<taxonomicName id="22A5BDF1FF89FFBA053AFB87FD34FBBA" authorityName="Simon" authorityYear="1897" box="[519,665,1123,1150]" class="Arachnida" family="Selenopidae" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="family">Selenopidae</taxonomicName>
