<taxonomicName id="22A5BDF1FF89FFBA053BFB2BFD21FB2E" authorityName="Blackwall" authorityYear="1864" box="[518,652,1231,1258]" class="Arachnida" family="Scytodidae" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="family">Scytodidae</taxonomicName>
