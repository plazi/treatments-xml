</taxonomicName>
, 
<taxonomicName id="D1D5FA2E5347BB89F546655A3F4C05B6" authorityName="Decne" authorityYear="1835" class="Dicotyledoneae" family="Apocynaceae" genus="Rhazya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rhazya stricta" order="Gentianales" pageId="30" pageNumber="31" phylum="Angiospermae" rank="species" species="stricta">
<emphasis id="474608D7F7D9DBCED006A46A5F5870BB" italics="true" pageId="30" pageNumber="31">Rhazya stricta</emphasis>
</taxonomicName>
and 
<taxonomicName id="61D28449137A6ADC92AC9189524E9282" class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="nummularia">
<emphasis id="77C6EB54E1968E8BA82DDCB20709323B" italics="true" pageId="30" pageNumber="31">Ziziphus nummularia</emphasis>
