<taxonomicName id="B4415C07CE3189CB7525F47C90C54E6E" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia ehrenbergiana" order="Fabales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="ehrenbergiana">
<emphasis id="CA3DE2FD5C09FB27C72DEF6463CF8912" italics="true" pageId="30" pageNumber="31">Acacia ehrenbergiana</emphasis>
</taxonomicName>
, 
<taxonomicName id="2E33CFE77AAB0D4DCC62424A0CD93CC0" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="30" pageNumber="31" phylum="Angiospermae" rank="species" species="shawii">
