<subSubSection id="1096DE60D4C92F50EED2D91DF85BAAB7" pageId="0" pageNumber="91" type="nomenclature">
<paragraph id="5CBE41D9E6D6A8A2BA282D8C7C52FF2B" pageId="0" pageNumber="91">
<taxonomicName id="254785CFAAF99D35F4A6F617A9A291F7" LSID="BA6AF1D7-4875-50A6-A448-7A5329BE7CFB" authority="Tennakoon, C. H Kuo &amp; K. D Hyde" class="Eurotiomycetes" family="Chaetothyriaceae" genus="Longihyalospora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Longihyalospora" order="Chaetothyriales" pageId="0" pageNumber="91" phylum="Ascomycota" rank="genus">Longihyalospora Tennakoon, C.H Kuo &amp; K. D Hyde</taxonomicName>
<taxonomicNameLabel id="22DAF42C7F950FD69BBB6C56479F614F" pageId="0" pageNumber="91">gen. nov.</taxonomicNameLabel>
