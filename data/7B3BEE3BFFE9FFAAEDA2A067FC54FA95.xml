<taxonomicName id="349224AEFFE9FFAAEF11A5D8FD0BFAB1" box="[548,741,1267,1291]" class="Insecta" family="Cicadellidae" genus="Makilingia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="496" phylum="Arthropoda" rank="species" species="banahaoensis">
