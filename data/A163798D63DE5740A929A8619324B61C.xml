<paragraph id="90813B2D8A2ADB25687CA031EE722258" pageId="228" pageNumber="229">
<taxonomicName id="D74AE6CAE7274CB2B73C6522EDCB79B6" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" baseAuthorityName="Kriebel" baseAuthorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia trichosantha" order="Myrtales" pageId="228" pageNumber="229" phylum="Tracheophyta" rank="species" species="trichosantha" status="comb. nov.">
