<typeStatus id="B14F886EFF8FFFD6FC26FC26FC51FC3B" box="[941,1004,937,963]" pageId="2" pageNumber="49">Type</typeStatus>
: 
<taxonomicName id="A9F44D4FFF8FFFD6FC71FC26FE76FC12" authority="(Martius 1817: 177) Dumortier (1835: 17)" authorityName="Dumortier" authorityPageNumber="17" authorityYear="1835" baseAuthorityName="Martius" baseAuthorityPageNumber="177" baseAuthorityYear="1817" class="Jungermanniopsida" family="Anastrophyllaceae" genus="Lophozia" kingdom="Plantae" order="Jungermanniales" pageId="2" pageNumber="49" phylum="Bryophyta" rank="species" species="attenuata">
<emphasis id="5C80EADEFF8FFFD6FC71FC26FB5DFC3A" box="[1018,1248,937,962]" italics="true" pageId="2" pageNumber="49">Lophozia attenuata</emphasis>
