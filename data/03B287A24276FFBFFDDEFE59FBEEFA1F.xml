<taxonomicName id="4C1B4D374276FFBFFEA8FE1FFD44FD88" authority="Borgmeier, 1967 b: 180" authorityName="Borgmeier" authorityPageNumber="180" authorityYear="1967" box="[313,738,493,515]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="538" phylum="Arthropoda" rank="species" species="claudia">
