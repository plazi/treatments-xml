<taxonomicName id="9B844D64FFEA6243D424F9A1FB86F9AD" authorityName="Chopard" authorityYear="1934" box="[882,1037,1620,1645]" class="Insecta" family="Rhaphidophoridae" genus="Troglophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="brevicauda">
