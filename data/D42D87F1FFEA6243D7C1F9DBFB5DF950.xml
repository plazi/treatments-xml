<taxonomicName id="9B844D64FFEA6243D303F983FB59F950" box="[1109,1234,1655,1680]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
