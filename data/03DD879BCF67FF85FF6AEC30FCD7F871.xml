<heading id="D08381E1CF67FF85FF6AEC30FD86FC60" bold="true" box="[151,545,872,898]" fontSize="11" level="1" pageId="36" pageNumber="37" reason="1">
<emphasis id="B900EA9FCF67FF85FF6AEC30FD86FC60" bold="true" box="[151,545,872,898]" pageId="36" pageNumber="37">
<taxonomicName id="4C744D0ECF67FF85FF6AEC30FED1FC60" authority="González-Obando &amp; Carrejo-Gironza &amp; García, 2017" authorityName="González-Obando &amp; Carrejo-Gironza &amp; García" authorityYear="2017" box="[151,374,872,898]" class="Insecta" family="Ptiloneuridae" genus="Triplocania" kingdom="Animalia" order="Psocodea" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="caribe" status="n. sp.">
<emphasis id="B900EA9FCF67FF85FF6AEC30FED1FC60" bold="true" box="[151,374,872,898]" italics="true" pageId="36" pageNumber="37">Triplocania caribe</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A23357E4CF67FF85FE80EC30FE18FC60" box="[381,447,872,898]" pageId="36" pageNumber="37" rank="species">n. sp.</taxonomicNameLabel>
