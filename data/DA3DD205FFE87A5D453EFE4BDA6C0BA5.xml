<subSubSection id="1A8E3098FFE97A5D450EFBC9DF9B0B7D" pageId="89" pageNumber="90" type="etymology">
<paragraph id="522B6313FFE97A5D450EFBC9DF9B0B7D" blockId="89.[264,1323,283,1190]" pageId="89" pageNumber="90">
<emphasis id="60E0BF01FFE97A5D450EFBC9DFAF0B55" bold="true" box="[312,445,1084,1110]" pageId="89" pageNumber="90">Etymology</emphasis>
