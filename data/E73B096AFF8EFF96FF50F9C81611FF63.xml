<taxonomicName id="A892C3FFFF8DFF97FEECF97B1614F902" authority="Appadoo &amp; Myers, 2006" authorityName="Appadoo &amp; Myers" authorityYear="2006" box="[299,823,1694,1716]" class="Malacostraca" family="Melitidae" genus="Elasmopus" kingdom="Animalia" order="Amphipoda" pageId="18" pageNumber="219" phylum="Arthropoda" rank="species" species="souillacensis">
