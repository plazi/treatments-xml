<taxonomicName id="D6D15DF6659B85B5D33D214EFD5B9BF9" authorityName="O'Hara &amp; Stohr" authorityYear="2006" box="[525,675,1127,1153]" class="Ophiuroidea" family="Ophiacanthidae" genus="Ophiohamus" kingdom="Animalia" order="Ophiurida" pageId="206" pageNumber="207" phylum="Echinodermata" rank="genus">Ophiohamus</taxonomicName>
</emphasis>
