<taxonomicName id="D6D15DF6659B85B5D052214EFDFB9BF9" authorityName="Thuy" authorityYear="2013" box="[354,515,1127,1153]" class="Ophiuroidea" family="Ophiacanthidae" genus="Sabinacantha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ophiurida" pageId="206" pageNumber="207" phylum="Echinodermata" rank="genus">Sabinacantha</taxonomicName>
