<specimenCount id="9D64FD63FFC7FFD3FD87FEE6DC2DE619" box="[590,611,261,284]" pageId="5" pageNumber="542" type="female">♀</specimenCount>
(n=3), 
<typeStatus id="54D98848FFC7FFD3FD72FEE7DD53E618" box="[699,797,260,285]" pageId="5" pageNumber="542">holotype</typeStatus>
