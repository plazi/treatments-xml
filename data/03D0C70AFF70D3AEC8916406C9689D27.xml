<taxonomicName id="4C790D9FFF70D3AECBC362F6C8C89D6F" authority="Kandil. However" authorityName="Kandil. However" class="Arachnida" family="Ascidae" genus="Neojordensia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="201" pageNumber="202" phylum="Arthropoda" rank="species" species="asetosa" subGenus="Kandilia">
