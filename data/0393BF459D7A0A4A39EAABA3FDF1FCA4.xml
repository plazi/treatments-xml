<taxonomicName id="4C3A75D09D780A4A3BC6A85FFCE2FCC8" box="[699,861,797,823]" class="Insecta" family="Cerambycidae" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="182" phylum="Arthropoda" rank="family">Cerambycidae</taxonomicName>
