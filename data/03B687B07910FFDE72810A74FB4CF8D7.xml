'' S
</geoCoordinate>
, 
<geoCoordinate id="EE2B50617910FFDE733C0AC7FE2EFB7B" box="[298,442,1193,1218]" direction="west" orientation="longitude" pageId="2" pageNumber="394" precision="15" value="-35.778057">
<number id="3DD724817910FFDE733C0AC7FED3FB78" box="[298,327,1193,1218]" pageId="2" pageNumber="394" value="35.0">35</number>
° 
