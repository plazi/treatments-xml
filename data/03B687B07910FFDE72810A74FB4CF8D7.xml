, 
<quantity id="4CE79B437910FFDE73D30AC4FD9AFB7B" box="[453,526,1194,1218]" metricMagnitude="2" metricUnit="m" metricValue="7.2" pageId="2" pageNumber="394" unit="m" value="720.0">720 m</quantity>
, 
