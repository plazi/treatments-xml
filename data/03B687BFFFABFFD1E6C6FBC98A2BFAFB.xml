<taxonomicName id="4C1F4D2AFFABFFD1E4D4FB1C8CA5FB23" authorityName="Tolkanitz" authorityYear="1993" box="[645,767,1230,1256]" class="Insecta" family="Curculionidae" genus="Exochus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="187" phylum="Arthropoda" rank="species" species="karazini">
