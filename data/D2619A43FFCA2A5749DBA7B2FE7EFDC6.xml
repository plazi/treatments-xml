<taxonomicName id="9DC850D6FFCA2A514883A176FDBAFAEF" baseAuthorityName="Nowakowski" baseAuthorityYear="1961" box="[463,583,1303,1329]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="51" pageNumber="52" phylum="Tracheophyta" rank="genus">
