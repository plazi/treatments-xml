Known only from 
<collectingRegion id="775F2B9FBB54FFDAD3C4C1B29CF5F87C" box="[1220,1469,1990,2014]" country="Thailand" name="Chiang Mai" pageId="3" pageNumber="215">Chiang Mai Province</collectingRegion>
, 
<collectingCountry id="CD8CA5EDBB54FFDAD443C1929AEDF85C" box="[835,933,2022,2046]" name="Thailand" pageId="3" pageNumber="215">Thailand</collectingCountry>
, at 
<quantity id="72634898BB54FFDAD4CDC1929D38F85C" box="[973,1136,2022,2046]" metricMagnitude="3" metricUnit="m" metricValue="2.525" metricValueMax="2.53" metricValueMin="2.52" pageId="3" pageNumber="215" unit="m" value="2525.0" valueMax="2530.0" valueMin="2520.0">
<geoCoordinate id="D0AF83BABB54FFDAD4CDC1929D41F85C" box="[973,1033,2022,2046]" degrees="2,520" direction="north" orientation="latitude" pageId="3" pageNumber="215" precision="55" value="2.52">2,520</geoCoordinate>
