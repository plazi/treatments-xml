<taxonomicName id="12C34D156D0A5247FDCD5E868E97FA15" authorityName="Riedel" authorityYear="2001" box="[556,692,1456,1480]" class="Insecta" family="Attelabidae" genus="Euops" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="1197" phylum="Arthropoda" rank="species" species="nothofagi">
<emphasis id="E7B7EA846D0A5247FDCD5E868E97FA15" box="[556,692,1456,1480]" italics="true" pageId="26" pageNumber="1197">E. nothofagi</emphasis>
</taxonomicName>
and 
<taxonomicName id="12C34D156D0A5247FD0F5E868F7AFA15" authorityName="Riedel" authorityYear="2001" box="[750,857,1456,1480]" class="Insecta" family="Attelabidae" genus="Euops" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="1197" phylum="Arthropoda" rank="species" species="bicolor">
<emphasis id="E7B7EA846D0A5247FD0F5E868F7AFA15" box="[750,857,1456,1480]" italics="true" pageId="26" pageNumber="1197">E. bicolor</emphasis>
</taxonomicName>
on the extensive leaf ¯ush of a 
<taxonomicName id="12C34D156D0A5247FEE25EE78DEDFA35" box="[259,462,1488,1512]" class="Magnoliopsida" family="Nothofagaceae" genus="Nothofagus" kingdom="Plantae" order="Fagales" pageId="26" pageNumber="1197" phylum="Tracheophyta" rank="species" species="perryi">
<emphasis id="E7B7EA846D0A5247FEE25EE78DEDFA35" box="[259,462,1488,1512]" italics="true" pageId="26" pageNumber="1197">Nothofagus perryi</emphasis>
</taxonomicName>
