<taxonomicName id="4C6B6DB9323C5C79FC45D377A8CFFDC1" box="[922,1159,578,602]" class="Magnoliopsida" family="Solanaceae" genus="Lycopersicon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Solanales" pageId="36" pageNumber="35" phylum="Tracheophyta" rank="species" species="pimpinellifolium">
