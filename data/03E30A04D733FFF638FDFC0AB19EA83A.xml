<date id="FFF49DD2D733FFF93CFAFB4AB16DAFF9" box="[1223,1363,1249,1273]" pageId="7" pageNumber="6" value="1916-04-29">29-IV-1916</date>
, sobre 
<taxonomicName id="4C4AC091D733FFF938FDFAA9B60FAE1A" authority=", A. H. Ritchie" authorityName="A. H. Ritchie" box="[192,561,1282,1306]" class="Magnoliopsida" family="Convolvulaceae" genus="Ipomoea" kingdom="Plantae" order="Solanales" pageId="7" pageNumber="6" phylum="Tracheophyta" rank="species" species="batatae">
