<taxonomicName id="D26B50C34736E63555EB8149055B4443" authority="Hübner, 1819" authorityName="Hübner" authorityYear="1819" class="Insecta" family="Arctiidae" genus="Halisidota" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Halisidota" order="Lepidoptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="genus">Halisidota</taxonomicName>
(sic) 
<taxonomicName id="78F5CCB219D1E0E793BD5A81D7EF37B2" family="Arctiidae" genus="L" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="L hyalinipuncta" order="Lepidoptera" pageId="2" pageNumber="59" rank="species" species="hyalinipuncta">hyalinipuncta</taxonomicName>
