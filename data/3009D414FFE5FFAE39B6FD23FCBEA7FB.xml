<taxonomicName id="7FA01E81FFE5FFAE39E2FD20FCB4A0AE" authority="Frey, 1975" authorityName="Frey" authorityYear="1975" box="[195,805,721,748]" class="Insecta" family="Melolonthidae" genus="Phyllophaga" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="129" phylum="Arthropoda" rank="species" species="martinezi" subGenus="Phyllophaga">
