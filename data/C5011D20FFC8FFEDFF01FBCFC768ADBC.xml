<taxonomicName id="8AA8D7B5FFCFFFEDFB26FEB7C36FAD27" authorityName="Thompson" authorityYear="1840" box="[1200,1347,261,284]" class="Actinopterygii" family="Bregmacerotidae" genus="Bregmaceros" kingdom="Animalia" order="Gadiformes" pageId="20" pageNumber="421" phylum="Chordata" rank="genus">
