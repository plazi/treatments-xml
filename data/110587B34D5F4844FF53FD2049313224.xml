; current records), 
<collectingCountry id="E1BB76354D5F4845FC83F94B48DF33CE" box="[839,942,1724,1750]" name="Bahamas" pageId="84" pageNumber="85">Bahamas</collectingCountry>
(
<bibRefCitation id="FD3D4B544D5F4845FC04F94B4FD733CE" author="Wiedenmayer, F." box="[960,1190,1724,1750]" pageId="84" pageNumber="85" pagination="1 - 287" refId="ref82989" refString="Wiedenmayer, F. (1977) Shallow-water sponge of the western Bahamas. Experientia Supplementum, 28, 1 - 287, pls. 1 - 43. https: // doi. org / 10.1007 / 978 - 3 - 0348 - 5797 - 0" type="journal article" year="1977">Wiedenmayer 1977</bibRefCitation>
), 
<collectingCountry id="E1BB76354D5F4845FB79F94B4F9033CE" box="[1213,1249,1724,1750]" name="United States of America" pageId="84" pageNumber="85">US</collectingCountry>
(
