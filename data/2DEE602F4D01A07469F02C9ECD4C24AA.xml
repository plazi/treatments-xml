<taxonomicName id="947B79E8377E9F28A891B2E80811567F" LSID="http://zoobank.org/9D213EA9-95C4-4C94-9B5A-5E0CDB0E1357" class="Collembola" family="Tullbergiidae" genus="Tullbergia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tullbergia alcirae" order="Poduromorpha" pageId="1" pageNumber="24" phylum="Arthropoda" rank="species" species="alcirae">Tullbergia alcirae</taxonomicName>
