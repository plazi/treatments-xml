<subSubSection id="C30065109B01FFF5FF534C239C3FF902" pageId="25" pageNumber="176" type="materials_examined">
<paragraph id="8BA5369B9B01FFF5FF534C239E45F926" pageId="25" pageNumber="176">
<emphasis id="B96EEA899B01FFF5FF534C239EBAF97B" bold="true" box="[151,496,259,284]" pageId="25" pageNumber="176">Type locality. AUSTRALIA</emphasis>
