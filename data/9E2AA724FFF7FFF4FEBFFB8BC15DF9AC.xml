<taxonomicName id="D1836DB1FFF7FFF4FEE8FB8BC619FB36" authority="Curtis" authorityName="Curtis" box="[351,577,1137,1163]" class="Insecta" family="Formicidae" genus="Myrmecina" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis id="24F7CA20FFF7FFF4FEE8FB8BC5B1FB36" bold="true" box="[351,489,1137,1163]" italics="true" pageId="33" pageNumber="34">Myrmecina</emphasis>
