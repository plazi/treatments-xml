<taxonomicName id="4C074D7C867E786560A9FC46855CFBAF" box="[1252,1350,1017,1040]" class="Amphibia" family="Hylodidae" genus="Hylodes" kingdom="Animalia" order="Anura" pageId="10" pageNumber="567" phylum="Chordata" rank="species" species="nasus">
