<quantity id="1F3B02D7E042BC7AFD1BFA3CF66952A0" box="[716,769,2168,2206]" metricMagnitude="-2" metricUnit="kg" metricValue="2.6" pageId="15" pageNumber="298" unit="g" value="26.0">269</quantity>
