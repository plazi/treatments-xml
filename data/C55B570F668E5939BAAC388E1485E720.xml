<taxonomicName id="6D67D9D0FCAB6BAA42249CF891C682C0" genus="C." lsidName="C. ligula" pageId="0" pageNumber="89" rank="species" species="ligula">
<emphasis id="1D7FD3B40CCD87ACC9DB39C6BA250CAC" italics="true" pageId="0" pageNumber="89">C. ligula</emphasis>
</taxonomicName>
(HKAS 35954) 
<emphasis id="ED27FD7DBBA151C642E27141E2C651BE" bold="true" pageId="0" pageNumber="89">h</emphasis>
