<taxonomicName id="2E8E4D0CFFFAFF9BD974FCA2FB514643" box="[1121,1161,812,849]" class="Insecta" family="Carabidae" genus="Xystosomus" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="25" phylum="Arthropoda" rank="species" species="gruti">X.</taxonomicName>
