(
<taxonomicName id="BC1E8C454535483926CC1A507FAACE36" authority="L." authorityName="L." class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="446" phylum="Tracheophyta" rank="species" species="pimpinellifolia">
