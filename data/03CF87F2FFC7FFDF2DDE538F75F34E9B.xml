<taxonomicName id="4C664D67FFC7FFDF2F6055CE75CA4E9B" authorityName="Sooty" baseAuthorityName="Gmelin" baseAuthorityYear="1789" box="[553,665,1479,1502]" class="Aves" family="Procellariidae" genus="Puffinus" kingdom="Animalia" order="Procellariiformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="griseus">
