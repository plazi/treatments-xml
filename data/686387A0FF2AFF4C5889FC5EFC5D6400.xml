<taxonomicName id="27CA4D35FF2AFF4C5A65FB7EFCE16474" authorityName="Rondani" authorityYear="1850" box="[635,850,1267,1293]" class="Insecta" family="Stratiomyidae" genus="Sargus" kingdom="Animalia" order="Diptera" pageId="231" pageNumber="232" phylum="Arthropoda" rank="species" species="claviventris">
