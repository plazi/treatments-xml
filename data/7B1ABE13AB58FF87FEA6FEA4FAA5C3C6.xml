<typeStatus id="2C08B1A7AB58FF87FE96FE57FEDFC406" box="[312,370,495,521]" pageId="85" pageNumber="86">Type</typeStatus>
material of 
<taxonomicName id="34B37486AB58FF87FDAFFE57FDDFC407" box="[513,626,495,520]" class="Malacostraca" family="Cyllopodidae" genus="Cyllopus" kingdom="Animalia" order="Amphipoda" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="lucasii">
