<taxonomicName id="0FEAF09141DA6D7A7C43749DBE03BE56" class="Arachnida" family="Suctobelbidae" genus="Suctobelba" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="138" phylum="Arthropoda" rank="species" species="regia">Suctobelba regia</taxonomicName>
