<paragraph id="70154088A2AA345B02937BFFF2089EB1" pageId="0" pageNumber="31">
<materialsCitation id="72969F6ED3FFC06F0EE4F168A2C2AC84" collectingDate="2017-06-07" collectorName="X. H. Xiong" country="China" elevation="2950" latitude="29.429316" location="Dayandong" longLatPrecision="1" longitude="95.049545" municipality="Medog" specimenCount="1" stateProvince="Tibet" typeStatus="holotype">
