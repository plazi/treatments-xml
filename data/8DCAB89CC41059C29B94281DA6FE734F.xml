, 
<geoCoordinate id="CFCEE963349E63BC7B844D0298964D3B" degrees="29" direction="north" minutes="25" orientation="latitude" precision="1" seconds="45.54" value="29.429316">29°25'45.54&quot;N</geoCoordinate>
