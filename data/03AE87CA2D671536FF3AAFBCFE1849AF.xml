<taxonomicName id="4C074D5F2D671535FECDAE6AFD5A4F41" authority="Schneider, 1845" authorityName="Schneider" authorityYear="1845" box="[275,722,1801,1843]" class="Insecta" family="Libellulidae" genus="Libellula" kingdom="Animalia" order="Odonata" pageId="1" pageNumber="194" phylum="Arthropoda" rank="species" species="ampullacea">
