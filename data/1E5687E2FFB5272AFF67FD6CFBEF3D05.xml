<taxonomicName id="51FF4D77FFB52729FD70F877FBCE3EF7" authority="Chopard, 1929" authorityName="Chopard" authorityYear="1929" box="[640,1063,1938,1960]" class="Insecta" family="Haglotettigoniidae" genus="Loxoblemmus" kingdom="Animalia" order="Orthoptera" pageId="9" pageNumber="338" phylum="Arthropoda" rank="species" species="intermedius">
