<taxonomicName id="4C663190FFA32542EBE7FD8C1F205EEE" authority="Saussure, 1870" authorityName="Saussure" authorityYear="1870" box="[151,575,619,645]" class="Insecta" family="Mantidae" genus="Stagmomantis" kingdom="Animalia" order="Mantodea" pageId="18" pageNumber="519" phylum="Arthropoda" rank="species" species="vicina">
