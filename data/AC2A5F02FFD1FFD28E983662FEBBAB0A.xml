<taxonomicName id="E3839597FFD1FFD18E9836E1FEADAE85" authority="Heyne &amp; Taschenberg, 1907" authorityName="Heyne &amp; Taschenberg" authorityYear="1907" box="[151,278,1237,1259]" class="Insecta" family="Cerambycidae" genus="Trepanideus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="254" phylum="Arthropoda" rank="genus">Trepanideus</taxonomicName>
