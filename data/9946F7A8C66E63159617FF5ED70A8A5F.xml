1. 
<taxonomicName id="6EA43CE9AAA32E90B1A088DCCF7E9F70" class="Arachnida" family="Parasitidae" genus="Parasitus" kingdom="Animalia" order="Mesostigmata" pageId="0" pageNumber="139" phylum="Arthropoda" rank="species" species="hortivagus">Parasitus hortivagus</taxonomicName>
