<taxonomicName id="D9854D5FAB68FFE4F80F65E69CEE5F9F" authorityName="Moure" authorityYear="1947" box="[1068,1192,388,411]" class="Insecta" family="Halictidae" genus="Rhectomia" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis id="2CF1EACEAB68FFE4F80F65E69CEE5F9F" box="[1068,1192,388,411]" italics="true" pageId="53" pageNumber="54">Rhectomia</emphasis>
</taxonomicName>
occur in northern 
<collectingCountry id="6692764CAB68FFE4FF5065C39BAC5FBC" box="[883,1002,417,440]" name="Argentina" pageId="53" pageNumber="54">Argentina</collectingCountry>
, southern 
<collectingCountry id="6692764CAB68FFE4F84865C39CF35FBC" box="[1131,1205,417,440]" name="Brazil" pageId="53" pageNumber="54">Brazil</collectingCountry>
, 
<collectingCountry id="6692764CAB68FFE4F8E665C39B5E5FD1" name="Bolivia" pageId="53" pageNumber="54">Bolivia</collectingCountry>
, 
<collectingCountry id="6692764CAB68FFE4FF0565DC9BD25FD1" box="[806,916,446,469]" name="Paraguay" pageId="53" pageNumber="54">Paraguay</collectingCountry>
, and southern 
<collectingCountry id="6692764CAB68FFE4F86765DC9C3B5FD1" box="[1092,1149,446,469]" name="Peru" pageId="53" pageNumber="54">Peru</collectingCountry>
