</taxonomicName>
(
<taxonomicName id="20884D6EFFCEFFBF2B349EF0FCEAFA91" authority="Stone, 1944" authorityName="Stone" authorityYear="1944" box="[545,869,1266,1292]" pageId="9" pageNumber="44" rank="variety" variety="townsendi">
var. 
