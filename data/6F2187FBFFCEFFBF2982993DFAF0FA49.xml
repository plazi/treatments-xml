<taxonomicName id="20884D6EFFCEFFBF2A6A9F69FB14FA19" authority="Rondani" authorityName="Rondani" box="[895,1179,1386,1412]" class="Insecta" family="Culicidae" genus="Trichoprosopon" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="44" phylum="Arthropoda" rank="species" species="digitatum">
<emphasis id="D5FCEAFFFFCEFFBF2A6A9F69FC17FA19" box="[895,920,1387,1412]" italics="true" pageId="9" pageNumber="44">
<collectingCountry id="9F9F767DFFCEFFBF2A6A9F69FC17FA19" box="[895,920,1387,1412]" name="Chile" pageId="9" pageNumber="44">Tr</collectingCountry>
