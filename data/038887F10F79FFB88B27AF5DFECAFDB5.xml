</taxonomicName>
, 
<taxonomicName id="4C214D640F79FFA78E8FAFC8FA8BF923" authorityName="Cappetta" authorityYear="1980" box="[1193,1331,1667,1688]" class="Elasmobranchii" family="Sclerorhynchidae" genus="Micropristis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="14" pageNumber="330" phylum="Chordata" rank="genus">
<emphasis id="B955EAF50F79FFA78E8FAFC8FA8BF923" box="[1193,1331,1667,1688]" italics="true" pageId="14" pageNumber="330">Micropristis</emphasis>
