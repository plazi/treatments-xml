<emphasis id="B955EAF50F79FFA78F0AAF2FFCEEF923" italics="true" pageId="14" pageNumber="330">Ctenopristis</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C214D640F79FFA7894EAFC8FC55F923" authorityName="Arambourg" authorityYear="1935" box="[872,1005,1667,1688]" class="Elasmobranchii" family="Sclerorhynchidae" genus="Ganopristis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rajiformes" pageId="14" pageNumber="330" phylum="Chordata" rank="genus">
<emphasis id="B955EAF50F79FFA7894EAFC8FC55F923" box="[872,1005,1667,1688]" italics="true" pageId="14" pageNumber="330">Ganopristis</emphasis>
</taxonomicName>
, 
