<taxonomicName id="E9D933B5FFA2DD7FFB042D70624611F3" box="[1198,1300,1915,1939]" class="Insecta" family="Chrysomelidae" genus="Menippus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="issikii">
