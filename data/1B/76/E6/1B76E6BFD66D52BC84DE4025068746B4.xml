<taxonomicName class="Actinopteri" genus="H." higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="H. mormyrops" order="Characiformes" pageId="0" pageNumber="25" phylum="Chordata" rank="species" species="mormyrops">
<emphasis italics="true" pageId="0" pageNumber="25">H. mormyrops</emphasis>
</taxonomicName>
