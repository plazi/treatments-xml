<paragraph pageId="0" pageNumber="25">Diagnosis.</paragraph>
<paragraph pageId="0" pageNumber="25">
<taxonomicName baseAuthorityName="Borodin" baseAuthorityYear="1929" class="Actinopterygii" family="Anostomidae" genus="Hypomasticus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypomasticus thayeri" order="Characiformes" pageId="0" pageNumber="25" phylum="Chordata" rank="species" species="thayeri">
<emphasis italics="true" pageId="0" pageNumber="25">Hypomasticus thayeri</emphasis>
</taxonomicName>
