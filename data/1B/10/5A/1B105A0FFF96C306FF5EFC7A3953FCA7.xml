locality (
<figureCitation box="[763,831,799,825]" captionStart="FIGURE 8" captionStartId="10.[151,250,1889,1913]" captionTargetBox="[151,1436,957,1865]" captionTargetId="figure-445@10.[151,1436,957,1865]" captionTargetPageId="10" captionText="FIGURE 8. Distribution of the genus Fluctus gen. nov." figureDoi="http://doi.org/10.5281/zenodo.4398368" httpUri="https://zenodo.org/record/4398368/files/figure.png" pageId="3" pageNumber="164">Fig. 8</figureCitation>
).
</paragraph>
</subSubSection>
