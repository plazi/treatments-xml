<collectingRegion box="[296,447,295,321]" country="Guatemala" name="Baja Verapaz" pageId="8" pageNumber="189">Baja Verapaz</collectingRegion>
, 
<collectingCountry box="[458,583,295,321]" name="Guatemala" pageId="8" pageNumber="189">Guatemala</collectingCountry>
</materialsCitation>
. 
<materialsCitation ID-GBIF-Occurrence="3107844303" box="[593,1381,295,321]" country="Mexico" location="Oaxaca" pageId="8" pageNumber="189" specimenCount="1" stateProvince="Oaxaca" typeStatus="holotype">
