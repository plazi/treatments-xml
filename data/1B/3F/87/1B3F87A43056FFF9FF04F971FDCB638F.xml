<collectingRegion box="[1194,1281,295,321]" country="Mexico" name="Oaxaca" pageId="8" pageNumber="189">Oaxaca</collectingRegion>
, 
<collectingCountry box="[1292,1381,295,321]" name="Mexico" pageId="8" pageNumber="189">Mexico</collectingCountry>
