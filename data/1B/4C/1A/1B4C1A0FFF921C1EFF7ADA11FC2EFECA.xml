<taxonomicName authority="Distant." authorityName="Distant." box="[288,698,392,418]" class="Insecta" family="Coreidae" genus="Cletomorpha" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="mackayensis">
