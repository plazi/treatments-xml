<taxonomicName box="[1184,1429,152,177]" class="Insecta" family="Coreidae" genus="Plinachtus" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="refulgens">
