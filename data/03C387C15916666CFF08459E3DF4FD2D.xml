<subSubSection id="C370655C5916666FFF0845DB3C18FE47" box="[151,821,471,495]" pageId="9" pageNumber="466" type="type_taxon">
<paragraph id="8BD536D75916666FFF0845DB3C18FE47" blockId="9.[151,821,471,495]" box="[151,821,471,495]" pageId="9" pageNumber="466">
<emphasis id="B91EEAC55916666FFF0845DB3E0BFE47" bold="true" box="[151,294,471,495]" pageId="9" pageNumber="466">
<typeStatus id="54D188755916666FFF0845DB3FE1FE47" box="[151,204,471,495]" pageId="9" pageNumber="466">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C6A4D545916666FFEB345DB3D0EFE47" authority="Brailovsky, 2021" authorityName="Brailovsky" authorityYear="2021" box="[300,547,471,495]" class="Insecta" family="Coreidae" genus="Cryptopetalops" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="466" phylum="Arthropoda" rank="species" species="llorentei" status="sp. nov.">
<emphasis id="B91EEAC55916666FFEB345DB3D0EFE47" box="[300,547,471,495]" italics="true" pageId="9" pageNumber="466">Cryptopetalops llorentei</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A22D57BE5916666FFDAF45DB3DAFFE46" box="[560,642,471,495]" pageId="9" pageNumber="466" rank="species">
<emphasis id="B91EEAC55916666FFDAF45DB3D51FE47" bold="true" box="[560,636,471,495]" pageId="9" pageNumber="466">sp. nov</emphasis>
