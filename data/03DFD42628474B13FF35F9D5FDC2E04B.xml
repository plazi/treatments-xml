<number id="3DBE771728474B13FD83F99EFD9DE163" box="[545,586,1609,1631]" pageId="14" pageNumber="215" value="902.0">902</number>
</treatmentCitation>
. 
<taxonomicName id="4C761EB328474B13FF35F9BFFE9DE142" box="[151,330,1640,1662]" class="Insecta" family="Cecidomyiidae" genus="Haplusia" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="hondrui">
