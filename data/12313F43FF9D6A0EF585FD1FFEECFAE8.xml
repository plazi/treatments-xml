<taxonomicName id="5D98F5D6FF9C6A0DF651F9C8FD7CF9DB" authority="(Shiino, 1949)" baseAuthorityName="Shiino" baseAuthorityYear="1949" box="[259,657,1604,1624]" class="Malacostraca" family="Bopyridae" genus="Parabopyrella" kingdom="Animalia" order="Isopoda" pageId="53" pageNumber="53" phylum="Arthropoda" rank="species" species="elongata">
