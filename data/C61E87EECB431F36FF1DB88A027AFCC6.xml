<taxonomicName id="89B74D7BCB431F36FE90B95B00F2FEE2" authorityName="Chu &amp; Wang" authorityYear="1988" baseAuthorityName="Chu &amp; Wang" baseAuthorityYear="1988" box="[282,477,325,349]" class="Insecta" family="Drepanidae" genus="Tridrepana" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="44" phylum="Arthropoda" rank="subSpecies" species="arikana" subSpecies="emina">
