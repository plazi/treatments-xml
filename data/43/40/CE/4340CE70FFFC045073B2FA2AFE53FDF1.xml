<emphasis box="[398,619,1827,1850]" italics="true" pageId="7" pageNumber="35">Acorystus albicornis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[627,713,1826,1850]" pageId="7" pageNumber="35" rank="species">
<emphasis bold="true" box="[627,713,1826,1850]" pageId="7" pageNumber="35">sp. nov.</emphasis>
</taxonomicNameLabel>
4, Holotype Ψ, head, dorsal; 
<emphasis box="[1033,1060,1827,1850]" italics="true" pageId="7" pageNumber="35">ob</emphasis>
, orbital band. 5, Paratype Ψ, head, frontal. 6, Holotype Ψ, metapleuron, propodeum and first metasomal tergite, lateral. 7, Holotype Ψ, ovipositor tip, lateral. 8–11, 
<taxonomicName box="[220,387,1891,1914]" class="Insecta" family="Ichneumonidae" genus="Acorystus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="35" phylum="Arthropoda" rank="species" species="circumflexus" status="sp. nov.">
<emphasis box="[220,387,1891,1914]" italics="true" pageId="7" pageNumber="35">A. circumflexus</emphasis>
