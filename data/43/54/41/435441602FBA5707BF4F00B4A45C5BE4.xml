<paragraph pageId="6" pageNumber="23">
<taxonomicName authorityName="Kobelt &amp; Moellendorff" authorityYear="1902" class="Gastropoda" family="Enidae" genus="Rhabdoena" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="6" pageNumber="23" phylum="Mollusca" rank="genus">Rhabdoena</taxonomicName>
