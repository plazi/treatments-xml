<typeStatus box="[334,426,1907,1933]" pageId="92" pageNumber="93">Syntype</typeStatus>
(1), 
<specimenCode box="[480,702,1907,1933]" collectionCode="NHMUK" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="92" pageNumber="93" type="Museum">NHMUK 20141112</specimenCode>
