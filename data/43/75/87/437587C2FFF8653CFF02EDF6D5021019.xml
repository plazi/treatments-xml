<taxonomicName authority="Pfeiffer, 1853" authorityName="Pfeiffer" authorityYear="1853" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="61" pageNumber="62" phylum="Mollusca" rank="species" species="varicosa">
