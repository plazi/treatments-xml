<taxonomicName authorityName="Reeve" authorityYear="1852" box="[1241,1419,1474,1500]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="28" pageNumber="29" phylum="Mollusca" rank="species" species="barbatula">
