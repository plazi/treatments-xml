<taxonomicName authorityName="Reeve" authorityYear="1852" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="53" pageNumber="54" phylum="Mollusca" rank="species" species="compressivoluta">
