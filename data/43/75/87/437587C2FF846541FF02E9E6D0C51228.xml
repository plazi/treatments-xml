<taxonomicName authority="Gould, 1846" authorityName="Gould" authorityYear="1846" box="[151,560,441,468]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="64" pageNumber="65" phylum="Mollusca" rank="species" species="ziczac" subGenus="Theba">
<emphasis bold="true" box="[151,560,441,468]" pageId="64" pageNumber="65">
