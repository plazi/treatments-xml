<taxonomicName authority="Le Guillou, 1842" authorityName="Le Guillou" authorityYear="1842" class="Gastropoda" family="Vitrinidae" genus="Vitrina" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="43" pageNumber="44" phylum="Mollusca" rank="species" species="zebra">
