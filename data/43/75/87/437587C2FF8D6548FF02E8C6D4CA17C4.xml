<taxonomicName authority="(Gray, 1843)" authorityName="Gray" authorityYear="1843" baseAuthorityName="Gray" baseAuthorityYear="1843" box="[558,876,796,822]" class="Gastropoda" family="Punctidae" genus="Laoma" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="73" pageNumber="74" phylum="Mollusca" rank="species" species="mariae">
