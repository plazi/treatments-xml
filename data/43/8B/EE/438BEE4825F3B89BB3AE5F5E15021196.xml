<emphasis italics="true" pageId="5" pageNumber="20">Amphitecna latifolia</emphasis>
</taxonomicName>
(purple circles) 
<taxonomicName authorityName="L. O. Williams" authorityYear="1973" class="Magnoliopsida" family="Bignoniaceae" genus="Amphitecna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Amphitecna montana" order="Lamiales" pageId="5" pageNumber="20" phylum="Tracheophyta" rank="species" species="montana">
<emphasis italics="true" pageId="5" pageNumber="20">Amphitecna montana</emphasis>
</taxonomicName>
(green cross), 
<taxonomicName authorityName="Ortiz-Rodr. &amp; Burelo" authorityYear="2016" class="Magnoliopsida" family="Bignoniaceae" genus="Amphitecna" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Amphitecna loreae" order="Lamiales" pageId="5" pageNumber="20" phylum="Tracheophyta" rank="species" species="loreae">
