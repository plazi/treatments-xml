<paragraph pageId="0" pageNumber="1115">
<taxonomicName class="Insecta" family="Apidae" genus="Bombus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bombus gerstaeckeri" order="Hymenoptera" pageId="0" pageNumber="1115" phylum="Arthropoda" rank="species" species="gerstaeckeri">Bombus gerstaeckeri</taxonomicName>
