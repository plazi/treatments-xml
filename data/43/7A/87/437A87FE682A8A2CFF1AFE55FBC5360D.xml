<taxonomicName box="[447,588,1412,1438]" class="Insecta" family="Pterophoridae" genus="Cnaemidophorus" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="species" species="vernicosa">
<emphasis box="[447,588,1412,1438]" italics="true" pageId="12" pageNumber="213">C. vernicosa</emphasis>
</taxonomicName>
) (
<taxonomicName box="[612,722,1411,1438]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="12" pageNumber="213" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
), 
<taxonomicName box="[741,963,1411,1437]" class="Magnoliopsida" family="Nyctaginaceae" genus="Boerhavia" kingdom="Plantae" order="Caryophyllales" pageId="12" pageNumber="213" phylum="Tracheophyta" rank="species" species="coccinea">
<emphasis box="[741,963,1411,1437]" italics="true" pageId="12" pageNumber="213">Boerhavia coccinea</emphasis>
</taxonomicName>
, 
<taxonomicName box="[978,1078,1412,1437]" class="Magnoliopsida" family="Nyctaginaceae" genus="Boerhavia" kingdom="Plantae" order="Caryophyllales" pageId="12" pageNumber="213" phylum="Tracheophyta" rank="species" species="erecta">
<emphasis box="[978,1078,1412,1437]" italics="true" pageId="12" pageNumber="213">B. erecta</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1092,1227,1411,1437]" class="Magnoliopsida" family="Nyctaginaceae" genus="Boerhavia" kingdom="Plantae" order="Caryophyllales" pageId="12" pageNumber="213" phylum="Tracheophyta" rank="species" species="chinensis">
<emphasis box="[1092,1227,1411,1437]" italics="true" pageId="12" pageNumber="213">B. chinensis</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Nyctaginaceae" genus="Commicarpus" kingdom="Plantae" order="Caryophyllales" pageId="12" pageNumber="213" phylum="Tracheophyta" rank="species" species="tuberosus">
<emphasis italics="true" pageId="12" pageNumber="213">Commicarpus tuberosus</emphasis>
