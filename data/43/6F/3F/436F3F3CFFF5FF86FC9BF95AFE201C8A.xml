<subSubSection lastPageId="39" lastPageNumber="104" pageId="38" pageNumber="103" type="materials_examined">
<paragraph blockId="38.[145,567,1185,1239]" box="[145,246,1185,1209]" pageId="38" pageNumber="103">
<emphasis box="[145,246,1185,1209]" italics="true" pageId="38" pageNumber="103">Material</emphasis>
</paragraph>
<paragraph blockId="38.[145,567,1185,1239]" box="[145,567,1217,1239]" pageId="38" pageNumber="103">
<collectingCountry box="[145,267,1217,1239]" name="Seychelles" pageId="38" pageNumber="103">Seychelles</collectingCountry>
: 
<collectingCountry box="[279,346,1217,1239]" name="India" pageId="38" pageNumber="103">Mahé</collectingCountry>
