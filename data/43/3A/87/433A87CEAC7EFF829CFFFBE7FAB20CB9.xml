<subSubSection pageId="5" pageNumber="594" type="biology_ecology">
<paragraph blockId="5.[151,1436,152,752]" box="[199,842,188,213]" pageId="5" pageNumber="594">
<emphasis bold="true" box="[199,331,188,213]" pageId="5" pageNumber="594">Host plant.</emphasis>
<taxonomicName box="[338,633,188,213]" class="Magnoliopsida" family="Asteraceae" genus="Tragopogon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="594" phylum="Tracheophyta" rank="species" species="graminifolius">
<emphasis box="[338,633,188,213]" italics="true" pageId="5" pageNumber="594">Tragopogon graminifolius</emphasis>
</taxonomicName>
DC. (
<taxonomicName box="[700,830,188,212]" class="Magnoliopsida" family="Asteraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="5" pageNumber="594" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
