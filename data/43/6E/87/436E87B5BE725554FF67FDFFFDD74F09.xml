<taxonomicName authority="Cuvier" authorityName="Cuvier" box="[1081,1436,812,837]" class="Actinopterygii" family="Cheilodactylidae" genus="Cheilodactylus" kingdom="Animalia" order="Perciformes" pageId="20" pageNumber="171" phylum="Chordata" rank="species" species="zonatus">
