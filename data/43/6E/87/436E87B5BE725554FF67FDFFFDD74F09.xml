<taxonomicName authority="Cuvier" authorityName="Cuvier" authorityYear="1862" box="[558,872,847,872]" class="Actinopterygii" family="Cheilodactylidae" genus="Goniistius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="20" pageNumber="171" phylum="Chordata" rank="species" species="zonatus">
