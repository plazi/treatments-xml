<paragraph blockId="21.[151,1436,222,275]" pageId="21" pageNumber="172">
<emphasis bold="true" box="[151,271,222,245]" pageId="21" pageNumber="172">Synonyms:</emphasis>
<taxonomicName authority="Caballero" authorityName="Caballero" box="[277,584,222,245]" class="Trematoda" family="Opecoelidae" genus="Emmettrema" kingdom="Animalia" order="Plagiorchiida" pageId="21" pageNumber="172" phylum="Platyhelminthes" rank="species" species="lariosi">
