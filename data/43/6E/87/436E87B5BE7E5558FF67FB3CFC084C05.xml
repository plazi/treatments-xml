<emphasis box="[151,251,1386,1411]" italics="true" pageId="24" pageNumber="175">mahsena</emphasis>
</taxonomicName>
, 
<taxonomicName box="[264,405,1386,1411]" class="Actinopterygii" family="Lethrinidae" genus="Lethrinus" kingdom="Animalia" order="Perciformes" pageId="24" pageNumber="175" phylum="Chordata" rank="species" species="nebulosus">
<emphasis box="[264,279,1388,1411]" italics="true" pageId="24" pageNumber="175">L</emphasis>
