<emphasis bold="true" box="[716,736,1471,1494]" pageId="2" pageNumber="345">KEY</emphasis>
TO THE
</emphasis>
<collectionCode box="[853,934,1471,1494]" pageId="2" pageNumber="345">
<emphasis bold="true" box="[853,873,1471,1494]" pageId="2" pageNumber="345">
<emphasis bold="true" box="[874,934,1477,1493]" pageId="2" pageNumber="345">KNOWN</emphasis>
</emphasis>
</collectionCode>
<emphasis bold="true" box="[957,1058,1477,1493]" pageId="2" pageNumber="345">
<collectionCode box="[942,1024,1471,1494]" pageId="2" pageNumber="345">
<emphasis bold="true" box="[942,957,1471,1494]" pageId="2" pageNumber="345">SPECIES</emphasis>
</collectionCode>
