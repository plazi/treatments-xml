<taxonomicName authorityName="Jacoby" authorityYear="1887" box="[524,615,1265,1288]" class="Insecta" family="Chrysomelidae" genus="Triarius" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="350" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[524,540,1265,1288]" italics="true" pageId="7" pageNumber="350">
<emphasis bold="true" box="[540,615,1270,1286]" italics="true" pageId="7" pageNumber="350">TRIARIUS</emphasis>
