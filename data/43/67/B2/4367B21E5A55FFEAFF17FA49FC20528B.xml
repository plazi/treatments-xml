and 
<taxonomicName authority="Miller, Davis, Luong, Do, Pham, Ziegler, Lee, De Queiroz, Reynolds &amp; Nguyen" authorityName="Miller, Davis, Luong, Do, Pham, Ziegler, Lee, De Queiroz, Reynolds &amp; Nguyen" class="Reptilia" family="Xenodermatidae" genus="Achalinus" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="537" phylum="Chordata" rank="species" species="zugorum">
