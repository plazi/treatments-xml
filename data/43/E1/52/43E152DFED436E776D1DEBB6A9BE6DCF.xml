, 
<taxonomicName class="Insecta" family="Crambidae" genus="Desmia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Desmia" order="Lepidoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">Desmia</taxonomicName>
spp. with interim names Janzen18 and Janzen19; 
<taxonomicName class="Insecta" family="Crambidae" genus="Spoladea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Spoladea recurvalis" order="Lepidoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="recurvalis">Spoladea recurvalis</taxonomicName>
(
