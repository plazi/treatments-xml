<taxonomicName authorityName="Gapud" authorityYear="2003" box="[1304,1404,1223,1247]" class="Insecta" family="Ochteridae" genus="Ochterus" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="48" phylum="Arthropoda" rank="species" species="zetteli">
