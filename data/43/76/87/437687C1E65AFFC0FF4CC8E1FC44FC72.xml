, 
<taxonomicName authorityName="Fain" authorityYear="1956" box="[577,731,792,817]" class="Arachnida" family="Gastronyssidae" genus="Rodhainyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="brutsaerti">O. brutsaerti</taxonomicName>
, 
<taxonomicName authorityName="Fain" authorityYear="1959" box="[744,879,792,817]" class="Arachnida" family="Gastronyssidae" genus="Opsonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="eidoloni">O. eidoloni</taxonomicName>
, 
<taxonomicName authorityName="Bochkov &amp; Zabludovskaya &amp; Oconnor" authorityYear="2008" box="[891,1038,792,817]" class="Arachnida" family="Gastronyssidae" genus="Opsonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="klompeni">O. klompeni</taxonomicName>
, 
<taxonomicName authorityName="Bochkov &amp; Zabludovskaya &amp; Oconnor" authorityYear="2008" box="[1050,1249,792,817]" class="Arachnida" family="Gastronyssidae" genus="Opsonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="macroglossus">O. macroglossus</taxonomicName>
, 
<taxonomicName authorityName="Bochkov &amp; Zabludovskaya &amp; Oconnor" authorityYear="2008" class="Arachnida" family="Gastronyssidae" genus="Opsonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="pseudoindicus">O. pseudoindicus</taxonomicName>
, 
<taxonomicName authorityName="Bochkov &amp; Zabludovskaya &amp; Oconnor" authorityYear="2008" box="[204,353,832,857]" class="Arachnida" family="Gastronyssidae" genus="Opsonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="pteropodi">O. pteropodi</taxonomicName>
</emphasis>
