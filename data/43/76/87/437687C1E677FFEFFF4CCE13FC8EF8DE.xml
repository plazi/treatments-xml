<typeStatus box="[159,211,1492,1518]" pageId="110" pageNumber="111">type</typeStatus>
host) from Europe (
<collectingCountry box="[451,554,1492,1518]" name="Belgium" pageId="110" pageNumber="111">Belgium</collectingCountry>
and 
<collectingCountry box="[611,801,1492,1518]" name="Netherlands" pageId="110" pageNumber="111">the Netherlands</collectingCountry>
), 
<collectingCountry box="[822,916,1492,1518]" name="Ukraine" pageId="110" pageNumber="111">Ukraine</collectingCountry>
(
<bibRefCitation author="Zabludovskaya, S. A." box="[932,1183,1492,1518]" pageId="110" pageNumber="111" pagination="32 - 35" refId="ref78040" refString="Zabludovskaya, S. A. (1990) Mites of the genus Yunkeracarus (Acariformes, Gastronyssidae) - parasites of the nasal cavities of rodents. Vestnik zoologii, 4, 32 - 35. [in Russian]" type="journal article" year="1990">Zabludovskaya 1990</bibRefCitation>
), 
<taxonomicName authority="(Pallas)" baseAuthorityName="Pallas" baseAuthorityYear="1771" box="[1204,1436,1492,1518]" class="Mammalia" family="Muridae" genus="Apodemus" kingdom="Animalia" order="Rodentia" pageId="110" pageNumber="111" phylum="Chordata" rank="species" species="agrarius">
<emphasis box="[1204,1339,1492,1517]" italics="true" pageId="110" pageNumber="111">A. agrarius</emphasis>
(Pallas)
</taxonomicName>
from 
<collectingCountry box="[215,362,1532,1558]" name="South Korea" pageId="110" pageNumber="111">South Korea</collectingCountry>
(
<bibRefCitation author="Fain, A. &amp; Lukoschus, F. S. &amp; Ah, H. S." box="[377,565,1532,1558]" pageId="110" pageNumber="111" pagination="79 - 83" refId="ref76594" refString="Fain, A., Lukoschus, F. S. &amp; Ah, H. S. (1967) Note sur un acarien du genre Yunkeracarus Fain, 1957 (Gastronyssidae: Sarcoptiformes). Acta Zoologica et Pathologica Antverpiensia, 43, 79 - 83." type="journal article" year="1967">
Fain 
<emphasis box="[438,488,1532,1557]" italics="true" pageId="110" pageNumber="111">et al</emphasis>
. 1967
</bibRefCitation>
), and 
<taxonomicName baseAuthorityName="Wroughton" baseAuthorityYear="1908" box="[636,735,1532,1557]" class="Mammalia" family="Muridae" genus="Apodemus" kingdom="Animalia" order="Rodentia" pageId="110" pageNumber="111" phylum="Chordata" rank="species" species="wardi">
<emphasis box="[636,735,1532,1557]" italics="true" pageId="110" pageNumber="111">A. wardi</emphasis>
</taxonomicName>
