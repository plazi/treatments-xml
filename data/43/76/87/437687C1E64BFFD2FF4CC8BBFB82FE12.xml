<typeStatus box="[267,319,432,458]" pageId="83" pageNumber="84">type</typeStatus>
host) from New 
<collectingCountry box="[519,603,432,458]" name="Guinea-Bissau" pageId="83" pageNumber="84">Guinea</collectingCountry>
and 
<taxonomicName baseAuthorityName="Hodgson" baseAuthorityYear="1835" box="[661,791,432,457]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="83" pageNumber="84" phylum="Chordata" rank="species" species="armiger">
