<emphasis bold="true" box="[197,402,1871,1897]" pageId="89" pageNumber="90">Type deposition.</emphasis>
<typeStatus box="[410,519,1872,1898]" pageId="89" pageNumber="90">Holotype</typeStatus>
and 
<typeStatus box="[576,688,1872,1898]" pageId="89" pageNumber="90">paratypes</typeStatus>
in UMMZ, other 
<typeStatus box="[898,1010,1872,1898]" pageId="89" pageNumber="90">paratypes</typeStatus>
