<taxonomicName box="[539,650,552,577]" class="Arachnida" family="Gastronyssidae" genus="Yunkeracarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="102" pageNumber="103" phylum="Arthropoda" rank="species" species="otomys">Y. otomys</taxonomicName>
</emphasis>
, 
<emphasis box="[665,769,552,577]" italics="true" pageId="102" pageNumber="103">
<taxonomicName box="[665,765,552,577]" class="Mammalia" family="Muridae" genus="Rattus" kingdom="Animalia" order="Rodentia" pageId="102" pageNumber="103" phylum="Chordata" rank="species" species="rattus">Y. rattus</taxonomicName>
