<emphasis bold="true" box="[701,881,417,438]" pageId="5" pageNumber="5">
<specimenCode box="[701,881,417,438]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="5" pageNumber="5">AMNH 716684</specimenCode>
