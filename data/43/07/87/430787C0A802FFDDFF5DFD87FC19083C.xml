<emphasis bold="true" pageId="26" pageNumber="26">
<specimenCode collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="26" pageNumber="26">AMNH 41863</specimenCode>
