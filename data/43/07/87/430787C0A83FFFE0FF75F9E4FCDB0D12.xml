<collectionCode box="[701,788,921,942]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="39" pageNumber="39" type="Museum">USNM</collectionCode>
