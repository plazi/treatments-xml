<collectionCode box="[410,503,272,293]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="19" pageNumber="19">AMNH</collectionCode>
