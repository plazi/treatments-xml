<specimenCode box="[219,295,1412,1433]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">271864</specimenCode>
