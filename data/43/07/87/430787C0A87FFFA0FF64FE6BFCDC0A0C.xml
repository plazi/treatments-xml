<collectionCode box="[179,272,1236,1257]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">AMNH</collectionCode>
collection. The 
<specimenCount box="[482,654,1236,1257]" pageId="103" pageNumber="103" type="generic" typeStatus="paratypes">two paratypes</specimenCount>
from 
<quantity box="[192,273,1265,1287]" metricMagnitude="2" metricUnit="m" metricValue="9.906" pageId="103" pageNumber="103" unit="ft" value="3250.0">
<elevation box="[192,273,1265,1287]" metricMagnitude="2" metricUnit="m" metricValue="9.906" pageId="103" pageNumber="103" unit="ft" value="3250.0">3250 ft</elevation>
</quantity>
are 
<emphasis bold="true" box="[325,505,1265,1286]" pageId="103" pageNumber="103">
<specimenCode box="[325,505,1265,1286]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">AMNH 271842</specimenCode>
</emphasis>
and 
<emphasis bold="true" box="[564,646,1265,1286]" pageId="103" pageNumber="103">271843</emphasis>
, the latter of which was exchanged to Colección Phelps in 
<date box="[295,444,1324,1345]" pageId="103" pageNumber="103" value="1940-08">
<collectingDate box="[295,444,1324,1345]" pageId="103" pageNumber="103" value="1940-08">August 1940</collectingDate>
</date>
. The 
<specimenCount box="[507,654,1324,1346]" pageId="103" pageNumber="103" type="generic" typeStatus="paratypes">25 paratypes</specimenCount>
from 
<quantity box="[205,298,1353,1375]" metricMagnitude="3" metricUnit="m" metricValue="1.43256" pageId="103" pageNumber="103" unit="ft" value="4700.0">
<elevation box="[205,298,1353,1375]" metricMagnitude="3" metricUnit="m" metricValue="1.43256" pageId="103" pageNumber="103" unit="ft" value="4700.0">4700 ft</elevation>
</quantity>
are: 
<emphasis bold="true" pageId="103" pageNumber="103">
<specimenCode box="[383,649,1353,1374]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">AMNH 271813–271815</specimenCode>
, 
<specimenCode box="[128,289,1383,1404]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">271817–271821</specimenCode>
, 
<specimenCode box="[301,377,1383,1404]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">271845</specimenCode>
, 
<specimenCode box="[390,466,1383,1404]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">271848</specimenCode>
, 
<specimenCode box="[478,554,1383,1404]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">271852</specimenCode>
