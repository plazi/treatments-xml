<materialsCitation collectingDate="1930-06" collectingDateMax="1985-04" collectingDateMin="1930-06" collectionCode="AMNH, ANSP, BMNH, USNM" elevation="168" pageId="103" pageNumber="103" specimenCode="AMNH 271849, AMNH 271842, AMNH 271813-271815, 271817-271821, 271845, 271848, 271852, 271854- 271857, 271864, 271873-271875, 271883-271886, 271889, AMNH 271814, AMNH 271875, AMNH 271834, 271836-271840, 271847, 271859, 271862, 271863, 271865, 271867-271869, 271872, 271876, 271878, AMNH 271859, AMNH 271862, AMNH 271872, AMNH 271881" specimenCount="93" typeStatus="holotype">
