<specimenCode box="[128,201,1442,1463]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">271889</specimenCode>
