<emphasis bold="true" box="[128,307,1117,1138]" pageId="103" pageNumber="103">
<specimenCode box="[128,307,1117,1138]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="103" pageNumber="103">AMNH 271849</specimenCode>
</emphasis>
