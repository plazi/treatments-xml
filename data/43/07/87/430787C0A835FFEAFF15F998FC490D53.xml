<collectorName box="[701,770,665,687]" pageId="45" pageNumber="45">Miller</collectorName>
and 
<collectorName box="[859,1030,665,687]" pageId="45" pageNumber="45">Griscom. The</collectorName>
<typeStatus box="[1054,1162,665,686]" pageId="45" pageNumber="45">paratypes</typeStatus>
