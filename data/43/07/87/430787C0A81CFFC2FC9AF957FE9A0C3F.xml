<collectingDate pageId="5" pageNumber="5" value="1900-01">January 1900</collectingDate>
</date>
by Schlüter
</materialsCitation>
; 
<materialsCitation collectingDate="1901-02" collectingDateMax="1901-03-31" collectingDateMin="1901-02" collectionCode="AMNH" location="Issik-Kul" municipality="Turkestan" pageId="5" pageNumber="5" specimenCode="AMNH 716643-716645" specimenCount="1" typeStatus="paratype">
