<taxonomicName baseAuthorityName="Woodhouse" baseAuthorityYear="1853" class="Aves" family="Emberizidae" genus="Junco" kingdom="Animalia" order="Passeriformes" pageId="16" pageNumber="16" phylum="Chordata" rank="subSpecies" species="hyemalis" subSpecies="caniceps">
