<specimenCount box="[830,974,1187,1209]" pageId="71" pageNumber="71" type="female">two females</specimenCount>
and 
<specimenCount box="[1043,1167,1187,1209]" pageId="71" pageNumber="71" type="male">five males</specimenCount>
