</materialsCitation>
; 
<materialsCitation collectingDate="1925-02" collectingDateMax="1925-04-31" collectingDateMin="1925-02" collectionCode="AMNH" collectorName="La Lejia &amp; Watkins" location="The" pageId="63" pageNumber="63" specimenCode="AMNH 235355-235358" specimenCount="2" specimenCount-female="1" specimenCount-male="1" typeStatus="paratype">
