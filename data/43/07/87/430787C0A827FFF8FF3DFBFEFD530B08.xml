<collectionCode box="[361,454,1471,1492]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="63" pageNumber="63">AMNH</collectionCode>
