<paragraph blockId="80.[667,1193,1291,1445]" pageId="80" pageNumber="80">
<treatmentCitationGroup pageId="80" pageNumber="80">
Now 
<taxonomicName authorityName="Lafresnaye" authorityYear="1837" box="[724,970,1345,1365]" class="Aves" family="Thraupidae" genus="Ramphocelus" kingdom="Animalia" order="Passeriformes" pageId="80" pageNumber="80" phylum="Chordata" rank="species" species="dimidiatus">
<emphasis box="[724,970,1345,1365]" italics="true" pageId="80" pageNumber="80">Ramphocelus dimidiatus</emphasis>
</taxonomicName>
