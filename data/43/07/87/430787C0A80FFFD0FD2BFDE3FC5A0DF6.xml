<emphasis bold="true" box="[896,1087,1149,1170]" pageId="23" pageNumber="23">
<specimenCode box="[896,1087,1149,1170]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="23" pageNumber="23">AMNH 175858</specimenCode>
</emphasis>
