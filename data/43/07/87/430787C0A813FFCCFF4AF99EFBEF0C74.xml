<collectionCode box="[1025,1121,782,803]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="11" pageNumber="11">AMNH</collectionCode>
