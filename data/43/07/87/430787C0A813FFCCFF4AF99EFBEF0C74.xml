<location LSID="urn:lsid:plazi:treatment:430787C0A813FFCCFF4AF99EFBEF0C74:CE71600DA813FFCCFD70FD4CFCC20BB5" box="[741,895,665,687]" county="In" municipality="McGregor" name="Sanak Island" pageId="11" pageNumber="11" stateProvince="Alaska">Sanak Island</location>
