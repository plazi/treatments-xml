<location LSID="urn:lsid:plazi:treatment:430787C0A813FFCCFF4AF99EFBEF0C74:CE71600DA813FFCCFD28FCFEFCD20A5B" box="[701,879,811,833]" county="In" municipality="McGregor" name="Sanaka Island" pageId="11" pageNumber="11" stateProvince="Alaska">Sanaka Island</location>
