<location LSID="urn:lsid:plazi:treatment:430787C0A813FFCCFF4AF99EFBEF0C74:CE71600DA813FFCCFD28FCDBFCBC0A3E" box="[701,769,782,804]" county="In" municipality="McGregor" name="There" pageId="11" pageNumber="11" stateProvince="Alaska">There</location>
