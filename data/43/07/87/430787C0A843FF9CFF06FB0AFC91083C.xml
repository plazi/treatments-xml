<collectionCode box="[560,653,1588,1609]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="91" pageNumber="91">AMNH</collectionCode>
