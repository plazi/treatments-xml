<collectionCode box="[864,957,1531,1552]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="101" pageNumber="101">AMNH</collectionCode>
