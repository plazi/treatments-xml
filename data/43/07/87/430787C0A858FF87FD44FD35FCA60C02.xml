<collectionCode box="[1004,1095,1224,1245]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="64" pageNumber="64" type="Museum">FMNH</collectionCode>
