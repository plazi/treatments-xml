and 
<taxonomicName authorityName="Maximiliani. Hellmayr" authorityYear="1938" box="[814,947,359,382]" class="Aves" family="Thraupidae" genus="Oryzoborus" kingdom="Animalia" order="Passeriformes" pageId="28" pageNumber="28" phylum="Chordata" rank="genus">
<emphasis box="[814,947,359,382]" italics="true" pageId="28" pageNumber="28">Oryzoborus</emphasis>
