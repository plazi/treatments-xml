<paragraph blockId="52.[93,619,844,1050]" pageId="52" pageNumber="52">
<treatmentCitationGroup pageId="52" pageNumber="52">
Now 
<taxonomicName authority="(Lawrence, 1874)" baseAuthorityName="Lawrence" baseAuthorityYear="1874" class="Aves" family="Emberizidae" genus="Arremon" kingdom="Animalia" order="Passeriformes" pageId="52" pageNumber="52" phylum="Chordata" rank="subSpecies" species="torquatus" subSpecies="atricapillus">
<emphasis box="[160,488,897,917]" italics="true" pageId="52" pageNumber="52">Arremon torquatus atricapillus</emphasis>
