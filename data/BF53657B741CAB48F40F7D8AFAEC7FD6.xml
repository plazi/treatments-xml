<subSubSection id="7FE087E6741CAB48F6287A8EFAEB7DB6" pageId="1" pageNumber="26" type="materials_examined">
<paragraph id="3745D46D741CAB48F6287A8EFAEB7DB6" blockId="1.[813,1452,193,248]" pageId="1" pageNumber="26">
<typeStatus id="E8416ACF741CAB48F6287A8EFC967D96" box="[813,873,193,216]" pageId="1" pageNumber="26">Type</typeStatus>
species. – 
<taxonomicName id="F0FAAFEE741CAB48F6F87A8EFBFE7DB6" authority="(O. P. Cambridge, 1874)" baseAuthorityName="O. P. Cambridge" baseAuthorityYear="1874" class="Arachnida" family="Ctenizidae" genus="Bothriocyrtum" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="26" phylum="Arthropoda" rank="species" species="californicum">
<emphasis id="058E087F741CAB48F6F87A8EFAAC7D96" box="[1021,1363,193,216]" italics="true" pageId="1" pageNumber="26">Bothriocyrtum californicum</emphasis>
