<taxonomicName id="4C6B0081FFB1D26DFDFCFA20FC47FA6C" box="[629,902,1512,1535]" class="Insecta" family="Buprestidae" genus="Anthaxia" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="ulmi" status="n. sp." subGenus="Anthaxia">
