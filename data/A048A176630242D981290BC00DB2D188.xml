<taxonomicName id="141F4E6149196CAAF364DAF8AA908A9A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:212640" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Tetramorium sanetrai Schulz &amp; Csösz" order="Hymenoptera" pageNumber="31" phylum="Arthropoda" rank="species" species="sanetrai">sanetrai</taxonomicName>
