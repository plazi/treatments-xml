<geoCoordinate id="CB00D71030C8EFA366D137AFE4D1D2D8" degrees="26" direction="north" minutes="21.36" orientation="latitude" precision="9" value="26.356">26°21.36'N</geoCoordinate>
, 
<geoCoordinate id="C838B7B109C369EFCDC54B13A51C60F8" degrees="108" direction="east" minutes="01.82" orientation="longitude" precision="9" value="108.030334">108°01.82'E</geoCoordinate>
