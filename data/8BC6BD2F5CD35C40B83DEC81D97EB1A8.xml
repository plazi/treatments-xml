<taxonomicName id="942868793F97EF74E0D3975F0B0D363B" class="Insecta" family="Cerambycidae" genus="Amamiclytus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amamiclytus" order="Coleoptera" pageId="0" pageNumber="57" phylum="Arthropoda" rank="genus">Amamiclytus</taxonomicName>
