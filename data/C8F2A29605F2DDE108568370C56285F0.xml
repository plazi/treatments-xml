<taxonomicName id="E8F33B2F04E6C6BF1791E0F8CAAD8FF5" class="Polychaeta" family="Polynoidae" genus="Malmgrenia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Malmgrenia lunulata" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="lunulata">Malmgrenia lunulata</taxonomicName>
