<collectingRegion id="953BF1BBFF88FF86FC64CFE06D6CFC61" box="[912,1116,954,978]" country="Brazil" name="Rio Grande do Sul" pageId="10" pageNumber="193">Rio Grande do Sul</collectingRegion>
