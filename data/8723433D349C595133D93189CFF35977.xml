</subSubSection>
<subSubSection id="72467F6292B007FA5B743C1DD46980CF" type="reference_group">
<paragraph id="D6DC7FD326AA54E920418B1AACA5DFEB" pageId="34" pageNumber="35">
<taxonomicName id="E6DE4BB2BAE1BDD4637BF42C61CB1783" class="Gastropoda" family="Nassariidae" genus="Tritia" higherTaxonomySource="CoL" kingdom="Animalia" order="Neogastropoda" pageId="34" pageNumber="35" phylum="Mollusca" rank="species" species="nuda">Tritia nuda Berlese</taxonomicName>
