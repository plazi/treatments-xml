<treatmentCitationGroup id="AB7011A1C406DB7BFF67F91959B6F939" box="[151,1159,1690,1712]" pageId="7" pageNumber="204">
<taxonomicName id="4C604D0CC406DB7BFF67F9195F2FF939" authority="Walker, 1848: 145" authorityName="Walker" authorityPageNumber="145" authorityYear="1848" box="[151,542,1690,1712]" class="Insecta" family="Eulophidae" genus="Diglyphus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="204" phylum="Arthropoda" rank="species" species="poppoea">
