<emphasis id="3DE4EA8C2A0CFFF5FD1AF8D8AE9FF886" box="[721,983,1895,1919]" italics="true" pageId="2" pageNumber="589">Zuluclavodes mulanjensis</emphasis>
</taxonomicName>
<emphasis id="3DE4EA8C2A0CFFF5FC17F8D7A964F879" bold="true" box="[988,1068,1896,1920]" pageId="2" pageNumber="589">
<taxonomicNameLabel id="26D757F72A0CFFF5FC17F8D7A964F879" box="[988,1068,1896,1920]" pageId="2" pageNumber="589" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, male holotype; 2, 
<taxonomicName id="C8904D1D2A0CFFF5FB27F8D8A83BF886" authorityName="Hlavac" authorityYear="2007" box="[1260,1395,1895,1919]" class="Insecta" family="Staphylinidae" genus="Zuluclavodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="589" phylum="Arthropoda" rank="genus">
