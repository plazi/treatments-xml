<paragraph id="8B8D36AAFFA88672FCCCF9E3580EF9CC" blockId="4.[831,1436,1546,1635]" pageId="4" pageNumber="49">
<taxonomicName id="4C324D29FFA88672FCCCF9E35FADF98E" box="[831,979,1546,1569]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="leloi">
