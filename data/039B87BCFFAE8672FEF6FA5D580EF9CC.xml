<taxonomicName id="4C324D29FFA88672FC89F9C55860F9EE" box="[890,1054,1578,1603]" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="quadricuspis">
