<collectionCode id="ED514697FFD0FFA9FEA5FE7C2C38FDA2" box="[290,362,509,533]" country="USA" lsid="urn:lsid:biocol.org:col:15761" name="Western Kentucky University" pageId="1" pageNumber="196" type="Herbarium">WKU</collectionCode>
)
