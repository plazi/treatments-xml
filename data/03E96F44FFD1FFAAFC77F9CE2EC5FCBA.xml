<taxonomicName id="4C40A5D1FFD3FFAAFABCFD31296CFD5D" authority="(Needham &amp; Smith)" baseAuthorityName="Needham &amp; Smith" class="Insecta" family="Peltoperlidae" genus="Tallaperla" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="197" phylum="Arthropoda" rank="species" species="maria">
