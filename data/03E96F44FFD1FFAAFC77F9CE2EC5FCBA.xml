</taxonomicName>
, 
<taxonomicName id="4C40A5D1FFD3FFAAFB7AFD0A2EF1FD70" authority="(Ricker)" baseAuthorityName="Ricker" baseAuthorityYear="1936" class="Insecta" family="Chloroperlidae" genus="Sweltsa" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="197" phylum="Arthropoda" rank="species" species="onkos">
