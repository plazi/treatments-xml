, 
<taxonomicName id="4C40A5D1FFD3FFAAFC3DFD2E29BAFD70" authority="(Ricker)" baseAuthorityName="Ricker" baseAuthorityYear="1952" box="[954,1256,687,711]" class="Insecta" family="Peltoperlidae" genus="Tallaperla" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="197" phylum="Arthropoda" rank="species" species="laurie">
