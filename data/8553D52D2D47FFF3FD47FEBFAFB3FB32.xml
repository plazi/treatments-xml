; thickness, 
<quantity id="CA02C9DE2D47FFF2FCC9F942AD9DF932" box="[888,978,1673,1700]" metricMagnitude="-3" metricUnit="m" metricValue="8.1" pageId="28" pageNumber="75" unit="mm" value="8.1">8.1 mm</quantity>
