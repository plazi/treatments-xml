<quantity id="CA02C9DE2D47FFF2FD40FD6EAD17FD56" box="[753,856,677,704]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" pageId="28" pageNumber="75" unit="mm" value="10.5">10.5 mm</quantity>
