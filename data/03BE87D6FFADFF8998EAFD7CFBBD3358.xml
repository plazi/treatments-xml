<quantity id="4CEF9B25FFADFF89998CFCECF824333C" box="[497,613,798,825]" metricMagnitude="2" metricUnit="m" metricValue="2.94" metricValueMax="5.13" metricValueMin="0.75" pageId="60" pageNumber="61" unit="m" value="294.0" valueMax="513.0" valueMin="75.0">75–513 m</quantity>
