<paragraph id="8BA00D4E9942B15B61C29D5BFE76921A" blockId="1.[151,509,866,894]" box="[151,509,866,894]" pageId="1" pageNumber="318">
<emphasis id="B96BD15C9942B15B61C29D5BFEC49219" bold="true" box="[151,335,867,894]" pageId="1" pageNumber="318">Key to females</emphasis>
<emphasis id="B96BD15C9942B15B601A9D5DFED39214" bold="true" box="[335,344,866,880]" pageId="1" pageNumber="318">
<superScript id="7C6AA0069942B15B601A9D5DFED39214" attach="left" box="[335,344,866,880]" fontSize="6" pageId="1" pageNumber="318">1</superScript>
</emphasis>
<emphasis id="B96BD15C9942B15B600A9D5CFE76921A" bold="true" box="[351,509,867,894]" pageId="1" pageNumber="318">
of 
