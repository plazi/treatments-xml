<quantity id="4CBF05D4C502140FFBF4FDDDFB5B1C1D" box="[1070,1229,541,565]" metricMagnitude="-3" metricUnit="m" metricValue="3.13" metricValueMax="3.15" metricValueMin="3.11" pageId="6" pageNumber="7" unit="mm" value="3.13" valueMax="3.15" valueMin="3.11">3.11–3.15 mm</quantity>
, D = 
<quantity id="4CBF05D4C502140FFAD7FDDDFA381C1D" box="[1293,1454,541,565]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="1.78" metricValueMin="1.72" pageId="6" pageNumber="7" unit="mm" value="1.75" valueMax="1.78" valueMin="1.72">1.72–1.78 mm</quantity>
