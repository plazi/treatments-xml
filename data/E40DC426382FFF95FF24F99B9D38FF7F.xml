<number id="DA6C6717382FFF92FB0EF9649894F945" box="[1213,1273,1717,1741]" pageId="11" pageNumber="312" value="2012.0">2012</number>
), 
<collectingCountry id="14B335A0382FFF92FABCF96499F5F945" box="[1295,1432,1717,1741]" name="Madagascar" pageId="11" pageNumber="312">Madagascar</collectingCountry>
