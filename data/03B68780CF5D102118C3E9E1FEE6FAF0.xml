<taxonomicName id="4C1F4D15CF5D102118C3E9E1FD06FD51" authority="Rochebrune, 1884" authorityName="Rochebrune" authorityYear="1884" box="[151,632,728,754]" class="Reptilia" family="Pelomedusidae" genus="Pelomedusa" kingdom="Animalia" order="Testudines" pageId="13" pageNumber="514" phylum="Chordata" rank="species" species="gasconi">
