<taxonomicName id="A39973FFFF8AFFC7571C897EFC0AF9D3" box="[769,939,1577,1603]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="28" pageNumber="29" phylum="Tracheophyta" rank="species" species="tschonoskii">
