<subSubSection id="7D8B40F6FFCBAC5CFF77FA96F831856A" box="[199,1407,1338,1363]" pageId="6" pageNumber="407" type="etymology">
<paragraph id="352E137DFFCBAC5CFF77FA96F831856A" blockId="6.[151,1437,474,1363]" box="[199,1407,1338,1363]" pageId="6" pageNumber="407">
<emphasis id="07E5CF6FFFCBAC5CFF77FA96FC00856A" bold="true" box="[199,334,1338,1363]" pageId="6" pageNumber="407">Etymology.</emphasis>
