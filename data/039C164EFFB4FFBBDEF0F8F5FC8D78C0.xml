<subSubSection id="C32FF4D3FFB4FFB8DEF0F8F5FBEB7FE5" box="[706,1137,1506,1529]" pageId="53" pageNumber="54" type="nomenclature">
<paragraph id="8B8AA758FFB4FFB8DEF0F8F5FBEB7FE5" blockId="53.[706,1137,1506,1529]" box="[706,1137,1506,1529]" pageId="53" pageNumber="54">
<heading id="D0C21034FFB4FFB8DEF0F8F5FBEB7FE5" box="[706,1137,1506,1529]" centered="true" fontSize="9" level="2" pageId="53" pageNumber="54" reason="2">
<taxonomicName id="4C35DCDBFFB4FFB8DEF0F8F5FC407FE4" box="[706,986,1506,1529]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="samiatus" status="new species">
<emphasis id="B9417B4AFFB4FFB8DEF0F8F5FC407FE4" bold="true" box="[706,986,1506,1529]" italics="true" pageId="53" pageNumber="54">Paramblynotus samiatus</emphasis>
</taxonomicName>
