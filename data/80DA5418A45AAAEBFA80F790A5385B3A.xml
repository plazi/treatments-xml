<subSubSection id="49FC76874CD4BFA5B27F0FB8B9B0A0FF" pageId="28" pageNumber="67" type="reference_group">
<paragraph id="56187E03CF0C5B6712A6E261AB3DA3C1" pageId="28" pageNumber="67">
<taxonomicName id="1F68B3B0A04270D278FC5224CBDE65FF" class="Malacostraca" family="Cymothoidae" genus="Ceratothoa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ceratothoa triglae" order="Isopoda" pageId="28" pageNumber="67" phylum="Arthropoda" rank="species" species="triglae">Ceratothoa triglae</taxonomicName>
