<geoCoordinate id="EE7E501DFFC9AB447FFFF9C1FDE9ED6C" box="[448,519,1532,1564]" degrees="1" direction="north" minutes="1.3" orientation="latitude" pageId="32" pageNumber="33" precision="92" value="1.0216666">
<subScript id="17CE349FFFC9AB447FFFF9C1FE39ED66" attach="left" box="[448,471,1550,1564]" fontSize="6" pageId="32" pageNumber="33">n-1</subScript>
1.3
</geoCoordinate>
