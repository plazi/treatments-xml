, 
<taxonomicName id="C3804D4D1E3026723B6CFA1FEDFA7A57" box="[778,975,1535,1558]" class="Aves" family="Passeridae" genus="Passer" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="88" phylum="Chordata" rank="species" species="montanus">
