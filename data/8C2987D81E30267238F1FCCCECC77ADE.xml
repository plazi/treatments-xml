<number id="B24824E91E3026723943F9C3EF6B7A7A" box="[293,350,1571,1595]" pageId="11" pageNumber="88" value="2011.0">2011</number>
</bibRefCitation>
, as 
<taxonomicName id="C3804D4D1E30267239EFF9C2EC4D7A7A" box="[393,632,1570,1595]" class="Arachnida" family="Macronyssidae" genus="Pellonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="11" pageNumber="88" phylum="Arthropoda" rank="species" species="reedi">
