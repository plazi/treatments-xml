<taxonomicName id="C3804D4D1E3026723C80FA1EEB5F7A56" box="[1254,1386,1534,1559]" class="Arachnida" family="Macronyssidae" genus="Pellonyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="11" pageNumber="88" phylum="Arthropoda" rank="genus">
<emphasis id="36F4EADC1E3026723C80FA1EEB5F7A56" box="[1254,1386,1534,1559]" italics="true" pageId="11" pageNumber="88">Pellonyssus</emphasis>
