<paragraph id="6C1F6438100D136BB3995DD39FBA29F8" pageId="6" pageNumber="109">
<taxonomicName id="5960F7D58ACD7B98BD0AAC701C7455DB" class="Insecta" family="Noctuidae" genus="Mamestra" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mamestra acutipennis" order="Lepidoptera" pageId="6" pageNumber="109" phylum="Arthropoda" rank="species" species="acutipennis">Mamestra acutipennis</taxonomicName>
