<pageBreakToken id="05A333DFB0992C7E1B8C080E29E19F98" pageId="0" pageNumber="164" start="start">5</pageBreakToken>
. 
<taxonomicName id="BFC129CEA1A15D68E9C8F3504789F979" authority="Verh." authorityName="Verh." class="Chilopoda" family="Lithobiidae" genus="Lithobius" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="164" phylum="Arthropoda" rank="subSpecies" species="leptopus" subSpecies="Broelemanni">
leptopus 
<normalizedToken id="7D6E36C90F699757B752D7B9A7FBE355" originalValue="Brölemanni" pageId="0" pageNumber="164">Broelemanni</normalizedToken>
<authority id="7D031E12BAA4FA62345A3AEB866F5828" pageId="0" pageNumber="164">Verh.</authority>
</taxonomicName>
</paragraph>
