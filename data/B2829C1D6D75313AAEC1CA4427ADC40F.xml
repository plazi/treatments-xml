<taxonomicName id="C7B474990A45A63641D60C84077F810A" authorityName="Ferrari" authorityYear="1867" class="Insecta" family="Lamiaceae" genus="Anisandrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="family">Lamiaceae</taxonomicName>
