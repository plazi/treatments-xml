<emphasis id="BF92B674FEA2FEBFB49C1E6B7A7523C3" bold="true" box="[498,839,1142,1172]" pageId="357" pageNumber="358">
<taxonomicName id="4AE611E5FEA2FEBFB49C1E6B7B6623C3" baseAuthorityName="Forel" baseAuthorityYear="1899" box="[498,596,1142,1172]" class="Insecta" family="Formicidae" genus="Temnothorax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="357" pageNumber="358" phylum="Arthropoda" rank="species" species="salvini">
<emphasis id="BF92B674FEA2FEBFB49C1E6B7B6623C3" bold="true" box="[498,596,1142,1172]" italics="true" pageId="357" pageNumber="358">salvini</emphasis>
</taxonomicName>
group overview
