<materialsCitation id="3B25524FCE66EC4C7EEEFF21C5EDC8EC" box="[151,748,186,212]" country="Italy" location="Pisa" municipality="San Giuliano" pageId="9" pageNumber="10" specimenCount="1" typeStatus="holotype">
<typeStatus id="54F6E6B0CE66EC4C7EEEFF21C7D0C8EC" box="[151,209,186,212]" pageId="9" pageNumber="10">Type</typeStatus>
locality: 
<collectingCountry id="F35A1882CE66EC4C7F39FF21C676C8EC" box="[320,375,186,212]" name="Italy" pageId="9" pageNumber="10">Italy</collectingCountry>
, province of 
<location id="8E920EC9CE66EC4C7C76FF21C543C8EC" LSID="urn:lsid:plazi:treatment:03E4E904CE66EC4C7EEEFF0CC442C9DB:8E920EC9CE66EC4C7C76FF21C543C8EC" box="[527,578,186,212]" country="Italy" municipality="San Giuliano" name="Pisa" pageId="9" pageNumber="10">Pisa</location>
