<taxonomicName id="E8BF4D3CE449FFB484DA1F072CFBF992" authorityName="Tahami &amp; Muilwijk &amp; Lohaj &amp; Sadeghi" authorityYear="2017" box="[205,424,1597,1619]" class="Insecta" family="Carabidae" genus="Laemostenus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="123" phylum="Arthropoda" rank="species" species="farsicus">
