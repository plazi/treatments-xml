<taxonomicName id="EFD8FD118ED09300722B1CDB199EAB57" class="Hexapoda" family="Carabidae" genus="Sarothrocrepis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="788" pageNumber="789" phylum="Arthropoda" rank="genus">
