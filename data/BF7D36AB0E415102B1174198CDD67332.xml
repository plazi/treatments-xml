<taxonomicName id="BC5B80D5B7CD71659D012D69B92A6A69" class="Hexapoda" family="Carabidae" genus="Sarothrocrepis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="788" pageNumber="789" phylum="Arthropoda" rank="genus">
