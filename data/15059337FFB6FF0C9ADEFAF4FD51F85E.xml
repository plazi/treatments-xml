<quantity id="5A548FC4FFB6FF0C9EA0F8A6FA62F8E0" box="[1257,1431,1888,1914]" metricMagnitude="3" metricUnit="m" metricValue="1.298" metricValueMax="1.721" metricValueMin="0.875" pageId="15" pageNumber="40" unit="m" value="1298.0" valueMax="1721.0" valueMin="875.0">875 to 1721 m</quantity>
