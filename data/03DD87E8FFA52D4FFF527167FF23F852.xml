<taxonomicName id="4C744D7DFFA52D4CFF2B7167FDEDF8C7" authority="Kudon, 1982" authorityName="Kudon" authorityYear="1982" box="[238,548,1838,1864]" class="Arachnida" family="Proctophyllodidae" genus="Platyacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
<emphasis id="B900EAECFFA52D4CFF2B7167FEB4F8C7" bold="true" box="[238,381,1838,1864]" italics="true" pageId="5" pageNumber="6">Platyacarus</emphasis>
