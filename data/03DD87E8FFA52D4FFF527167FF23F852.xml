Type species: 
<taxonomicName id="4C744D7DFFA52D4CFEE3713CFD66F804" authority="Kudon, 1982" authorityName="Kudon" authorityYear="1982" box="[294,687,1909,1931]" class="Arachnida" family="Proctophyllodidae" genus="Platyacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="oligolaccius">
