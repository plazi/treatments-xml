<paragraph id="47443BC1237F508E535B9CAC4407D364" pageId="5" pageNumber="48">Host plant.</paragraph>
<paragraph id="58F9ACCF042BD10C82F363DB450B24B2" pageId="5" pageNumber="48">
<taxonomicName id="BB4F642DD68B6A6397B89FE8E987BDDF" genus="Euphorbiaceae" lsidName="Euphorbiaceae" pageId="5" pageNumber="48" rank="genus">Euphorbiaceae</taxonomicName>
