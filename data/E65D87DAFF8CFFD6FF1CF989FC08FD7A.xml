<taxonomicName id="A9F44D4FFF8CFFD5FF74F9DBFE2EF993" box="[255,403,1620,1643]" class="Jungermanniopsida" family="Lophoziaceae" kingdom="Plantae" order="Jungermanniales" pageId="1" pageNumber="48" phylum="Bryophyta" rank="family">Lophoziaceae</taxonomicName>
