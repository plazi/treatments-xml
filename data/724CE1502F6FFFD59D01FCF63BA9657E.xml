, Quinghai Prov., Bei Shan National Park, 
<quantity id="3D1DFDA32F6FFFD598ADFC053D9760AE" box="[1188,1276,959,985]" metricMagnitude="5" metricUnit="m" metricValue="1.2" pageId="8" pageNumber="285" unit="km" value="120.0">120 km</quantity>
NE Xining, 
<quantity id="3D1DFDA32F6FFFD59D64FC5D3B7E6776" box="[365,533,999,1025]" metricMagnitude="3" metricUnit="m" metricValue="2.5" metricValueMax="2.7" metricValueMin="2.3" pageId="8" pageNumber="285" unit="m" value="2500.0" valueMax="2700.0" valueMin="2300.0">2300–2700 m</quantity>
, 
<date id="8E5B76862F6FFFD59E2FFC5D3B806776" box="[550,747,999,1025]" pageId="8" pageNumber="285" value="1996-05-23" valueMax="1996-06-08" valueMin="1996-05-23">
<number id="4C2D42612F6FFFD59E2FFC5D3B2E6776" box="[550,581,999,1025]" pageId="8" pageNumber="285" value="23.0">23</number>
.V.– 
<number id="4C2D42612F6FFFD59E7CFC5D3BE96776" box="[629,642,999,1025]" pageId="8" pageNumber="285" value="8.0">8</number>
