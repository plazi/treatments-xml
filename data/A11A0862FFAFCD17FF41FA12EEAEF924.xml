<taxonomicName id="EEB3C2F7FFAFCD17FF38FA12EEADFA33" authority="Wang, Xu &amp; Li, 2008" authorityName="Wang, Xu &amp; Li" authorityYear="2008" box="[238,673,1416,1442]" class="Arachnida" family="Agelenidae" genus="Notiocoelotes" kingdom="Animalia" order="Araneae" pageId="80" pageNumber="81" phylum="Arthropoda" rank="genus">
