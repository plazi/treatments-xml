, on 
<taxonomicName id="4C32D3EBFFFAF561E208F9CEFB791264" baseAuthorityName="Fraser" baseAuthorityYear="1944" box="[1126,1248,1549,1573]" class="Phaeophyceae" family="Sargassaceae" genus="Sargassum" kingdom="Chromista" order="Fucales" pageId="37" pageNumber="38" phylum="Ochrophyta" rank="genus">
