<paragraph id="BB542FC4686577A30D168A058A7BE0E1" pageId="1" pageNumber="110">
<taxonomicName id="00D61F253FF9EE83D943275BD04F06B3" class="Insecta" family="Tineidae" genus="Thisizima" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Thisizima" order="Lepidoptera" pageId="1" pageNumber="110" phylum="Arthropoda" rank="genus">Thisizima</taxonomicName>
