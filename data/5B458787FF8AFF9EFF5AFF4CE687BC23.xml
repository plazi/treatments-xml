<quantity id="14149B74FF8DFF9EFCA3FF04E6F2BDC8" box="[878,1021,223,248]" metricMagnitude="3" metricUnit="m" metricValue="1.4709999999999999" metricValueMax="2.027" metricValueMin="0.915" pageId="12" pageNumber="413" unit="m" value="1471.0" valueMax="2027.0" valueMin="915.0">915–2027 m</quantity>
