= 
<treatmentCitation id="0A8C10CFEC492000ACCC34164B36F9FC" author="O. Berg, Fl. Bras." box="[157,781,1580,1602]" issue="1" journal="Bras." page="334" pageId="0" pageNumber="297" score="0.013475891861195003" volume="14" year="1857">
