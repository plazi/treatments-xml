<taxonomicName id="E2D54D0E1C04FFA75E81FBDAFAFFFB46" authority="Radoszkowski 1887" authorityName="Radoszkowski" authorityYear="1887" box="[718,1365,1131,1158]" class="Insecta" family="Eumenidae" genus="Polistes" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="47" phylum="Arthropoda" rank="species" species="jokahamae" subGenus="Gyrostoma">
