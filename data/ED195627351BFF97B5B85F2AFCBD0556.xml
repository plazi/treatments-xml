<taxonomicName id="A2B09CB2351AFF90B626581DFBDD0506" authority="Caruso and Lombardo, 1977" authorityName="Caruso and Lombardo" authorityYear="1977" box="[569,1055,1377,1402]" class="Malacostraca" family="Spelaeoniscidae" genus="Spelaeoniscus" kingdom="Animalia" order="Isopoda" pageId="3" pageNumber="1927" phylum="Arthropoda" rank="species" species="ragonesei">
