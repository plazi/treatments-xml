<quantity id="DF569983FF8A2A0BFEA65F8B88A0FC56" box="[310,465,974,998]" metricMagnitude="3" metricUnit="m" metricValue="3.62" metricValueMax="3.87" metricValueMin="3.37" pageId="47" pageNumber="48" unit="m" value="3620.0" valueMax="3870.0" valueMin="3370.0">3370–3870 m</quantity>
