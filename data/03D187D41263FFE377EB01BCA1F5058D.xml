<elevation id="0055D1F11263FFE37573009EA11902E1" box="[527,614,443,471]" metricMagnitude="3" metricUnit="m" metricValue="1.955" pageId="68" pageNumber="69" unit="m" value="1955.0">1955 m</elevation>
</quantity>
, native forest, 
<date id="FFC610021263FFE374770099A0B402E0" box="[779,971,443,470]" pageId="68" pageNumber="69" value="2015-10-29">
<collectingDate id="EF82E9EA1263FFE374770099A0B402E0" box="[779,971,443,470]" pageId="68" pageNumber="69" value="2015-10-29">29 October 2015</collectingDate>
</date>
, Y. Zhong &amp; 
<collectorName id="268D53141263FFE373230098A7D402E0" box="[1119,1195,444,471]" pageId="68" pageNumber="69">Y. Zhu</collectorName>
leg.
