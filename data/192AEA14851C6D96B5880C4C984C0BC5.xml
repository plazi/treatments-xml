<taxonomicName id="EB5D97E88723CEAA7412A3EA0C1C905D" class="Actinopterygii" family="Melanonidae" genus="Melanonus" kingdom="Animalia" order="Gadiformes" pageId="2" pageNumber="19" phylum="Chordata" rank="genus">Melanonus</taxonomicName>
