<taxonomicName id="4C2C4D04A436F35623C4A71FFC72F888" authority="Simon, 1895" authorityName="Simon" authorityYear="1895" box="[710,960,1813,1840]" class="Arachnida" family="Araneidae" genus="Eustala" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
