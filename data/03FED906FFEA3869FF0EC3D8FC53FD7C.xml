, 
<collectingRegion id="4993A6F2FFEB3869FB07C043FB3EFDA0" box="[1182,1274,583,607]" country="United States of America" name="Georgia" pageId="9" pageNumber="10">Georgia</collectingRegion>
, 
<collectingRegion id="4993A6F2FFEB3869FA9EC04CFA9FFDA0" box="[1287,1371,584,607]" country="United States of America" name="Kansas" pageId="9" pageNumber="10">Kansas</collectingRegion>
, 
<collectingRegion id="4993A6F2FFEB3869FAFEC04CFF1AFD7B" country="United States of America" name="New Jersey" pageId="9" pageNumber="10">New Jersey</collectingRegion>
