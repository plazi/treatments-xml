<taxonomicName id="4C6858E8FFE1423DFF3AF207FDECFD5B" authority="Kowarz, 1887: 149" authorityName="Kowarz" authorityPageNumber="149" authorityYear="1887" box="[151,628,618,641]" class="Insecta" family="Pipunculidae" genus="Pipunculus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="semifumosus">
