<taxonomicName id="FE884D73FF952F26FE10D3A387F4FB18" authority="Zheng et Deng, 2004" authorityName="Zheng et Deng" authorityYear="2004" box="[452,987,1166,1191]" class="Insecta" family="Tetrigidae" genus="Coptotettix" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="2556" phylum="Arthropoda" rank="species" species="circinihumerus">
