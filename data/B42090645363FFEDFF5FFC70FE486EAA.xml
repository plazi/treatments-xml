<subSubSection id="749372F95362FFEDFF0FFC49FE486EAA" pageId="31" pageNumber="182" type="discussion">
<paragraph id="3C3621725362FFEDFF0FFC49FE486EAA" blockId="31.[151,1437,618,1003]" pageId="31" pageNumber="182">
<emphasis id="0EFDFD605362FFEDFF0FFC49FEF96E87" bold="true" box="[199,315,942,967]" pageId="31" pageNumber="182">Remarks.</emphasis>
Superficially resembling 
<taxonomicName id="FB895AF15362FFEDFDAAFC48FCEB6E86" box="[610,809,943,967]" class="Insecta" family="Aphrophoridae" genus="Tremapterus" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="182" phylum="Arthropoda" rank="species" species="muiri" subGenus="Nyanja">
<emphasis id="0EFDFD605362FFEDFDAAFC48FDB46E86" box="[610,630,943,966]" italics="true" pageId="31" pageNumber="182">T.</emphasis>
(
<emphasis id="0EFDFD605362FFEDFD41FC48FD186E86" box="[649,730,943,966]" italics="true" pageId="31" pageNumber="182">Nyanja</emphasis>
) 
<emphasis id="0EFDFD605362FFEDFD24FC48FCEB6E86" box="[748,809,943,966]" italics="true" pageId="31" pageNumber="182">muiri</emphasis>
</taxonomicName>
