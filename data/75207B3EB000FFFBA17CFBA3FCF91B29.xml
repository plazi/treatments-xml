</emphasis>
: 
<taxonomicName id="3A89B1ABB000FFFBA038FB09FDF21DC7" authority="Simon, 1875" authorityName="Simon" authorityYear="1875" box="[313,626,1207,1231]" class="Arachnida" family="Agelenidae" genus="Agelena" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="371" phylum="Arthropoda" rank="species" species="livida">
