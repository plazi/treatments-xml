<taxonomicName id="71C81569113CB069616725B0A55A0739" family="Scathophagidae" lsidName="" pageId="0" pageNumber="131" rank="family">Scathophagidae</taxonomicName>
larvae. 
<taxonomicName id="800C5C2C777CB1197657C813DD532FF8" authority="Foerster, 1863" authorityName="Foerster" authorityYear="1863" class="Insecta" family="Braconidae" genus="Grammospila" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Grammospila isabella" order="Hymenoptera" pageId="0" pageNumber="131" phylum="Arthropoda" rank="species" species="isabella">Grammospila isabella</taxonomicName>
