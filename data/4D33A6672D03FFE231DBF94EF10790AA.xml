<treatmentCitationGroup id="E58A305F2D03FFE031DBF839F63F92F5" pageId="0" pageNumber="13">
<taxonomicName id="029A6CF22D03FFE031DBF839F36C92B5" authorityName="E. Pessoa &amp; M. Alves" authorityYear="2012" box="[146,291,1946,1969]" class="Liliopsida" family="Orchidaceae" genus="Anathallis" kingdom="Plantae" order="Asparagales" pageId="0" pageNumber="13" phylum="Tracheophyta" rank="species" species="amazonica">
