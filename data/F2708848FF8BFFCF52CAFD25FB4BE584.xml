<taxonomicName id="BDD942DDFF8DFFCF5084FC3FFC94E1F4" box="[729,826,952,973]" class="Actinopterygii" family="Cyprinidae" genus="Sinigarra" higherTaxonomySource="CoL" kingdom="Animalia" order="Cypriniformes" pageId="4" pageNumber="21" phylum="Chordata" rank="genus">Sinigarra</taxonomicName>
napoense
</emphasis>
