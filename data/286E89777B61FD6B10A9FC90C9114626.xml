: 
<taxonomicName id="67C743E27B61FD741131F840CDC540D9" box="[272,488,1986,2012]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Diphasium" kingdom="Plantae" order="Lycopodiales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="jussiaei">
