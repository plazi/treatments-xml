<taxonomicName id="67C743E27B61FD74125AF841C83940D9" authority="(G. Forster) Rothmaler" authorityName="Rothmaler" baseAuthorityName="G. Forster" box="[635,1044,1986,2013]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Lycopodium" kingdom="Plantae" order="Lycopodiales" pageId="47" pageNumber="48" phylum="Tracheophyta" rank="species" species="scariosum">
