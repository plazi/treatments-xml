, 
<location id="15A09BA59BDFC8E88EE076198DA9B854" LSID="urn:lsid:plazi:treatment:33041BB281DBACA86EBBEA4C9C116E82:15A09BA59BDFC8E88EE076198DA9B854" country="Schweden" latitude="60.9120" longitude="14.4000" name="Mora, Siljansfors Versuchspark" stateProvince="Dalarna">Mora, Siljansfors Versuchspark</location>
