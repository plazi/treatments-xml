<collectionCode id="ED28AE0EF9058433FDB8315EB054FDEB" box="[524,592,596,622]" country="United Kingdom" httpUri="http://grbio.org/cool/h8hj-4rxx" name="National Museum of Scotland - Natural Sciences" pageId="12" pageNumber="13" type="Museum">NMS</collectionCode>
