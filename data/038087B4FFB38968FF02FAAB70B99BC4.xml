<taxonomicName id="4C294D21FFB3896CFF49FA1D71859CD3" authority="(Leske)" authorityName="Leske" baseAuthorityName="Leske" baseAuthorityYear="1778" box="[220,657,1449,1471]" class="Echinoidea" family="Clypeasteridae" genus="Clypeaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clypeasteroida" pageId="25" pageNumber="26" phylum="Echinodermata" rank="species" species="humilis" subGenus="Stolonoclypus">
