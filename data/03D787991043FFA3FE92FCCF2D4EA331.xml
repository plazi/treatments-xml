<paragraph id="8BC1368F1043FFACFF2BFB9F2CFAA22F" blockId="7.[160,1155,1037,1133]" pageId="7" pageNumber="1331">
<materialsCitation id="3B163CD21043FFACFF2BFB9F2CFAA22F" collectionCode="OUMNH" collectorName="E. A. Newbery" country="India" location="INDIA" pageId="7" pageNumber="1331" specimenCount="1" typeStatus="holotype">
