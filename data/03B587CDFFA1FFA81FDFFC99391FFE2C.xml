<paragraph id="8BA336DBFFA0FFAF1FDFFD0539E8FC8C" blockId="11.[151,659,728,781]" pageId="11" pageNumber="210">
<taxonomicName id="4C1C4D58FFA0FFAF1FDFFD053AA4FD6E" box="[151,448,728,750]" class="Malacostraca" family="Lophogastridae" genus="Gnathophausia" kingdom="Animalia" order="Lophogastrida" pageId="11" pageNumber="210" phylum="Arthropoda" rank="subSpecies" species="elegans" subSpecies="fagei">
<emphasis id="B968EAC9FFA0FFAF1FDFFD053AA4FD6E" box="[151,448,728,750]" italics="true" pageId="11" pageNumber="210">Gnathophausia elegans fagei</emphasis>
</taxonomicName>
—Bacescu 
<number id="3DD424FCFFA0FFAF1D7CFD05390EFD6D" box="[564,618,728,749]" pageId="11" pageNumber="210" value="1991.0">1991</number>
: 
<number id="3DD424FCFFA0FFAF1D3CFD0539F4FD6D" box="[628,656,728,749]" pageId="11" pageNumber="210" value="88.0">88</number>
. 
<taxonomicName id="4C1C4D58FFA0FFAF1FDFFD2A3937FC8C" authority="Casanova, 1996a: 129" authorityName="Casanova" authorityPageNumber="129" authorityYear="1996" box="[151,595,758,781]" class="Malacostraca" family="Lophogastridae" genus="Gnathophausia" kingdom="Animalia" order="Lophogastrida" pageId="11" pageNumber="210" phylum="Arthropoda" rank="species" species="fagei">
<emphasis id="B968EAC9FFA0FFAF1FDFFD2A3A08FC8D" box="[151,364,759,781]" italics="true" pageId="11" pageNumber="210">Gnathophausia fagei</emphasis>
