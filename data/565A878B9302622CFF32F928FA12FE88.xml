<taxonomicName id="19F34D1E9305622CFAA1FF6DFA19FF6C" authorityName="Wiedemann" authorityYear="1828" box="[1284,1424,225,248]" class="Insecta" family="Tipulidae" genus="Megistocera" kingdom="Animalia" order="Diptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="genus">
