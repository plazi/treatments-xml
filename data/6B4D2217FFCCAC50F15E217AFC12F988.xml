<quantity id="241C3EE4FFF3AC51F3312004FC00FADD" box="[827,900,1338,1358]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="32" unit="mm" value="0.5">0.5 mm</quantity>
; 100-102, 105-106: 
<quantity id="241C3EE4FFF3AC51F4362004FB01FADD" box="[1084,1157,1338,1358]" metricMagnitude="-4" metricUnit="m" metricValue="2.0" pageId="32" unit="mm" value="0.2">0.2 mm</quantity>
; 103-104, 107: 
<quantity id="241C3EE4FFF3AC51F188206FFE4EFAF6" box="[386,458,1361,1381]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="32" unit="mm" value="0.1">0.1 mm</quantity>
