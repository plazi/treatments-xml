</taxonomicName>
, 
<taxonomicName id="D9844D26FFB39C476150F9964B16F9BC" authority="(SCOTT, 1936)" baseAuthorityName="SCOTT" baseAuthorityYear="1936" box="[580,836,1558,1580]" class="Insecta" family="Cryptophagidae" genus="Mnioticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="917" phylum="Arthropoda" rank="species" species="jeanneli">
<emphasis id="2CF0EAB7FFB39C476150F9964AE1F9BB" box="[580,691,1558,1580]" italics="true" pageId="2" pageNumber="917">M. jeanneli</emphasis>
