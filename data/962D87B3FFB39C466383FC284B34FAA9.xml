<taxonomicName id="D9844D26FFB39C4760BDFA784977F9BC" authority="(SCOTT, 1936)" baseAuthorityName="SCOTT" baseAuthorityYear="1936" class="Insecta" family="Cryptophagidae" genus="Mnioticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="917" phylum="Arthropoda" rank="species" species="gibbinsi">
