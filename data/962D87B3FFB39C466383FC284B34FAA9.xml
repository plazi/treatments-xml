(
<bibRefCitation id="7A154B54FFB39C47638BF997494CF9BC" author="SCOTT H" box="[159,286,1558,1580]" pageId="2" pageNumber="917" pagination="235 - 245" refId="ref6631" refString="SCOTT H. (1936): Coleoptera associated with the giant Lobelias and arborescent Senecios of eastern Africa. - Journal of the Linnean Society London 39: 235 - 245; 252 - 281." type="journal article" year="1936">SCOTT, 1936</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName id="D9844D26FFB39C47623AF9964A68F9BC" authority="(SCOTT, 1936)" baseAuthorityName="SCOTT" baseAuthorityYear="1936" box="[302,570,1558,1580]" class="Insecta" family="Cryptophagidae" genus="Mnioticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="917" phylum="Arthropoda" rank="species" species="hancocki">
