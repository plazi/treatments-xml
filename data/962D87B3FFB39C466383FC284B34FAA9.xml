(
<bibRefCitation id="7A154B54FFB29C46605DFF704B9EFE97" author="SCOTT H" box="[841,972,241,263]" pageId="3" pageNumber="918" pagination="235 - 245" refId="ref6631" refString="SCOTT H. (1936): Coleoptera associated with the giant Lobelias and arborescent Senecios of eastern Africa. - Journal of the Linnean Society London 39: 235 - 245; 252 - 281." type="journal article" year="1936">SCOTT, 1936</bibRefCitation>
)
</taxonomicName>
(
<figureCitation id="86BF2A20FFB29C4660F6FF704C48FE97" box="[994,1050,241,263]" captionStart="Figs 1-4" captionStartId="10.[151,191,1517,1539]" captionTargetBox="[161,1061,211,1506]" captionTargetId="figure-46@10.[153,1061,197,1507]" captionTargetPageId="10" captionText="Figs 1-4: (1) Mnioticus gibbinsi (SCOTT, 1936), male; (2) Mnioticus hancocki (SCOTT, 1936), male; (3) Mnioticus rhynchopetali (SCOTT, 1936), holotype, male; (4) Mnioticus rougemonti ESSER, 2014, paratype, male." figureDoi="http://doi.org/10.5281/zenodo.5277503" httpUri="https://zenodo.org/record/5277503/files/figure.png" pageId="3" pageNumber="918">fig. 3</figureCitation>
) differs by its longer legs and more slender antennae. 
<taxonomicName id="D9844D26FFB29C4661A6FE8E4B9CFEB3" authority="ESSER, 2014" authorityName="ESSER" authorityYear="2014" box="[690,974,270,292]" class="Insecta" family="Cryptophagidae" genus="Mnioticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="918" phylum="Arthropoda" rank="species" species="rougemonti">
