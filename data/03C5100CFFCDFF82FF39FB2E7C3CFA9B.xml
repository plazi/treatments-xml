<taxonomicName id="4C6CDA99FFCDFF83FDD4F8BC7D53F8F8" authority="Papadoulis &amp; Emmanouel" authorityName="Papadoulis &amp; Emmanouel" box="[634,1350,1865,1890]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="2" pageNumber="231" phylum="Arthropoda" rank="species" species="krimbasi" subGenus="Anthoseius">
<emphasis id="B9187D08FFCDFF83FDD4F8BC7B0DF8F8" box="[634,792,1865,1890]" italics="true" pageId="2" pageNumber="231">Typhlodromus</emphasis>
(
<emphasis id="B9187D08FFCDFF83FC87F8BC7BB1F8F8" box="[809,932,1865,1890]" italics="true" pageId="2" pageNumber="231">Anthoseius</emphasis>
) 
<emphasis id="B9187D08FFCDFF83FC1BF8BC7C02F8F8" box="[949,1047,1865,1890]" italics="true" pageId="2" pageNumber="231">krimbasi</emphasis>
