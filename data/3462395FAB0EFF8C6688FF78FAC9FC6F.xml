; 
<bibRefCitation id="D85AF5B8AB0EFF8C626BFC89FD64FC4B" author="Stephensen, K." box="[1140,1334,873,898]" pageId="10" pageNumber="11" pagination="1 - 140" refId="ref28478" refString="Stephensen, K. (1935) The Amphipoda of N. Norway and Spitsbergen with adjacent waters. Tromso Museums Skrifter, 3, 1 - 140." type="journal article" year="1935">Stephensen 1935</bibRefCitation>
). 
<emphasis id="8EBF545BAB0EFF8C6355FC8AFDC7FC48" box="[1354,1429,874,897]" italics="true" pageId="10" pageNumber="11">
<collectingCountry id="C4DCC8D9AB0EFF8C6355FC8AFDC7FC48" box="[1354,1429,874,897]" name="Russia" pageId="10" pageNumber="11">Russia</collectingCountry>
