<date id="591BAB96FFC0AF50FB0AFD62FB6BFD74" box="[1175,1210,690,714]" pageId="2" pageNumber="187">
<number id="9B6D9F71FFC0AF50FB0AFD62FB72FD74" box="[1175,1187,690,714]" pageId="2" pageNumber="187" value="3.0">3</number>
X
</date>
longer than wide; postpedicel about 
<date id="591BAB96FFC0AF50FEF7FD05FE5DFD50" box="[362,396,725,750]" pageId="2" pageNumber="187">
<number id="9B6D9F71FFC0AF50FEF7FD05FEA6FD50" box="[362,375,725,750]" pageId="2" pageNumber="187" value="6.0">6</number>
X
</date>
