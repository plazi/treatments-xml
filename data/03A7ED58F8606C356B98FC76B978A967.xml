<collectionCode id="ED1FC48BF8606C356E2BF922BE1BA8F7" box="[1316,1395,1677,1701]" pageId="38" pageNumber="39">MZFS</collectionCode>
); 
<number id="3DC64E69F8606C356E81F922BEF4A8F7" box="[1422,1436,1677,1701]" pageId="38" pageNumber="39" value="2.0">2</number>
