<subSubSection id="DFB1DFDC68505D889663AF7A461F2110" lastPageId="28" lastPageNumber="109" pageId="27" pageNumber="108" type="studied material">
<paragraph id="F92DE8E35FC4664A26D58BCB87430CFE" pageId="27" pageNumber="108">Studied material.</paragraph>
<paragraph id="F99F911C54791B4D015B07679881B32B" lastPageId="28" lastPageNumber="109" pageId="27" pageNumber="108">
