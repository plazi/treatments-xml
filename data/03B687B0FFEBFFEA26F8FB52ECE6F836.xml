<quantity id="4CE79B43FFEBFFED27FFF9A2ED1FF9EB" box="[400,570,1599,1623]" metricMagnitude="-3" metricUnit="m" metricValue="2.81" metricValueMax="3.13" metricValueMin="2.49" pageId="27" pageNumber="128" unit="mm" value="2.81" valueMax="3.13" valueMin="2.49">2.49–3.13 mm</quantity>
