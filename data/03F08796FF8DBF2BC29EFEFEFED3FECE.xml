<taxonomicName id="4C594D03FF8CBF2BC78DFF18FF34FF06" authorityName="Nabozhenko et Tichy" authorityYear="2011" class="Insecta" family="Tenebrionidae" genus="Entomogonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="552" phylum="Arthropoda" rank="species" species="bialookii">
<emphasis id="B92DEA92FF8CBF2BC78DFF18FF34FF06" italics="true" pageId="3" pageNumber="552">E. bialookii</emphasis>
