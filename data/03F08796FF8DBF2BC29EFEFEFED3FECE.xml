and 
<taxonomicName id="4C594D03FF8CBF2BC339FF3CFE0BFF06" authorityName="Nabozhenko et Tichy" authorityYear="2011" box="[304,452,188,212]" class="Insecta" family="Tenebrionidae" genus="Entomogonus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="552" phylum="Arthropoda" rank="species" species="makovskyi">
