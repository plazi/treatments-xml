<location id="8E81B4CEAE78FF98FE84F8AE3EE6822B" LSID="urn:lsid:plazi:treatment:03F75303AE78FF98FF7EFF5B3C4182F3:8E81B4CEAE78FF98FE84F8AE3EE6822B" box="[365,410,1890,1916]" country="Haiti" name="Dry" pageId="38" pageNumber="39">Dry</location>
<collectingCountry id="F349A285AE78FF98FE4BF8AE3D77822B" box="[418,523,1890,1916]" name="Haiti" pageId="38" pageNumber="39">Tortugas</collectingCountry>
