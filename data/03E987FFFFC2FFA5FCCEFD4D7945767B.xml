<taxonomicName id="4C404D6AFFC2FFA5FBF4F91F789C7645" baseAuthorityName="Temminck &amp; Schlegel" baseAuthorityYear="1845" box="[1047,1172,1782,1806]" class="Actinopterygii" family="Leiognathidae" genus="Nuchequula" kingdom="Animalia" order="Perciformes" pageId="6" pageNumber="249" phylum="Chordata" rank="species" species="nuchalis">
