<quantity id="4CB89B0CFFC2FFA5FC9BFA017FB9774A" box="[888,945,1513,1537]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="6" pageNumber="249" unit="m" value="10.0">10 m</quantity>
