<heading id="761D81CEF426A37F4AC8F395FE79295D" bold="true" box="[151,494,509,535]" fontSize="11" level="1" pageId="1" pageNumber="394" reason="1">
<taxonomicName id="EAEA4D21F426A37F4AC8F395FE79295D" authority="Distant" authorityName="Distant" box="[151,494,509,535]" class="Liliopsida" family="Orchidaceae" genus="Tolumnia" kingdom="Plantae" order="Asparagales" pageId="1" pageNumber="394" phylum="Tracheophyta" rank="species" species="antennata">
<emphasis id="1F9EEAB0F426A37F4AC8F395FE79295D" bold="true" box="[151,494,509,535]" pageId="1" pageNumber="394">
