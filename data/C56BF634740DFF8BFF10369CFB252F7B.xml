<taxonomicName id="8AC23CA1740DFF8BFEF33294FB992EB3" authority="Say" authorityName="Say" box="[372,724,1721,1745]" class="Insecta" family="Chrysomelidae" genus="Paranapiacaba" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="tricincta">
