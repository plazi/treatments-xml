group of 
<taxonomicName id="8AC23CA1740DFF8BFDF132B8FA692ECF" box="[630,804,1685,1709]" class="Insecta" family="Chrysomelidae" genus="Paranapiacaba" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="322" phylum="Arthropoda" rank="genus">
