<taxonomicName id="4C764D090B43EC41FD5D5B6997E0F8CE" authority="DeLong, 1975" authorityName="DeLong" authorityYear="1975" box="[765,1174,1891,1918]" class="Insecta" family="Cicadellidae" genus="Freytagana" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="88" phylum="Arthropoda" rank="species" species="gibsoni">
