<taxonomicName id="49BD4D37FFD8D14FFE8FFA46FDCEFA16" box="[320,553,1455,1477]" class="Insecta" family="Sphaeroceridae" genus="Afrolimosina" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="104" phylum="Arthropoda" rank="species" species="albitarsis" status="sp. n.">
<emphasis id="BCC9EAA6FFD8D14FFE8FFA46FDCEFA16" box="[320,553,1455,1477]" italics="true" pageId="3" pageNumber="104">Afrolimosina albitarsis</emphasis>
