<taxonomicName id="AFD9A596126DFFE0FED5F950FD33F97F" authority="Monterosato, 1877" authorityName="Monterosato" authorityYear="1877" box="[278,662,1697,1722]" class="Gastropoda" family="Scissurellidae" genus="Schizotrochus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Archaeogastropoda" pageId="23" pageNumber="24" phylum="Mollusca" rank="genus">
