<taxonomicName id="4C204D1FFFB3214FD94CFAEEFF5E67F7" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[123,241,1400,1424]" class="Bacilli" family="Lactobacillaceae" genus="Lentilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="62" pageNumber="2844" phylum="Firmicutes" rank="species" species="buchneri">
<emphasis id="B954EA8EFFB3214FD94CFAEEFF5E67F7" box="[123,241,1400,1424]" italics="true" pageId="62" pageNumber="2844">L. buchneri</emphasis>
