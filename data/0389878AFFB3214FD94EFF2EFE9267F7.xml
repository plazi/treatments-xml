<taxonomicName id="4C204D1FFFB3214FD9C9FACFFE186716" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" box="[254,439,1369,1393]" class="Bacilli" family="Lactobacillaceae" genus="Lentilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="62" pageNumber="2844" phylum="Firmicutes" rank="genus">
<emphasis id="B954EA8EFFB3214FD9C9FACFFE186716" box="[254,439,1369,1393]" italics="true" pageId="62" pageNumber="2844">Lentilactobacillus</emphasis>
