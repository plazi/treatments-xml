<quantity id="4CF79B33FFA7FF8EC0E2FE0AFD47FE0B" box="[667,721,467,487]" metricMagnitude="2" metricUnit="m" metricValue="4.25" pageId="72" pageNumber="227" unit="m" value="425.0">425m</quantity>
