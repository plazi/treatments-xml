<quantity id="4CF79B33FFA7FF8EC3EBFD81FE51FD80" box="[402,455,600,620]" metricMagnitude="2" metricUnit="m" metricValue="4.6" pageId="72" pageNumber="227" unit="m" value="460.0">460m</quantity>
