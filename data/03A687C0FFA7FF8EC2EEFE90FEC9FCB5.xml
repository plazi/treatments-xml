<quantity id="4CF79B33FFA7FF8EC30AFD57FE3EFD4E" box="[371,424,654,674]" metricMagnitude="2" metricUnit="m" metricValue="4.4" pageId="72" pageNumber="227" unit="m" value="440.0">440m</quantity>
