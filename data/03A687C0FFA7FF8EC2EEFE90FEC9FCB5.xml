<quantity id="4CF79B33FFA7FF8EC0D3FDFAFD76FDDB" box="[682,736,547,567]" metricMagnitude="2" metricUnit="m" metricValue="4.4" pageId="72" pageNumber="227" unit="m" value="440.0">440m</quantity>
