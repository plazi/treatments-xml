<taxonomicName class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="563" pageNumber="564" phylum="Arthropoda" rank="subSpecies" species="alpina" subSpecies="brunnipennis">
