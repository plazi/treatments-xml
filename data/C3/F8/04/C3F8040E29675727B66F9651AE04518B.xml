<taxonomicName baseAuthorityName="Say" baseAuthorityYear="1823" class="Hexapoda" family="Carabidae" genus="Amara" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="564" pageNumber="565" phylum="Arthropoda" rank="species" species="angustata">
