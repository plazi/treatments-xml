Southern 
<collectingCountry box="[467,562,1955,1981]" name="Vietnam" pageId="5" pageNumber="580">Vietnam</collectingCountry>
(
<collectingRegion box="[576,798,1955,1982]" country="Vietnam" name="Dong Nai" pageId="5" pageNumber="580">Dong Nai province</collectingRegion>
).
</paragraph>
</subSubSection>
