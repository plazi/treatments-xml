, 
<number box="[606,658,1940,1964]" pageId="7" pageNumber="204" value="1.07">1.07</number>
, 
<number box="[673,724,1940,1964]" pageId="7" pageNumber="204" value="1.37">1.37</number>
, 
<number box="[740,794,1940,1964]" pageId="7" pageNumber="204" value="0.74">0.74</number>
