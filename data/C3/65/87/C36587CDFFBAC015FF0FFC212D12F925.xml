<number box="[1379,1431,1555,1579]" pageId="12" pageNumber="209" value="1.37">1.37</number>
, 
<number box="[151,203,1591,1615]" pageId="12" pageNumber="209" value="0.42">0.42</number>
, 
<number box="[216,267,1591,1615]" pageId="12" pageNumber="209" value="1.28">1.28</number>
, 
<number box="[280,331,1590,1615]" pageId="12" pageNumber="209" value="1.35">1.35</number>
, 
<number box="[345,399,1591,1615]" pageId="12" pageNumber="209" value="0.78">0.78</number>
