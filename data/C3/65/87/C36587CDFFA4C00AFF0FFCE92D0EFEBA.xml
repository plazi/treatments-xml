(
<number box="[584,636,1590,1615]" pageId="18" pageNumber="215" value="1.35">1.35</number>
, 
<number box="[649,700,1591,1615]" pageId="18" pageNumber="215" value="0.38">0.38</number>
, 
<number box="[713,765,1590,1615]" pageId="18" pageNumber="215" value="1.26">1.26</number>
