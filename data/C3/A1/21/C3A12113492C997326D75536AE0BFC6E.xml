<geoCoordinate direction="south" orientation="latitude" precision="1" value="-2.5296">S02°31.776'</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="140.50359">E140°30.215'</geoCoordinate>
