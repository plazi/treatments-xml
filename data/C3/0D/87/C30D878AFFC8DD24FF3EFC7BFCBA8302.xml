<emphasis bold="true" box="[151,297,1103,1128]" pageId="5" pageNumber="44">Description.</emphasis>
MALE (
<typeStatus box="[399,503,1103,1127]" pageId="5" pageNumber="44">holotype</typeStatus>
). Body 
<number box="[591,633,1103,1127]" pageId="5" pageNumber="44" value="370.0">370</number>
long (
<quantity box="[706,835,1103,1127]" metricMagnitude="0" metricUnit="m" metricValue="9.4615" metricValueMax="9.652" metricValueMin="9.271" pageId="5" pageNumber="44" unit="in" value="372.5" valueMax="380.0" valueMin="365.0">365–380 in</quantity>
<number box="[842,870,1103,1127]" pageId="5" pageNumber="44" value="10.0">10</number>
<typeStatus box="[878,987,1103,1127]" pageId="5" pageNumber="44">paratypes</typeStatus>
) and 
<number box="[1051,1093,1103,1127]" pageId="5" pageNumber="44" value="175.0">175</number>
