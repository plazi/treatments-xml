<quantity box="[1170,1269,1428,1452]" metricMagnitude="-1" metricUnit="m" metricValue="5.715" metricValueMax="6.35" metricValueMin="5.08" pageId="5" pageNumber="44" unit="in" value="22.5" valueMax="25.0" valueMin="20.0">20–25 in</quantity>
