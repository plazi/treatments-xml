<taxonomicName class="Insecta" family="Curculionidae" genus="Erineophilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Erineophilus" order="Coleoptera" pageId="272" pageNumber="273" phylum="Arthropoda" rank="genus">Erineophilus</taxonomicName>
