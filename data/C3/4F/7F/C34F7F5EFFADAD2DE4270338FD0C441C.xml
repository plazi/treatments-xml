<taxonomicName authority="(Banks, 1913)" baseAuthorityName="Banks" baseAuthorityYear="1913" box="[1207,1474,2006,2027]" genus="Malacomyza" kingdom="Animalia" pageId="0" pageNumber="365" rank="species" species="terminalis">
