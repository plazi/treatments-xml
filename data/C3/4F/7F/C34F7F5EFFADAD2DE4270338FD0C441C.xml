<emphasis box="[501,630,337,358]" italics="true" pageId="1" pageNumber="366">
<taxonomicName authority="Liu &amp; Yang &amp; Shen, 2004" authorityName="Liu &amp; Yang &amp; Shen" authorityYear="2004" box="[501,626,337,358]" class="Insecta" family="Coniopterygidae" genus="Heteroconis" kingdom="Animalia" order="Neuroptera" pageId="1" pageNumber="366" phylum="Arthropoda" rank="species" species="unicornis" status="new species">H. unicornis</taxonomicName>
