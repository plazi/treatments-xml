<emphasis box="[1230,1351,1814,1835]" italics="true" pageId="0" pageNumber="365">
<taxonomicName authority="Liu &amp; Yang &amp; Shen, 2004" authorityName="Liu &amp; Yang &amp; Shen" authorityYear="2004" box="[1230,1347,1814,1835]" class="Insecta" family="Coniopterygidae" genus="Heteroconis" kingdom="Animalia" order="Neuroptera" pageId="0" pageNumber="365" phylum="Arthropoda" rank="species" species="tricornis" status="new species">H. tricornis</taxonomicName>
