<subSubSection box="[151,754,1012,1036]" pageId="3" pageNumber="90" type="materials_examined">
<paragraph blockId="3.[151,754,1012,1036]" box="[151,754,1012,1036]" pageId="3" pageNumber="90">
<typeStatus box="[151,201,1012,1036]" pageId="3" pageNumber="90">Type</typeStatus>
species: 
<taxonomicName authority="Sáfián &amp; Bartsch, 2022" authorityName="Sáfián &amp; Bartsch" authorityYear="2022" box="[294,491,1012,1036]" class="Insecta" family="Sesiidae" genus="Chlorosphecia" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="90" phylum="Arthropoda" rank="species" species="bele" status="sp. nov.">
<emphasis box="[294,491,1012,1036]" italics="true" pageId="3" pageNumber="90">Chlorosphecia bele</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[497,578,1012,1035]" pageId="3" pageNumber="90" rank="species">sp. nov.</taxonomicNameLabel>
, designated here.
