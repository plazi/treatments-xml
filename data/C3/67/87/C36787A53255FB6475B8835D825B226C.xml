<emphasis box="[848,1150,986,1011]" italics="true" pageId="2" pageNumber="55">Brachionichthys australis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1158,1251,986,1012]" pageId="2" pageNumber="55" rank="species">
<emphasis bold="true" box="[1158,1251,986,1012]" pageId="2" pageNumber="55">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName authority="Lacepede, 1804" authorityName="Lacepede" authorityYear="1804" class="Actinopterygii" family="Lophiidae" genus="Lophius" kingdom="Animalia" order="Lophiiformes" pageId="2" pageNumber="55" phylum="Chordata" rank="species" species="hirsutus">
<emphasis box="[1309,1436,986,1011]" italics="true" pageId="2" pageNumber="55">B. hirsutus</emphasis>
(
<bibRefCitation author="Lacepede" box="[159,350,1026,1052]" pageId="2" pageNumber="68" refString="Lacepede, B. G. E. (1804) Memoire sur plusieurs animaux de la Nouvelle Hollande dont la description n'a pas encore ete publiee. Annales du Museum, Paris, 4, 184 - 211." type="journal article" year="1804">
Lacepède, 
<number box="[287,350,1026,1052]" pageId="2" pageNumber="55" value="1804.0">1804</number>
</bibRefCitation>
)
</taxonomicName>
