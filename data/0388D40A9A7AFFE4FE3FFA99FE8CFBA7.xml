<quantity id="4CD9C8F99A7AFFE3FE05F90CFDF9F931" box="[434,597,1668,1693]" metricMagnitude="-3" metricUnit="m" metricValue="1.555" metricValueMax="1.67" metricValueMin="1.44" pageId="27" pageNumber="1851" unit="mm" value="1.555" valueMax="1.67" valueMin="1.44">1.44–1.67 mm</quantity>
