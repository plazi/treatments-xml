<quantity id="4CD9C8F99A7AFFE3FD0FF90CFCF6F931" box="[696,858,1668,1693]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" metricValueMax="6.7" metricValueMin="6.3" pageId="27" pageNumber="1851" unit="mm" value="0.65" valueMax="0.67" valueMin="0.63">0.63–0.67 mm</quantity>
