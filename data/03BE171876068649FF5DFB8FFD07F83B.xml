<taxonomicName id="4C17DD8D76068646FF5DFB8FFE72FB26" authority="Saussure, 1869" authorityName="Saussure" authorityYear="1869" box="[151,490,1145,1171]" class="Insecta" family="Mantidae" genus="Coptopteryx" kingdom="Animalia" order="Mantodea" pageId="7" pageNumber="342" phylum="Arthropoda" rank="genus">
<emphasis id="B9637A1C76068646FF5DFB8FFE72FB26" bold="true" box="[151,490,1145,1171]" pageId="7" pageNumber="342">
<emphasis id="B9637A1C76068646FF5DFB8FFEB2FB26" bold="true" box="[151,298,1145,1171]" italics="true" pageId="7" pageNumber="342">Coptopteryx</emphasis>
Saussure, 
<number id="3DDFB42976068646FE65FB8FFE72FB26" box="[431,490,1145,1171]" pageId="7" pageNumber="342" value="1869.0">1869</number>
</emphasis>
