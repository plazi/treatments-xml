<date id="E86C6D9AFFD48D323DFAFC6FFC17FC11" box="[916,986,938,962]" bridgedPair="-" pageId="13" pageNumber="316" value="2007-01">İ.-2007</date>
-45.
</paragraph>
<paragraph id="9C6D4B5AFFD48D323CCBFC1FFB88FC21" blockId="13.[677,1093,986,1010]" box="[677,1093,986,1010]" pageId="13" pageNumber="316">
<typeStatus id="4369F5F8FFD48D323CCBFC1FFCC6FC23" box="[677,779,986,1009]" pageId="13" pageNumber="316">PARATYPES</typeStatus>
