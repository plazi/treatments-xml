<bibRefCitation id="DA284B40FFE5FF8055ABFBA35143C934" author="Dendy, A." box="[151,297,1030,1057]" pageId="32" pageNumber="33" pagination="57 - 246" refId="ref130615" refString="Dendy, A. (1905) Report on the sponges collected by Professor Herdman, at Ceylon, in 1902. In: Herdman, W. A. (Ed.), Report to the Government of Ceylon on the Pearl Oyster Fisheries of the Gulf of Manaar, 3 (Supplement 18), pp. 57 - 246, pls. I-XVI." type="book chapter" year="1905">Dendy, 1905</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="79B94D32FFE5FF80545FFBA25371C935" authority="Burton &amp; Rao, 1932" authorityName="Burton &amp; Rao" authorityYear="1932" box="[355,795,1030,1056]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="monanchorata">
<emphasis id="8CCDEAA3FFE5FF80545FFBA25246C935" box="[355,556,1030,1056]" italics="true" pageId="32" pageNumber="33">M. monanchorata</emphasis>
