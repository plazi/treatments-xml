<taxonomicName id="79B94D32FFE5FF805691FC1B5564CECC" authority="Wilson, 1925" authorityName="Wilson" authorityYear="1925" box="[941,1294,958,985]" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="regularis" subGenus="Arenochalina">
<emphasis id="8CCDEAA3FFE5FF805691FC1B5406CECD" box="[941,1132,958,984]" italics="true" pageId="32" pageNumber="33">M.(Ar.) regularis</emphasis>
<bibRefCitation id="DA284B40FFE5FF805148FC1B5564CECC" author="Wilson, H. V." box="[1140,1294,958,985]" pageId="32" pageNumber="33" pagination="273 - 532" refId="ref138784" refString="Wilson, H. V. (1925) Siliceous and horny sponges collected by the U. S. Fisheries Steamer ' Albatross' during the Philippine Expedition, 1907 - 10. In: Contributions to the biology of the Philippine Archipelago and adjacent regions. Bulletin of the United States National Museum, 100 (2, part 4), 273 - 532, pls. 37 - 52." type="journal article" year="1925">Wilson, 1925</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="79B94D32FFE5FF805077FC1B5240CEE9" authority="Pulitzer-Finali, 1982 a" authorityName="Pulitzer-Finali" authorityYear="1982" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="tylostrongyla" subGenus="Arenochalina">
<emphasis id="8CCDEAA3FFE5FF805077FC1B5140CEE9" italics="true" pageId="32" pageNumber="33">M.(Ar.) tylostrongyla</emphasis>
<bibRefCitation id="DA284B40FFE5FF80540EFC475240CEE9" author="Pulitzer-Finali, G." box="[306,554,994,1020]" pageId="32" pageNumber="33" pagination="87 - 141" refId="ref134636" refString="Pulitzer-Finali, G. (1982 a [1980 - 1981]) Some new or little-known sponges from the Great Barrier Reef of Australia. Bollettino dei Musei e degli Istituti Biologici dell'Universit di Genova, 48 - 49, 87 - 141." type="journal article" year="1982">Pulitzer-Finali, 1982a</bibRefCitation>
</taxonomicName>
. Additional likely 
<taxonomicName id="79B94D32FFE5FF8057C0FC4753F9CEE9" authority="Lendenfeld, 1887" authorityName="Von Lendenfeld" authorityYear="1887" box="[764,915,994,1020]" class="Demospongiae" family="Mycalidae" genus="Arenochalina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="genus">
<emphasis id="8CCDEAA3FFE5FF8057C0FC4753F9CEE9" box="[764,915,994,1020]" italics="true" pageId="32" pageNumber="33">Arenochalina</emphasis>
</taxonomicName>
species from the region are 
<taxonomicName id="79B94D32FFE5FF8051EBFC465143C934" authority="Dendy, 1905" authorityName="Dendy" authorityYear="1905" class="Demospongiae" family="Mycalidae" genus="Mycale" kingdom="Animalia" order="Poecilosclerida" pageId="32" pageNumber="33" phylum="Porifera" rank="species" species="tenuispiculata">
