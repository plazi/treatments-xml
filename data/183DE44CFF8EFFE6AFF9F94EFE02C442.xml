http://zoobank.org/ 
<uuid id="E4326F8FFF8EFFF8AEACF92AFC12C1F1" box="[405,951,1756,1782]" pageId="14" pageNumber="13">8CC4BEC6-D009-4F8A-B2CC-9AC227EB20A7</uuid>
