: 
<collectingCounty id="62DB4E78FF3BFF4DFB83FC2B3A58FC44" pageId="128" pageNumber="128">Admiralty Gulf</collectingCounty>
, 
<emphasis id="B971EAE6FF3BFF4DFCA5FC4D3D79FC7B" bold="true" box="[858,1054,970,991]" pageId="128" pageNumber="128">
<specimenCode id="DBA39E8FFF3BFF4DFCA5FC4D3D79FC7B" box="[858,1054,970,991]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="128" pageNumber="128">AMNH 697019</specimenCode>
