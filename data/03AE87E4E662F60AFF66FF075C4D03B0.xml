<taxonomicName id="4C074D71E662F60AFF66FEBF5A6D0053" authority="Priesner, 1938: 349" authorityName="Priesner" authorityPageNumber="349" authorityYear="1938" box="[151,625,261,284]" class="Insecta" family="Thripidae" genus="Taeniothrips" kingdom="Animalia" order="Thysanoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="fumosoides">
<emphasis id="B973EAE0E662F60AFF66FEBF59870053" box="[151,411,261,284]" italics="true" pageId="7" pageNumber="8">Taeniothrips fumosoides</emphasis>
<treatmentCitation id="0AA610E3E662F60AFE53FEBF5A6D0053" author="Priesner" box="[418,625,261,284]" page="349" pageId="7" pageNumber="8" year="1938">
<bibRefCitation id="EF964B03E662F60AFE53FEBF5A230053" author="Priesner" box="[418,575,261,284]" pageId="7" pageNumber="19" refString="Priesner, H. (1938) Thysanopteren aus dem Belgischen Congo (4. Beitrag). Revue de Zoologie et Botanique Africaine, 30, 343 - 355." type="journal article" year="1938">
Priesner, 
<number id="3DCF24D5E662F60AFDF7FEBF5A230053" box="[518,575,261,284]" pageId="7" pageNumber="8" value="1938.0">1938</number>
</bibRefCitation>
