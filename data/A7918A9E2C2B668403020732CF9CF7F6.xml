<subSubSection id="D99DCFEB1A534D002AA601572EA82491" type="reference_group">
<paragraph id="E1864BEE115BD9816BA015695B99A465" pageId="49" pageNumber="50">
<taxonomicName id="052AF24584E376F1A5DF7397CE7858E9" class="Arachnida" family="Phthiracaridae" genus="Phthiracarus" kingdom="Animalia" order="Sarcoptiformes" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="subglobosus">Phthiracarus subglobosus Berlese</taxonomicName>
