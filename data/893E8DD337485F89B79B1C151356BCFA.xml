3 range 100-160; 
<emphasis id="5D1A7E2B59C7AE236E22C9962180CCB1" italics="true" pageId="0" pageNumber="1">lm</emphasis>
, 
<emphasis id="02AC4B21B8C42CCC7BBE6797C61AA033" italics="true" pageId="0" pageNumber="1">lp</emphasis>
, 
<emphasis id="E68C147A411AB4C6CB128FA18F55FF75" italics="true" pageId="0" pageNumber="1">h</emphasis>
1, 
