<heading id="BF2381DA736CFFE0629AF862FC6312BB" bold="true" box="[151,549,2013,2038]" fontSize="10" level="2" pageId="25" pageNumber="368" reason="2">
<taxonomicName id="23D44D35736CFFE0629AF862FC6312BB" authority="Griffini, 1909" authorityName="Griffini" authorityYear="1909" box="[151,549,2013,2038]" class="Insecta" family="Tettigoniidae" genus="Hexacentrus" kingdom="Animalia" order="Orthoptera" pageId="25" pageNumber="368" phylum="Arthropoda" rank="species" species="karnyi">
<emphasis id="D6A0EAA4736CFFE0629AF862FC6312BB" bold="true" box="[151,549,2013,2038]" pageId="25" pageNumber="368">
<emphasis id="D6A0EAA4736CFFE0629AF862FF3B12BB" bold="true" box="[151,381,2013,2038]" italics="true" pageId="25" pageNumber="368">Hexacentrus karnyi</emphasis>
<bibRefCitation id="80454B47736CFFE0638EF861FC6312BB" author="Griffini" box="[387,549,2013,2038]" pageId="25" pageNumber="368" refString="Griffini, A. (1909) Note sopra alcune Phasgonuridae del Congo. Annales de la Societe entomologique de Belgique, 53 (1908), 9 - 28." type="journal article" year="1909">
Griffini, 
<number id="521C2491736CFFE063E0F861FC6312BB" box="[493,549,2013,2038]" pageId="25" pageNumber="368" value="1909.0">1909</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
