<collectingCountry id="F31B763E2A6279732BD8DA2BF57E2999" box="[760,874,367,392]" name="Argentina" pageId="61" pageNumber="62">Argentina</collectingCountry>
, 
<collectingCountry id="F31B763E2A6279732A55DA2BF5DE2999" box="[885,970,367,392]" name="Bolivia" pageId="61" pageNumber="62">Bolivia</collectingCountry>
, 
<collectingCountry id="F31B763E2A6279732AF5DA2BF2082999" box="[981,1052,367,392]" name="Brazil" pageId="61" pageNumber="62">Brazil</collectingCountry>
, 
<collectingCountry id="F31B763E2A6279732D07DA2BF28C2999" box="[1063,1176,367,392]" name="Colombia" pageId="61" pageNumber="62">Colombia</collectingCountry>
, 
<collectingCountry id="F31B763E2A6279732D83DA2BF3162999" box="[1187,1282,367,392]" name="Ecuador" pageId="61" pageNumber="62">Ecuador</collectingCountry>
, 
<collectingRegion id="49C8F84C2A6279732C2DDA34F37D2996" box="[1293,1385,368,391]" country="Panama" name="Panama" pageId="61" pageNumber="62">
