<taxonomicName id="4C0C4D2D2A6279732D89DAFCF36B29C1" authorityName="Torres" authorityYear="1948" box="[1193,1407,439,464]" class="Insecta" family="Cicadidae" genus="Carineta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="maculosa">
