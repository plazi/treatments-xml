</taxonomicName>
and 
<taxonomicName id="4C0C4D2D2A62797328F9DA09F47C2974" box="[473,616,332,357]" class="Insecta" family="Cicadidae" genus="Carineta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="fasciulata">
