<location id="8ED4606F4812316399B4669EFBB7F84F" LSID="urn:lsid:plazi:treatment:03A287A248123160998E64CBFD5AFC9F:8ED4606F4812316399B4669EFBB7F84F" box="[1004,1142,1927,1951]" country="France" name="Pliensbachian" pageId="81" pageNumber="1266">Pliensbachian</location>
. 
<typeStatus id="54B0881648123163997566A1FC9CF81D" box="[813,861,1976,1999]" pageId="81" pageNumber="1266">TYPE</typeStatus>
