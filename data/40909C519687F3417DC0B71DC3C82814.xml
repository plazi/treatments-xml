</paragraph>
<paragraph id="B732EFE99824A893089E4280248C2794" pageId="626" pageNumber="627">
<taxonomicName id="DE70399A7E41FB1F5DE0699AF5600CD0" authorityName="Casey" authorityYear="1920" class="Insecta" family="Carabidae" genus="Diplocheila" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="626" pageNumber="627" phylum="Arthropoda" rank="species" species="parallela">Diplocheila parallela</taxonomicName>
