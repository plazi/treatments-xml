<taxonomicName id="4C7071ED945DFFC1FD1128D5BAB8FB5F" authority="Bedoti" authorityName="Bedoti" authorityYear="1896" box="[657,1177,1172,1205]" class="Homoscleromorpha" family="Plakinidae" genus="Placinolopha" kingdom="Animalia" order="Homosclerophorida" pageId="8" pageNumber="429" phylum="Porifera" rank="genus" status="n. sp.">
<emphasis id="B904D67C945DFFC1FD1128D5BAB8FB5F" box="[657,1177,1172,1205]" italics="true" pageId="8" pageNumber="429">Placinolopha Bedoti</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2376B07945DFFC1FB3C28DDBB66FB5F" box="[1212,1351,1178,1205]" pageId="8" pageNumber="429" rank="species">n. sp.</taxonomicNameLabel>
