Island, 
<specimenCount id="9D22FD464124FFF63C994F7BF293B30A" box="[904,975,819,844]" pageId="9" pageNumber="426" type="generic">
<quantity id="4CDC9B2A4124FFF63C994F7BF297B30A" box="[904,971,819,844]" metricMagnitude="2" metricUnit="m" metricValue="2.5" pageId="9" pageNumber="426" unit="m" value="250.0">250 m</quantity>
