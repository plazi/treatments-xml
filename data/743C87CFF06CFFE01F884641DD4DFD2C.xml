<typeStatus id="232E887BF06CFFE01F884606DECCFEAB" box="[192,253,356,380]" pageId="3">Type</typeStatus>
genus: 
<taxonomicName id="3B954D5AF06CFFE01E1E4601DCE3FEAC" authority="Benesh 1946: 175" authorityName="Benesh" authorityPageNumber="175" authorityYear="1946" box="[342,722,355,379]" class="Insecta" family="Lucanidae" genus="Platyceroides" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="genus">
<emphasis id="CEE1EACBF06CFFE01E1E4601DFC8FEAC" box="[342,505,355,379]" italics="true" pageId="3">Platyceroides</emphasis>
