<taxonomicName authorityName="Regel &amp; Schmalh" authorityYear="1879" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus subhirsutus" order="Solanales" pageId="176" pageNumber="177" phylum="Tracheophyta" rank="species" species="subhirsutus">
