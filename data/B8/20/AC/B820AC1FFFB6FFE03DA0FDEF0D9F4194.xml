<taxonomicName authorityName=": Li &amp; Yang" authorityYear="2002" box="[1282,1401,1925,1948]" class="Insecta" family="Chrysomelidae" genus="Oides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="109" pageNumber="110" phylum="Arthropoda" rank="species" species="tarsata">
