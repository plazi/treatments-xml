<taxonomicName class="Insecta" family="Hesperiidae" genus="Chioides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chioides zilpa" order="Lepidoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="zilpa">Chioides zilpa</taxonomicName>
, 
<taxonomicName class="Insecta" family="Hesperiidae" genus="Polygonus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polygonus leo" order="Lepidoptera" pageId="131" pageNumber="132" phylum="Arthropoda" rank="species" species="leo">Polygonus leo</taxonomicName>
