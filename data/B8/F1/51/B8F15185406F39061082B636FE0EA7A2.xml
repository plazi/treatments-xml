<taxonomicName class="Magnoliopsida" family="Polygonaceae" genus="Coccoloba" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Coccoloba caracasana" order="Caryophyllales" pageId="2" pageNumber="57" phylum="Tracheophyta" rank="species" species="caracasana">Coccoloba caracasana</taxonomicName>
