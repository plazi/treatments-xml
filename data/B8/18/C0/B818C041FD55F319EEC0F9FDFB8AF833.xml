sts; tarsi II with 
<number box="[370,384,1150,1174]" pageId="19" pageNumber="20" value="1.0">1</number>
bsl, 
<number box="[438,452,1150,1174]" pageId="19" pageNumber="20" value="1.0">1</number>
tsl, 
<number box="[501,529,1150,1174]" pageId="19" pageNumber="20" value="18.0">18</number>
sts; tarsi III with 
<number box="[729,743,1150,1174]" pageId="19" pageNumber="20" value="1.0">1</number>
tsl, 
