Idiosoma 
<number box="[418,516,953,977]" pageId="5" pageNumber="6" value="481.5" valueMax="575.0" valueMin="388.0">388–575</number>
(
<number box="[531,577,953,977]" pageId="5" pageNumber="6" value="471.0">471</number>
, n= 
<number box="[620,634,953,977]" pageId="5" pageNumber="6" value="6.0">6</number>
) long, 
<number box="[712,810,953,977]" pageId="5" pageNumber="6" value="394.0" valueMax="475.0" valueMin="313.0">313–475</number>
(
<number box="[825,869,953,977]" pageId="5" pageNumber="6" value="371.0">371</number>
) wide.
</paragraph>
