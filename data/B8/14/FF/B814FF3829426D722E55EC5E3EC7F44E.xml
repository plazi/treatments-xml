<paragraph pageId="704" pageNumber="705">
<taxonomicName LSID="B814FF38-2942-6D72-2E55-EC5E3EC7F44E" authority="Dejean, 1829" authorityName="Dejean" authorityYear="1829" class="Hexapoda" family="Carabidae" genus="Harpalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Harpalus solitaris" order="Coleoptera" pageId="704" pageNumber="705" phylum="Arthropoda" rank="species" species="solitaris">
