<taxonomicName authority="Lelej" authorityName="Lelej" box="[1308,1436,1473,1495]" class="Insecta" family="Mutillidae" genus="Nemka" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
