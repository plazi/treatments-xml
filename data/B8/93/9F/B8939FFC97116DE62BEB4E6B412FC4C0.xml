<taxonomicName LSID="http://zoobank.org/EC93B8A9-1413-4EB1-B139-63AF641FD6E3" authority="Liao, Wang &amp; Che, 2019" authorityName="Liao, Wang &amp; Che" authorityYear="2019" class="Insecta" family="Blattidae" genus="Laevifacies" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Laevifacies" order="Blattodea" pageId="3" pageNumber="88" phylum="Arthropoda" rank="genus">Laevifacies</taxonomicName>
<taxonomicNameLabel pageId="3" pageNumber="88">gen. nov.</taxonomicNameLabel>
