<taxonomicName class="Insecta" family="Curculionidae" genus="Hylesinopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hylesinopsis ficus" order="Coleoptera" pageId="5" pageNumber="162" phylum="Arthropoda" rank="species" species="ficus">Hylesinopsis ficus</taxonomicName>
, 
<taxonomicName class="Insecta" family="Curculionidae" genus="Hylesinopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hylesinopsis joveri" order="Coleoptera" pageId="5" pageNumber="162" phylum="Arthropoda" rank="species" species="joveri">Hylesinopsis joveri</taxonomicName>
, 
