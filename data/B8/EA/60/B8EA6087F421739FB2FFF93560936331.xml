<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:8DF8C2F2-A5F1-4548-BC19-AD4E00BDC5AB" class="Actinopterygii" family="Rivulidae" genus="Austrofundulus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Austrofundulus transilis Myers 1932:160" order="Cyprinodontiformes" pageNumber="13" phylum="Chordata" rank="species" species="transilis">A. transilis</taxonomicName>
