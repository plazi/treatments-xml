<taxonomicName authorityName="Yang" authorityYear="1999" class="Insecta" family="Hymenopodidae" genus="Theopropus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Theopropus sinecus" order="Mantodea" pageId="0" pageNumber="163" phylum="Arthropoda" rank="species" species="sinecus">
