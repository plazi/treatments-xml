<quantity box="[733,917,1653,1691]" metricMagnitude="-2" metricUnit="m" metricValue="2.35" metricValueMax="2.4" metricValueMin="2.3" pageId="7" pageNumber="368" unit="mm" value="23.5" valueMax="24.0" valueMin="23.0">23-24 mm</quantity>
