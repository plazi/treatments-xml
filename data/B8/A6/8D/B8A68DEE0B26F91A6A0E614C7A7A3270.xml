<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181903" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole mincana Wilson" lsidName-HNS="Pheidole mincana Wilson" order="Hymenoptera" pageNumber="457" phylum="Arthropoda" rank="species" species="mincana">Pheidole mincana</taxonomicName>
