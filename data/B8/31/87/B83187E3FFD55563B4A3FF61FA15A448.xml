<subSubSection pageId="12" pageNumber="471" type="biology_ecology">
<paragraph blockId="12.[151,1437,258,1651]" pageId="12" pageNumber="471">
<emphasis bold="true" box="[199,287,1374,1399]" pageId="12" pageNumber="471">Biology</emphasis>
