<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:830D572B-79ED-4DBD-84EC-A10E3E7E748C" class="Actinopterygii" family="Ariidae" genus="Arius" kingdom="Animalia" lsidName-ZBK="Arius Valenciennes 1840:53" order="Siluriformes" pageNumber="57" phylum="Chordata" rank="genus">Arius</taxonomicName>
