<emphasis id="8E96EAA3FFFEFFFDD25FFF2AFF1AFF52" bold="true" box="[113,284,219,244]" pageId="3" pageNumber="606">Current status.</emphasis>
<taxonomicName id="7BE24D32FFFEFFFDD30EFF2CFD28FF52" authority="(Schenkling, 1932)" baseAuthorityName="Schenkling" baseAuthorityYear="1932" box="[288,814,220,244]" class="Insecta" family="Cleridae" genus="Xenorthrius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="606" phylum="Arthropoda" rank="species" species="abruptepunctatus">
