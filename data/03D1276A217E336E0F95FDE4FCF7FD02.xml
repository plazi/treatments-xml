<taxonomicName id="4C78EDFF2171336F0F95F898FD08F8E3" authority=": Liu &amp; Yin, 2004" authorityName="Liu &amp; Yin" authorityYear="2004" box="[151,661,1815,1839]" class="Insecta" family="Gryllacrididae" genus="Diaphanogryllacris" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="199" phylum="Arthropoda" rank="species" species="curvicauda">
