<taxonomicName id="AB3584E1BD36100E04B4B289265BA242" class="Bivalvia" family="Lucinidae" genus="Funafutia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Funafutia levukana" order="Lucinida" pageId="0" pageNumber="109" phylum="Mollusca" rank="species" species="levukana">
<emphasis id="78D62829FD098449EC038AFDC749D782" italics="true" pageId="0" pageNumber="109">Funafutia levukana</emphasis>
</taxonomicName>
(
<taxonomicName id="2235D15E082FB50BDBF627A425C88F9C" lsidName="" pageId="0" pageNumber="109" rank="subfamily" subfamily="Lucininae">Lucininae</taxonomicName>
