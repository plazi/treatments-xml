, 
<taxonomicName id="15DB0294B52E3AE518ADB150C30F9116" class="Bivalvia" family="Lucinidae" genus="Epicodakia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Epicodakia" order="Lucinida" pageId="0" pageNumber="109" phylum="Mollusca" rank="genus">
<emphasis id="F97800E1461056901156CC4B9DE134F6" italics="true" pageId="0" pageNumber="109">Epicodakia</emphasis>
</taxonomicName>
and 
<taxonomicName id="D83C05EA029EC7A6E375AF4978F60CEB" class="Bivalvia" family="Lucinidae" genus="Codakia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Codakia" order="Lucinida" pageId="0" pageNumber="109" phylum="Mollusca" rank="genus">
<emphasis id="88ABDC86E4D98795E1640CB058A67F04" italics="true" pageId="0" pageNumber="109">Codakia</emphasis>
</taxonomicName>
