<taxonomicName id="88CA41B2AEA2318E3654345C66408339" class="Polyplacophora" family="Chitonidae" genus="Chiton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chiton granosus" order="Chitonida" pageId="0" pageNumber="45" phylum="Mollusca" rank="species" species="granosus">Chiton granosus</taxonomicName>
