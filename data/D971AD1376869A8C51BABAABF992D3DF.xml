<taxonomicName id="8A83C3995B5AC1039781BE454F0934A9" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia gerrardii" order="Fabales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="species" species="gerrardii">
