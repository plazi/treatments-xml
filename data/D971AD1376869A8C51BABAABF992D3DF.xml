<taxonomicName id="4A2C5B1832DA729CA40142391FEC1CCF" authorityName="Roem &amp; Schult" authorityYear="1819" class="Dicotyledoneae" family="Solanaceae" genus="Lycium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Lycium shawii" order="Tubiflorae" pageId="62" pageNumber="63" phylum="Angiospermae" rank="species" species="shawii">
