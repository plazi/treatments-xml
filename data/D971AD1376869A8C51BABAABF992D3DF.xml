<taxonomicName id="FCA57FA36732AA1F888A3ED1CC163EF6" class="Dicotyledoneae" family="Apocynaceae" genus="Calotropis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Calotropis procera" order="Gentianales" pageId="62" pageNumber="63" phylum="Angiospermae" rank="species" species="procera">
