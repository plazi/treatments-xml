<date id="FD401003A22B5737FF74F207DB69F9EC" box="[221,310,1603,1627]" pageId="6" pageNumber="43" value="2018-09">IX.2018</date>
, coll. 
<collectionCode id="EFEFAE06A22B5737FEDEF200DBD4F9EC" box="[375,395,1604,1627]" country="Chile" name="Departamento de Geologia, Universidad de Chile" pageId="6" pageNumber="43">R</collectionCode>
