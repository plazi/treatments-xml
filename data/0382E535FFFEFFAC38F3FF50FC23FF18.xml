<emphasis id="B95F8831FFFFFFAF3AFDF8C6FCDAF8FA" bold="true" box="[665,868,1793,1818]" pageId="5" pageNumber="132">
<collectingCountry id="F33C14B3FFFFFFAF3AFDF8C6FD44F8FA" box="[665,762,1793,1818]" name="China" pageId="5" pageNumber="132">CHINA</collectingCountry>
