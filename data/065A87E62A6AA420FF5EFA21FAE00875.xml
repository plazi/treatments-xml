<elevation id="05DED1C32A6AA423FD73F99FFCBB0C37" box="[698,813,1538,1561]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="25" pageNumber="26" unit="m" value="5.0">
<quantity id="490B9B152A6AA423FD73F99FFD700C37" box="[698,742,1538,1561]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="25" pageNumber="26" unit="m" value="5.0">5 m</quantity>
