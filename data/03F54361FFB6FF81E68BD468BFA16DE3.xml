<emphasis id="B9282E65FFB6FF81E504D51CBE916D3A" box="[792,1000,819,843]" italics="true" pageId="14" pageNumber="18">
<taxonomicName id="4C5C89F4FFB6FF81E504D51CBE006D3A" box="[792,889,819,843]" class="Clitellata" family="Acanthodrilidae" genus="Eodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="14" pageNumber="18" phylum="Annelida" rank="genus">Eodrilus</taxonomicName>
