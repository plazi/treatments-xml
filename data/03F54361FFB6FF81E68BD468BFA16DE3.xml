and 
<taxonomicName id="4C5C89F4FFB6FF81E789D577BFFF6D1F" box="[405,646,854,879]" class="Clitellata" family="Acanthodrilidae" genus="Eodrilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crassiclitellata" pageId="14" pageNumber="18" phylum="Annelida" rank="subSpecies" species="avelinus" subSpecies="arenicola">
