(
<figureCitation id="135D2A69FF8AFFEBAF06FBE5FEBA7E0A" box="[272,341,1039,1065]" captionStart="FIGURE 1" captionStartId="7.[264,367,1692,1716]" captionTargetBox="[272,1312,569,1681]" captionTargetId="figure@7.[272,1314,569,1683]" captionTargetPageId="7" captionText="FIGURE 1. Dorsal habitus of species of Onega Distant. (A) O. avella Distant, male, LACM; (B) O. bracteata Young, male, DZRJ; (C) O. fassli Young, male, INHS; (D) O. freytagi sp. nov., male holotype, UKL; (E) O. krameri sp. nov., male holotype, USNM; (F) O. orphne sp. nov., male holotype, USNM; (G) O. sanguinicollis (Latreille) comb. nov., female, NHMW; (H) O. stipata (Walker), male, OSU. Scale bar = 2.0 mm." httpUri="https://zenodo.org/record/169499/files/figure.png" pageId="14" pageNumber="15">
Figs 
<number id="3DAE24CBFF8AFFEBAF5DFBE5FEBA7E0A" box="[331,341,1039,1065]" pageId="14" pageNumber="15" value="1.0">1</number>
</figureCitation>
G, 
<number id="3DAE24CBFF8AFFEBAF6CFBE5FE667E0A" box="[378,393,1039,1065]" pageId="14" pageNumber="15" value="5.0">5</number>
)
