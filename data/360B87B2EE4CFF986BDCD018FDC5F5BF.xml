<subSubSection id="F6B8652FEE4CFF986B8DD40BFBF1F631" box="[198,1049,1355,1382]" pageId="10" pageNumber="11" type="distribution">
<paragraph id="BE1D36A4EE4CFF986B8DD40BFBF1F631" blockId="10.[151,1436,582,1768]" box="[198,1049,1355,1382]" pageId="10" pageNumber="11">
<emphasis id="8CD6EAB6EE4CFF986B8DD40BFEB7F632" bold="true" box="[198,351,1355,1381]" pageId="10" pageNumber="11">Distribution</emphasis>
:— 
<collectingCountry id="C6B57634EE4CFF986AC8D40CFE27F631" box="[387,463,1356,1382]" name="Brazil" pageId="10" pageNumber="11">Brazil</collectingCountry>
