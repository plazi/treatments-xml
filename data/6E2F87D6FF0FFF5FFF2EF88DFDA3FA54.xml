<collectingCountry id="9E917650FF0CFF5FFF5FFF69FE96FBC0" box="[230,359,189,215]" name="Philippines" pageId="142" pageNumber="143">Philippines</collectingCountry>
, New 
<collectingCountry id="9E917650FF0CFF5FFE17FF69FE0EFBC0" box="[430,511,189,215]" name="Guinea-Bissau" pageId="142" pageNumber="143">Guinea</collectingCountry>
