<normalizedToken id="3BAFC00E1644672D913FD1E94BBD7ED1" originalValue="Grundständige" pageId="null" pageNumber="175">Grundstaendige</normalizedToken>
<normalizedToken id="AC95C2E5E31689B43B62843895435F84" originalValue="Blätter" pageId="null" pageNumber="175">Blaetter</normalizedToken>
