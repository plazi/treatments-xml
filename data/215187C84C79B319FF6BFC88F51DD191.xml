<taxonomicName id="6EF84D5D4C79B319FF1BFBD0F6D7D179" authorityName="Bierig" authorityYear="1937" box="[231,386,1028,1053]" class="Insecta" family="Staphylinidae" genus="Pterygolaetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="genus">
