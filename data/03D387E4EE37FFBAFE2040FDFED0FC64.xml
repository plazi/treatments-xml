<taxonomicName id="4C7A4D71EE35FFBAFC0247A8FBCEFD04" authority="Švihla, 2009" authorityName="Švihla" authorityYear="2009" box="[905,1088,733,756]" class="Insecta" family="Cantharidae" genus="Malthodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="210" phylum="Arthropoda" rank="species" species="anadyomenae" status="sp. nov.">
