<paragraph id="CA103B42E827FFF1FDB4B877FA78F91A" blockId="5.[491,1519,1571,1733]" pageId="5" pageNumber="65">
<taxonomicName id="0DAF40C1E827FFF1FDB4B877FC0CF982" authorityName="Ivan and Vasiliu" authorityYear="1999" box="[531,923,1571,1595]" class="Arachnida" family="Oppiidae" genus="Multioppia" kingdom="Animalia" order="Sarcoptiformes" pageId="5" pageNumber="65" phylum="Arthropoda" rank="species" species="insolita" subGenus="Hammeroppia">
<emphasis id="F8DBE750E827FFF1FDB4B877FD18F982" box="[531,655,1571,1595]" italics="true" pageId="5" pageNumber="65">Multioppia</emphasis>
(
<emphasis id="F8DBE750E827FFF1FD3CB870FCA3F982" box="[667,820,1572,1595]" italics="true" pageId="5" pageNumber="65">Hammeroppia</emphasis>
) 
<emphasis id="F8DBE750E827FFF1FCE2B877FC0CF982" box="[837,923,1571,1595]" italics="true" pageId="5" pageNumber="65">insolita</emphasis>
</taxonomicName>
