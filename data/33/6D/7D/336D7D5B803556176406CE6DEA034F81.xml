<paragraph pageId="0" pageNumber="8151">
<taxonomicName authority="Fabricius, 1804" authorityName="Fabricius" authorityYear="1804" class="Insecta" family="Braconidae" genus="Bracon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bracon" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Bracon Fabricius, 1804</taxonomicName>
