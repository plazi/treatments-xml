<collectingCountry box="[227,328,1228,1252]" name="Trinidad and Tobago" pageId="5" pageNumber="53">Trinidad</collectingCountry>
; 
<collectingCountry box="[340,412,1228,1252]" name="Brazil" pageId="5" pageNumber="53">Brazil</collectingCountry>
; 
<collectingCountry box="[423,540,1228,1252]" name="Argentina" pageId="5" pageNumber="53">Argentina</collectingCountry>
).
</paragraph>
