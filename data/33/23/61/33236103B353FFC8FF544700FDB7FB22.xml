<specimenCount box="[861,961,1156,1180]" pageId="5" pageNumber="53" type="female">
<number box="[861,875,1156,1180]" pageId="5" pageNumber="53" value="1.0">1</number>
female
</specimenCount>
, and nymphs. Note 
<number box="[260,277,1191,1215]" pageId="5" pageNumber="53" value="2.0">2</number>
