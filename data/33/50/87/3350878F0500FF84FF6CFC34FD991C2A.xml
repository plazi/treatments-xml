<taxonomicName baseAuthorityName="Avibase" baseAuthorityYear="2012" box="[217,314,1042,1065]" class="Aves" family="Aves (awaiting allocation)" genus="Riccordia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aves (awaiting allocation)" pageId="19" pageNumber="420" phylum="Chordata" rank="species" species="bracei">
