<emphasis bold="true" box="[618,678,1920,1944]" pageId="8" pageNumber="61">
<taxonomicNameLabel box="[618,678,1920,1944]" pageId="8" pageNumber="61" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
