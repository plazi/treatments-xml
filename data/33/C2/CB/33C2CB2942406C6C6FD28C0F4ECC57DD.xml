<paragraph pageId="8" pageNumber="83">
<taxonomicName class="Arachnida" family="Theraphosidae" genus="Sericopelma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sericopelma panamana" order="Araneae" pageId="8" pageNumber="83" phylum="Arthropoda" rank="species" species="panamana">Sericopelma panamana</taxonomicName>
