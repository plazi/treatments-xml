<paragraph blockId="1.[151,678,1525,1551]" box="[151,678,1525,1551]" pageId="1" pageNumber="2">
<heading bold="true" box="[151,678,1525,1551]" fontSize="11" level="1" pageId="1" pageNumber="2" reason="1">
<emphasis bold="true" box="[151,678,1525,1551]" pageId="1" pageNumber="2">
Place of 
<taxonomicName box="[257,381,1525,1551]" class="Diplopoda" family="Glomeridae" genus="Mauriesia" kingdom="Animalia" order="Glomerida" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[257,381,1525,1551]" italics="true" pageId="1" pageNumber="2">Mauriesia</emphasis>
</taxonomicName>
in the order 
<taxonomicName box="[543,678,1525,1551]" class="Diplopoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Glomerida" pageId="1" pageNumber="2" phylum="Arthropoda" rank="order">Glomerida</taxonomicName>
</emphasis>
