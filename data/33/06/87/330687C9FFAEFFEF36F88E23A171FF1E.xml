<subSubSection pageId="43" pageNumber="44" type="discussion">
<paragraph blockId="43.[151,1436,151,248]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[199,335,151,176]" pageId="43" pageNumber="44">Comments.</emphasis>
<taxonomicName box="[346,587,152,175]" class="Insecta" family="Braconidae" genus="Centistes" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="zurquiensis">
<emphasis box="[346,587,152,175]" italics="true" pageId="43" pageNumber="44">Centistes zurquiensis</emphasis>
</taxonomicName>
and 
<taxonomicName box="[651,769,152,175]" class="Insecta" family="Braconidae" genus="Centistes" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="janzeni">
<emphasis box="[651,769,152,175]" italics="true" pageId="43" pageNumber="44">C. janzeni</emphasis>
</taxonomicName>
