<subSubSection box="[199,1332,476,501]" pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[151,1436,151,501]" box="[199,1332,476,501]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[199,334,476,500]" pageId="21" pageNumber="22">Etymology.</emphasis>
