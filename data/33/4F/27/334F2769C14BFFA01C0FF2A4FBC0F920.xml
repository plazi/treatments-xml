<paragraph blockId="40.[189,248,1559,1585]" box="[189,248,1559,1585]" pageId="40" pageNumber="41">
<heading bold="true" box="[189,248,1559,1585]" fontSize="11" level="3" pageId="40" pageNumber="41" reason="3">
<emphasis bold="true" box="[189,248,1559,1585]" pageId="40" pageNumber="41">Note</emphasis>
