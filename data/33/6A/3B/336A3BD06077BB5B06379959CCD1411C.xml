<subSubSection pageId="null" pageNumber="479" type="nomenclature">
<paragraph pageId="null" pageNumber="479">
<taxonomicName authority="L." class="Magnoliopsida" family="Asteraceae" genus="Gnaphalium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="479" phylum="Tracheophyta" rank="species" species="uliginosum">
Gnaphalium 
<normalizedToken originalValue="uliginósum" pageId="null" pageNumber="479">uliginosum</normalizedToken>
<authorityName pageId="null" pageNumber="479">L.</authorityName>
</taxonomicName>
</paragraph>
