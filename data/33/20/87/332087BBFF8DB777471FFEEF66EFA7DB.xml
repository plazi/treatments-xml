<typeStatus box="[1315,1402,1079,1099]" pageId="14" pageNumber="15">Holotype</typeStatus>
(
<specimenCount box="[1419,1435,1077,1099]" pageId="14" pageNumber="15" type="male">♂</specimenCount>
