<subSubSection pageId="5" pageNumber="586" type="diagnosis">
<paragraph blockId="5.[151,1436,541,710]" pageId="5" pageNumber="586">
<emphasis bold="true" box="[199,321,613,638]" pageId="5" pageNumber="586">Diagnosis.</emphasis>
