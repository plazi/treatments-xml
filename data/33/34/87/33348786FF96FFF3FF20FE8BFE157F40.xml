<taxonomicName class="Actinopterygii" family="Nemacheilidae" genus="Oxynoemacheilus" kingdom="Animalia" order="Cypriniformes" pageId="10" pageNumber="43" phylum="Chordata" rank="species" species="argyrogramma">
<emphasis italics="true" pageId="10" pageNumber="43">
<collectingCountry box="[1409,1432,944,967]" name="Iceland" pageId="10" pageNumber="43">O</collectingCountry>
