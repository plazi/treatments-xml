<taxonomicName id="7B05407E592851E7F8DC7AB4B13C2329" authorityName="Olivier" authorityYear="1804" class="Gastropoda" family="Melanopsidae" genus="Melania" kingdom="Animalia" lsidName="Melania costata" order="Pteropoda" pageId="64" pageNumber="65" phylum="Mollusca" rank="species" species="costata">
