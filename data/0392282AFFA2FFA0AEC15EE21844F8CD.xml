<taxonomicName id="4C3BE2BFFFA2FFA0AAE059DF1C1CF975" authorityName="Clarke" authorityYear="1879" box="[1206,1325,1766,1791]" class="Hydrozoa" family="Campanulariidae" genus="Obelia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="68" pageNumber="69" phylum="Cnidaria" rank="species" species="hyalina">
