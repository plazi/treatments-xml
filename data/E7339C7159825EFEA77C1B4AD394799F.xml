<subSubSection id="467365DD38AD117EBB337A7265D470DD" pageId="0" pageNumber="51" type="materials_examined">
<paragraph id="36C697625A9028B2C00E1D0CE4267ACD" pageId="0" pageNumber="51">Material examined.</paragraph>
<paragraph id="A711BD422E40A15C921EA6F9BCA6A660" pageId="0" pageNumber="51">
<emphasis id="B5A2F1E7D333290F6FDD6BB2DDDD18DF" bold="true" italics="true" pageId="0" pageNumber="51">Holotype</emphasis>
