<taxonomicName id="49C04D6BFFA4F7648683A0AAFED9F973" box="[151,261,1693,1714]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="31" pageNumber="34" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
