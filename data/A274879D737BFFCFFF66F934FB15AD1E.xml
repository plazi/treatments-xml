<taxonomicName id="EDDD4D087379FFCFFD1AFD56FC96AD34" box="[747,855,749,774]" class="Insecta" family="Encyrtidae" genus="Copidosomopsis" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="61" phylum="Arthropoda" rank="species" species="indica">
