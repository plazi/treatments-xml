<taxonomicName id="EDDD4D087379FFCFFA70FD73FDC1AD35" authority="Caltagirone, 1966" authorityName="Caltagirone" authorityYear="1966" class="Insecta" family="Encyrtidae" genus="Copidosomopsis" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="61" phylum="Arthropoda" rank="species" species="plethorica">
