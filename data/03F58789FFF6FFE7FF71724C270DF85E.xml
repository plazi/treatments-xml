<number id="3D9424B8FFF6FFE7FCA3765D271AF8CE" box="[837,850,1806,1830]" pageId="7" pageNumber="35" value="1.0">1</number>
: 
<number id="3D9424B8FFF6FFE7FCBB765D27C2F8CE" box="[861,906,1806,1830]" pageId="7" pageNumber="35" value="375.0">375</number>
, t 
<number id="3D9424B8FFF6FFE7FC78765D27E6F8CE" box="[926,942,1806,1830]" pageId="7" pageNumber="35" value="2.0">2</number>
: 
<number id="3D9424B8FFF6FFE7FC5F765D279FF8CE" box="[953,983,1806,1830]" pageId="7" pageNumber="35" value="95.0">95</number>
, ctt 
<number id="3D9424B8FFF6FFE7FBE0765D2058F8CE" box="[1030,1040,1806,1830]" pageId="7" pageNumber="35" value="1.0">1</number>
: 
<number id="3D9424B8FFF6FFE7FBFD765D2071F8CE" box="[1051,1081,1806,1830]" pageId="7" pageNumber="35" value="19.0">19</number>
, Mx 
<number id="3D9424B8FFF6FFE7FB89765D2036F8CE" box="[1135,1150,1806,1830]" pageId="7" pageNumber="35" value="4.0">4</number>
: 
