<taxonomicName id="8FEF7D8ACB2D0D34FB295B1AFE6A8A76" authority="Malm &amp; Johanson, 2008" authorityName="Malm &amp; Johanson" authorityYear="2008" box="[1225,1315,870,889]" class="Insecta" family="Leptoceridae" genus="Gracilipsodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="429" phylum="Arthropoda" rank="species" species="robustus" status="sp. nov.">
