<date id="954C80E3F60BFFEFA9E4E010FBA8FCBD" box="[1023,1044,827,855]" pageId="1" pageNumber="321">
<emphasis id="D3867A31F60BFFEFA9E4E010FBB7FCB8" box="[1023,1035,827,850]" italics="true" pageId="1" pageNumber="321">v</emphasis>
<number id="573AB404F60BFFEFAE17E062FBA8FCBD" box="[1036,1044,841,855]" pageId="1" pageNumber="321" value="2.0">2</number>
</date>
, and 
<emphasis id="D3867A31F60BFFEFAE4CE010FBD2FCB8" box="[1111,1134,827,850]" italics="true" pageId="1" pageNumber="321">sc</emphasis>
<number id="573AB404F60BFFEFAE75E062FBCAFCBD" box="[1134,1142,841,855]" pageId="1" pageNumber="321" value="2.0">2</number>
