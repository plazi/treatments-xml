<emphasis id="D3867A31F60BFFEFAB77E049FE3FFC93" box="[364,387,866,889]" italics="true" pageId="1" pageNumber="321">sc</emphasis>
<number id="573AB404F60BFFEFAB98E05BFE37FC94" box="[387,395,880,894]" pageId="1" pageNumber="321" value="1.0">1</number>
