<collectingRegion id="49ADF8384858FF93FF4AFEE2FEB72923" box="[151,332,332,357]" country="Brazil" name="Distrito Federal" pageId="4" pageNumber="405">Distrito Federal</collectingRegion>
. 
<taxonomicName id="4C694D594858FF93FE8BFEE2FD4F2922" box="[342,692,332,357]" class="Reptilia" family="Scincidae" genus="Copeoglossum" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="405" phylum="Chordata" rank="species" species="nigropunctatum">
<emphasis id="B91DEAC84858FF93FE8BFEE2FD4F2922" box="[342,692,332,357]" italics="true" pageId="4" pageNumber="405">Copeoglossum nigropunctatum</emphasis>
</taxonomicName>
