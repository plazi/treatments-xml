<paragraph id="8BC536E8D238FFAFFF66C899BA86FB9B" blockId="3.[146,1103,977,1706]" pageId="3" pageNumber="798">
<materialsCitation id="3B123CB5D238FFAFFF66C899BA66FB9B" latitude="114.0" location="One" municipality="Fan Tsang Chau (Conic Island) Cave" pageId="3" pageNumber="798" specimenCount="1">
<location id="8EA56033D238FFAFFF66C899BBE8FC7B" LSID="urn:lsid:plazi:treatment:03D387FED238FFAFFE4DC810B942F938:8EA56033D238FFAFFF66C899BBE8FC7B" box="[146,193,977,1001]" latitude="114.0" municipality="Fan Tsang Chau (Conic Island) Cave" name="One" pageId="3" pageNumber="798">One</location>
