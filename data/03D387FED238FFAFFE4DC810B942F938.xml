<collectingMunicipality id="6BA1AC92D238FFAFFD9CC899BF63FC7B" box="[616,1098,977,1001]" pageId="3" pageNumber="798">Fan Tsang Chau (Conic Island) Cave</collectingMunicipality>
, 22 
<emphasis id="B90EEAFAD238FFAFFF59C8B9BB9EFB9B" bold="true" box="[173,183,1009,1033]" italics="true" pageId="3" pageNumber="798">°</emphasis>
219540 
<geoCoordinate id="EE4E502FD238FFAFFF0AC8B9BA66FB9B" box="[254,335,1009,1033]" degrees="114" direction="north" orientation="latitude" pageId="3" pageNumber="798" precision="55555" value="114.0">
