<taxonomicName id="4C0F4D17243DFFADFD44F8D9A06EF8AD" authority="Ritsema" authorityName="Ritsema" box="[656,775,1844,1867]" pageId="7" pageNumber="64" rank="species" species="unifasciata">unifasciata</taxonomicName>
