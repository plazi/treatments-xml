<taxonomicName id="4C494D486D5CDE761FF8F8722382E3B9" authority="Park, 1970" authorityName="Park" authorityYear="1970" box="[737,1012,2010,2035]" class="Maxillopoda" family="Discoidae" genus="Disco" higherTaxonomySource="GBIF" kingdom="Animalia" order="Calanoida" pageId="5" pageNumber="38" phylum="Arthropoda" rank="species" species="oviformis">
