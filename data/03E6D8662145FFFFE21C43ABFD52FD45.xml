<collectingRegion id="498BA7922145FFFFE20941D9FE67FD45" box="[284,485,733,757]" country="Indonesia" name="Kalimantan Tengah" pageId="4" pageNumber="85">Kalimantan Tengah</collectingRegion>
(
<collectingRegion id="498BA7922145FFFFE2E641D9FD47FD45" box="[499,709,733,757]" country="Indonesia" name="Kalimantan Tengah" pageId="4" pageNumber="85">Central Kalimantan</collectingRegion>
