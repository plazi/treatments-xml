<paragraph id="B852B484165CB4AC81C98FB68238EA9C" pageId="null" pageNumber="269">
<taxonomicName id="DD872E103C20886E40C8ACB39CFE1187" authority="L." class="Liliopsida" family="Poaceae" genus="Alopecurus" kingdom="Plantae" order="Poales" pageId="null" pageNumber="269" phylum="Tracheophyta" rank="species" species="pratensis">
<pageBreakToken id="803BAA54442365FC9275266243CC8769" pageId="null" pageNumber="269" start="start">Alopecurus</pageBreakToken>
<normalizedToken id="2DBFB57381CC7F94BD7E64F4414A943E" originalValue="praténsis" pageId="null" pageNumber="269">pratensis</normalizedToken>
<authorityName id="B4341A168F25C097F587733970CA876F" pageId="null" pageNumber="269">L.</authorityName>
