<taxonomicName id="1E9D4D0E1D63392EFC39FDCEFBBDFD2F" box="[992,1132,620,645]" class="Insecta" family="Geometridae" genus="Prasinocyma" kingdom="Animalia" order="Lepidoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="aetheraea">
