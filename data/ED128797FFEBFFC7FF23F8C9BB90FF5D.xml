<taxonomicName id="A2BB4D02FFEBFFD9FF23F8C9B9DEF88D" authority="Rondani, 1840" authorityName="Rondani" authorityYear="1840" box="[151,498,1809,1836]" class="Insecta" family="Cecidomyiidae" genus="Porricondyla" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="166" phylum="Arthropoda" rank="genus">
<emphasis id="57CFEA93FFEBFFD9FF23F8C9B9DEF88D" bold="true" box="[151,498,1809,1836]" pageId="15" pageNumber="166">
<emphasis id="57CFEA93FFEBFFD9FF23F8C9B919F88A" bold="true" box="[151,309,1809,1836]" italics="true" pageId="15" pageNumber="166">Porricondyla</emphasis>
