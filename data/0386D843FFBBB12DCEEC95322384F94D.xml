<taxonomicName id="4C2F12D6FFBCB12FCC609337246BFC48" box="[539,637,332,356]" class="Insecta" family="Hesperiidae" genus="Osmodes" kingdom="Animalia" order="Lepidoptera" pageId="28" pageNumber="229" phylum="Arthropoda" rank="genus">
<emphasis id="B95BB547FFBCB12FCC609337246BFC48" box="[539,637,332,356]" italics="true" pageId="28" pageNumber="229">Osmodes</emphasis>
