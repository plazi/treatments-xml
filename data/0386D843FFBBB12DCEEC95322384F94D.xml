spp. (all 
<taxonomicName id="4C2F12D6FFBCB12FCC9A93372578FC4F" box="[737,878,332,355]" class="Liliopsida" family="Marantaceae" kingdom="Plantae" order="Zingiberales" pageId="28" pageNumber="229" phylum="Tracheophyta" rank="family">Marantaceae</taxonomicName>
