<location id="A24408A8FFECFFC1AC4D24AAFE44FE6D" LSID="urn:lsid:plazi:treatment:2F32EF65FFECFFC1AD972580FBA3FCF9:A24408A8FFECFFC1AC4D24AAFE44FE6D" box="[333,484,435,461]" country="Vietnam" county="Central" municipality="Annamite Mts." name="Bach Ma Mt." pageId="5" pageNumber="160" stateProvince="Thua Thien-Hue Province">Bach Ma Mt.</location>
, 
<quantity id="6063F396FFECFFC1ACF024AAFDE0FE6D" box="[496,576,435,461]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="5" pageNumber="160" unit="m" value="1400.0">
