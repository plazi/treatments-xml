<taxonomicName id="4C644D75FF97C61870A9FB12FF5CFACF" authorityName="Emery" authorityYear="1878" class="Insecta" family="Formicidae" genus="Leptothorax" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="249" phylum="Arthropoda" rank="species" species="nigrita">
