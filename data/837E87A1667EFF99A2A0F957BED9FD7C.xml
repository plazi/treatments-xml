<caption id="5FA8663F667FFF9EA2A0F8E9B8F1F833" httpUri="https://zenodo.org/record/217700/files/figure.png" pageId="27" pageNumber="328" targetBox="[205,1383,210,1849]" targetPageId="27">
<paragraph id="0B6836B7667FFF9EA2A0F8E9B8F1F833" blockId="27.[151,1436,1899,1983]" pageId="27" pageNumber="328">
<emphasis id="39A3EAA5667FFF9EA2A0F8E9BD50F80C" bold="true" box="[151,287,1899,1921]" pageId="27" pageNumber="328">FIGURE 14.</emphasis>
<taxonomicName id="CCD74D34667FFF9EA313F8EEBDFBF80D" box="[292,436,1900,1921]" class="Insecta" family="Hydropsychidae" genus="Macrostemum" kingdom="Animalia" order="Trichoptera" pageId="27" pageNumber="328" phylum="Arthropoda" rank="genus">
