<number id="FB02AB3AFFDC3A70FF216C0AF099F9DB" box="[151,182,1555,1581]" pageId="14" pageNumber="15" value="10.0">10</number>
. 
<taxonomicName id="8ACAC29EFFDC3A70FF756C0DF21AF9DB" authority="Fall, 1916" authorityName="Fall" authorityYear="1916" box="[195,565,1555,1582]" class="Insecta" family="Derodontidae" genus="Laricobius" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="laticollis">
<emphasis id="7FBE650FFFDC3A70FF756C0DF19CF9D8" bold="true" box="[195,435,1556,1582]" italics="true" pageId="14" pageNumber="15">Laricobius laticollis</emphasis>
