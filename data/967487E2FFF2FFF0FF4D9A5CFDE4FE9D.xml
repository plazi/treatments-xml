<taxonomicName id="D9DD4D77FFF2FFF1FEBE9A93FF6DFB6E" authority="Cresson" authorityName="Cresson" box="[356,637,1178,1203]" class="Insecta" family="Ephydridae" genus="Setacera" kingdom="Animalia" order="Diptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="durani">
