(
<collectionCode id="AFF0FBC1046DFFEEFE7CF89F7405175B" box="[472,535,1874,1900]" pageId="61" pageNumber="62">AZO</collectionCode>
: 
<elevation id="42CC8437046DFFEEFD86F89C74DC175B" box="[546,718,1873,1900]" metricMagnitude="0" metricUnit="m" metricValue="2.5564999999999998" metricValueMax="3.05" metricValueMin="2.063" pageId="61" pageNumber="62" unit="m" value="2.5564999999999998" valueMax="3.05" valueMin="2.063">
