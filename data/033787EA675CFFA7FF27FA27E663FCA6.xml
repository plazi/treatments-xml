</taxonomicName>
as “dwarf 
<taxonomicName id="4C9E4D7F675CFFA5FC35FC3CE30EFB80" authorityName="Montfort" authorityYear="1808" box="[938,1044,1008,1034]" class="Cephalopoda" family="Amaltheidae" genus="Amaltheus" kingdom="Animalia" order="Ammonoidea" pageId="13" pageNumber="322" phylum="Mollusca" rank="genus">
