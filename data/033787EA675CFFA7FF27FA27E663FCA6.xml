<taxonomicName id="4C9E4D7F675CFFA5FD20FC3CE45AFB80" authorityName="Buckman" authorityYear="1913" box="[703,832,1008,1034]" class="Cephalopoda" family="Amaltheidae" genus="Amauroceras" kingdom="Animalia" order="Ammonoidea" pageId="13" pageNumber="322" phylum="Mollusca" rank="genus">
