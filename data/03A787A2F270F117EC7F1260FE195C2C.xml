<location id="8ED1606FF270F116ECC513CEFB9158CB" LSID="urn:lsid:plazi:treatment:03A787A2F270F117EC7F1260FE195C2C:8ED1606FF270F116ECC513CEFB9158CB" box="[927,1040,1442,1464]" municipality="Roda" name="Rodathal" pageId="68" pageNumber="68" stateProvince="Rivers">Rodathal</location>
