<emphasis id="B971EA84FF96FFF68052089C71DF4ABF" box="[1109,1202,137,161]" italics="true" pageId="11" pageNumber="324">Aselliscus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C054D15FF96FFF680E8089C70514ABF" box="[1263,1340,137,161]" class="Mammalia" family="Hipposideridae" genus="Coelops" kingdom="Animalia" order="Chiroptera" pageId="11" pageNumber="324" phylum="Chordata" rank="genus">
