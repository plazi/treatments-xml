<taxonomicName id="EAAB4D66FFD5FFDBFF44FA4FFD60FA44" authority="Hewitson, 1877" authorityName="Hewitson" authorityYear="1877" box="[221,645,1481,1506]" class="Insecta" family="Hesperiidae" genus="Platylesches" kingdom="Animalia" order="Lepidoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="galesa">
