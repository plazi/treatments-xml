<geoCoordinate id="258223392C5B58727E28DBE8F141A954" direction="north" orientation="latitude" precision="5" value="45.6848">45.6848°N</geoCoordinate>
, 
<geoCoordinate id="E76B1D289EFCCAD4A23C7A40679A571F" direction="west" orientation="longitude" precision="5" value="-66.8821">66.8821°W</geoCoordinate>
