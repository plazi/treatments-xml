(F.); emerged 
<date id="FF92101F7443AA0844CBF89DFCD100C7" box="[654,828,1902,1928]" pageId="44" pageNumber="45" value="1950-06-28">
<collectingDate id="EFD6E9F77443AA0844CBF89DFCD100C7" box="[654,828,1902,1928]" pageId="44" pageNumber="45" value="1950-06-28">June 28, 1950</collectingDate>
