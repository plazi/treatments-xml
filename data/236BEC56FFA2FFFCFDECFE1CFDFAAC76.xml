<taxonomicName id="6CC226C3FFAFFFFCFF79FC2CFE8BAB88" authorityName="Wahlberg" authorityYear="1844" box="[223,363,979,1005]" class="Insecta" family="Dolichopodidae" genus="Thinophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
