<paragraph id="6ADBCC17288D2426C54AED0351B21349" pageId="0" pageNumber="1">
<taxonomicName id="0011FC48BDB53ED01021C6D909F6FDA7" authorityName="Wulfen in Jacq., Coll. 1: 237. 1786. [January-September" authorityYear="1786" class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dianthus sylvestris" order="Caryophyllales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="sylvestris">Dianthus sylvestris</taxonomicName>
