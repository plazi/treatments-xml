<taxonomicName id="98A3A22429CFDD448841639FF4CE39DF" LSID="C9556865-ABBB-520D-B352-9DA3807663A4" authorityName="Clemens" authorityYear="1860" class="Insecta" family="Heliozelidae" genus="Coptodisca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coptodisca lucifluella" order="Lepidoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="lucifluella">Coptodisca lucifluella</taxonomicName>
