<paragraph id="B97D087EFE9E485E6C6C1588B4420EDF" pageId="0" pageNumber="75">
<taxonomicName id="F75B247F634F227FA5F23AD455ECDB35" LSID="75B5741D-3538-55D0-AFAC-84D8F118A6CB" authority="Narakusumo &amp; Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus laratensis" order="Coleoptera" pageId="0" pageNumber="75" phylum="Arthropoda" rank="species" species="laratensis">Trigonopterus laratensis Narakusumo &amp; Riedel</taxonomicName>
