<taxonomicName id="08AC08E5FF9AFFC61A95FEA03459FE1E" authorityName="Rudolphi" authorityYear="1808" box="[729,855,447,473]" class="Trematoda" kingdom="Animalia" pageId="3" pageNumber="66" phylum="Platyhelminthes" rank="class">Trematoda</taxonomicName>
