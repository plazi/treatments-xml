<collectingCountry id="69AA766F9614FFACFD65EFFAA0952D97" box="[746,930,727,752]" name="Czech Republic" pageId="3">Czech Republic</collectingCountry>
, 
<collectingCountry id="69AA766F9614FFACFC3EEFFAA7242D97" box="[945,1043,727,752]" name="Slovakia" pageId="3">Slovakia</collectingCountry>
and 
