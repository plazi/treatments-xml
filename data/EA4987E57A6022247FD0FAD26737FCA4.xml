<taxonomicName id="A5E04D707A6122247DB1FF4F622DFEC1" baseAuthorityName="Vieillot" baseAuthorityYear="1818" box="[134,284,241,265]" class="Aves" family="Psittacidae" genus="Aprosmictus" kingdom="Animalia" order="Psittaciformes" pageId="21" pageNumber="484" phylum="Chordata" rank="species" species="jonquillaceus">P. jonquillaceus</taxonomicName>
