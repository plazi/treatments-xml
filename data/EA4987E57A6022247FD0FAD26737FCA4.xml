<taxonomicName id="A5E04D707A6022257F8AF9CF606EF941" baseAuthorityName="Vieillot" baseAuthorityYear="1818" box="[701,863,1649,1673]" class="Aves" family="Psittacidae" genus="Aprosmictus" kingdom="Animalia" order="Psittaciformes" pageId="20" pageNumber="483" phylum="Chordata" rank="species" species="jonquillaceus">Ps. jonquillaceus</taxonomicName>
