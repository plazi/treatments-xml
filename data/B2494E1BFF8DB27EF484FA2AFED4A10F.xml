. — 
<taxonomicName id="FDE0848EFF8DB27FF560F9D6FD81A338" authority="Hinde, 1900" authorityName="Hinde" authorityYear="1900" box="[297,613,1562,1586]" class="Calcarea" family="Minchinellidae" genus="Plectroninia" kingdom="Animalia" order="Lithonida" pageId="54" pageNumber="257" phylum="Porifera" rank="species" species="halli">
