<number id="3DD424E10A00AA45AE04F8D3FCC5B677" box="[828,856,1794,1818]" pageId="2" pageNumber="58" value="14.0">14</number>
marginal between asters, 
<number id="3DD424E10A00AA45A9B9F8D3FB36B677" box="[1153,1195,1794,1818]" pageId="2" pageNumber="58" value="5.5" valueMax="7.0" valueMin="4.0">4–7</number>
