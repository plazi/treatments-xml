<number id="3DD424E10A00AA45AF3FF96BFDCDB7BF" box="[519,592,1722,1746]" pageId="2" pageNumber="58" value="16.5" valueMax="17.0" valueMin="16.0">16–17</number>
; III, 
<number id="3DD424E10A00AA45AFBDF96BFD39B7BF" box="[645,676,1722,1746]" pageId="2" pageNumber="58" value="18.0">18</number>
