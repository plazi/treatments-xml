<taxonomicName id="3B5C08AE0BD5C75ECBF2FC98AD18EF1E" LSID="http://zoobank.org/C32D000A-111C-4E96-B0EC-4C1DBFA649AF" authority="Talamas" class="Insecta" family="Scelionidae" genus="Plaumannion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Plaumannion fistulosum" order="Hymenoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="fistulosum">Plaumannion fistulosum Talamas</taxonomicName>
<taxonomicNameLabel id="09BF650186EDBFF29F48C5EA2A958F54" pageId="26" pageNumber="27">sp. n.</taxonomicNameLabel>
