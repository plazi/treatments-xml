<location id="8EDA602FFFAEFFD8FEE7FB5138D8FB4F" LSID="urn:lsid:plazi:treatment:03AC87E2FFAEFFD8FF44FC5F3A57FE56:8EDA602FFFAEFFD8FEE7FB5138D8FB4F" box="[280,447,1237,1259]" country="Australia" county="Mathews Collection" latitude="-20.19" longLatPrecision="761" longitude="118.34" municipality="From" name="Port Hedland" pageId="21" pageNumber="21" stateProvince="Western Australia">Port Hedland</location>
(not Headland), 
<geoCoordinate id="EE315033FFAEFFD8FF7FFB7439B7FAAC" box="[128,208,1267,1288]" degrees="20.19" direction="south" orientation="latitude" pageId="21" pageNumber="21" precision="555" value="-20.19">20.19S</geoCoordinate>
