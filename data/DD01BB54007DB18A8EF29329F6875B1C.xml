<taxonomicName id="3983C7118CF005488A16DD0D682F192D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:238794" class="Insecta" family="Formicidae" genus="Vollenhovia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Vollenhovia kaselela Clouse" order="Hymenoptera" pageNumber="13" phylum="Arthropoda" rank="species" species="kaselela">Vollenhovia kaselela</taxonomicName>
