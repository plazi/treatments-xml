<taxonomicName id="684309C8D9B545250B1AA7618342924A" authorityName="Don" authorityYear="1825" class="Insecta" family="Noctuidae" genus="Neuronia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="asplenioides">
