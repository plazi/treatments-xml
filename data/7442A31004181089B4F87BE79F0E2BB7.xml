<taxonomicName id="F7606ABAA9E8B53D68B731C2086E9B18" class="Polypodiopsida" family="Tectariaceae" genus="Aspidium" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Polypodiales" pageId="18" pageNumber="19" phylum="Pteridophyta" rank="species" species="wallichii">
