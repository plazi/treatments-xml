(
<number box="[986,1025,1740,1765]" pageId="16" pageNumber="417" value="5.6">5.6</number>
, 2.0, 
<number box="[1084,1122,1740,1765]" pageId="16" pageNumber="417" value="5.7">5.7</number>
, 
<number box="[1132,1170,1740,1765]" pageId="16" pageNumber="417" value="5.4">5.4</number>
