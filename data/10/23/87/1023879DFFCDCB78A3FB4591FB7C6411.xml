, 
<number box="[772,824,1703,1727]" pageId="18" pageNumber="419" value="0.14">0.14</number>
, 
<number box="[837,889,1702,1727]" pageId="18" pageNumber="419" value="0.5">0.50</number>
, 
<number box="[901,953,1702,1727]" pageId="18" pageNumber="419" value="0.46">0.46</number>
, 
<number box="[966,1018,1703,1727]" pageId="18" pageNumber="419" value="0.28">0.28</number>
, 
<number box="[1031,1084,1703,1727]" pageId="18" pageNumber="419" value="0.2">0.20</number>
