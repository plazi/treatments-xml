) and 
<taxonomicName authorityName="Volynkin &amp; László" authorityYear="2019" box="[839,935,1959,1985]" class="Insecta" family="Erebidae" genus="Tumicla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="431" phylum="Arthropoda" rank="species" species="smithi">
