, 
<number box="[1047,1099,443,467]" pageId="28" pageNumber="429" value="0.1">0.10</number>
, 
<number box="[1114,1166,443,467]" pageId="28" pageNumber="429" value="0.4">0.40</number>
, 
<number box="[1181,1233,443,467]" pageId="28" pageNumber="429" value="0.44">0.44</number>
, 
<number box="[1248,1300,443,467]" pageId="28" pageNumber="429" value="0.24">0.24</number>
, 
<number box="[1315,1368,443,467]" pageId="28" pageNumber="429" value="0.2">0.20</number>
