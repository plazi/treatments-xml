<number box="[257,306,1485,1510]" pageId="13" pageNumber="414" value="16.7">16.7</number>
(
<number box="[323,361,1485,1510]" pageId="13" pageNumber="414" value="5.5">5.5</number>
, 
<number box="[376,413,1486,1510]" pageId="13" pageNumber="414" value="2.3">2.3</number>
