(
<figureCitation box="[366,429,153,179]" captionStart="FIGURE 1" captionStartId="6.[151,250,1960,1985]" captionTargetBox="[151,1436,944,1932]" captionTargetId="figure@6.[151,1436,944,1932]" captionTargetPageId="6" captionText="FIGURE 1. Distribution of Lebinthus in the Philippines. Triangular symbols refer to species of the bitaeniatus species group; round symbols to species of the sanchezi species group." figureDoi="http://doi.org/10.5281/zenodo.3954525" httpUri="https://zenodo.org/record/3954525/files/figure.png" pageId="11" pageNumber="412">Fig. 1</figureCitation>
) 
<collectingCountry box="[446,577,153,179]" name="Philippines" pageId="11" pageNumber="412">Philippines</collectingCountry>
: 
<collectingRegion box="[586,667,153,179]" country="Philippines" name="Bataan" pageId="11" pageNumber="412">Bataan</collectingRegion>
, Mt. Natib 
<emphasis bold="true" box="[789,941,152,179]" pageId="11" pageNumber="412">(new record)</emphasis>
; 
