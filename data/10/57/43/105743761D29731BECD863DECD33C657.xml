<subSubSection pageId="null" pageNumber="152" type="vernacular_name">
<paragraph pageId="null" pageNumber="152">
Hoher 
<normalizedToken originalValue="Wolfsfuß" pageId="null" pageNumber="152">Wolfsfuss</normalizedToken>
</paragraph>
