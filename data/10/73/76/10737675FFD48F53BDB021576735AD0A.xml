<subSubSection pageId="12" pageNumber="113" type="materials_examined">
<paragraph blockId="12.[151,1437,654,1867]" pageId="12" pageNumber="113">
<emphasis bold="true" box="[151,329,654,679]" pageId="12" pageNumber="113">
<typeStatus box="[151,210,654,679]" pageId="12" pageNumber="113">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[341,445,654,679]" pageId="12" pageNumber="113">Holotype</typeStatus>
reproductive male (mounted on slide) and 
<number box="[962,990,655,679]" pageId="12" pageNumber="113" value="12.0">12</number>
<typeStatus box="[1002,1110,655,679]" pageId="12" pageNumber="113">paratypes</typeStatus>
(
<number box="[1130,1144,654,679]" pageId="12" pageNumber="113" value="5.0">5</number>
unreproductive males, 
<specimenCount pageId="12" pageNumber="113" type="female">
<number box="[1422,1436,654,679]" pageId="12" pageNumber="113" value="6.0">6</number>
females
</specimenCount>
, 
<specimenCount box="[261,336,690,715]" pageId="12" pageNumber="113" type="generic">
<number box="[261,275,691,715]" pageId="12" pageNumber="113" value="1.0">1</number>
juv.
</specimenCount>
, mounted on 
<number box="[515,529,690,715]" pageId="12" pageNumber="113" value="6.0">6</number>
slides); moss near Kok Zhaiyk waterfall, litter under “archa” (
<taxonomicName class="Entognatha" family="Onychiuridae" genus="Juniperus" kingdom="Animalia" order="Collembola" pageId="12" pageNumber="113" phylum="Arthropoda" rank="species" species="turkestanica">
