<taxonomicName class="Insecta" family="Aulacidae" genus="Pristaulacus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pristaulacus kiefferi" order="Hymenoptera" pageId="27" pageNumber="104" phylum="Arthropoda" rank="species" species="kiefferi">Pristaulacus kiefferi</taxonomicName>
