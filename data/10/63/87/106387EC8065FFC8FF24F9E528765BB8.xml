, 
<materialsCitation collectorName="U. Schiecke" country="Malta" elevation="9" location="Malta" pageId="5" pageNumber="130" specimenCount="1">
<quantity box="[981,1068,151,175]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="5" pageNumber="130" unit="m" value="9.0" valueMax="10.0" valueMin="8.0">
<elevation box="[981,1068,151,175]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="5" pageNumber="130" unit="m" value="9.0" valueMax="10.0" valueMin="8.0">8–10 m</elevation>
</quantity>
, 
<collectorName box="[1081,1217,151,175]" pageId="5" pageNumber="130">U. Schiecke</collectorName>
