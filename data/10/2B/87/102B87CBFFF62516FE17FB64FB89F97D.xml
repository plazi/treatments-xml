<taxonomicName authorityName="Castro" authorityYear="2007" baseAuthorityName="Castro" baseAuthorityYear="2007" box="[764,1006,1454,1478]" class="Malacostraca" family="Goneplacidae" genus="Ommatocarcinus" kingdom="Animalia" order="Decapoda" pageId="116" pageNumber="725" phylum="Arthropoda" rank="species" species="elegans">
