<taxonomicName authorityName="Castro" authorityYear="2007" box="[718,927,1512,1536]" class="Malacostraca" family="Goneplacidae" genus="Microgoneplax" kingdom="Animalia" order="Decapoda" pageId="116" pageNumber="725" phylum="Arthropoda" rank="species" species="caenis">
