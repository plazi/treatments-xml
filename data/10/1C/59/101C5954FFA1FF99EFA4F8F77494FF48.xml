<taxonomicName authority="Uvarov (1931)" authorityName="Uvarov" authorityYear="1931" box="[366,663,192,218]" class="Insecta" family="Mantidae" genus="Microthespis" kingdom="Animalia" order="Mantodea" pageId="11" pageNumber="54" phylum="Arthropoda" rank="species" species="evansi">
