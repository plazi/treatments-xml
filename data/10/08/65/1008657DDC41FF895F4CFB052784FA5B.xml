<taxonomicName authorityName="Fabricius" authorityYear="1803" box="[1234,1307,1966,1991]" class="Insecta" family="Cicadellidae" genus="Iassus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
