<quantity box="[537,648,1412,1439]" metricMagnitude="-4" metricUnit="m" metricValue="3.8" pageId="31" pageNumber="32" unit="mm" value="0.38">0.38 mm</quantity>
, width 
<quantity box="[739,849,1412,1439]" metricMagnitude="-4" metricUnit="m" metricValue="6.8" pageId="31" pageNumber="32" unit="mm" value="0.68">0.68 mm</quantity>
; mesosoma length 
<quantity box="[1082,1192,1412,1439]" metricMagnitude="-4" metricUnit="m" metricValue="9.1" pageId="31" pageNumber="32" unit="mm" value="0.91">0.91 mm</quantity>
, width 
<quantity box="[1283,1394,1412,1439]" metricMagnitude="-4" metricUnit="m" metricValue="5.5" pageId="31" pageNumber="32" unit="mm" value="0.55">0.55 mm</quantity>
; metasoma length 
<quantity box="[396,504,1448,1474]" metricMagnitude="-3" metricUnit="m" metricValue="1.13" pageId="31" pageNumber="32" unit="mm" value="1.13">1.13 mm</quantity>
