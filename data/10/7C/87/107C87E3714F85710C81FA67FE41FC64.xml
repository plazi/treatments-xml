<taxonomicName authorityName="Cui &amp; Xue &amp; Jiang" authorityYear="2018" class="Insecta" family="Geometridae" genus="Aquilargilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="435" phylum="Arthropoda" rank="species" species="basifixa">
