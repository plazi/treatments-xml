<taxonomicName authorityName="Deshayes" authorityYear="1862" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis proboscidea" order="Pteropoda" pageId="231" pageNumber="232" phylum="Mollusca" rank="species" species="proboscidea">
