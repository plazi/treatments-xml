<collectingRegion id="220EF854FFF3FF955A08F9F2FD1D67E0" box="[534,686,1663,1689]" country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="62" pageNumber="63">Buenos Aires</collectingRegion>
