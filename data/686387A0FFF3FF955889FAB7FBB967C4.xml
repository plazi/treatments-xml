<emphasis id="D2BEEAA4FFF3FF955A71F9BAFC9F6728" box="[623,812,1591,1617]" italics="true" pageId="62" pageNumber="63">
<taxonomicName id="27CA4D35FFF3FF955A71F9BAFC9A6728" box="[623,809,1591,1617]" class="Insecta" family="Bibionidae" genus="Dilophus" kingdom="Animalia" order="Diptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="similis">Dilophus similis</taxonomicName>
