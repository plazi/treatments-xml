<vernacularName id="D9CC46B8FFD0FFC0FC49F32E6B3EEC88" box="[989,1142,3285,3306]" pageId="5" pageNumber="310">Titi de Aripuané</vernacularName>
<emphasis id="65BBEA84FFD0FFC0FF25F3076EEFED73" box="[177,423,3324,3345]" pageId="5" pageNumber="310">Other common names:</emphasis>
<vernacularName id="D9CC46B8FFD0FFC0FE24F3076D3FED73" box="[432,631,3324,3345]" pageId="5" pageNumber="310">Aripuana Marmoset</vernacularName>
, 
<vernacularName id="D9CC46B8FFD0FFC0FD12F3076C30ED73" box="[646,888,3324,3345]" pageId="5" pageNumber="310">Hershkovitz's Marmoset</vernacularName>
