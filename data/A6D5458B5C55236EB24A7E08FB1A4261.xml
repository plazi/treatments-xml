<taxonomicName id="1D94C5FE49546A558506E86F9C3EEBF1" LSID="http://zoobank.org/3FF40A9D-ED14-4140-9923-7B9FB0989248" authority="Chen &amp; Ren" class="Insecta" family="Coccinellidae" genus="Scymnus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scymnus (Orthoscymnus) paradoxus" order="Coleoptera" pageId="6" pageNumber="97" phylum="Arthropoda" rank="species" species="paradoxus" subGenus="Orthoscymnus">Scymnus (Orthoscymnus) paradoxus Chen &amp; Ren</taxonomicName>
<taxonomicNameLabel id="E13A9F7D554D946D00CC4569AE86EA1A" pageId="6" pageNumber="97">sp. n.</taxonomicNameLabel>
