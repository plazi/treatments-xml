<taxonomicName id="CEC254B630FCDF1B79E6B0DEAFD4FD5C" genus="G." lsidName="G. maria" pageId="0" pageNumber="41" rank="species" species="maria">
<emphasis id="82830D1845F5ABE2346F25A6A798136A" italics="true" pageId="0" pageNumber="41">G. maria</emphasis>
