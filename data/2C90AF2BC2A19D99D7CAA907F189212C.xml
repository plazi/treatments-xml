<taxonomicName id="272481CB3B971FEA99DE96897EE68523" LSID="http://bioguid.osu.edu/osuc_concepts:7825" authority="Kozlov" class="Insecta" family="Platygastridae" genus="Criomica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Criomica" order="Hymenoptera" pageId="3" pageNumber="102" phylum="Arthropoda" rank="genus">Criomica Kozlov</taxonomicName>
