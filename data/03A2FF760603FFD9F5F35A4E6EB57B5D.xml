<quantity id="4CF3E3850603FFDEF6025AD96C237CA2" box="[835,888,1491,1513]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="19" pageNumber="360" unit="km" value="2.0">2 km</quantity>
