<quantity id="4CF3E3850603FFDEF4F75AD96EB17CA2" box="[438,490,1491,1513]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="19" pageNumber="360" unit="m" value="20.0">20 m</quantity>
