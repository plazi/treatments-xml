<taxonomicName id="F157D72F94262844CB964B994614FEE0" authorityName="Rapp" authorityYear="1842" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="genus">Hyperolius</taxonomicName>
