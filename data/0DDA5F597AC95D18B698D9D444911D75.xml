<emphasis id="CFF75DC5243DE594F3E277B37BA46427" bold="true" pageId="0" pageNumber="407">Present determination.</emphasis>
A 
<taxonomicName id="99D6D846916651FF6C7788F1ECD0981B" authorityName="Rapp" authorityYear="1842" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="genus">
<emphasis id="17BCD94DC7093E7DCD7388346BD701BC" italics="true" pageId="0" pageNumber="407">Hyperolius</emphasis>
