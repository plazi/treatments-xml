<taxonomicName id="1D2B02F828EFB88AC891F03416B89E6E" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius viridiflavus subsp. ferniquei" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="subSpecies" species="viridiflavus" subSpecies="ferniquei">
