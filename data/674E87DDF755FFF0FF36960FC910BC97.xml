species.
</emphasis>
<taxonomicName id="28E74D48F755FFF0FEE1965DCB42BA53" authority="Reinhardt &amp; Lutken, 1862:214" authorityName="Reinhardt &amp; Lutken" authorityPageNumber="214" authorityYear="1862" box="[320,882,848,874]" class="Reptilia" family="Colubridae" genus="Liophis" kingdom="Animalia" order="Squamata" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="andreae">
