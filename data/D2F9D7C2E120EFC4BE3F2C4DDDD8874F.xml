<taxonomicName id="EA35B0D314A1B838C6A94B87BAE7AC60" LSID-ZBK="urn:lsid:zoobank.org:act:347CE897-3860-4DA6-85B8-033B2318B81A" class="Actinopterygii" family="Ariidae" genus="Aspistor" kingdom="Animalia" lsidName-ZBK="Aspistor Jordan &amp; Evermann 1898:2763" order="Siluriformes" pageNumber="91" phylum="Chordata" rank="genus">Aspistor</taxonomicName>
