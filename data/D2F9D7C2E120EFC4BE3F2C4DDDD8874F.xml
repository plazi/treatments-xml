, 
<taxonomicName id="B21E1295123E6C68F6DE90F775B3FBE7" LSID-ZBK="urn:lsid:zoobank.org:act:3F1F6AF2-6F38-42C0-9C82-C1468F19FAF0" class="Actinopterygii" family="Ariidae" genus="Hemiarius" kingdom="Animalia" lsidName-ZBK="Hemiarius Bleeker 1862:7, 29" order="Siluriformes" pageNumber="91" phylum="Chordata" rank="genus">Hemiarius</taxonomicName>
, 
<taxonomicName id="FFBADD4547AA1D2FDA246E7128D5A48C" LSID-ZBK="urn:lsid:zoobank.org:act:086E7B0F-D030-44BD-9DCC-8CAEB178CBAA" class="Actinopterygii" family="Ariidae" genus="Notarius" kingdom="Animalia" lsidName-ZBK="Notarius Gill 1863:171" order="Siluriformes" pageNumber="91" phylum="Chordata" rank="genus">Notarius</taxonomicName>
, 
<taxonomicName id="86A2498E3FDF785403C397643D34BAED" class="Insecta" family="Cerambycidae" genus="Sciades" kingdom="Animalia" order="Coleoptera" pageNumber="91" phylum="Arthropoda" rank="species" species="couma">Sciades couma</taxonomicName>
