<taxonomicName id="EE924D3AFFAEF614FD651D3CFC4EF9AC" authority="Dijkstra &amp; Maestrati, 2013" authorityName="Dijkstra &amp; Maestrati" authorityYear="2013" box="[677,948,1547,1572]" class="Bivalvia" family="Propeamussiidae" genus="Parvamussium" kingdom="Animalia" order="Pectinida" pageId="3" pageNumber="472" phylum="Mollusca" rank="species" species="perspicuum" status="n. sp.">
