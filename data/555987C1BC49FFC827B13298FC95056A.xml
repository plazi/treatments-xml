<paragraph id="DD4F36D7BC49FFC827B13298FCFC02B9" blockId="6.[151,778,152,211]" box="[151,778,152,178]" pageId="6" pageNumber="233">
<heading id="860781BBBC49FFC827B13298FCFC02B9" bold="true" box="[151,778,152,178]" fontSize="11" level="1" pageId="6" pageNumber="233" reason="1">
<emphasis id="EF84EAC5BC49FFC827B13298FCFC02B9" bold="true" box="[151,778,152,178]" pageId="6" pageNumber="233">
<taxonomicName id="1AF04D54BC49FFC827B13298FD3602B9" authority="Starý &amp; Oboňa, 2018" authorityName="Starý &amp; Oboňa" authorityYear="2018" box="[151,704,152,178]" class="Insecta" family="Limoniidae" genus="Thaumastoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="233" phylum="Arthropoda" rank="species" species="spathifera" status="sp. n." subGenus="Thaumastoptera">
<emphasis id="EF84EAC5BC49FFC827B13298FEA802B9" bold="true" box="[151,350,152,178]" italics="true" pageId="6" pageNumber="233">Thaumastoptera</emphasis>
(
<emphasis id="EF84EAC5BC49FFC826483298FDC302B9" bold="true" box="[366,565,152,178]" italics="true" pageId="6" pageNumber="233">Thaumastoptera</emphasis>
