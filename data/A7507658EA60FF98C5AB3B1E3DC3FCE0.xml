. 
<taxonomicName id="E8F9BCCDEA63FF99C41C3F3B3E34F80B" authorityName="S. Julia &amp; Kiew" authorityYear="2013" box="[288,522,1898,1921]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="10" pageNumber="11" phylum="Tracheophyta" rank="species" species="conniegeriae">
