<taxonomicName id="DF070107666A243C427156629CD6DA24" authorityName="Simon" authorityYear="1885" class="Arachnida" family="Salticidae" genus="Stagetillus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stagetillus" order="Araneae" pageId="0" pageNumber="27" phylum="Arthropoda" rank="genus">
