<taxonomicName id="AECB8CB811242B7C3D80F741BC421C40" authorityName="Simon" authorityYear="1885" class="Arachnida" family="Salticidae" genus="Stagetillus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stagetillus" order="Araneae" pageId="0" pageNumber="27" phylum="Arthropoda" rank="genus">
<emphasis id="86B040EF26B9D71D27BB8AF395C531DA" italics="true" pageId="0" pageNumber="27">Stagetillus</emphasis>
