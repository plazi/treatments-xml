<taxonomicName id="1B12DAB4AD90C8089BBAE011E033D384" class="Insecta" family="Aphididae" genus="Aphidura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphidura pakistanensis" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="pakistanensis">Aphidura pakistanensis</taxonomicName>
sp. n., 
<taxonomicName id="4584CEA286EBD1BB63D0121B34F7EA45" class="Insecta" family="Aphididae" genus="Aphidura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphidura graeca" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="graeca">Aphidura graeca</taxonomicName>
sp. n., 
<taxonomicName id="DD4E4E45C1D66EFA03AD5DF2EFEF990A" class="Insecta" family="Aphididae" genus="Aphidura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphidura urmiensis" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="urmiensis">Aphidura urmiensis</taxonomicName>
sp. n., and 
<taxonomicName id="5A65799EF4C97CFD34543390B53AA720" class="Insecta" family="Aphididae" genus="Aphidura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aphidura iranensis" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="iranensis">Aphidura iranensis</taxonomicName>
sp. n.; n, number of measured specimens.&lt;br/&gt;
</paragraph>
