<taxonomicName id="A6D74D4FE70F04798C20363EE9EEF873" authority="Xu &amp; Zhang &amp; Yao, 2019" authorityName="Xu &amp; Zhang &amp; Yao" authorityYear="2019" box="[277,452,1900,1921]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="387" phylum="Arthropoda" rank="species" species="mixiaoqii" status="sp. nov.">
