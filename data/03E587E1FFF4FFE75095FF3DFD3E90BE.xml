<collectionCode id="ED5DAE32FFF4FFE1519AFC75FE5A95F7" box="[408,478,940,966]" country="Turkey" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14530" lsid="urn:lsid:biocol.org:col:14530" name="University of Istanbul" pageId="2" pageNumber="40" type="Herbarium">ISTE</collectionCode>
