<taxonomicName id="34B37486AB44FF9BFC3FFD07FC72C4D7" authorityName="Milne-Edwards" authorityYear="1830" box="[913,991,703,728]" class="Malacostraca" family="Vibiliidae" genus="Vibilia" kingdom="Animalia" order="Amphipoda" pageId="73" pageNumber="74" phylum="Arthropoda" rank="genus">
