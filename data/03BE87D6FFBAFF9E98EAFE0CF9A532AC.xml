<quantity id="4CEF9B25FFBAFF9E9A7EFD99F87D3280" box="[515,572,619,645]" metricMagnitude="1" metricUnit="m" metricValue="1.0" pageId="43" pageNumber="44" unit="m" value="10.0">10 m</quantity>
