<quantity id="4CEF9B25FFBAFF9E9BE8FE0DF99B321C" box="[917,986,511,537]" metricMagnitude="-1" metricUnit="m" metricValue="2.4" pageId="43" pageNumber="44" unit="cm" value="24.0">24 cm</quantity>
