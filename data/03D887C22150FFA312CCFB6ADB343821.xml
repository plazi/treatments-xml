</taxonomicName>
, male; 
<number id="3DB924F32152FFA311F2FD17D9953ED7" box="[937,966,687,710]" pageId="7" pageNumber="8" value="19.0">19</number>
, 
<taxonomicName id="4C714D572152FFA3118AFD08DE063ED6" box="[977,1109,688,711]" class="Insecta" family="Chrysomelidae" genus="Agetocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="discedens">
<emphasis id="B905EAC62152FFA3118AFD08DE063ED6" box="[977,1109,688,711]" italics="true" pageId="7" pageNumber="8">A. discedens</emphasis>
</taxonomicName>
, female; 
<number id="3DB924F32152FFA316E2FD17DE843ED7" box="[1209,1239,687,710]" pageId="7" pageNumber="8" value="20.0">20</number>
. 
<taxonomicName id="4C714D572152FFA316BBFD08DF003ED6" box="[1248,1363,688,711]" class="Insecta" family="Chrysomelidae" genus="Agetocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="taiwana">
<emphasis id="B905EAC62152FFA316BBFD08DF003ED6" box="[1248,1363,688,711]" italics="true" pageId="7" pageNumber="8">A. taiwana</emphasis>
