<taxonomicName id="4C5E719DFF91FFC282F5FB18FBEAFB77" box="[906,1032,1218,1240]" class="Insecta" family="Ichneumonidae" genus="Itoplectis" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="435" phylum="Arthropoda" rank="species" species="alternans">
<emphasis id="B92AD60CFF91FFC282F5FB18FBEAFB77" box="[906,1032,1218,1240]" italics="true" pageId="16" pageNumber="435">I. alternans</emphasis>
</taxonomicName>
<specimenCount id="9D58C197FF91FFC28568FB18FEF9FB5A" pageId="16" pageNumber="435" type="generic">two specimens</specimenCount>
reared from 
<taxonomicName id="4C5E719DFF91FFC280D6FB04FDA5FB5A" authorityName="Linnaeus" authorityYear="1758" box="[425,583,1247,1269]" class="Insecta" family="Tortricidae" genus="Archips" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="435" phylum="Arthropoda" rank="species" species="rosana">
<emphasis id="B92AD60CFF91FFC280D6FB04FDA5FB5A" box="[425,583,1247,1269]" italics="true" pageId="16" pageNumber="435">Archips rosana</emphasis>
</taxonomicName>
L (
<taxonomicName id="4C5E719DFF91FFC2830DFB04FD10FB5A" box="[626,754,1247,1269]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="435" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName id="4C5E719DFF91FFC28380FB04FC93FB5A" box="[767,881,1247,1269]" class="Insecta" family="Tortricidae" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="435" phylum="Arthropoda" rank="family">Tortricidae</taxonomicName>
