<taxonomicName id="4C5E719DFF91FFC28199FB18FE8DFB77" box="[230,367,1219,1240]" class="Insecta" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="435" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
