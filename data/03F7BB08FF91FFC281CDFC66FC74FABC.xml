<taxonomicName id="4C5E719DFF91FFC28392FB7EFCBDFB14" authorityName="Linnaeus" authorityYear="1758" box="[749,863,1189,1211]" class="Insecta" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="435" phylum="Arthropoda" rank="order">Coleoptera</taxonomicName>
