<collectionCode id="EFA8AE2D2736FFACAF1B13B33CB1FD3C" box="[1072,1160,655,679]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="4" pageNumber="839">MNHN</collectionCode>
