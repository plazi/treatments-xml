<heading id="D0B2F839EE34FF9A30DFF90CE943C7CF" bold="true" box="[151,560,1707,1734]" fontSize="11" level="1" pageId="3" pageNumber="46" reason="1">
<emphasis id="B9319347EE34FF9A30DFF90CE943C7CF" bold="true" box="[151,560,1707,1734]" pageId="3" pageNumber="46">
<taxonomicName id="4C4534D6EE34FF9A30DFF90CEAE1C7CC" box="[151,402,1708,1734]" class="Insecta" family="Psychomyiidae" genus="Paduniella" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="46" phylum="Arthropoda" rank="species" species="dongdata" status="new species">
<emphasis id="B9319347EE34FF9A30DFF90CEAE1C7CC" bold="true" box="[151,402,1708,1734]" italics="true" pageId="3" pageNumber="46">Paduniella dongdata</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A2022E3CEE34FF9A31E9F90BE943C7CF" box="[417,560,1707,1733]" pageId="3" pageNumber="46" rank="species">new species</taxonomicNameLabel>
</emphasis>
