<taxonomicName id="4C1B4D374211FFD8FEA8FA65FC0FFA26" authority="Santos Abreu, 1921: 47" authorityName="Santos Abreu" authorityPageNumber="47" authorityYear="1921" box="[313,937,1431,1453]" class="Insecta" family="Phoridae" genus="Aphiochaeta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="69" pageNumber="573" phylum="Arthropoda" rank="variety" species="pulicaria" variety="luteimana">
