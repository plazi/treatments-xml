<taxonomicName id="4C1B4D374213FFDAFE30FC48FDC2FC45" baseAuthorityName="Wood" baseAuthorityYear="1909" box="[417,612,954,975]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="71" pageNumber="575" phylum="Arthropoda" rank="species" species="pleuralis">
