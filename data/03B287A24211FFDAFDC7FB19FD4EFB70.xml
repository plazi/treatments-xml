<taxonomicName id="4C1B4D374212FFDBFE30FA5DFDCEFA4E" baseAuthorityName="Wood" baseAuthorityYear="1909" box="[417,616,1455,1476]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="70" pageNumber="574" phylum="Arthropoda" rank="species" species="pleuralis">
