<taxonomicName id="4C264D42FFDEA239ADD3FC17FA7B21B0" authority="Cabezas, 2010" authorityName="Cabezas" authorityYear="2010" box="[1274,1435,992,1017]" class="Malacostraca" family="Munididae" genus="Paramunida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="mozambica" status="n. sp.">
