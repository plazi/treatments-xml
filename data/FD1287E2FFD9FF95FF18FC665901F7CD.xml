<taxonomicName id="B2BB4D77FFD9FF95FD17F9815B35F9A4" authority=", UC" authorityName="UC" box="[647,833,1626,1652]" class="Myxomycetes" family="Stemonitidaceae" genus="Lamproderma" kingdom="Protozoa" order="Stemonitales" pageId="5" pageNumber="180" phylum="Mycetozoa" rank="species" species="carestiae">
