<taxonomicName id="B2BB4D77FFD9FF95FDD2F9A55A8BF947" authority=", UC" authorityName="UC" box="[578,767,1661,1687]" class="Myxomycetes" family="Stemonitidaceae" genus="Lamproderma" kingdom="Protozoa" order="Stemonitales" pageId="5" pageNumber="180" phylum="Mycetozoa" rank="species" species="carestiae">
