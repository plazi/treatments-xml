<heading id="D0A681F1FFA7FFF2F5EAF99B45F1F9B7" bold="true" box="[151,427,1636,1662]" fontSize="11" level="1" pageId="38" pageNumber="39" reason="1">
<taxonomicName id="4C514D1EFFA7FFF2F5EAF99B45F1F9B7" authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" box="[151,427,1636,1662]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
