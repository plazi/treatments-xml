<geoCoordinate id="A8D54AFC3DC0FAB0462DFDC5F177AA5E" direction="north" orientation="latitude" precision="1" value="18.150183">18°09.011'N</geoCoordinate>
, 
<geoCoordinate id="B83CFACF7CCD00C3AC2AD891C8D7A5D0" direction="west" orientation="longitude" precision="1" value="-71.62237">71°37.342'W</geoCoordinate>
