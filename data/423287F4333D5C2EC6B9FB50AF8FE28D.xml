<taxonomicName id="0D9B4D61333D5C2EC6B9FB50AEFEE3C1" baseAuthorityName="Erichson" baseAuthorityYear="1840" box="[490,697,1227,1251]" class="Insecta" family="Staphylinidae" genus="Micrillus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="984" phylum="Arthropoda" rank="species" species="testaceus">
<emphasis id="F8EFEAF0333D5C2EC6B9FB50AEFEE3C1" bold="true" box="[490,697,1227,1251]" italics="true" pageId="7" pageNumber="984">Micrillus testaceus</emphasis>
