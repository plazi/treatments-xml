<taxonomicName id="FFB34D7C4B55FFE6CD28B6E7FC22BCF4" authority="Xi &amp; Wang &amp; Yang &amp; Liu &amp; Yang, 2021" authorityName="Xi &amp; Wang &amp; Yang &amp; Liu &amp; Yang" authorityYear="2021" box="[807,1020,1944,1968]" class="Insecta" family="Cantharidae" genus="Lycocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="549" phylum="Arthropoda" rank="species" species="rufipennis" status="sp. nov.">
