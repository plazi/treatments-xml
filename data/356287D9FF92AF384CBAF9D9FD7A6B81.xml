<taxonomicName id="7ACB4D4CFF92AF3C4CD2F9DAFABC6D64" authority="SOLIER, 1834" authorityName="Solier" authorityYear="1834" box="[1011,1347,1573,1598]" class="Insecta" family="Hydrophilidae" genus="Tropisternus" kingdom="Animalia" order="Coleoptera" pageId="10" phylum="Arthropoda" rank="genus">
