<taxonomicName id="229E4D6697060D3964BC5DA8FEB7B8D7" authorityName="Dejean" authorityYear="1821" box="[192,352,1156,1179]" class="Insecta" family="Curculionidae" genus="Camptocerus" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="53" phylum="Arthropoda" rank="genus">
