<subSubSection id="AD84656E97060D3964BC5D4FFB53B8D7" pageId="54" pageNumber="53" type="etymology">
<paragraph id="E52136E597060D3964BC5D4FFB53B8D7" blockId="54.[192,1440,1123,1179]" pageId="54" pageNumber="53">
<emphasis id="D7EAEAF797060D3964BC5D4FFE8AB837" bold="true" box="[192,349,1123,1147]" pageId="54" pageNumber="53">Etymology.</emphasis>
