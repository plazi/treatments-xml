<taxonomicName id="4C2297D6C01FFFA597C3FF7CC8128EF4" authority="Jacobs, 2021" authorityName="Jacobs" authorityYear="2021" box="[151,579,151,178]" class="Magnoliopsida" family="Convolvulaceae" genus="Dinetus" kingdom="Plantae" order="Solanales" pageId="4" pageNumber="73" phylum="Tracheophyta" rank="species" species="deserticus" status="sp. nov." subGenus="Venustidinetus">
<emphasis id="B9563047C01FFFA597C3FF7CCAA78EF7" bold="true" box="[151,246,151,177]" italics="true" pageId="4" pageNumber="73">Dinetus</emphasis>
(
<emphasis id="B9563047C01FFFA59653FF7CCBE88EF4" bold="true" box="[263,441,151,178]" italics="true" pageId="4" pageNumber="73">Venustidinetus</emphasis>
) 
