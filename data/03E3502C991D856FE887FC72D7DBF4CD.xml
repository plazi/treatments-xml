<taxonomicName id="4C4A9AB9991D856FEBFFFBE2D1C1F459" authorityName="Beetle" authorityYear="1978" baseAuthorityName="Mill." box="[1007,1168,1053,1079]" class="Magnoliopsida" family="Fagaceae" genus="Arundo" kingdom="Plantae" order="Fagales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="donax">
