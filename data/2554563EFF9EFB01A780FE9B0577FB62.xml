<taxonomicName id="6AFD9CABFF9EFB01A6EDFCFC04C3FCF4" authorityName="Felder &amp; Manning" authorityYear="1986" box="[506,646,800,825]" class="Malacostraca" family="Alpheidae" genus="Salmoneus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="25" pageNumber="202" phylum="Arthropoda" rank="species" species="cavicolus">
