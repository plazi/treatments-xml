<taxonomicName id="D5FE9156DC69E2B37F0AB3FADED63B4D" authorityName="Wahlberg" authorityYear="1844" class="Insecta" family="Dolichopodidae" genus="Hydrophorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hydrophorus alpinus" order="Diptera" pageId="0" pageNumber="551" phylum="Arthropoda" rank="species" species="alpinus">
