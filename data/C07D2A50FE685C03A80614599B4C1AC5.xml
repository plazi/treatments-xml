<taxonomicName id="594658CCD63167F301A24DBC45B7AACD" authorityName="Emelyanov &amp; Piskunov" authorityYear="1982" class="Insecta" family="Gelechiidae" genus="Gelechia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gelechia jakovlevi subsp. mongoliae" order="Lepidoptera" pageId="0" pageNumber="105" phylum="Arthropoda" rank="subSpecies" species="jakovlevi" subSpecies="mongoliae">Gelechia jakovlevi mongoliae</taxonomicName>
