<emphasis id="B94F452EFFD6FFD4AE915B75180AFBEC" bold="true" box="[199,315,1101,1126]" pageId="48" pageNumber="49">Remarks.</emphasis>
<taxonomicName id="4C3BE2BFFFD6FFD4AF175B751B26FBEF" authorityName="Fraser" authorityYear="1937" box="[321,535,1101,1126]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="48" pageNumber="49" phylum="Cnidaria" rank="species" species="ornata">
