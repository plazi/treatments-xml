<emphasis id="B937EAA1FFB61236F11F8DC9305EF3EE" box="[528,864,151,176]" italics="true" pageId="31" pageNumber="32">
<taxonomicName id="4C434D30FFB61236F11F8DC9306BF3EE" authority="Boulenger" authorityName="Boulenger" box="[528,853,151,176]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="albopunctulatus">Hyloscirtus albopunctulatus</taxonomicName>
*
</emphasis>
(Boulenger), 
<taxonomicName id="4C434D30FFB61236F7088DC93638F3EE" authority="Duellman" authorityName="Duellman" box="[1031,1286,151,176]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="alytolylax">
<emphasis id="B937EAA1FFB61236F7088DC9374BF3EE" box="[1031,1141,151,176]" italics="true" pageId="31" pageNumber="32">alytolylax</emphasis>
