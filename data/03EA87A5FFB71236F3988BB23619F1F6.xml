<taxonomicName id="4C434D30FFB61236F3988D813257F3A6" authority="Rivero" authorityName="Rivero" box="[151,361,223,248]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="lascinius">
<emphasis id="B937EAA1FFB61236F3988D8133C5F3A6" box="[151,251,223,248]" italics="true" pageId="31" pageNumber="32">lascinius</emphasis>
