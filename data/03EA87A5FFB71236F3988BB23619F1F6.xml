<emphasis id="B937EAA1FFB71237F3988A6F3232F419" box="[151,268,1841,1863]" italics="true" pageId="30" pageNumber="31">Hyloscirtus</emphasis>
Peters, 
<number id="3D8B2494FFB71237F2538A6F32ACF419" box="[348,402,1841,1863]" pageId="30" pageNumber="31" value="1882.0">1882</number>
</taxonomicName>
. 
<typeStatus id="54F88811FFB71237F2948A6C32F3F419" box="[411,461,1842,1863]" pageId="30" pageNumber="31">Type</typeStatus>
species: 
<taxonomicName id="4C434D30FFB71237F1248A6F30A6F419" authority="Peters, 1862" authorityName="Peters" authorityYear="1862" box="[555,920,1841,1863]" class="Amphibia" family="Hylidae" genus="Hyloscirtus" kingdom="Animalia" order="Anura" pageId="30" pageNumber="31" phylum="Chordata" rank="species" species="bogotensis">
<emphasis id="B937EAA1FFB71237F1248A6F302CF419" box="[555,786,1841,1863]" italics="true" pageId="30" pageNumber="31">Hyloscirtus bogotensis</emphasis>
Peters, 
<number id="3D8B2494FFB71237F06C8A6F30A6F419" box="[867,920,1841,1863]" pageId="30" pageNumber="31" value="1862.0">1862</number>
</taxonomicName>
