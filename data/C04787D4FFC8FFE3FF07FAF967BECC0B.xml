<taxonomicName id="8FEE4D41FFC8FFE3FBA4F8A9613ECC6F" authorityName="Guglielmone &amp; Petney &amp; Robbins" authorityYear="2020" box="[1076,1149,1806,1832]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
