<taxonomicName id="8FEE4D41FFC8FFE3FB2FF8A96012CC6F" authorityName="Dhanda and Kulkarni" authorityYear="1969" box="[1215,1361,1806,1832]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="himalayensis">
