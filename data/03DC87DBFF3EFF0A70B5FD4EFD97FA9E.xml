<geoCoordinate id="EE41500AFF3EFF0D73D2F907FB90FCD3" box="[1008,1073,1693,1718]" degrees="2" direction="east" minutes="35" orientation="longitude" pageId="178" pageNumber="179" precision="925" value="2.5833333">
<emphasis id="B901EADFFF3EFF0D73D2F907FBABFCD0" box="[1008,1034,1694,1718]" italics="true" pageId="178" pageNumber="179">e2</emphasis>
35
</geoCoordinate>
, 
<emphasis id="B901EADFFF3EFF0D741CF907FBFBFCD0" box="[1086,1114,1694,1718]" italics="true" pageId="178" pageNumber="179">h2</emphasis>
