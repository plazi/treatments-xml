<taxonomicName id="1EE64D287F5FFF81BBCBF9FDFC4E962D" authority="Mahunka, 1983" authorityName="Mahunka" authorityYear="1983" box="[684,1003,1643,1662]" class="Arachnida" family="Euphthiracaridae" genus="Rhysotritia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="55" pageNumber="57" phylum="Arthropoda" rank="species" species="comteae">
