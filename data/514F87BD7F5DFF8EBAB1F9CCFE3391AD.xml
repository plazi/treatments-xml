<caption id="8D9966237F5FFF81B9F8F9FDFE1696E6" ID-DOI="http://doi.org/10.5281/zenodo.5400832" ID-Zenodo-Dep="5400832" httpUri="https://zenodo.org/record/5400832/files/figure.png" pageId="55" pageNumber="57" startId="55.[159,171,1643,1662]" targetBox="[242,1346,524,1611]" targetPageId="55">
