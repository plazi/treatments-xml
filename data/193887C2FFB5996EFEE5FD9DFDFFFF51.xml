<paragraph id="912E36D4FFB5996CFEE5FD9DFD82FDD5" blockId="16.[264,525,550,624]" pageId="16">
<heading id="CA6681B8FFB5996CFEE5FD9DFD82FDD5" centered="true" fontSize="10" level="2" pageId="16" reason="2">
<emphasis id="A3E5EAC6FFB5996CFEE5FD9DFD82FDD5" bold="true" pageId="16">
Tribe 
<taxonomicName id="56914D57FFB5996CFEABFD9DFD8FFD9E" authority="Jeannel" authorityName="Jeannel" authorityYear="1949" box="[326,525,550,571]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="16" phylum="Arthropoda" rank="tribe" tribe="Trogastrini">Trogastrini Jeannel</taxonomicName>
Subtribe 
<taxonomicName id="56914D57FFB5996CFE9DFDFAFD86FDF3" authority="Park" authorityName="Park" authorityYear="1951" box="[368,516,577,598]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="16" phylum="Arthropoda" rank="subTribe" subTribe="Rhexiina">Rhexiina Park</taxonomicName>
<taxonomicName id="56914D57FFB5996CFEF8FDE0FD82FDD5" authority="LeConte, 1849" authorityName="LeConte" authorityYear="1849" box="[277,512,603,624]" class="Insecta" family="Staphylinidae" genus="Rhexius" kingdom="Animalia" order="Coleoptera" pageId="16" phylum="Arthropoda" rank="genus">
