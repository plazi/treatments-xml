</taxonomicName>
(
<taxonomicName id="24F550E94749FFCFC661E574FE1AFD53" box="[353,463,616,640]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="24" pageNumber="425" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
