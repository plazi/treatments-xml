<collectionCode id="D0CBAE200656F673AA71FC9A90A38433" box="[835,910,784,810]" country="USA" httpUri="http://grbio.org/cool/a6xx-dd75" name="Virginia Institute of Marine Science" pageId="6" pageNumber="227">VIMS</collectionCode>
