<taxonomicName id="508D7C929C11FFBED0BBFBC3FD47FBCC" authority="Meyer &amp; Spungis, 1994" authorityName="Meyer &amp; Spungis" authorityYear="1994" box="[151,714,1112,1138]" class="Insecta" family="Cecidomyiidae" genus="Allaretella" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="547" phylum="Arthropoda" rank="species" species="germanica">
