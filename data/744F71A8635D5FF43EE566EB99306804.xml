96. - 
<taxonomicName id="BCDF38D926FC8F0243489B08FE676316" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145135" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" lsidName="Tetramorium pullulum Santschi" lsidName-HNS="Tetramorium pullulum Santschi" order="Hymenoptera" pageNumber="211" phylum="Arthropoda" rank="subSpecies" species="guineense" subSpecies="pullulum">Tetramorium guineense F. st. pullulum</taxonomicName>
