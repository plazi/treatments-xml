Known from New 
<collectingCountry id="F3407614FFE08C6BFDB3AEAB0FAAFB19" box="[570,648,1244,1269]" name="United Kingdom" pageId="1" pageNumber="446">Britain</collectingCountry>
(
<collectingCountry id="F3407614FFE08C6BFD1FAEAB0E50FB19" box="[662,882,1244,1269]" name="Papua New Guinea" pageId="1" pageNumber="446">Papua New Guinea</collectingCountry>
) and 
