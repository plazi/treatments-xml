<taxonomicName id="48E048BAFFCF601D6A97CC9EC995ADCA" authority="Cartwright, 2008" authorityName="Cartwright" authorityYear="2008" box="[231,433,1971,1994]" class="Insecta" family="Ecnomidae" genus="Ecnomina" kingdom="Animalia" order="Trichoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="incisura" status="sp. nov.">
