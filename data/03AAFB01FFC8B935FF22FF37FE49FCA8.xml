<subSubSection id="C319199CFFC8B935FF22FEA2FCA0FD40" pageId="25" pageNumber="104" type="reference_group">
<paragraph id="8BBC4A17FFC8B935FF22FEA2FD51FEEA" blockId="25.[151,1437,258,311]" pageId="25" pageNumber="104">
<materialsCitation id="3B6B404AFFC8B935FF22FEA2FD51FEEA" ID-GBIF-Occurrence="1702562721" country="Vietnam" location="Russian Journal of Zoology" pageId="25" pageNumber="104" specimenCount="1" stateProvince="Moscow">
