<taxonomicName id="7DFF4D07FFCDFFCD46BB621FFAC09A98" authority="Gray, 1838 Javan Pipistrelle" authorityName="Gray" authorityYear="1838" box="[941,1334,1913,1939]" class="Mammalia" family="Vespertilionidae" genus="Pipistrellus" kingdom="Animalia" order="Chiroptera" pageId="12" pageNumber="431" phylum="Chordata" rank="species" species="javanicus">
<emphasis id="888BEA96FFCDFFCD46BB621FFB5D9A98" box="[941,1195,1913,1939]" italics="true" pageId="12" pageNumber="431">Pipistrellus javanicus</emphasis>
