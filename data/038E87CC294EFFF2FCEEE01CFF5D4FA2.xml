<quantity id="4CDF9B3F294FFFEDFE6BE540FDF24E33" box="[445,575,848,878]" metricMagnitude="-4" metricUnit="m" metricValue="5.699999999999999" pageId="15" pageNumber="14" unit="mm" value="0.57">0.57 mm</quantity>
