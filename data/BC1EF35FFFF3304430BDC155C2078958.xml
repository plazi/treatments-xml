<collectingMunicipality id="D46CD833FFF63044322FC4A0C132889E" box="[551,633,213,237]" pageId="6" pageNumber="297">Bejuma</collectingMunicipality>
, 
<collectingRegion id="F6738CABFFF63044328FC4A0C01A889E" box="[647,849,213,237]" country="Venezuela" name="Carabobo" pageId="6" pageNumber="297">Carabobo province</collectingRegion>
, northern 
<collectingCountry id="4CA002D9FFF6304433CDC4A0C764889E" box="[965,1071,213,237]" name="Venezuela" pageId="6" pageNumber="297">Venezuela</collectingCountry>
. 
<collectingCounty id="DD693AC5FFF630443433C4A0C72E889E" box="[1083,1125,213,237]" pageId="6" pageNumber="297">The</collectingCounty>
