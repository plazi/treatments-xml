<specimenCount id="9D237DD2C308FF88FACEFE4A479EE5C4" pageId="3" pageNumber="274" type="generic">
<elevation id="00085168C308FF88FACEFE4A479AE5C4" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="274" unit="m" value="600.0">
<quantity id="4CDD1BBEC308FF88FACEFE4A479AE5C4" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="274" unit="m" value="600.0">600 m</quantity>
