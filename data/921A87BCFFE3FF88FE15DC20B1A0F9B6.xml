<heading id="414481C6FFE3FF88FE15DC20B331FBC7" bold="true" box="[391,886,1084,1107]" centered="true" fontSize="8" level="2" pageId="14" pageNumber="309" reason="3">
<taxonomicName id="DDB34D29FFE3FF88FE15DC20B331FBC7" authority="Alonso-Zarazaga &amp; Lyal, 1999" authorityName="Alonso-Zarazaga &amp; Lyal" authorityYear="1999" box="[391,886,1084,1107]" class="Insecta" family="Curculionidae" genus="Marvaldiella" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="309" phylum="Arthropoda" rank="genus">
