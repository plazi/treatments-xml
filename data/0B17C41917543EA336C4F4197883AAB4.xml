<normalizedToken id="79AC990D4EBC14811FC9CBD1850EE317" originalValue="tête">tete</normalizedToken>
du %\ 
<normalizedToken id="2E247E98A8DEA792DB4394EBB27F24B3" originalValue="très">tres</normalizedToken>
<normalizedToken id="A6A854F5191C49CD6A2C578F683478EB" originalValue="élargie">elargie</normalizedToken>
<normalizedToken id="7EFA25D1B7FC2FD42419D6A7FA23E2F9" originalValue="derrière">derriere</normalizedToken>
