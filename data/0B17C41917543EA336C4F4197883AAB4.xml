<taxonomicName id="E491AD79F43FFF146671342157375D8C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33860" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole megacephala (Fabricius)" order="Hymenoptera" pageNumber="225" phylum="Arthropoda" rank="species" species="megacephala">Ph. megacephala</taxonomicName>
