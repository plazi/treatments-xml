<taxonomicName id="21CC6A09F032A85EA321A771BE1D2133" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:189108" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole o'swaldi Forel" order="Hymenoptera" pageNumber="225" phylum="Arthropoda" rank="species" species="o'swaldi">Ph. O'Swaldi</taxonomicName>
