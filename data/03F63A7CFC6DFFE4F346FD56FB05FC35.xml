<geoCoordinate id="EE6BEDADFC6DFFE4F607FD39FB86FD06" box="[1045,1123,719,739]" degrees="18" direction="north" minutes="32" orientation="latitude" pageId="44" pageNumber="1053" precision="925" value="18.533333">18°32‘N</geoCoordinate>
