</taxonomicName>
have been recorded under the bark of dead fir (
<taxonomicName id="88A048DDFF9AFFE7FEEF8F70325F8E50" box="[317,382,284,309]" class="Pinopsida" family="Pinaceae" genus="Abies" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="40" phylum="Tracheophyta" rank="genus">
<emphasis id="7DD4EF4CFF9AFFE7FEEF8F70325F8E50" box="[317,382,284,309]" italics="true" pageId="3" pageNumber="40">Abies</emphasis>
</taxonomicName>
) (
<taxonomicName id="88A048DDFF9AFFE7FE728F70312F8E53" box="[416,526,284,310]" class="Pinopsida" family="Pinaceae" kingdom="Plantae" order="Pinales" pageId="3" pageNumber="40" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
), poplar (
<taxonomicName id="88A048DDFF9AFFE7FD428F7031D18E50" box="[656,752,284,309]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="40" phylum="Tracheophyta" rank="genus">
<emphasis id="7DD4EF4CFF9AFFE7FD428F7031D18E50" box="[656,752,284,309]" italics="true" pageId="3" pageNumber="40">Populus</emphasis>
</taxonomicName>
) (
<taxonomicName id="88A048DDFF9AFFE7FCC08F7030B58E53" box="[786,916,284,310]" class="Magnoliopsida" family="Salicaceae" kingdom="Plantae" order="Malpighiales" pageId="3" pageNumber="40" phylum="Tracheophyta" rank="family">Salicaceae</taxonomicName>
), and oak (
<taxonomicName id="88A048DDFF9AFFE7FBFC8F7037B18E50" box="[1070,1168,284,309]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="3" pageNumber="40" phylum="Tracheophyta" rank="genus">
