<taxonomicName id="88A048DDFF9BFFE6FEEA88B5318B8996" authority="LeConte" authorityName="LeConte" box="[312,682,1753,1779]" class="Insecta" family="Mycteridae" genus="Lacconotus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="39" phylum="Arthropoda" rank="species" species="punctatus">
