<paragraph id="146536E2FFA9FFAAFF00FA8FFEBE2A14" blockId="3.[151,708,1291,1351]" box="[151,267,1326,1351]" pageId="3" pageNumber="285">
(
<figureCitation id="8CE12A67FFA9FFAAFF08FA8FFEB72A14" box="[159,258,1326,1351]" captionStart-0="FIGURES 1 – 6" captionStart-1="FIGURES 7 – 9" captionStartId-0="4.[151,264,1558,1581]" captionStartId-1="5.[151,264,1539,1562]" captionTargetBox-0="[217,1371,379,1533]" captionTargetBox-1="[344,1239,778,1511]" captionTargetId-0="figure@4.[209,1379,373,1537]" captionTargetId-1="figure@5.[340,1247,772,1518]" captionTargetPageId-0="4" captionTargetPageId-1="5" captionText-0="FIGURES 1 – 6. Sphaeropthalma tapio sp. nov., holotype, 1. habitus; 2. head, dorsal view; 3. head, frontal view; 4. head, ventral view; 5. mesosoma, lateral view; 6. metasoma, lateral view." captionText-1="FIGURES 7 – 9. Sphaeropthalma tapio sp. nov., paratype, genitalia, 7. dorsal view; 8. ventral view; and 9. lateral view." httpUri-0="https://zenodo.org/record/233748/files/figure.png" httpUri-1="https://zenodo.org/record/233749/files/figure.png" pageId="3" pageNumber="285">
Figs 
<number id="A21224C5FFA9FFAAFF40FA8EFEB72A14" box="[215,258,1327,1351]" pageId="3" pageNumber="285" value="5.0" valueMax="9.0" valueMin="1.0">1–9</number>
</figureCitation>
