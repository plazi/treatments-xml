<quantity id="4CD1EF8FFFD77F3E98F5E1EBD545FD04" box="[452,599,660,686]" metricMagnitude="3" metricUnit="m" metricValue="1.625" metricValueMax="2.5" metricValueMin="0.75" pageId="18" pageNumber="19" unit="m" value="1625.0" valueMax="2500.0" valueMin="750.0">750–2500 m</quantity>
