<subSubSection pageId="7" pageNumber="96" type="nomenclature">
<paragraph pageId="7" pageNumber="96">
<taxonomicName LSID="http://zoobank.org/B14F37A9-6A33-446F-80AF-2C65472362D3" class="Arachnida" family="Agelenidae" genus="Pireneitega" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pireneitega tyurai" order="Araneae" pageId="7" pageNumber="96" phylum="Arthropoda" rank="species" species="tyurai">Pireneitega tyurai</taxonomicName>
