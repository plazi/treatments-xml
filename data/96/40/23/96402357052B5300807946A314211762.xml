Russia, North-East • ♂; Magadan Province, &quot;Magadan-Ola Road (closer to Ola) [~ 
<geoCoordinate degrees="59" direction="north" minutes="35.25" orientation="latitude" precision="9" value="59.5875">59°35.25'N</geoCoordinate>
, 
<geoCoordinate degrees="151" direction="east" minutes="08.00" orientation="longitude" precision="9" value="151.13333">151°08.00'E</geoCoordinate>
]; treeless bog with 
<taxonomicName class="Dicotyledoneae" family="Ericaceae" genus="Ledum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Ledum" order="Ericales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="1">Ledum</emphasis>
</taxonomicName>
, 
<taxonomicName class="Eudicots" family="Ericaceae" genus="Arctostaphylos" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Arctostaphylos" order="Ericales" pageId="0" pageNumber="1" phylum="Magnoliophyta" rank="genus">
<emphasis italics="true" pageId="0" pageNumber="1">Arctostaphylos</emphasis>
</taxonomicName>
, 
<taxonomicName class="Dicotyledoneae" family="Ericaceae" genus="Empetrum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Empetrum" order="Ericales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="genus">
