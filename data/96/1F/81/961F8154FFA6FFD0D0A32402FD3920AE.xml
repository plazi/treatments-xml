<taxonomicName box="[602,818,1933,1955]" pageId="7" pageNumber="580" rank="species" species="cohnhafti" status="sp. nov." subGenus="Miroculis">
(
<emphasis box="[611,708,1933,1955]" italics="true" pageId="7" pageNumber="580">Miroculis</emphasis>
) 
<taxonomicName box="[722,818,1933,1955]" pageId="7" pageNumber="580" rank="species" species="cohnhafti" status="sp. nov.">
<emphasis box="[722,818,1933,1955]" italics="true" pageId="7" pageNumber="580">cohnhafti</emphasis>
</taxonomicName>
</taxonomicName>
</taxonomicName>
<taxonomicNameLabel box="[824,904,1933,1954]" pageId="7" pageNumber="580" rank="species">
