<emphasis bold="true" box="[199,353,186,212]" pageId="8" pageNumber="110">Distribution.</emphasis>
<collectingCountry box="[359,474,187,213]" name="Cambodia" pageId="8" pageNumber="110">Cambodia</collectingCountry>
(
<collectingRegion box="[489,624,187,214]" country="Cambodia" name="Mondolkiri" pageId="8" pageNumber="110">Mondulkiri</collectingRegion>
