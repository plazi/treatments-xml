<taxonomicName authority="Lohmander, 1945" authorityName="Lohmander" authorityYear="1945" class="Arachnida" family="Clubionidae" genus="Epiclubiona" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epiclubiona" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="genus">
