<taxonomicName authorityName="Stal" authorityYear="1866" box="[1330,1436,1883,1905]" class="Insecta" family="Reduviidae" genus="Pyrrhosphodrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="397" phylum="Arthropoda" rank="species" species="militaris">
