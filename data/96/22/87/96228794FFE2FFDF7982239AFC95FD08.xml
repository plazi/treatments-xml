, 
<collectionCode box="[417,473,1266,1287]" pageId="21" pageNumber="1166">JNRC</collectionCode>
, 
<collectionCode box="[481,542,1266,1287]" pageId="21" pageNumber="1166">RHTC</collectionCode>
, 
