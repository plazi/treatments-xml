<taxonomicName class="Arachnida" family="Harpirhynchidae" genus="Harpyrhynchoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="calidris" status="sp. nov.">
<emphasis italics="true" pageId="22" pageNumber="23">Harpyrhynchoides calidris</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[247,340,439,464]" pageId="22" pageNumber="23" rank="species">
<emphasis bold="true" box="[247,333,439,464]" pageId="22" pageNumber="23">sp. nov</emphasis>
.
</taxonomicNameLabel>
(see differential diagnosis above) and 
<taxonomicName box="[797,930,439,464]" class="Arachnida" family="Harpirhynchidae" genus="Harpyrhynchoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="capellae">
