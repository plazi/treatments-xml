<typeStatus box="[394,492,757,782]" pageId="107" pageNumber="108">holotype</typeStatus>
and some 
<typeStatus box="[613,720,758,782]" pageId="107" pageNumber="108">paratypes</typeStatus>
of 
<taxonomicName box="[758,1122,757,782]" class="Arachnida" family="Harpirhynchidae" genus="Neharpyrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="chlorospingus">
<emphasis box="[758,1122,757,782]" italics="true" pageId="107" pageNumber="108">Neharpyrhynchus chlorospingus</emphasis>
</taxonomicName>
