<paragraph blockId="44.[151,1437,1834,2003]" pageId="44" pageNumber="45">
MALE (
<number box="[300,312,1835,1859]" pageId="44" pageNumber="45" value="1.0">1</number>
<typeStatus box="[323,422,1835,1859]" pageId="44" pageNumber="45">paratype</typeStatus>
, 
<figureCitation box="[437,533,1834,1859]" captionStart="FIGURE 37" captionStartId="45.[151,250,1336,1358]" captionTargetBox="[151,1423,456,1301]" captionTargetId="figure@45.[151,1436,445,1315]" captionTargetPageId="45" captionText="FIGURE 37. Harpyrhynchoides coccyzus sp. nov., male: A—dorsal view; B—ventral view." httpUri="https://zenodo.org/record/399874/files/figure.png" pageId="44" pageNumber="45">Figs. 37</figureCitation>
, 
