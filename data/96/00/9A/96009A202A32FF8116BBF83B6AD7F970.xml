<number box="[941,1012,1632,1657]" pageId="104" pageNumber="105" value="33.5" valueMax="35.0" valueMin="32.0">32–35</number>
long in 
<taxonomicName box="[1115,1309,1632,1657]" class="Arachnida" family="Harpirhynchidae" genus="Neharpyrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="chlorospingus">
<emphasis box="[1115,1309,1632,1657]" italics="true" pageId="104" pageNumber="105">N. chlorospingus</emphasis>
</taxonomicName>
), subequal palpalae 
<emphasis box="[251,281,1668,1693]" italics="true" pageId="104" pageNumber="105">dF</emphasis>
and 
<emphasis box="[336,370,1668,1693]" italics="true" pageId="104" pageNumber="105">dG</emphasis>
(vs. 
