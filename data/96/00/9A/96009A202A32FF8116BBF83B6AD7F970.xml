<number box="[462,497,1668,1693]" pageId="104" pageNumber="105" value="1.5">1.5</number>
times longer than 
<emphasis box="[705,739,1668,1693]" italics="true" pageId="104" pageNumber="105">dG</emphasis>
in 
<taxonomicName box="[774,878,1669,1693]" class="Arachnida" family="Harpirhynchidae" genus="Neharpyrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="spinus">
<emphasis box="[774,878,1669,1693]" italics="true" pageId="104" pageNumber="105">N. spinus</emphasis>
</taxonomicName>
and 
<number box="[932,967,1669,1693]" pageId="104" pageNumber="105" value="1.3">1.3</number>
times longer in 
<taxonomicName box="[1148,1337,1668,1693]" class="Arachnida" family="Harpirhynchidae" genus="Neharpyrhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="chlorospingus">
<emphasis box="[1148,1337,1668,1693]" italics="true" pageId="104" pageNumber="105">N. chlorospingus</emphasis>
