(
<taxonomicName box="[723,905,1374,1399]" class="Aves" kingdom="Animalia" order="Columbiformes" pageId="43" pageNumber="44" phylum="Chordata" rank="order">Columbiformes</taxonomicName>
: 
<taxonomicName box="[919,1058,1374,1399]" class="Aves" family="Columbidae" kingdom="Animalia" order="Columbiformes" pageId="43" pageNumber="44" phylum="Chordata" rank="family">Columbidae</taxonomicName>
