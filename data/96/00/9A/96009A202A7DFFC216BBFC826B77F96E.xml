<subSubSection pageId="43" pageNumber="44" type="etymology">
<paragraph blockId="43.[151,1437,1014,1759]" box="[199,1378,1338,1363]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[199,326,1338,1363]" pageId="43" pageNumber="44">Etymology</emphasis>
