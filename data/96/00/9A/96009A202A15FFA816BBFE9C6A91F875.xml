<typeStatus box="[151,201,530,551]" pageId="65" pageNumber="66">Type</typeStatus>
species: 
<taxonomicName box="[294,517,529,551]" class="Arachnida" family="Harpirhynchidae" genus="Crassacarus" kingdom="Animalia" order="Trombidiformes" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="alexfaini" status="sp. nov." superOrder="Acariformes">
<emphasis box="[294,517,529,551]" italics="true" pageId="65" pageNumber="66">Crassacarus alexfaini</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[523,603,529,551]" pageId="65" pageNumber="66" rank="species">
<emphasis bold="true" box="[523,598,529,550]" pageId="65" pageNumber="66">sp. nov</emphasis>
