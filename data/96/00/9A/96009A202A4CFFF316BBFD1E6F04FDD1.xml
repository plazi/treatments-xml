in 
<collectionCode LSID="urn:lsid:biocol.org:col:34094" box="[541,618,152,176]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34094" name="Ohio State University Acarology Laboratory" pageId="26" pageNumber="27">OSAL</collectionCode>
