<taxonomicName authority="Walker" authorityName="Walker" authorityYear="1853" baseAuthorityName="Walker" box="[298,476,813,838]" class="Insecta" family="Rhinophoridae" genus="Tachina" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="924" phylum="Arthropoda" rank="species" species="apicalis">
