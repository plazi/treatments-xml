(van der Wulp, 
<number box="[492,550,777,802]" pageId="20" pageNumber="924" value="1867.0">1867</number>
) (
<bibRefCitation author="O'Hara" box="[577,910,777,802]" firstAuthor="O'Hara" pageId="20" pageNumber="924" refString="O'Hara, J. E. &amp; Wood, D. M. (2004) Catalogue of the Tachinidae (Diptera) of America north of Mexico. Memoirs on Entomology (International), 18, 1 - 410." type="journal article" year="2004">
O’Hara &amp; Wood 
<number box="[788,849,778,802]" pageId="20" pageNumber="924" value="2004.0">2004</number>
: 
<number box="[864,910,778,802]" pageId="20" pageNumber="924" value="321.0">321</number>
</bibRefCitation>
)
</taxonomicName>
