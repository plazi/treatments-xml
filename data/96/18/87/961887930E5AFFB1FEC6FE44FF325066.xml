</paragraph>
<paragraph blockId="13.[106,1096,758,874]" pageId="13" pageNumber="178">
Spannweite 
<quantity box="[235,313,758,782]" metricMagnitude="-2" metricUnit="m" metricValue="2.5" pageId="13" pageNumber="178" unit="mm" value="25.0">25 mm</quantity>
