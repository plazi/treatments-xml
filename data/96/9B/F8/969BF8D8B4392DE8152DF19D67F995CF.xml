<subSubSection pageId="null" pageNumber="147" type="vernacular_name">
<paragraph pageId="null" pageNumber="147">
<normalizedToken originalValue="Vierblättriger" pageId="null" pageNumber="147">Vierblaettriger</normalizedToken>
Kleefarn
</paragraph>
