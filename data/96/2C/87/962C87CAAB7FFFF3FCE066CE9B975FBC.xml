<subSubSection pageId="34" pageNumber="35" type="description">
<paragraph blockId="34.[166,695,759,833]" pageId="34" pageNumber="35">
<taxonomicName authority="Moure, 1958 b: 188" authorityName="Moure" authorityPageNumber="188" authorityYear="1958" class="Insecta" family="Halictidae" genus="Augochloropsis" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="subGenus" subGenus="Glyptochlora">
<emphasis box="[166,337,759,780]" italics="true" pageId="34" pageNumber="35">Augochloropsis</emphasis>
(
<emphasis box="[360,505,759,780]" italics="true" pageId="34" pageNumber="35">Glyptochlora</emphasis>
) Moure, 1958b: 188
</taxonomicName>
. 
<typeStatus box="[251,304,785,806]" pageId="34" pageNumber="35">Type</typeStatus>
species: 
<taxonomicName authority="Smith, 1879" authorityName="Smith" authorityYear="1879" class="Insecta" family="Halictidae" genus="Megalopta" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="ornata">
<emphasis box="[414,612,786,807]" italics="true" pageId="34" pageNumber="35">Megalopta ornata</emphasis>
Smith, 1879
</taxonomicName>
