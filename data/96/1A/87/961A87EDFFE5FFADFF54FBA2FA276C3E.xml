<number box="[1437,1449,1148,1169]" pageId="4" pageNumber="153" value="2.0">2</number>
<number box="[136,148,1180,1201]" pageId="4" pageNumber="153" value="2.0">2</number>
With 
<number box="[242,254,1180,1201]" pageId="4" pageNumber="153" value="4.0">4</number>
