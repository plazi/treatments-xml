<paragraph blockId="6.[151,1437,295,501]" pageId="6" pageNumber="165">
<emphasis bold="true" box="[151,351,295,320]" pageId="6" pageNumber="165">
<taxonomicName box="[151,346,295,320]" class="Insecta" family="Cecidomyiidae" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="165" phylum="Arthropoda" rank="subFamily" subFamily="Porricondylinae">Porricondylinae</taxonomicName>
