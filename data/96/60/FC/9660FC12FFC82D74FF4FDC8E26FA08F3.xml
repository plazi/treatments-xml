<taxonomicName authority="Kieffer" authorityName="Kieffer" box="[847,1043,367,392]" class="Insecta" family="Cecidomyiidae" genus="Dicerura" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="165" phylum="Arthropoda" rank="genus">
