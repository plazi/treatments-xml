<paragraph blockId="11.[151,1436,650,819]" pageId="11" pageNumber="170">
<taxonomicName box="[151,387,651,675]" class="Insecta" family="Cecidomyiidae" genus="Aprionus" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="170" phylum="Arthropoda" rank="species" species="caucasicus">
