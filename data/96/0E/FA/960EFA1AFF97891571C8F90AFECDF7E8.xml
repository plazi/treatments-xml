<taxonomicName authority="Reitter, 1889" authorityName=": Reitter" authorityYear="1889" box="[1097,1339,930,949]" class="Insecta" family="Tenebrionidae" genus="Calyptopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="338" phylum="Arthropoda" rank="species" species="emarginata">
