<collectingCountry box="[351,415,949,972]" name="China" pageId="3">China</collectingCountry>
(
<collectingRegion box="[429,557,949,972]" country="China" name="Guangdong" pageId="3">Guangdong</collectingRegion>
, 
<collectingRegion box="[568,662,949,972]" country="China" name="Guangxi" pageId="3">Guangxi</collectingRegion>
