<taxonomicName authorityName="Morrone" authorityYear="1994" class="Insecta" family="Curculionidae" genus="Nacodius" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="10" pageNumber="25" phylum="Arthropoda" rank="species" species="brevirostris">
