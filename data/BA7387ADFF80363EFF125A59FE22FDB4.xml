<emphasis id="00AEEAA9FF87363FFE1A5AA7FD9AF80A" box="[415,574,1933,1958]" italics="true" pageId="12" pageNumber="313">G. solidissima</emphasis>
<bibRefCitation id="564B4B4AFF87363FFDC25AA4FD46F80A" author="Tomlin" box="[583,738,1933,1958]" pageId="12" pageNumber="313" refString="Tomlin, J. R. le B. (1932) Reports on the Marine Mollusca in the collections of the South African Museum. Annals of the South African Museum, 30, 157 - 169." type="journal article" year="1932">Tomlin, 1932</bibRefCitation>
</taxonomicName>
, and 
<taxonomicName id="F5DA4D38FF87363FFCA45AA7FBC6F80A" authority="Tomlin, 1945" authorityName="Tomlin" authorityYear="1945" box="[801,1122,1933,1958]" class="Gastropoda" family="Buccinidae" genus="Glypteuthria" kingdom="Animalia" order="Neogastropoda" pageId="12" pageNumber="313" phylum="Mollusca" rank="species" species="sculpturata">
<emphasis id="00AEEAA9FF87363FFCA45AA7FC64F80A" box="[801,960,1933,1958]" italics="true" pageId="12" pageNumber="313">G. sculpturata</emphasis>
<bibRefCitation id="564B4B4AFF87363FFC4F5AA4FBC6F80A" author="Tomlin" box="[970,1122,1933,1958]" pageId="12" pageNumber="313" refString="Tomlin, J. R. le B. (1945) Two South African species renamed. Journal of Conchology, 22, 135." type="journal article" year="1945">Tomlin, 1945</bibRefCitation>
</taxonomicName>
(new name for 
<taxonomicName id="F5DA4D38FF87363FFA9E5AA7FE89F865" authority="Tomlin, 1932" authorityName="Tomlin" authorityYear="1932" class="Gastropoda" family="Buccinidae" genus="Glypteuthria" kingdom="Animalia" order="Neogastropoda" pageId="12" pageNumber="313" phylum="Mollusca" rank="species" species="capensis">
