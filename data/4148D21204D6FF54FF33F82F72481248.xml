<taxonomicName id="0EE1188704D7FF54FB95FD8F732D126C" baseAuthorityName="Muller" baseAuthorityYear="1776" box="[1073,1343,577,603]" class="Holothuroidea" family="Synaptidae" genus="Leptosynapta" kingdom="Animalia" order="Apodida" pageId="135" pageNumber="136" phylum="Echinodermata" rank="species" species="inhaerens">
