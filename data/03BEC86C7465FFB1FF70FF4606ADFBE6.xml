<paragraph id="8BA8797A7465FFB1FF70FC41064EFC76" blockId="6.[151,1436,732,1081]" box="[151,776,912,937]" pageId="6" pageNumber="97">
<taxonomicName id="4C1702F97465FFB1FF70FC41070CFC76" authority="Aube, 1838" authorityName="Aube" authorityYear="1838" box="[151,586,912,937]" class="Insecta" family="Dytiscidae" genus="Meridiorhantus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="97" phylum="Arthropoda" rank="species" species="limbatus" status="n. comb.">
