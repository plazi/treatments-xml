<treatmentCitationGroup id="D8FEC3666A5BFFF6FFC0FC98096A2CF6" pageId="21" pageNumber="930">
<taxonomicName id="3FEE9FCB6A5BFFF6FFC0FC98094D2C38" baseAuthorityName="Keij" baseAuthorityYear="1957" box="[113,324,854,878]" class="Ostracoda" family="Cushmanideidae" genus="Cushmanidea" kingdom="Animalia" order="Podocopida" pageId="21" pageNumber="930" phylum="Arthropoda" rank="species" species="grosjeani">
