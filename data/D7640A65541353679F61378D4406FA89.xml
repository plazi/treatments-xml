<taxonomicName id="AA270E500423576B873E7A04B4BAAF08" class="Liliopsida" family="Commelinaceae" genus="Commelina" kingdom="Plantae" lsidName="" order="Commelinales" pageId="0" pageNumber="43" phylum="Tracheophyta" rank="family">Commelinaceae</taxonomicName>
