<taxonomicName id="6DA767AD73BC9C8AEA180B92D554726D" class="Magnoliopsida" family="Solanaceae" genus="Lycianthes" kingdom="Plantae" lsidName="" order="Solanales" pageId="0" pageNumber="43" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
