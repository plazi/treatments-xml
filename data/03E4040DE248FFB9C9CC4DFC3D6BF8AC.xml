(
<figureCitation id="1376A99EE248FFB9C9C44DD138B1FF75" box="[159,252,186,210]" captionStart="FIGURES 1–11" captionStartId="3.[151,264,1906,1929]" captionTargetBox="[202,1375,193,1882]" captionTargetId="figure-131@3.[202,1375,193,1882]" captionTargetPageId="3" captionText="FIGURES 1–11. Head, anterior view. 1. Homoneura (Homoneura) baihualingensis sp. nov.; 2. H. (H.) brevituba sp. nov.; 3. H. (H.) conica sp. nov.; 4. H. (H.) cornis sp. nov.; 5. H. (H.) dulongjiangica sp. nov.; 6. H. (H.) ensata sp. nov.; 7. H. (H.) gemmiformis sp. nov.; 8. H. (H.) latizona sp. nov.; 9. H. (H.) longiprocessa sp. nov.; 10. H. (H.) pilifera sp. nov.; 11. H. (H.) trilineata sp. nov.." pageId="25" pageNumber="26">Figs. 11</figureCitation>
, 
<figureCitation id="1376A99EE248FFB9C8534DD1396BFF75" box="[264,294,186,210]" captionStart="FIGURES 12–22" captionStartId="4.[151,264,1877,1900]" captionTargetBox="[185,1402,208,1840]" captionTargetId="figure-133@4.[185,1402,193,1856]" captionTargetPageId="4" captionText="FIGURES 12–22. Thorax, dorsal view. 12. Homoneura (Homoneura) baihualingensis sp. nov.; 13. H. (H.) brevituba sp. nov.; 14. H. (H.) conica sp. nov.; 15. H. (H.) cornis sp. nov.; 16. H. (H.) dulongjiangica sp. nov.; 17. H. (H.) ensata sp. nov.; 18. H. (H.) gemmiformis sp. nov.; 19. H. (H.) latizona sp. nov.; 20. H. (H.) longiprocessa sp. nov.; 21. H. (H.) pilifera sp. nov.; 22. H. (H.) trilineata sp. nov.." pageId="25" pageNumber="26">22</figureCitation>
, 
<figureCitation id="1376A99EE248FFB9C8694DD1391DFF75" box="[306,336,186,210]" captionStart="FIGURES 23–33" captionStartId="5.[151,264,1877,1900]" captionTargetBox="[207,1392,212,1827]" captionTargetId="figure-129@5.[185,1402,193,1856]" captionTargetPageId="5" captionText="FIGURES 23–33. Wing. 23. Homoneura (Homoneura) baihualingensis sp. nov.; 24. H. (H.) brevituba sp. nov.; 25. H. (H.) conica sp. nov.; 26. H. (H.) cornis sp. nov.; 27. H. (H.) dulongjiangica sp. nov.; 28. H. (H.) ensata sp. nov.; 29. H. (H.) gemmiformis sp. nov.; 30. H. (H.) latizona sp. nov.; 31. H. (H.) longiprocessa sp. nov.; 32. H. (H.) pilifera sp. nov.; 33. H. (H.) trilineata sp. nov.." pageId="25" pageNumber="26">33</figureCitation>
, 
<figureCitation id="1376A99EE248FFB9C8004DD139EEFF75" box="[347,419,186,210]" captionStart-0="FIGURES 86–90" captionStart-1="FIGURES 91–95" captionStartId-0="24.[151,264,1939,1962]" captionStartId-1="26.[151,264,1656,1679]" captionTargetBox-0="[151,1424,509,1906]" captionTargetBox-1="[170,1431,215,1619]" captionTargetId-0="figure-204@24.[151,1436,483,1918]" captionTargetId-1="figure-126@26.[151,1436,195,1635]" captionTargetPageId-0="24" captionTargetPageId-1="26" captionText-0="FIGURES 86–90. Homoneura (Homoneura) pilifera sp. nov. Male. 86. epandrial complex, posterior view; 87. protandrium and epandrium, lateral view; 88. protandrium, anterior view; 89. aedeagal complex, ventral view; 90. aedeagal complex, lateral view. Scale 0.5 mm." captionText-1="FIGURES 91–95. Homoneura (Homoneura) trilineata sp. nov. Male. 91. protandrium and epandrium, lateral view; 92. epandrial complex, posterior view; 93. protandrium, anterior view; 94. aedeagal complex, ventral view; 95. aedeagal complex, lateral view. Scale 1 mm." pageId="25" pageNumber="26">90–95</figureCitation>
)
