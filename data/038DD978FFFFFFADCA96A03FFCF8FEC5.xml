locality: Sandakan, Borneo. 
<taxonomicName id="4C2413EDFFFFFFAECA96A094FDCEF823" authority="Barlow, 1982" authorityName="Barlow" authorityYear="1982" box="[151,517,2010,2033]" class="Insecta" family="Bombycidae" genus="Prismosticta" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="66" phylum="Arthropoda" rank="species" species="tiretta">
