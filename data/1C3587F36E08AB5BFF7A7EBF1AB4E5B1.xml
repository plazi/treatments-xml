, 
<number id="225424C26E17AB5BFA8D79F31D85E496" box="[1376,1432,667,693]" pageId="17" pageNumber="18" value="0.24">0.24</number>
, 
<number id="225424C26E17AB5BFF7A79A818D0E4F9" box="[151,205,704,730]" pageId="17" pageNumber="18" value="1.58">1.58</number>
, 
<number id="225424C26E17AB5BFF3479A81912E4F9" box="[217,271,704,730]" pageId="17" pageNumber="18" value="1.04">1.04</number>
