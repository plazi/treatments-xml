</taxonomicName>
, 
<taxonomicName id="31BA4D1BFF8C1B41FD121C30FCC98E84" authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[684,800,871,897]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="salmoni">
<emphasis id="C4CEEA8AFF8C1B41FD121C30FCC98E84" box="[684,800,871,897]" italics="true" pageId="58" pageNumber="59">R. salmoni</emphasis>
</taxonomicName>
, 
<taxonomicName id="31BA4D1BFF8C1B41FC911C30FC6C8E85" authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[815,901,871,896]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="scuta">
<emphasis id="C4CEEA8AFF8C1B41FC911C30FC6C8E85" box="[815,901,871,896]" italics="true" pageId="58" pageNumber="59">R. scuta</emphasis>
</taxonomicName>
