</emphasis>
<taxonomicName id="31BA4D1BFF8C1B41FE8F1C13FDFA8E58" authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[305,531,836,862]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="propinqua">
