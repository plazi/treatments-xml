<taxonomicName id="31BA4D1BFF8C1B41FC011C30FBFD8E85" authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[959,1044,871,896]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="wisei">
