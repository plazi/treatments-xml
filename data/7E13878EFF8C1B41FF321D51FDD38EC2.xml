<taxonomicName id="31BA4D1BFF8C1B41FB9F1C13FF268E84" authorityName="Rix &amp; Harvey" authorityYear="2010" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
<emphasis id="C4CEEA8AFF8C1B41FB9F1C13FF268E84" italics="true" pageId="58" pageNumber="59">Rayforstia</emphasis>
</taxonomicName>
described from 
<collectingCountry id="8EAD7608FF8C1B41FE361C30FDC88E84" box="[392,545,871,897]" name="New Zealand" pageId="58" pageNumber="59">New Zealand</collectingCountry>
(
<taxonomicName id="31BA4D1BFF8C1B41FD8C1C30FD778E84" authorityName="Rix &amp; Harvey" authorityYear="2010" baseAuthorityName="Rix &amp; Harvey" baseAuthorityYear="2010" box="[562,670,871,897]" class="Arachnida" family="Micropholcommatidae" genus="Rayforstia" kingdom="Animalia" order="Araneae" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="plebeia">
