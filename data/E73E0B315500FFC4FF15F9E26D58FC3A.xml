<emphasis id="5DE366355503FFCAFF15F8456D02F81C" bold="true" box="[151,341,1973,1996]" pageId="22" pageNumber="223">FIGURES 57–61.</emphasis>
<taxonomicName id="A897C1A45503FFCAFEDEF8466E1AF81B" authority="García Aldrete, 2017" authorityName="García Aldrete" authorityYear="2017" box="[348,589,1974,1996]" class="Insecta" family="Lachesillidae" genus="Lachesilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocodea" pageId="22" pageNumber="223" phylum="Arthropoda" rank="species" species="walsinghami" status="n. sp.">
<emphasis id="5DE366355503FFCAFEDEF8466E1AF81B" box="[348,589,1974,1996]" italics="true" pageId="22" pageNumber="223">Lachesilla walsinghami</emphasis>
</taxonomicName>
<taxonomicNameLabel id="46D0DB4E5503FFCAFDD7F8466ED8F81C" box="[597,655,1974,1995]" pageId="22" pageNumber="223" rank="species">
