</taxonomicName>
, 
<taxonomicName id="D6B50DDDFFC6FFC34522FAEDFE33FA29" authorityName="Keraudren" authorityYear="1983" box="[270,402,1388,1414]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="3" pageNumber="84" phylum="Tracheophyta" rank="species" species="humbertii">
