<paragraph id="110A765EFFC6FFC3445DFAADFEA7FA69" blockId="3.[113,647,1293,1478]" pageId="3" pageNumber="84">
<taxonomicName id="D6B50DDDFFC6FFC3445DFAADFEEFFAE8" authorityName="M. Hughes" authorityYear="2011" box="[113,334,1325,1351]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="3" pageNumber="84" phylum="Tracheophyta" rank="species" species="masoalaensis">
