<taxonomicName id="B8C1A3C27D01FFCD3424FF0A8BA86F9B" authority="Holthuis, 1951" authorityName="Holthuis" authorityYear="1951" box="[294,707,222,244]" class="Malacostraca" family="Palaemonidae" genus="Pseudocoutierea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="538" phylum="Arthropoda" rank="species" species="elegans">
