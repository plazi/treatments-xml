BL = 
<quantity id="4CDFA384FFD91874FE8F968CC19FFCEB" box="[295,400,767,791]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="11" pageNumber="2003" unit="mm" value="2.8">2.80 mm</quantity>
in 
<typeStatus id="549CB0C3FFD91874FE6A968CC229FCEB" box="[450,550,767,791]" pageId="11" pageNumber="2003">holotype</typeStatus>
(
<quantity id="4CDFA384FFD91874FD93968CC29FFCEB" box="[571,656,767,791]" metricMagnitude="-2" metricUnit="m" metricValue="7.7216" pageId="11" pageNumber="2003" unit="in" value="3.04">3.04 in</quantity>
<specimenCount id="9D21C5E8FFD91874FD359773C2EDFCEB" box="[669,738,767,791]" pageId="11" pageNumber="2003" type="female">one ♀</specimenCount>
<typeStatus id="549CB0C3FFD91874FD59968CC357FCEB" box="[753,856,767,791]" pageId="11" pageNumber="2003">paratype</typeStatus>
), ThL = 
<quantity id="4CDFA384FFD91874FC7D968CC431FCEB" box="[981,1086,767,791]" metricMagnitude="-3" metricUnit="m" metricValue="1.28" pageId="11" pageNumber="2003" unit="mm" value="1.28">1.28 mm</quantity>
(1.36), WL = 
