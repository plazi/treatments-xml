<collectingCountry id="A1FD70D27E57FF90FC2213B13CADFA11" box="[941,990,1477,1496]" name="India" pageId="9" pageNumber="10">India</collectingCountry>
, 
<collectingCountry id="A1FD70D27E57FF90FC6913B23B32FA10" box="[998,1089,1478,1497]" name="Myanmar" pageId="9" pageNumber="10">Myanmar</collectingCountry>
, 
<collectingCountry id="A1FD70D27E57FF90FBC713B13BEEFA11" box="[1096,1181,1477,1496]" name="Thailand" pageId="9" pageNumber="10">Thailand</collectingCountry>
, 
<collectingCountry id="A1FD70D27E57FF90FB2913B23BA7FA10" box="[1190,1236,1478,1497]" name="Laos" pageId="9" pageNumber="10">Laos</collectingCountry>
, 
<collectingCountry id="A1FD70D27E57FF90FB5313B13A66FA11" box="[1244,1301,1477,1496]" name="China" pageId="9" pageNumber="10">China</collectingCountry>
, 
<collectingCountry id="A1FD70D27E57FF90FA9213B13A2FFA11" box="[1309,1372,1477,1496]" name="Russia" pageId="9" pageNumber="10">Russia</collectingCountry>
, 
<collectingCountry id="A1FD70D27E57FF90FAEA13B23AD3FA10" box="[1381,1440,1478,1497]" name="South Korea" pageId="9" pageNumber="10">Korea</collectingCountry>
. 
<emphasis id="EB9EEC507E57FF90FCBD13963CE7FA3C" bold="true" box="[818,916,1506,1525]" pageId="9" pageNumber="10">Discussion</emphasis>
. This is a high altitude species, found between 
<quantity id="1E129DA77E57FF90FAEC13973AD1FA3F" box="[1379,1442,1507,1526]" metricMagnitude="2" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="10" unit="m" value="200.0">200 m</quantity>
