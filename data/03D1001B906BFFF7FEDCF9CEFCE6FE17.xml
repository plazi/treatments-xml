<number id="3DB0A32A906BFFF6FEDCF9CEFEFFF9CF" box="[264,310,1548,1574]" pageId="6" pageNumber="65" value="374.0">374</number>
. 
<taxonomicName id="4C78CA8E906BFFF6FE97F9CEFD13F9CF" authority="Fabricius, 1804" authorityName="Fabricius" authorityYear="1804" box="[323,730,1548,1574]" class="Insecta" family="Ichneumonidae" genus="Ophion" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="pennator">
<emphasis id="B90C6D1F906BFFF6FE97F9CEFDDBF9CF" bold="true" box="[323,530,1548,1574]" italics="true" pageId="6" pageNumber="65">Ophion pennator</emphasis>
<bibRefCitation id="EFE9CCFC906BFFF6FDCEF9CEFD13F9CF" author="Fabricius" box="[538,730,1548,1574]" pageId="6" pageNumber="67" refString="Fabricius, J. C. (1804) Systema Piezatorum. Carolum Reichard, Brunsviga. 439 pp." type="book" year="1804">
Fabricius, 
<number id="3DB0A32A906BFFF6FD4BF9CEFD13F9CF" box="[671,730,1548,1574]" pageId="6" pageNumber="65" value="1804.0">1804</number>
</bibRefCitation>
