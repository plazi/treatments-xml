<paragraph blockId="8.[843,1057,334,356]" box="[843,1057,334,356]" pageId="8" pageNumber="647">
<heading box="[843,1057,334,356]" centered="true" fontSize="9" level="2" pageId="8" pageNumber="647" reason="2">
<emphasis bold="true" box="[843,1057,334,356]" pageId="8" pageNumber="647">
<taxonomicName authorityName="Chaudoir" authorityYear="1866" box="[843,911,334,356]" class="Insecta" family="Carabidae" genus="Agra" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="647" phylum="Arthropoda" rank="species" species="truquii">
