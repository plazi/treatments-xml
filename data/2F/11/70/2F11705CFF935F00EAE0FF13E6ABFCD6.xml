, PL 
<quantity box="[1079,1174,350,373]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="3" pageNumber="534" unit="mm" value="0.5">0.50 mm</quantity>
, PW 
<quantity box="[1227,1322,350,373]" metricMagnitude="-4" metricUnit="m" metricValue="9.5" pageId="3" pageNumber="534" unit="mm" value="0.95">0.95 mm</quantity>
