<paragraph blockId="45.[151,1436,1895,2028]" pageId="45" pageNumber="46">
<taxonomicName box="[199,314,1895,1919]" class="Dothideomycetes" family="Euantennariaceae" genus="Antennula" higherTaxonomySource="GBIF" kingdom="Fungi" order="Capnodiales" pageId="45" pageNumber="46" phylum="Ascomycota" rank="genus">
