(
<emphasis bold="true" box="[292,404,1569,1595]" italics="true" pageId="0" pageNumber="579">Ocybatus</emphasis>
) 
<emphasis bold="true" box="[421,572,1569,1595]" italics="true" pageId="0" pageNumber="579">flaviguttatus</emphasis>
