: 53 (50 – 60); 
<emphasis box="[826,876,1562,1586]" italics="true" pageId="2" pageNumber="194">f1-f1</emphasis>
: 35 (35 – 45); 
<emphasis box="[1040,1096,1562,1586]" italics="true" pageId="2" pageNumber="194">f1-h1</emphasis>
: 28 (34 – 35); 
<emphasis box="[1260,1322,1562,1586]" italics="true" pageId="2" pageNumber="194">h1-h1</emphasis>
: 16 (15 – 19); 
<emphasis box="[875,937,1597,1621]" italics="true" pageId="2" pageNumber="194">h1-h2</emphasis>
: 29 (21 – 30); 
<emphasis box="[1095,1157,1597,1621]" italics="true" pageId="2" pageNumber="194">h2-h2</emphasis>
