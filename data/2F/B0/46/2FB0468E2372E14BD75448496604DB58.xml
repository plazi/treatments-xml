<taxonomicName class="Polychaeta" family="Syllidae" genus="Branchiosyllis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Branchiosyllis exilis" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="exilis">Branchiosyllis exilis</taxonomicName>
