<td box="[704,795,1604,1630]" gridcol="1" gridrow="0" pageId="10" pageNumber="138">Exopod</td>
<td box="[1154,1260,1604,1630]" gridcol="2" gridrow="0" pageId="10" pageNumber="138">Endopod</td>
</tr>
