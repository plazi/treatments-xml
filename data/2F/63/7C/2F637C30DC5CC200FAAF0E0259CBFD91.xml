<quantity box="[617,762,1560,1586]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" metricValueMax="1.6" metricValueMin="1.4" pageId="1" pageNumber="30" unit="mm" value="1.5" valueMax="1.6" valueMin="1.4">1.4–1.6 mm</quantity>
