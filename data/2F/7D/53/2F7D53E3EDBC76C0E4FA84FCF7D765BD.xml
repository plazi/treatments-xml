, 
<taxonomicName class="Arachnida" genus="Yuria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Yuria pulcra" order="Opiliones" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pulcra">Yuria pulcra</taxonomicName>
