<paragraph pageId="333" pageNumber="334">Records.</paragraph>
<paragraph pageId="333" pageNumber="334">
<emphasis bold="true" pageId="333" pageNumber="334">USA</emphasis>
: AZ, CA, CO, ID, NM, UT, WA
</paragraph>
