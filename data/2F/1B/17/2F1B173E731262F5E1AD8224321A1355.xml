<subSubSection pageId="11" pageNumber="36" type="notes">
<paragraph pageId="11" pageNumber="36">Notes.</paragraph>
<paragraph pageId="11" pageNumber="36">
<taxonomicName class="Sordariomycetes" family="Nectriaceae" genus="Calonectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Calonectria montana" order="Hypocreales" pageId="11" pageNumber="36" phylum="Ascomycota" rank="species" species="montana">Calonectria montana</taxonomicName>
