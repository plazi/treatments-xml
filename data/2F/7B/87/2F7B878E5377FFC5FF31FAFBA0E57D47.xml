<emphasis box="[926,1132,152,175]" italics="true" pageId="7" pageNumber="64">Crocidura sokolovi</emphasis>
</taxonomicName>
and 
<taxonomicName box="[1185,1317,152,175]" class="Mammalia" family="Soricidae" genus="Crocidura" kingdom="Animalia" order="Soricomorpha" pageId="7" pageNumber="64" phylum="Chordata" rank="species" species="attenuata">
