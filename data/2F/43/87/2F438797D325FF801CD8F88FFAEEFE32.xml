</taxonomicName>
[
<taxonomicName authorityName="Martinov" authorityYear="1820" box="[952,1130,1014,1041]" class="Magnoliopsida" family="Phyllanthaceae" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="293" phylum="Tracheophyta" rank="family">Phyllanthaceae</taxonomicName>
], Shulan Yang
</materialsCitation>
. 
