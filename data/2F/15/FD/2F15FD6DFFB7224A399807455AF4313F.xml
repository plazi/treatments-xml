<number box="[429,516,692,717]" pageId="4" pageNumber="395" value="5.15" valueMax="5.4" valueMin="4.9">4.9–5.4</number>
; length of mid femur 5.0; length of mid tibia 5.0– 
<number box="[1085,1120,692,717]" pageId="4" pageNumber="395" value="5.1">5.1</number>
; length of hind femur 
<number pageId="4" pageNumber="395" value="10.6" valueMax="10.7" valueMin="10.5">10.5– 10.7</number>
