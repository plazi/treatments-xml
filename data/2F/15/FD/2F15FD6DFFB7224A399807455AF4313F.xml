<number box="[428,544,728,752]" pageId="4" pageNumber="395" value="10.350000000000001" valueMax="10.4" valueMin="10.3">10.3–10.4</number>
; length of first segment of hind tarsi (without apical spine) 
<number box="[1203,1241,727,752]" pageId="4" pageNumber="395" value="1.6">1.6</number>
; length of second segment 
<number box="[251,289,764,789]" pageId="4" pageNumber="395" value="0.5">0.5</number>
