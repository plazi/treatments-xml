<collectingRegion box="[598,788,1218,1245]" country="Indonesia" name="Papua Barat" pageId="1" pageNumber="2">Papua Province</collectingRegion>
, Yapen Island, Ambaidiru, 
<quantity box="[1120,1214,1219,1245]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="1" pageNumber="2" unit="m" value="1000.0">1000 m</quantity>
, 
