<taxonomicName box="[1109,1252,1473,1497]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="1" pageNumber="58" phylum="Nematoda" rank="genus">
