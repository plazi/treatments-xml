<number box="[923,965,1473,1497]" pageId="1" pageNumber="58" value="570.0">570</number>
(
<number box="[983,1088,1473,1497]" pageId="1" pageNumber="58" value="607.5" valueMax="655.0" valueMin="560.0">560–655</number>
