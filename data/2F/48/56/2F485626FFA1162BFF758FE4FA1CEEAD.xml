<number box="[1422,1435,1628,1651]" pageId="1" pageNumber="50" value="2.0">2</number>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="1" pageNumber="50">
<paragraph blockId="1.[151,1437,1532,1971]" pageId="1" pageNumber="50">
<keyLead pageId="1" pageNumber="50">
<number box="[151,167,1660,1683]" pageId="1" pageNumber="50" value="2.0">2</number>
