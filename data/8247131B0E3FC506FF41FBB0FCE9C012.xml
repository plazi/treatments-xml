<taxonomicName id="CDEED98E0E3EC506FDC2FB30FDFAC1DE" box="[523,612,1140,1166]" class="Agaricomycetes" family="Cortinariaceae" genus="Cortinarius" kingdom="Fungi" order="Agaricales" pageId="5" pageNumber="4" phylum="Basidiomycota" rank="section" section="Illumini">
