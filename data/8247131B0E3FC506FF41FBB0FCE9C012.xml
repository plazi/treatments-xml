<taxonomicName id="CDEED98E0E3EC506FC7DFB14FB4EC13A" authorityName="M. L. Xie, T. Z. Wei &amp; Y. Li" authorityYear="2021" box="[948,1232,1104,1130]" class="Agaricomycetes" family="Cortinariaceae" genus="Cortinarius" kingdom="Fungi" order="Agaricales" pageId="5" pageNumber="6" phylum="Basidiomycota" rank="species" species="khinganensis">
