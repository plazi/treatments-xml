<collectingCountry id="F317766EFF96FFD8FEE5D87BFE5EFC84" box="[359,502,799,824]" name="South Africa" pageId="38" pageNumber="39">
<collectingRegion id="49C4F81CFF96FFD8FEE5D87BFE00FC84" box="[359,424,799,824]" country="Tanzania" name="Kusini Unguja" pageId="38" pageNumber="39">South</collectingRegion>
