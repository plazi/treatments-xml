<emphasis id="1A9713699C14FFD84712FA55FDF01BA7" box="[332,553,1466,1491]" italics="true" pageId="91" pageNumber="92">
<taxonomicName id="EFE3B4F89C14FFD84712FA55FE4B1BA7" box="[332,402,1466,1491]" class="Gastropoda" family="Plakobranchidae" genus="Elysia" kingdom="Animalia" order="Sacoglossa" pageId="91" pageNumber="92" phylum="Mollusca" rank="genus">Elysia</taxonomicName>
