<taxonomicName id="4C753F91FFFF9875FE9F28BB48AEC1FD" authorityName="Lelej" authorityYear="1979" box="[305,462,2009,2033]" class="Insecta" family="Mutillidae" genus="Orientilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="vietnamica">
