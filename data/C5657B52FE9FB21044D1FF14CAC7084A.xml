<taxonomicName id="8ACCB1C7FE9FB2104674FCCFCB200832" baseAuthorityName="Dana" baseAuthorityYear="1852" box="[562,806,774,798]" class="Malacostraca" family="Upogebiidae" genus="Upogebia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="259" pageNumber="260" phylum="Arthropoda" rank="species" species="pugettensis">
