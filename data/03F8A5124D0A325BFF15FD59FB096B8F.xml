<taxonomicName id="4C516F874D0A325BFAB8FABBFEDD6B3F" authority="Strobl. This" authorityName="Strobl. This" class="Insecta" family="Mycetophilidae" genus="Novakia" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="38" phylum="Arthropoda" rank="genus">
