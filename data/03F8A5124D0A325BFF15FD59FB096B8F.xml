<taxonomicName id="4C516F874D0A325BFE5EFA0BFC626BD7" authority="Winnertz 1849" authorityName="Winnertz" authorityYear="1849" box="[476,927,1408,1434]" class="Insecta" family="Mycetophilidae" genus="Tetragoneura" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="38" phylum="Arthropoda" rank="species" species="distincta">
