<taxonomicName id="4C25CDD8C30CFF8CFDC9FD6645E7E680" box="[629,717,667,688]" class="Insecta" family="Hydrophilidae" genus="Globulosis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="278" phylum="Arthropoda" rank="species" species="flavus">
