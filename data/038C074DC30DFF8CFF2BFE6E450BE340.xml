<taxonomicName id="4C25CDD8C30DFF8DFDC9F86645E7E380" box="[629,717,1947,1968]" class="Insecta" family="Hydrophilidae" genus="Globulosis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="277" phylum="Arthropoda" rank="species" species="flavus">
