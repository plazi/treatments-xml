<number id="D71A248BFF85480B4051F9CFFE096B72" box="[369,489,1536,1562]" pageId="16" pageNumber="17" value="0.2" valueMax="0.21" valueMin="0.19">0.19–0.21</number>
, PL 
<number id="D71A248BFF85480B4301F9CFFDB76B72" box="[545,599,1536,1562]" pageId="16" pageNumber="17" value="0.16">0.16</number>
