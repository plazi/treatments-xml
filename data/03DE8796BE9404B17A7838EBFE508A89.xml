<subSubSection id="C36D650BBE9404B37A7838EBFC9E8E7F" pageId="173" pageNumber="173" type="nomenclature">
<paragraph id="8BC83680BE9404B37A7838EBFB838D81" blockId="173.[817,1111,449,472]" box="[817,1111,449,472]" pageId="173" pageNumber="173">
<heading id="D08081ECBE9404B37A7838EBFB838D81" box="[817,1111,449,472]" centered="true" fontSize="9" level="10" pageId="173" pageNumber="173" reason="4">
<taxonomicName id="4C774D03BE9404B37A7838EBFC1B8D8E" box="[817,975,449,472]" class="Insecta" family="Miridae" genus="Pulvillophylus" kingdom="Animalia" order="Hemiptera" pageId="173" pageNumber="173" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B903EA92BE9404B37A7838EBFC1B8D8E" bold="true" box="[817,975,449,472]" italics="true" pageId="173" pageNumber="173">Pulvillophylus</emphasis>
</taxonomicName>
