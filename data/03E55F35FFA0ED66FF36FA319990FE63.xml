<taxonomicName id="4C4C95A0FFA0ED67FBC1FE6E9E73FE61" authorityName="Chamberlin and Ivie, O" authorityYear="1935" class="Arachnida" family="Oonopidae" genus="Orchestina" kingdom="Animalia" order="Araneae" pageId="28" phylum="Arthropoda" rank="species" species="utahana">
