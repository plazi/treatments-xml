<emphasis id="46DE290FFFF2FF92EDF049CFFC55F50E" bold="true" box="[747,902,2728,2757]" pageId="16" pageNumber="223">Taxonomy.</emphasis>
<taxonomicName id="B3AA8E9EFFF2FF92ECBF49CFFCE3F520" authority="Pucheran, 1855" authorityName="Pucheran" authorityYear="1855" class="Mammalia" family="Viverridae" genus="Genetta" kingdom="Animalia" order="Carnivora" pageId="16" pageNumber="223" phylum="Chordata" rank="species" species="servalina">Genetta servalina Pucheran, 1855</taxonomicName>
, 
<materialsCitation id="C4C2FF40FFF2FF92EC5B49A9FC77F520" ID-GBIF-Occurrence="3712312308" box="[832,932,2766,2795]" pageId="16" pageNumber="223">
