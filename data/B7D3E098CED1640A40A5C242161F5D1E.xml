<taxonomicName id="2C54133E917B1E02D9D896E85A21AD8D" authority="Hulcr &amp; Cognato, 2020" authorityName="Hulcr &amp; Cognato" authorityYear="2020" baseAuthorityName="Blandford" baseAuthorityYear="1894" class="Insecta" family="Curculionidae" genus="Planiculus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Planiculus bicolor" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="bicolor">
<emphasis id="269C72979F5260622FB1C6F7EA668B7B" italics="true" pageId="0" pageNumber="1">Planiculus bicolor</emphasis>
</taxonomicName>
