<taxonomicName id="52792CAF7C05FD1B82B4CCC9C32A1411" authorityName="Chaudhri, Akbar &amp; Rasool" authorityYear="1974" class="Arachnida" family="Tetranychidae" genus="Oligonychus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oligonychus longus" order="Trombidiformes" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="longus">Oligonychus longus</taxonomicName>
