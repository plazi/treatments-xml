<taxonomicName class="Liliopsida" family="Poaceae" genus="Poa" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Poa" order="Poales" pageId="0" pageNumber="17" phylum="Tracheophyta" rank="genus">
