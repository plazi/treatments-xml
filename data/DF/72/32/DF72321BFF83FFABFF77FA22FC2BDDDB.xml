<taxonomicName authorityName="Hartley, Andrews &amp; McHugh" authorityYear="2008" box="[722,906,437,458]" class="Insecta" family="Latridiidae" genus="Akalyptoischion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="bathytrematos">
