<taxonomicName authority="Forskal 1775, p. 125" authorityName="Forskal" authorityPageNumber="125" authorityYear="1775" box="[516,956,1304,1331]" class="Cephalopoda" family="Nautilidae" genus="Nautilus" kingdom="Animalia" order="Nautilida" pageId="6" pageNumber="157" phylum="Mollusca" rank="species" species="pertusus">
