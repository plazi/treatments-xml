<taxonomicName authorityName="de Montfort" authorityYear="1808" box="[641,761,1484,1510]" class="Tubothalamea" family="Peneroplidae" genus="Peneroplis" kingdom="Chromista" order="Miliolida" pageId="6" pageNumber="157" phylum="Foraminifera" rank="genus">
<emphasis box="[641,761,1484,1510]" italics="true" pageId="6" pageNumber="157">Peneroplis</emphasis>
</taxonomicName>
