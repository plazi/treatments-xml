<typeStatus box="[391,495,585,611]" pageId="9" pageNumber="310">Holotype</typeStatus>
male, labeled “ 
<collectingCountry box="[685,784,585,611]" name="Curaçao" pageId="9" pageNumber="310">Curaçao</collectingCountry>
, Christoffel Nat. Pk., 
<location LSID="urn:lsid:plazi:treatment:DF72B52DDE01FF80FF541905FE2E0954:520452E0DE0FFF80FBD91F6FFB430F00" box="[1050,1220,585,611]" country="Curaçao" latitude="12.344619" name="Christoffelberg" pageId="9" pageNumber="310">Christoffelberg</location>
trailhead parking, 
<geoCoordinate box="[151,522,620,647]" pageId="9" pageNumber="310">12°20ʹ40.63ʹʹ N, 69°67ʹ11.38ʹʹ W</geoCoordinate>
, 
<date box="[533,654,621,647]" pageId="9" pageNumber="310" value="2014-11-09">
<collectingDate box="[533,654,621,647]" pageId="9" pageNumber="310" value="2014-11-09">9-XI-2014</collectingDate>
</date>
, 
<collectorName box="[665,829,621,647]" pageId="9" pageNumber="310">M. C. Thomas</collectorName>
, 
<collectingMethod box="[840,928,621,647]" pageId="9" pageNumber="310">BL trap</collectingMethod>
” (deposited in 
<collectionCode box="[1097,1172,621,647]" country="USA" httpUri="http://grbio.org/cool/jekt-x82m" name="Florida State Collection of Arthropods, The Museum of Entomology" pageId="9" pageNumber="310" type="Museum">FSCA</collectionCode>
