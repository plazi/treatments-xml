<materialsCitation ID-GBIF-Occurrence="2556250904" collectingDate="2014-11-09" collectingMethod="BL trap" collectionCode="FSCA" collectorName="M. C. Thomas" country="Curaçao" latitude="12.344619" location="Christoffelberg" pageId="9" pageNumber="310" specimenCount="1" typeStatus="holotype">
