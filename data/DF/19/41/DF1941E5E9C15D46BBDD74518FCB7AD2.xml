<taxonomicName authority="var. scortechinii" class="Magnoliopsida" family="Capparaceae" genus="Capparis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Capparis scortechinii subsp. var. var. scortechinii" order="Brassicales" pageId="0" pageNumber="99" phylum="Tracheophyta" rank="variety" species="scortechinii" subSpecies="var." variety="scortechinii">Capparis scortechinii var. scortechinii</taxonomicName>
