<quantity box="[193,346,620,644]" metricMagnitude="3" metricUnit="m" metricValue="1.75" metricValueMax="2.0" metricValueMin="1.5" pageId="8" pageNumber="9" unit="m" value="1750.0" valueMax="2000.0" valueMin="1500.0">1500-2000 m</quantity>
