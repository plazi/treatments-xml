<quantity box="[620,725,906,930]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" metricValueMax="1.1" metricValueMin="0.9" pageId="18" pageNumber="19" unit="mm" value="10.0" valueMax="11.0" valueMin="9.0">9–11 mm</quantity>
(
<typeStatus box="[739,843,906,930]" pageId="18" pageNumber="19">holotype</typeStatus>
: about 
<quantity box="[922,1011,906,930]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" pageId="18" pageNumber="19" unit="mm" value="9.5">9.5 mm</quantity>
); fore wing length 
<quantity box="[1221,1361,906,930]" metricMagnitude="-2" metricUnit="m" metricValue="1.075" metricValueMax="1.15" metricValueMin="1.0" pageId="18" pageNumber="19" unit="mm" value="10.75" valueMax="11.5" valueMin="10.0">10–11.5 mm</quantity>
(
<typeStatus pageId="18" pageNumber="19">holotype</typeStatus>
: about 
<quantity box="[283,388,942,966]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" pageId="18" pageNumber="19" unit="mm" value="10.5">10.5 mm</quantity>
