<paragraph blockId="3.[151,1436,1494,1547]" pageId="3" pageNumber="540">
<taxonomicName authority="Banks, 1947" authorityName="Banks" authorityYear="1947" box="[151,549,1494,1516]" class="Insecta" family="Pompilidae" genus="Austrochares" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="540" phylum="Arthropoda" rank="species" species="brevitarsus">
