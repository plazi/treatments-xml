<paragraph blockId="13.[264,1322,1033,1139]" pageId="13" pageNumber="60">
<taxonomicName authorityName="Kroyer" authorityYear="1855" box="[312,544,1073,1098]" class="Malacostraca" family="Sergestidae" genus="Sergestes" kingdom="Animalia" order="Decapoda" pageId="13" pageNumber="60" phylum="Arthropoda" rank="species" species="edwardsii">
