<taxonomicName box="[242,381,1277,1301]" class="Polypodiopsida" kingdom="Plantae" order="Polypodiales" pageId="16" pageNumber="199" phylum="Tracheophyta" rank="order">Polypodiales</taxonomicName>
(suborder 
<taxonomicName authority="Dumort." authorityName="Dumort." box="[504,764,1277,1302]" class="Polypodiopsida" kingdom="Plantae" order="Polypodiales" pageId="16" pageNumber="199" phylum="Tracheophyta" rank="subOrder" subOrder="Polypodiineae">Polypodiineae Dumort.</taxonomicName>
