<paragraph blockId="73.[144,758,418,541]" pageId="73" pageNumber="773">
<emphasis box="[144,296,489,510]" italics="true" pageId="73" pageNumber="773">Type species:</emphasis>
<taxonomicName authority="Marks" authorityName="Marks" authorityYear="1963" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="73" pageNumber="773" phylum="Arthropoda" rank="species" species="spilotus" subGenus="Ochlerotatus">
<taxonomicName box="[313,661,488,510]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="73" pageNumber="773" phylum="Arthropoda" rank="species" species="spilotus" subGenus="Ochlerotatus">
<emphasis box="[313,378,488,509]" italics="true" pageId="73" pageNumber="773">Aedes</emphasis>
(
<emphasis box="[403,548,488,509]" italics="true" pageId="73" pageNumber="773">Ochlerotatus</emphasis>
