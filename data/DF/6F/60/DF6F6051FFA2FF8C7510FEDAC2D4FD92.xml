<emphasis box="[864,882,336,357]" italics="true" pageId="66" pageNumber="766">
<emphasis box="[883,1020,339,355]" italics="true" pageId="66" pageNumber="766">BIFIDISTYLUS</emphasis>
</emphasis>
REINERT, HARBACH &amp; KITCHING
</taxonomicName>
,
</heading>
</paragraph>
<paragraph blockId="66.[826,1441,335,457]" box="[1075,1192,368,389]" pageId="66" pageNumber="766">
<heading allCaps="true" box="[1075,1192,368,389]" centered="true" fontSize="8" level="5" pageId="66" pageNumber="766" reason="4">
<taxonomicNameLabel box="[1075,1192,368,389]" pageId="66" pageNumber="766">GEN. NOV.</taxonomicNameLabel>
</heading>
</paragraph>
<paragraph blockId="66.[826,1441,335,457]" pageId="66" pageNumber="766">
<emphasis box="[826,969,406,427]" italics="true" pageId="66" pageNumber="766">Type species:</emphasis>
<taxonomicName authority="Edwards" authorityName="Edwards" authorityYear="1923" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="766" phylum="Arthropoda" rank="species" species="lamborni" subGenus="Aedimorphus">
<taxonomicName box="[975,1326,406,427]" class="Insecta" family="Culicidae" genus="Aedes" kingdom="Animalia" order="Diptera" pageId="66" pageNumber="766" phylum="Arthropoda" rank="species" species="lamborni" subGenus="Aedimorphus">
<emphasis box="[975,1040,406,427]" italics="true" pageId="66" pageNumber="766">Aedes</emphasis>
(
<emphasis box="[1055,1207,406,427]" italics="true" pageId="66" pageNumber="766">Aedimorphus</emphasis>
) 
<emphasis box="[1222,1326,406,427]" italics="true" pageId="66" pageNumber="766">lamborni</emphasis>
</taxonomicName>
<bibRefCitation author="Edwards FW." journalOrPublisher="Bulletin of Entomological Research" pageId="66" pageNumber="766" pagination="397 - 399" part="13" refString="Edwards FW. 1923 a. Four new African mosquitos [sic]. Bulletin of Entomological Research 13: 397 - 399." title="Four new African mosquitos [sic]" type="journal article" year="1923">Edwards, 1923</bibRefCitation>
</taxonomicName>
.
</paragraph>
