species: 
<taxonomicName authority="Worthy, 2011" authorityName="Worthy" authorityYear="2011" box="[319,607,535,556]" class="Aves" family="Phalacrocoracidae" genus="Nambashag" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pelecaniformes" pageId="7" pageNumber="284" phylum="Chordata" rank="species" species="billerooensis" status="sp. nov.">Nambashag billerooensis</taxonomicName>
