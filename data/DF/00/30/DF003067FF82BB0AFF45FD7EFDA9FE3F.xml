<subSubSection box="[151,495,759,786]" pageId="4" pageNumber="535" type="nomenclature">
<paragraph blockId="4.[151,495,759,786]" box="[151,495,759,786]" pageId="4" pageNumber="535">
<heading bold="true" box="[151,495,759,786]" fontSize="11" level="1" pageId="4" pageNumber="535" reason="1">
<emphasis bold="true" box="[151,495,759,786]" pageId="4" pageNumber="535">
Genus 
<taxonomicName box="[237,405,759,785]" class="Chromadorea" family="Sphaerulariidae" genus="Veleshkinema" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Tylenchida" pageId="4" pageNumber="535" phylum="Nematoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[237,405,759,785]" italics="true" pageId="4" pageNumber="535">Veleshkinema</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[413,495,760,786]" pageId="4" pageNumber="535" rank="genus">n. gen.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
