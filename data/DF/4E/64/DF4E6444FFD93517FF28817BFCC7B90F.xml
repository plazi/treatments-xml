<taxonomicName authority="Park and Byun, 2008: 110" authorityName="Park and Byun" authorityPageNumber="110" authorityYear="2008" box="[295,808,1082,1104]" class="Insecta" family="Lecithoceridae" genus="Pectinimura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="44" phylum="Arthropoda" rank="species" species="montiatilis">
