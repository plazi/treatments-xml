<vernacularName box="[1528,1763,688,709]" pageId="33" pageNumber="338">Tamarin a face marbrée</vernacularName>
/ 
<emphasis box="[1784,1876,688,709]" pageId="33" pageNumber="338">German:</emphasis>
<vernacularName box="[1886,2112,688,709]" pageId="33" pageNumber="338">Marmorgesichttamarin</vernacularName>
/ 
<emphasis box="[2134,2225,688,709]" pageId="33" pageNumber="338">Spanish:</emphasis>
<vernacularName box="[2233,2406,688,709]" pageId="33" pageNumber="338">Tamarin jaspeado</vernacularName>
<emphasis box="[1442,1689,728,749]" pageId="33" pageNumber="338">Other common names:</emphasis>
