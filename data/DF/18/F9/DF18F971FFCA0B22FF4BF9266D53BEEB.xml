<taxonomicName authority="(Gordon, 1976)" authorityName="Gordon" authorityYear="1976" baseAuthorityName="Gordon" baseAuthorityYear="1976" class="Insecta" family="Coccinellidae" genus="Decadiomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="264" phylum="Arthropoda" rank="species" species="austrinus">
