<taxonomicName authority="Segarra, 2014" authorityName="Segarra" authorityYear="2014" box="[232,600,620,645]" class="Insecta" family="Coccinellidae" genus="Decadiomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="264" phylum="Arthropoda" rank="species" species="seini">
