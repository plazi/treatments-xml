<taxonomicName authority="(Fall, 1901)" authorityName="Fall" authorityYear="1901" baseAuthorityName="Fall" baseAuthorityYear="1901" box="[523,796,583,608]" class="Insecta" family="Coccinellidae" genus="Decadiomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="264" phylum="Arthropoda" rank="species" species="taedatus">
