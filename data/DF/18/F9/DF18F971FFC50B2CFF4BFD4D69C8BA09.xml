<taxonomicName authority="Vandenberg &amp; Hanson, 2019" authorityName="Vandenberg &amp; Hanson" authorityYear="2019" box="[249,378,1531,1553]" class="Magnoliopsida" family="Piperaceae" genus="Moiradiomus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Piperales" pageId="7" pageNumber="264" phylum="Magnoliopsida" rank="genus" status="gen. nov.">
