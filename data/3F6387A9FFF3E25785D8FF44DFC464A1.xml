<taxonomicName id="70CA4D3CFFF3E2578489FEF4DE0D6469" box="[454,771,295,320]" class="Cubozoa" family="Chiropsalmidae" genus="Chiropsalmus" kingdom="Animalia" order="Chirodropida" pageId="30" pageNumber="31" phylum="Cnidaria" rank="species" species="quadrumanus">
