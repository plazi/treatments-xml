<taxonomicName id="4C6CA1CF6605B47DC4104F0C3D8325C4" box="[1324,1433,1148,1173]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="80" pageNumber="81" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
