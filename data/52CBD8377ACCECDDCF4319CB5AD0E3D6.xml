<taxonomicName id="5002C8649D80B242399CAD14B63C8244" LSID="urn:lsid:zoobank.org:act:FD590891-3410-449E-9961-33FC189561CC" authority="Heiss, 2013" authorityName="Heiss" authorityYear="2013" class="Insecta" family="Aradidae" genus="Onorecoris" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Onorecoris" order="Hemiptera" pageId="10" pageNumber="147" phylum="Arthropoda" rank="genus">Onorecoris</taxonomicName>
