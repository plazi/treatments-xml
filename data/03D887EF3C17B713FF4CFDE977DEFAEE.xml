<quantity id="4C899B1C3C17B713FCF1FDCE7704FD51" box="[783,843,639,659]" metricMagnitude="4" metricUnit="m" metricValue="1.9" pageId="92" pageNumber="705" unit="km" value="19.0">19 km</quantity>
NW Nono, 
<quantity id="4C899B1C3C17B713FC41FDCE77B1FD51" box="[959,1022,639,659]" metricMagnitude="3" metricUnit="m" metricValue="2.7" pageId="92" pageNumber="705" unit="m" value="2700.0">2700m</quantity>
