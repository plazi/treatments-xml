<collectionCode id="0699AE3FD74C9D54FBF1F8BD10C45103" box="[1101,1152,1848,1872]" pageId="8" pageNumber="799">SVL</collectionCode>
<quantity id="A7709B1FD74C9D54FB32F8BD10A7511C" box="[1166,1251,1848,1872]" metricMagnitude="-2" metricUnit="m" metricValue="5.7" pageId="8" pageNumber="799" unit="mm" value="57.0">57 mm</quantity>
