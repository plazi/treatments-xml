<quantity id="A7709B1FD74C9D54FA99F8BD11C05103" box="[1317,1412,1848,1872]" metricMagnitude="-1" metricUnit="m" metricValue="1.66" pageId="8" pageNumber="799" unit="mm" value="166.0">166 mm</quantity>
