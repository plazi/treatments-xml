<typeStatus id="54F18862FFB1FFCDDF81F08AE7840CD6" box="[663,756,1701,1724]" pageId="29" pageNumber="86">holotype</typeStatus>
and 
<specimenCount id="9D4CFD49FFB1FFCDDDD1F0EAE42E0CB6" box="[199,350,1733,1756]" pageId="29" pageNumber="86" type="generic" typeStatus="paratypes">five paratypes</specimenCount>
