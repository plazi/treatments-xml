<number id="9044DB75FFDEFFB8FC29F97DC4EF228B" box="[959,1000,1700,1724]" pageId="0" pageNumber="41" value="139.0">139</number>
° 
<number id="9044DB75FFDEFFB8FC67F97DC34E228B" box="[1009,1097,1700,1724]" pageId="0" pageNumber="41" value="916817.0">916817</number>
E, 
<specimenCount id="308A02DBFFDEFFB8FBF9F97DC3AF228B" box="[1135,1192,1700,1724]" pageId="0" pageNumber="41" type="generic">
<quantity id="E17464B7FFDEFFB8FBF9F97DC3A2228B" box="[1135,1189,1700,1724]" metricMagnitude="1" metricUnit="m" metricValue="5.2" pageId="0" pageNumber="41" unit="m" value="52.0">52m</quantity>
.
