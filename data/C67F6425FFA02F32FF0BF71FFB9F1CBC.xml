<subSubSection id="06CC86B8FFA02F31FF5BF5B3FC0019F9" box="[199,999,1948,1973]" pageId="1" pageNumber="157" type="etymology">
<paragraph id="4E69D533FFA02F31FF5BF5B3FC0019F9" blockId="1.[151,1437,1588,1973]" box="[199,999,1948,1973]" pageId="1" pageNumber="157">
<emphasis id="7CA20921FFA02F31FF5BF5B3FEA119F9" bold="true" box="[199,326,1948,1973]" pageId="1" pageNumber="157">Etymology</emphasis>
