<collectingCountry id="DEDF764C7B1BFFE0FEFFFF50FE6DFF7B" box="[359,426,153,179]" name="China" pageId="6" pageNumber="559">China</collectingCountry>
(
<collectingRegion id="640CF83E7B1BFFE0FE21FF50FDE7FF7C" box="[441,544,153,180]" country="China" name="Guangxi" pageId="6" pageNumber="559">Guangxi</collectingRegion>
