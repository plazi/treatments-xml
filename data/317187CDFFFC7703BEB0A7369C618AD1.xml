<taxonomicName id="7ED84D58FFFC7702BA2AA45D9C168CBF" authority="Witenberg, 1923" authorityName="Witenberg" authorityYear="1923" box="[1037,1400,1661,1686]" class="Trematoda" family="Cyclocoelidae" genus="Prohyptiasmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Echinostomida" pageId="16" pageNumber="17" phylum="Platyhelminthes" rank="genus">
