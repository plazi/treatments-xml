<collectionCode id="F693AE61FF98FF9BFBB02C520CD9BF4E" box="[1123,1185,1880,1904]" country="Canada" httpUri="http://grbio.org/cool/a5s1-0agm" name="Provincial Museum of Alberta" pageId="11">PMA</collectionCode>
