<taxonomicName id="4C5E4D26D44BFFC3FF681629FCCF6D42" authority="(Stainton)" baseAuthorityName="Stainton" baseAuthorityYear="1851" box="[140,770,1116,1144]" class="Insecta" family="Nepticulidae" genus="Ectoedemia" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="atrifrontella" subGenus="Zimmermannia">
