(5, 23, 24); 
<taxonomicName id="4C2678DDFF89FF8E8C07C233215E9A2D" class="Actinopterygii" family="Salmonidae" genus="Salvelinus" kingdom="Animalia" order="Salmoniformes" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="malma">
<emphasis id="B952DF4CFF89FF8E8C07C233215E9A2D" italics="true" pageId="56" pageNumber="57">Salvelinus malma</emphasis>
</taxonomicName>
(6)
</paragraph>
