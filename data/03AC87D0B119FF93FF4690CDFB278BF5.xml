<paragraph id="8BBA36C6B119FF92FF4690CDFEFB8EC4" blockId="12.[151,297,1737,1764]" box="[151,297,1737,1764]" pageId="12" pageNumber="13">
<heading id="D0F281AAB119FF92FF4690CDFEFB8EC4" bold="true" box="[151,297,1737,1764]" fontSize="11" level="1" pageId="12" pageNumber="13" reason="1">
<emphasis id="B971EAD4B119FF92FF4690CDFEFB8EC4" bold="true" box="[151,297,1737,1764]" pageId="12" pageNumber="13">
<taxonomicName id="4C054D45B119FF92FF4690CDFF2C8EC3" box="[151,254,1738,1764]" class="Reptilia" family="Colubridae" genus="Lycodon" kingdom="Animalia" order="Squamata" pageId="12" pageNumber="13" phylum="Chordata" rank="genus">
<emphasis id="B971EAD4B119FF92FF4690CDFF2C8EC3" bold="true" box="[151,254,1738,1764]" italics="true" pageId="12" pageNumber="13">Lycodon</emphasis>
