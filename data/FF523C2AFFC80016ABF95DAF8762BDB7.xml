<quantity id="B00320D9FFC90016A802580786DCBD5F" box="[876,965,260,285]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="5" pageNumber="485" unit="mm" value="1.6">1.6 mm</quantity>
