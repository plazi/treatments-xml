<emphasis id="82F63128976E9C14FF06FA34FECC0597" box="[151,298,1502,1527]" italics="true" pageId="18" pageNumber="19">
<typeStatus id="6F395398976E9C14FF06FA34FF360597" box="[151,208,1502,1527]" pageId="18" pageNumber="19">Type</typeStatus>
