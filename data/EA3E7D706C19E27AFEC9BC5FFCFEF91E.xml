<taxonomicName id="A597B7E56C1BE27AFD49B942FC28FEE5" box="[752,902,328,352]" class="Arachnida" family="Ameroseiidae" genus="Neocypholaelaps" kingdom="Animalia" order="Mesostigmata" pageId="18" pageNumber="716" phylum="Arthropoda" rank="species" species="ceylonicus">
