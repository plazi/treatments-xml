<taxonomicName id="16AECEBCC363BA23FF18F96C8328F924" authority="Bouche, 1844" authorityName="Bouche" authorityYear="1844" box="[151,564,1744,1770]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="tuberculatus">
