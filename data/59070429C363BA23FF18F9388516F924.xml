<taxonomicName id="16AECEBCC363BA23FD87F91682A8F90A" authority="Kotinsky, 1908" authorityName="Kotinsky" authorityYear="1908" box="[520,948,1706,1732]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="tuberculatus">
