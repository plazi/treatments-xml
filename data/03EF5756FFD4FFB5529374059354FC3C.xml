<number id="3D8EF467FFDAFFB750F575739538F8F4" box="[753,806,1840,1864]" pageId="9" pageNumber="428" value="0.14">0.14</number>
; HL, 
<number id="3D8EF467FFDAFFB7516275739584F8F4" box="[870,922,1840,1864]" pageId="9" pageNumber="428" value="0.34">0.34</number>
; PW, 
<number id="3D8EF467FFDAFFB751D97573920FF8F4" box="[989,1041,1840,1864]" pageId="9" pageNumber="428" value="0.21">0.21</number>
; ML, 
<number id="3D8EF467FFDAFFB7565275739295F8F4" box="[1110,1163,1840,1864]" pageId="9" pageNumber="428" value="0.12">0.12</number>
