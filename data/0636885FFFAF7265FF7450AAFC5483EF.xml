<paragraph id="8E203949FFAF7265FF245219FB9E831A" blockId="30.[151,1437,1446,1759]" box="[199,1043,1554,1579]" pageId="30" pageNumber="31">
<emphasis id="BCEBE55BFFAF7265FF245219FED2831A" bold="true" box="[199,351,1554,1579]" pageId="30" pageNumber="31">
<typeStatus id="512487EBFFAF7265FF245219FE8F831A" box="[199,258,1554,1579]" pageId="30" pageNumber="31">Type</typeStatus>
locality
</emphasis>
: 
<collectingCountry id="F68879D9FFAF7265FE8D5219FE6D831A" box="[366,480,1554,1579]" name="Sri Lanka" pageId="30" pageNumber="31">
<collectingRegion id="4C5BF7ABFFAF7265FE8D5219FE02831A" box="[366,399,1554,1579]" country="United Kingdom" name="Surrey" pageId="30" pageNumber="31">Sri</collectingRegion>
Lanka
</collectingCountry>
: 
<collectingRegion id="4C5BF7ABFFAF7265FE085219FD21831A" box="[491,684,1554,1579]" country="Sri Lanka" name="Central" pageId="30" pageNumber="31">Central Province</collectingRegion>
