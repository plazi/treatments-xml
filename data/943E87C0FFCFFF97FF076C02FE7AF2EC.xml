<taxonomicName id="DB974D55FFCFFF97FE8A6F22FD9EF24C" authorityName="Pessagno" authorityYear="1969" box="[320,556,1547,1573]" class="Polycystina" family="Neosciadiocapsidae" higherTaxonomySource="GBIF" kingdom="Rhizaria" order="Nassellaria" pageId="41" pageNumber="46" phylum="Radiolaria" rank="family">Neosciadiocapsidae</taxonomicName>
