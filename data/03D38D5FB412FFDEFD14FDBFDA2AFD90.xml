<taxonomicName id="4C7A47CAB412FFDCFCECF922DB8AF95E" authorityName="Chevalier" authorityYear="1961" box="[797,968,1673,1697]" class="Anthozoa" family="Plesiastreidae" genus="Plesiastrea" kingdom="Animalia" order="Scleractinia" pageId="5" phylum="Cnidaria" rank="species" species="tazarinensis" subGenus="Paleoplesiastraea">
