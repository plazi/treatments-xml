<taxonomicName id="6FAC4D41FFBB2852FC9BFA0F2AC8A51E" authority="(Esben-Petersen 1914)" authorityName="Esben-Petersen" authorityYear="1914" baseAuthorityName="Esben-Petersen" baseAuthorityYear="1914" box="[833,1229,1526,1551]" class="Insecta" family="Bittacidae" genus="Neobittacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mecoptera" pageId="2" pageNumber="305" phylum="Arthropoda" rank="genus">
