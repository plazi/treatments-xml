<taxonomicName id="AAC94D464B2AFFC3FEE6F929FE3E41D8" authorityName="Krishnachaitanya and Manickavasagam" authorityYear="2016" box="[260,385,1745,1768]" class="Insecta" family="Encyrtidae" genus="Homalotylus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="2379" phylum="Arthropoda" rank="species" species="montanus">
