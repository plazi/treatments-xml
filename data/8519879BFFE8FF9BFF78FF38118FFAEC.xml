<collectingCountry id="75A7761DFFE8FF9BFE77FAB512F9FA84" box="[408,471,1306,1332]" name="Malta" pageId="20" pageNumber="21">Male</collectingCountry>
, 
<collectingCountry id="75A7761DFFE8FF9BFE0BFAB51151FA84" box="[484,639,1306,1332]" name="Malaysia" pageId="20" pageNumber="21">MALAYSIA</collectingCountry>
, Pahang, Bukit Fraser, 
<quantity id="CA489B68FFE8FF9BFC7FFAB510C9FA84" box="[912,999,1306,1332]" metricMagnitude="3" metricUnit="m" metricValue="1.225296" pageId="20" pageNumber="21" unit="ft" value="4020.0">4020 ft</quantity>
, Malaise in forest 
<date id="790E104DFFE8FF9BFB2EFAB5165DFA84" box="[1217,1395,1306,1332]" pageId="20" pageNumber="21" value="1997-03-14" valueMax="1997-03-17" valueMin="1997-03-14">
<number id="BB7824AAFFE8FF9BFB2EFAB51622FA84" box="[1217,1292,1306,1332]" pageId="20" pageNumber="21" value="15.5" valueMax="17.0" valueMin="14.0">14â€“17</number>
.III. 
<number id="BB7824AAFFE8FF9BFADBFAB5165DFA84" box="[1332,1395,1306,1332]" pageId="20" pageNumber="21" value="1997.0">1997</number>
