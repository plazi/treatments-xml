, 
<taxonomicName id="060E86C3F7BFE0036D3D5E22CAB12F85" authorityName="Pfeiffer" authorityYear="1857" class="Gastropoda" family="Helicidae" genus="Simpulopsis" kingdom="Animalia" lsidName="Simpulopsis (Simpulopsis) miersi" order="Pulmonata" pageId="21" pageNumber="38" phylum="Mollusca" rank="species" species="miersi" subGenus="Simpulopsis">Simpulopsis (Simpulopsis) miersi</taxonomicName>
