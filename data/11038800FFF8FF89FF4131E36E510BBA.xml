<emphasis id="ABDEE504FFF8FF89FC56331B69AD0B2A" box="[896,992,1560,1583]" italics="true" pageId="23" pageNumber="24">Parinari</emphasis>
</taxonomicName>
(
<taxonomicName id="5EAA4295FFF8FF89FC2B33146E820B35" box="[1021,1231,1559,1584]" class="Magnoliopsida" family="Chrysobalanaceae" kingdom="Plantae" order="Malpighiales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="family">Chrysobalanaceae</taxonomicName>
), 
<taxonomicName id="5EAA4295FFF8FF89FB2733146FD10B35" box="[1265,1436,1559,1584]" class="Magnoliopsida" family="Polygalaceae" genus="Xanthophyllum" kingdom="Plantae" order="Fabales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
