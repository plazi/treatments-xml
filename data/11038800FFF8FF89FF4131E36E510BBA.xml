</taxonomicName>
(
<taxonomicName id="5EAA4295FFF8FF89FF4833386B780B51" box="[158,309,1595,1620]" class="Magnoliopsida" family="Polygalaceae" kingdom="Plantae" order="Fabales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="family">Polygalaceae</taxonomicName>
) and an unidentified genus of 
<taxonomicName id="5EAA4295FFF8FF89FD5A333869950B51" authority="(Browne 1961 a)" baseAuthorityName="Browne" baseAuthorityYear="1961" box="[652,984,1595,1620]" class="Magnoliopsida" family="Annonaceae" kingdom="Plantae" order="Magnoliales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="family">
