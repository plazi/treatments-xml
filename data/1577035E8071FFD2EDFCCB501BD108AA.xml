<subSubSection id="D5C4E1C38071FFD2EDACC8781BD108AA" pageId="117" pageNumber="118" type="distribution">
<paragraph id="9D61B2488071FFD2EDACC8781BD108AA" blockId="117.[151,1436,1401,1750]" pageId="117" pageNumber="118">
<emphasis id="AFAA6E5A8071FFD2EDACC8781B5E0816" bold="true" box="[199,515,1617,1642]" pageId="117" pageNumber="118">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA8071FFD2EF78C87B1BF10816" box="[531,684,1617,1642]" country="Papua New Guinea" name="New Ireland" pageId="117" pageNumber="118">New Ireland</collectingRegion>
