<taxonomicName id="4C194D02D244FF93FC73FAF08394DD48" authority="Greene" authorityName="Greene" authorityYear="1934" box="[920,1153,1341,1368]" class="Insecta" family="Tephritidae" genus="Anastrepha" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="144" phylum="Arthropoda" rank="species" species="chiclayae">
