<paragraph id="8BD50729FFE8FFC7FF1DA1B4FCFD6CFC" blockId="1.[151,1058,942,1023]" pageId="1" pageNumber="1364">
<taxonomicName id="4C6A7CAAFFE8FFC7FF1DA1B4FEFD6CC1" authorityName="Madl" authorityYear="2019" box="[151,325,942,963]" class="Insecta" family="Halictidae" genus="Halictus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="1364" phylum="Arthropoda" rank="species" species="decorus">
