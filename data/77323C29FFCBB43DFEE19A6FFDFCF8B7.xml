<emphasis id="CDEF512DFFCBB43DFEE69A0DFEA8FB60" box="[275,344,1149,1183]" italics="true" pageId="12" pageNumber="12">
<taxonomicName id="389BF6BCFFCBB43DFEE69A0DFEA3FB60" authorityName="Diakonoff" authorityYear="1982" box="[275,339,1149,1183]" class="Insecta" family="Tortricidae" genus="Age" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="12" phylum="Arthropoda" rank="genus">Age</taxonomicName>
