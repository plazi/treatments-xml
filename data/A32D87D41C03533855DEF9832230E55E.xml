<taxonomicName id="EC844D411C025338542CFD6B22C5E372" authorityName="Kieffer" authorityYear="1898" box="[357,470,753,777]" class="Insecta" family="Cecidomyiidae" genus="Stefaniella" kingdom="Animalia" order="Diptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="genus">Stefaniella</taxonomicName>
