<taxonomicName id="4B37B52603598287FC9804B0A677CFFD" authorityName="L., Sp. Pl. 1: 156. 1753. (Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus siculus" order="Solanales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="species" species="siculus">
<emphasis id="692B4E1E20C4119CA0F6B37ADAE597AE" italics="true" pageId="117" pageNumber="118">Convolvulus siculus</emphasis>
