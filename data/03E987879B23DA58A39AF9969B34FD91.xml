<quantity id="4CB89B749B23DA59A1CDF89A9B6EF882" box="[704,787,1879,1906]" metricMagnitude="-2" metricUnit="m" metricValue="3.4" pageId="10" pageNumber="349" unit="mm" value="34.0">34 mm</quantity>
