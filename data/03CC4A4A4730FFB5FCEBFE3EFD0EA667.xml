<taxonomicName id="4C6580DF4733FFB5FD00FCDFFD1EA107" baseAuthorityName="Mickel" baseAuthorityYear="1937" box="[645,744,868,891]" class="Insecta" family="Mutillidae" genus="Sinotilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="493" phylum="Arthropoda" rank="species" species="petina">
