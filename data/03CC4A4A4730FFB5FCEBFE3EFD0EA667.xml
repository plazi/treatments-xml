<taxonomicName id="4C6580DF4733FFB5FDF6FE5FFF44A067" authorityName="Lelej" authorityYear="1996" class="Insecta" family="Mutillidae" genus="Sinotilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="493" phylum="Arthropoda" rank="species" species="lambirensis">
