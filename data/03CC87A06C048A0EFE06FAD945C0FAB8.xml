<taxonomicName id="4C654D356C058A0EFEC1FBA346B8FBD5" baseAuthorityName="W. Blanford" baseAuthorityYear="1865" box="[313,433,1122,1149]" class="Gastropoda" family="Plectopylidae" genus="Chersaecia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="41" pageNumber="42" phylum="Mollusca" rank="species" species="feddeni">
