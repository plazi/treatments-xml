<taxonomicName id="4C295DDBAE5D242D52C6FB76CBB0FB6D" box="[444,759,1225,1250]" class="Malacostraca" family="Lysiosquillidae" genus="Lysiosquilloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stomatopoda" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="siamensis">
