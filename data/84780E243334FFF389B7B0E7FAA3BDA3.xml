<geoCoordinate id="69E5D9F53335FFF38BA0B5B1FD78BDF4" box="[640,743,151,176]" direction="north" orientation="latitude" pageId="13" pageNumber="320" precision="925" value="21.866667">
<number id="BA19AD153335FFF38BA0B5B1FD02BDEB" box="[640,669,151,176]" pageId="13" pageNumber="320" value="21.0">21</number>
° 
<number id="BA19AD153335FFF38B86B5B1FD59BDEB" box="[678,710,151,176]" pageId="13" pageNumber="320" value="52.0">52</number>
' N
</geoCoordinate>
, 
<geoCoordinate id="69E5D9F53335FFF38BD2B5B1FCC3BDEB" box="[754,860,151,176]" direction="east" orientation="longitude" pageId="13" pageNumber="320" precision="925" value="107.916664">
<number id="BA19AD153335FFF38BD2B5B1FC81BDEB" box="[754,798,151,176]" pageId="13" pageNumber="320" value="107.0">107</number>
° 
<number id="BA19AD153335FFF38A08B5B1FCD9BDEB" box="[808,838,151,176]" pageId="13" pageNumber="320" value="55.0">55</number>
'E
</geoCoordinate>
, 
<quantity id="CB2912D73335FFF38A48B5BEFC20BDEB" box="[872,959,152,176]" metricMagnitude="3" metricUnit="m" metricValue="1.022" pageId="13" pageNumber="320" unit="m" value="1022.0">1022 m</quantity>
), 
<date id="786F99F23335FFF38AF3B5BEFBF3BDEB" box="[979,1132,152,176]" pageId="13" pageNumber="320" value="2011-08-17">
<number id="BA19AD153335FFF38AF3B5BEFC72BDEB" box="[979,1005,152,176]" pageId="13" pageNumber="320" value="17.0">17</number>
-VIII- 
