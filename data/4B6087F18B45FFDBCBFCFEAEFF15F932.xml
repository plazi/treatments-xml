<elevation id="48E4D1D48B5AFFDBC936FF7CFC52FEB6" box="[883,955,249,273]" metricMagnitude="-1" metricUnit="m" metricValue="5.0" pageId="16" pageNumber="622" unit="m" value="0.5">0.5 m</elevation>
