<taxonomicName id="D0AE4D1EFF93FFCAD203FEAFFE64FE8D" box="[323,410,260,285]" class="Insecta" family="Cicadellidae" genus="Litura" kingdom="Animalia" order="Hemiptera" pageId="18" pageNumber="119" phylum="Arthropoda" rank="species" species="unda">
