<taxonomicName id="4C594D52FF9AFFC1B1EEF65FFC0DF8EF" authority="Yang &amp; Hu" authorityName="Yang &amp; Hu &amp; Morse" authorityYear="2020" box="[475,948,1892,1917]" class="Insecta" family="Leptoceridae" genus="Ceraclea" kingdom="Animalia" order="Trichoptera" pageId="7" pageNumber="145" phylum="Arthropoda" rank="species" species="lamellata" status="sp. n." subGenus="Athripsodina">
