<taxonomicName id="4C594D52FF9BFFC0B12EF4CDFD4EF99E" authority="Yang &amp; Hu" authorityName="Yang &amp; Hu &amp; Morse" authorityYear="2020" box="[283,759,1526,1550]" class="Insecta" family="Leptoceridae" genus="Ceraclea" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="144" phylum="Arthropoda" rank="species" species="lamellata" status="sp. n." subGenus="Athripsodina">
<emphasis id="B92DEAC3FF9BFFC0B12EF4CDFEC1F99D" box="[283,376,1526,1550]" italics="true" pageId="6" pageNumber="144">Ceraclea</emphasis>
