<subSubSection id="D9BC651EFF95FF94D4C5E52953FC186C" pageId="73" pageNumber="74" type="reference_group">
<paragraph id="91193695FF95FF94D4C5E529550F1F92" blockId="73.[151,589,894,917]" box="[151,589,894,917]" pageId="73" pageNumber="74">
<treatmentCitationGroup id="B1B611BBFF95FF94D4C5E529550F1F92" box="[151,589,894,917]" pageId="73" pageNumber="74">
<taxonomicName id="56A64D16FF95FF94D4C5E529550B1F92" authority="Hood, 1938: 236" authorityName="Hood" authorityPageNumber="236" authorityYear="1938" box="[151,585,894,917]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="permagnus">
<emphasis id="A3D2EA87FF95FF94D4C5E52956D71F93" box="[151,405,894,917]" italics="true" pageId="73" pageNumber="74">Holopothrips permagnus</emphasis>
<treatmentCitation id="10071084FF95FF94D5C9E529550B1F92" author="" box="[411,585,894,916]" page="236" pageId="73" pageNumber="74">
<bibRefCitation id="F5374B64FF95FF94D5C9E52955551F92" box="[411,535,894,916]" pageId="73" pageNumber="74" refId="ref58731" refString="Hood, J. D. (1938) Studies in Neotropical Thysanoptera VII. Revista de Entomologia, 9 (1 - 2), 218 - 247." type="journal article">Hood, 1938</bibRefCitation>
: 236
</treatmentCitation>
</taxonomicName>
