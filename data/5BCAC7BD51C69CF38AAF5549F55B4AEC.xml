<taxonomicName id="B9FD33A8D7DBEC565A513BE4CED68AD3" authorityName="Spencer" authorityYear="1973" class="Insecta" family="Phytomyzinae" genus="Phytoliriomyza" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phytoliriomyza pilosella" order="Diptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pilosella">Phytoliriomyza pilosella</taxonomicName>
