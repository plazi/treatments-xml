<specimenCount id="9D35C7CC075CFFA141D4A4D5B5E99597" box="[432,506,1475,1498]" pageId="10" pageNumber="117" type="generic">
<quantity id="4CCBA1A0075CFFA141D4A4D5B5E49597" box="[432,503,1475,1498]" metricMagnitude="2" metricUnit="m" metricValue="4.6" pageId="10" pageNumber="117" unit="m" value="460.0">460 m</quantity>
