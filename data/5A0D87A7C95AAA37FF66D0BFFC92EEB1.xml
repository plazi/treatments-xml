</taxonomicNameLabel>
and 
<taxonomicName id="15A44D32C95AAA34FCECD633FABFEAF0" authority="Flechtmann et al. 1995" authorityName="Flechtmann et al." authorityYear="1995" box="[797,1330,1148,1173]" class="Arachnida" family="Eriophyidae" genus="Propeaceota" kingdom="Animalia" order="Prostigmata" pageId="1" pageNumber="173" phylum="Arthropoda" rank="species" species="secundum" status="n. comb.">
<emphasis id="E0D0EAA3C95AAA34FCECD633FB91EAF0" box="[797,1052,1148,1173]" italics="true" pageId="1" pageNumber="173">Propeaceota secundum</emphasis>
