<paragraph id="272B92112447FFA2513EFC31FA16FC02" blockId="3.[820,1459,966,1022]" box="[858,1459,966,990]" pageId="3" pageNumber="104">
<materialsCitation id="97FC984C2447FFA2513EFC31FA16FC02" box="[858,1459,966,990]" collectionCode="M, HNHM-PBO" pageId="3" pageNumber="104" specimenCount="1" typeStatus="lectotype">
<collectionCode id="41850AD42447FFA2513EFC31FCD7FC01" box="[858,882,966,989]" country="Germany" lsid="urn:lsid:biocol.org:col:15637" name="Botanische Staatssammlung München" pageId="3" pageNumber="104" type="Herbarium">M</collectionCode>
