<taxonomicName id="AA9C7EF9FFAFFFC1C707FA3D29CE1EA6" authority="Timm 1961" authorityName="Timm" authorityYear="1961" box="[264,652,1444,1470]" class="Adenophorea" family="Monhysteridae" genus="Amphimonhystrella" kingdom="Animalia" order="Monhysterida" pageId="2" pageNumber="21" phylum="Nematoda" rank="genus">
