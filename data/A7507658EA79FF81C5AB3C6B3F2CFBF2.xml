<emphasis id="1D8D1B5CEA78FF82C5AB3F943D1EF857" bold="true" box="[151,288,1989,2013]" pageId="17" pageNumber="18">FIGURE 10</emphasis>
. 
<taxonomicName id="E8F9BCCDEA78FF82C4113F973E2AF857" authorityName="Kiew &amp; S. Julia" authorityYear="2013" box="[301,532,1990,2013]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="magentifolia">
