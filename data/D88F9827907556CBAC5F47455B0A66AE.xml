<taxonomicName id="507C8F7252234F5778309655C59D263C" baseAuthorityName="Burnup" baseAuthorityYear="1914" class="Gastropoda" family="Streptaxidae" genus="Gulella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gulella inhluzaniensis" order="Stylommatophora" pageId="0" pageNumber="107" phylum="Mollusca" rank="species" species="inhluzaniensis">
