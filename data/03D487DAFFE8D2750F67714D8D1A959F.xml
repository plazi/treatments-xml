</emphasis>
<typeStatus id="54C6886EFFE8D2770FD371DE8B2795C8" box="[444,553,399,425]" pageId="29" pageNumber="30">Holotype</typeStatus>
male, 
<collectingCountry id="F36A765CFFE8D2770C1471DE8BDD95C8" box="[635,723,399,425]" name="Brazil" pageId="29" pageNumber="30">BRAZIL</collectingCountry>
