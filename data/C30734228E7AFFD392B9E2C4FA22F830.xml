<taxonomicName id="8CAEFEB78E7AFFD39355E2C4FE4CFAB0" box="[379,498,1271,1297]" class="Eutardigrada" family="Hypsibiidae" genus="Thulinius" kingdom="Animalia" order="Parachela" pageId="4" pageNumber="62" phylum="Tardigrada" rank="genus">
