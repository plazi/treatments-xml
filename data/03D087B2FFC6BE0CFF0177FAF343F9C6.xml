<location id="8EA6607FFFC6BE0CFE567537F6DAFC8C" LSID="urn:lsid:plazi:treatment:03D087B2FFC6BE0CFF0177FAF343F9C6:8EA6607FFFC6BE0CFE567537F6DAFC8C" box="[421,499,856,879]" country="New Caledonia" name="Known" pageId="34" pageNumber="621">Known</location>
from 
<collectingCountry id="F36E7634FFC6BE0CFDC07537F5FFFC8C" box="[563,726,855,879]" name="New Caledonia" pageId="34" pageNumber="621">New Caledonia</collectingCountry>
and 
