<taxonomicName id="4C6303A2FF8DFFBBFF0381FD62D20C33" authorityName="Kai &amp; Robillard" authorityYear="2012" box="[211,393,2034,2054]" class="Insecta" family="Haglotettigoniidae" genus="Svistella" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="413" phylum="Arthropoda" rank="species" species="chekjawa">
