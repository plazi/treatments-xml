<emphasis id="56FD4A66FFFCFFD89580B01D3132602F" bold="true" box="[501,517,1287,1306]" pageId="1" pageNumber="96">2.</emphasis>
<taxonomicName id="A389EDF7FFFCFFD89679B01231D8602E" authority="(J. E. LeConte)" baseAuthorityName="J. E. LeConte" box="[524,751,1288,1307]" class="Insecta" family="Curculionidae" genus="Eulechriops" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="96" phylum="Arthropoda" rank="species" species="minutus">
<emphasis id="56FD4A66FFFCFFD89679B012315F602E" box="[524,616,1288,1307]" italics="true" pageId="1" pageNumber="96">E. minutus</emphasis>
