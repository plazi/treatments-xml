<taxonomicName id="A389EDF7FFFCFFD8956BB01232DB602E" authority="Champion" authorityName="Champion" box="[286,492,1288,1307]" class="Insecta" family="Curculionidae" genus="Eulechriops" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="96" phylum="Arthropoda" rank="species" species="elongatus">
