<taxonomicName id="4C553AC0FFE0FA55D8553F4CFD136A81" authority="M.Hughes" authorityName="M. Hughes" authorityYear="2015" box="[279,677,1894,1920]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="species" species="kemumuensis" status="sp. nov.">
<emphasis id="B9219D51FFE0FA55D8553F4CFECF6A81" box="[279,377,1895,1920]" italics="true" pageId="19" pageNumber="20">Begonia</emphasis>
<emphasis id="B9219D51FFE0FA55D8C13F4DFDA96A81" box="[387,543,1894,1920]" italics="true" pageId="19" pageNumber="20">kemumuensis</emphasis>
M.Hughes
</taxonomicName>
