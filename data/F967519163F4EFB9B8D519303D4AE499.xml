<paragraph id="29267BDB90E57D703B7B0BAD2894082E" pageId="29" pageNumber="30">
<taxonomicName id="C3214CFFBEA82F3431BBDA84DD9FF920" class="Insecta" family="Hydroptilidae" genus="Leucotrichia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leucotrichia mutica" order="Trichoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="mutica">mutica</taxonomicName>
