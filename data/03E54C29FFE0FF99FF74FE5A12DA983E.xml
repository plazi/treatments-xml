<taxonomicName id="4C4C86BCFFE1FF9AFBD4F86B12919E88" box="[1079,1189,1980,2003]" class="Crustacea (awaiting allocation)" family="Crustacea (awaiting allocation)" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea (awaiting allocation)" pageId="5" pageNumber="156" phylum="Arthropoda" rank="genus">mesosoma</taxonomicName>
