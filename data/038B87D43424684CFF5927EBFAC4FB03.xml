<taxonomicName id="4C224D413424684CFED620CEFE6FFB3F" box="[280,395,1206,1231]" class="Polychaeta" family="Spionidae" genus="Pygospio" kingdom="Animalia" order="Spionida" pageId="16" pageNumber="55" phylum="Annelida" rank="species" species="elegans">
