species. 
<emphasis id="2BE5EAB3FFE6FF9DD12AFB10FE47FB26" box="[293,404,1225,1246]" italics="true" pageId="24" pageNumber="75">Apiocrinus</emphasis>
<taxonomicName id="DE914D22FFE6FF9DD194FB10FCBBFB26" authority="von Hagenow, in Quenstedt, 1876" authorityName="von Hagenow, in Quenstedt" authorityYear="1876" box="[411,872,1224,1246]" class="Crinoidea" family="Bourgueticrinidae" genus="Bourgueticrinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Comatulida" pageId="24" pageNumber="75" phylum="Echinodermata" rank="species" species="constrictus">
