<emphasis id="B9326F6585130A0BFEDED341FAA6FDFE" bold="true" box="[362,1305,546,573]" pageId="2" pageNumber="369">
<typeStatus id="54FD0DD585130A0BFEDED341FDB6FDFE" box="[362,521,546,572]" pageId="2" pageNumber="369">HOLOTYPE</typeStatus>
/ 
<taxonomicName id="4C46C8F485130A0BFDAAD340FD36FDFF" box="[542,649,547,573]" class="Insecta" family="Anthribidae" genus="Merarius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="369" phylum="Arthropoda" rank="genus">
