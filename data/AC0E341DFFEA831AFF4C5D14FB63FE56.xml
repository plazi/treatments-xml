<collectionCode id="42B61DCEFFEA831AFC485936FC36FE96" box="[908,934,342,365]" country="Austria" lsid="urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="87" pageNumber="87" type="Herbarium">W</collectionCode>
