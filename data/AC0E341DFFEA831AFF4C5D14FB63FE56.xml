<collectionCode id="42B61DCEFFEA831AFA4D5976FA35FED6" box="[1417,1445,278,301]" country="Austria" lsid="urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="87" pageNumber="87" type="Herbarium">W</collectionCode>
