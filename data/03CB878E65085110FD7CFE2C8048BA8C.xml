<quantity id="4C9A9B7D65085112FE2CFD758030BDDF" box="[468,512,719,746]" metricMagnitude="-2" metricUnit="m" metricValue="7.62" pageId="33" pageNumber="380" unit="in" value="3.0">3 in</quantity>
