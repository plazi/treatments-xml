<taxonomicName id="B9A9EEF2A632CEE2DF3371B5671C25FB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:144457" class="Insecta" family="Formicidae" genus="Sphinctomyrmex" kingdom="Animalia" lsidName="Sphinctomyrmex (Eusphinctus) fallax Forel" lsidName-HNS="Sphinctomyrmex (Eusphinctus) fallax Forel" order="Hymenoptera" pageNumber="73" phylum="Arthropoda" rank="species" species="fallax" subGenus="Eusphinctus">Sphinctomyrmex (Eusphinctus) fallax</taxonomicName>
