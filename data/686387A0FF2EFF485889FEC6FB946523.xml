<taxonomicName id="27CA4D35FF2EFF485A78FCEAFC9D62F8" authorityName="Rondani" authorityYear="1880" box="[614,814,871,897]" class="Insecta" family="Sphaeroceridae" genus="Limosina" kingdom="Animalia" order="Diptera" pageId="227" pageNumber="228" phylum="Arthropoda" rank="species" species="puerula">
<emphasis id="D2BEEAA4FF2EFF485A78FCEAFC9D62F8" box="[614,814,871,897]" italics="true" pageId="227" pageNumber="228">Limosina puerula</emphasis>
</taxonomicName>
