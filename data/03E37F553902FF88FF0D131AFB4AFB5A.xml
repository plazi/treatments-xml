<paragraph id="8BF5CE433902FF88FF0D1363FA7CFCBE" blockId="5.[151,580,864,886]" box="[151,580,864,886]" pageId="5" pageNumber="244">
<taxonomicName id="4C4AB5C03902FF88FF0D1363FA7CFCBE" authority="Bagnall, 1932: 510" authorityName="Bagnall" authorityPageNumber="510" authorityYear="1932" box="[151,580,864,886]" class="Insecta" family="Phlaeothripidae" genus="Phaulothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="244" phylum="Arthropoda" rank="species" species="caudatus">
