<taxonomicName id="4C274D17FFF9FE52FAF1FEA2FA3CFEF3" authority="E.L.Greene, 1906" authorityName="E.L.Greene" authorityYear="1906" box="[1307,1436,312,337]" class="Magnoliopsida" family="Malvaceae" genus="Disella" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malvales" pageId="4" pageNumber="67" phylum="Tracheophyta" rank="species" species="octcella">
<emphasis id="B953EA86FFF9FE52FAF1FEA2FA3CFEF3" box="[1307,1436,312,337]" italics="true" pageId="4" pageNumber="67">D. octcella</emphasis>
</taxonomicName>
