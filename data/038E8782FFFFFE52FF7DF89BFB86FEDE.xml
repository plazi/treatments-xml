<emphasis id="B953EA86FFFEFE55FD4FFE8AFD61FE8B" box="[677,705,272,297]" italics="true" pageId="3" pageNumber="66">bv</emphasis>
) 
<number id="3DEF24B3FFFEFE55FD39FE8AFD58FE88" box="[723,760,272,298]" pageId="3" pageNumber="66" value="7.5">7.5</number>
(
<number id="3DEF24B3FFFEFE55FCE1FE8AFC9AFE88" box="[779,826,272,298]" pageId="3" pageNumber="66" value="7.5" valueMax="8.0" valueMin="7.0">7–8</number>
