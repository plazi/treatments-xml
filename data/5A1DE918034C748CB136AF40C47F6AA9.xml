<taxonomicName id="06C0225664B15F920622E9F63611DA17" LSID="http://zoobank.org/8450329E-15FC-47FD-BBC9-F1FEAC01EA45" class="Insecta" family="Dytiscidae" genus="Laccophilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Laccophilus furthi" order="Coleoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="furthi">Laccophilus furthi</taxonomicName>
<taxonomicNameLabel id="5918795A80B90C43333ED097317C1FA2" pageId="35" pageNumber="36">sp. n.</taxonomicNameLabel>
