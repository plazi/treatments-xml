<taxonomicName id="41ACC26A654B7FA311FA8448878F6E8D" class="Gastropoda" family="Clausiliidae" genus="Montenegrina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Montenegrina janinensis" order="Stylommatophora" pageId="9" pageNumber="10" phylum="Mollusca" rank="subSpecies" species="janinensis" subSpecies="jakupicensis">Montenegrina janinensis jakupicensis</taxonomicName>
