<taxonomicName authority="Laporte, 1840" authorityName="Laporte" authorityYear="1840" genus="Agriotina" lsidName="Agriotina" pageId="154" pageNumber="155" rank="tribe" tribe="Agriotina">Agriotina Laporte, 1840</taxonomicName>
