<taxonomicName box="[460,625,1832,1854]" class="Insecta" family="Empididae" genus="Asymphyloptera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="555" phylum="Arthropoda" rank="genus">
<emphasis box="[460,625,1832,1854]" italics="true" pageId="2" pageNumber="555">Asymphyloptera</emphasis>
</taxonomicName>
habitats. 
<emphasis bold="true" box="[725,737,1832,1853]" pageId="2" pageNumber="555">4</emphasis>
, type locality of 
<taxonomicName box="[915,1045,1833,1854]" class="Insecta" family="Empididae" genus="Asymphyloptera" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="555" phylum="Arthropoda" rank="species" species="cajanuma" status="sp. nov.">
<emphasis box="[915,1045,1833,1854]" italics="true" pageId="2" pageNumber="555">A. cajanuma</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1052,1134,1832,1853]" pageId="2" pageNumber="555">
<taxonomicNameLabel box="[1052,1134,1832,1853]" pageId="2" pageNumber="555" rank="species">sp. nov.</taxonomicNameLabel>
