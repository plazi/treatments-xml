</subSubSection>
<subSubSection pageId="0" pageNumber="67" type="habitat">
<paragraph pageId="0" pageNumber="67">Habitat.</paragraph>
<paragraph pageId="0" pageNumber="67">
<taxonomicName authorityName="C. Norman" authorityYear="1938" baseAuthorityName="DC." class="Magnoliopsida" family="Apiaceae" genus="Physospermopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Physospermopsis obtusiuscula" order="Apiales" pageId="0" pageNumber="67" phylum="Tracheophyta" rank="species" species="obtusiuscula">
