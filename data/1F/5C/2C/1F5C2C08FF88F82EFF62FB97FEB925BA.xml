<paragraph blockId="7.[151,1437,151,1472]" pageId="7" pageNumber="358">
FEMALE (
<number box="[329,341,152,176]" pageId="7" pageNumber="358" value="1.0">1</number>
<typeStatus box="[349,448,152,176]" pageId="7" pageNumber="358">paratype</typeStatus>
, 
<figureCitation box="[460,570,151,176]" captionStart-0="FIGURE 3" captionStart-1="FIGURE 4" captionStartId-0="4.[151,250,1794,1816]" captionStartId-1="5.[151,250,1793,1815]" captionText-0="FIGURE 3. Timalinyssus actinodurae sp. nov., female idiosoma, dorsal view." captionText-1="FIGURE 4. Timalinyssus actinodurae sp. nov., female idiosoma, ventral view." pageId="7" pageNumber="358">
Figs. 
<number box="[523,570,152,176]" pageId="7" pageNumber="358" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
</figureCitation>
