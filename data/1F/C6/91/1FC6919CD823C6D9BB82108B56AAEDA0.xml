<taxonomicName authority="Seidlitz, 1899" authorityName="Seidlitz" authorityYear="1899" class="Insecta" family="Oedemeridae" genus="Oncomerina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Oncomerina" order="Coleoptera" pageId="191" pageNumber="192" phylum="Arthropoda" rank="genus">Oncomerina</taxonomicName>
