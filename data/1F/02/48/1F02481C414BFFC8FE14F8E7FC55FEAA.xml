<taxonomicName authorityName="Constant" authorityYear="2014" baseAuthorityName="Lallemand" baseAuthorityYear="1959" box="[822,957,821,847]" class="Insecta" family="Fulgoridae" genus="Egregia" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="brevirostris">
