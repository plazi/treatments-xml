<caption ID-DOI="http://doi.org/10.5281/zenodo.4761121" ID-Zenodo-Dep="4761121" httpUri="https://zenodo.org/record/4761121/files/figure.png" pageId="6" pageNumber="65" startId="6.[189,241,1180,1204]" targetBox="[251,1360,314,1071]" targetPageId="6">
<paragraph blockId="6.[189,1442,1180,1276]" pageId="6" pageNumber="65">
<emphasis bold="true" box="[189,328,1180,1204]" pageId="6" pageNumber="65">
Figs. 23 
<emphasis bold="true" box="[281,294,1182,1203]" italics="true" pageId="6" pageNumber="65">–</emphasis>
26.
</emphasis>
<taxonomicName authorityName="Hitchcock" authorityYear="1974" box="[335,524,1180,1204]" class="Insecta" family="Leuctridae" genus="Leuctra" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="duplicata">
