<typeStatus box="[151,201,1051,1072]" pageId="18" pageNumber="121">Type</typeStatus>
species: 
<taxonomicName authority="Gaud and Atyeo, 1990" authorityName="Gaud and Atyeo" authorityYear="1990" box="[294,776,1050,1072]" class="Arachnida" family="Pterolichidae" genus="Aniibius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="18" pageNumber="121" phylum="Arthropoda" rank="species" species="drepanophorus">
