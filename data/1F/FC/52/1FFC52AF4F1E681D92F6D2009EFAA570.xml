<subSubSection pageId="2" pageNumber="77" type="nomenclature">
<paragraph pageId="2" pageNumber="77">
<taxonomicName LSID="urn:lsid:zoobank.org:act:8B1618EC-5FBD-4C51-9301-7E0076EA742D" class="Insecta" family="Mantophasmatidae" genus="Striatophasma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Striatophasma" order="Mantophasmatodea" pageId="2" pageNumber="77" phylum="Arthropoda" rank="genus">Striatophasma</taxonomicName>
<taxonomicNameLabel pageId="2" pageNumber="77">gen. n.</taxonomicNameLabel>
</paragraph>
