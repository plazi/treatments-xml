<paragraph pageId="0" pageNumber="168" type="mainText">
<pageBreakToken pageId="0" pageNumber="168" start="start">22</pageBreakToken>
. 
<taxonomicName class="Chilopoda" family="Lithobiidae" genus="Lithobius" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="168" phylum="Arthropoda" rank="species" species="Apfelbecki">L. Apfelbecki</taxonomicName>
