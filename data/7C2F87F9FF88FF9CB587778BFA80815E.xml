<emphasis id="C6F2EAFDFF88FF9CB58777FFFE9F8172" bold="true" box="[151,267,762,788]" pageId="8" pageNumber="119">Remarks.</emphasis>
<bibRefCitation id="90174B1EFF88FF9CB40277FEFE538172" author="Jonassen, T." box="[274,455,762,789]" pageId="8" pageNumber="119" pagination="105 - 106" refId="ref8878" refString="Jonassen, T. (1990) Notes on Norwegian Dolichopodidae (Diptera). Fauna norvegica, Series B, 37, 105 - 106." type="journal article" year="1990">Jonassen (1990)</bibRefCitation>
synonymized 
<taxonomicName id="33864D6CFF88FF9CB77A77FEFD698172" authorityName="Negrobov, in Negrobov &amp; Stackelberg" authorityYear="1971" box="[618,765,762,788]" class="Insecta" family="Dolichopodidae" genus="Thrypticus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="119" phylum="Arthropoda" rank="species" species="paludicola">
