</taxonomicName>
with 
<taxonomicName id="33864D6CFF88FF9CB62C77FEFC438172" authorityName="Negrobov" authorityYear="1967" box="[828,983,762,788]" class="Insecta" family="Dolichopodidae" genus="Thrypticus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="119" phylum="Arthropoda" rank="species" species="intercedens">
