<taxonomicName id="B7F85194C1BDB73E9D50FCF6A727D698" authority="Fernandez, 2003" authorityName="Fernandez" authorityYear="2003" class="Insecta" family="Formicidae" genus="Tyrannomyrmex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="288" phylum="Arthropoda" rank="species" species="legatus">Tyrannomyrmex legatus</taxonomicName>
