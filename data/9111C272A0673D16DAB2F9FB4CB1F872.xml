<typeStatus id="C603CDC6A0673D16DAB2F9B14DBEF92D" box="[151,204,1657,1679]" pageId="2" pageNumber="491">Type</typeStatus>
species.
</emphasis>
<emphasis id="2BCCAF76A0673D16DB0CF9B14CE7F92D" box="[297,405,1657,1679]" italics="true" pageId="2" pageNumber="491">Amphitrite</emphasis>
<taxonomicName id="DEB808E7A0673D16DBB9F9B24FFBF92D" authority="Muller, 1776" authorityName="Muller" authorityYear="1776" box="[412,649,1657,1679]" class="Polychaeta" family="Pectinariidae" genus="Amphictene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Terebellida" pageId="2" pageNumber="491" phylum="Annelida" rank="species" species="auricoma">
<emphasis id="2BCCAF76A0673D16DBB9F9B24C8FF92D" box="[412,509,1658,1679]" italics="true" pageId="2" pageNumber="491">auricoma</emphasis>
<bibRefCitation id="7D290E95A0673D16D826F9B14FFBF92D" author="Muller, O. F." box="[515,649,1657,1679]" pageId="2" pageNumber="491" pagination="1 - 274" refId="ref9451" refString="Muller, O. F. (1776) Zoologicae Danicae Prodromus, seu Animalium Daniae et Norvegiae indigenarum characteres, nomina et synonyma imprimis popularium. Typiis Hallageriis, Copenhagen, pp. 1 - 274." type="book chapter" year="1776">Müller, 1776</bibRefCitation>
</taxonomicName>
