</geoCoordinate>
/ 
<geoCoordinate id="53CB50188F1EFF86FEBDC723DE5EFCD0" box="[347,539,780,805]" direction="east" orientation="longitude" pageId="1" pageNumber="128" precision="1" value="165.35522">
