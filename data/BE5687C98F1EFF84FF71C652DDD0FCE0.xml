<emphasis id="048BEACD8F1CFF84FB66C5BAD8E6FE58" box="[1152,1187,405,428]" italics="true" pageId="3" pageNumber="130">i.e.</emphasis>
: 
<taxonomicName id="F1FF4D5C8F1CFF84FB55C5BADC8AFE24" authority="Glover, 1973" authorityName="Glover" authorityYear="1973" class="Insecta" family="Chironomidae" genus="Cladotanytarsus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="130" phylum="Arthropoda" rank="species" species="bilinearis">
<emphasis id="048BEACD8F1CFF84FB55C5BAD905FE59" box="[1203,1344,404,429]" italics="true" pageId="3" pageNumber="130">C. bilinearis</emphasis>
<bibRefCitation id="526E4B2E8F1CFF84FAAEC5BBDC8AFE24" author="Glover" pageId="3" pageNumber="130" refString="Glover, B. (1973) The Tanytarsini (Diptera: Chironomidae) of Australia. Australian Journal of Zoology, supplement number 23, 403 - 478. http: // dx. doi. org / 10.1071 / AJZS 023" type="journal article" year="1973">
Glover, 
<number id="803724F88F1CFF84FF71C597DC8AFE24" box="[151,207,440,464]" pageId="3" pageNumber="130" value="1973.0">1973</number>
</bibRefCitation>
</taxonomicName>
(South 
<collectingCountry id="4EE8764F8F1CFF84FECFC598DDD6FE24" box="[297,403,439,464]" name="Australia" pageId="3" pageNumber="130">Australia</collectingCountry>
, Queensland), 
<taxonomicName id="F1FF4D5C8F1CFF84FDDDC597DFE7FE24" authority="Johannsen, 1932" authorityName="Johannsen" authorityYear="1932" box="[571,930,439,464]" class="Insecta" family="Chironomidae" genus="Cladotanytarsus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="130" phylum="Arthropoda" rank="species" species="conversus">
<emphasis id="048BEACD8F1CFF84FDDDC597DE89FE24" box="[571,716,440,464]" italics="true" pageId="3" pageNumber="130">C. conversus</emphasis>
(Johannsen, 
<number id="803724F88F1CFF84FC86C597DFDFFE24" box="[864,922,440,464]" pageId="3" pageNumber="130" value="1932.0">1932</number>
)
</taxonomicName>
(southern 
<collectingCountry id="4EE8764F8F1CFF84FBFDC598D824FE24" box="[1051,1121,439,464]" name="China" pageId="3" pageNumber="130">China</collectingCountry>
