<subSubSection id="C30F653F5D34FFEFFF63BC67329A8211" pageId="114" pageNumber="115" type="biology_ecology">
<paragraph id="8BAA36B45D34FFEFFF63BC67342E827E" blockId="114.[189,1249,774,847]" box="[189,597,774,800]" pageId="114" pageNumber="115">
<heading id="D0E281D85D34FFEFFF63BC67342E827E" bold="true" box="[189,597,774,800]" fontSize="11" level="3" pageId="114" pageNumber="115" reason="3">
