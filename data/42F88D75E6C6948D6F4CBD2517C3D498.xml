<taxonomicName id="D6D093625C579BD8D43881F58A03257C" LSID="urn:lsid:zoobank.org:act:63DFDE88-8154-412E-B02E-47A71157F286" class="Arachnida" family="Theridiosomatidae" genus="Menglunia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Menglunia inaffecta" order="Araneae" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="inaffecta">Menglunia inaffecta</taxonomicName>
