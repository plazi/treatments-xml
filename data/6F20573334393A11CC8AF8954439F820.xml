<taxonomicName id="20899DA634383A11CF61FE744227FAAD" authorityName="Mills" authorityYear="1938" box="[892,1045,439,464]" class="Entognatha" family="Paronellidae" genus="Trogolaphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="19" pageNumber="170" phylum="Arthropoda" rank="genus">
<emphasis id="D5FD3A3734383A11CF61FE744227FAAD" box="[892,1045,439,464]" italics="true" pageId="19" pageNumber="170">Trogolaphysa</emphasis>
</taxonomicName>
