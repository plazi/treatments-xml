<taxonomicName id="20899DA634383A11C881FE1E4349FA89" authority="Arle 1939" authorityName="Arle" authorityYear="1939" box="[1180,1403,476,501]" class="Entognatha" family="Paronellidae" genus="Trogolaphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="19" pageNumber="170" phylum="Arthropoda" rank="species" species="millsi">
