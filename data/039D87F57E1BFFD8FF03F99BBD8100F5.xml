<taxonomicName id="4C344D607E18FFD9FED7FA26BF9205B7" authority="Gonçalves &amp; Brescovit, 2020" authorityName="Gonçalves &amp; Brescovit" authorityYear="2020" box="[323,500,1443,1467]" class="Arachnida" family="Zodariidae" genus="Epicratinus" kingdom="Animalia" order="Araneae" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="zelda" status="sp. nov.">
<emphasis id="B940EAF17E18FFD9FED7FA26BF9205B7" box="[323,500,1443,1467]" italics="true" pageId="46" pageNumber="47">Epicratinus zelda</emphasis>
</taxonomicName>
<emphasis id="B940EAF17E18FFD9FE6FFA21BC2D05B7" bold="true" box="[507,587,1443,1467]" pageId="46" pageNumber="47">
<taxonomicNameLabel id="A273578A7E18FFD9FE6FFA21BC2D05B7" box="[507,587,1443,1467]" pageId="46" pageNumber="47" rank="species">sp. nov.</taxonomicNameLabel>
