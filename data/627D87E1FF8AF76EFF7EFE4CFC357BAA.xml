<taxonomicName id="2DD44D74FF8AF76EFF7EFE4CFCDD7CF9" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,781,471,497]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="116" pageNumber="117" phylum="Arthropoda" rank="species" species="amdakaramaya" status="new species">
<emphasis id="D8A0EAE5FF8AF76EFF7EFE4CFDF87CF9" bold="true" box="[151,552,471,497]" italics="true" pageId="116" pageNumber="117">Cheumatopsyche amdakaramaya</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="C393579EFF8AF76EFCF3FE4CFC797CF9" box="[794,937,471,497]" pageId="116" pageNumber="117" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
