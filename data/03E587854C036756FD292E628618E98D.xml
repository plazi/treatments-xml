<collectingCountry id="F35B76034C046756FE622FBB848EE9DC" box="[425,503,1469,1490]" name="Antigua and Barbuda" pageId="20">Antigua</collectingCountry>
, 
<collectingCountry id="F35B76034C046756FDC92FBB8703E9DC" box="[514,634,1469,1490]" name="Bonaire, Sint Eustatius and Saba" pageId="20">St. Eustatius</collectingCountry>
