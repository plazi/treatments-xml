, between 605 and 
<specimenCount id="3191FD66FF896C1B8E1FB42565EFFE99" box="[565,646,315,342]" pageId="24" pageNumber="25" type="generic">
<quantity id="E06F9B0AFF896C1B8E1FB42565EBFE99" box="[565,642,315,342]" metricMagnitude="2" metricUnit="m" metricValue="6.12" pageId="24" pageNumber="25" unit="m" value="612.0">612 m</quantity>
