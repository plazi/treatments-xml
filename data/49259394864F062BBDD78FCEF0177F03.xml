<taxonomicName id="AD9128A313F834BA99043543D05AD79A" baseAuthorityName="Birstein &amp; Vinogradov" baseAuthorityYear="1958" class="Malacostraca" family="Scopelocheiridae" genus="Bathycallisoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bathycallisoma schellenbergi" order="Amphipoda" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="schellenbergi">
