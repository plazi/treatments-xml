<typeStatus id="102D885FEB75FFF8FE392DBAFD93FC7E" box="[439,543,908,933]" pageId="3" pageNumber="82">Holotype</typeStatus>
female, 
<emphasis id="FDE2EAEFEB75FFF8FD092DBAFCDFFC7E" bold="true" box="[647,851,908,933]" pageId="3" pageNumber="82">
<collectingCountry id="B781766DEB75FFF8FD092DBAFD6AFC7E" box="[647,742,908,933]" name="China" pageId="3" pageNumber="82">CHINA</collectingCountry>
, 
<collectingRegion id="0D52F81FEB75FFF8FD7B2DBAFCDFFC7E" box="[757,851,908,933]" country="China" name="Yunnan" pageId="3" pageNumber="82">Yunnan</collectingRegion>
</emphasis>
, 
<collectingCounty id="26484E71EB75FFF8FCEA2DBAFB9AFC7F" box="[868,1046,908,933]" pageId="3" pageNumber="82">Menglun Town</collectingCounty>
, 
