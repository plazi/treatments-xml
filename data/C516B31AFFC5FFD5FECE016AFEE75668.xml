<emphasis id="7FCBDE1EFFC5FFDAFEA50F52FEA85A58" box="[357,380,2053,2076]" italics="true" pageId="5" pageNumber="26">i.e</emphasis>
., 
<emphasis id="7FCBDE1EFFC5FFDAFE520F52FE7C5A58" box="[402,424,2053,2076]" italics="true" pageId="5" pageNumber="26">ca</emphasis>
. 
<quantity id="8A47AFE9FFC5FFDAFE790F52FE265A59" box="[441,498,2053,2077]" metricMagnitude="1" metricUnit="m" metricValue="4.6" pageId="5" pageNumber="26" unit="m" value="46.0">
<elevation id="C692E53FFFC5FFDAFE790F52FE265A59" box="[441,498,2053,2077]" metricMagnitude="1" metricUnit="m" metricValue="4.6" pageId="5" pageNumber="26" unit="m" value="46.0">46 m</elevation>
</quantity>
) deep, 
<geoCoordinate id="288B64CBFFC5FFDAFD840F53FD5A5A58" box="[580,654,2051,2076]" degrees="33" direction="north" minutes="5" orientation="latitude" pageId="5" pageNumber="26" precision="925" value="33.083332">33°5′N</geoCoordinate>
, 
<geoCoordinate id="288B64CBFFC5FFDAFD5D0F52FD2F5A58" box="[669,763,2051,2077]" degrees="128" direction="east" minutes="22" orientation="longitude" pageId="5" pageNumber="26" precision="925" value="128.36667">128°22′E</geoCoordinate>
, 
<collectingRegion id="8F7BCCEEFFC5FFDAFCF404A4FC46564F" box="[820,914,1011,1035]" country="Japan" name="Nagasaki" pageId="5" pageNumber="26">Nagasaki</collectingRegion>
, 
<collectingCountry id="35A8429CFFC5FFDAFC5C04A3FC03564F" box="[924,983,1012,1035]" name="Japan" pageId="5" pageNumber="26">Japan</collectingCountry>
