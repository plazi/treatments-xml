<taxonomicName id="4C164D5848148F05FD95FCC2FD3FFC88" box="[556,649,801,824]" class="Insecta" family="Momphidae" genus="Urodeta" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="tortuosa">
