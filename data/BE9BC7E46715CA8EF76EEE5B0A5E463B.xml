<taxonomicName id="9CADEFE5C32ED2164571BB913085EA6D" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" infraspecific-rank="n. sp." kingdom="Animalia" lsidName="Chrysis cylindrica" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="cylindrica">Chrysis n. sp. cylindrica</taxonomicName>
Evm. [handwritten by Eversmann] // 12 [printed] // 
<taxonomicName id="8D01393652275AAF4F777B3894372164" genus="Perrisi" lsidName="Perrisi viridula" pageId="14" pageNumber="15" rank="species" species="viridula">viridula</taxonomicName>
