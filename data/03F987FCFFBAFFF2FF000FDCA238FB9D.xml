, 
<quantity id="4CA89B0FFFBAFFF2FC120E4CA34BFCCB" box="[901,1061,798,824]" metricMagnitude="-2" metricUnit="m" metricValue="1.05" metricValueMax="1.17" metricValueMin="0.93" pageId="13" pageNumber="64" unit="mm" value="10.5" valueMax="11.7" valueMin="9.3">9.3–11.7 mm</quantity>
