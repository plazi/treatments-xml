<taxonomicName id="4C7FBFD3CB71C87EFC1EBB38FB68FDBB" authority="ZITTEL, 1877" authorityName="Zittel" authorityYear="1877" box="[988,1279,1135,1157]" class="Hexactinellida" family="Euretidae" kingdom="Animalia" order="Sceptrulophora" pageId="5" pageNumber="1008" phylum="Porifera" rank="family">EURETIDAE ZITTEL, 1877</taxonomicName>
