<taxonomicName id="4C604D0CC406DB7BFF67FB9A5F62FBA6" authority="Graham, 1963: 176" authorityName="Graham" authorityPageNumber="176" authorityYear="1963" box="[151,595,1049,1071]" class="Insecta" family="Eulophidae" genus="Diglyphus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="204" phylum="Arthropoda" rank="species" species="pachyneurus">
<emphasis id="B914EA9DC406DB7BFF67FB9A5CB6FBA6" box="[151,391,1049,1071]" italics="true" pageId="7" pageNumber="204">Diglyphus pachyneurus</emphasis>
<treatmentCitation id="0AC1109EC406DB7BFE7DFB9A5F62FBA6" author="Graham" box="[397,595,1049,1071]" page="176" pageId="7" pageNumber="204" year="1963">
