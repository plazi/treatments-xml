<taxonomicName id="4C3C0280FF8FDC07FE1EB99685EE51DB" box="[448,654,482,505]" class="Liliopsida" family="Musaceae" genus="Musa" kingdom="Plantae" order="Zingiberales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="sapientum">
