<taxonomicName id="4C2A17C7FFC6FFE8FF408B41FCA1FD8E" authority="(Wang, Zhang &amp; Bourgoin, 2019)" baseAuthorityName="Wang, Zhang &amp; Bourgoin" baseAuthorityYear="2019" box="[151,772,610,634]" class="Insecta" family="Issidae" genus="Nikomiklukha" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="598" phylum="Arthropoda" rank="species" species="maculifrons">
