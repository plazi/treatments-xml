<taxonomicName id="4C5645C2F323FFBAD3AB6682FD391C8E" authority="ROSINA et RUMMEL, 2012" authorityName="ROSINA et RUMMEL" authorityYear="2012" box="[168,682,1507,1531]" class="Mammalia" family="Vespertilionidae" genus="Hanakia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chiroptera" pageId="9" pageNumber="421" phylum="Chordata" rank="species" species="agadjaniani">
