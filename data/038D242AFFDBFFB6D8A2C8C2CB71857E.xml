<taxonomicName id="4C24EEBFFFDBFFB6DCB6CAACCA9584EA" authorityName="Seksjaeva" authorityYear="1996" box="[1155,1266,1690,1716]" class="Insecta" family="Bucculatricidae" genus="Bucculatrix" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="188" phylum="Arthropoda" rank="species" species="notella">
