<paragraph id="8B8936A3FFB3475FFE9FFE02FDC9FE5A" blockId="119.[242,515,474,533]" box="[272,515,474,497]" pageId="119" pageNumber="218">
<heading id="D0C181CFFFB3475FFE9FFE02FDC9FE5A" box="[272,515,474,497]" fontSize="10" level="2" pageId="119" pageNumber="218" reason="2">
<taxonomicName id="4C364D20FFB3475FFE9FFE02FE09FE5A" authority="Kuschel, 2008" authorityName="Kuschel" authorityYear="2008" box="[272,451,474,497]" class="Insecta" family="Curculionidae" genus="Idastes" kingdom="Animalia" order="Coleoptera" pageId="119" pageNumber="218" phylum="Arthropoda" rank="species" species="grandis" status="n. sp.">
<emphasis id="B942EAB1FFB3475FFE9FFE02FE09FE5A" bold="true" box="[272,451,474,497]" italics="true" pageId="119" pageNumber="218">Idastes grandis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27157CAFFB3475FFE46FE02FDC9FE5A" box="[457,515,474,497]" pageId="119" pageNumber="218" rank="species">n. sp.</taxonomicNameLabel>
