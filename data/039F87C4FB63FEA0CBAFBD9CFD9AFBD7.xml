<taxonomicName id="4C364D51FB62FEAFCA11BE56FD49F8C9" authority="Tippett, 2007" authorityName="Tippett" authorityYear="2007" box="[297,673,1869,1891]" class="Gastropoda" family="Drilliidae" genus="Neodrillia" kingdom="Animalia" order="Neogastropoda" pageId="263" pageNumber="264" phylum="Mollusca" rank="species" species="blakensis">
