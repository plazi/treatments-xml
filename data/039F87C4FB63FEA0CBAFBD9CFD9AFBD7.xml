; 
<collectingCountry id="F3217642FB63FEAECF95BF75FA74F92D" box="[1197,1436,1646,1671]" name="Curaçao" pageId="262" pageNumber="263">Netherlands Antilles</collectingCountry>
(
<collectingCountry id="F3217642FB63FEAECBA7BF89FEEAF901" box="[159,258,1682,1707]" name="Curaçao" pageId="262" pageNumber="263">Curaçao</collectingCountry>
). The 
<collectingCountry id="F3217642FB63FEAECA71BF88FE4EF901" box="[329,422,1683,1707]" name="Curaçao" pageId="262" pageNumber="263">Curaçao</collectingCountry>
specimen (Pl. 
<number id="3DFE24F5FB63FEAEC968BF88FD94F901" box="[592,636,1683,1707]" pageId="262" pageNumber="263" value="134.0">134</number>
