<taxonomicName id="DB974D55FFD7FF8FFCB46AC2FBBCF06C" authority="Dumitrica &amp; Zügel, 2003" authorityName="Dumitrica &amp; Zügel" authorityYear="2003" box="[894,1038,1003,1029]" class="Polycystina" family="Ultranaporidae" genus="Anaticapitula" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="49" pageNumber="54" phylum="Radiozoa" rank="genus" status="n. gen.">
<emphasis id="2EE3EAC4FFD7FF8FFCB46AC2FBBCF06C" box="[894,1038,1003,1029]" italics="true" pageId="49" pageNumber="54">Anaticapitula</emphasis>
</taxonomicName>
<taxonomicNameLabel id="35D057BFFFD7FF8FFBDD6AC3FBD7F06D" box="[1047,1125,1002,1028]" pageId="49" pageNumber="54" rank="genus">n. gen.</taxonomicNameLabel>
