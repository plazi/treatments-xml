<taxonomicName id="DB974D55FFD7FF8FFD006AE2FC11F78C" authority="Dumitrica &amp; Zügel, 2003" authorityName="Dumitrica &amp; Zügel" authorityYear="2003" box="[714,931,971,997]" class="Polycystina" family="Ultranaporidae" genus="Anaticapitula" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="49" pageNumber="54" phylum="Radiozoa" rank="species" species="clauda" status="n. gen., n. sp.">
<emphasis id="2EE3EAC4FFD7FF8FFD006AE2FC11F78C" box="[714,931,971,997]" italics="true" pageId="49" pageNumber="54">Anaticapitula clauda</emphasis>
</taxonomicName>
<taxonomicNameLabel id="35D057BFFFD7FF8FFC636AE3FB89F78D" box="[937,1083,970,996]" pageId="49" pageNumber="54" rank="species">n. gen., n. sp.</taxonomicNameLabel>
