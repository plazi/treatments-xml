<quantity id="80739B78FFE9FFFF5AD572D12B31FDD1" box="[741,804,491,513]" metricMagnitude="2" metricUnit="m" metricValue="8.0" pageId="14" pageNumber="1251" unit="m" value="800.0">800 m</quantity>
