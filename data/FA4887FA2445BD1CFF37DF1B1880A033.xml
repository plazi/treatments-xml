<geoCoordinate id="17D5502B2445BD1CFC12DCE61F21A1BF" box="[946,1054,676,702]" degrees="79" direction="west" minutes="08" orientation="longitude" pageId="91" pageNumber="92" precision="925" value="-79.13333">79°08’W</geoCoordinate>
