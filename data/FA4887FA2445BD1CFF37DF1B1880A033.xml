<geoCoordinate id="17D5502B2445BD1CFDC4DCE619FCA1BF" box="[612,707,676,702]" degrees="9" direction="south" minutes="19" orientation="latitude" pageId="91" pageNumber="92" precision="925" value="-9.316667">9°19’S–</geoCoordinate>
