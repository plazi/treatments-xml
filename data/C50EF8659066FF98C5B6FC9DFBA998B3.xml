<taxonomicName id="8AA732F09066FF98C70AFA1CFDA89F6D" authority="(Graham, 1928) (Paperna 1979)" authorityName="Paperna" authorityYear="1979" baseAuthorityName="Graham" baseAuthorityYear="1928" class="Actinopterygii" family="Cichlidae" genus="Oreochromis" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="861" phylum="Chordata" rank="species" species="esculentus">
<emphasis id="7FD395619066FF98C70AFA1CFF779F6D" italics="true" pageId="12" pageNumber="861">Oreochromis esculentus</emphasis>
