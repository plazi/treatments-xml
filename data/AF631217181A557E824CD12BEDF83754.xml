<quantity id="7CE258EB8E779F66379CF5F89F16D4CB" metricMagnitude="-3" metricUnit="m" metricValue="1.0" unit="mm" value="1.0">1 mm</quantity>
(
<emphasis id="BC69E6B9E6ECDD1EEAD876A28A6656D7" bold="true" pageId="0" pageNumber="31">A, B</emphasis>
); 
<quantity id="87E84829AEAB0421243CFE00C877BEC6" metricMagnitude="-4" metricUnit="m" metricValue="5.0" unit="mm" value="0.5">0.5 mm</quantity>
(
<emphasis id="F46F47CD2975EA0FA006B7CA00204FA3" bold="true" pageId="0" pageNumber="31">C, D</emphasis>
).
</paragraph>
