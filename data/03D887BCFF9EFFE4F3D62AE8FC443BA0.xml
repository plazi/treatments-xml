<geoCoordinate id="EE45506DFF9EFFE4F7232855FB5F3A3F" box="[1122,1270,1552,1578]" degrees="22" direction="north" minutes="18.337" orientation="latitude" pageId="2" pageNumber="277" precision="1" value="22.305616">22°18.337’N</geoCoordinate>
