<subSubSection id="C6B86545FFA2FF8FFF533AF6FE9E0C24" lastPageId="33" lastPageNumber="34" pageId="32" pageNumber="33" type="reference_group">
<paragraph id="8E1D36CEFFA2FF8EFF533AF6FCEE09D4" blockId="32.[151,812,2003,2026]" box="[151,812,2003,2026]" pageId="32" pageNumber="33">
<treatmentCitationGroup id="AEB211E0FFA2FF8EFF533AF6FCEE09D4" box="[151,812,2003,2026]" pageId="32" pageNumber="33">
<taxonomicName id="49A24D4DFFA2FF8EFF533AF6FD3F09D4" authority="Deng, Geng, Liu &amp; Li, 1995: 58" authorityName="Deng, Geng, Liu &amp; Li" authorityPageNumber="58" authorityYear="1995" box="[151,765,2003,2026]" class="Insecta" family="Anthomyiidae" genus="Pegohylemyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="nigribella">
