<caption id="DF6D663A1512FF9CFF08FBDBFB768A1E" ID-DOI="http://doi.org/10.5281/zenodo.5383556" ID-Zenodo-Dep="5383556" box="[149,1228,1060,1082]" httpUri="https://zenodo.org/record/5383556/files/figure.png" pageId="31" pageNumber="32" startId="31.[149,187,1060,1081]" targetBox="[126,1226,187,1037]" targetPageId="31">
