<taxonomicName id="16EB4D5ED26DFFC2FEC582E0FCEFF96A" authority="Petrunkevitch, 1925" authorityName="Petrunkevitch" authorityYear="1925" box="[306,790,1765,1791]" class="Arachnida" family="Araneidae" genus="Acacesia" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="23" phylum="Arthropoda" rank="species" species="cornigera">
