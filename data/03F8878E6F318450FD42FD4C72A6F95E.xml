of 
<taxonomicName id="4C514D1B6F378450FC06FB0275B2F8D4" authorityName="Peloso &amp; Sturaro &amp; Forlani &amp; Gaucher &amp; Motta &amp; Wheeler" authorityYear="2014" box="[950,1228,1188,1211]" class="Amphibia" family="Microhylidae" genus="Chiasmocleis" kingdom="Animalia" order="Anura" pageId="69" pageNumber="69" phylum="Chordata" rank="species" species="papachibe">
<emphasis id="B925EA8A6F378450FC06FB0275B2F8D4" box="[950,1228,1188,1211]" italics="true" pageId="69" pageNumber="69">Chiasmocleis papachibe</emphasis>
</taxonomicName>
