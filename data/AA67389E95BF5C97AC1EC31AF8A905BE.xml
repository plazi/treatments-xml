<paragraph id="2120076C8CA17AC72D52F8666DA1A2A4" pageId="2" pageNumber="15">
<taxonomicName id="1A0FD0EEFA1BB7DE70E4F3549D2C05BD" authority="Choi, Chandler &amp; Park, 2019" authorityName="Choi, Chandler &amp; Park" authorityYear="2019" class="Insecta" family="Staphylinidae" genus="Porongurup" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Porongurup angulatus" order="Coleoptera" pageId="2" pageNumber="15" phylum="Arthropoda" rank="species" species="angulatus">
<emphasis id="185CFA4923E96FA1B1C19AC9B6848006" italics="true" pageId="2" pageNumber="15">Porongurup angulatus</emphasis>
