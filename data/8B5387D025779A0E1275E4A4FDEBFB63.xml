<paragraph id="034536C625779A0E1174E578FC45FCF3" blockId="26.[313,1047,754,837]" box="[313,900,815,837]" pageId="26" pageNumber="1636">
<taxonomicName id="C4FA4D4525779A0E1174E578FC41FCF3" authority="Monniot and Monniot, 2001: 302" authorityName="Monniot and Monniot" authorityPageNumber="302" authorityYear="2001" box="[313,896,815,837]" class="Ascidiacea" family="Perophoridae" genus="Ecteinascidia" kingdom="Animalia" order="Phlebobranchia" pageId="26" pageNumber="1636" phylum="Chordata" rank="species" species="remanea">
<emphasis id="318EEAD425779A0E1174E578FDD8FCF2" box="[313,537,815,837]" italics="true" pageId="26" pageNumber="1636">Ecteinascidia remanea</emphasis>
