<taxonomicName id="C4FA4D4525779A0E1174E542FD3FFC9D" authority="Tokioka, 1950: 129" authorityName="Tokioka" authorityPageNumber="129" authorityYear="1950" box="[313,766,788,811]" class="Ascidiacea" family="Perophoridae" genus="Ecteinascidia" kingdom="Animalia" order="Phlebobranchia" pageId="26" pageNumber="1636" phylum="Chordata" rank="species" species="imperfecta">
