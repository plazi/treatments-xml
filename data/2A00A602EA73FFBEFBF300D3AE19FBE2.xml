<number id="14610533EA73FFBEFA6F020CAB13FDDA" box="[267,280,584,608]" pageId="2" pageNumber="180" value="1.0">1</number>
</emphasis>
<number id="14610533EA73FFBEFA44020CAB42FDDA" box="[288,329,584,608]" pageId="2" pageNumber="180" value="110.0">110</number>
