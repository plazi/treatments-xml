<paragraph id="0019A115A27EFF96FF7D3EF8FA5EFC45" blockId="8.[151,1436,835,1400]" pageId="8" pageNumber="9">
<collectingCountry id="78B1E185A27EFF96FF7D3EF8FE59FC6E" box="[201,413,943,968]" name="United States of America" pageId="8" pageNumber="9">UNITED STATES</collectingCountry>
