<taxonomicName id="BAD94D678538FFA5FF6F9A000135FDD8" authorityName="Haldeman" authorityYear="1847" box="[151,307,544,570]" class="Insecta" family="Cerambycidae" genus="Aegomorphus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="541" phylum="Arthropoda" rank="genus">
