</taxonomicName>
and 
<taxonomicName id="B899ADC82764FFDEFE596BB6FDFAFA9E" authorityName="Bird et al." authorityYear="2013" baseAuthorityName="Ashmead" baseAuthorityYear="1881" box="[400,560,1274,1292]" class="Insecta" family="Cynipidae" genus="Disholcaspis" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="quercusvirens">
<emphasis id="4DED0A592764FFDEFE596BB6FDFAFA9E" box="[400,560,1274,1292]" italics="true" pageId="8" pageNumber="9">D. quercusvirens</emphasis>
</taxonomicName>
