<materialsCitation id="58E6F8259020FFFBFE8C764CFCABFB29" ID-GBIF-Occurrence="3015234309" collectingDate="2017-05-05" collectorName="M. Masumoto" country="Japan" county="Kurihamadai" location="Honshu" municipality="Yokosuka-shi" pageId="3" pageNumber="407" specimenCount="1" typeStatus="holotype">
