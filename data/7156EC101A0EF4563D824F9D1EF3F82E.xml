<taxonomicName id="3EFF26851A0EF4563F644C8A1803FC60" authority="(Mirb.) Franco" authorityName="Franco" baseAuthorityName="Mirb." box="[625,1093,896,922]" class="Pinopsida" family="Pinaceae" genus="Pseudotsuga" kingdom="Plantae" order="Pinales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="menziesii">
