, 
<date id="385D5392FD448E51B47FFA2ABBF20CEB" box="[1057,1167,1493,1517]" pageId="18" pageNumber="2165" value="1937-07">
<collectingDate id="2819AA7AFD448E51B47FFA2ABBF20CEB" box="[1057,1167,1493,1517]" pageId="18" pageNumber="2165" value="1937-07">July 1937</collectingDate>
</date>
, 
<collectorName id="E1161084FD448E51B08AFA0ABE040F0B" box="[212,377,1525,1549]" pageId="18" pageNumber="2165">A. de Cooman</collectorName>
