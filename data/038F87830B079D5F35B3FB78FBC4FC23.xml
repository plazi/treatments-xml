<number id="3DEE24B20B079D5C35B3FB78FF05FB53" box="[151,166,1234,1260]" pageId="1" pageNumber="61" value="1.0">1</number>
. 
<taxonomicName id="4C264D160B079D5C3590FB78FD65FB53" authority="Schlegel, 1844" authorityName="Schlegel" authorityYear="1844" box="[180,710,1234,1260]" class="Reptilia" family="Varanidae" genus="Monitor" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="1" pageNumber="61" phylum="Chordata" rank="subSpecies" species="bivittatus" subSpecies="celebensis">
<emphasis id="B952EA870B079D5C3590FB78FDADFB53" bold="true" box="[180,526,1234,1260]" italics="true" pageId="1" pageNumber="61">Monitor bivittatus celebensis</emphasis>
Schlegel, 
<number id="3DEE24B20B079D5C37AFFB78FD65FB53" box="[651,710,1234,1260]" pageId="1" pageNumber="61" value="1844.0">1844</number>
</taxonomicName>
