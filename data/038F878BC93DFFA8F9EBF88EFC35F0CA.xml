); from 
<quantity id="4CDE9B78C922FFA8F8F3FD6CFE4FF0D5" box="[399,470,693,719]" metricMagnitude="2" metricUnit="m" metricValue="9.24" pageId="49" pageNumber="50" unit="m" value="924.0">924 m</quantity>
(
<bibRefCitation id="EFB74B6CC922FFA8F898FD6CFD38F0D5" author="Van Pesch, A. J." box="[484,673,693,719]" pageId="49" pageNumber="50" refId="ref50100" refString="Van Pesch, A. J. (1914) The Antipatharia of the Siboga Expedition. EJ Brill, Leiden, 259 pp." type="book" year="1914">Van Pesch, 1914</bibRefCitation>
) to 
<quantity id="4CDE9B78C922FFA8FBB1FD6CFCBBF0D5" box="[717,802,693,719]" metricMagnitude="3" metricUnit="m" metricValue="1.876" pageId="49" pageNumber="50" unit="m" value="1876.0">1876 m</quantity>
