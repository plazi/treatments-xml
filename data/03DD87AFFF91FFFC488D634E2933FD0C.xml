<taxonomicName id="4C744D3AFF91FFFC4A2E600D29AFFDD1" box="[564,717,583,606]" class="Polychaeta" family="Hesionidae" genus="Gyptis" kingdom="Animalia" order="Phyllodocida" pageId="42" pageNumber="43" phylum="Annelida" rank="species" species="propinqua">
