<taxonomicName id="4C6CA1CF6621B459C1EF4D3E397D2736" baseAuthorityName="Korotyaev &amp; Gultekin" baseAuthorityYear="2007" box="[211,359,1614,1639]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="116" pageNumber="117" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
