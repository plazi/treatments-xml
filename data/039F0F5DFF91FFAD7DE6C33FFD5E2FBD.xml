(n = 
<number id="3DFEAC6CFF91FFAD7CC1C43AFE4E2E5C" box="[432,464,1122,1146]" pageId="11" pageNumber="12" value="54.0">54</number>
): HB = 
<number id="3DFEAC6CFF91FFAD7F58C43AFD1E2E5C" box="[553,640,1122,1146]" pageId="11" pageNumber="12" value="126.5" valueMax="155.0" valueMin="98.0">98–155</number>
, T = 
