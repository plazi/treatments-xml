, HF = 
<number id="3DFEAC6CFF91FFAD7E1AC43AFC2A2E5C" box="[875,948,1122,1146]" pageId="11" pageNumber="12" value="22.0" valueMax="26.0" valueMin="18.0">18–26</number>
, E = 
<number id="3DFEAC6CFF91FFAD7E9FC43AFBA92E5C" box="[1006,1079,1122,1146]" pageId="11" pageNumber="12" value="24.0" valueMax="28.0" valueMin="20.0">20–28</number>
