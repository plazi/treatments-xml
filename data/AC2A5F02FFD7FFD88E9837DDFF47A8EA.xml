<taxonomicName id="E3839597FFD7FFD78E9837DDFDC8AC6C" authority="Waterhouse, 1890" authorityName="Waterhouse" authorityYear="1890" box="[151,627,1512,1539]" class="Insecta" family="Cerambycidae" genus="Trypanidius" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="260" phylum="Arthropoda" rank="species" species="isolatus">
