<typeStatus id="D27C88142F1722355564BD7AF064FDF1" box="[531,638,564,590]" pageId="20" pageNumber="21">lectotype</typeStatus>
of 
<taxonomicName id="CAC74D352F17223555DABD7AF17BFDF2" box="[685,865,564,589]" class="Insecta" family="Staphylinidae" genus="Homalota" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="puncticeps">
