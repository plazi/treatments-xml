<quantity id="4CF89B6FFFBFF47D471536A67FFC3D27" box="[593,741,744,769]" metricMagnitude="-2" metricUnit="m" metricValue="1.005" metricValueMax="1.16" metricValueMin="0.85" pageId="65" pageNumber="66" unit="mm" value="10.05" valueMax="11.6" valueMin="8.5">
8.5 
