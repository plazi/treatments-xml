<taxonomicName id="4C004D09FFC1F403404E3326788539A7" box="[1290,1436,1896,1920]" class="Insecta" family="Bibionidae" genus="Bibio" kingdom="Animalia" order="Diptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="serresi">
