<paragraph id="ECEEDCB36D361A890155CF40079232AC" pageId="2" pageNumber="37">
<taxonomicName id="B49FC4C83BF7F7ABD946B6B924CD993A" LSID="urn:lsid:zoobank.org:act:9BA5299B-0BA0-4E50-8EAC-C76C2D66B2EA" class="Clitellata" family="Megascolecidae" genus="Amynthas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Amynthas jinburi" order="Opisthopora" pageId="2" pageNumber="37" phylum="Annelida" rank="species" species="jinburi">Amynthas jinburi</taxonomicName>
