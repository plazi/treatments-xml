<number id="3DDA2910FF97A330FBCC4CFF6E722B33" box="[1026,1078,488,514]" pageId="1" pageNumber="64" value="0.9">0.90</number>
/ 
<number id="3DDA2910FF97A330FBF14CFF6E372B33" box="[1087,1139,488,514]" pageId="1" pageNumber="64" value="0.9">0.90</number>
/ 
<number id="3DDA2910FF97A330FBB54CFF6EEA2B33" box="[1147,1198,488,514]" pageId="1" pageNumber="64" value="0.08">0.08</number>
/ 
<number id="3DDA2910FF97A330FB794CFF6EAF2B33" box="[1207,1259,488,514]" pageId="1" pageNumber="64" value="0.22">0.22</number>
