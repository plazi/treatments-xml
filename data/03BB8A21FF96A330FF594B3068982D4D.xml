<number id="3DDA2910FF97A330FEB84FB56BEE2B8D" box="[374,426,674,700]" pageId="1" pageNumber="64" value="0.12">0.12</number>
/ 
<number id="3DDA2910FF97A330FE7A4FB56BAC2B8D" box="[436,488,674,700]" pageId="1" pageNumber="64" value="0.06">0.06</number>
, 
<figureCitation id="132927B2FF97A330FE3A4FB5687D2B8D" box="[500,569,674,700]" captionStart="FIGURES 1 – 4" captionStartId="1.[151,270,1951,1975]" captionTargetBox="[154,1431,1206,1902]" captionTargetId="figure@1.[151,1436,1205,1925]" captionTargetPageId="1" captionText="FIGURES 1 – 4. Rhagovelia rosarensis sp. nov., 1 – 3 male, 1 fore tibiae, 2 hind femur, 3 paramere, 4 female habitus." httpUri="https://zenodo.org/record/198102/files/figure.png" pageId="1" pageNumber="64">
