<taxonomicName id="4C024D29FFAAFFC1FEA3F86CFEF165CA" box="[280,363,1972,1994]" class="Insecta" family="Colletidae" genus="Colletes" higherTaxonomySource="CoL" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="89" phylum="Arthropoda" rank="genus">Colletes</taxonomicName>
<taxonomicName id="4C024D29FFAAFFC1FEC9F86CFE5565CA" authority="Kuhlmann" authorityName="Kuhlmann" box="[370,463,1972,1994]" pageId="8" pageNumber="89" rank="species" species="kazakhus">kazakhus</taxonomicName>
</emphasis>
Kuhlmann &amp; Proshchalykin
</taxonomicName>
, 
<taxonomicNameLabel id="A24557C3FFAAFFC1FD47F86DFCD765CA" box="[764,845,1973,1994]" pageId="8" pageNumber="89" rank="species">
<emphasis id="B976EAB8FFAAFFC1FD47F86DFCD765CA" bold="true" box="[764,845,1973,1994]" pageId="8" pageNumber="89">sp. nov.</emphasis>
</taxonomicNameLabel>
