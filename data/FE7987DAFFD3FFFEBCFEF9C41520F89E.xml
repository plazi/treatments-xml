<taxonomicName id="B1D04D4FFFD3FFFEBD25F98C17CEF92C" authorityName="Jiang &amp; Li" authorityYear="2020" box="[339,481,1651,1675]" class="Arachnida" genus="Pilosarachne" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="268" phylum="Arthropoda" rank="genus">Pilosarachne</taxonomicName>
