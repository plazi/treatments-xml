<quantity id="4CE09B51FF82DF1EFE6481DFFD85528F" box="[486,573,655,679]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="25" pageNumber="26" unit="m" value="1000.0">1000 m</quantity>
