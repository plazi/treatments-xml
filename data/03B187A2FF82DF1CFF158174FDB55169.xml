<emphasis id="B96CEAA6FF81DF1DFF15841CFE87574A" bold="true" box="[151,319,1868,1891]" pageId="26" pageNumber="27">FIGURES 6–6f</emphasis>
— 
<taxonomicName id="4C184D37FF81DF1DFEDA8419FCB0574A" authority="Bello &amp; Osella" authorityName="Bello &amp; Osella" authorityYear="2016" box="[344,776,1865,1891]" class="Insecta" family="Curculionidae" genus="Baldorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="ruzzieri" status="sp. n.">
<emphasis id="B96CEAA6FF81DF1DFEDA8419FDB1574A" box="[344,521,1865,1891]" italics="true" pageId="26" pageNumber="27">Baldorhynchus</emphasis>
<emphasis id="B96CEAA6FF81DF1DFD91841EFDD8574A" box="[531,608,1870,1891]" italics="true" pageId="26" pageNumber="27">ruzzieri</emphasis>
Bellò &amp; Osella
</taxonomicName>
<taxonomicNameLabel id="A25F57DDFF81DF1DFC93841EFCF3574A" box="[785,843,1870,1891]" pageId="26" pageNumber="27" rank="species">sp. n.</taxonomicNameLabel>
<typeStatus id="54A38816FF81DF1DFCD6841DFC14574A" box="[852,940,1869,1891]" pageId="26" pageNumber="27">holotype</typeStatus>
<specimenCount id="9D1EFD3DFF81DF1DFC34841EFC71574A" box="[950,969,1870,1891]" pageId="26" pageNumber="27" type="male">♂</specimenCount>
from “Malga Zovel, 
<quantity id="4CE09B51FF81DF1DFB2F841DFB47574A" box="[1197,1279,1869,1891]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="27" unit="m" value="1000.0">1000 m</quantity>
