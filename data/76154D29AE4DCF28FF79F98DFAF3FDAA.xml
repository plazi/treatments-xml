<taxonomicName id="39BC87BCAE4CCF2FFB59F84CFAD5F80B" authorityName="Mityaev" authorityYear="1971" box="[1207,1378,1944,1968]" class="Insecta" family="Cicadellidae" genus="Anaceratagallia" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="274" phylum="Arthropoda" rank="species" species="fragariae" subGenus="Anaceratagallia">
