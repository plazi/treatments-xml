<taxonomicName id="39BC87BCAE4CCF2FFD67F84CFB80F80B" baseAuthorityName="Ossiannilsson" baseAuthorityYear="1938" box="[649,1079,1944,1968]" class="Insecta" family="Cicadellidae" genus="Anaceratagallia" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="269" phylum="Arthropoda" rank="species" species="ribauti" subGenus="Anaceratagallia">
