<paragraph id="8B93368F4202FFD598537C1AA885FE27" blockId="62.[806,1422,1606,1904]" lastBlockId="63.[162,778,195,401]" lastPageId="63" lastPageNumber="64" pageId="62" pageNumber="63">
<taxonomicName id="4C2C4D0C4202FFD498537C1AAD37F9CC" authorityName="Vandel" authorityYear="1968" box="[806,1113,1637,1658]" class="Malacostraca" family="Scleropactidae" genus="Sphaeroniscus" kingdom="Animalia" order="Isopoda" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="gerstaeckeri">
