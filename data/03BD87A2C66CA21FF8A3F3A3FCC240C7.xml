{
<taxonomicName id="4C144D37C66DA21FFBD8FDC9FB24413C" baseAuthorityName="Peters" baseAuthorityYear="1871" box="[1056,1227,595,634]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="27" pageNumber="254" phylum="Chordata" rank="species" species="doriae">H. doriae</taxonomicName>
