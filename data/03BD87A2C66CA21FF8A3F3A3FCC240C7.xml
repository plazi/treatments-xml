<taxonomicName id="4C144D37C66CA21EF473F25BF3DD4EAE" authorityName="Thomas" authorityYear="1902" box="[2955,3122,3521,3560]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="26" pageNumber="253" phylum="Chordata" rank="species" species="dyacorum">
