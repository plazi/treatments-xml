L. 
<taxonomicName id="4C29E3C4FF94FF9AFD85FBE9FD6CFBD0" box="[571,657,1096,1118]" pageId="15" pageNumber="16" rank="species" species="schmidti">schmidti</taxonomicName>
</emphasis>
</taxonomicName>
. 25, 
<taxonomicName id="4C29E3C4FF94FF9AFD7AFBE8FCC9FBD0" box="[708,820,1096,1118]" class="Insecta" family="Pergidae" genus="Lagideus" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="longicus">
