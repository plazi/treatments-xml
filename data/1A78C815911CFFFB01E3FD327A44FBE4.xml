<heading id="C926CE6F911CFFF801E3FD327B95FD00" bold="true" box="[151,691,693,719]" fontSize="11" level="1" pageId="13" pageNumber="42" reason="1">
<emphasis id="A0A5A511911CFFF801E3FD327B95FD00" bold="true" box="[151,691,693,719]" pageId="13" pageNumber="42">
<taxonomicName id="55D10280911CFFF801E3FD32784DFD00" box="[151,363,693,719]" class="Insecta" family="Ichneumonidae" genus="Polyphrix" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="42" phylum="Arthropoda" rank="species" species="stellata" status="sp. nov.">
<emphasis id="A0A5A511911CFFF801E3FD32784DFD00" bold="true" box="[151,363,693,719]" italics="true" pageId="13" pageNumber="42">Polyphrix stellata</emphasis>
</taxonomicName>
