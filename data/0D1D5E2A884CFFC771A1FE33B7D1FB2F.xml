<taxonomicName id="42B494BF884EFFC07354F848B1D1F801" box="[610,735,1995,2019]" class="Actinopterygii" family="Nettastomatidae" genus="Nettenchelys" kingdom="Animalia" order="Anguilliformes" pageId="11" pageNumber="116" phylum="Chordata" rank="species" species="gephyra">
<emphasis id="B7C0332E884EFFC07354F848B1D1F801" box="[610,735,1995,2019]" italics="true" pageId="11" pageNumber="116">N. gephyra</emphasis>
</taxonomicName>
and 
<taxonomicName id="42B494BF884EFFC07223F848B095F801" box="[789,923,1996,2019]" class="Actinopterygii" family="Nettastomatidae" genus="Nettenchelys" kingdom="Animalia" order="Anguilliformes" pageId="11" pageNumber="116" phylum="Chordata" rank="species" species="pygmaea">
<emphasis id="B7C0332E884EFFC07223F848B095F801" box="[789,923,1996,2019]" italics="true" pageId="11" pageNumber="116">N. pygmaea</emphasis>
</taxonomicName>
