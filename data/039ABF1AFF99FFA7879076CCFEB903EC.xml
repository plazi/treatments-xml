<collectingCountry id="F3244E9CFF99FFA787FD7686FEE80333" box="[223,332,1271,1291]" name="Germany" pageId="25" pageNumber="1320">Deutschland</collectingCountry>
(HAM, SAL, ZSM), 
<collectingCountry id="F3244E9CFF99FFA7852B7686FDD70333" box="[521,627,1271,1291]" name="Netherlands" pageId="25" pageNumber="1320">Niederlande</collectingCountry>
ZWA), Grossbritannien (
<collectingCountry id="F3244E9CFF99FFA7846E7686FC330333" box="[844,919,1271,1291]" name="United Kingdom" pageId="25" pageNumber="1320">England</collectingCountry>
) (EDI), 
<collectingCountry id="F3244E9CFF99FFA784FD7686FB990333" box="[991,1085,1271,1291]" name="France" pageId="25" pageNumber="1320">Frankreich</collectingCountry>
(PAR, ZSM), 
<collectingCountry id="F3244E9CFF99FFA7867D777EFE0D031B" box="[351,425,1295,1315]" name="Switzerland" pageId="25" pageNumber="1320">Schweiz</collectingCountry>
(PAR, ZSM), 
<collectingCountry id="F3244E9CFF99FFA7850D777EFD2D031B" box="[559,649,1295,1315]" name="Austria" pageId="25" pageNumber="1320">Österreich</collectingCountry>
(SWA, ZSM), 
<collectingCountry id="F3244E9CFF99FFA78435777EFCEA031B" box="[791,846,1295,1315]" name="Italy" pageId="25" pageNumber="1320">Italien</collectingCountry>
(
<collectingRegion id="49F7C0EEFF99FFA78479777EFC15031B" box="[859,945,1295,1315]" country="Italy" name="Piemonte" pageId="25" pageNumber="1320">Piemonte</collectingRegion>
, Roma) (TOR), 
<collectingCountry id="F3244E9CFF99FFA787FD7756FE910303" box="[223,309,1319,1339]" name="Bulgaria" pageId="25" pageNumber="1320">Bulgarien</collectingCountry>
