<taxonomicName id="DA9E4D2CFF99FFFE584BFC5B81DDFC5B" box="[1068,1190,973,995]" class="Asteroidea" family="Goniasteridae" genus="Nymphaster" kingdom="Animalia" order="Valvatida" pageId="22" pageNumber="123" phylum="Echinodermata" rank="genus">
