. 
<taxonomicName id="4C144D2FBA10B0199B98752E04AA6301" authority="Lamarck, 1814" authorityName="Lamarck" authorityYear="1814" box="[404,1014,1559,1583]" class="Demospongiae" family="Callyspongiidae" genus="Callyspongia" kingdom="Animalia" order="Haplosclerida" pageId="3" pageNumber="4" phylum="Porifera" rank="species" species="vaginalis" subGenus="Cladochalina">
