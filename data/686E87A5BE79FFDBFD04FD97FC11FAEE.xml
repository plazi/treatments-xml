<subSubSection id="A8DD6538BE79FFDBFD1EFDFAFC11FAEE" pageId="8" pageNumber="19" type="materials_examined">
<paragraph id="E07836B3BE79FFDBFD1EFDFAFB23FD53" blockId="8.[703,1211,620,645]" box="[703,1211,620,645]" pageId="8" pageNumber="19">
