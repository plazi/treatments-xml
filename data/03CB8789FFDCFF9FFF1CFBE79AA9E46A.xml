<taxonomicName id="4C624D1CFFDCFF9FFEDFFB0898BBE44C" authorityName="Robineau-Desvoidy" authorityYear="1863" box="[340,434,1272,1297]" class="Insecta" family="Tachinidae" genus="Moretia" kingdom="Animalia" order="Diptera" pageId="109" pageNumber="110" phylum="Arthropoda" rank="genus">
