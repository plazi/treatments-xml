<collectingCountry id="F34B25CCFFA0765BFEE6FE18FEC0E199" box="[277,348,427,446]" name="Ukraine" pageId="20" pageNumber="549">Ukraine</collectingCountry>
, 
<collectingRegion id="4998ABBEFFA0765BFE97FE18FE39E199" box="[356,421,427,446]" country="Ukraine" name="Crimea" pageId="20" pageNumber="549">Crimea</collectingRegion>
, Kara-Dagh massif, ca. 
<geoCoordinate id="EE68039BFFA0765BFD81FE18FD3AE199" box="[626,678,427,446]" degrees="44.9" direction="north" orientation="latitude" pageId="20" pageNumber="549" precision="5555" value="44.9">44.9N</geoCoordinate>
