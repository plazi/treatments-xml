<location id="8ED3D2BEFFECFFC9FEEFFC79FE2DFBAE" LSID="urn:lsid:plazi:treatment:03A53573FFECFFC9FF35FCB9FBCDFCCE:8ED3D2BEFFECFFC9FEEFFC79FE2DFBAE" box="[312,495,1021,1045]" county="Lake Sampaloc" municipality="Lake Tambo" name="Guangzhou City" pageId="12" pageNumber="783" stateProvince="Laguna">Guangzhou City</location>
