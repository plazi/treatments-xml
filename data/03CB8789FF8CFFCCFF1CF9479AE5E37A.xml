<typeStatus id="54D9883DFF8FFFCCFAE2FF129C92E1A1" box="[1385,1435,226,252]" pageId="62" pageNumber="63">type</typeStatus>
designation for 
<taxonomicName id="4C624D1CFF8FFFCCFE0DFEF89A4CE07F" authority="Robineau-Desvoidy, 1830" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[390,837,264,290]" class="Insecta" family="Conopidae" genus="Dalmannia" kingdom="Animalia" order="Diptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="genus">
