<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Insecta" family="Crambidae" genus="Syllepte" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Syllepte nitidalis" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="nitidalis">
<emphasis italics="true" pageId="0" pageNumber="1">Syllepte nitidalis</emphasis>
</taxonomicName>
Dognin (
<taxonomicName family="Crambidae" lsidName="" pageId="0" pageNumber="1" rank="family">Crambidae</taxonomicName>
: 
<taxonomicName lsidName="" pageId="0" pageNumber="1" rank="subfamily" subfamily="Spilomelinae">Spilomelinae</taxonomicName>
) feeding on 
<taxonomicName class="Magnoliopsida" family="Malvaceae" genus="Malvaviscus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Malvaviscus arboreus" order="Malvales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="arboreus">
<emphasis italics="true" pageId="0" pageNumber="1">Malvaviscus arboreus</emphasis>
