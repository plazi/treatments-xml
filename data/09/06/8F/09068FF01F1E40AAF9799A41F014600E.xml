<taxonomicName family="Malvaceae" genus="Chalcidoidea" lsidName="Chalcidoidea" pageId="0" pageNumber="1" rank="family">Malvaceae</taxonomicName>
) and 
<taxonomicName class="Insecta" family="Crambidae" genus="Trichaea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trichaea pilicornis" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="pilicornis">
<emphasis italics="true" pageId="0" pageNumber="1">Trichaea pilicornis</emphasis>
</taxonomicName>
<normalizedToken originalValue="Herrich-Schäffer">Herrich-Schaeffer</normalizedToken>
(
<taxonomicName family="Crambidae" lsidName="" pageId="0" pageNumber="1" rank="family">Crambidae</taxonomicName>
