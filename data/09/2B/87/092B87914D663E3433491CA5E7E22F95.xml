<geoCoordinate box="[703,841,324,350]" direction="north" orientation="latitude" pageId="3" pageNumber="4" precision="9" value="25.171667">
<number box="[703,733,324,350]" pageId="3" pageNumber="4" value="25.0">25</number>
° 
<number box="[743,813,324,350]" pageId="3" pageNumber="4" value="10.3">10.30</number>
’N
</geoCoordinate>
<geoCoordinate box="[850,995,324,350]" direction="west" orientation="longitude" pageId="3" pageNumber="4" precision="9" value="-80.21367">
<number box="[850,880,324,350]" pageId="3" pageNumber="4" value="80.0">80</number>
° 
<number box="[890,959,324,350]" pageId="3" pageNumber="4" value="12.82">12.82</number>
’W
</geoCoordinate>
