<collectingCountry box="[1031,1136,1742,1768]" name="Brazil" pageId="85" pageNumber="86">BRAZIL</collectingCountry>
. 
<collectingRegion box="[1153,1266,1741,1767]" country="Brazil" name="Rondonia" pageId="85" pageNumber="86">Rondônia</collectingRegion>
