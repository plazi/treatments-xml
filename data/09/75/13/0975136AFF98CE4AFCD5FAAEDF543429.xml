<subSubSection pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[810,1452,1517,2093]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[863,998,1700,1726]" pageId="21" pageNumber="22">Etymology.</emphasis>
