<taxonomicName authority="Sankararaman &amp; Manickavasagam &amp; Triapitsyn &amp; Huber &amp; Kharbisnop, 2020" authorityName="Sankararaman &amp; Manickavasagam &amp; Triapitsyn &amp; Huber &amp; Kharbisnop" authorityYear="2020" box="[321,780,1976,2000]" class="Insecta" family="Mymaridae" genus="Camptopteroides" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="246" phylum="Arthropoda" rank="species" species="formosa" status="sp. n." subGenus="Camptopteroides">
