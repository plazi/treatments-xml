<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28547" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex strigatus Mayr" order="Hymenoptera" pageNumber="5" phylum="Arthropoda" rank="species" species="strigatus">strigatus</taxonomicName>
