<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Insecta" family="Hydrophilidae" genus="Laccobius" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="88" phylum="Arthropoda" rank="species" species="minutus">
