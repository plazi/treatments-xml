<taxonomicName authority="Kuwert, 1886" authorityName="Kuwert" authorityYear="1886" class="Insecta" family="Helophoridae" genus="Helophorus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="88" phylum="Arthropoda" rank="subGenus" subGenus="Rhopalohelophorus">
