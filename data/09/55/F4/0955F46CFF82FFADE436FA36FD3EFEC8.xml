<taxonomicName authorityName="Schonmann" authorityYear="1994" box="[452,753,252,276]" class="Insecta" family="Hydrophilidae" genus="Pelthydrus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="105" phylum="Arthropoda" rank="species" species="jendeki" subGenus="Globipelthydrus">
