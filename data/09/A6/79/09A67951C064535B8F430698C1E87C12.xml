<emphasis italics="true" pageId="0" pageNumber="45">Waldheimia</emphasis>
</taxonomicName>
are 
<taxonomicName class="Magnoliopsida" family="Vitaceae" genus="Cissus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cissus pseudosicyoidea" order="Vitales" pageId="0" pageNumber="45" phylum="Tracheophyta" rank="species" species="pseudosicyoidea">
