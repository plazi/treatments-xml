<collectingCountry box="[1111,1168,819,842]" name="Laos" pageId="19" pageNumber="20">Laos</collectingCountry>
, 
<collectingCountry box="[1183,1288,818,843]" name="Malaysia" pageId="19" pageNumber="20">Malaysia</collectingCountry>
