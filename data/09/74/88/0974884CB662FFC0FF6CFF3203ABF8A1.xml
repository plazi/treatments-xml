, 
<collectingCountry box="[1303,1432,1562,1587]" name="Philippines" pageId="17" pageNumber="18">Philippines</collectingCountry>
, 
<collectingCountry box="[151,236,1598,1623]" name="Taiwan" pageId="17" pageNumber="18">Taiwan</collectingCountry>
, 
<collectingCountry box="[247,347,1598,1623]" name="Thailand" pageId="17" pageNumber="18">Thailand</collectingCountry>
and 
<collectingCountry box="[401,498,1598,1623]" name="Vietnam" pageId="17" pageNumber="18">Vietnam</collectingCountry>
