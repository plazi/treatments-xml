<taxonomicName baseAuthorityName="Green" baseAuthorityYear="1904" class="Insecta" family="Coccidae" genus="Paralecanium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="quadratum">
