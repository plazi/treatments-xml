(
<taxonomicName box="[917,1015,542,566]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Araceae" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="order">Araceae</taxonomicName>
