<collectingRegion box="[692,872,1596,1621]" country="Morocco" name="Oriental" pageId="15" pageNumber="16">
<collectingRegion box="[692,784,1596,1621]" country="Egypt" name="Ash Sharqiyah" pageId="15" pageNumber="16">Oriental</collectingRegion>
Region
</collectingRegion>
(
<collectingCountry box="[887,949,1596,1621]" name="India" pageId="15" pageNumber="16">India</collectingCountry>
, 
<collectingCountry box="[960,1072,1596,1621]" name="Indonesia" pageId="15" pageNumber="16">Indonesia</collectingCountry>
, 
<collectingCountry box="[1083,1186,1596,1621]" name="Malaysia" pageId="15" pageNumber="16">Malaysia</collectingCountry>
