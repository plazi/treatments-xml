<subSubSection pageId="18" pageNumber="111" type="nomenclature">
<paragraph pageId="18" pageNumber="111">
<taxonomicName LSID="http://zoobank.org/40757AA3-B52C-4733-AEF2-334C861F561F" authority="Kang" authorityName="Förster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Lytopylus gisukae" order="Hymenoptera" pageId="18" pageNumber="111" phylum="Arthropoda" rank="species" species="gisukae">Lytopylus gisukae Kang</taxonomicName>
