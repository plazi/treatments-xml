<taxonomicName class="Insecta" family="Curculionidae" genus="Acanthurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acanthurus triostegus" order="Coleoptera" pageId="0" pageNumber="85" phylum="Arthropoda" rank="species" species="triostegus">Acanthurus triostegus</taxonomicName>
(
<taxonomicName family="Acanthuridae" lsidName="" pageId="0" pageNumber="85" rank="family">Acanthuridae</taxonomicName>
