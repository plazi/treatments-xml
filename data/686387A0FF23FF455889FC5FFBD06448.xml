<taxonomicName id="27CA4D35FF23FF455A74FB42FCD76590" authorityName="Rondani" authorityYear="1868" box="[618,868,1231,1257]" class="Insecta" family="Syrphidae" genus="Cheilosia" kingdom="Animalia" order="Diptera" pageId="238" pageNumber="239" phylum="Arthropoda" rank="species" species="erytrocheila">
