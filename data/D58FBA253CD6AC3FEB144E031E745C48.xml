<taxonomicName id="1FB9B0A9AC15E95F8D482FA33CF225EE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:141798" authority="Forel" authorityName="Forel" class="Hexapoda" family="Formicidae" genus="Euponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Euponera (Xiphopelta) picea Bernard" order="Hymenoptera" pageNumber="192" phylum="Arthropoda" rank="species" species="picea" subGenus="Xiphopelta">E. (Xiphopelta) picea</taxonomicName>
