<taxonomicName id="A5E04D707A7F223A7DC6FDC062B5FD5E" authorityName="Lafresnaye" authorityYear="1847" box="[241,388,638,662]" class="Aves" family="Psittacidae" genus="Ara" kingdom="Animalia" order="Psittaciformes" pageId="11" pageNumber="474" phylum="Chordata" rank="species" species="rubrogenys">ara rubrogenys</taxonomicName>
