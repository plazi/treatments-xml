<taxonomicName id="A5E04D707A7F223A7C36FCEF62E7FCA1" authority="(Lafr.)" baseAuthorityName="Lafr." box="[257,470,849,873]" class="Aves" family="Psittacidae" genus="Ara" kingdom="Animalia" order="Psittaciformes" pageId="11" pageNumber="474" phylum="Chordata" rank="species" species="rubrogenys">M. rubrogenys (Lafr.)</taxonomicName>
<typeStatus id="BD5B88517A7F223A7CECFCEF613AFCA1" box="[475,523,849,873]" pageId="11" pageNumber="474">Type</typeStatus>
