<taxonomicName id="CA81D4AF4768B65EEB59470DCFCDB83D" authority="Wesmael, 1839" authorityName="Wesmael" authorityYear="1839" class="Insecta" family="Braconidae" genus="Braco" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Braco" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Braco</taxonomicName>
