<taxonomicName id="FD4F1AD2E5EA94B932073380AFE83285" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. (Lucobracon) erythrostictus" order="Biddulphiales" pageId="0" pageNumber="8151" phylum="Diatomeae" rank="species" species="erythrostictus" subGenus="Lucobracon">B. (Lucobracon) erythrostictus</taxonomicName>
