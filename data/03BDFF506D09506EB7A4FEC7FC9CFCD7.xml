<paragraph id="8BAB4E466D09506EB7A4FE81FC64FE8C" blockId="16.[151,906,355,378]" box="[151,906,355,378]" pageId="16" pageNumber="237">
<taxonomicName id="4C1435C56D09506EB7A4FE81FE19FE8F" authority="Laporte, 1840: 2" authorityName="Laporte" authorityPageNumber="2" authorityYear="1840" box="[151,503,356,378]" class="Insecta" family="Silphidae" genus="Necrophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="237" phylum="Arthropoda" rank="species" species="sayi">
