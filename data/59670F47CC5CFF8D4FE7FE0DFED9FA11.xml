<collectingCountry id="A9D9FEC1CC5FFF8D4E8FFF22FF41FA11" box="[144,216,1396,1420]" name="China" pageId="7" pageNumber="1092">China</collectingCountry>
: 
<collectingRegion id="130A70B3CC5FFF8D4EFBFF22FEA5FA11" box="[228,316,1396,1420]" country="China" name="Hainan" pageId="7" pageNumber="1092">Hainan</collectingRegion>
