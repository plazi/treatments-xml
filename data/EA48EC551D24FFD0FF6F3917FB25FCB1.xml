<paragraph id="625E5D431D20FFD0FF673B3AFB25FE56" blockId="6.[159,1330,283,842]" pageId="6" pageNumber="126">
Range 
<number id="D4294F641D20FFD0FE373B39FD8AFE78" box="[463,577,365,387]" pageId="6" pageNumber="126" value="12.17" valueMax="14.45" valueMin="9.89">9,89–14,45</number>
