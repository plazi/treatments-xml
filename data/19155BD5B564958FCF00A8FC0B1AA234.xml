<th id="963F29829AFF374E9AF2A10D3D594612" colspan="1" pageId="3" pageNumber="110" rowspan="1" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<taxonomicName id="806F7FEB9DA78446404D20DA4978F584" class="Reptilia" family="Hoplocercidae" genus="Enyalioides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enyalioides anisolepis" order="Squamata" pageId="3" pageNumber="110" phylum="Chordata" rank="species" species="anisolepis">Enyalioides anisolepis</taxonomicName>
N = 15
</th>
<th id="29CAC1D8B921D1636ED71AA30FD96791" colspan="1" pageId="3" pageNumber="110" rowspan="1" xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
<taxonomicName id="195711C2DB3AF10CCA866C2E825443E7" class="Reptilia" family="Hoplocercidae" genus="Enyalioides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enyalioides sophiarothschildae" order="Squamata" pageId="3" pageNumber="110" phylum="Chordata" rank="species" species="sophiarothschildae">Enyalioides sophiarothschildae</taxonomicName>
