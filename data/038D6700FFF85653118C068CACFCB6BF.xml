<taxonomicName id="4C24AD95FFF8565313B907F2ACF5B337" box="[674,766,489,513]" class="Arachnida" family="Linyphiidae" genus="Agyneta" kingdom="Animalia" order="Araneae" pageId="68" pageNumber="69" phylum="Arthropoda" rank="genus">
