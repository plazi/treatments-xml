<taxonomicName id="1F8D71EFFF8B6D2CFB963F8BD42AFE38" box="[1149,1374,386,411]" class="Insecta" family="Muscidae" genus="Phyronota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="species" species="platensis">
