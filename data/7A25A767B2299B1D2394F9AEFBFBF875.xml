<taxonomicName id="358C6DF2B2299B1D2756F8DCFB74F8AA" authorityName="Kilic et al." authorityYear="2012" baseAuthorityName="Berlese" baseAuthorityYear="1889" box="[1109,1262,1893,1917]" class="Arachnida" family="Macrochelidae" genus="Macrocheles" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="569" phylum="Arthropoda" rank="species" species="merdarius">
