<quantity id="F35231C2FF88FFC46D77FA24FCB656FE" box="[791,873,1478,1501]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="26" pageNumber="377" unit="m" value="1000.0">1000 m</quantity>
