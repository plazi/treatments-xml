<subSubSection box="[199,1165,223,248]" pageId="7" pageNumber="129" type="etymology">
<paragraph blockId="7.[151,1437,151,429]" box="[199,1165,223,248]" pageId="7" pageNumber="129">
<emphasis bold="true" box="[199,334,223,248]" pageId="7" pageNumber="129">Etymology.</emphasis>
