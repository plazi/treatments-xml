<subSubSection box="[416,1450,137,161]" pageId="7" pageNumber="8" type="etymology">
<paragraph blockId="7.[416,1450,137,161]" box="[416,1450,137,161]" pageId="7" pageNumber="8">
<heading bold="true" box="[416,549,137,161]" fontSize="10" level="2" pageId="7" pageNumber="8" reason="0">
<emphasis bold="true" box="[416,549,137,161]" pageId="7" pageNumber="8">Etymology.</emphasis>
</heading>
