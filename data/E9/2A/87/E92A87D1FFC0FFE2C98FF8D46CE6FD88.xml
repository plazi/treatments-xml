<taxonomicName authorityName="Kieffer" authorityYear="1913" box="[208,332,585,611]" class="Insecta" family="Cecidomyiidae" genus="Semudobia" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="426" phylum="Arthropoda" rank="genus">
