<paragraph blockId="3.[151,1437,151,1149]" pageId="3" pageNumber="288">
<emphasis bold="true" box="[199,439,943,968]" pageId="3" pageNumber="288">Examined material.</emphasis>
<typeStatus box="[451,545,944,968]" pageId="3" pageNumber="288">Syntype</typeStatus>
, ♀, 
<collectingCountry box="[600,701,944,968]" name="Brazil" pageId="3" pageNumber="288">BRAZIL</collectingCountry>
(
<collectionCode box="[720,810,944,968]" pageId="3" pageNumber="288">BMNH</collectionCode>
); 
<collectingCountry box="[833,939,944,968]" name="Brazil" pageId="3" pageNumber="288">BRAZIL</collectingCountry>
