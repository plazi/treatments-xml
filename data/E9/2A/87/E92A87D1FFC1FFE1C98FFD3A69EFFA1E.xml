(
<taxonomicName box="[1274,1433,1942,1968]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="426" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
: 
<taxonomicName box="[151,268,1978,2005]" class="Insecta" family="Cynipidae" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="426" phylum="Arthropoda" rank="family">Cynipidae</taxonomicName>
