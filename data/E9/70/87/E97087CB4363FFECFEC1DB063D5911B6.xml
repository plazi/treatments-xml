<subSubSection pageId="7" pageNumber="8" type="diagnosis">
<paragraph blockId="7.[189,1398,1633,1741]" box="[189,407,1633,1659]" pageId="7" pageNumber="8">
<heading bold="true" box="[189,407,1633,1659]" fontSize="11" level="2" pageId="7" pageNumber="8" reason="6">
