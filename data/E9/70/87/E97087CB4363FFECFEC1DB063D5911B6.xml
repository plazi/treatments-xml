<paragraph blockId="7.[189,1399,1775,1883]" pageId="7" pageNumber="8">
<taxonomicName box="[189,319,1821,1847]" class="Florideophyceae" family="Rhodomelaceae" genus="Ohelopapa" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ceramiales" pageId="7" pageNumber="8" phylum="Rhodophyta" rank="genus">
