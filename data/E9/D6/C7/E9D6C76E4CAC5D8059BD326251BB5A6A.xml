<taxonomicName baseAuthorityName="G.B.Sowerby I" baseAuthorityYear="1833" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="143" pageNumber="144" phylum="Mollusca" rank="species" species="strigatus" subGenus="Drymaeus">
