<paragraph blockId="2.[136,1059,1480,1660]" pageId="2" pageNumber="213">
<taxonomicName authority="Lienhard" authorityName="Lienhard" authorityYear="2014" box="[136,436,1511,1533]" class="Insecta" family="Psyllipsocidae" genus="Psyllipsocus" kingdom="Animalia" order="Psocodea" pageId="2" pageNumber="213" phylum="Arthropoda" rank="species" species="spinifer" status="n. spec.">
