<taxonomicName authority="(Daday, 1888)" authorityName="Daday" authorityYear="1888" baseAuthorityName="Daday" baseAuthorityYear="1888" box="[198,614,437,462]" class="Branchiopoda" family="Macrothricidae" genus="Bunops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diplostraca" pageId="7" pageNumber="419" phylum="Arthropoda" rank="species" species="serricaudata">
