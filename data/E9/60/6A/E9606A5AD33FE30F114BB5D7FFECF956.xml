<emphasis box="[188,434,1279,1302]" italics="true" pageId="53" pageNumber="54">C. pseudoterrestris</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Schenkel" authorityYear="1963" box="[521,641,1279,1302]" class="Arachnida" family="Agelenidae" genus="Coelotes" kingdom="Animalia" order="Araneae" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="denisi">
<emphasis box="[521,641,1279,1302]" italics="true" pageId="53" pageNumber="54">C. denisi</emphasis>
</taxonomicName>
(
<date box="[121,306,1308,1331]" pageId="53" pageNumber="54" value="1925-03-02">March 2, 1925</date>
), were collected from 
<collectingRegion country="China" name="Yunnan" pageId="53" pageNumber="54">Yunnan</collectingRegion>
