<paragraph blockId="6.[151,784,1661,1722]" box="[151,450,1696,1722]" pageId="6" pageNumber="33">
<figureCitation box="[151,233,1696,1722]" captionStart="FIGURES 1 – 6" captionStartId="2.[151,269,1817,1841]" captionTargetBox="[186,1403,219,1816]" captionTargetId="figure@2.[151,1436,193,1887]" captionTargetPageId="2" captionText="FIGURES 1 – 6. Labia. 1. Canthochilum brodzinskyorum; 2. Canthochilum magnum; 3. Canthochilum matthewsi; 4. Canthochilum mimicum; 5. Canthochilum nebulonemi; 6. Canthochilum platycnemis." httpUri="https://zenodo.org/record/181297/files/figure.png" pageId="6" pageNumber="33">
Figs. 
<number box="[216,233,1696,1722]" pageId="6" pageNumber="33" value="4.1">4</number>
</figureCitation>
, 10, 
<number box="[289,365,1696,1722]" pageId="6" pageNumber="33" value="16.22">16, 22</number>
, 
<number box="[377,450,1696,1722]" pageId="6" pageNumber="33" value="28.37">28, 37</number>
