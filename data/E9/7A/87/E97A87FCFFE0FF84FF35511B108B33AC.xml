<number box="[216,232,1109,1135]" pageId="7" pageNumber="34" value="5.11">5</number>
</figureCitation>
, 11, 
<number box="[288,364,1109,1135]" pageId="7" pageNumber="34" value="17.23">17, 23</number>
, 
<number box="[376,452,1109,1135]" pageId="7" pageNumber="34" value="29.38">29, 38</number>
