<taxonomicName authorityName="Schweinf" authorityYear="1867" class="Magnoliopsida" family="Convolvulaceae" genus="Evolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Evolvulus agrestis" order="Solanales" pageId="117" pageNumber="118" phylum="Tracheophyta" rank="species" species="agrestis">
