<taxonomicName class="Hexapoda" family="Carabidae" genus="Poecilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="495" pageNumber="496" phylum="Arthropoda" rank="species" species="subcordatus">
