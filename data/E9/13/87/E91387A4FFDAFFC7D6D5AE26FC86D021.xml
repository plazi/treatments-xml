<taxonomicName authorityName="Mayr" authorityYear="1906" box="[189,364,606,631]" class="Pinopsida" family="Pinaceae" genus="Larix" kingdom="Plantae" order="Pinales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="principis">Larix principis</taxonomicName>
