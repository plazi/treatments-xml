<quantity id="4CE547C20437FFA7FB94FAD5F9D77063" box="[1088,1174,1325,1349]" metricMagnitude="-3" metricUnit="m" metricValue="2.2" pageId="16" pageNumber="17" unit="mm" value="2.2">2.2 mm</quantity>
: 
<quantity id="4CE547C20437FFA7FB7FFAD5F8447063" box="[1195,1285,1325,1349]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="16" pageNumber="17" unit="mm" value="0.8">0.8 mm</quantity>
