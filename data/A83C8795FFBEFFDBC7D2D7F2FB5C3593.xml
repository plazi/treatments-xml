<taxonomicName id="E7954D00FFBEFFDAC7D2D7F2FE7332A8" authority="Laporte, 1835" authorityName="Laporte" authorityYear="1835" box="[151,424,1690,1715]" class="Insecta" family="Gyrinidae" genus="Necticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="97" phylum="Arthropoda" rank="genus">
