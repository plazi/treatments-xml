<taxonomicName id="9BA34D16FFF52E3E7350FC0BFDC5DF1D" authority="Becker, 1922b: 33" authorityName="Becker" authorityPageNumber="33" authorityYear="1922" box="[264,627,1017,1041]" class="Insecta" family="Dolichopodidae" genus="Metaparaclius" kingdom="Animalia" order="Diptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="genus">
<emphasis id="6ED7EA87FFF52E3E7350FC0BFE15DF1C" box="[264,419,1018,1041]" italics="true" pageId="73" pageNumber="74">Metaparaclius</emphasis>
<treatmentCitation id="DD021084FFF52E3E73F3FC08FDC5DF1D" author="Becker" box="[427,627,1017,1040]" page="33" pageId="73" pageNumber="74" year="1922">
<bibRefCitation id="38324B64FFF52E3E73F3FC08FDFCDF1D" author="Becker" box="[427,586,1017,1040]" pageId="73" pageNumber="74" refString="Becker, T. (1922 b) Dipterologische Studien. Dolichopodidae der Indo-Australische Region. Capita Zoologica, 1 (4), 1 - 247, 19 pls." type="journal article" year="1922" yearSuffix="b">
Becker, 
<number id="EA6B24B2FFF52E3E705CFC08FD8BDF1D" box="[516,573,1017,1040]" pageId="73" pageNumber="74" value="1922.0">1922</number>
b
</bibRefCitation>
: 
<number id="EA6B24B2FFF52E3E700DFC08FDC5DF1D" box="[597,627,1017,1040]" pageId="73" pageNumber="74" value="33.0">33</number>
