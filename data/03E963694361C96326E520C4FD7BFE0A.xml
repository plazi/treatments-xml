<figureCitation id="137BCEFA4361C96324C2213CFD69FE9E" box="[688,757,294,319]" captionStart="FIGURE 5" captionStartId="6.[151,255,882,905]" captionTargetBox="[254,1332,485,857]" captionTargetId="figure@6.[252,1335,482,860]" captionTargetPageId="6" captionText="FIGURE 5. Capnopsis schilleri archaica Zwick, 1984" httpUri="https://zenodo.org/record/270747/files/figure.png" pageId="6" pageNumber="7">
