<quantity id="406CCC986031FFFEFB6970E5FAE3FB71" box="[1209,1351,1211,1233]" metricMagnitude="-2" metricUnit="m" metricValue="1.15" metricValueMax="1.25" metricValueMin="1.05" pageId="5" pageNumber="570" unit="mm" value="11.5" valueMax="12.5" valueMin="10.5">10.5–12.5 mm</quantity>
