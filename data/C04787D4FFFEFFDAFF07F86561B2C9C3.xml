<quantity id="8F169B27FFFEFFD5FE41F865674ACC9B" box="[465,521,1986,2012]" metricMagnitude="1" metricUnit="m" metricValue="4.82346" pageId="23" pageNumber="24" unit="in" value="1899.0">1899</quantity>
