<taxonomicName id="4C13D0C1FFB0FFDCFE69FEF10B727547" box="[409,583,281,305]" class="Insecta" family="Lampyridae" genus="Vesta" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="vitellinithorax">
