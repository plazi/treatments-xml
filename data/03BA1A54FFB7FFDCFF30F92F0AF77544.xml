<taxonomicName id="4C13D0C1FFB0FFDCFCF3FEF10A837547" authorityName="Perty" authorityYear="1830" box="[771,950,281,305]" class="Insecta" family="Lampyridae" genus="Lampyris" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="vitellinothorax">
