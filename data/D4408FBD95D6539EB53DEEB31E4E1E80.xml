<taxonomicName id="19C6944B0CDF2253161B619AC2C3D925" authorityName="Chiu" authorityYear="1954" class="Insecta" family="Ichneumonidae" genus="Enicospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enicospilus nasutus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="nasutus">
