<taxonomicName id="4C694D56D8539335FD3CFA8CFCC1FC75" authorityName="LeConte" authorityYear="1850" box="[689,810,1372,1391]" class="Insecta" family="Dytiscidae" genus="Ilybius" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="51" phylum="Arthropoda" rank="species" species="pleuriticus">
<emphasis id="B91DEAC7D8539335FD3CFA8CFCC1FC75" box="[689,810,1372,1391]" italics="true" pageId="15" pageNumber="51">I. pleuriticus</emphasis>
</taxonomicName>
