</taxonomicName>
, 
<emphasis id="B91DEAC7D8539335FD89FA8BFDF8FC74" bold="true" box="[516,531,1371,1390]" pageId="15" pageNumber="51">B</emphasis>
<taxonomicName id="4C694D56D8539335FD91FA8CFD60FC75" box="[540,651,1372,1391]" class="Insecta" family="Dytiscidae" genus="Ilybius" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="51" phylum="Arthropoda" rank="species" species="discedens">
