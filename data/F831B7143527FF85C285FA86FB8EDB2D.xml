<paragraph id="702706023524FF83C29AFD3BFD8ADE4A" blockId="2.[136,1059,681,784]" pageId="2" pageNumber="653">
<taxonomicName id="B7987D813524FF83C29AFD3BFE0DDF84" authorityName="Kadej &amp; Hava" authorityYear="2011" box="[136,417,712,734]" class="Insecta" family="Dermestidae" genus="Anthrenus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="653" phylum="Arthropoda" rank="species" species="ardoi" subGenus="Anthrenus">
<emphasis id="42ECDA103524FF83C29AFD3BFF46DF84" box="[136,234,712,734]" italics="true" pageId="2" pageNumber="653">Anthrenus</emphasis>
(
<emphasis id="42ECDA103524FF83C2E9FD3BFEF1DF84" box="[251,349,712,734]" italics="true" pageId="2" pageNumber="653">Anthrenus</emphasis>
) 
<emphasis id="42ECDA103524FF83C37CFD3BFE0DDF84" box="[366,417,712,734]" italics="true" pageId="2" pageNumber="653">ardoi</emphasis>
