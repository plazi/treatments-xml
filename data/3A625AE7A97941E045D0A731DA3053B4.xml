<paragraph id="A261D26C09577661CA886E121B195DE9" pageId="10" pageNumber="11">
<taxonomicName id="CB2AFEA17EC2D1B60F7FD32AE7622B08" class="Demospongiae" family="Spongiidae" genus="Spongia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Spongia pulchella" order="Dictyoceratida" pageId="10" pageNumber="11" phylum="Porifera" rank="species" species="pulchella">Spongia pulchella</taxonomicName>
