<number id="3DA624F1FFF22C11FF11F94AFF61F92C" box="[220,250,1762,1787]" pageId="1" pageNumber="268" value="85.0">85</number>
, extending beyond level of trochanters IV, setae 
<geoCoordinate id="EE5A5011FFF22C11FCD3F94BFCC0F92C" box="[798,859,1762,1787]" direction="east" orientation="longitude" pageId="1" pageNumber="268" precision="925" value="2.25">
<emphasis id="B91AEAC4FFF22C11FCD3F94BFCA3F92C" box="[798,824,1763,1787]" italics="true" pageId="1" pageNumber="268">
e 
<number id="3DA624F1FFF22C11FCE7F94BFCA3F92C" box="[810,824,1763,1787]" pageId="1" pageNumber="268" value="2.0">2</number>
</emphasis>
<number id="3DA624F1FFF22C11FCF2F94AFCC0F92C" box="[831,859,1762,1787]" pageId="1" pageNumber="268" value="15.0">15</number>
