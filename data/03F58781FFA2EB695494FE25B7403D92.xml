<taxonomicName id="4C5C4D14FFA2EB695672FAEFB7313D78" box="[625,847,1373,1398]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="species" species="pyrenaica">
