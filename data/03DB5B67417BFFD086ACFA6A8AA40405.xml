<taxonomicName id="4C7291F2417BFFD082B8F9E98D9D0466" baseAuthorityName="Hewitson" baseAuthorityYear="1867" box="[1236,1424,1570,1594]" class="Insecta" family="Lycaenidae" genus="Tmolus" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="10" phylum="Arthropoda" rank="species" species="mutina">
