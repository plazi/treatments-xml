<taxonomicName id="4C7BA1B7FF8E0F43590BFA1AFCA9F99A" authority="Gmelin" authorityName="Gmelin" box="[625,871,1519,1545]" class="Aves" family="Scolopacidae" genus="Tringa" kingdom="Animalia" order="Charadriiformes" pageId="5" pageNumber="56" phylum="Chordata" rank="species" species="flavipes">
