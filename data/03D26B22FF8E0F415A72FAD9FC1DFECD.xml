<taxonomicName id="4C7BA1B7FF8E0F435F6FF9CAFB4DF9CB" box="[1045,1155,1599,1624]" class="Arachnida" family="Syringophilidae" genus="Syringophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="5" pageNumber="56" phylum="Arthropoda" rank="species" species="helleri">
