<taxonomicName id="4C7BA1B7FF8E0F43590CFA32FCF4FA73" box="[630,826,1479,1504]" class="Aves" family="Scolopacidae" genus="Tringa" kingdom="Animalia" order="Charadriiformes" pageId="5" pageNumber="56" phylum="Chordata" rank="species" species="ochropus">
