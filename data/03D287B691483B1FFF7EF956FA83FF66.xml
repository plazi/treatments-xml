<taxonomicName id="4C7B4D2391493B1FFE80FF22FD29FF2E" authority="Levi, 1964" authorityName="Levi" authorityYear="1964" box="[361,721,153,179]" class="Demospongiae" family="Cladorhizidae" genus="Abyssocladia" kingdom="Animalia" order="Poecilosclerida" pageId="7" pageNumber="8" phylum="Porifera" rank="species" species="bruuni">
