<quantity id="A00E84FE095DFFDCFD441F317402FACD" box="[750,821,1336,1356]" metricMagnitude="3" metricUnit="m" metricValue="1.04" pageId="25" pageNumber="926" unit="m" value="1040.0">1040 m</quantity>
