<quantity id="A00E84FE095DFFDCFC141F5974CEFAE5" box="[958,1017,1360,1380]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="25" pageNumber="926" unit="m" value="750.0">750 m</quantity>
