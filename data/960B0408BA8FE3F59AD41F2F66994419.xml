<taxonomicName id="C16FFF60DE9F3166431AAAC00445E703" class="Arachnida" family="Agelenidae" genus="Pireneitega" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pireneitega tyurai" order="Araneae" pageId="7" pageNumber="96" phylum="Arthropoda" rank="species" species="tyurai">Pireneitega tyurai</taxonomicName>
