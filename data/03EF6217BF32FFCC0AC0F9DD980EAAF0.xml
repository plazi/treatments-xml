<taxonomicName id="4C46A882BF34FFCF0B0CF8329D9AACB8" authorityName="Hita Garcia &amp; Fisher" authorityYear="2012" box="[347,449,1978,1999]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="artemis">
