<typeStatus id="54838856FB05FFFCD262FC24FCB0FC28" box="[779,868,971,992]" pageId="6" pageNumber="127">holotype</typeStatus>
), 1.32–1.65 (
<typeStatus id="54838856FB05FFFCD288FC24FB96FC28" box="[993,1090,971,992]" pageId="6" pageNumber="127">paratypes</typeStatus>
