<taxonomicName id="44B84D35440BCC50A9D6FAA174B1340E" authority="Brown" authorityName="Brown" box="[164,396,1346,1373]" class="Liliopsida" family="Orchidaceae" genus="Pleurothallis" kingdom="Plantae" order="Asparagales" pageId="1" pageNumber="135" phylum="Tracheophyta" rank="genus">
