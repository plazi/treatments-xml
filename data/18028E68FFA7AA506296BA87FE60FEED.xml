<elevation id="1B86D84DFFA7AA536318B99AFEEAFD0B" box="[281,369,690,716]" metricMagnitude="3" metricUnit="m" metricValue="3.5" pageId="21" pageNumber="22" unit="m" value="3500.0">3500m</elevation>
