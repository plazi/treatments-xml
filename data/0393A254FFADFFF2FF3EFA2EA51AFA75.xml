<emphasis id="B94ECF50FFAFFFF3FF3EF9BDA162F9AD" bold="true" box="[151,282,1612,1636]" pageId="2" pageNumber="39">FIGURE 2.</emphasis>
<taxonomicName id="4C3A68C1FFAFFFF3FE8AF9BDA362F9AA" authority="Machado and Rafael" authorityName="Machado and Rafael" box="[291,794,1612,1635]" class="Insecta" family="Mantispidae" genus="Leptomantispa" kingdom="Animalia" order="Neuroptera" pageId="2" pageNumber="39" phylum="Arthropoda" rank="species" species="catarinae" status="new species">
<emphasis id="B94ECF50FFAFFFF3FE8AF9BDA257F9AA" box="[291,559,1612,1635]" italics="true" pageId="2" pageNumber="39">Leptomantispa catarinae</emphasis>
Machado and Rafael
</taxonomicName>
, 
<taxonomicNameLabel id="A27D722BFFAFFFF3FC8FF9BDA3D2F9AA" box="[806,938,1612,1635]" pageId="2" pageNumber="39" rank="species">new species</taxonomicNameLabel>
, male: 
<emphasis id="B94ECF50FFAFFFF3FC53F9BDA47FF9AD" bold="true" box="[1018,1031,1612,1636]" pageId="2" pageNumber="39">a</emphasis>
