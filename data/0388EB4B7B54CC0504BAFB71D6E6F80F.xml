<caption id="DF5E0AD57B55CC0504BAFB47D3A3FADB" ID-DOI="http://doi.org/10.5281/zenodo.4596248" ID-Zenodo-Dep="4596248" httpUri="https://zenodo.org/record/4596248/files/figure.png" pageId="5" pageNumber="132" startId="5.[151,264,1247,1271]" targetBox="[155,1435,374,1228]" targetPageId="5">
