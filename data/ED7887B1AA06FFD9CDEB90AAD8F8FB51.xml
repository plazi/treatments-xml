<taxonomicName id="A2D14D24AA06FFDBCDEB90AADA68F936" authority="(Ulmer 1915)" baseAuthorityName="Ulmer" baseAuthorityYear="1915" box="[151,559,1708,1735]" class="Insecta" family="Hydropsychidae" genus="Potamyia" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="551" phylum="Arthropoda" rank="species" species="chinensis">
