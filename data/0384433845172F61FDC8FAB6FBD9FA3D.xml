<quantity id="4CD55FCB45162F7EFCFBFE74FBA6FE0F" box="[888,1033,447,474]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" metricValueMax="4.5" metricValueMin="3.5" pageId="15" pageNumber="16" unit="mm" value="4.0" valueMax="4.5" valueMin="3.5">3.5–4.5 mm</quantity>
