</taxonomicName>
<taxonomicName id="4C7A4D16FF86FF84E3396876FAA06000" authorityName="Simon" authorityYear="1880" box="[1309,1385,1563,1587]" class="Arachnida" family="Zorocratidae" genus="Uduba" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="53" phylum="Arthropoda" rank="genus">
