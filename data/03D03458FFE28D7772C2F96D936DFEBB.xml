<taxonomicName id="4C79FECDFFE48D777009FEE090D3FE9E" box="[604,685,297,320]" class="Insecta" family="Sciaridae" genus="Pseudolycoriella" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="spicata">
