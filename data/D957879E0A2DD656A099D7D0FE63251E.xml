<emphasis id="638AEA9A0A2DD656A0C9D2A9FE812557" bold="true" box="[199,352,1518,1543]" inLineHeading="true" pageId="28" pageNumber="429" reason="1">Distribution.</emphasis>
<collectingCountry id="29E976180A2DD656A169D2A9FE2C2556" box="[359,461,1518,1542]" name="Malaysia" pageId="28" pageNumber="429">Malaysia</collectingCountry>
(
<collectingRegion id="933AF86A0A2DD656A1D3D2A9FDD72556" box="[477,566,1518,1542]" country="Malaysia" name="Pahang" pageId="28" pageNumber="429">Pahang</collectingRegion>
