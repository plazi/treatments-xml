<taxonomicName id="4C6D74DFFFFE075DFDB9F8D4FB51F8DC" authority="Banner &amp; Banner, 1971" authorityName="Banner &amp; Banner" authorityYear="1971" box="[567,1171,1865,1888]" class="Malacostraca" family="Alpheidae" genus="Prionalpheus" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="31" phylum="Arthropoda" rank="species" species="brachytomeus">
