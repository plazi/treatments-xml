, 
<specimenCount id="E1F414B5F1532F3374ADBAA3A2539D9E" box="[234,339,721,747]" pageId="12" pageNumber="13" type="female">
<number id="413ACD1BF1532F3374ADBAA3A3F89D9E" box="[234,248,721,747]" pageId="12" pageNumber="13" value="1.0">1</number>
