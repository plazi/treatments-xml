<taxonomicName id="4C184D05FF87FF90FD87F994FCBEF9F2" box="[570,814,1607,1632]" class="Insecta" family="Psychodidae" genus="Brumptomyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="154" phylum="Arthropoda" rank="species" species="hamata">
