</taxonomicName>
<taxonomicName id="4C184D05FF87FF90FAB9F994FA0CF9F2" box="[1284,1436,1609,1632]" class="Insecta" family="Psychodidae" genus="Brumptomyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="154" phylum="Arthropoda" rank="genus">
