<taxonomicName id="247F734A062AAD2404B691BA598847ED" class="Polychaeta" family="Phyllodocidae" genus="Phyllodoce" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phyllodoce madeirensis" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="madeirensis">Phyllodoce madeirensis</taxonomicName>
