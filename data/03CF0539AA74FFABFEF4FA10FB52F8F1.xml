<taxonomicName id="4C66CFACAA74FFABFB65F903FE78F8A9" authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="tortuosus">
