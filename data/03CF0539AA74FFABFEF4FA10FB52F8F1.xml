<taxonomicName id="4C66CFACAA74FFABFB9BF93BFE7AF881" authority="Azevedo, 1999" authorityName="Azevedo" authorityYear="1999" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="largidentatus">
