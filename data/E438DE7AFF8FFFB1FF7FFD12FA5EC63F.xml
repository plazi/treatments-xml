<taxonomicName id="AB9114EFFF8FFFB1FBAEFC91FA83C067" authority="Li &amp; Zhou &amp; Che, 2019" authorityName="Li &amp; Zhou &amp; Che" authorityYear="2019" box="[1094,1350,833,855]" class="Actinopterygii" family="Gastromyzontidae" genus="Fanmanenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="5" pageNumber="130" phylum="Chordata" rank="species" species="microcephala" status="sp. nov.">
