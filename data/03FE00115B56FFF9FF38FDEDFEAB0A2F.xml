(
<collectionCode id="ED4629C25B56FFF9FE53FCACFDAA0D5F" box="[508,595,868,894]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="43" pageNumber="175" type="Museum">USNM</collectionCode>
