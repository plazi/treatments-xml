<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:143207" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName-HNS="Polyrhachis mutatus Smith" order="Hymenoptera" pageId="64" pageNumber="64" phylum="Arthropoda" rank="species" species="mutatus">Polyrhachis mutatus</taxonomicName>
