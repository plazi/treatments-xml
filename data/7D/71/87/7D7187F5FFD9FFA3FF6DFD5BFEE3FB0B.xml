; FL 
<number box="[456,511,190,216]" pageId="9" pageNumber="43" value="10.9">10.9</number>
; 
<number box="[523,535,190,216]" pageId="9" pageNumber="43" value="3.0">3</number>
FD 
<number box="[582,622,190,216]" pageId="9" pageNumber="43" value="0.6">0.6</number>
