<taxonomicName baseAuthorityName="Zincken" baseAuthorityYear="1818" box="[268,410,1590,1617]" class="Insecta" family="Pyralidae" genus="Assara" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="369" phylum="Arthropoda" rank="species" species="terebrella">
<emphasis box="[268,410,1590,1617]" italics="true" pageId="8" pageNumber="369">A. terebrella</emphasis>
