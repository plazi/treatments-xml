<taxonomicName authorityName="Wainstein" authorityYear="1962" box="[151,238,785,810]" class="Arachnida" family="Phytoseiidae" genus="Euseius" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="313" phylum="Arthropoda" rank="genus">
<emphasis box="[151,238,785,810]" italics="true" pageId="12" pageNumber="313">Euseius</emphasis>
</taxonomicName>
