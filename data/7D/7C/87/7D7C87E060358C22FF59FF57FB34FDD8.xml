<subSubSection lastPageId="5" lastPageNumber="62" pageId="4" pageNumber="61" type="description">
<paragraph blockId="4.[151,1437,258,858]" box="[199,649,330,355]" pageId="4" pageNumber="61">
<emphasis bold="true" box="[199,345,330,355]" pageId="4" pageNumber="61">Description.</emphasis>
BL: 
<quantity box="[409,497,330,354]" metricMagnitude="-3" metricUnit="m" metricValue="6.2" pageId="4" pageNumber="61" unit="mm" value="6.2">6.2 mm</quantity>
, FL: 
<quantity box="[556,645,330,354]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="4" pageNumber="61" unit="mm" value="3.1">3.1 mm</quantity>
