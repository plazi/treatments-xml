, Lin’an City, Tianmu Mountain Nature Reserve, Wuliting, 
<quantity box="[225,292,1660,1686]" metricMagnitude="2" metricUnit="m" metricValue="8.61" pageId="1" pageNumber="471" unit="m" value="861.0">861m</quantity>
, 
<geoCoordinate box="[304,449,1659,1686]" degrees="30" direction="north" minutes="20.228" orientation="latitude" pageId="1" pageNumber="471" precision="1" value="30.337133">30°20.228′N</geoCoordinate>
, 
<geoCoordinate box="[461,617,1659,1686]" degrees="119" direction="east" minutes="26.136" orientation="longitude" pageId="1" pageNumber="471" precision="1" value="119.4356">119°26.136′E</geoCoordinate>
, 
<date box="[629,761,1660,1686]" pageId="1" pageNumber="471" value="2018-04-22">2018-IV-22</date>
