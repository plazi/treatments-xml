and 
<taxonomicName class="Insecta" family="Rhamnaceae" genus="Ziziphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Ziziphus nummularia" order="Coleoptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="nummularia">
