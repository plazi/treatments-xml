<taxonomicName authority="Ahmad et al." authorityName="Ahmad et al." authorityYear="1973" box="[151,538,521,543]" class="Insecta" family="Coreidae" genus="Paranotocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="399" phylum="Arthropoda" rank="species" species="echinus">
