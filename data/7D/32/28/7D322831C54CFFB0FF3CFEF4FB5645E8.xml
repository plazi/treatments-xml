: figs 
<number box="[714,770,522,543]" pageId="2" pageNumber="399" value="4.24">4, 24</number>
, 
<number box="[784,810,522,544]" pageId="2" pageNumber="399" value="37.0">37</number>
) (catalogue, habitus figure); Ahmad (
<number box="[1210,1265,522,543]" pageId="2" pageNumber="399" value="1979.0">1979</number>
: 
<number box="[1278,1305,522,544]" pageId="2" pageNumber="399" value="42.0">42</number>
) (checklist); Ahmad (
<number box="[287,342,552,573]" pageId="2" pageNumber="399" value="1980.0">1980</number>
