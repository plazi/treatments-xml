</taxonomicName>
, 
<taxonomicName authority="Luther, 1947" authorityName="Luther" authorityYear="1947" class="Liliopsida" family="Orchidaceae" genus="Macrostomum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="8" phylum="Tracheophyta" rank="species" species="curvituba">
<emphasis italics="true" pageId="8">M. curvituba</emphasis>
<bibRefCitation author="Luther" box="[267,415,260,285]" pageId="8" refString="Luther, A. (1947) Untersuchungen an rhabdocoelen Turbellarien VI. Macrostomiden aus Finnland. Acta Zoologica Fennica, 49, 1 - 38" type="journal article" year="1947">Luther, 1947</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Wang &amp; Sun, 2015" authorityName="Wang &amp; Sun" authorityYear="2015" box="[428,865,260,285]" class="Liliopsida" family="Orchidaceae" genus="Macrostomum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="8" phylum="Tracheophyta" rank="species" species="dongyuanensis">
<emphasis box="[428,633,260,285]" italics="true" pageId="8">M. dongyuanensis</emphasis>
<bibRefCitation author="Wang" box="[643,865,260,285]" pageId="8" refString="Sun, T., Zhang, L., Wang, A. - T. &amp; Zhang, Y. (2015) Three new species of freshwater Macrostomum (Platyhelminthes, Macrostomida) from southern China. Zootaxa, 4012 (1), 120 - 134. http: // dx. doi. org / 10.11646 / zootaxa. 4012.1.6" type="journal article" year="2015">Wang &amp; Sun, 2015</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Young, 1976" authorityName="Young" authorityYear="1976" box="[878,1197,260,285]" class="Liliopsida" family="Orchidaceae" genus="Macrostomum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="8" phylum="Tracheophyta" rank="species" species="georgeense">
