<taxonomicName box="[404,566,1753,1776]" class="Amphibia" family="Hylidae" genus="Litoria" kingdom="Animalia" order="Anura" pageId="13" pageNumber="54" phylum="Chordata" rank="species" species="sauroni">
<emphasis box="[404,566,1753,1776]" italics="true" pageId="13" pageNumber="54">Litoria sauroni</emphasis>
</taxonomicName>
(A) and 
<taxonomicName box="[667,787,1753,1776]" class="Amphibia" family="Hylidae" genus="Litoria" kingdom="Animalia" order="Anura" pageId="13" pageNumber="54" phylum="Chordata" rank="species" species="dux">
