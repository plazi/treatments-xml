<figureCitation box="[684,811,1564,1590]" captionStart="FIGURE 3" captionStartId="4.[151,250,1757,1781]" captionTargetBox="[151,1433,242,1733]" captionTargetId="figure-40@4.[151,1435,237,1733]" captionTargetPageId="4" captionText="FIGURE 3. Male and female genital anatomy of Songthela jinyun sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–K, vulva dorsal view; L–O, vulva ventral view; A–G, XUX-2017-293 (holotype); H, L, XUX-2017-289; I, M, XUX-2012-189; J, N, XUX-2017-291; K, O, XUX-2012-165A. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863696" httpUri="https://zenodo.org/record/5863696/files/figure.png" pageId="1" pageNumber="547">Figs 3A–E</figureCitation>
, 
<figureCitation box="[825,908,1564,1590]" captionStart="FIGURE 4" captionStartId="6.[151,250,1869,1893]" captionTargetBox="[151,1435,195,1845]" captionTargetId="figure-20@6.[151,1435,181,1845]" captionTargetPageId="6" captionText="FIGURE 4. Male and female genital anatomy of Songthela longbao sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–J, vulva dorsal view; K–M, vulva ventral view; A–G, XUX-2012-252 (holotype); H, K, XUX-2012-251; I, L, XUX-2012-257; J, M, XUX-2012-258. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863698" httpUri="https://zenodo.org/record/5863698/files/figure.png" pageId="1" pageNumber="547">4A–4E</figureCitation>
, 
<figureCitation box="[921,1003,1564,1591]" captionStart="FIGURE 5" captionStartId="8.[151,250,1902,1926]" captionTargetBox="[151,1435,182,1878]" captionTargetId="figure-20@8.[151,1435,181,1878]" captionTargetPageId="8" captionText="FIGURE 5. Male and female genital anatomy of Songthela serriformis sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–J, vulva dorsal view; K–M, vulva ventral view; A–C, G, XUX-2012- 245 (holotype); D–F, XUX-2012-241A; H, K, XUX-2012-238; I, L, XUX-2012-237; J, M, XUX-2012-241. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863702" httpUri="https://zenodo.org/record/5863702/files/figure.png" pageId="1" pageNumber="547">5A–5E</figureCitation>
, 
<figureCitation box="[1017,1086,1564,1591]" captionStart="FIGURE 6" captionStartId="11.[151,250,1875,1899]" captionTargetBox="[151,1435,193,1851]" captionTargetId="figure-24@11.[151,1435,181,1851]" captionTargetPageId="11" captionText="FIGURE 6. Male and female genital anatomy of Songthela wangerbao sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–J, vulva dorsal view; K–M, vulva ventral view; A–C, XUX-2012-271 (holotype); D–G, XUX-2012-263; H, K, XUX-2012-267; I, L, XUX-2012-261; J, M, XUX-2012-265. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863708" httpUri="https://zenodo.org/record/5863708/files/figure.png" pageId="1" pageNumber="547">6A–E</figureCitation>
); by embolus with a wide and flat opening (
<figureCitation box="[301,443,1600,1626]" captionStart="FIGURE 3" captionStartId="4.[151,250,1757,1781]" captionTargetBox="[151,1433,242,1733]" captionTargetId="figure-40@4.[151,1435,237,1733]" captionTargetPageId="4" captionText="FIGURE 3. Male and female genital anatomy of Songthela jinyun sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–K, vulva dorsal view; L–O, vulva ventral view; A–G, XUX-2017-293 (holotype); H, L, XUX-2017-289; I, M, XUX-2012-189; J, N, XUX-2017-291; K, O, XUX-2012-165A. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863696" httpUri="https://zenodo.org/record/5863696/files/figure.png" pageId="1" pageNumber="547">Figs 3A, 3D</figureCitation>
, 
<figureCitation box="[455,540,1600,1626]" captionStart="FIGURE 4" captionStartId="6.[151,250,1869,1893]" captionTargetBox="[151,1435,195,1845]" captionTargetId="figure-20@6.[151,1435,181,1845]" captionTargetPageId="6" captionText="FIGURE 4. Male and female genital anatomy of Songthela longbao sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–J, vulva dorsal view; K–M, vulva ventral view; A–G, XUX-2012-252 (holotype); H, K, XUX-2012-251; I, L, XUX-2012-257; J, M, XUX-2012-258. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863698" httpUri="https://zenodo.org/record/5863698/files/figure.png" pageId="1" pageNumber="547">4A, 4D</figureCitation>
, 
<figureCitation box="[552,637,1600,1627]" captionStart="FIGURE 5" captionStartId="8.[151,250,1902,1926]" captionTargetBox="[151,1435,182,1878]" captionTargetId="figure-20@8.[151,1435,181,1878]" captionTargetPageId="8" captionText="FIGURE 5. Male and female genital anatomy of Songthela serriformis sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–J, vulva dorsal view; K–M, vulva ventral view; A–C, G, XUX-2012- 245 (holotype); D–F, XUX-2012-241A; H, K, XUX-2012-238; I, L, XUX-2012-237; J, M, XUX-2012-241. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863702" httpUri="https://zenodo.org/record/5863702/files/figure.png" pageId="1" pageNumber="547">5A, 5D</figureCitation>
, 
<figureCitation box="[649,735,1600,1627]" captionStart="FIGURE 6" captionStartId="11.[151,250,1875,1899]" captionTargetBox="[151,1435,193,1851]" captionTargetId="figure-24@11.[151,1435,181,1851]" captionTargetPageId="11" captionText="FIGURE 6. Male and female genital anatomy of Songthela wangerbao sp. nov. A, D, palp prolateral view; B, E, palp ventral view; C, F, palp retrolateral view; G, palp distal view; H–J, vulva dorsal view; K–M, vulva ventral view; A–C, XUX-2012-271 (holotype); D–G, XUX-2012-263; H, K, XUX-2012-267; I, L, XUX-2012-261; J, M, XUX-2012-265. Scale bars: 0.3 mm." figureDoi="http://doi.org/10.5281/zenodo.5863708" httpUri="https://zenodo.org/record/5863708/files/figure.png" pageId="1" pageNumber="547">6A, 6D</figureCitation>
). 
<taxonomicName authorityName="Ono" authorityYear="2000" box="[755,867,1600,1626]" class="Arachnida" family="Liphistiidae" genus="Songthela" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="547" phylum="Arthropoda" rank="genus">
<emphasis box="[755,867,1600,1626]" italics="true" pageId="1" pageNumber="547">Songthela</emphasis>
</taxonomicName>
