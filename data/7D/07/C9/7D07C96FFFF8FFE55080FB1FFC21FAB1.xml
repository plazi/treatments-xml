); from 
<taxonomicName authority="Chen &amp; Liu &amp; Li &amp; Xu, 2022" authorityName="Chen &amp; Liu &amp; Li &amp; Xu" authorityYear="2022" box="[422,574,1843,1869]" class="Arachnida" family="Liphistiidae" genus="Songthela" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="551" phylum="Arthropoda" rank="species" species="serriformis" status="sp. nov.">
