<taxonomicName authorityName="Zumpt" authorityYear="1933" class="Insecta" family="Curculionidae" genus="Dorytomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dorytomus peneckei" order="Coleoptera" pageId="0" pageNumber="375" phylum="Arthropoda" rank="species" species="peneckei">
<emphasis italics="true" pageId="0" pageNumber="375">Dorytomus peneckei</emphasis>
