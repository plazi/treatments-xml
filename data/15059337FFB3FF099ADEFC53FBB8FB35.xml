<taxonomicName id="5AAC59A2FFB3FF099E53FBE3FA90FBA4" box="[1050,1381,1061,1086]" class="Actinopterygii" family="Macrouridae" genus="Hymenocephalus" kingdom="Animalia" order="Gadiformes" pageId="10" pageNumber="35" phylum="Chordata" rank="species" species="aterrimus">
