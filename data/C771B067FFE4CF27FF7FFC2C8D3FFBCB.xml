Genus 
<taxonomicName id="88D87AF2FFE4CF27FF06FC2C8A2DFC37" authority="K. Viets, 1929" authorityName="K. Viets" authorityYear="1929" box="[238,560,945,972]" class="Arachnida" family="Teratothyadidae" genus="Teratothyas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="44" pageNumber="45" phylum="Arthropoda" rank="genus">
