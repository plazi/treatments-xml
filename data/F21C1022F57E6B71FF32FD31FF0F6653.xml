<emphasis id="48C17D26F57D6B70FDF6FA6EFDA867B3" bold="true" box="[561,594,1428,1445]" pageId="22" pageNumber="43">C -E</emphasis>
, 
<emphasis id="48C17D26F57D6B70FD9AFA6EFD9167B3" bold="true" box="[605,619,1428,1445]" pageId="22" pageNumber="43">G</emphasis>
, holotype (MNHN 317 BF); 
<emphasis id="48C17D26F57D6B70FC96FA6EFCA467B3" bold="true" box="[849,862,1428,1445]" pageId="22" pageNumber="43">B</emphasis>
, 
<emphasis id="48C17D26F57D6B70FCAFFA6EFC8967B3" bold="true" box="[872,883,1428,1445]" pageId="22" pageNumber="43">F</emphasis>
, 
<emphasis id="48C17D26F57D6B70FCB9FA6EFC7667B3" bold="true" box="[894,908,1428,1445]" pageId="22" pageNumber="43">H</emphasis>
, paratype (MNHN 333 BF); 
<emphasis id="48C17D26F57D6B70FBB4FA6EFB7A67B3" bold="true" box="[1139,1152,1428,1445]" pageId="22" pageNumber="43">A</emphasis>
, total view; 
<emphasis id="48C17D26F57D6B70FF50FA53FF5E67AC" bold="true" box="[151,164,1449,1466]" pageId="22" pageNumber="43">B</emphasis>
, total view; 
<emphasis id="48C17D26F57D6B70FECDFA53FEE267AC" bold="true" box="[266,280,1449,1466]" pageId="22" pageNumber="43">C</emphasis>
, cephalic end; 
<emphasis id="48C17D26F57D6B70FE5FFA53FE5C67AC" bold="true" box="[408,422,1449,1466]" pageId="22" pageNumber="43">D</emphasis>
