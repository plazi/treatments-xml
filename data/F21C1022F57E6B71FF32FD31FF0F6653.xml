<taxonomicName id="BDB5DAB7F57D6B70FF36FA6EFE3867B3" authority="Miljutin, 2004" authorityName="Miljutin" authorityYear="2004" box="[241,450,1428,1445]" class="Chromadorea" family="Benthimermithidae" genus="Benthimermis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Benthimermithida" pageId="22" pageNumber="43" phylum="Nematoda" rank="species" species="turpicauda" status="n. sp.">
<emphasis id="48C17D26F57D6B70FF36FA6EFE3867B3" box="[241,450,1428,1445]" italics="true" pageId="22" pageNumber="43">Benthimermis turpicauda</emphasis>
</taxonomicName>
<taxonomicNameLabel id="53F2C05DF57D6B70FE00FA6EFE0367B3" box="[455,505,1428,1445]" pageId="22" pageNumber="43" rank="species">n. sp.</taxonomicNameLabel>
,; 
<emphasis id="48C17D26F57D6B70FDDDFA6EFDDD67B3" bold="true" box="[538,551,1428,1445]" pageId="22" pageNumber="43">A</emphasis>
