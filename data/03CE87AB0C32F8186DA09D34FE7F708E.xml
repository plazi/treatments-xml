<taxonomicName id="4C674D3E0C33F81E6C079C0DFDEE703A" box="[359,517,1562,1583]" class="Insecta" family="Erotylidae" genus="Altisessor" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="34" phylum="Arthropoda" rank="species" species="undetermined">
