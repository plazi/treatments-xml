<emphasis id="B913EAAF0C33F81E691C9C0EFB4B703A" bold="true" box="[1148,1184,1561,1583]" pageId="35" pageNumber="34">58)</emphasis>
<taxonomicName id="4C674D3E0C33F81E69C59C0DFADB703A" authorityName="Zayas" authorityYear="1988" box="[1189,1328,1562,1583]" class="Insecta" family="Erotylidae" genus="Oocyanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="34" phylum="Arthropoda" rank="species" species="gundlachi">
