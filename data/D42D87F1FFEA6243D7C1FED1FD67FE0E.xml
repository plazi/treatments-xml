<taxonomicName id="9B844D64FFEA6243D797FED1FD21FEFE" authority="(Panzer, 1799)" baseAuthorityName="Panzer" baseAuthorityYear="1799" box="[193,682,293,318]" class="Insecta" family="Tettigoniidae" genus="Myrmecophilus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="acervorum">
<emphasis id="6EF0EAF5FFEA6243D797FED1FE72FEFD" bold="true" box="[193,505,293,318]" italics="true" pageId="44" pageNumber="45">Myrmecophilus acervorum</emphasis>
