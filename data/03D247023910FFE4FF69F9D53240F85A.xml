, 
<taxonomicName id="4C7B8D973917FFE4FB83FEEA340FFEE2" baseAuthorityName="Zhu &amp; Cao" baseAuthorityYear="1988" box="[1149,1298,333,359]" class="Actinopterygii" family="Nemacheilidae" genus="Homatula" kingdom="Animalia" order="Cypriniformes" pageId="12" pageNumber="342" phylum="Chordata" rank="species" species="erhaiensis">
