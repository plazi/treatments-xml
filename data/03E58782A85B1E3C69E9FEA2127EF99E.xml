<taxonomicName id="4C4C4D17A85B1E3D6B06FDE013B8FDD7" authority="Wasowska 2007" authorityName="Wasowska" authorityYear="2007" box="[632,1012,565,591]" class="Insecta" family="Coleoptera" genus="Labidostomis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="genus">
