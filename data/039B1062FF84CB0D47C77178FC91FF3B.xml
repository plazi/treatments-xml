</taxonomicName>
prp. 
<taxonomicName id="4C32DAF7FF84CB0E455971A5FD3DF8EB" authority="Gerecke 1991" authorityName="K. Viets" authorityYear="1925" box="[521,666,1771,1797]" class="Arachnida" family="Hygrobatidae" genus="Atractides" kingdom="Animalia" order="Trombidiformes" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="panniculatus">
