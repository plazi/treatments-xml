<paragraph id="C763E1029C4073E521743F340F5D9410" pageId="0" pageNumber="189">
<taxonomicName id="F81916C4568C839ACA4824BD3F3B8273" authority="Borutzky, 1957" authorityName="Borutzky" authorityYear="1957" class="Malacostraca" family="Agnaridae" genus="Protracheoniscus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Protracheoniscus kryszanovskii" order="Isopoda" pageId="0" pageNumber="189" phylum="Arthropoda" rank="species" species="kryszanovskii">Protracheoniscus kryszanovskii Borutzky, 1957</taxonomicName>
