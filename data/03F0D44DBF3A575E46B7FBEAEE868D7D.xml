<taxonomicName id="4C591ED8BF3A575E42EEFB5AE9B98CC1" box="[1230,1436,1196,1220]" class="Magnoliopsida" family="Acanthaceae" genus="Avicennia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="3" pageNumber="224" phylum="Tracheophyta" rank="species" species="marina">
