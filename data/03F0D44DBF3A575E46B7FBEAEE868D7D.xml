<collectingRegion id="499DABB9BF3A575E46B7FB5AED318CC0" box="[151,276,1196,1221]" country="Mozambique" name="Inhambane" pageId="3" pageNumber="224">Inhambane</collectingRegion>
(
<collectingCountry id="F34E25CBBF3A575E4709FB5AEDE58CC0" box="[297,448,1196,1221]" name="Mozambique" pageId="3" pageNumber="224">Mozambique</collectingCountry>
