<quantity id="4CD00792FF84FF83A02CFBC4FAA8FF04" box="[1287,1372,188,212]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="13" pageNumber="354" unit="m" value="2000.0">2000 m</quantity>
(
<collectingRegion id="49EC6495FF84FF83A041FBC4FF09FF28" country="Saudi Arabia" name="'Asir" pageId="13" pageNumber="354">Asir Province</collectingRegion>
, Qaraah) 
<materialsCitation id="3B40A02AFF84FF83A447FB98FEA6FEEF" ID-GBIF-Occurrence="2608963575" collectionCode="GRNR" elevation="1897" location="Asir, Baha and Jizan" pageId="13" pageNumber="354" specimenCount="1" stateProvince="Asir, Baha and Jizan">
