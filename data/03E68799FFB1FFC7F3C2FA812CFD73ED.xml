<taxonomicName id="4C4F4D0CFFB1FFC7F709F88E2AC073B5" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1116,1224,1907,1930]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="pacata" status="sp. nov.">
<emphasis id="B93BEA9DFFB1FFC7F709F88E2AC073B5" box="[1116,1224,1907,1930]" italics="true" pageId="82" pageNumber="83">R. pacata</emphasis>
