<taxonomicName id="4C4F4D0CFFB1FFC7F029F88E2DC173B4" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[892,969,1907,1931]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="fera" status="sp. nov.">
