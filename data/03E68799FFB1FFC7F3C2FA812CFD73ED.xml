<taxonomicName id="4C4F4D0CFFB1FFC7F21DF88E2C0A73B5" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[328,514,1907,1931]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="ramosa">
