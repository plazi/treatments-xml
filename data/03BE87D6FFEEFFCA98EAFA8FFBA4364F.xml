<geoCoordinate id="EE235007FFEEFFCA99C8F9FEF86B3622" box="[437,554,1548,1575]" degrees="117" direction="west" minutes="25" orientation="longitude" pageId="127" pageNumber="128" precision="925" value="-117.416664">117°25’W</geoCoordinate>
) (NWFSC-FRAM). Benthic; depth: 
<quantity id="4CEF9B25FFEEFFCA9BBEF9FEFE093622" box="[963,1096,1548,1575]" metricMagnitude="2" metricUnit="m" metricValue="8.07" metricValueMax="15.959999999999999" metricValueMin="0.18" pageId="127" pageNumber="128" unit="m" value="807.0" valueMax="1596.0" valueMin="18.0">18–1,596 m</quantity>
(
<quantity id="4CEF9B25FFEEFFCA9C2BF9FEFE983622" box="[1110,1241,1548,1575]" metricMagnitude="2" metricUnit="m" metricValue="8.068056" metricValueMax="15.95628" metricValueMin="0.179832" pageId="127" pageNumber="128" unit="ft" value="2647.0" valueMax="5235.0" valueMin="59.0">59–5,235 ft</quantity>
) (min.: 
<bibRefCitation id="EF864B31FFEEFFCA9D52F9FEFB0A364F" author="Allen, M. J. &amp; Smith, G. B." pageId="127" pageNumber="128" refId="ref259821" refString="Allen, M. J. &amp; Smith, G. B. (1988) Atlas and zoogeography of common fishes in the Bering Sea and northeastern Pacific. NOAA Technical Report NMFS, 66." type="book" year="1988">Allen and Smith 1988</bibRefCitation>
