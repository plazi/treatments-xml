. To 
<quantity id="4CEF9B25FFEEFFCA9B1EFA8FF9E9359D" box="[867,936,1405,1432]" metricMagnitude="-1" metricUnit="m" metricValue="2.9" pageId="127" pageNumber="128" unit="cm" value="29.0">29 cm</quantity>
