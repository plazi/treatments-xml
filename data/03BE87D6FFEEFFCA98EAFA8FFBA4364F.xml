<geoCoordinate id="EE235007FFEEFFCA9A37FA1BF8EC3606" box="[586,685,1513,1539]" degrees="34" direction="north" minutes="34" orientation="latitude" pageId="127" pageNumber="128" precision="925" value="34.566666">34°34’N</geoCoordinate>
