<geoCoordinate id="EE235007FFEEFFCA993BF9FEFBEB3623" box="[326,426,1548,1574]" degrees="33" direction="north" minutes="04" orientation="latitude" pageId="127" pageNumber="128" precision="925" value="33.066666">33°04’N</geoCoordinate>
