<quantity id="B60B9B1DFF82C20CAB4CC8BBFA8BFB28" box="[1288,1355,1196,1220]" metricMagnitude="2" metricUnit="m" metricValue="2.4" pageId="11" pageNumber="12" unit="m" value="240.0">240m</quantity>
