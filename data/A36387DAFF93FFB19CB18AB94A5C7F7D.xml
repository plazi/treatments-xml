<quantity id="EC329B29FF92FFB19D278DA3480C7ABD" box="[257,379,672,698]" metricMagnitude="-1" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="21" pageNumber="22" unit="m" value="0.2" valueMax="0.3" valueMin="0.1">0.1–0.3 m</quantity>
