<taxonomicName id="ECCA4D4FFF92FFB19E518D7B4A477A96" box="[631,816,632,657]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="inthanonense">
