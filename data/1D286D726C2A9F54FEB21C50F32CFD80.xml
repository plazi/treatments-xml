<number id="2349CE436C289F53FEAC197AF098FC98" box="[278,404,804,830]" pageId="3" pageNumber="40" value="1.4500000000000002" valueMax="1.53" valueMin="1.37">1.37–1.53</number>
(
<number id="2349CE436C289F53FE11197AF0EFFC98" box="[427,483,804,830]" pageId="3" pageNumber="40" value="1.43">1.43</number>
). Oesophagus distinct, 
<number id="2349CE436C289F53FCB2197AF26CFC98" box="[776,864,804,830]" pageId="3" pageNumber="40" value="120.5" valueMax="157.0" valueMin="84.0">84–157</number>
(
<number id="2349CE436C289F53FCC2197AF2AAFC98" box="[888,934,804,830]" pageId="3" pageNumber="40" value="111.0">111</number>
