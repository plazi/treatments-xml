<taxonomicName id="FADA4D117A48FFC1FE9BF84609F1F814" authority="Navas." authorityName="Navas." box="[347,641,1942,1964]" class="Insecta" family="Ascalaphidae" genus="Disparomitus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neuroptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="citernii">
