, 
<taxonomicName id="AECA4D3CFFE39439B49A5E7FFBB74829" authority="Morch" authorityName="Morch" box="[679,1058,775,801]" class="Polychaeta" family="Serpulidae" genus="Pyrgopolon" kingdom="Animalia" order="Sabellida" pageId="6" pageNumber="53" phylum="Annelida" rank="species" species="ctenactis">
