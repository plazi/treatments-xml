<taxonomicName id="8ECE4D24FFDDFFDDFE05F64FFDF02E5B" authorityName="Tachikawa" authorityYear="1956" box="[475,565,284,307]" class="Insecta" family="Encyrtidae" genus="Leptomastidea" kingdom="Animalia" order="Hymenoptera" pageId="31" pageNumber="376" phylum="Arthropoda" rank="species" species="rubra">
