<paragraph id="CE7EAA6C0039A99BA70AF5DDE64678D8">
<taxonomicName id="3AD89C2F442F1E55538DD06B1AB39090" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:140180" class="Insecta" family="Formicidae" genus="Messor" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Messor plinii Santschi" lsidName-HNS="Messor plinii Santschi" order="Hymenoptera" phylum="Arthropoda" rank="species" species="plinii">Messor plinii</taxonomicName>
, 
<taxonomicNameLabel id="A1F41549C5B4869A15666BD6977090D8" rank="species">n. sp.</taxonomicNameLabel>
</paragraph>
