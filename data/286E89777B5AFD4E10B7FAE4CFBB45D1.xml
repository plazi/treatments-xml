<quantity id="673F95847B5AFD4F14D3F974C954400E" box="[1266,1401,1782,1803]" metricMagnitude="3" metricUnit="m" metricValue="2.35" metricValueMax="3.0" metricValueMin="1.7" pageId="20" pageNumber="21" unit="m" value="2350.0" valueMax="3000.0" valueMin="1700.0">1700–3000 m</quantity>
