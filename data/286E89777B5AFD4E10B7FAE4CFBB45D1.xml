<paragraph id="A07838617B5AFD4F10A9F974CF3C4035" blockId="20.[136,1452,1458,1840]" pageId="20" pageNumber="21">
<taxonomicName id="67C743E27B5AFD4F10A9F974CE754009" authority="Nessel (1935: 68 - 69)" authorityName="Nessel" authorityPageNumber="68 - 69" authorityYear="1935" box="[136,600,1782,1804]" class="Lycopodiopsida" family="Lycopodiaceae" genus="Urostachys" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lycopodiales" pageId="20" pageNumber="21" phylum="Tracheophyta" rank="species" species="ringshausenii">
