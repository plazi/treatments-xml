<quantity id="4CC19B042C5BFFA703DBFA7CFD52F983" box="[698,757,1522,1542]" metricMagnitude="2" metricUnit="m" metricValue="3.0" pageId="32" pageNumber="153" unit="m" value="300.0">300 m</quantity>
