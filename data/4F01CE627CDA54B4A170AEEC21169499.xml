<taxonomicName id="96F52C7379BA55A6A2FB3ED4C51DF447" class="Insecta" genus="P." kingdom="Animalia" lsidName="P. delphius" order="Coleoptera" pageId="0" pageNumber="193" phylum="Arthropoda" rank="species" species="delphius">
