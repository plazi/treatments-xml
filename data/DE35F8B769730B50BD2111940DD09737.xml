75. 
<taxonomicName id="84211A465FA78CFA30E7442AD508693C" LSID="urn:lsid:zoobank.org:act:5E783DAA-30CE-41D6-A8DE-0FEE8E94B143" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus scissops" order="Coleoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="scissops">Trigonopterus scissops Riedel</taxonomicName>
<taxonomicNameLabel id="20F609CE2963F01A7B4F56C0A53FC63B" pageId="66" pageNumber="67">sp. n.</taxonomicNameLabel>
