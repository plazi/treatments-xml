<geoCoordinate id="BEC4438797EFFF39FE1FFB89FDAEBB32" box="[509,592,1028,1053]" direction="north" orientation="latitude" pageId="184" pageNumber="185" precision="925" value="36.083332">36°5''N</geoCoordinate>
