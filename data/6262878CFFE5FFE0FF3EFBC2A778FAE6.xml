<caption id="BEB46612FFE3FFE1FF3EF8C0A76CF802" ID-DOI="http://doi.org/10.5281/zenodo.3708780" ID-Zenodo-Dep="3708780" httpUri="https://zenodo.org/record/3708780/files/figure.png" pageId="12" pageNumber="463" startId="12.[151,250,1822,1847]" targetBox="[208,1379,182,1796]" targetPageId="12">
<paragraph id="EA74369AFFE3FFE1FF3EF8C0A76CF802" blockId="12.[151,1437,1822,2027]" pageId="12" pageNumber="463">
<emphasis id="D8BFEA88FFE3FFE1FF3EF8C0A71BF8DE" bold="true" box="[151,276,1822,1847]" pageId="12" pageNumber="463">FIGURE 7.</emphasis>
<taxonomicName id="2DCB4D19FFE3FFE1FEB2F8C0A7D4F8DF" authority="Gordon, 2020" authorityName="Gordon" authorityYear="2020" box="[283,475,1822,1846]" class="Gymnolaemata" family="Hippothoidae" genus="Jessethoa" kingdom="Animalia" order="Cheilostomatida" pageId="12" pageNumber="463" phylum="Bryozoa" rank="species" species="ausubeli" status="n. gen., n. sp.">
<emphasis id="D8BFEA88FFE3FFE1FEB2F8C0A7D4F8DF" box="[283,475,1822,1846]" italics="true" pageId="12" pageNumber="463">Jessethoa ausubeli</emphasis>
</taxonomicName>
<emphasis id="D8BFEA88FFE3FFE1FE4AF8C1A47CF8DE" bold="true" box="[483,627,1823,1847]" pageId="12" pageNumber="463">
