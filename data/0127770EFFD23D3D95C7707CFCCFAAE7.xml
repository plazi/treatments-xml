<typeStatus id="563578BAFFD23D3C97877772FCE4AF1B" box="[727,834,1296,1320]" pageId="16" pageNumber="17">paratypes</typeStatus>
(
<specimenCode id="D9286E63FFD23D3C9602776DFB94AF14" box="[850,1074,1295,1319]" collectionCode="ANIC" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" lsid="urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="16" pageNumber="17">ANIC # 42–000005</specimenCode>
), 
<location id="8C5190C3FFD23D3C911A776DFB18AF14" LSID="urn:lsid:plazi:treatment:0127770EFFD23D3D95C7707CFCCFAAE7:8C5190C3FFD23D3C911A776DFB18AF14" box="[1098,1214,1295,1319]" country="Indonesia" name="Australian" pageId="16" pageNumber="17">Australian</location>
border interception (N121257) from timber ornaments imported from 
<emphasis id="BBFA1A0AFFD23D3C97987751FCE3AF7F" bold="true" box="[712,837,1331,1356]" pageId="16" pageNumber="17">
