<subSubSection id="C32F655F4C494056E968DB5CFEC40CD9" pageId="2" pageNumber="3" type="etymology">
<paragraph id="8B8A36D44C494056E968DB5CFEC40CD9" blockId="2.[264,1323,284,1870]" pageId="2" pageNumber="3">
<emphasis id="B941EAC64C494056E968DB5CFE7B0CF2" bold="true" box="[312,452,363,389]" pageId="2" pageNumber="3">Etymology.</emphasis>
