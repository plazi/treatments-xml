<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182074" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole rutilana Wilson" lsidName-HNS="Pheidole rutilana Wilson" order="Hymenoptera" pageNumber="673" phylum="Arthropoda" rank="species" species="rutilana">rutilana</taxonomicName>
