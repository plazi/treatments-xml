<taxonomicName class="Insecta" family="Curculionidae" genus="Cylindrocorynus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cylindrocorynus" order="Coleoptera" pageId="259" pageNumber="260" phylum="Arthropoda" rank="genus">Cylindrocorynus</taxonomicName>
<normalizedToken originalValue="Schönherr">Schoenherr</normalizedToken>
