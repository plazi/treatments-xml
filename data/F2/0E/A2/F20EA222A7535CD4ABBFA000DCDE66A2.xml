<taxonomicName authorityName="Jing Z. Sun &amp; X. Z. Liu" authorityYear="2020" class="Sordariomycetes" family="Hypocreaceae" genus="Trichoderma" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="vermifimicola" order="Hypocreales" pageId="0" pageNumber="109" phylum="Ascomycota" rank="species" species="vermifimicola">
