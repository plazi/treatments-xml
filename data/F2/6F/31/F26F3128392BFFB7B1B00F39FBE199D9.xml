<emphasis box="[1004,1148,1079,1101]" italics="true" pageId="36" pageNumber="37">I. dobrogensis</emphasis>
</taxonomicName>
(RO: Popina Island); 175— 
<taxonomicName box="[151,272,1109,1131]" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="zubowskii">
<emphasis box="[151,272,1109,1131]" italics="true" pageId="36" pageNumber="37">I. zubowskii</emphasis>
</taxonomicName>
(RO: Babadag, NMB); 176— 
<taxonomicName box="[577,733,1109,1131]" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="camptoxypha">
<emphasis box="[577,733,1109,1131]" italics="true" pageId="36" pageNumber="37">I. camptoxypha</emphasis>
</taxonomicName>
(RO: “Bucec b. Sinaia, 13–1800 m, 25.8.41, Ramme”, NMB); 177— 
<taxonomicName box="[151,231,1140,1162]" class="Insecta" family="Phaneropteridae" genus="Isophya" kingdom="Animalia" order="Orthoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="gulae">
