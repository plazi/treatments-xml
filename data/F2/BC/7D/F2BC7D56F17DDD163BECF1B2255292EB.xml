<taxonomicName class="Insecta" family="Curculionidae" genus="Coryssomerus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coryssomerus" order="Coleoptera" pageId="258" pageNumber="259" phylum="Arthropoda" rank="genus">Coryssomerus</taxonomicName>
