<taxonomicName class="Insecta" family="Curculionidae" genus="Metialma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Metialma" order="Coleoptera" pageId="258" pageNumber="259" phylum="Arthropoda" rank="genus">Metialma</taxonomicName>
