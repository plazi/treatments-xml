<subSubSection pageId="786" pageNumber="787" type="nomenclature">
<paragraph pageId="786" pageNumber="787">
Subgenus 
<taxonomicName LSID="F25DFB4E-E689-5D9F-9195-C4B03DDB7090" authority="Liebke, 1938" authorityName="Liebke" authorityYear="1938" class="Insecta" family="Carabidae" genus="Cosnania" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="(Mimocasnonia)" order="Coleoptera" pageId="786" pageNumber="787" phylum="Arthropoda" rank="subGenus" subGenus="Mimocasnonia">Mimocasnonia Liebke, 1938</taxonomicName>
