<taxonomicName class="Magnoliopsida" family="Fagaceae" genus="S." kingdom="Plantae" lsidName="S. wugongshanensis" order="Fagales" pageId="0" pageNumber="27" phylum="Tracheophyta" rank="species" species="wugongshanensis">
