<collectingRegion box="[843,947,539,561]" country="China" name="Shandong" pageId="18" pageNumber="1273">Shandong</collectingRegion>
, 
<collectingRegion box="[960,1036,539,561]" country="China" name="Jiangxi" pageId="18" pageNumber="1273">Jiangxi</collectingRegion>
, and 
<collectingRegion box="[178,243,568,590]" country="China" name="Fujian" pageId="18" pageNumber="1273">Fujian</collectingRegion>
