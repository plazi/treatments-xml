</taxonomicName>
in 
<collectingCountry box="[1037,1109,1104,1130]" name="Brazil" pageId="14" pageNumber="49">Brazil</collectingCountry>
(
<bibRefCitation author="Yamaguti, S." box="[1124,1307,1104,1130]" pageId="14" pageNumber="49" refId="ref15787" refString="Yamaguti, S. (1971) Synopsis of Digenetic Trematodes of Vertebrates, Keigaku Publishing Company, Tokyo, Japan, 1,074 + 349 pp." type="book" year="1971">Yamaguti 1971</bibRefCitation>
