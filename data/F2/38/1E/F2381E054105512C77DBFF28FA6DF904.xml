<taxonomicName authority="(Hagen)" baseAuthorityName="Hagen" box="[1070,1440,1313,1338]" class="Insecta" family="Libellulidae" genus="Erythemis" kingdom="Animalia" order="Odonata" pageId="9" pageNumber="8" phylum="Arthropoda" rank="species" species="credula">
