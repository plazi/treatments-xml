<taxonomicName authority="(Rambur)" baseAuthorityName="Rambur" baseAuthorityYear="1842" box="[1010,1440,673,698]" class="Insecta" family="Libellulidae" genus="Erythemis" kingdom="Animalia" order="Odonata" pageId="9" pageNumber="8" phylum="Arthropoda" rank="species" species="peruviana">
