<taxonomicName authority="Faust, 1895" authorityName="Faust" authorityYear="1895" box="[454,794,1540,1562]" class="Insecta" family="Curculionidae" genus="Dermatodina" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="326" phylum="Arthropoda" rank="species" species="vitiosa">
