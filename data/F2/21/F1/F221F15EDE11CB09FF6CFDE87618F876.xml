<paragraph blockId="1.[151,431,530,556]" box="[151,431,530,556]" pageId="1" pageNumber="48">
<heading bold="true" box="[151,431,530,556]" fontSize="11" level="1" pageId="1" pageNumber="48" reason="1">
<emphasis bold="true" box="[151,431,530,556]" pageId="1" pageNumber="48">
<taxonomicName box="[151,342,530,556]" class="Insecta" family="Reduviidae" genus="Enigmocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="48" phylum="Arthropoda" rank="genus" status="gen. n.">
<emphasis bold="true" box="[151,342,530,556]" italics="true" pageId="1" pageNumber="48">Enigmocephala</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[348,431,530,556]" pageId="1" pageNumber="48" rank="genus">gen. n.</taxonomicNameLabel>
</emphasis>
