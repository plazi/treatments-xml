and 
<taxonomicName authorityName="Oldroyd" authorityYear="1956" box="[470,614,925,949]" class="Insecta" family="Dolichopodidae" genus="Thambemyia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="259" phylum="Arthropoda" rank="genus">
