<emphasis id="95C9F02FFF80FFD5FF40FD021E3BFD5A" bold="true" box="[199,352,691,716]" pageId="9" pageNumber="10">Distribution.</emphasis>
Eastern 
<collectingCountry id="DFAA6CADFF80FFD5FE45FD041D00FD5A" box="[450,603,692,716]" name="New Zealand" pageId="9" pageNumber="10">New Zealand</collectingCountry>
, 
<specimenCount id="B1BBE7B4FF80FFD5FDE1FD051DEFFD5A" box="[614,692,692,716]" pageId="9" pageNumber="10" type="generic">
<quantity id="604581D8FF80FFD5FDE1FD051DEBFD5A" box="[614,688,692,716]" metricMagnitude="2" metricUnit="m" metricValue="3.06" pageId="9" pageNumber="10" unit="m" value="306.0">306 m</quantity>
