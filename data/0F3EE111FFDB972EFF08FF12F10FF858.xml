<caption id="D3E8008FFFDB9729FF08FBD1F1C3FBE9" ID-DOI="http://doi.org/10.5281/zenodo.5057487" ID-Zenodo-Dep="5057487" box="[151,1167,1108,1132]" httpUri="https://zenodo.org/record/5057487/files/figure.png" pageId="11" pageNumber="534" startId="11.[151,250,1108,1132]" targetBox="[162,1414,564,1068]" targetPageId="11">
