<caption id="DF796CFAFFC4FFC4FF4EF8C9FB34EBE1" httpUri="https://zenodo.org/record/262314/files/figure.png" pageId="53" pageNumber="54" targetBox="[151,1434,193,1856]" targetPageId="53">
