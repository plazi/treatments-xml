<taxonomicName id="A4CDD3ABE22AA358871AFDABFE6F099B" box="[293,483,978,999]" class="Actinopterygii" family="Bythitidae" genus="Saccogaster" kingdom="Animalia" order="Ophidiiformes" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="parva">
