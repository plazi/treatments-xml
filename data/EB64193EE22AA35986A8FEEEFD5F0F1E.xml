<taxonomicName id="A4CDD3ABE22AA358870EF9DAFE780DC4" box="[305,500,1955,1976]" class="Actinopterygii" family="Bythitidae" genus="Saccogaster" kingdom="Animalia" order="Ophidiiformes" pageId="18" pageNumber="19" phylum="Chordata" rank="species" species="parva">
