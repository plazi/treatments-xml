<emphasis id="D9F34038FFD9FFCDFE74FED0D5B3FEDF" box="[432,445,346,371]" italics="true" pageId="4" pageNumber="5">e</emphasis>
<emphasis id="D9F34038FFD9FFCDFE79FEE0D5CBFED4" box="[445,453,362,376]" italics="true" pageId="4" pageNumber="5">
<number id="5D4F8E0DFFD9FFCDFE79FEE0D5CBFED4" box="[445,453,362,376]" pageId="4" pageNumber="5" value="2.0">2</number>
</emphasis>
, 
<emphasis id="D9F34038FFD9FFCDFE10FED0D5EDFEDF" box="[468,483,346,371]" italics="true" pageId="4" pageNumber="5">h</emphasis>
<emphasis id="D9F34038FFD9FFCDFE27FEE0D5E5FED4" box="[483,491,362,376]" italics="true" pageId="4" pageNumber="5">
<number id="5D4F8E0DFFD9FFCDFE27FEE0D5E5FED4" box="[483,491,362,376]" pageId="4" pageNumber="5" value="2.0">2</number>
