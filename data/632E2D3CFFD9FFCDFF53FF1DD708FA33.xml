<typeStatus id="343C2288FFD9FFCDFF02FBEED503FBD2" box="[198,269,1124,1150]" pageId="4" pageNumber="5">Types</typeStatus>
.
</emphasis>
<typeStatus id="343C2288FFD9FFCDFED9FBEED584FBD2" box="[285,394,1124,1150]" pageId="4" pageNumber="5">Holotype</typeStatus>
and 
<number id="5D4F8E0DFFD9FFCDFE07FBEED5EEFBD2" box="[451,480,1124,1150]" pageId="4" pageNumber="5" value="33.0">33</number>
<typeStatus id="343C2288FFD9FFCDFE2CFBEED653FBD2" box="[488,605,1124,1150]" pageId="4" pageNumber="5">paratypes</typeStatus>
: BO- 
<number id="5D4F8E0DFFD9FFCDFD59FBEED6A2FBD2" box="[669,684,1124,1150]" pageId="4" pageNumber="5" value="2.0">2</number>
