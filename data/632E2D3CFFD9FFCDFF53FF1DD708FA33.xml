<taxonomicName id="2C87E7A9FFD9FFCDFF3AFB55D6E8FB55" authority="Niedbala, 1982" authorityName="Niedbala" authorityYear="1982" box="[254,742,1247,1273]" class="Arachnida" family="Phthiracaridae" genus="Plonaphacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="eximius">
