<emphasis id="D9F34038FFD9FFCDFD09FB3ED6D5FB61" box="[717,731,1204,1229]" italics="true" pageId="4" pageNumber="5">h</emphasis>
<emphasis id="D9F34038FFD9FFCDFD1FFB4ED6EDFB7E" box="[731,739,1220,1234]" italics="true" pageId="4" pageNumber="5">
<number id="5D4F8E0DFFD9FFCDFD1FFB4ED6EDFB7E" box="[731,739,1220,1234]" pageId="4" pageNumber="5" value="2.0">2</number>
</emphasis>
