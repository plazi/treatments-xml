<taxonomicName id="4C0D6BF6FFC4E364B87346D59DCF4B82" authority="Chemnitz" authorityName="Chemnitz" box="[827,1181,1412,1438]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="2" pageNumber="3" phylum="Mollusca" rank="species" species="labyrinthus">
