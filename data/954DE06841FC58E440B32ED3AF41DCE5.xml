4. 
<taxonomicName id="C0FF81893F7DEB576F280F25788D4B92" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:143480" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" lsidName-HNS="Polyrhachis sculpturatus Smith" order="Hymenoptera" pageId="3" pageNumber="70" phylum="Arthropoda" rank="species" species="sculpturatus">Polyrhachis sculpturatus</taxonomicName>
