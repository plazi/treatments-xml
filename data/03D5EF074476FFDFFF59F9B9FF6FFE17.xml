<collectionCode id="ED6DC6D44470FFDFFB68FF7FFB64FEE2" LSID="urn:lsid:biocol.org:col:33074" box="[1190,1256,232,258]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33074" name="China Agricultural University" pageId="5" pageNumber="30">CAU</collectionCode>
