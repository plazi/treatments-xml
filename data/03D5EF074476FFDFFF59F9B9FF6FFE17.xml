<typeStatus id="54C7E0B34470FFDFFF08FF28FE8DFF39" box="[198,257,191,217]" pageId="5" pageNumber="30">Type</typeStatus>
material.
</emphasis>
<typeStatus id="54C7E0B34470FFDFFE42FF57FE75FF3A" box="[396,505,192,218]" pageId="5" pageNumber="30">Holotype</typeStatus>
male, Guangdong: Nankunshan National Park, 
<date id="FFC278D14470FFDFFB8AFF57FB5BFF3A" box="[1092,1239,192,218]" pageId="5" pageNumber="30" value="2004-05-12">
<number id="3DB44C364470FFDFFB8AFF57FBE8FF3A" box="[1092,1124,192,218]" pageId="5" pageNumber="30" value="12.0">12</number>
