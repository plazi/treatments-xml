<taxonomicName id="4C7E3589FF8DFFABFCFFFFDE3518FB38" baseAuthorityName="Pereira and Martinez" baseAuthorityYear="1956" box="[887,1124,1200,1224]" class="Insecta" family="Scarabaeidae" genus="Canthon" kingdom="Animalia" order="Coleoptera" pageId="70" pageNumber="69" phylum="Arthropoda" rank="species" species="vulcanoae">
