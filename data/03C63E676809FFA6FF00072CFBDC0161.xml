<taxonomicName id="4C6FF4F2680AFFA6FDF7023FFC2A0242" authority="(REDTENBACHER 1843)" baseAuthorityName="REDTENBACHER" baseAuthorityYear="1843" box="[581,968,260,281]" class="Insecta" family="Carabidae" genus="Laemostenus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="1117" phylum="Arthropoda" rank="species" species="quadricollis" subGenus="Laemostenus">
