<bibRef id="8C61A228FFFCFF9F741FFBABBFB8FBE1" author="AMYOTC. J. B. &amp; AUDINET- SERVILLE J. G. A." bookContentInfo="675 p." journalOrPublisher="Paris: Roret" pageId="11" pageNumber="52" refId="ref7304" title="Histoire Naturelle des Insectes. Hemipteres" type="book" year="1843">
