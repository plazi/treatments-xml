<quantity id="03699B3BFFB8A3652CCCDE08BA12FC89" box="[226,344,839,862]" metricMagnitude="-2" metricUnit="m" metricValue="1.95" metricValueMax="2.0" metricValueMin="1.9" pageId="10" pageNumber="289" unit="mm" value="19.5" valueMax="20.0" valueMin="19.0">19–20 mm</quantity>
