<collectingCountry id="F37456A94923FFD8FE5AF991FDC4D904" box="[468,538,1573,1599]" name="China" pageId="4" pageNumber="51">China</collectingCountry>
(
<collectingRegion id="49A7D8DB4923FFD8FDA7F991FD66D904" box="[553,696,1573,1599]" country="China" name="Guangdong" pageId="4" pageNumber="51">Guangdong</collectingRegion>
