<taxonomicName id="B3944D4BB01A3E401653FED2EEEDB744" authority="SCHWARZ" authorityName="Schwarz" authorityYear="2015" box="[321,506,310,332]" class="Insecta" family="Ichneumonidae" genus="Cryptus" kingdom="Animalia" order="Hymenoptera" pageId="104" pageNumber="853" phylum="Arthropoda" rank="species" species="valesiacus">
<emphasis id="46E0EADAB01A3E401653FED2EE8CB744" box="[321,411,310,331]" italics="true" pageId="104" pageNumber="853">valesiacus</emphasis>
