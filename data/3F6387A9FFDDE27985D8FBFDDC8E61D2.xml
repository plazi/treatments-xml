<taxonomicName id="70CA4D3CFFDDE27987ADFB49DE75619A" box="[738,891,1178,1203]" class="Hydrozoa" family="Calycopsidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecata" pageId="48" pageNumber="49" phylum="Cnidaria" rank="family">Calycopsidae</taxonomicName>
