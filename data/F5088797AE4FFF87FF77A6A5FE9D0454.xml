<typeStatus id="A21A8823AE4FFF87FF77A6D7FF010398" box="[151,209,229,255]" pageId="3" pageNumber="32">Type</typeStatus>
species, 
<taxonomicName id="BAA14D02AE4FFF87FEDCA6D7FDB20399" authority="Woodley, 2008" authorityName="Woodley" authorityYear="2008" box="[316,610,229,254]" class="Insecta" family="Stratiomyidae" genus="Panamamyia" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="32" phylum="Arthropoda" rank="species" species="silbergliedi" status="sp. nov.">
