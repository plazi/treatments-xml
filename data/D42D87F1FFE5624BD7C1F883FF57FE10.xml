76. 
<taxonomicName id="9B844D64FFE5624CD797F883FCD6F850" authority="(Brunner von Wattenwyl, 1882)" authorityName="Brunner von Wattenwyl" authorityYear="1882" baseAuthorityName="Brunner von Wattenwyl" baseAuthorityYear="1882" box="[193,861,1911,1936]" class="Insecta" family="Tettigoniidae" genus="Zeuneriana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="amplipennis">
