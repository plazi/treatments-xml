20,65’E, 
<quantity id="4C898BB7D657FF9DFD95E520FDE31172" box="[515,613,1078,1103]" metricMagnitude="3" metricUnit="m" metricValue="1.353" pageId="11" pageNumber="208" unit="m" value="1353.0">1353 m</quantity>
, 
<date id="FFCF0092D657FF9DFFFBE541FEA4114C" box="[109,290,1111,1136]" pageId="11" pageNumber="208" value="2007-03-19" valueMax="2007-03-26" valueMin="2007-03-19">19-26.iii.2007</date>
, Patikhom Tumtip leg., 
<collectionCode id="ED60BE97D657FF9DFFFBE56EFF0611AD" box="[109,128,1144,1169]" name="Tavera, Department of Geology and Geophysics" pageId="11" pageNumber="208">T</collectionCode>
