<quantity id="AD49A8F1FFE45163FAABFC86499ACA22" box="[1359,1436,776,802]" metricMagnitude="2" metricUnit="m" metricValue="1.3" pageId="23" pageNumber="24" unit="m" value="130.0">130 m</quantity>
