<emphasis id="AFAA6E5A8127FE84EDACCF9E18B20FAC" bold="true" box="[199,495,439,464]" pageId="291" pageNumber="292">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA8127FE84EC9DCF911BD80FAC" box="[502,645,439,464]" country="Papua New Guinea" name="New Ireland" pageId="291" pageNumber="292">New Ireland</collectingRegion>
