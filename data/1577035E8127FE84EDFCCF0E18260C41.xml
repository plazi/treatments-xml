<collectingCountry id="E5C9F2D88127FE84EC19CFF518E30F88" box="[370,446,476,500]" name="Samoa" pageId="291" pageNumber="292">Samoa</collectingCountry>
and 
<collectingCountry id="E5C9F2D88127FE84EC9DCFF41B620F89" box="[502,575,477,501]" name="Tonga" pageId="291" pageNumber="292">Tonga</collectingCountry>
