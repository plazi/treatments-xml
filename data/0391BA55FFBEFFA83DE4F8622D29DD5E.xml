<taxonomicName id="4C3870C0FFB3FFA838FAFF1E2E9FDD5E" authorityName="Yin, Bao &amp; Zhang" authorityYear="1995" class="Arachnida" family="Lycosidae" genus="Lycosa" kingdom="Animalia" order="Araneae" pageId="24" pageNumber="40" phylum="Arthropoda" rank="species" species="vittata">
