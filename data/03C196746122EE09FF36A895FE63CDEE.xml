<emphasis id="B91CFB706122EE0FFF36AF01FEB4CB12" bold="true" box="[151,326,1795,1819]" pageId="2" pageNumber="66">FIGURES 1–3.</emphasis>
<taxonomicName id="4C685CE16122EE0FFEEFAF01FCA1CB13" authority="Ocampo and Colby" authorityName="Ocampo and Colby" box="[334,851,1795,1818]" class="Insecta" family="Hybosoridae" genus="Parallidiostoma" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="66" phylum="Arthropoda" rank="species" species="tricornum" status="new genus">
<emphasis id="B91CFB706122EE0FFEEFAF01FD87CB13" box="[334,629,1795,1818]" italics="true" pageId="2" pageNumber="66">Parallidiostoma tricornum</emphasis>
Ocampo and Colby
</taxonomicName>
