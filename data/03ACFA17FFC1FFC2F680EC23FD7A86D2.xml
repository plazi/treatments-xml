<taxonomicName id="4C053082FFC1FFC2F4B3EE2BFC438636" authority="De Geer, 1773" authorityName="De Geer" authorityYear="1773" box="[676,941,1889,1913]" class="Insecta" family="Ectobiidae" genus="Ischnoptera" kingdom="Animalia" order="Blattodea" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="rufa">
