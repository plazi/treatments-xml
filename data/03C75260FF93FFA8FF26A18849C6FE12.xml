. Besides 
<typeStatus id="54D55DD4FF90FFA8FE2CA05D4B24FEFF" box="[413,514,332,358]" pageId="10" pageNumber="491">holotype</typeStatus>
, 
<typeStatus id="54D55DD4FF90FFA8FDBCA05C4B4BFEFE" box="[525,621,333,359]" pageId="10" pageNumber="491">paratype</typeStatus>
<collectionCode id="ED7F7BB3FF90FFA8FDC4A05C4B83FEFE" box="[629,677,333,359]" country="Russia" httpUri="http://grbio.org/cool/kjjc-ziy5" name="Paleontological Institute, Russian Academy of Sciences" pageId="10" pageNumber="491">PIN</collectionCode>
