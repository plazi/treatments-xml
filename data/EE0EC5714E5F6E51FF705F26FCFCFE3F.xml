<taxonomicName id="A1A70FE44E586E51FD625860FCF1FEEC" authorityName=", Av. Andre Araujo" authorityYear="2936" box="[645,796,256,279]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="139" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
