<taxonomicName id="4C357092FFE8FFFAA8F7FAD95CEAFAF3" authorityName="Antonietto &amp; Carmo &amp; Viviers &amp; Neto &amp; Hunt" authorityYear="2016" baseAuthorityName="Rosenfeld &amp; Raab" baseAuthorityYear="1983" box="[983,1203,1328,1354]" class="Ostracoda" family="Krithidae" genus="Aracajuia" kingdom="Animalia" order="Podocopida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="antiqua">
