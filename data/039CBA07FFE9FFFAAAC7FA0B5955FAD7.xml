<taxonomicName id="4C357092FFE8FFFAAA8BFAC65AC0FAF3" box="[427,665,1327,1353]" class="Ostracoda" family="Trachyleberididae" genus="Sergipella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="viviersae">
<emphasis id="B941D703FFE8FFFAAA8BFAC65AC0FAF3" box="[427,665,1327,1353]" italics="true" pageId="19" pageNumber="20">Sergipella viviersae</emphasis>
</taxonomicName>
