<taxonomicName id="DCE64D4E39624E78AFAA81632E54FA18" box="[1002,1067,1396,1420]" class="Insecta" family="Formicidae" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="55" phylum="Arthropoda" rank="tribe" tribe="Attini">Attini</taxonomicName>
(
<bibRefCitation id="7F774B3C39624E78A87D81632E9BFA18" author="Wheeler" box="[1085,1252,1396,1420]" pageId="20" pageNumber="55" refString="Wheeler, W. M. (1925) A new guest-ant and other new Formicidae from Barro Colorado Island, Panama. Biological Bulletin (Woods Hole), 49, 150 - 181." type="journal article" year="1925">
Wheeler 
<number id="AD2E24EA39624E78A8E981632E9BFA18" box="[1193,1252,1396,1420]" pageId="20" pageNumber="55" value="1925.0">1925</number>
</bibRefCitation>
, 
<bibRefCitation id="7F774B3C39624E78A8B381632FE7FA18" author="Brandao" box="[1267,1432,1396,1420]" pageId="20" pageNumber="55" refString="Brandao, C. R. F. (1990) Systematic revision of the Neotropical ant genus Megalomyrmex Forel (Hymenoptera: Formicidae: Myrmicinae), with the description of thirteen new species. Arquivos de Zoologia, Sao Paulo, 31, 411 - 481." type="journal article" year="1990">
Brand√£o 
<number id="AD2E24EA39624E78A91D81632FE7FA18" box="[1373,1432,1396,1420]" pageId="20" pageNumber="55" value="1990.0">1990</number>
</bibRefCitation>
