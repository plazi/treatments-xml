<paragraph id="A35036B69E40FF88FFC976AFFD93150F" blockId="68.[93,620,693,1450]" pageId="68" pageNumber="68">
<typeStatus id="7C5488149E40FF88FFC976AFFF131486" box="[122,229,693,717]" pageId="68" pageNumber="68">SYNTYPE</typeStatus>
