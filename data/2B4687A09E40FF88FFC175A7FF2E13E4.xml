<geoCoordinate id="C6DB50719E40FF88FFEE76EBFF5B154B" box="[93,173,753,774]" degrees="11.17" direction="south" orientation="latitude" pageId="68" pageNumber="68" precision="555" value="-11.17">11.17S</geoCoordinate>
