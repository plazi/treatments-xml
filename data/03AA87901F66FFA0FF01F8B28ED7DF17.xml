<taxonomicName id="4C034D051F67FFA6FED4F8E98F03DCAC" box="[322,560,1900,1922]" class="Insecta" family="Ptiloneuridae" genus="Brasineura" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Psocodea" pageId="3" pageNumber="171" phylum="Arthropoda" rank="species" species="diamantina" status="n. sp.">
<emphasis id="B977EA941F67FFA6FED4F8E98F03DCAC" box="[322,560,1900,1922]" italics="true" pageId="3" pageNumber="171">Brasineura diamantina</emphasis>
