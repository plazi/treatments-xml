<collectionCode id="61F6BFC4414EDF08DEC74880FF3A4580" box="[166,184,1236,1262]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="7" pageNumber="45" type="Herbarium">P</collectionCode>
