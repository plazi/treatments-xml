<specimenCode id="57418F7A414EDF08DC194887FC8B4580" box="[632,777,1235,1262]" collectionCode="PE" country="China" lsid="urn:lsid:biocol.org:col:15858" name="Institute of Botany, Chinese Academy of Sciences" pageId="7" pageNumber="45" type="Herbarium">PE00022555</specimenCode>
