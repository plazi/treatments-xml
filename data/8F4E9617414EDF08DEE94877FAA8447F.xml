<collectionCode id="61F6BFC4414EDF08DAA34880FB5A4580" box="[1218,1240,1236,1262]" country="Denmark" name="University of Copenhagen" pageId="7" pageNumber="45" type="Herbarium">C</collectionCode>
. 
