<quantity id="E6799B1DFFE9706AFC4FFAC559A3744D" box="[911,974,1358,1378]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="68" pageNumber="1347" unit="m" value="2500.0">2500m</quantity>
, 
<date id="553F1038FFE9706AFC1DFAC55E57744D" box="[989,1082,1358,1378]" pageId="68" pageNumber="1347" value="1984-10-28">28.X.1984</date>
