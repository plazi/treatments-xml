<paragraph id="6323C852A7F29F25A30DFDD8978E2EE2" pageNumber="356">
<taxonomicName id="50A2F0FAF56F4A1CCFBAB409D7FAFB04" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:35535" class="Insecta" family="Formicidae" genus="Probolomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Probolomyrmex dammermani Wheeler" lsidName-HNS="Probolomyrmex dammermani Wheeler" order="Hymenoptera" phylum="Arthropoda" rank="species" species="dammermani">Probolomyrmex dammermani Wheeler</taxonomicName>
