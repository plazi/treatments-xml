<taxonomicName id="B20A15D5F9939B802DD8F2E3034ACF83" class="Magnoliopsida" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Castanopsis" order="Fagales" pageId="12" pageNumber="115" phylum="Tracheophyta" rank="genus">Castanopsis</taxonomicName>
