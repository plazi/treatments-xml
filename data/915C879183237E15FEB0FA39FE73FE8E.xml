<taxonomicName id="DEF54D04832C7E15FDCFFE99FCD4FEE5" box="[631,872,284,309]" class="Insecta" family="Tingidae" genus="Xenotingis" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="61" phylum="Arthropoda" rank="species" species="papuana">
