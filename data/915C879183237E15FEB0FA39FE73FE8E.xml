<collectingCountry id="61E2761783237E1AFB25F89AFB4AF8E9" box="[1181,1270,1823,1849]" name="Taiwan" pageId="23" pageNumber="60">Taiwan</collectingCountry>
, the 
<collectingCountry id="61E2761783237E1AFEB0F8C2FE33F8B1" box="[264,399,1863,1889]" name="Philippines" pageId="23" pageNumber="60">Philippines</collectingCountry>
, and New 
<collectingCountry id="61E2761783237E1AFDB5F8C2FDD9F8B1" box="[525,613,1863,1889]" name="Guinea-Bissau" pageId="23" pageNumber="60">Guinea</collectingCountry>
, with four species being restricted to New 
<collectingCountry id="61E2761783237E1AFBE6F8C2FB0FF8B1" box="[1118,1203,1863,1889]" name="Guinea-Bissau" pageId="23" pageNumber="60">Guinea</collectingCountry>
(
<bibRefCitation id="7D644B7683237E15FB7AF8C2FE19FEE6" author="Drake" lastPageId="24" lastPageNumber="61" pageId="23" pageNumber="68" refString="Drake, C. J. &amp; Ruhoff, F. A. (1965) Lacebugs from New Guinea, Borneo, Solomons, and other islands of the South Pacific and Indian oceans (Hemiptera: Tingidae). Pacific Insects, 7, 243 - 290." type="journal article" year="1965">
Drake &amp; Ruhoff 
<number id="AF3D24A0832C7E15FEDFFE99FE19FEE6" box="[359,421,284,310]" pageId="24" pageNumber="61" value="1965.0">1965</number>
</bibRefCitation>
; 
<bibRefCitation id="7D644B76832C7E15FE0FFE99FDE3FEE6" author="Pericart" box="[439,607,284,310]" pageId="24" pageNumber="68" refString="Pericart, J. (2000) Tingidae nouveaux ou interessants collectes en Papouasie-Nouvelle-Guinee (Heteroptera). Belgian Journal of Entomology, 2, 205 - 225." type="journal article" year="2000">
