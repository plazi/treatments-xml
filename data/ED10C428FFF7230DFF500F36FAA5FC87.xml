<subSubSection id="2DA326B5FFF7230DFF500FA1FAD1FD33" box="[151,1285,721,748]" pageId="5" pageNumber="326" type="materials_examined">
<paragraph id="6506753EFFF7230DFF500FA1FAD1FD33" blockId="5.[151,1437,721,856]" box="[151,1285,721,748]" pageId="5" pageNumber="326">
<emphasis id="57CDA92CFFF7230DFF500FA1FDE5FD34" bold="true" box="[151,561,721,747]" pageId="5" pageNumber="326">Material examined. TAN 0602/420</emphasis>
, two stems up to 
<quantity id="A241D8DBFFF7230DFD310FA1FC91FD33" box="[758,837,721,748]" metricMagnitude="-2" metricUnit="m" metricValue="1.0" pageId="5" pageNumber="326" unit="mm" value="10.0">10 mm</quantity>
high, on 
