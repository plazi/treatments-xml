<paragraph id="300714B2D3E763CCE6DE2D84292993A0" pageId="10" pageNumber="97">
<taxonomicName id="94CE4FDC2664AB381C1961D9AC04690F" authority="Dejean, 1833" authorityName="Dejean" authorityYear="1833" class="Insecta" family="Gyrinidae" genus="Orectochilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Orectochilus regimbarti" order="Coleoptera" pageId="10" pageNumber="97" phylum="Arthropoda" rank="species" species="regimbarti">Orectochilus regimbarti</taxonomicName>
