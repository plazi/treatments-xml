<emphasis id="27CC2459743AFFE2FF3CF94E6724D290" bold="true" box="[151,341,1689,1713]" pageId="5" pageNumber="38">FIGURES 7–12.</emphasis>
<taxonomicName id="D2B883C8743AFFE2FEF6F94D644BD290" authority="Rheims, 2010" authorityName="Rheims" authorityYear="2010" box="[349,570,1690,1713]" class="Arachnida" family="Sparassidae" genus="Guadana" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="38" phylum="Arthropoda" rank="species" species="manauara" status="sp. nov.">
<emphasis id="27CC2459743AFFE2FEF6F94D644BD290" box="[349,570,1690,1713]" italics="true" pageId="5" pageNumber="38">Guadana manauara</emphasis>
</taxonomicName>
