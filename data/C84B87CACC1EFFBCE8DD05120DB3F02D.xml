</taxonomicName>
, 
<taxonomicName id="87E24D5FCC1EFFBDE9D907850D19F28A" box="[403,518,1986,2009]" class="Hydrozoa" family="Halopterididae" genus="Schizotricha" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="turqueti">
