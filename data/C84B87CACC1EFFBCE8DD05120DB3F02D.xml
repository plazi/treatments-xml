<taxonomicName id="87E24D5FCC1EFFBDEA7707860C14F289" box="[573,779,1985,2010]" class="Hydrozoa" family="Sertulariidae" genus="Staurotheca" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="9" phylum="Cnidaria" rank="species" species="densa">
