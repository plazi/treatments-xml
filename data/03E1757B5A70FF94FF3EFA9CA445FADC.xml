Oriental: 
<collectingCountry id="F35F84FD5A73FF94FE7BFB59A47BFB44" box="[466,524,1183,1209]" name="India" pageId="39" pageNumber="40">India</collectingCountry>
(
<collectingRegion id="498C0A8F5A73FF94FDB3FB59A41CFB47" box="[538,619,1183,1210]" country="India" name="Kerala" pageId="39" pageNumber="40">Kerala</collectingRegion>
).
