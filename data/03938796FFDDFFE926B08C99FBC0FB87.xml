<taxonomicName id="4C3A4D03FFDDFFED27BA8CE2FDEFFA77" authority="Kennedy, 1925: 295" authorityName="Kennedy" authorityPageNumber="295" authorityYear="1925" box="[159,548,1494,1518]" class="Insecta" family="Megapodagrionidae" genus="Celebargiolestes" kingdom="Animalia" order="Odonata" pageId="3" pageNumber="238" phylum="Arthropoda" rank="genus">
