<collectingCountry box="[1251,1318,439,463]" name="China" pageId="9" pageNumber="55">China</collectingCountry>
(
<number box="[1334,1364,439,463]" pageId="9" pageNumber="55" value="42.0">42</number>
° 
<number box="[1373,1402,439,463]" pageId="9" pageNumber="55" value="24.0">24</number>
ʹN, 
<number box="[151,193,476,500]" pageId="9" pageNumber="55" value="128.0">128</number>
°06ʹE): 
<quantity box="[282,346,476,500]" metricMagnitude="2" metricUnit="m" metricValue="7.6" pageId="9" pageNumber="55" unit="m" value="760.0">760m</quantity>
a.s.l., annual mean temperature 
<number box="[726,761,476,500]" pageId="9" pageNumber="55" value="2.6">2.6</number>
