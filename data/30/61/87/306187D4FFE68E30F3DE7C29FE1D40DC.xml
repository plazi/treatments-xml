<quantity box="[1124,1223,476,500]" metricMagnitude="-1" metricUnit="m" metricValue="6.909999999999999" pageId="9" pageNumber="55" unit="mm" value="691.0">691 mm</quantity>
; tree species were dominated by a mixture of coniferous 
<taxonomicName box="[597,785,512,536]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="9" pageNumber="55" phylum="Tracheophyta" rank="species" species="koraiensis">
<emphasis box="[597,785,512,536]" italics="true" pageId="9" pageNumber="55">Pinus koraiensis</emphasis>
</taxonomicName>
, broadleaved 
<taxonomicName box="[949,1043,512,535]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="9" pageNumber="55" phylum="Tracheophyta" rank="genus">
<emphasis box="[949,1043,512,535]" italics="true" pageId="9" pageNumber="55">Quercus</emphasis>
</taxonomicName>
spp. and 
<taxonomicName box="[1158,1211,512,536]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="9" pageNumber="55" phylum="Tracheophyta" rank="genus">
<emphasis box="[1158,1211,512,536]" italics="true" pageId="9" pageNumber="55">Acer</emphasis>
</taxonomicName>
spp. on Albi-Boric Argosols.
</paragraph>
