<taxonomicName class="Insecta" family="Vespidae" genus="Vespa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Vespa argentata" order="Hymenoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="argentata">Vespa argentata</taxonomicName>
