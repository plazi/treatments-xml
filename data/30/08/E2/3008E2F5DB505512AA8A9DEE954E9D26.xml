<subSubSection pageId="0" pageNumber="119" type="reference_group">
<paragraph pageId="0" pageNumber="119">
<taxonomicName authorityName="van der Vecht" authorityYear="1963" class="Insecta" family="Vespidae" genus="Allorhynchium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Allorhynchium" order="Hymenoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="genus">Allorhynchium</taxonomicName>
