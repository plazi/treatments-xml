<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:238387" authority="Deans &amp; Kawada, 2008" authorityName="Deans &amp; Kawada" authorityYear="2008" class="Insecta" family="Evaniidae" genus="Alobevania" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Alobevania tavaresi" order="Hymenoptera" pageNumber="5" phylum="Arthropoda" rank="species" species="tavaresi">A. tavaresi</taxonomicName>
