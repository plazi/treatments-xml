<taxonomicName authority="Dozsa-Farkas, 1970" authorityName="Dozsa-Farkas" authorityYear="1970" class="Clitellata" family="Enchytraeidae" genus="Fridericia" kingdom="Animalia" order="Enchytraeida" pageId="10" pageNumber="181" phylum="Annelida" rank="species" species="semisetosa">
<emphasis italics="true" pageId="10" pageNumber="181">F. semisetosa</emphasis>
Dózsa-Farkas, 
<number box="[458,517,1843,1867]" pageId="10" pageNumber="181" value="1970.0">1970</number>
</taxonomicName>
, 
<taxonomicName authority="Cech &amp; Dozsa-Farkas, 2005" authorityName="Cech &amp; Dozsa-Farkas" authorityYear="2005" box="[533,1010,1842,1867]" class="Clitellata" family="Enchytraeidae" genus="Fridericia" kingdom="Animalia" order="Enchytraeida" pageId="10" pageNumber="181" phylum="Annelida" rank="species" species="schmelzi">
<emphasis box="[533,662,1843,1867]" italics="true" pageId="10" pageNumber="181">F. schmelzi</emphasis>
Cech &amp; Dózsa-Farkas, 
<number box="[952,1010,1842,1867]" pageId="10" pageNumber="181" value="2005.0">2005</number>
</taxonomicName>
, 
