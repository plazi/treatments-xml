<collectingCountry box="[186,257,1735,1758]" name="South Korea" pageId="10" pageNumber="181">Korea</collectingCountry>
, 
<number box="[268,297,1734,1759]" pageId="10" pageNumber="181" value="36.0">36</number>
˚06’ 
