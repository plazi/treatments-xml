<taxonomicName authority="Deichmann, 1930" authorityName="Deichmann" authorityYear="1930" box="[288,711,1689,1712]" class="Holothuroidea" family="Cucumariidae" genus="Euthyonacta" kingdom="Animalia" order="Dendrochirotida" pageId="13" pageNumber="14" phylum="Echinodermata" rank="species" species="solida">
