<subSubSection box="[151,481,1035,1062]" pageId="17" pageNumber="18" type="nomenclature">
<paragraph blockId="17.[151,481,1035,1062]" box="[151,481,1035,1062]" pageId="17" pageNumber="18">
<heading bold="true" box="[151,481,1035,1062]" fontSize="11" level="1" pageId="17" pageNumber="18" reason="1">
<emphasis bold="true" box="[151,481,1035,1062]" pageId="17" pageNumber="18">
<taxonomicName box="[151,337,1036,1062]" class="Holothuroidea" family="Cucumariidae" genus="Paracolochirus" kingdom="Animalia" order="Dendrochirotida" pageId="17" pageNumber="18" phylum="Echinodermata" rank="genus" status="new genus">
<emphasis bold="true" box="[151,337,1036,1062]" italics="true" pageId="17" pageNumber="18">Paracolochirus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[351,481,1035,1061]" pageId="17" pageNumber="18" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
