, 
<taxonomicName genus="P." lsidName="P. megacalyx" pageId="0" pageNumber="129" rank="species" species="megacalyx">
<emphasis italics="true" pageId="0" pageNumber="129">P. megacalyx</emphasis>
</taxonomicName>
