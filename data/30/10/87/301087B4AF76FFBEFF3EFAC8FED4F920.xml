<typeStatus box="[526,631,1516,1542]" pageId="6" pageNumber="5">lectotype</typeStatus>
. 
<location LSID="urn:lsid:plazi:treatment:301087B4AF76FFBEFF3EFAC8FED4F920:BD666079AF76FFBEFD7FFA72FD7AF9AA" box="[641,683,1516,1542]" county="We" municipality="Volcan" name="The" pageId="6" pageNumber="5" stateProvince="Chiriqui">The</location>
