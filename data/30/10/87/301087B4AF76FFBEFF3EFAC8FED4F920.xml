<collectingMunicipality box="[637,714,1482,1508]" pageId="6" pageNumber="5">Volcan</collectingMunicipality>
de 
<collectingRegion box="[761,857,1482,1508]" country="Panama" name="Chiriqui" pageId="6" pageNumber="5">Chiriqui</collectingRegion>
