</taxonomicName>
species: 
<taxonomicName authority="Kott, 1957" authorityName="Kott" authorityYear="1957" box="[514,764,788,812]" class="Ascidiacea" family="Ritterellidae" genus="Ritterella" kingdom="Animalia" order="Enterogona" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="dispar">
<emphasis box="[514,618,788,812]" italics="true" pageId="19" pageNumber="20">R. dispar</emphasis>
(
<bibRefCitation author="Kott" box="[633,756,788,812]" pageId="19" pageNumber="20" refString="Kott, P. (1957) The ascidians of Australia. II. Aplousobranchiata Lahille: Clavelinidae Forbes &amp; Hanley and Polyclinidae Verrill. Australian Journal of marine &amp; freshwater Research, 8 (1), 64 - 110." type="journal article" year="1957">
Kott, 
<number box="[699,756,788,812]" pageId="19" pageNumber="20" value="1957.0">1957</number>
</bibRefCitation>
)
</taxonomicName>
is also recorded from 
<collectingCountry box="[1019,1167,788,812]" name="Mozambique" pageId="19" pageNumber="20">Mozambique</collectingCountry>
and 
<collectingCountry box="[1222,1323,788,812]" name="Tanzania" pageId="19" pageNumber="20">Tanzania</collectingCountry>
