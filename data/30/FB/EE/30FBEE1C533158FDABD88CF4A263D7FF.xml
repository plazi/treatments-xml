<paragraph pageId="0" pageNumber="95">Type species.</paragraph>
<paragraph pageId="0" pageNumber="95">
<taxonomicName authority="Zamani &amp; Marusik, 2021" authorityName="Zamani &amp; Marusik" authorityYear="2021" class="Arachnida" family="Liocranidae" genus="Sestakovaia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Sestakovaia hyrcania" order="Araneae" pageId="0" pageNumber="95" phylum="Arthropoda" rank="species" species="hyrcania">
