<paragraph pageId="null" pageNumber="122">
<taxonomicName authority="(L.) Roth" class="Polypodiopsida" family="Athyriaceae" genus="Athyrium" kingdom="Plantae" order="Polypodiales" pageId="null" pageNumber="122" phylum="Tracheophyta" rank="species" species="filix-femina">
