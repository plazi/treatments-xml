<subSubSection pageId="0" pageNumber="119" type="etymology">
<paragraph pageId="0" pageNumber="119">Etymology.</paragraph>
<paragraph pageId="0" pageNumber="119">
<taxonomicName class="Insecta" family="Braconidae" genus="Zelomorpha" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zelomorpha danjohnsoni" order="Hymenoptera" pageId="0" pageNumber="119" phylum="Arthropoda" rank="species" species="danjohnsoni">
<emphasis italics="true" pageId="0" pageNumber="119">Zelomorpha danjohnsoni</emphasis>
</taxonomicName>
