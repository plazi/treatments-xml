<emphasis bold="true" box="[151,968,1268,1294]" pageId="4" pageNumber="105">
<taxonomicName box="[151,283,1268,1294]" class="Insecta" family="Nitidulidae" genus="Meligethes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[151,283,1268,1294]" italics="true" pageId="4" pageNumber="105">Meligethes</emphasis>
</taxonomicName>
(
<emphasis bold="true" box="[300,355,1268,1294]" italics="true" pageId="4" pageNumber="105">s.str.</emphasis>
) 
<taxonomicName authority="Audisio, Sabatelli &amp; Jelinek, 2015" authorityName="Audisio, Sabatelli &amp; Jelinek" authorityYear="2015" box="[372,968,1268,1294]" class="Insecta" family="Nitidulidae" genus="Meligethes" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="species" species="aureolineatus">
<emphasis bold="true" box="[372,538,1268,1294]" italics="true" pageId="4" pageNumber="105">aureolineatus</emphasis>
<bibRefCitation author="Audisio" box="[546,968,1268,1294]" pageId="4" pageNumber="105" refString="Audisio, P., Sabatelli, S. &amp; Jelinek, J. (2015) Revision of the pollen beetle genus Meligethes Stephens, 1830 (Coleoptera: Nitidulidae: Meligethinae). Fragmenta entomologica, 46 (2014), 19 - 112." type="journal article" year="2015">
Audisio, Sabatelli &amp; Jelínek, 
<number box="[909,968,1268,1294]" pageId="4" pageNumber="105" value="2015.0">2015</number>
</bibRefCitation>
</taxonomicName>
</emphasis>
