<collectionCode box="[692,783,392,418]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="98" pageNumber="99" type="Museum">USNM</collectionCode>
