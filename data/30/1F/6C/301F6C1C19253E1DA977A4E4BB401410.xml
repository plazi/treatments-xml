a and 
<taxonomicName authority="Muller, 1990" authorityName="Muller" authorityYear="1990" box="[264,589,1164,1190]" class="Malacostraca" family="Expanathuridae" genus="Eisothistos" kingdom="Animalia" order="Isopoda" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="societensis">
