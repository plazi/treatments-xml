<subSubSection box="[312,1186,284,310]" pageId="20" pageNumber="21" type="etymology">
<paragraph blockId="20.[264,1324,284,430]" box="[312,1186,284,310]" pageId="20" pageNumber="21">
<emphasis box="[312,437,284,309]" italics="true" pageId="20" pageNumber="21">Etymology</emphasis>
