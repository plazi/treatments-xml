<subSubSection box="[312,934,324,350]" pageId="45" pageNumber="46" type="distribution">
<paragraph blockId="45.[264,1323,284,550]" box="[312,934,324,350]" pageId="45" pageNumber="46">
<emphasis box="[312,455,324,349]" italics="true" pageId="45" pageNumber="46">Distribution</emphasis>
: Tasmania; algal community, 
<quantity box="[808,853,324,350]" metricMagnitude="0" metricUnit="m" metricValue="2.0" pageId="45" pageNumber="46" unit="m" value="2.0">2 m</quantity>
