<number box="[426,441,1753,1779]" pageId="1" pageNumber="2" value="5.0">5</number>
and 
<number box="[497,511,1753,1779]" pageId="1" pageNumber="2" value="6.0">6</number>
not longer than pleonites 
<number box="[815,861,1753,1779]" pageId="1" pageNumber="2" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
; pleonite 
<number box="[975,990,1753,1779]" pageId="1" pageNumber="2" value="6.0">6</number>
