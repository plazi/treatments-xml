; female 
<quantity id="FF5F9B43FF83FFB987CFFBCFFD05FBD4" box="[717,862,1072,1098]" metricMagnitude="-3" metricUnit="m" metricValue="4.25" metricValueMax="4.8" metricValueMin="3.7" pageId="10" pageNumber="11" unit="mm" value="4.25" valueMax="4.8" valueMin="3.7">3.7–4.8 mm</quantity>
