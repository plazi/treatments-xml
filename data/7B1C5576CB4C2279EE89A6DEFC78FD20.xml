<collectionCode id="95A47CA5CB4C2279EB59A743FA11FEE4" box="[1351,1437,295,321]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="19" pageNumber="520">MNHN</collectionCode>
