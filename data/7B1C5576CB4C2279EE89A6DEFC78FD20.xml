, 
<collectingRegion id="31712A82CB4C2279EC1AA70BFDA4FE2C" box="[516,552,367,393]" country="Cape Verde" name="Sal" pageId="19" pageNumber="520">Sal</collectingRegion>
<location id="F66AB2BBCB4C2279EC31A70BFDFFFE2C" LSID="urn:lsid:plazi:treatment:7B1C5576CB4C2279EE89A6DEFC78FD20:F66AB2BBCB4C2279EC31A70BFDFFFE2C" box="[559,627,367,393]" country="Cape Verde" county="Archipelago" municipality="Island" name="Island" pageId="19" pageNumber="520" stateProvince="Sal">Island</location>
