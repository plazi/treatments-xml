<number id="222324FEB009DF05FBBC5512FB2214B8" box="[1146,1177,874,898]" pageId="17" pageNumber="18" value="36.0">36</number>
</figureCitation>
); 
<number id="222324FEB009DF05FB695512FB7014B8" box="[1199,1227,874,898]" pageId="17" pageNumber="18" value="25.0">25</number>
