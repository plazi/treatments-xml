(
<bibRefCitation id="493A4B14FFE4FFEAFD81F924FD3CF91D" author="Ungemach" box="[536,729,1698,1723]" pageId="9" pageNumber="10" refString="Ungemach, H. (1932) Contribution a l'etude des Lepidopteres d'Abyssinie. Memoires de la Societe des Sciences Naturelles (et Physiques) du Maroc, 32, 1 - 122." type="journal article" year="1932">Ungemach, 1932</bibRefCitation>
