<subSubSection id="C373653C8438491AFF46F8CCAAF21B01" box="[151,693,1802,1827]" pageId="1" pageNumber="588" type="etymology">
<paragraph id="8BD636B78438491AFF46F8CCAAF21B01" blockId="1.[151,1436,1802,2007]" box="[151,693,1802,1827]" pageId="1" pageNumber="588">
<emphasis id="B91DEAA58438491AFF46F8CCA9511B01" bold="true" box="[151,278,1802,1827]" pageId="1" pageNumber="588">Etymology</emphasis>
. 
<taxonomicName id="4C694D348438491AFEF5F8CDA9C71B01" box="[292,384,1803,1827]" class="Insecta" family="Anthribidae" genus="Aurigeripilus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="592" phylum="Arthropoda" rank="species" species="andinus">
