<taxonomicName id="4C2C71FEFFDD9F37FF45FE72FDCAAC7C" authorityName="Roxb., Flora Indica" authorityYear="1820" box="[189,519,1846,1873]" class="Magnoliopsida" family="Oleaceae" genus="Jasminum" kingdom="Plantae" order="Lamiales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="chrysanthemum">
<emphasis id="B958D66FFFDD9F37FF45FE72FDCAAC7C" bold="true" box="[189,519,1846,1873]" italics="true" pageId="14" pageNumber="15">Jasminum chrysanthemum</emphasis>
</taxonomicName>
</heading>
</paragraph>
