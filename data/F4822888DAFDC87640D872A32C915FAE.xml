<taxonomicName id="C53EB1D0892E2253B73C0116BD42F6F2" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. muscorum" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="muscorum" subSpecies="agricolae">B. muscorum agricolae</taxonomicName>
