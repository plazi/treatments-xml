<taxonomicName id="B8BAEECA02116062933F166196EA8DDA" authority="" authorityName="" class="Centricae" family="Biddulphiaceae" genus="B." higherTaxonomySource="GBIF" kingdom="Chromista" lsidName="B. muscorum" order="Biddulphiales" pageId="0" pageNumber="8050" phylum="Diatomeae" rank="subSpecies" species="muscorum" subSpecies="scyllonius">B. muscorum scyllonius</taxonomicName>
