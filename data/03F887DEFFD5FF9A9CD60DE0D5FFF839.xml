</taxonomicName>
, 
<taxonomicName id="4C514D4BFFD5FF9A9EA80A3AD458FA5C" box="[745,865,1395,1418]" class="Phaeophyceae" family="Sargassaceae" genus="Cystoseira" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fucales" pageId="35" pageNumber="36" phylum="Heterokontophyta" rank="genus">
