<emphasis id="B925EADAFFD5FF9A9D090A3BD51CFA5D" box="[328,549,1394,1419]" italics="true" pageId="35" pageNumber="36">Posidonia oceanica</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C514D4BFFD5FF9A9E750A3BD5E2FA5D" box="[564,731,1394,1419]" class="Ulvophyceae" family="Halimedaceae" genus="Halimeda" higherTaxonomySource="GBIF" kingdom="Plantae" order="Bryopsidales" pageId="35" pageNumber="36" phylum="Chlorophyta" rank="species" species="tuna">
