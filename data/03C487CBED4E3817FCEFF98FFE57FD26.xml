<td id="768C5DE3ED73C5CAFF2DFF01FE8AFF2B" box="[232,335,207,234]" gridcol="1" gridrow="1" pageId="546" pageNumber="547">1-0</td>
<td id="768C5DE3ED73C5CAFE6DFF01FE3AFF2B" box="[424,511,207,234]" gridcol="2" gridrow="1" pageId="546" pageNumber="547">1-0; 5</td>
<td id="768C5DE3ED73C5CAFDADFF01FD08FF2B" box="[616,717,207,234]" gridcol="3" gridrow="1" pageId="546" pageNumber="547">0-0; 5</td>
</tr>
<tr id="355D349FED73C5CAFF4DFF3FFD08FECA" box="[136,717,241,267]" gridrow="2" pageId="546" pageNumber="547">
<th id="768C5DE3ED73C5CAFF4DFF3FFF0CFECA" box="[136,201,241,267]" gridcol="0" gridrow="2" pageId="546" pageNumber="547">Leg 2</th>
