<specimenCode id="DB923C0EBA4D9D24FEC5FED6FE4FFED5" box="[298,496,262,285]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="56" pageNumber="57">AMNH 585600</specimenCode>
