<taxonomicName id="59E304AABE1EFFAAFEB7FDB6FD55B7CF" authority="J.C. Fabricius, 1775" authorityName="J.C. Fabricius" authorityYear="1775" box="[299,658,618,642]" class="Malacostraca" family="Gammaridae" genus="Gammarus" kingdom="Animalia" order="Amphipoda" pageId="57" pageNumber="58" phylum="Arthropoda" rank="genus">
