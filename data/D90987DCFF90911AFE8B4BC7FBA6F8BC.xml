<taxonomicName id="96A04D49FF9F911AFD024CB4FC6DF934" authority="Huang, 2001" authorityName="Huang" authorityYear="2001" box="[641,958,1743,1769]" class="Arachnida" family="Eriophyidae" genus="Tetra" kingdom="Animalia" order="Prostigmata" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="toona">
