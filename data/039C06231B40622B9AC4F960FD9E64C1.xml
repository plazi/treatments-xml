<quantity id="4CCD1AD01B47622B98D0FE92FD366454" box="[643,726,264,290]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="4" pageNumber="60" unit="km" value="2.5">2.5 km</quantity>
