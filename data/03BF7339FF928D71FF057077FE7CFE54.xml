<typeStatus id="54AD7C8DFF8D8D71FE0974D5FD8BFF11" box="[411,515,151,176]" pageId="16" pageNumber="141">Holotype</typeStatus>
male microptera, 
<emphasis id="B9621E3DFF8D8D71FD5D74D5FCF6FF11" bold="true" box="[719,894,151,176]" pageId="16" pageNumber="141">
<collectingCountry id="F30182BFFF8D8D71FD5D74D5FCF6FF11" box="[719,894,151,176]" name="Norfolk Island" pageId="16" pageNumber="141">Norfolk Island</collectingCountry>
