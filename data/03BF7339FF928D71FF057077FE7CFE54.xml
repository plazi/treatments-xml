<taxonomicName id="4C16B9ACFF8D8D71FA8475F5FAE5FE71" authorityName="Mound &amp; Tree" authorityYear="2014" box="[1302,1389,439,464]" class="Insecta" family="Phlaeothripidae" genus="Holoengythrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="16" pageNumber="141" phylum="Arthropoda" rank="species" species="kathyae">
<emphasis id="B9621E3DFF8D8D71FA8475F5FAE5FE71" box="[1302,1389,439,464]" italics="true" pageId="16" pageNumber="141">kathyae</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C16B9ACFF8D8D71FF05759FFF64FE55" authorityName="Mound &amp; Tree" authorityYear="2014" box="[151,236,477,500]" class="Insecta" family="Phlaeothripidae" genus="Holoengythrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="16" pageNumber="141" phylum="Arthropoda" rank="species" species="turcoae">
<emphasis id="B9621E3DFF8D8D71FF05759FFF64FE55" box="[151,236,477,500]" italics="true" pageId="16" pageNumber="141">turcoae</emphasis>
</taxonomicName>
from eastern 
<collectingCountry id="F30182BFFF8D8D71FE14759EFE78FE54" box="[390,496,476,501]" name="Australia" pageId="16" pageNumber="141">Australia</collectingCountry>
.
</paragraph>
