<taxonomicNameLabel id="A4B857A3FFB7320BFAC022CEFA81EE9C" box="[1323,1388,1225,1250]" pageId="29" pageNumber="180" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4AFF4D49FFB7320BFA9622CDFED9EF78" class="Gastropoda" family="Mitridae" genus="Mitra" kingdom="Animalia" order="Neogastropoda" pageId="29" pageNumber="180" phylum="Mollusca" rank="species" species="columbellaria">
