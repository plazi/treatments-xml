, 
<collectorName id="80485373C0494B2BFCD14C9EFCC135F2" box="[784,886,459,485]" pageId="40" pageNumber="39">Gounelle</collectorName>
(
<collectionCode id="4BACAE60C0494B2BFC454C9EFC5735F2" box="[900,992,459,485]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="40" pageNumber="39">MNHN</collectionCode>
