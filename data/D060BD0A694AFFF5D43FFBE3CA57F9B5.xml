<collectionCode id="3ED894D9694AFFF5D457FA36C98BF99D" LSID="urn:lsid:biocol.org:col:34988" box="[352,445,1513,1539]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="32" pageNumber="33">MNHN</collectionCode>
