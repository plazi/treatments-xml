<taxonomicName id="4C074D12FF97FFDFFBD1F991E83AF9DB" box="[1072,1147,1552,1577]" class="Insecta" family="Formicidae" genus="Centromyrmex" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="raptor">
