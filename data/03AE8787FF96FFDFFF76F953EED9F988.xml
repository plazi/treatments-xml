<number id="3DCF24B6FF97FFDFFB98FCE9E8AEFC70" box="[1145,1263,872,898]" pageId="21" pageNumber="22" value="2.98" valueMax="3.08" valueMin="2.88">2.88–3.08</number>
(
<number id="3DCF24B6FF97FFDFFAE3FCE9E950FC70" box="[1282,1297,872,898]" pageId="21" pageNumber="22" value="5.0">5</number>
