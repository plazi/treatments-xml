<number id="3DCF24B6FF97FFDFFB01FB89E957FBD0" box="[1248,1302,1032,1058]" pageId="21" pageNumber="22" value="1.24">1.24</number>
, SI 
<number id="3DCF24B6FF97FFDFFAA2FB89E922FBD0" box="[1347,1379,1032,1058]" pageId="21" pageNumber="22" value="66.0">66</number>
, PW 
<number id="3DCF24B6FF97FFDFFF76FBB1EC8CFBB8" box="[151,205,1072,1098]" pageId="21" pageNumber="22" value="1.78">1.78</number>
