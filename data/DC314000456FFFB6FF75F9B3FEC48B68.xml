<paragraph id="5427F116456FFFB6FF58F959FEC48B68" blockId="22.[212,816,1728,1880]" pageId="22" pageNumber="53">
<taxonomicName id="93988A95456FFFB6FF58F959FE478AE8" baseAuthorityName="WOLLASTON" baseAuthorityYear="1864" box="[212,421,1728,1752]" class="Insecta" family="Staphylinidae" genus="Astenus" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="53" phylum="Arthropoda" rank="species" species="pallidulus">
