<taxonomicName id="E9BE4D22DC49FF96FF4AFAF3FE61FABC" authority="Gray, 1867" authorityName="Gray" authorityYear="1867" box="[238,473,1338,1364]" class="Demospongiae" family="Theneidae" genus="Thenea" kingdom="Animalia" order="Astrophorida" pageId="32" pageNumber="33" phylum="Porifera" rank="genus">
