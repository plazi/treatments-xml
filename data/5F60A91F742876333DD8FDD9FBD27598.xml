<taxonomicName id="10C9638A742876333944FD54FAE97395" authority="Klug, 1845" authorityName="Klug" authorityYear="1845" box="[1035,1403,642,665]" class="Insecta" family="Bolboceratidae" genus="Bolboceras" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="castaneum">
