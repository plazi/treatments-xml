<taxonomicName id="4C6CA1CF6756B52EC50748653D09227C" box="[1083,1299,788,813]" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="259" pageNumber="260" phylum="Tracheophyta" rank="species" species="corniculatus">
