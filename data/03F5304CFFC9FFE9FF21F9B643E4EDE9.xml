<subSubSection id="C346D2D1FFC9FFECFF21F8E34115EF1F" pageId="27" pageNumber="28" type="etymology">
<paragraph id="8BE3815AFFC9FFECFF21F8E34115EF1F" blockId="27.[151,1437,1801,2006]" pageId="27" pageNumber="28">
<emphasis id="B9285D48FFC9FFECFF21F8E34373EF57" bold="true" box="[151,278,1801,1826]" pageId="27" pageNumber="28">Etymology</emphasis>
