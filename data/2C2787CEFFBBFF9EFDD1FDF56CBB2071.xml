<quantity id="63769B3DFFBAFF9BFDB2FF476F832320" box="[533,640,346,373]" metricMagnitude="-2" metricUnit="m" metricValue="1.01" pageId="3" pageNumber="74" unit="mm" value="10.1">10.1 mm</quantity>
