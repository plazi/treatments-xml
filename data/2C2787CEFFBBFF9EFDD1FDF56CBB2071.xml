<quantity id="63769B3DFFBAFF9BFCF3FF476EB22320" box="[852,945,346,373]" metricMagnitude="-3" metricUnit="m" metricValue="7.9" pageId="3" pageNumber="74" unit="mm" value="7.9">7.9 mm</quantity>
