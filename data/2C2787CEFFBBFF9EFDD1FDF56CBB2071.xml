<quantity id="63769B3DFFBAFF9BFB6FFF4768262320" box="[1224,1317,346,373]" metricMagnitude="-3" metricUnit="m" metricValue="2.8" pageId="3" pageNumber="74" unit="mm" value="2.8">2.8 mm</quantity>
