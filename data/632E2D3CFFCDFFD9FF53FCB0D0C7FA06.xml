</bibRefCitation>
)
</taxonomicName>
and 
<taxonomicName id="2C87E7A9FFCDFFD9FEBAFB7AD6E9FAA6" authority="Ewing, 1909" authorityName="Ewing" authorityYear="1909" box="[382,743,1264,1290]" class="Arachnida" family="Phthiracaridae" genus="Atropacarus" kingdom="Animalia" order="Sarcoptiformes" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="hamatus" subGenus="Hoplophorella">
