(
<emphasis id="D9F34038FFCDFFD9FBA2FACAD0E2FAF5" box="[1126,1260,1344,1369]" italics="true" pageId="16" pageNumber="17">Tropacarus</emphasis>
) 
<emphasis id="D9F34038FFCDFFD9FAC4FACAD192FAF5" box="[1280,1436,1344,1369]" italics="true" pageId="16" pageNumber="17">pulcherrimus</emphasis>
(Berlese, 
<number id="5D4F8E0DFFCDFFD9FECFFAE2D549FA2E" box="[267,327,1384,1410]" pageId="16" pageNumber="17" value="1987.0">1987</number>
)
</taxonomicName>
with 
<taxonomicName id="2C87E7A9FFCDFFD9FE5DFAE2D03BFA2E" authority="C.L.Koch, 1841" authorityName="C.L.Koch" authorityYear="1841" box="[409,1077,1384,1410]" class="Arachnida" family="Phthiracaridae" genus="Steganacarus" kingdom="Animalia" order="Sarcoptiformes" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="carinatus" subGenus="Tropacarus">
