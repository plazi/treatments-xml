<taxonomicName id="4C16329CFFC87062FF44A183FECF1159" box="[239,306,1015,1040]" class="Maxillopoda" family="Telsidae" genus="Telson" kingdom="Animalia" order="Poecilostomatoida" pageId="31" pageNumber="32" phylum="Arthropoda" rank="genus">telson</taxonomicName>
