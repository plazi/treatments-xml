<quantity id="CA02C9DE2D4AFFFFFCE4FE1EADE2FE66" box="[853,941,469,496]" metricMagnitude="-3" metricUnit="m" metricValue="5.6" pageId="17" pageNumber="64" unit="mm" value="5.6">5.6 mm</quantity>
