<quantity id="CA02C9DE2D4AFFFFFB98FE1EAACDFE66" box="[1065,1154,469,496]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="17" pageNumber="64" unit="mm" value="4.4">4.4 mm</quantity>
