<quantity id="CA02C9DE2D4AFFFFFFDEFA63AE8BFA55" box="[111,196,1448,1475]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="17" pageNumber="64" unit="mm" value="3.5">3.5 mm</quantity>
; width, 
<quantity id="CA02C9DE2D4AFFFFFEA6FA63AF23FA55" box="[279,364,1448,1475]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="17" pageNumber="64" unit="mm" value="3.3">3.3 mm</quantity>
