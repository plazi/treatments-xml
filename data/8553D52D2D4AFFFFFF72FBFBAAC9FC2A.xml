<quantity id="CA02C9DE2D4AFFFFFD14FE1EACB2FE66" box="[677,765,469,496]" metricMagnitude="-3" metricUnit="m" metricValue="6.8" pageId="17" pageNumber="64" unit="mm" value="6.8">6.8 mm</quantity>
