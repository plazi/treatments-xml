<quantity id="CA02C9DE2D4AFFFFFC79FE7EAA51FE46" box="[968,1054,437,464]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="17" pageNumber="64" unit="mm" value="0.6">0.6 mm</quantity>
