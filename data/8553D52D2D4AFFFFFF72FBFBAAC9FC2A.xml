<quantity id="CA02C9DE2D4AFFFFFE53FA63AC77FA55" box="[482,568,1448,1475]" metricMagnitude="-3" metricUnit="m" metricValue="2.3" pageId="17" pageNumber="64" unit="mm" value="2.3">2.3 mm</quantity>
