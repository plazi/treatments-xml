<taxonomicName id="46C2B9DDDD62FF8AFBCEFB40FF74FAF9" authority="(MNHN)" baseAuthorityName="MNHN" class="Gastropoda" family="Polyceridae" genus="Thecacera" kingdom="Animalia" order="Nudibranchia" pageId="8" pageNumber="23" phylum="Mollusca" rank="species" species="pennigera">
