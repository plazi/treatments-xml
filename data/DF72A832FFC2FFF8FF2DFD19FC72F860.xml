<taxonomicName id="90DB62A7FFC2FFF8FD22FC15FC01FC50" authorityName="d'Orbigny" authorityYear="1839" box="[664,961,980,1006]" class="Tubothalamea" family="Hauerinidae" genus="Quinqueloculina" kingdom="Chromista" order="Miliolida" pageId="5" pageNumber="156" phylum="Foraminifera" rank="species" species="bosciana">
