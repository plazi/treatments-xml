(
<bibRefCitation id="A60D028AC41F232AFDDEDE1DFAEBBD78" author="Tyagi, N. &amp; Baijal, H. N." box="[517,765,1125,1151]" pageId="2" pageNumber="281" pagination="6 - 10" refId="ref7117" refString="Tyagi, N. &amp; Baijal, H. N. (1982) Three New Species of Hypogastrura (Collembola: Hypogastruridae) from India. Bulletin of Pure &amp; Applied Sciences, 1, 6 - 10." type="journal article" year="1982">Tyagi &amp; Baijal, 1982</bibRefCitation>
)
