<taxonomicName id="67C2F0DC4E8A6CEA63FFDC59F8D679E8" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Fagus" order="Fagales" pageId="0" pageNumber="35" phylum="Tracheophyta" rank="genus">Fagus</taxonomicName>
