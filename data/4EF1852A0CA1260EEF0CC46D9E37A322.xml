<taxonomicName id="7962F3D2F21A35295393DD735EC027B9" class="Magnoliopsida" family="Magnoliaceae" genus="Liriodendron" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Liriodendron" order="Magnoliales" pageId="0" pageNumber="35" phylum="Tracheophyta" rank="genus">Liriodendron</taxonomicName>
