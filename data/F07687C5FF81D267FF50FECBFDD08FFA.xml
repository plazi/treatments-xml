<taxonomicName id="BFDF4D50FF81D267FF50FE1BFD948E5D" authority="Kingsley" authorityName="Kingsley" box="[151,549,505,531]" class="Malacostraca" family="Palaemonidae" genus="Periclimenes" kingdom="Animalia" order="Decapoda" pageId="30" pageNumber="331" phylum="Arthropoda" rank="species" species="americanus">
