); as 
<taxonomicName id="BFDF4D50FF81D267FBB9FE1BFEBE8E79" authority="(Kingsley)" authorityName="Kingsley" baseAuthorityName="Kingsley" baseAuthorityYear="1878" class="Malacostraca" family="Palaemonidae" genus="Periclimenes" kingdom="Animalia" order="Decapoda" pageId="30" pageNumber="331" phylum="Arthropoda" rank="species" species="americanus">
