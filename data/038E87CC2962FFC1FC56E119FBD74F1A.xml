<number id="3DEF24FD2963FFC1FDF4E58AFD804EEA" box="[546,589,922,951]" pageId="35" pageNumber="34" value="0.5" valueMax="1.0" valueMin="0.0">0-1</number>
-0. Leg formula 
<number id="3DEF24FD2963FFC1FFBEE5AFFF614E81" box="[104,172,959,988]" pageId="35" pageNumber="34" value="1243.0">1243</number>
