<taxonomicName id="1038EA7378A33BB06DE90061BE02F224" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:179449" class="Insecta" family="Formicidae" genus="Ponera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ponera punctatissima indifferens Forel" lsidName-HNS="Ponera punctatissima indifferens Forel" order="Hymenoptera" pageNumber="245" phylum="Arthropoda" rank="subSpecies" species="punctatissima" subSpecies="indifferens">Ponera punctatissima r. indifferens</taxonomicName>
