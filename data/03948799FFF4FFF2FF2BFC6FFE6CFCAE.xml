<paragraph id="8B82368FFFEBFFF0FF41FEEAFECAFEB2" blockId="48.[221,392,160,277]" box="[221,378,259,277]" pageId="48" pageNumber="49">
<emphasis id="B949EA9DFFEBFFF0FF41FEEAFECAFEB2" bold="true" box="[221,378,259,277]" pageId="48" pageNumber="49">
Scale bar = 
<quantity id="4CC59B6AFFEBFFF0FED9FEEAFEC6FEB2" box="[325,374,259,277]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="48" pageNumber="49" unit="mm" value="5.0">5 mm</quantity>
