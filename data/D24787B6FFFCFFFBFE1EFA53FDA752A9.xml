<taxonomicName id="9DEE4D23FFFCFFF8FF11F9D3FDA251E3" authority="(Santos Abreu)" baseAuthorityName="Santos Abreu" baseAuthorityYear="1921" box="[144,530,1661,1685]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="52" pageNumber="158" phylum="Arthropoda" rank="species" species="comosa">
