, 
<taxonomicName id="16904D5FFFFCFFBD58A3CFCAFE6CF808" authority="(Hanzawa, 1930)" baseAuthorityName="Hanzawa" baseAuthorityYear="1930" box="[154,431,1985,2003]" class="Tubothalamea" family="Alveolinidae" genus="Borelis" kingdom="Chromista" order="Miliolida" pageId="8" pageNumber="281" phylum="Foraminifera" rank="species" species="pygmaea">
