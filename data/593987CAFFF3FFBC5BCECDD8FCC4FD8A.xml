, 
<taxonomicName id="16904D5FFFFCFFBD5B42CFCAFB01F808" authority="(Henson, 1950)" baseAuthorityName="Henson" baseAuthorityYear="1950" box="[891,1218,1985,2003]" class="Tubothalamea" family="Soritidae" genus="Penarchaias" higherTaxonomySource="GBIF" kingdom="Chromista" order="Miliolida" pageId="8" pageNumber="281" phylum="Foraminifera" rank="species" species="glynnjonesi">
