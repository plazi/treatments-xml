<taxonomicName id="1DFB83C3FFA2FFE3AFB93E32FDB3FEFF" box="[421,563,284,310]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="33" pageNumber="40" phylum="Chordata" rank="species" species="weigoldicus">
