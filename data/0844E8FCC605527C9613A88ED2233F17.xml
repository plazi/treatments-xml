<taxonomicName id="4FD7F589C01E6A5235A84A18A4654C1A" class="Magnoliopsida" family="Goodeniaceae" genus="Goodenia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Goodenia rotundifolia" order="Asterales" pageId="0" pageNumber="27" phylum="Tracheophyta" rank="species" species="rotundifolia">
