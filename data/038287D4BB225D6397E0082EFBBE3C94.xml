<taxonomicName id="4C2B4D41BB225D6394EA0EAAFBE43EFF" baseAuthorityName="Lopes" baseAuthorityYear="1935" box="[925,1081,1477,1504]" class="Insecta" family="Sarcophagidae" genus="Oxysarcodexia" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="favorabilis">
