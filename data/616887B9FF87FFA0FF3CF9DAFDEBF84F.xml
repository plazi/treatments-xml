<taxonomicName id="2EC14D2CFF87FFA0FC4EF896FAAFF887" authority="(Savi) Ten." authorityName="(Savi) Ten." box="[997,1313,1866,1891]" class="Magnoliopsida" family="Asteraceae" genus="Cirsium" kingdom="Plantae" order="Asterales" pageId="26" pageNumber="67" phylum="Tracheophyta" rank="species" species="vulgare">
