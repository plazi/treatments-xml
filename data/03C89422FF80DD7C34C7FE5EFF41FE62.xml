<caption id="DF1E75BCFF82DD7B34C7F850FEDCF87C" pageId="83" pageNumber="84" startId="83.[151,255,1971,1995]" targetBox="[174,1415,190,1939]" targetPageId="83">
<paragraph id="8BDE2534FF82DD7B34C7F850FEDCF87C" blockId="83.[151,1437,1971,2028]" pageId="83" pageNumber="84">
<emphasis id="B915F926FF82DD7B34C7F850FE91F85B" bold="true" box="[151,291,1971,1995]" pageId="83" pageNumber="84">FIGURE 30</emphasis>
. 
<taxonomicName id="4C615EB7FF82DD7B3563F850FDB8F85A" authorityName="Woltereck" authorityYear="1909" box="[307,522,1971,1994]" class="Malacostraca" family="Chuneolidae" genus="Chuneola" kingdom="Animalia" order="Amphipoda" pageId="83" pageNumber="84" phylum="Arthropoda" rank="species" species="paradoxa">
