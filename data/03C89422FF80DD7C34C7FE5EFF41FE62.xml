. 
<taxonomicName id="4C615EB7FF83DD7A357CF869FDB2F831" authorityName="Woltereck" authorityYear="1909" box="[300,512,1930,1953]" class="Malacostraca" family="Chuneolidae" genus="Chuneola" kingdom="Animalia" order="Amphipoda" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="paradoxa">
<emphasis id="B915F926FF83DD7A357CF869FDB2F831" box="[300,512,1930,1953]" italics="true" pageId="82" pageNumber="83">Chuneola paradoxa</emphasis>
</taxonomicName>
; 
<emphasis id="B915F926FF83DD7A365EF86AFD93F831" bold="true" box="[526,545,1929,1953]" pageId="82" pageNumber="83">A</emphasis>
, lateral view, 
<emphasis id="B915F926FF83DD7A36E6F86AFD7AF831" bold="true" box="[694,712,1929,1953]" pageId="82" pageNumber="83">B</emphasis>
