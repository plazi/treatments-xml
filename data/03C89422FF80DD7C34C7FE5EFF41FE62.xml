<typeStatus id="54DA9B96FF80DD7935DBFCC3FE0FFCAA" box="[395,445,800,826]" pageId="81" pageNumber="82">type</typeStatus>
locality is the Indian Ocean [
<geoCoordinate id="EE5543F3FF80DD793745FCC3FC3CFCAA" box="[789,910,800,826]" degrees="06" direction="south" minutes="38.5" orientation="latitude" pageId="81" pageNumber="82" precision="92" value="-6.6416664">06°38.5’S</geoCoordinate>
<geoCoordinate id="EE5543F3FF80DD7937C5FCC3FBA0FCAA" box="[917,1042,800,826]" degrees="70" direction="east" minutes="58.1" orientation="longitude" pageId="81" pageNumber="82" precision="92" value="70.96833">70°58.1’E</geoCoordinate>
]; 
