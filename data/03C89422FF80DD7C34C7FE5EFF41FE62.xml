<typeStatus id="54DA9B96FF80DD793690FCABFD40FCF2" box="[704,754,840,866]" pageId="81" pageNumber="82">type</typeStatus>
of 
<taxonomicName id="4C615EB7FF80DD79374EFCABFC08FCF1" authorityName="Vinogradov" authorityYear="1956" box="[798,954,840,865]" class="Malacostraca" family="Chuneolidae" genus="Chuneola" kingdom="Animalia" order="Amphipoda" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="parasitica">
