<taxonomicName id="43944D572220E367FDD02068B0EAFDF5" authority="Fabricius, 1803" authorityName="Salini" authorityYear="2019" box="[584,732,583,608]" class="Insecta" family="Pentatomidae" genus="Halys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="360" phylum="Arthropoda" rank="species" species="mudigerensis">
