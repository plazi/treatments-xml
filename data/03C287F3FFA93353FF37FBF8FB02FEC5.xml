<collectingCountry id="F37C7675FFAB3353FECAFF2FFE6AFF15" box="[362,418,152,176]" name="United States of America" pageId="14" pageNumber="407">USA</collectingCountry>
(
<bibRefCitation id="EFFA4B14FFAB3353FE14FF20FD80FF15" author="Girault" box="[436,584,151,176]" pageId="14" pageNumber="407" refString="Girault, A. A. (1911) Descriptions of nine new genera of the chalcidoid family Trichogrammatidae. Transactions of the American Entomological Society, 37, 1 - 42." type="journal article" year="1911">
Girault 
<number id="3DA324C2FFAB3353FDB0FF2FFD80FF15" box="[528,584,152,176]" pageId="14" pageNumber="407" value="1911.0">1911</number>
</bibRefCitation>
) and 
<collectingCountry id="F37C7675FFAB3353FD2DFF2FFD27FF15" box="[653,751,152,176]" name="Uruguay" pageId="14" pageNumber="407">Uruguay</collectingCountry>
(
<bibRefCitation id="EFFA4B14FFAB3353FCA1FF2FFC71FF15" author="De" box="[769,953,151,176]" pageId="14" pageNumber="407" refString="De Santis, L. (1979) Catalogo de los himenopteros calcidoideos de America al sur de los Estados Unidos. Publicacion especial, Comision de Investigaciones Cientificas de la provincia de Buenos Aires, La Plata, 488 pp." type="book" year="1979">
