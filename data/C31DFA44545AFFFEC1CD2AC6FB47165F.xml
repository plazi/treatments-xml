<paragraph id="4B0B4B52545AFFFEC1CD2AC6FE731478" blockId="6.[151,489,1269,1295]" box="[151,489,1269,1295]" pageId="6" pageNumber="31">
<heading id="1043FC3E545AFFFEC1CD2AC6FE731478" bold="true" box="[151,489,1269,1295]" fontSize="11" level="1" pageId="6" pageNumber="31" reason="1">
<taxonomicName id="8CB430D1545AFFFEC1CD2AC6FE731478" authority="Mound" authorityName="Mound" box="[151,489,1269,1295]" class="Insecta" family="Thripidae" genus="Bhattithrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="6" pageNumber="31" phylum="Arthropoda" rank="species" species="dahmsi">
