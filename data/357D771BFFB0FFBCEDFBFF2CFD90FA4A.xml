<paragraph id="BD6BC60DFFB0FFBBEDAFFE17FB8FFDD1" blockId="27.[667,1194,257,1759]" pageId="27" pageNumber="28">
<typeStatus id="626F78AFFFB0FFBBEDAFFE17FC87FD8B" box="[696,824,494,518]" pageId="27" pageNumber="28">HOLOTYPE</typeStatus>
