<taxonomicName id="94AA4D53FFB1FFEA9E8FFB61FCEE4C39" box="[678,821,1266,1292]" class="Insecta" family="Adelidae" genus="Nemophora" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="85" phylum="Arthropoda" rank="genus">
<emphasis id="61DEEAC2FFB1FFEA9E8FFB61FCEE4C39" bold="true" box="[678,821,1266,1292]" italics="true" pageId="1" pageNumber="85">Nemophora</emphasis>
