<paragraph id="8BED36C7100CFFA943D80BDF85CE87F9" blockId="28.[236,1351,1153,1272]" box="[236,393,1153,1178]" pageId="28" pageNumber="1505">
<typeStatus id="54E98865100CFFA943D80BDF856287F9" box="[236,293,1154,1178]" pageId="28" pageNumber="1505">Type</typeStatus>
