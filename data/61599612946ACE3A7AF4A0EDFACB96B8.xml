<taxonomicName id="2EF05C87946ACE3A7AF4A0EDFCF39785" authorityName="Gilbert" authorityYear="1891" box="[699,847,1210,1236]" class="Actinopterygii" family="Congridae" genus="Xenomystax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="12" pageNumber="13" phylum="Chordata" rank="genus">
<emphasis id="DB84FB16946ACE3A7AF4A0EDFCF39785" bold="true" box="[699,847,1210,1236]" italics="true" pageId="12" pageNumber="13">Xenomystax</emphasis>
</taxonomicName>
