<taxonomicName id="10B04D13FFCC57322E09F880FDD54E90" authorityName="Emmerich &amp; Nieto" authorityYear="2009" box="[356,592,1903,1926]" class="Insecta" family="Baetidae" genus="Paracloeodes" kingdom="Animalia" order="Ephemeroptera" pageId="4" pageNumber="61" phylum="Arthropoda" rank="species" species="morellii" status="sp. n.">
