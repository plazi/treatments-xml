<normalizedToken id="15E0A90E1091B799EF11D11F7775E34A" originalValue="Kilpisjärvi">Kilpisjaervi</normalizedToken>
, 
<normalizedToken id="8D7CD702E9E2A3C823DF30A4AE16DA92" originalValue="Enontekiö">Enontekioe</normalizedToken>
