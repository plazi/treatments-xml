<taxonomicName id="F0B13CA6A951FFB42F75C321FB42FD67" authorityName="Guettard" authorityYear="1770" box="[1150,1259,620,644]" class="Gastropoda" family="Siliquariidae" genus="Tenagodus" kingdom="Animalia" pageId="81" pageNumber="82" phylum="Mollusca" rank="genus">
<emphasis id="05C59B37A951FFB42F75C321FB42FD67" box="[1150,1259,620,644]" italics="true" pageId="81" pageNumber="82">Tenagodus</emphasis>
