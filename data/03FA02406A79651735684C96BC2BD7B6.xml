<taxonomicName id="4C53C8D56A786514356A4B59BF23D618" authorityName="McAlpine" authorityYear="2007" box="[320,513,1876,1897]" class="Insecta" family="Heleomyzidae" genus="Borboroides" kingdom="Animalia" order="Diptera" pageId="61" pageNumber="204" phylum="Arthropoda" rank="species" species="fimbria">
<emphasis id="B9276F446A786514356A4B59BF23D618" box="[320,513,1876,1897]" italics="true" pageId="61" pageNumber="204">Borboroides fimbria</emphasis>
