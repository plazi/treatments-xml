<taxonomicName id="6DEF4D56FFEBC13AA17EFE4DFA9C154E" authority="MA et DENG, 2002" authorityName="MA et DENG" authorityYear="2002" box="[798,1270,510,536]" class="Insecta" family="Muscidae" genus="Phaonia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="subfuscibasicosta">
