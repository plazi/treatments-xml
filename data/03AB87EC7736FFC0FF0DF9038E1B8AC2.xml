<specimenCode id="DBA49E817736FFC0FBFAF84C8E178AC2" box="[1120,1285,2006,2030]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="8" pageNumber="49">CHUNB 1221</specimenCode>
