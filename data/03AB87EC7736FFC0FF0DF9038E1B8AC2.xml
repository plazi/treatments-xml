<materialsCitation id="3B6A3CA77736FFC0FCF2F84C8E178AC2" box="[872,1285,2006,2030]" collectionCode="CHUNB" pageId="8" pageNumber="49" specimenCode="CHUNB 1221" specimenCount="1" specimenCount-male="1">
