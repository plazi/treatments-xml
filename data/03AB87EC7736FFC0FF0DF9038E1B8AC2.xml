<specimenCode id="DBA49E817736FFC0FCB9F84C884F8AC2" box="[803,861,2006,2030]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="8" pageNumber="49">1212</specimenCode>
</materialsCitation>
