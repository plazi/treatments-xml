<number id="3DDF0578CF08EB55FD17FAA2FD54B03D" box="[733,746,1319,1345]" pageId="22" pageNumber="23" value="5.0">5</number>
<number id="3DDF0578CF08EB55FD3BFAA2FCACB03D" box="[753,786,1319,1345]" pageId="22" pageNumber="23" value="66.0">66</number>
