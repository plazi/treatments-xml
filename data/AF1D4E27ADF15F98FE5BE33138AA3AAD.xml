<taxonomicName id="E0B484B2ADF35F99FE94E5B8395E3ADF" authority="(Belokobylskij, 1988)" baseAuthorityName="Belokobylskij" baseAuthorityYear="1988" box="[290,890,1921,1947]" class="Insecta" family="Braconidae" genus="Rhacontsira" kingdom="Animalia" order="Hymenoptera" pageId="138" pageNumber="139" phylum="Arthropoda" rank="species" species="heterospiloides">
