<number id="104BD92CFFB9E026FDCB626EFDFEFCA1" box="[534,567,844,870]" pageId="105" pageNumber="106" value="33.0">33</number>
</figureCitation>
): 
<taxonomicName id="6183B088FFB9E026FD93626EFC8EFCA2" box="[590,839,844,869]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="105" pageNumber="106" phylum="Arthropoda" rank="species" species="amitae">
