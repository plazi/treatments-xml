), 
<taxonomicName id="64D74D71FF86FFBCF0B2F8DEFF3AF88D" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" class="Insecta" family="Calamoceratidae" genus="Anisocentropus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="thinlin" status="new species">
