<taxonomicName id="64D74D71FF86FFBCF4D9F8DEFDB5F8D5" authority="Oláh &amp; Johanson, 2010" authorityName="Oláh &amp; Johanson" authorityYear="2010" box="[488,620,1827,1852]" class="Insecta" family="Calamoceratidae" genus="Anisocentropus" kingdom="Animalia" order="Trichoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="maralus" status="new species">
<emphasis id="91A3EAE0FF86FFBCF4D9F8DEFDB5F8D5" box="[488,620,1827,1852]" italics="true" pageId="43" pageNumber="44">A. maralus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="8A90579BFF86FFBCF74FF8DFFCD4F8D5" box="[638,781,1826,1852]" pageId="43" pageNumber="44" rank="species">new species</taxonomicNameLabel>
(
<collectingCountry id="DBC07662FF86FFBCF611F8DFFC48F8D5" box="[800,913,1826,1852]" name="Malaysia" pageId="43" pageNumber="44">Malaysia</collectingCountry>
