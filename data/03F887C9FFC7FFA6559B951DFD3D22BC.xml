<subSubSection id="C34B6554FFC7FFA4559B95A9FDFD2737" pageId="1" type="diagnosis">
<paragraph id="8BEE36DFFFC7FFA4559B95A9FDFD2737" blockId="1.[151,1436,650,769]" pageId="1">
<emphasis id="B925EACDFFC7FFA4559B95A9FDFD2737" italics="true" pageId="1">
Haec species 
<taxonomicName id="4C514D5CFFC7FFA4542695A9FE712697" box="[298,492,650,673]" class="Liliopsida" family="Orchidaceae" genus="Encyclia" kingdom="Plantae" order="Asparagales" pageId="1" phylum="Tracheophyta" rank="subSpecies" species="fowliei" subSpecies="floribus">E. fowliei floribus</taxonomicName>
