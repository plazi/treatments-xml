<location id="8EF4C195FFE6326AFE4450D75242FF11" LSID="urn:lsid:plazi:treatment:03822658FFE6326AFF1E50965563FD9F:8EF4C195FFE6326AFE4450D75242FF11" box="[466,571,223,245]" country="Hungary" municipality="Type" name="Davidhaza" pageId="1" pageNumber="142" stateProvince="Vas">Dávidháza</location>
