<taxonomicName id="B06C0D23AE3D9D70029802B30154401C" class="Magnoliopsida" family="Amaranthaceae" genus="Chenopodium" higherTaxonomySource="CoL" kingdom="Plantae" order="Caryophyllales" pageId="14" pageNumber="18" phylum="Tracheophyta" rank="species" species="pratericola">Chenopodium pratericola</taxonomicName>
