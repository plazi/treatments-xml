<taxonomicName id="4C7E3589FF21FF07FC72F88A35DEFC0B" authorityName="Gory and Percheron" authorityYear="1833" box="[1018,1186,995,1019]" class="Insecta" family="Cetoniidae" genus="Gymnetis" kingdom="Animalia" order="Coleoptera" pageId="234" pageNumber="233" phylum="Arthropoda" rank="species" species="albiventris">
