<taxonomicName id="4C7E3589FF21FF07FE2EF88D33D0FC0B" baseAuthorityName="Gory and Percheron" baseAuthorityYear="1833" box="[422,684,995,1019]" class="Insecta" family="Cetoniidae" genus="Hoplopyga" kingdom="Animalia" order="Coleoptera" pageId="234" pageNumber="233" phylum="Arthropoda" rank="species" species="singularis">
