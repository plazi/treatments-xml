<typeStatus id="C11295942A32FF8F16EBFEA96F23FDA9" box="[199,258,511,536]" pageId="102" pageNumber="103">Type</typeStatus>
material
</emphasis>
. 
<typeStatus id="C11295942A32FF8F1751FEA96FC4FDA9" box="[381,485,511,536]" pageId="102" pageNumber="103">Holotype</typeStatus>
