<taxonomicName id="E7D44D1EFFE4FFE909CEF8AFFBEEF8EE" authorityName="Cockerell" authorityYear="1911" box="[753,904,1886,1910]" class="Lingulata" family="Obolidae" genus="Palaeoglossa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lingulida" pageId="18" pageNumber="19" phylum="Brachiopoda" rank="genus">
<emphasis id="12A0EA8FFFE4FFE909CEF8AFFBEEF8EE" box="[753,904,1886,1910]" italics="true" pageId="18" pageNumber="19">Palaeoglossa</emphasis>
</taxonomicName>
.
</paragraph>
