<typeStatus id="DC5B7FBCBE77FFC9AE8CF9DBFF4AF98A" box="[151,200,1590,1611]" pageId="3" pageNumber="39">Type</typeStatus>
species: 
<taxonomicName id="C4E0BA9DBE77FFC9AF3DF9D8FDF2F98A" authority="Reuss, 1847" authorityName="Reuss" authorityYear="1847" box="[294,624,1589,1611]" class="Gymnolaemata" family="Celleporidae" genus="Cellepora" kingdom="Animalia" order="Cheilostomatida" pageId="3" pageNumber="39" phylum="Bryozoa" rank="species" species="decorata">
