<taxonomicName id="2796DABAFF8AFF8D99E7F85AFCADF82B" authority="Krabbe, 1882" authorityName="Krabbe" authorityYear="1882" box="[499,784,1952,1974]" pageId="11" pageNumber="72" rank="species" species="vesiculigera">
