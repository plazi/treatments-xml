<taxonomicName id="B5201FAACD1CD3C8FB8F8B3633730889" class="Gastropoda" family="Trochomorphidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Helix sapeca" order="Stylommatophora" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="sapeca">Helix sapeca</taxonomicName>
