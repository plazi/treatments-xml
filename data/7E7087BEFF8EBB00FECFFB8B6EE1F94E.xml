<taxonomicName id="31D94D2BFF8EBB00FE05FBB66D95FBC2" authorityName="Badonnel" authorityYear="1987" box="[423,492,1058,1074]" class="Insecta" family="Psocidae" genus="Trichadenotecnum" kingdom="Animalia" order="Psocodea" pageId="59" pageNumber="710" phylum="Arthropoda" rank="species" species="decui">
<emphasis id="C4ADEABAFF8EBB00FE05FBB66D95FBC2" box="[423,492,1058,1074]" italics="true" pageId="59" pageNumber="710">DECUI</emphasis>
