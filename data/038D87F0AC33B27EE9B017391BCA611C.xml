<taxonomicName id="4C244D65AC33B27EEB8117611963629C" authorityName="W.E.Leach" authorityYear="1815" box="[643,782,1392,1414]" class="Insecta" family="Pentatomidae" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="948" phylum="Arthropoda" rank="family">Pentatomidae</taxonomicName>
, 
<taxonomicName id="4C244D65AC33B27EEA151761191C629C" authorityName="Leach" authorityYear="1815" box="[791,881,1392,1414]" class="Insecta" family="Coreidae" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="948" phylum="Arthropoda" rank="family">Coreidae</taxonomicName>
und 
<taxonomicName id="4C244D65AC33B27EEAA017611E63629C" authorityName="Schilling" authorityYear="1829" box="[930,1038,1392,1414]" class="Insecta" family="Lygaeidae" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="948" phylum="Arthropoda" rank="family">Lygaeidae</taxonomicName>
).
</paragraph>
