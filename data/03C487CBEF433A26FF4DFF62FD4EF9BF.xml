<taxonomicName id="4C6D4D5EEF423A26FDE0F9AAFD41F9BC" authorityName="Thorell" authorityYear="1859" box="[549,644,1635,1662]" class="Maxillopoda" family="Notodelphyidae" genus="Notodelphys" kingdom="Animalia" order="Cyclopoida" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="agilis">
<emphasis id="B919EACFEF423A26FDE0F9AAFDFDF9BC" box="[549,568,1636,1661]" italics="true" pageId="19" pageNumber="20">N</emphasis>
. 
<emphasis id="B919EACFEF423A26FD83F9ADFD41F9BC" box="[582,644,1635,1661]" italics="true" pageId="19" pageNumber="20">agilis</emphasis>
</taxonomicName>
