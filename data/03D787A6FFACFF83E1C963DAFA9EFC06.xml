<subSubSection id="C364653BFFB1FF83E198644EFE1AFD16" pageId="16" pageNumber="17" type="distribution">
<paragraph id="8BC136B0FFB1FF83E198644EFE1AFD16" blockId="16.[151,1437,152,938]" pageId="16" pageNumber="17">
<emphasis id="B90AEAA2FFB1FF83E198644EFA04FDC5" bold="true" box="[198,359,591,617]" pageId="16" pageNumber="17">Distribution.</emphasis>
<taxonomicName id="4C7E4D33FFB1FF83E0336451F917FDC5" box="[365,628,592,617]" class="Insecta" family="Dolichopodidae" genus="Tachytrechus" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="peruicus">
