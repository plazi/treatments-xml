<taxonomicName id="4C4638C7F30A7F6D15E6A47CFDB0F997" baseAuthorityName="Peringuey" baseAuthorityYear="1896" box="[462,579,1551,1576]" class="Insecta" family="Carabidae" genus="Craspedophorus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="rikatlensis">
