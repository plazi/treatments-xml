<taxonomicName id="55155454262EB278A5491CA1E991A64F" class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus" order="Malpighiales" pageId="6" pageNumber="263" phylum="Tracheophyta" rank="genus">Populus</taxonomicName>
