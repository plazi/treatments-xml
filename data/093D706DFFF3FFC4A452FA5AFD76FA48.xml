<number id="375CD35CFFF3FFC7A409FA7FFEC0FA35" box="[339,353,1444,1470]" pageId="5" pageNumber="62" value="6.0">6</number>
–8
</figureCitation>
, 
<figureCitation id="19AFDDFEFFF3FFC7A4D6FA7FFE08FA35" box="[396,425,1444,1470]" captionStart="FIGURES 9 – 11" captionStartId="3.[264,382,1571,1595]" captionTargetBox="[368,1214,331,1549]" captionTargetId="figure@3.[366,1214,321,1568]" captionTargetPageId="3" captionText="FIGURES 9 – 11. Adults of Ceroprpepes. 9. C. atribasilaris sp. nov.; 10. C. jilongensis sp. nov.; 11. C. lunata sp. nov." httpUri="https://zenodo.org/record/273312/files/figure.png" pageId="5" pageNumber="62">11</figureCitation>
