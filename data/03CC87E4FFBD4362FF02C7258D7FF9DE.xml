<collectionCode id="ED74AE37FFBD4362FB9FC7D18C64FD5A" box="[1034,1117,690,714]" httpUri="http://grbio.org/cool/m2jc-wmeq" name="University of California, Riverside" pageId="57" pageNumber="58">UCRC</collectionCode>
