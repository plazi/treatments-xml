<emphasis id="B911EAE0FFBD4362FED8C03E8984FAE6" box="[333,445,1373,1398]" italics="true" pageId="57" pageNumber="58">comstocki</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C654D71FFBD4362FE5EC03D8A51FAE6" box="[459,616,1373,1398]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="longispinus">
<emphasis id="B911EAE0FFBD4362FE5EC03D89E0FAE5" box="[459,473,1374,1397]" italics="true" pageId="57" pageNumber="58">P</emphasis>
. 
<emphasis id="B911EAE0FFBD4362FE72C03E8A51FAE6" box="[487,616,1373,1398]" italics="true" pageId="57" pageNumber="58">longispinus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C654D71FFBD4362FDE3C03D8B3AFAE5" box="[630,771,1374,1397]" class="Insecta" family="Pseudococcidae" genus="Pseudococcus" kingdom="Animalia" order="Hemiptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="maritimus">
<emphasis id="B911EAE0FFBD4362FDE3C03D8ABDFAE5" box="[630,644,1374,1397]" italics="true" pageId="57" pageNumber="58">P</emphasis>
. 
<emphasis id="B911EAE0FFBD4362FD07C03D8B3AFAE5" box="[658,771,1374,1397]" italics="true" pageId="57" pageNumber="58">maritimus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C654D71FFBD4362FC84C03D8B47FAE6" box="[785,894,1373,1398]" class="Insecta" family="Coccidae" genus="Lichtensia" kingdom="Animalia" order="Hemiptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="viburni">
<emphasis id="B911EAE0FFBD4362FC84C03D8B26FAE5" box="[785,799,1374,1397]" italics="true" pageId="57" pageNumber="58">P</emphasis>
. 
<emphasis id="B911EAE0FFBD4362FCB8C03E8B47FAE6" box="[813,894,1373,1398]" italics="true" pageId="57" pageNumber="58">viburni</emphasis>
