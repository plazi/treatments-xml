<taxonomicName id="4C1D4D4FFF2AD987C9C81A82FDF4FE55" authority="(Haswell, 1879)" baseAuthorityName="Haswell" baseAuthorityYear="1879" box="[151,512,402,429]" class="Malacostraca" family="Ischyroceridae" genus="Jassa" kingdom="Animalia" order="Amphipoda" pageId="159" pageNumber="160" phylum="Arthropoda" rank="species" species="australis">
