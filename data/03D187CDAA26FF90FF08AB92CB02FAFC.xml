<emphasis id="B90CEAC9AA28FF90FF5AADFBCA38FBAC" bold="true" box="[197,591,1013,1039]" pageId="9" pageNumber="10">
<typeStatus id="54C38879AA28FF90FF5AADFBC970FBAC" box="[197,263,1013,1039]" pageId="9" pageNumber="10">Type</typeStatus>
material. 
<typeStatus id="54C38879AA28FF90FE03ADFBCA38FBAC" box="[412,591,1013,1039]" pageId="9" pageNumber="10">HOLOTYPE</typeStatus>
</emphasis>
ɗ, Guizhou: Daozhen, Dashahe, 
<quantity id="4C809B3EAA28FF90FB91ADFBCC02FBAC" box="[1038,1141,1013,1039]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="9" pageNumber="10" unit="m" value="1400.0">1400 m</quantity>
, 
<date id="FFC6101BAA28FF90FB1BADFBCD78FBAC" box="[1156,1295,1013,1039]" pageId="9" pageNumber="10" value="2006-05-16">
<number id="3DB024FCAA28FF90FB1BADFBCCD3FBAC" box="[1156,1188,1013,1039]" pageId="9" pageNumber="10" value="16.0">16</number>
.V. 
<number id="3DB024FCAA28FF90FB59ADFBCD78FBAC" box="[1222,1295,1013,1039]" pageId="9" pageNumber="10" value="2006.0">2006</number>
</date>
, ZH Yang. 
<emphasis id="B90CEAC9AA28FF90FF08AA14C935FB97" bold="true" box="[151,322,1050,1076]" pageId="9" pageNumber="10">
