<taxonomicName id="3DF94D26C156AF3E2600AC66FD5572A7" authority="Fabricius, 1803" authorityName="Fabricius" authorityYear="1803" box="[313,644,372,397]" class="Insecta" family="Pentatomidae" genus="Halys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="281" phylum="Arthropoda" rank="species" species="planus">
