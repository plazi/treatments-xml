<taxonomicName id="11D24D6AFFA0FFB4B8B1F583EAD7FC94" authorityName=": Gorochov" authorityYear="2005" box="[665,934,756,778]" class="Insecta" family="Phaneropteridae" genus="Abaxisotima" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="2542" phylum="Arthropoda" rank="species" species="multipunctata">
