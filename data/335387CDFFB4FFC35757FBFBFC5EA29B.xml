<quantity id="7C029B3EFFB4FFC3577DFB10FD02A23C" box="[514,688,1260,1284]" metricMagnitude="-2" metricUnit="m" metricValue="1.095" metricValueMax="1.3199999999999998" metricValueMin="0.8699999999999999" pageId="5" pageNumber="257" unit="mm" value="10.95" valueMax="13.2" valueMin="8.7">8.70–13.20 mm</quantity>
; male: 
<quantity id="7C029B3EFFB4FFC3567CFB10FC03A23C" box="[771,945,1260,1284]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" metricValueMax="10.4" metricValueMin="7.6" pageId="5" pageNumber="257" unit="mm" value="9.0" valueMax="10.4" valueMin="7.6">7.60–10.40 mm</quantity>
