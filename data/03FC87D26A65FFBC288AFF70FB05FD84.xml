<number id="3D9D24E36A65FFBB29EAFEE4FD85FEA3" box="[503,524,259,283]" pageId="11" pageNumber="46" value="3.0">3</number>
labelled: 
<collectingCountry id="F34276546A65FFBB2A67FEE5FD3FFEA2" box="[634,694,258,282]" name="United States of America" pageId="11" pageNumber="46">USA</collectingCountry>
