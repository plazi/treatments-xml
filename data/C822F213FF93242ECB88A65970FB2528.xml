<taxonomicName id="878B3886FF92242ECFA8A26B702D2563" authority="Yin, 2022" authorityName="Yin" authorityYear="2022" box="[1207,1354,807,828]" class="Insecta" family="Staphylinidae" genus="Batrisodes" kingdom="Animalia" order="Coleoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="guoguotang" status="sp. nov.">
