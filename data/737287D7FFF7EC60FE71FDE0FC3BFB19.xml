<taxonomicName id="3CDB4D42FFF7EC60FED3FB3FFD94FB7D" authorityName="Conde" authorityYear="1956" box="[341,577,1159,1185]" class="Entognatha" family="Campodeidae" genus="Plusiocampa" kingdom="Animalia" order="Diplura" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="rybaki">
