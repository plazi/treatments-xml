<taxonomicName id="748F1269C1BD02471512F1F06278E495" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:243401" authority="Lue." authorityName="Lue." class="Insecta" family="Formicidae" genus="Aphaenogaster" kingdom="Animalia" lsidName-HNS="Aphaenogaster testaceo-pilosa (Lucas)" order="Hymenoptera" pageId="8" pageNumber="381" phylum="Arthropoda" rank="species" species="testaceo-pilosa">
Aphaenogaster testaceo-pilosa 
<authority id="3F265C65849F41732F22B582E6ABD0A0" pageId="8" pageNumber="381">Lue.</authority>
</taxonomicName>
</paragraph>
