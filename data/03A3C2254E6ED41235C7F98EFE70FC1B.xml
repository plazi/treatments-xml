(
<figureCitation id="13316FB64E6ED41035CEF9A3FEDFF9A3" box="[158,266,1578,1603]" captionStart="FIGURES 1 – 4" captionStartId="2.[151,264,1368,1391]" captionTargetBox="[157,1431,195,1341]" captionTargetId="figure@2.[150,1436,195,1344]" captionTargetPageId="2" captionText="FIGURES 1 – 4. Mohelia habitus, lateral view. 1. M. nigricauda, ♂ holotype. 2. M. matilei sp. n., ♂ paratype. 3. M. amorimi sp. n., ♂ paratype. 4. M. chandleri sp. n., ♂ holotype." httpUri="https://zenodo.org/record/240207/files/figure.png" pageId="5" pageNumber="256">Figures 3</figureCitation>
, 
<figureCitation id="13316FB64E6ED4103447F9A2FEF2F9A3" box="[279,295,1579,1603]" captionStart="FIGURES 9 – 10" captionStartId="4.[151,264,1539,1562]" captionTargetBox="[156,1429,196,1499]" captionTargetId="figure@4.[150,1437,193,1518]" captionTargetPageId="4" captionText="FIGURES 9 – 10. Mohelia wings. 9. M. amorimi sp. n., ♀ paratype. 10. M. chandleri sp. n., ♂ paratype." httpUri="https://zenodo.org/record/240209/files/figure.png" pageId="5" pageNumber="256">9</figureCitation>
, 
<figureCitation id="13316FB64E6ED4103463F9A2FE84F9A3" box="[307,337,1579,1603]" captionStart="FIGURE 12" captionStartId="6.[151,250,1336,1358]" captionTargetBox="[164,1436,301,1298]" captionTargetId="figure@6.[156,1441,301,1305]" captionTargetPageId="6" captionText="FIGURE 12. Mohelia amorimi sp. n., ♂ paratype, terminalia. A. Tergite 9 and cerci, dorsal view. B. Dorsal view, T 9 and cerci removed. C. Gonostylus, dorsal view. D. Lateral view. Abbreviations: ce, cercus; ej ap, ejaculatory apodeme; gc ap, gonocoxal apodeme; gcx, gonocoxite; gs, gonostyle; par, parameres; T, tergite; T 9 proj, T 9 projection." httpUri="https://zenodo.org/record/240211/files/figure.png" pageId="5" pageNumber="256">12</figureCitation>
, 
<figureCitation id="13316FB64E6ED410340DF9A3FE71F9A3" box="[349,420,1578,1603]" captionStart-0="FIGURES 14 – 15" captionStart-1="FIGURE 16" captionStartId-0="9.[151,264,788,811]" captionStartId-1="9.[151,250,1677,1699]" captionTargetBox-0="[348,1239,193,766]" captionTargetBox-1="[151,1436,891,1655]" captionTargetId-0="figure@9.[348,1240,193,767]" captionTargetId-1="figure@9.[151,1436,891,1655]" captionTargetPageId-0="9" captionTargetPageId-1="9" captionText-0="FIGURES 14 – 15. Mohelia paratypes, ♀ terminalia, lateral view. 14. Mohelia matilei sp. n. 15. Mohelia amorimi sp. n. Abbreviations: ce 1, first cercus of female terminalia; ce 2, second cercus of female terminalia; S, sternite; T, tergite." captionText-1="FIGURE 16. Distributional map of Mohelia in Afrotropical region." httpUri-0="https://zenodo.org/record/240213/files/figure.png" httpUri-1="https://zenodo.org/record/240214/files/figure.png" pageId="5" pageNumber="256">
15– 
<number id="3DC261144E6ED41034D8F9A3FE71F9A3" box="[392,420,1578,1603]" pageId="5" pageNumber="256" value="16.0">16</number>
