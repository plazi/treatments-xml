<taxonomicName id="4C4F4D0CFFC3FFB5F21CFC532C3A77F9" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[329,562,942,966]" class="Insecta" family="Derbidae" genus="Dichotropis" kingdom="Animalia" order="Hemiptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="flexuosa">
