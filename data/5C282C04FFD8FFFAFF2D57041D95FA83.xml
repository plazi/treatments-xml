<taxonomicName id="1381E691FFD8FFFAFF2D57041CBAFCC3" authority="(Olivier, 1789)" authorityName="Olivier" authorityYear="1789" baseAuthorityName="Olivier" baseAuthorityYear="1789" box="[151,672,799,824]" class="Insecta" family="Scarabaeidae" genus="Canthidium" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="476" phylum="Arthropoda" rank="species" species="melanocephalum">
<emphasis id="E6F54100FFD8FFFAFF2D57041CBAFCC3" bold="true" box="[151,672,799,824]" pageId="25" pageNumber="476">
<emphasis id="E6F54100FFD8FFFAFF2D57041FF5FCC3" bold="true" box="[151,495,799,824]" italics="true" pageId="25" pageNumber="476">Canthidium melanocephalum</emphasis>
