<date id="321262952A68FFE6F5692DBFDECF153E" box="[1123,1251,752,776]" pageId="3" pageNumber="74" value="2010-05-20">
<number id="F06456722A68FFE6F5692DBFDE53153E" box="[1123,1151,752,776]" pageId="3" pageNumber="74" value="20.0">20</number>
