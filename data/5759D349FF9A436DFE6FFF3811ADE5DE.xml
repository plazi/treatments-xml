<taxonomicName id="18F019DCFF9A436DFF03FC0B12E9E541" authorityName="Bouchet &amp; Sysoev" authorityYear="2001" box="[248,509,1016,1040]" class="Gastropoda" family="Raphitomidae" genus="Typhlosyrinx" kingdom="Animalia" order="Neogastropoda" pageId="11" pageNumber="1702" phylum="Mollusca" rank="species" species="panamica">
