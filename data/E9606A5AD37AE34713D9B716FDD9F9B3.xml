<taxonomicName id="A6C9A0CFD370E3461070B529FEB7FB11" authority="(Paik)" baseAuthorityName="Paik" baseAuthorityYear="1976" box="[330,668,1141,1163]" class="Arachnida" family="Agelenidae" genus="Tegecoelotes" kingdom="Animalia" order="Araneae" pageId="124" pageNumber="125" phylum="Arthropoda" rank="species" species="bicaudatus">
