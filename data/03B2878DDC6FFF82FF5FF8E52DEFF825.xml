<typeStatus id="54A08839DC6FFF83FF5FF85029DEF825" box="[151,207,2011,2035]" pageId="0" pageNumber="109">Type</typeStatus>
species. 
<taxonomicName id="4C1B4D18DC6FFF83FEFCF8502B18F824" authority="Komai &amp; Fujita, 2018" authorityName="Komai &amp; Fujita" authorityYear="2018" box="[308,521,2011,2034]" class="Malacostraca" family="Macrophthalmidae" genus="Mirograpsus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="109" phylum="Arthropoda" rank="species" species="asper" status="n. sp.">
<emphasis id="B96FEA89DC6FFF83FEFCF8502B18F824" box="[308,521,2011,2034]" italics="true" pageId="0" pageNumber="109">Mirograpsus asper</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25C57F2DC6FFF83FDD8F8502B5DF824" box="[528,588,2011,2034]" pageId="0" pageNumber="109" rank="species">n. sp.</taxonomicNameLabel>
