<taxonomicName id="4C250EECFF8CBD20FB7A22D68798F9F0" authorityName=": Illies" authorityYear="1966" box="[1259,1438,1512,1536]" class="Insecta" family="Peltoperlidae" genus="Soliperla" kingdom="Animalia" order="Plecoptera" pageId="9" pageNumber="91" phylum="Arthropoda" rank="species" species="quadrispinula">
<emphasis id="B951A97DFF8CBD20FB7A22D68798F9F0" box="[1259,1438,1512,1536]" italics="true" pageId="9" pageNumber="91">S. quadrispinula</emphasis>
