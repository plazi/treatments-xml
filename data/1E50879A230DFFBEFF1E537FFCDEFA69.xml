<taxonomicName id="51F94D0F230DFFBEFAA652CFFAE0FA45" box="[1328,1403,1468,1494]" class="Magnoliopsida" family="Lythraceae" genus="Rotala" kingdom="Plantae" order="Myrtales" pageId="4" pageNumber="171" phylum="Tracheophyta" rank="genus">
