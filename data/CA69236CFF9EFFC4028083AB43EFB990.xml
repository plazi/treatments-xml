<taxonomicName id="85C0E9F9FF98FFC50192826042F2BC1F" authority="Xu, 2000" authorityName="Xu" authorityYear="2000" box="[901,1007,1080,1103]" pageId="12" pageNumber="55" rank="species" species="sexspinus">sexspinus</taxonomicName>
