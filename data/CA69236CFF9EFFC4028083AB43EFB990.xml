<taxonomicName id="85C0E9F9FF98FFC503DF820343A2BC22" box="[456,703,1115,1138]" class="Insecta" family="Formicidae" genus="Cerapachys" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="55" phylum="Arthropoda" rank="species" species="doryloides" status="n. sp.">
