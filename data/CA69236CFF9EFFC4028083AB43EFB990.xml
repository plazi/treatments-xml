<taxonomicName id="85C0E9F9FF9FFFC203CB83A0435FBE5F" authority="Xu, 2000" authorityName="Xu" authorityYear="2000" box="[476,578,1528,1551]" pageId="11" pageNumber="54" rank="species" species="sexspinus">sexspinus</taxonomicName>
