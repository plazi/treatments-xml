<taxonomicName id="85C0E9F9FF9FFFC2034183A043DCBE5F" authority="Xu, 2000" authorityName="Xu" authorityYear="2000" box="[342,705,1528,1551]" class="Insecta" family="Formicidae" genus="Cerapachys" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="54" phylum="Arthropoda" rank="species" species="sexspinus">
