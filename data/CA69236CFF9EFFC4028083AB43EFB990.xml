<taxonomicName id="85C0E9F9FF98FFC501B582034584BC22" box="[930,1177,1115,1138]" class="Insecta" family="Formicidae" genus="Cerapachys" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="55" phylum="Arthropoda" rank="species" species="paradoxus" status="n. sp.">
