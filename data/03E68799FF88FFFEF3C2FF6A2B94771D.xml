(
<taxonomicName id="4C4F4D0CFF88FFFEF784FEE72B127510" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Bierman" baseAuthorityYear="1910" box="[1233,1306,282,303]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="107" pageNumber="108" phylum="Arthropoda" rank="species" species="pavo">
