<subSubSection id="C35C2985920749645E60F275FBA1A3C8" box="[819,1080,1134,1156]" pageId="0" pageNumber="391" type="nomenclature">
<paragraph id="8BF97A0E920749645E60F275FBA1A3C8" blockId="0.[819,1080,1134,1156]" box="[819,1080,1134,1156]" pageId="0" pageNumber="391">
<heading id="D0B1CD62920749645E60F275FBA1A3C8" box="[819,1080,1134,1156]" centered="true" fontSize="10" level="2" pageId="0" pageNumber="391" reason="2">
<emphasis id="B932A61C920749645E60F275FBA1A3C8" bold="true" box="[819,1080,1134,1156]" pageId="0" pageNumber="391">
<taxonomicName id="4C46018D920749645E60F275FC58A3C8" authority="Casari" authorityName="Casari" authorityYear="2014" box="[819,961,1134,1156]" class="Insecta" family="Elateridae" genus="Toctoc" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="391" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B932A61C920749645E60F275FCEAA3CF" bold="true" box="[819,883,1134,1155]" italics="true" pageId="0" pageNumber="391">Toctoc</emphasis>
Casari
</taxonomicName>
, 
<taxonomicNameLabel id="A2011B67920749645E9DF274FBA1A3C8" box="[974,1080,1135,1156]" pageId="0" pageNumber="391" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
