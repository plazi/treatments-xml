<emphasis id="2AB1EAC7FFF26705FD0C4001ED076463" bold="true" box="[764,788,1306,1327]" pageId="18" pageNumber="285">24</emphasis>
<taxonomicName id="DFC54D56FFF26705FCEB4000ED93647C" box="[795,896,1307,1328]" class="Crustacea (awaiting allocation)" family="Crustacea (awaiting allocation)" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea (awaiting allocation)" pageId="18" pageNumber="351" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
