<emphasis id="2AB1EAC7FFF26705FC3D4001EDF66463" bold="true" box="[973,997,1306,1327]" pageId="18" pageNumber="285">25</emphasis>
<taxonomicName id="DFC54D56FFF26705FC1B4000EA43647C" box="[1003,1104,1307,1328]" class="Crustacea (awaiting allocation)" family="Crustacea (awaiting allocation)" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea (awaiting allocation)" pageId="18" pageNumber="351" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
