<heading id="B406C8714E539921FF20F9ECCB3BDE02" bold="true" box="[151,521,1610,1636]" fontSize="11" level="1" pageId="11" pageNumber="12" reason="1">
<emphasis id="DD85A30F4E539921FF20F9ECCB3BDE02" bold="true" box="[151,521,1610,1636]" pageId="11" pageNumber="12">
Genus 
<taxonomicName id="28F1049E4E539921FF5AF9ECCB3BDE02" authority="Townsend" authorityName="Townsend" box="[237,521,1610,1636]" class="Insecta" family="Calliphoridae" genus="Cochliomyia" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
<emphasis id="DD85A30F4E539921FF5AF9ECC8B4DE02" bold="true" box="[237,390,1610,1636]" italics="true" pageId="11" pageNumber="12">Cochliomyia</emphasis>
