<number id="3DC624BDE37CFFCC539ABFE18363FD1D" box="[223,295,724,748]" pageId="10" pageNumber="11" value="16582.0" valueMax="33140.0" valueMin="24.0">24–33</number>
</figureCitation>
, 
<figureCitation id="13352A1FE37CFFCC5277BFE18319FD1D" box="[306,349,724,748]" captionStart="FIGURES 140 – 141" captionStartId="46.[151,264,2010,2033]" captionTargetBox="[284,1314,202,1982]" captionTargetId="figure@46.[276,1315,193,1983]" captionTargetPageId="46" captionText="FIGURES 140 – 141. Distribution map of species of Forcipomyia (Metaforcipomyia)." httpUri="https://zenodo.org/record/215052/files/figure.png" pageId="10" pageNumber="11">140</figureCitation>
