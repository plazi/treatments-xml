<taxonomicName id="A2D44D02FF993F4BFBEF26866DC282B6" authorityName="Logunov &amp; Ponomarev" authorityYear="2020" baseAuthorityName="Pocock" baseAuthorityYear="1889" box="[1105,1218,904,927]" class="Arachnida" family="Lycosidae" genus="Tarentula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="13" pageNumber="287" phylum="Arthropoda" rank="species" species="medica">
