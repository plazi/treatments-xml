<taxonomicName id="1FECFB9CFFC58923FEA94FEAFE7A4B95" authorityName="Buquet" authorityYear="1857" box="[301,447,729,755]" class="Insecta" family="Cerambycidae" genus="Hemicladus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="458" phylum="Arthropoda" rank="species" species="thomsonii">
