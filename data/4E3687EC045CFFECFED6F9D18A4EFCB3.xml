<subSubSection id="8E856571045CFFECFF09F9998EFFF888" pageId="3" pageNumber="4" type="distribution">
<paragraph id="C62036FA045CFFECFF09F9998EFFF888" blockId="3.[189,798,1631,1908]" pageId="3" pageNumber="4">
<emphasis id="F4EBEAE8045CFFECFF09F9998E19F9F8" bold="true" box="[189,349,1631,1655]" pageId="3" pageNumber="4">Distribution.</emphasis>
Previously known from 
<collectingRegion id="045BF818045CFFECFD13F9998C5CF9F8" box="[679,792,1631,1655]" country="United States of America" name="Arkansas" pageId="3" pageNumber="4">Arkansas</collectingRegion>
, 
<collectingRegion id="045BF818045CFFECFF09F9458E55F914" box="[189,273,1667,1691]" country="United States of America" name="Illinois" pageId="3" pageNumber="4">Illinois</collectingRegion>
, 
<collectingRegion id="045BF818045CFFECFE9AF9458EF2F914" box="[302,438,1667,1691]" country="United States of America" name="Mississippi" pageId="3" pageNumber="4">Mississippi</collectingRegion>
, 
<collectingRegion id="045BF818045CFFECFE67F9458D7FF914" box="[467,571,1667,1691]" country="United States of America" name="Missouri" pageId="3" pageNumber="4">Missouri</collectingRegion>
