<taxonomicName id="E284CC8BFFD1FF91FD39FD1CFC20FD16" authority="HORSTMANN" authorityName="HORSTMANN" box="[650,935,691,712]" class="Insecta" family="Ichneumonidae" genus="Gelis" kingdom="Animalia" order="Hymenoptera" pageId="42" pageNumber="1719" phylum="Arthropoda" rank="species" species="fumipennis">
