<paragraph id="DDD2E5CDF7DD4565833687CDBC55EF84" pageId="null" pageNumber="358">
Skabiose, 
<normalizedToken id="26A5A1C67C980730D85172ED025FA657" originalValue="Krätzkraut" pageId="null" pageNumber="358">Kraetzkraut</normalizedToken>
</paragraph>
