<taxonomicName id="9FCFF381FF9AFFFF191FF89BFD119255" box="[514,689,1896,1918]" class="Bacillariophyceae" family="Cocconeidaceae" genus="Cocconeis" kingdom="Chromista" order="Achnanthales" pageId="16" pageNumber="165" phylum="Bacillariophyta" rank="species" species="undetermined">
