and 
<collectingCountry id="F367764D5E16FFE2198C5495FC59FC00" box="[907,1017,910,935]" name="Swaziland" pageId="46" pageNumber="201">Swaziland</collectingCountry>
; on soils derived from sandstone or quartzite in montane and subalpine grassland from 
<quantity id="4C889B385E16FFE2199754D7FBE3FC42" box="[912,1091,972,997]" metricMagnitude="3" metricUnit="m" metricValue="2.125" metricValueMax="3.05" metricValueMin="1.2" pageId="46" pageNumber="201" unit="m" value="2125.0" valueMax="3050.0" valueMin="1200.0">
