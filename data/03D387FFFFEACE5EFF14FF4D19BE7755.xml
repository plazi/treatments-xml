<taxonomicName id="4C7A4D6AFFEACE5EFBAEFA26196E7721" authority="Boulard &amp; Yap" authorityName="Boulard &amp; Yap" box="[1069,1355,1523,1544]" class="Insecta" family="Cicadidae" genus="Psithyristria" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="297" phylum="Arthropoda" rank="species" species="isarogensis" status="syn. n.">
