(
<figureCitation id="88CD2A0EDF377276C2D5FD5FFF1D75FD" box="[159,253,726,750]" captionStart="FIGURES 21 – 30" captionStartId="158.[151,264,1696,1719]" captionTargetBox="[296,1308,213,1653]" captionTargetId="figure@158.[261,1327,193,1675]" captionTargetPageId="158" captionText="FIGURES 21 – 30. Wing venation of Blastobasinae. 21, Blastobasis vesta (slide 7051). 22, Blastobasis nivis (slide 7048). 23, Blastobasis phaedra (slide 7042). 24, Blastobasis aedes (slide 7050). 25, Blastobasis dicionis (slide 7039). 26, Hypatopa cladis (slide 7005). 27, Hypatopa juno (slide 7033). 28, Hypatopa pica (slide 7014). 29, Hypatopa hecate (slide 7015). 30, Hypatopa crux (slide 7035)." httpUri="https://zenodo.org/record/247399/files/figure.png" pageId="85" pageNumber="86">
Figs. 
<number id="A63E24ACDF377276C295FD5FFF1D75FD" box="[223,253,726,750]" pageId="85" pageNumber="86" value="28.147">28</number>
</figureCitation>
, 
<figureCitation id="88CD2A0EDF377276C342FD5FFE8D75FD" box="[264,365,726,750]" captionStart="FIGURES 147 – 152" captionStartId="177.[151,264,1966,1989]" captionTargetBox="[152,1432,202,1944]" captionTargetId="figure@177.[151,1436,193,1945]" captionTargetPageId="177" captionText="FIGURES 147 – 152. Male genitalia of Blastobasinae. 147 – 148, Hypatopa pica (slide 2556). 149 – 150, Hypatopa hecate (slide 2029). 151 – 152, Hypatopa acus (slide 2004)." httpUri="https://zenodo.org/record/247419/files/figure.png" pageId="85" pageNumber="86">147–148</figureCitation>
, 
<figureCitation id="88CD2A0EDF377276C332FD5FFE4475FD" box="[376,420,726,750]" captionStart="FIGURES 260 – 262" captionStartId="198.[151,264,1966,1989]" captionTargetBox="[164,1396,194,1942]" captionTargetId="figure@198.[151,1436,193,1945]" captionTargetPageId="198" captionText="FIGURES 260 – 262. Female genitalia of Blastobasinae. 260, Blastobasis dicionis (Slide 6007). 261, Hypatopa pica (Slide 4514). 262, Hypatopa hecate (Slide 4517)." httpUri="https://zenodo.org/record/247441/files/figure.png" pageId="85" pageNumber="86">261</figureCitation>
, 
<figureCitation id="88CD2A0EDF377276C3FAFD5FFE3D75FD" box="[432,477,726,750]" captionStart="FIGURES 360 – 367" captionStartId="215.[151,264,1932,1955]" captionTargetBox="[172,1418,187,1886]" captionTargetId="figure@215.[151,1436,165,1912]" captionTargetPageId="215" captionText="FIGURES 360 – 367. Imagos of Blastobasinae. 360, Hypatopa pica, holotype. 361, Hypatopa hecate, holotype. 362, Hypatopa acus, holotype. 363, Hypatopa crux, holotype. 364, Hypatopa limae, holotype. 365, Hypatopa hera, holotype. 366, Hypatopa arxcis, holotype. 367, Hypatopa caedis, holotype." httpUri="https://zenodo.org/record/247458/files/figure.png" pageId="85" pageNumber="86">360</figureCitation>
