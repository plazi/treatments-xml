, and 
<taxonomicName id="4C2F4D0CFF8EFFB3FECBFCE7FD063667" box="[375,547,820,841]" class="Magnoliopsida" family="Lauraceae" genus="Sassafras" kingdom="Plantae" order="Laurales" pageId="7" pageNumber="357" phylum="Tracheophyta" rank="species" species="albidum">
