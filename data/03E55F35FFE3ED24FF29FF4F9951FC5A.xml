<taxonomicName id="4C4C95A0FFE3ED24FE19FCBB9F32FCB4" authorityName="Izquierdo" authorityYear="2017" box="[431,515,789,812]" class="Arachnida" family="Oonopidae" genus="Orchestina" kingdom="Animalia" order="Araneae" pageId="95" phylum="Arthropoda" rank="species" species="pizarroi">pizarroi</taxonomicName>
