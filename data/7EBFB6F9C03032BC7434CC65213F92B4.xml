<paragraph id="877AE0C3FF0953623D63A467DA1FA895" pageId="3" pageNumber="4">
<taxonomicName id="D6077C79C3A5492FC96382DCD444CBC7" class="Insecta" family="Eupelmidae" genus="Calymmochilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calymmochilus dispar" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="dispar">Calymmochilus dispar</taxonomicName>
