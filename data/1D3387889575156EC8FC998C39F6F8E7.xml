<taxonomicName id="529A4D1D9575156EC8FC98773CBCFAF3" authorityName="Steindachner" authorityYear="1864" box="[151,443,1369,1391]" class="Amphibia" family="Dendrobatidae" genus="Dendrobates" kingdom="Animalia" order="Anura" pageId="53" pageNumber="54" phylum="Chordata" rank="species" species="quinquevittatus">
