<taxonomicName id="B5B94D66F66CDBEC1A2A972F63022C78" baseAuthorityName="I.Lea" baseAuthorityYear="1838" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="65" pageNumber="66" phylum="Mollusca" rank="species" species="columbianus" subGenus="Mesembrinus">
