<taxonomicName id="E3B64D6BFFF5FF97FE96FAFDFE09B21C" box="[352,435,1301,1327]" class="Insecta" family="Cynipidae" genus="Andricus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="468" phylum="Arthropoda" rank="species" species="amenti">
