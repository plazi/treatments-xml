<paragraph id="8BA236CADE22FFCDFF378D2AFC8D8BEC" blockId="4.[151,1436,151,716]" pageId="4" pageNumber="50">
<emphasis id="B969EAD8DE22FFCDFF378D2AFEA58B47" bold="true" box="[197,372,332,356]" pageId="4" pageNumber="50">Measurements</emphasis>
(
<typeStatus id="54A68868DE22FFCDFE778D2AFE388B47" box="[389,489,332,356]" pageId="4" pageNumber="50">holotype</typeStatus>
in microns). Length (width). Body 
<number id="3DD524EDDE22FFCDFC778D2AFC6C8B47" box="[901,957,332,356]" pageId="4" pageNumber="50" value="1540.0">1540</number>
(
<number id="3DD524EDDE22FFCDFC3C8D2AFC2C8B47" box="[974,1021,332,356]" pageId="4" pageNumber="50" value="400.0">400</number>
); head 
<number id="3DD524EDDE22FFCDFBA08D2AFBAA8B47" box="[1106,1147,332,356]" pageId="4" pageNumber="50" value="115.0">115</number>
(
<number id="3DD524EDDE22FFCDFB7F8D2AFB6A8B47" box="[1165,1211,332,356]" pageId="4" pageNumber="50" value="180.0">180</number>
); width across eyes 
<number id="3DD524EDDE22FFCDFF658D09FF678BA4" box="[151,182,367,391]" pageId="4" pageNumber="50" value="60.0">60</number>
; ocellar setae 
<date id="FFA3100ADE22FFCDFEA18D16FE448BA4" box="[339,405,367,391]" pageId="4" pageNumber="50" value="1955-03">
III 
<number id="3DD524EDDE22FFCDFE848D09FE448BA4" box="[374,405,367,391]" pageId="4" pageNumber="50" value="55.0">55</number>
</date>
. Maxillary palps 
<number id="3DD524EDDE22FFCDFDA48D09FDA48BA4" box="[598,629,367,391]" pageId="4" pageNumber="50" value="60.0">60</number>
. Pronotum 
<number id="3DD524EDDE22FFCDFD078D09FCCE8BA4" box="[757,799,367,391]" pageId="4" pageNumber="50" value="140.0">140</number>
(
