<collectionCode id="C0CBAE32EF00FFBDFE49FD05FE0EFD2E" box="[424,482,680,703]" httpUri="http://grbio.org/cool/xqn4-34sv" name="Port Elizabeth Museum" pageId="10">PEM</collectionCode>
