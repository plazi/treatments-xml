<subSubSection id="C310656BD9735D0406A4F8F845B2F8B7" pageId="8" pageNumber="9" type="etymology">
<paragraph id="8BB536E0D9735D0406A4F8F845B2F8B7" blockId="8.[151,1437,1063,2024]" pageId="8" pageNumber="9">
<emphasis id="B97EEAF2D9735D0406A4F8F84565F8D4" bold="true" box="[199,334,1820,1844]" pageId="8" pageNumber="9">Etymology.</emphasis>
