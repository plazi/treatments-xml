<taxonomicName id="4C3068D1FFC255131F48F882A13E4444" authorityName="Melander" authorityYear="1902" box="[372,576,1842,1868]" class="Insecta" family="Mythicomyiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="348" phylum="Arthropoda" rank="family">Mythicomyiidae</taxonomicName>
