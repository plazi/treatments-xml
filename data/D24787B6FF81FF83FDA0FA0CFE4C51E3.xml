<location id="5F31607BFF87FF83FB96FB54FBF35264" LSID="urn:lsid:plazi:treatment:D24787B6FF81FF83FDA0FA0CFE4C51E3:5F31607BFF87FF83FB96FB54FBF35264" box="[1047,1091,1274,1298]" county="Parc National de la Caldera de Taburiente" municipality="Playa de Taburiente" name="The" pageId="79" pageNumber="185" stateProvince="Angeles">The</location>
