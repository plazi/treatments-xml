<collectionCode id="ED2A01F9FFEBFFE9AEC859B419DFF92E" box="[158,238,1676,1700]" country="Canada" httpUri="http://grbio.org/cool/aa64-5nmw" name="British Columbia Provincial Museum" pageId="13" pageNumber="14">BCPM</collectionCode>
