<quantity id="4CC334D9FFEBFFE9AB5E59B41C72F92E" box="[1288,1347,1676,1700]" metricMagnitude="1" metricUnit="m" metricValue="2.2" pageId="13" pageNumber="14" unit="m" value="22.0">22 m</quantity>
