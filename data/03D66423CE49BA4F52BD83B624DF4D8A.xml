<taxonomicName id="4C7FAEB6CE56BA4F52BD866824844DEA" authority="(Galileo and Martins, 2004)" baseAuthorityName="Galileo and Martins" baseAuthorityYear="2004" box="[192,718,378,402]" class="Insecta" family="Cerambycidae" genus="Adetaptera" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="15" phylum="Arthropoda" rank="species" species="dominicana">
