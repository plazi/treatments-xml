<taxonomicName id="D2A74D65ED753210152BC87CCD72F7AF" box="[339,518,2071,2091]" class="Lecanoromycetes" family="Stereocaulaceae" genus="Stereocaulon" kingdom="Fungi" order="Lecanorales" pageId="8" pageNumber="1429" phylum="Ascomycota" rank="species" species="meyeri">
