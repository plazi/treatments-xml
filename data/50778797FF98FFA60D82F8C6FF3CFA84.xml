<taxonomicName id="1FDE4D02FF98FFA70EE3FB41FB00FB17" authorityName="Mondaca &amp; Seidel" authorityYear="2019" baseAuthorityName="Mondaca &amp; Seidel" baseAuthorityYear="1971" box="[944,1220,1193,1216]" class="Insecta" family="Rutelidae" genus="Mecopelidnota" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="565" phylum="Arthropoda" rank="species" species="rabinovichi">
