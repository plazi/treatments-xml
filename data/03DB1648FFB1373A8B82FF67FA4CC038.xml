<taxonomicName id="4C72DCDDFFB1373B8F4BF8C8FA95C550" baseAuthorityName="Sluiter" baseAuthorityYear="1905" box="[1118,1356,1848,1871]" class="Ascidiacea" family="Didemnidae" genus="Lissoclinum" kingdom="Animalia" order="Aplousobranchia" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="bistratum">
