<taxonomicName id="4C72DCDDFFB1373B8853F8C8FBE9C550" authorityName="Rocha &amp; Monniot" authorityYear="1993" box="[838,1072,1848,1871]" class="Ascidiacea" family="Didemnidae" genus="Didemnum" kingdom="Animalia" order="Aplousobranchia" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="rodriguesi">
