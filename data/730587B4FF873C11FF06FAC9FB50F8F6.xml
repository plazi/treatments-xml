<subSubSection id="B3B66529FF873C11FF56F90CFAD9F866" box="[199,1356,1780,1805]" pageId="10" pageNumber="489" type="description">
<paragraph id="FB1336A2FF873C11FF56F90CFAD9F866" blockId="10.[151,1437,1564,1949]" box="[199,1356,1780,1805]" pageId="10" pageNumber="489">
<emphasis id="C9D8EAB0FF873C11FF56F90CFEBFF866" bold="true" box="[199,298,1780,1805]" pageId="10" pageNumber="489">Habitat.</emphasis>
