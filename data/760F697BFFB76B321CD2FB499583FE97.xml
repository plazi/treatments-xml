<taxonomicName id="39A6A3EEFFB76B331DDFFAD49390FAF7" authorityName="Sangster &amp; Sweet &amp; Johnson" authorityYear="2018" baseAuthorityName="Sangster &amp; Sweet &amp; Johnson" baseAuthorityYear="1856" box="[410,699,1380,1405]" class="Aves" family="Columbidae" genus="Paraclaravis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Columbiformes" pageId="2" pageNumber="136" phylum="Chordata" rank="species" species="mondetoura" status="new comb.">
<emphasis id="CCD2047FFFB76B331DDFFAD49390FAF7" box="[410,699,1380,1405]" italics="true" pageId="2" pageNumber="136">Paraclaravis mondetoura</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="D7E1B904FFB76B331E8EFAD69260FAF7" box="[715,843,1380,1405]" pageId="2" pageNumber="136">new comb.</taxonomicNameLabel>
, and 
<taxonomicName id="39A6A3EEFFB76B331FC2FAD4944BFAF7" authority="(Temminck, 1811)" authorityName="Sangster &amp; Sweet &amp; Johnson" authorityYear="2018" baseAuthorityName="Temminck" baseAuthorityYear="1811" box="[903,1376,1380,1405]" class="Aves" family="Columbidae" genus="Paraclaravis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Columbiformes" pageId="2" pageNumber="136" phylum="Chordata" rank="species" species="geoffroyi" status="new comb.">
<emphasis id="CCD2047FFFB76B331FC2FAD495ADFAF7" box="[903,1158,1380,1405]" italics="true" pageId="2" pageNumber="136">Paraclaravis geoffroyi</emphasis>
(
<bibRefCitation id="9A37A59CFFB76B3318D0FAD49473FAF7" box="[1173,1368,1380,1405]" pageId="2" pageNumber="136" refId="ref4742" refString="Temminck, C. J. (1811) Knip, P. &amp; Temminck, C. J. Les Pigeons. Livraisons 14 &amp; 15. G. Dufour, Paris, 17 pp." type="book">Temminck, 1811</bibRefCitation>
