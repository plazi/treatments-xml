<taxonomicName id="4C644D10FFB90D45FD40FDA6FACF8C33" authority="Kondratieff and Kirchner" authorityName="Kondratieff and Kirchner" box="[845,1292,592,616]" class="Insecta" family="Perlidae" genus="Perlesta" kingdom="Animalia" order="Plecoptera" pageId="6" pageNumber="37" phylum="Arthropoda" rank="species" species="roblei">
