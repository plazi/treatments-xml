<taxonomicName id="01F64D05C755952C8D8ED7752072FA1B" authority="Gonzalez and Hanley" authorityName="Gonzalez and Hanley" authorityYear="2019" box="[222,795,1401,1426]" class="Insecta" family="Coccinellidae" genus="Cryptognatha" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="10" phylum="Arthropoda" rank="species" species="kellie" status="new species">
