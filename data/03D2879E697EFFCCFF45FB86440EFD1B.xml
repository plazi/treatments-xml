<quantity id="4C839B6D697EFFCBFB47FAE643CAFAF2" box="[1173,1263,1365,1389]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="72" unit="m" value="2000.0">2000 m</quantity>
., 
<date id="FFC51048697EFFCBFB2CFAE64254FAF2" box="[1278,1393,1365,1389]" pageId="3" pageNumber="72" value="2003-03-01">
