<taxonomicName id="C11A797A33C358F7461638063FA69FDB" genus="L." lsidName="L. cincta" pageId="47" pageNumber="78" rank="species" species="cincta">L. cincta</taxonomicName>
and 
<taxonomicName id="D85C0FB79D8F21C29FD3CD5CBD28CD45" genus="L." lsidName="L. kowalewskii" pageId="47" pageNumber="78" rank="species" species="kowalewskii">L. kowalewskii</taxonomicName>
