<taxonomicName id="FE8537791A4C74B7F1ACCE48F10B8D3F" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:149254" class="Insecta" family="Formicidae" genus="Crematogaster" genus.completionEvidence="missing" genus.completionRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Crematogaster castanea inversa Forel" lsidName-HNS="Crematogaster castanea inversa Forel" order="Hymenoptera" pageNumber="62" phylum="Arthropoda" rank="species" species="inversa">inversa</taxonomicName>
