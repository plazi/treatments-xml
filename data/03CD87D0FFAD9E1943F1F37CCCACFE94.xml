<collectingCounty id="62BA4E4AFFAE9E1E4712F19CC9E6F980" box="[1140,1228,1519,1544]" pageId="10" pageNumber="362">Aligarh</collectingCounty>
, 
<collectingCountry id="F3737656FFAE9E1E47BEF19CC83FF980" box="[1240,1301,1519,1544]" name="India" pageId="10" pageNumber="362">India</collectingCountry>
