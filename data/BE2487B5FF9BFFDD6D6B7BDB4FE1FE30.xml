<taxonomicName id="F18D4D20FF9AFFDD6C917C684938FEF9" authorityName="Darby" authorityYear="2019" box="[925,1032,313,339]" class="Insecta" family="Ptiliidae" genus="Dipentium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="latum">
