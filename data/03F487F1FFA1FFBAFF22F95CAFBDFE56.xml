<emphasis id="B929EAF5FFA1FFB8FF22F95CAF86F8FF" bold="true" box="[151,463,1770,1797]" pageId="28" pageNumber="244">
<taxonomicName id="4C5D4D64FFA1FFB8FF22F95CAF29F8FF" authority="Kieffer" authorityName="Kieffer" authorityYear="1922" box="[151,352,1770,1797]" class="Insecta" family="Chironomidae" genus="Tvetenia" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="244" phylum="Arthropoda" rank="genus">
