<taxonomicName id="4C5D4D64FFA0FFB9FC7BF8B3AA3AF8E7" authorityName="Thienemann" authorityYear="1935" box="[974,1139,1796,1820]" class="Insecta" family="Chironomidae" genus="Synorthocladius" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="245" phylum="Arthropoda" rank="genus">
