<taxonomicName id="6DD84D1EDD72FF8A4E28FE13FA01FE26" box="[1386,1428,475,500]" class="Pisces" family="Cyprinidae" genus="Syn" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="28" pageNumber="53" phylum="Chordata" rank="genus">
<emphasis id="98ACEA8FDD72FF8A4E28FE13FA01FE26" box="[1386,1428,475,500]" italics="true" pageId="28" pageNumber="53">Syn</emphasis>
</taxonomicName>
. 
<taxonomicName id="6DD84D1EDD72FF8A4BD5FDCBFDBEFDCF" authority="Mus. Lugd." authorityName="Mus. Lugd." box="[151,555,515,541]" genus="Bombinator" kingdom="Animalia" pageId="28" pageNumber="53" rank="species" species="ocellatus">Bombinator ocellatus Mus. Lugd.</taxonomicName>
