<quantity id="4C879B30FFF3FF8AFA8CBC2CFF286E7D" metricMagnitude="-4" metricUnit="m" metricValue="7.9" metricValueMax="8.2" metricValueMin="7.6" pageId="8" pageNumber="581" unit="mm" value="0.79" valueMax="0.82" valueMin="0.76">0.76–0.82 mm</quantity>
