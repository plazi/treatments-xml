<quantity id="4C879B30FFF3FF8AFE1ABC2FFDA86E19" box="[442,581,1298,1322]" metricMagnitude="-3" metricUnit="m" metricValue="4.85" metricValueMax="4.9" metricValueMin="4.8" pageId="8" pageNumber="581" unit="mm" value="4.85" valueMax="4.9" valueMin="4.8">4.8–4.9 mm</quantity>
, FL: 
<quantity id="4C879B30FFF3FF8AFD23BC2CFCE26E19" box="[643,783,1297,1322]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="2.9" metricValueMin="2.6" pageId="8" pageNumber="581" unit="mm" value="2.75" valueMax="2.9" valueMin="2.6">2.6–2.9 mm</quantity>
. HW: 
<quantity id="4C879B30FFF3FF8AFCF9BC2CFBED6E19" box="[857,1024,1297,1322]" metricMagnitude="-4" metricUnit="m" metricValue="9.399999999999999" metricValueMax="9.5" metricValueMin="9.3" pageId="8" pageNumber="581" unit="mm" value="0.94" valueMax="0.95" valueMin="0.93">0.93–0.95 mm</quantity>
, PL: 
<quantity id="4C879B30FFF3FF8AFB9FBC2FFB086E19" box="[1087,1253,1298,1322]" metricMagnitude="-4" metricUnit="m" metricValue="7.7" metricValueMax="8.0" metricValueMin="7.4" pageId="8" pageNumber="581" unit="mm" value="0.77" valueMax="0.8" valueMin="0.74">0.74–0.80 mm</quantity>
