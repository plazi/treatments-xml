<specimenCount id="9D1886D1C0296E65FEBEFB556EDDE4A3" box="[339,439,1236,1260]" pageId="20" pageNumber="345" type="generic">1 specimen</specimenCount>
(
<collectionCode id="ED0FD59DC0296E65FE2CFB556D7BE4A3" box="[449,529,1236,1260]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="20" pageNumber="345">MNHN</collectionCode>
