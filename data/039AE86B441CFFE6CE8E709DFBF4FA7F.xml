<taxonomicName id="4C3322FE441DFFE5CB7F7160FEF3F841" authorityName="Sartori &amp; Peters &amp; Hubbard" authorityYear="2008" class="Insecta" family="Teloganodidae" genus="Teloganodes" kingdom="Animalia" order="Ephemeroptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="jacobusi">
