<taxonomicName id="27CA4D35FF80FFE65A73F9F2FC3067E0" authorityName="Rondani" authorityYear="1840" box="[621,899,1663,1689]" class="Insecta" family="Cecidomyiidae" genus="Brachineura" kingdom="Animalia" order="Diptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="fuscogrisea">
