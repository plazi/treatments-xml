(
<specimenCount id="F6CCFD3FFF80FFE65D50F94AFAD76798" box="[1358,1380,1735,1761]" pageId="77" pageNumber="78" type="male">♂</specimenCount>
) and 
<typeStatus id="3F718814FF80FFE658D9F966FEED667C" box="[199,350,1771,1797]" pageId="77" pageNumber="78">paralectotype</typeStatus>
(
<specimenCount id="F6CCFD3FFF80FFE65973F966FE30667C" box="[365,387,1771,1797]" pageId="77" pageNumber="78" type="male">♂</specimenCount>
) but did not mention an additional 
<specimenCount id="F6CCFD3FFF80FFE65B1BF966FC19667C" box="[773,938,1771,1797]" pageId="77" pageNumber="78" type="generic">two specimens</specimenCount>
(sex not determinable) in the OUMNH (both of these additional specimens are 
<typeStatus id="3F718814FF80FFE65A5CF882FD5A6653" box="[578,745,1807,1834]" pageId="77" pageNumber="78">paralectotypes</typeStatus>
).
