<quantity id="0940E8EDFF84FFFCC220FC0CFDCBD783" box="[519,628,936,962]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" pageId="14" pageNumber="59" unit="mm" value="2.25">2.25 mm</quantity>
