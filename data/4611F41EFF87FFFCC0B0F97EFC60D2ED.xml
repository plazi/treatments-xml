<number id="7870572FFF84FFFCC3ABFC26FC15D7DD" box="[908,938,898,924]" pageId="14" pageNumber="59" value="15.0">15</number>
cmm; pronotal length 
<number id="7870572FFF84FFFCC4E4FC26FB5ED7DD" box="[1219,1249,898,924]" pageId="14" pageNumber="59" value="92.0">92</number>
