<subSubSection id="A9946559FFC63B4BFF5FF92FA26E8726" lastPageId="34" lastPageNumber="441" pageId="33" pageNumber="440" type="diagnosis">
<paragraph id="E13136D2FFC63B48FF5FF92FA57E86C4" blockId="33.[204,1436,1726,1785]" pageId="33" pageNumber="440">
<emphasis id="D3FAEAC0FFC63B48FF5FF92FA66C86EB" bold="true" box="[204,320,1727,1750]" pageId="33" pageNumber="440">DIAGNOSIS</emphasis>
