<taxonomicName id="4C22AFCEFFD0B9003B2919DA2DB2BF8F" box="[1068,1225,188,212]" class="Actinopterygii" family="Claroteidae" genus="Notoglanidium" kingdom="Animalia" order="Siluriformes" pageId="18" pageNumber="183" phylum="Chordata" rank="species" species="maculatum">
