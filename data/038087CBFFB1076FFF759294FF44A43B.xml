<taxonomicName id="4C294D5EFFB10750FDDF9379FD01A6BC" box="[573,648,1904,1930]" class="Insecta" family="Gracillariidae" genus="Philodoria" kingdom="Animalia" order="Lepidoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="funkae">
