<taxonomicName id="4E24C21EFC0C3D7C9DF1FEE315C4C6FC" class="Insecta" family="Dolichopodidae" genus="Sinosciapus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinosciapus tianmushanus" order="Diptera" pageId="1" pageNumber="12" phylum="Arthropoda" rank="species" species="tianmushanus">Sinosciapus tianmushanus</taxonomicName>
