<taxonomicName id="37F742F7FF4AAE4B3103F6BFFBB83E9A" box="[833,1120,975,999]" class="Insecta" family="Rhinotermitidae" genus="Reticulitermes" kingdom="Animalia" order="Blattodea" pageId="19" pageNumber="1097" phylum="Arthropoda" rank="species" species="assamensis">
<emphasis id="C283E566FF4AAE4B3103F6BFFBB83E9A" box="[833,1120,975,999]" italics="true" pageId="19" pageNumber="1097">Reticulitermes assamensis</emphasis>
