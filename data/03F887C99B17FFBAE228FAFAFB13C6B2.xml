<subSubSection id="C34B65549B14FFBAE218FE9DFE50C612" pageId="51" pageNumber="52" type="etymology">
<paragraph id="8BEE36DF9B14FFBAE218FE9DFE50C612" blockId="51.[264,1323,283,510]" pageId="51" pageNumber="52">
<emphasis id="B925EACD9B14FFBAE218FE9DFE04C679" bold="true" box="[312,452,283,309]" pageId="51" pageNumber="52">Etymology.</emphasis>
Named 
<emphasis id="B925EACD9B14FFBAE107FE9AFDA8C679" box="[551,616,284,309]" italics="true" pageId="51" pageNumber="52">
<taxonomicName id="4C514D5C9B14FFBAE107FE9AFDA3C679" authorityName="Ahyong &amp; Poore" authorityYear="2004" box="[551,611,284,309]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="latus">latus</taxonomicName>
