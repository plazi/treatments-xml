<collectionCode id="3ECAAE38FFCDFFF4E761F9617A6FF8F2" box="[521,566,1770,1794]" country="USA" httpUri="http://grbio.org/cool/fx78-973d" name="The University of Iowa (formerly State University of Iowa)" pageId="26" pageNumber="27" type="University or college">SUI</collectionCode>
