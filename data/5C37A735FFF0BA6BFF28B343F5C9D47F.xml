<taxonomicName id="139E6DA0FFF0BA6BFDC1B229F5CED47F" box="[638,728,1365,1388]" class="Gastropoda" family="Orthalicidae" genus="Dryptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="10" pageNumber="11" phylum="Mollusca" rank="genus">
