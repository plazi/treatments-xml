<subSubSection id="C19A656DFF8E8E68FF2C79B224E18630" pageId="2" pageNumber="583" type="etymology">
<paragraph id="893F36E6FF8E8E68FF2C79B224E18630" blockId="2.[151,1437,152,1284]" pageId="2" pageNumber="583">
<emphasis id="BBF4EAF4FF8E8E68FF2C79B2255D81CE" bold="true" box="[199,320,984,1007]" pageId="2" pageNumber="583">Etymology</emphasis>
