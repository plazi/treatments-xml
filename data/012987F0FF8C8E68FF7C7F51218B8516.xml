<taxonomicName id="4E804D65FF8E8E68FC1F7C42208B841E" box="[1012,1174,1575,1599]" class="Insecta" family="Cerambycidae" genus="Heterachthes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="583" phylum="Arthropoda" rank="species" species="signaticollis">
