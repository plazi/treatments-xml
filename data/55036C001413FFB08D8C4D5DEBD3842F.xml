<heading id="865D6A7A1413FFB28D8C4D5DE86385A5" bold="true" box="[151,835,874,900]" fontSize="11" level="1" pageId="1" pageNumber="58" reason="1">
<emphasis id="EFDE01041413FFB28D8C4D5DE86385A5" bold="true" box="[151,835,874,900]" pageId="1" pageNumber="58">
<taxonomicName id="1AAAA6951413FFB28D8C4D5DE98685A5" authority="Angrisano &amp; Sganga" authorityName="Angrisano &amp; Sganga" box="[151,678,874,900]" class="Insecta" family="Hydroptilidae" genus="Alisotrichia" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="58" phylum="Arthropoda" rank="species" species="cainguas" status="new species">
<emphasis id="EFDE01041413FFB28D8C4D5DEAB985A5" bold="true" box="[151,409,874,900]" italics="true" pageId="1" pageNumber="58">Alisotrichia cainguas</emphasis>
Angrisano &amp; Sganga
</taxonomicName>
, 
<taxonomicNameLabel id="F4EDBC7F1413FFB28FA84D5DE86385A5" box="[691,835,874,900]" pageId="1" pageNumber="58" rank="species">new species</taxonomicNameLabel>
</emphasis>
