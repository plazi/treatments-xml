<subSubSection id="95B08E9D1411FFB08DDE4FFAEBD3842F" pageId="3" pageNumber="60" type="etymology">
<paragraph id="DD15DD161411FFB08DDE4FFAEBD3842F" blockId="3.[151,1437,152,526]" pageId="3" pageNumber="60">
<emphasis id="EFDE01041411FFB08DDE4FFAEA7687C6" bold="true" box="[197,342,461,487]" pageId="3" pageNumber="60">Etymology.</emphasis>
