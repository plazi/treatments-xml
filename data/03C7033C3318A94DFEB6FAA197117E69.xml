<subSubSection id="C374E1A13318A94CFEB6FA1B916C7B81" pageId="6" pageNumber="7" type="materials_examined">
<paragraph id="8BD1B22A3318A94CFEB6FA1B969D78E1" blockId="6.[264,1323,1492,1878]" box="[264,1012,1492,1518]" pageId="6" pageNumber="7">
<emphasis id="B91A6E383318A94CFEB6FA1B94C378E1" bold="true" box="[264,426,1492,1518]" pageId="6" pageNumber="7">
<typeStatus id="54D50C883318A94CFEB6FA1B942C78E1" box="[264,325,1492,1518]" pageId="6" pageNumber="7">Type</typeStatus>
