<quantity id="4CE6F2D3147DFFC0FCBCFE2B6F03C5C6" box="[799,890,429,452]" metricMagnitude="-4" metricUnit="m" metricValue="9.5" pageId="1" pageNumber="34" unit="mm" value="0.95">0.95mm</quantity>
