<taxonomicName id="4C4D4D06FFE6FF89FF7EF92ABB89BF1C" authority="Barneby &amp; Fortunato" authorityName="Barneby &amp; Fortunato" box="[198,671,1780,1807]" class="Magnoliopsida" family="Fabaceae" genus="Mimosa" higherTaxonomySource="IPNI" kingdom="Plantae" order="Fabales" pageId="11" pageNumber="212" phylum="Magnoliophyta" rank="species" species="chacoensis">
