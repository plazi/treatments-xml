<taxonomicName id="B86EB5B4CA6CEA3E0AA7CC718B7CECF1" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:178489" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus maculatus agricola Forel" lsidName-HNS="Camponotus maculatus agricola Forel" order="Hymenoptera" pageNumber="454" phylum="Arthropoda" rank="subSpecies" species="maculatus" subSpecies="agricola">Camponotus maculatus subsp. Agricola</taxonomicName>
