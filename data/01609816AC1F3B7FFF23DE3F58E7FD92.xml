<materialsCitation id="39A1235DAC1F3B78FE18DF7458BCF8E4" box="[457,1279,1841,1861]" collectingDate="1956-10-30" location="Gbakore" pageId="83" pageNumber="476" specimenCount="1" stateProvince="Nimba">
