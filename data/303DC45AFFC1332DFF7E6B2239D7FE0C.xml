<paragraph id="B82B754CFFC1332CFF7E6B223B80FABD" blockId="8.[178,433,1314,1336]" box="[178,433,1314,1336]" pageId="8" pageNumber="1615">
<heading id="E363C220FFC1332CFF7E6B223B80FABD" box="[178,433,1314,1336]" fontSize="8" level="2" pageId="8" pageNumber="1615" reason="2">
<emphasis id="8AE0A95EFFC1332CFF7E6B223B80FABD" bold="true" box="[178,433,1314,1336]" pageId="8" pageNumber="1615">
<taxonomicName id="7F940ECFFFC1332CFF7E6B223B6FFABD" authority="Malicky, 2014" authorityName="Malicky" authorityYear="2014" box="[178,350,1314,1336]" class="Insecta" family="Psychomyiidae" genus="Tinodes" kingdom="Animalia" order="Trichoptera" pageId="8" pageNumber="1615" phylum="Arthropoda" rank="species" species="xanoryz" status="nov.sp.">
<emphasis id="8AE0A95EFFC1332CFF7E6B223B6FFABD" bold="true" box="[178,350,1314,1336]" italics="true" pageId="8" pageNumber="1615">Tinodes xanoryz</emphasis>
</taxonomicName>
<taxonomicNameLabel id="91D31425FFC1332CFEA86B223B80FABD" box="[356,433,1314,1335]" pageId="8" pageNumber="1615" rank="species">nov.sp.</taxonomicNameLabel>
