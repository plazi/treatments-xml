<caption id="1BFF66354E44666CFF64F939FEF26A8B" ID-DOI="http://doi.org/10.5281/zenodo.5323752" ID-Zenodo-Dep="5323752" httpUri="https://zenodo.org/record/5323752/files/figure.png" pageId="4" pageNumber="384" startId="4.[151,255,1682,1706]" targetBox="[190,1409,168,1671]" targetPageId="4">
<paragraph id="4F3F36BD4E44666CFF64F939FEF26A8B" blockId="4.[151,1435,1682,1740]" pageId="4" pageNumber="384">
<emphasis id="7DF4EAAF4E44666CFF64F939FECB6AED" bold="true" box="[151,282,1682,1706]" pageId="4" pageNumber="384">FIGURE 3.</emphasis>
<taxonomicName id="88804D3E4E44666CFED2F938FE026AED" authorityName="Azman" authorityYear="2009" box="[289,467,1683,1706]" class="Malacostraca" family="Cyproideidae" genus="Cyproidea" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="384" phylum="Arthropoda" rank="species" species="cobia" status="sp. nov.">
<emphasis id="7DF4EAAF4E44666CFED2F938FE026AED" box="[289,467,1683,1706]" italics="true" pageId="4" pageNumber="384">Cyproidea cobia</emphasis>
</taxonomicName>
<emphasis id="7DF4EAAF4E44666CFE2FF939FDE36AED" bold="true" box="[476,562,1682,1706]" pageId="4" pageNumber="384">
<taxonomicNameLabel id="66C757D44E44666CFE2FF939FDE36AED" box="[476,562,1682,1706]" pageId="4" pageNumber="384" rank="species">sp. nov.</taxonomicNameLabel>
