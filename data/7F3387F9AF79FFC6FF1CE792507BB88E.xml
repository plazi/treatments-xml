<number id="415224C8AF78FFC6FC63E6EA57F1BE86" box="[1000,1015,943,968]" pageId="7" pageNumber="408" value="5.0">5</number>
), 
<number id="415224C8AF78FFC6FB86E6F5502ABE86" box="[1037,1068,944,968]" pageId="7" pageNumber="408" value="10.0">10</number>
(
<number id="415224C8AF78FFC6FBBFE6F55056BE86" box="[1076,1104,944,968]" pageId="7" pageNumber="408" value="11.0">11</number>
)*, 
<number id="415224C8AF78FFC6FBF8E6F55089BE86" box="[1139,1167,944,968]" pageId="7" pageNumber="408" value="11.0">11</number>
(
<number id="415224C8AF78FFC6FB13E6F550A0BE86" box="[1176,1190,944,968]" pageId="7" pageNumber="408" value="3.0">3</number>
