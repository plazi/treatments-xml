(
<figureCitation id="9E902A134971FFE661293C402E63B448" box="[273,355,319,345]" captionStart="FIGURES 5 – 8" captionStartId="5.[264,382,1350,1374]" captionTargetBox="[550,1216,327,1350]" captionTargetId="figure@5.[550,1216,315,1350]" captionTargetPageId="5" captionText="FIGURES 5 – 8. Lateral silhouettes of Clavicornaltica spp. 5, C. lonsheng. 6, C. dali. 7, C. tamdao. 8, C. vietnamensis." httpUri="https://zenodo.org/record/169754/files/figure.png" pageId="12" pageNumber="61">Figs. 5</figureCitation>
, 
<figureCitation id="9E902A134971FFE661483C402EBCB448" box="[368,444,319,345]" captionStart="FIGURES 9 – 13" captionStartId="6.[264,382,1788,1812]" captionTargetBox="[264,1322,601,1776]" captionTargetId="figure@6.[264,1323,582,1786]" captionTargetPageId="6" captionText="FIGURES 9 – 13. Ventral views of Clavicornaltica legs and sternal structures. 9, meso­ and metasterna of C. dali. 10, meso­ and metasterna of C. longsheng. 11, 12, 13, ventral view of leg and abdomen of C. tamdao." httpUri="https://zenodo.org/record/169755/files/figure.png" pageId="12" pageNumber="61">10, 12</figureCitation>
, 
<figureCitation id="9E902A134971FFE661F03C402D11B448" box="[456,529,319,345]" captionStart="FIGURES 27 – 31" captionStartId="11.[264,383,1826,1850]" captionTargetBox="[270,1319,480,1810]" captionTargetId="figure@11.[264,1323,472,1822]" captionTargetPageId="11" captionText="FIGURES 27 – 31. Female genitalia and abdomen, Clavicornaltica longsheng. 27, spermatheca. 28, vaginal palpi. 29, tergum VII, dorsal view. 30, abdomen, ventral view. 31, tignum." httpUri="https://zenodo.org/record/169760/files/figure.png" pageId="12" pageNumber="61">
27– 
<number id="B06324B14971FFE661CB3C402D11B448" box="[499,529,319,345]" pageId="12" pageNumber="61" value="31.0">31</number>
