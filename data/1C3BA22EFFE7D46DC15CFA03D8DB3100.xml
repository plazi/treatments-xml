<collectingCountry id="EC8553A8FFE7D46DC139F7DADAAF3A55" box="[327,386,2055,2078]" name="Japan" pageId="4" pageNumber="93">Japan</collectingCountry>
, 
<collectingCountry id="EC8553A8FFE7D46DC1F5F7DADAFB3A54" box="[395,470,2055,2079]" name="Taiwan" pageId="4" pageNumber="93">Taiwan</collectingCountry>
