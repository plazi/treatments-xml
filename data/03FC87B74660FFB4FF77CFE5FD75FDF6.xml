<subSubSection id="C34F652A4660FFB7FF77CE5CFC4EFC9D" box="[151,935,831,856]" pageId="1" pageNumber="58" type="materials_examined">
<paragraph id="8BEA36A14660FFB7FF77CE5CFC4EFC9D" blockId="1.[151,1437,831,2008]" box="[151,935,831,856]" pageId="1" pageNumber="58">
<emphasis id="B921EAB34660FFB7FF77CE5CFEDBFC92" bold="true" box="[151,306,831,856]" pageId="1" pageNumber="58">
