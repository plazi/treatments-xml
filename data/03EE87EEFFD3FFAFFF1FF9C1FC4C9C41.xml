<taxonomicName id="4C474D7BFFD6FFAEFEADF861FD879EF3" authority="Mühlenhardt-Siegel, 2011" authorityName="Mühlenhardt-Siegel" authorityYear="2011" box="[293,543,1960,1982]" class="Insecta" family="Curculionidae" genus="Leucon" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="weddelensis" status="sp. n." subGenus="Crymoleucon">
