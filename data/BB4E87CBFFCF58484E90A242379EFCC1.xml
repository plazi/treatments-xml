<paragraph id="335836DDFFCF58484E90A157379EFD21" blockId="11.[151,1436,426,852]" pageId="11" pageNumber="66">
<keyLead id="881695E8FFCF58484E90A157379EFD21" pageId="11" pageNumber="66">
<number id="852F24FAFFCF58484E90A15732AEFDF1" box="[151,169,586,612]" pageId="11" pageNumber="66" value="2.0">2</number>
