<taxonomicName id="4C294D361800F32C51B5799E63FA9EDA" authority="A. Milne-Edwards, 1880" authorityName="A. Milne-Edwards" authorityYear="1880" box="[151,681,151,176]" class="Malacostraca" family="Galatheidae" genus="Galacantha" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="404" phylum="Arthropoda" rank="species" species="rostrata">
