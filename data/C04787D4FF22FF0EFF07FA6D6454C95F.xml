<taxonomicName id="8FEE4D41FF22FF09FF57FA6D6728CEA2" authority="Koch, 1844 a" authorityName="Koch" authorityYear="1844" box="[199,619,1482,1509]" class="Arachnida" family="Ixodidae" genus="Hyalomma" kingdom="Animalia" order="Ixodida" pageId="203" pageNumber="204" phylum="Arthropoda" rank="species" species="excavatum">
