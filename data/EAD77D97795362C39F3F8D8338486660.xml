<taxonomicName id="527D1941FB960152833CA8137E20E143" class="Agaricomycetes" family="Russulaceae" genus="Russula" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Russula capensis" order="Russulales" pageId="5" pageNumber="30" phylum="Basidiomycota" rank="species" species="capensis">Russula capensis</taxonomicName>
A. Pearson (
<taxonomicName id="5EA84E50866A9F92B13557282C852880" family="Russulaceae" lsidName="" pageId="5" pageNumber="30" rank="family">Russulaceae</taxonomicName>
