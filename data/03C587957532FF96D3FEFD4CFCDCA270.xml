<taxonomicName id="4C6C4D007532FF96D0FCFB18FCB6A0F4" box="[745,881,1166,1189]" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" kingdom="Plantae" order="Rosales" pageId="19" pageNumber="501" phylum="Tracheophyta" rank="species" species="communis">
