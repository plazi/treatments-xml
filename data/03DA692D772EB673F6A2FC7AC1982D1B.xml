<location id="8EAC8EE07723B673F5B7FA9BC6182D1B" LSID="urn:lsid:plazi:treatment:03DA692D772EB673F6A2FC7AC1982D1B:8EAC8EE07723B673F5B7FA9BC6182D1B" box="[682,772,1287,1311]" country="China" name="Kashgar" pageId="10" pageNumber="1442" stateProvince="Xinjiang Uygur">Kashgar</location>
