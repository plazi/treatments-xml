<materialsCitation id="3B1BD2667723B673F5CFFB78C19C2D1B" collectorName="We" country="China" location="Kashgar" pageId="10" pageNumber="1442" specimenCount="1" specimenCount-female="1" stateProvince="Xinjiang Uygur">
<collectorName id="2686BDED7723B673F5CFFB78C7EA2CF8" box="[722,758,1252,1276]" pageId="10" pageNumber="1442">We</collectorName>
