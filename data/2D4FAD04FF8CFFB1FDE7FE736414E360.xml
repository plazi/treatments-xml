<geoCoordinate id="C0D27AD5FF8CFFB3FDDBF8EC641FE25A" box="[608,772,1804,1831]" degrees="143" direction="east" minutes="56" orientation="longitude" pageId="13" pageNumber="14" precision="15" seconds="35" value="143.94305">143º56’35” E</geoCoordinate>
