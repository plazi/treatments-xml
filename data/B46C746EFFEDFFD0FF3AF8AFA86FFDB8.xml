, 
<collectingCountry id="44D285E8FFEAFFD0FC4BFE69A92FFE70" box="[998,1111,444,468]" name="Colombia" pageId="4" pageNumber="41">Colombia</collectingCountry>
, 
<collectingCountry id="44D285E8FFEAFFD0FBC5FE69A993FE70" box="[1128,1259,444,468]" name="Costa Rica" pageId="4" pageNumber="41">Costa Rica</collectingCountry>
