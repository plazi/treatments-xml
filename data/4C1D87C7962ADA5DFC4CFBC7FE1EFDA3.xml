<taxonomicName id="03B44D529629DA5DFB56FF57FA84FF72" authorityName="Janossy" authorityYear="1977" box="[1205,1363,169,192]" class="Aves" family="Strigidae" genus="Surnia" kingdom="Animalia" order="Strigiformes" pageId="22" pageNumber="179" phylum="Chordata" rank="species" species="robusta">
