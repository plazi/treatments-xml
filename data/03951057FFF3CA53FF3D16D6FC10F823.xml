<elevation id="00114672FFF3CA53FB7417BEFAE2F81E" box="[1246,1398,1972,1997]" metricMagnitude="2" metricUnit="m" metricValue="9.86" metricValueMax="18.13" metricValueMin="1.59" pageId="58" pageNumber="59" unit="m" value="986.0" valueMax="1813.0" valueMin="159.0">
