<taxonomicName id="4C3CDAC2FFF3CA53FF3D170AFCD4F8CA" authority="Takaoka &amp; Davies, 1995: 132" authorityName="Takaoka &amp; Davies" authorityPageNumber="132" authorityYear="1995" box="[151,832,1791,1816]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="brevipar" subGenus="Simulium">
