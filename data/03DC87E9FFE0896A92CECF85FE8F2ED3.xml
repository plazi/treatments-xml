species: 
<taxonomicName id="4C754D7CFFE0896B937FCC07FD922E5A" authority="Hampson, 1914" authorityName="Hampson" authorityYear="1914" box="[294,617,1582,1606]" class="Insecta" family="Erebidae" genus="Ilema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="65" phylum="Arthropoda" rank="species" species="proleuca">
