<subSubSection id="9C8B6553FFC2BB542FA52B4D0BA6F8C3" box="[151,684,1842,1869]" pageId="18" pageNumber="391" type="materials_examined">
<paragraph id="D42E36D8FFC2BB542FA52B4D0BA6F8C3" blockId="18.[151,1436,1842,2013]" box="[151,684,1842,1869]" pageId="18" pageNumber="391">
