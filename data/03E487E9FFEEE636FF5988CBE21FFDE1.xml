</emphasis>
<taxonomicName id="4C4D4D7CFFEEE635FEE58B0DE2ABF9F1" authority="Richardson, 2013" authorityName="Richardson" authorityYear="2013" box="[299,650,1559,1582]" class="Arachnida" family="Salticidae" genus="Pungalina" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Araneae" pageId="29" pageNumber="530" phylum="Arthropoda" rank="species" species="weiri">
