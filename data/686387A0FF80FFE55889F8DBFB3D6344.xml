: Box 65): 
<emphasis id="D2BEEAA4FF83FFE55A73FF36FD3D61AD" box="[621,654,187,212]" italics="true" pageId="78" pageNumber="79">Br.</emphasis>
[= 
<taxonomicName id="27CA4D35FF83FFE55AAAFF36FCB661AD" box="[692,773,187,212]" class="Insecta" family="Cecidomyiidae" genus="Bremia" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="genus">
<emphasis id="D2BEEAA4FF83FFE55AAAFF36FCB661AD" box="[692,773,187,212]" italics="true" pageId="78" pageNumber="79">Bremia</emphasis>
</taxonomicName>
] 
<taxonomicName id="27CA4D35FF83FFE55B08FF36FC2461AC" authorityName="Rondani" authorityYear="1848" box="[790,919,187,213]" class="Insecta" family="Cecidomyiidae" genus="Cecidomya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="78" pageNumber="79" phylum="Arthropoda" rank="species" species="aphidimyza">
