<taxonomicName id="4C5539E0FFA1D34A841F2961E698FD82" authority="(VanhoVen, 1914)" baseAuthorityName="VanhoVen" baseAuthorityYear="1914" class="Malacostraca" family="Santiidae" genus="Santia" kingdom="Animalia" order="Isopoda" pageId="23" pageNumber="1443" phylum="Arthropoda" rank="species" species="hispida">
