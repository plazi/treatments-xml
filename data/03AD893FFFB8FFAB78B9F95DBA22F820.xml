<taxonomicName id="4C0443AAFFB8FFAB7A85F816BA1DF81C" authority="Berlese 1887" authorityName="Berlese" authorityYear="1887" box="[683,1120,1972,1997]" class="Arachnida" family="Laelapidae" genus="Haemolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="casalis">
