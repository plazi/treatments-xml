<taxonomicName id="4C764D47FFD7AB4E9CAC13B745E63B8F" baseAuthorityName="Wirth and Blanton" baseAuthorityYear="1959" box="[755,955,914,940]" class="Insecta" family="Ceratopogonidae" kingdom="Animalia" order="Diptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="family">Ceratopogonidae</taxonomicName>
