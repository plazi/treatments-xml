<taxonomicName id="D2854D62FFAF404FFD0FF82C3D6BC288" box="[676,793,2011,2035]" class="Insecta" family="Hymenoptera" genus="Omalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="69" phylum="Arthropoda" rank="species" species="timidus">
