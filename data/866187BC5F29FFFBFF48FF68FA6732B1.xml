<collectingCountry id="76DF763A5F29FFFBFEFAFDC3FE6D3224" box="[293,407,572,598]" name="Argentina" pageId="9" pageNumber="544">Argentina</collectingCountry>
, 
<collectingCountry id="76DF763A5F29FFFBFE7CFDC3FE173224" box="[419,493,572,598]" name="Belize" pageId="9" pageNumber="544">Belize</collectingCountry>
, 
<collectingCountry id="76DF763A5F29FFFBFE26FDC3FDB43224" box="[505,590,572,598]" name="Bolivia" pageId="9" pageNumber="544">Bolivia</collectingCountry>
, 
<collectingCountry id="76DF763A5F29FFFBFD85FDC3FD5B3224" box="[602,673,572,598]" name="Brazil" pageId="9" pageNumber="544">Brazil</collectingCountry>
, 
<collectingCountry id="76DF763A5F29FFFBFD72FDC3FCE43224" box="[685,798,572,598]" name="Colombia" pageId="9" pageNumber="544">Colombia</collectingCountry>
, 
<collectingCountry id="76DF763A5F29FFFBFCF5FDC2FC5D3224" box="[810,935,572,599]" name="Costa Rica" pageId="9" pageNumber="544">Costa Rica</collectingCountry>
