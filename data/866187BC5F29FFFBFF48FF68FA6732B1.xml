<collectingCountry id="76DF763A5F29FFFBFC6CFDC3FBE83224" box="[947,1042,572,598]" name="Ecuador" pageId="9" pageNumber="544">Ecuador</collectingCountry>
, 
<collectingCountry id="76DF763A5F29FFFBFBC1FDC3FB593224" box="[1054,1187,572,598]" name="El Salvador" pageId="9" pageNumber="544">El Salvador</collectingCountry>
