Fig. 
<number id="3DF12959FFF5FFECFEB8FA381CBA2ECC" box="[336,411,1409,1432]" pageId="5" pageNumber="66" value="15.5" valueMax="18.0" valueMin="13.0">13–18</number>
. 
<taxonomicName id="4C3940FDFFF5FFECFE4FFA381F492ECC" box="[423,616,1409,1432]" class="Insecta" family="Hydrophilidae" genus="Hemiosus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="66" phylum="Arthropoda" rank="species" species="frades" status="sp. nov.">
<emphasis id="B94DE76CFFF5FFECFE4FFA381F492ECC" box="[423,616,1409,1432]" italics="true" pageId="5" pageNumber="66">Hemiosus frades</emphasis>
</taxonomicName>
, 
<emphasis id="B94DE76CFFF5FFECFD9FFA381FF42ECD" bold="true" box="[631,725,1409,1433]" pageId="5" pageNumber="66">
<taxonomicNameLabel id="A27E5A17FFF5FFECFD9FFA381FF42ECD" box="[631,725,1409,1433]" pageId="5" pageNumber="66" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
