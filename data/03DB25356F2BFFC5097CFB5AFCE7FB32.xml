<paragraph id="8BCD94236F2BFFC60BFCF910FCD1F916" blockId="85.[189,1399,1709,1911]" box="[189,865,1744,1770]" pageId="85" pageNumber="86">
<materialsCitation id="3B1A9E7E6F2BFFC60BFCF910FCEDF916" ID-GBIF-Occurrence="2382959834" box="[189,861,1744,1770]" collectionCode="MNBG" collectorName="v. Nolcken" country="Colombia" location="COLOMBIA" pageId="85" pageNumber="86" specimenCount="1" specimenCount-female="1" typeStatus="paralectotype">
<collectingCountry id="F365D4B36F2BFFC60BFCF910FEE8F916" box="[189,344,1744,1770]" name="Colombia" pageId="85" pageNumber="86">COLOMBIA</collectingCountry>
• 
<specimenCount id="9D745FAA6F2BFFC60A30F910FE38F916" box="[369,392,1744,1770]" pageId="85" pageNumber="86" type="female">♀</specimenCount>
; “n. Granada”; 
<collectorName id="2687F1F56F2BFFC60901F910FD0FF916" box="[576,703,1744,1770]" pageId="85" pageNumber="86">v. Nolcken</collectorName>
leg.; 
