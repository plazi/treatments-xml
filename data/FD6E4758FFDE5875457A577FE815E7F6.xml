<taxonomicName id="B2C78DCDFFDE5875457A577FE805E475" authority="Simroth, 1893" authorityName="Simroth" authorityYear="1893" box="[264,624,283,310]" class="Solenogastres" family="Rhopalomeniidae" genus="Rhopalomenia" kingdom="Animalia" order="Cavibelonia" pageId="2" pageNumber="45" phylum="Mollusca" rank="genus">
<emphasis id="47B32A5CFFDE5875457A577FE805E475" bold="true" box="[264,624,283,310]" pageId="2" pageNumber="45">
