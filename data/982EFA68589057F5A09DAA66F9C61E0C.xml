<normalizedToken id="8565E69FE84DA8D5E85D60B7DC8B6B9B" originalValue="μm">μm</normalizedToken>
(
<emphasis id="A7F41C26F4DE59B5824A602509BC1662" bold="true" pageId="0" pageNumber="81">D, E, F, G</emphasis>
); 200 
<normalizedToken id="2135A32AAAC475C58D2B8E0B71C843F5" originalValue="μm">μm</normalizedToken>
(
<emphasis id="6C14D0E6E53E01739F775D0C50D05065" bold="true" pageId="0" pageNumber="81">H</emphasis>
