<taxonomicName id="8A0F412FA999DBE113E4311003AFD7EF" class="Arachnida" family="Araneidae" genus="Araneus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Araneus granti" order="Araneae" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="granti">Araneus granti</taxonomicName>
