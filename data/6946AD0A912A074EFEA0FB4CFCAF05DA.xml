<taxonomicName id="26EF679F912A074EFC9AFB4CFE840537" authority="Hille Ris Lambers 1947" authorityName="Hille Ris Lambers" authorityYear="1947" class="Insecta" family="Aphididae" genus="Chaetosiphella" kingdom="Animalia" order="Aphidomorpha" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="stipae">
<emphasis id="D39BC00E912A074EFC9AFB4CFBF00502" bold="true" box="[818,1079,1259,1285]" italics="true" pageId="9" pageNumber="10">Chaetosiphella stipae</emphasis>
Hille Ris Lambers 
<number id="57270E3B912A074EFEA0FAB1FE840537" box="[264,323,1302,1328]" pageId="9" pageNumber="10" value="1947.0">1947</number>
