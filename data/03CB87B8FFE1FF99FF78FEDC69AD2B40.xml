, and 
<taxonomicName id="4C624D2DFFE1FF99FB1AFCDF6E832C0F" box="[1269,1429,869,893]" class="Insecta" family="Veliidae" genus="Paravelia" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="hungerfordi">
