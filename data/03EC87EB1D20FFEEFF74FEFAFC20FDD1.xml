<taxonomicName id="4C454D7E1D20FFEEFF74FDCCFE35FD89" baseAuthorityName="Walker" baseAuthorityYear="2009" box="[178,406,527,548]" class="Insecta" family="Eumenidae" genus="Antepipona" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="928" phylum="Arthropoda" rank="species" species="cingulifer">
