<quantity id="F06B9B13FFFBFFC5FB29FA651F0DFA75" box="[1248,1331,1416,1443]" metricMagnitude="5" metricUnit="m" metricValue="2.5" pageId="3" pageNumber="98" unit="km" value="250.0">250 km</quantity>
