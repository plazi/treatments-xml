<taxonomicName id="192C5A3BFD56429D44B77B9814E02B72" class="Insecta" family="Pteromalidae" genus="Dibrachys" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dibrachys yunnanensis" order="Hymenoptera" pageId="8" pageNumber="131" phylum="Arthropoda" rank="species" species="yunnanensis">Dibrachys yunnanensis</taxonomicName>
