<taxonomicName id="4C1A4D5DFE3EFFA8FDDD39223612CF45" authorityName="Clarke" authorityYear="2015" box="[619,810,1379,1403]" class="Insecta" family="Cerambycidae" genus="Dihammaphora" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="11" phylum="Arthropoda" rank="species" species="pilcomayoensis">
