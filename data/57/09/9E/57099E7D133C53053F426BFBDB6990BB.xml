<subSubSection pageId="61" pageNumber="62" type="nomenclature">
<paragraph pageId="61" pageNumber="62">
<taxonomicName LSID="http://zoobank.org/E271D0F5-AFF8-4E01-ADDC-1EA00F1B0A85" authority="Hansson" class="Insecta" family="Eulophidae" genus="Euplectrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euplectrus lubomirmasneri" order="Hymenoptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="lubomirmasneri">Euplectrus lubomirmasneri Hansson</taxonomicName>
