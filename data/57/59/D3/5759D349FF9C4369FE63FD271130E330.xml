m in a 
<quantity box="[917,990,362,386]" metricMagnitude="-2" metricUnit="m" metricValue="3.7" pageId="15" pageNumber="1706" unit="mm" value="37.0">37 mm</quantity>
high 
<typeStatus box="[93,193,395,419]" pageId="15" pageNumber="1706">paratype</typeStatus>
