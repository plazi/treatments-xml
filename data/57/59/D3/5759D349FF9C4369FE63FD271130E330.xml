<quantity box="[273,397,1510,1534]" metricMagnitude="2" metricUnit="m" metricValue="6.515" metricValueMax="6.9" metricValueMin="6.13" pageId="13" pageNumber="1704" unit="m" value="651.5" valueMax="690.0" valueMin="613.0">
