<quantity box="[948,1047,554,578]" metricMagnitude="-1" metricUnit="m" metricValue="2.794" metricValueMax="3.048" metricValueMin="2.54" pageId="13" pageNumber="1704" unit="in" value="11.0" valueMax="12.0" valueMin="10.0">10–12 in</quantity>
