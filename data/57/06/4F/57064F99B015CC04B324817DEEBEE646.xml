<subSubSection pageId="null" pageNumber="427" type="nomenclature">
<paragraph pageId="null" pageNumber="427">
<taxonomicName authority="All." class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="null" pageNumber="427" phylum="Tracheophyta" rank="species" species="curvula">
