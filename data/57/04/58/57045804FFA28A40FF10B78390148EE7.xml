<date box="[571,669,1123,1147]" pageId="39" pageNumber="40" value="1949-05-02" valueMax="1949-05-02" valueMin="1949-05-02">
<emphasis box="[571,633,1123,1147]" italics="true" pageId="39" pageNumber="40">v2-v2</emphasis>
49
</date>
, 
<emphasis box="[681,765,1123,1147]" italics="true" pageId="39" pageNumber="40">sc2-sc2</emphasis>
