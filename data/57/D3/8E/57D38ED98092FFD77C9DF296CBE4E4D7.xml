<paragraph pageId="null" pageNumber="547">
<taxonomicName authority="L." class="Liliopsida" family="Colchicaceae" genus="Colchicum" kingdom="Plantae" order="Liliales" pageId="null" pageNumber="547" phylum="Tracheophyta" rank="species" species="autumnale">
Colchicum 
<normalizedToken originalValue="autumnále" pageId="null" pageNumber="547">autumnale</normalizedToken>
<authorityName pageId="null" pageNumber="547">L.</authorityName>
</taxonomicName>
</paragraph>
