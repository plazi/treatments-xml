<subSubSection pageId="9" pageNumber="58" type="etymology">
<paragraph blockId="9.[264,1325,284,1230]" pageId="9" pageNumber="58">
<emphasis bold="true" box="[312,456,1044,1070]" pageId="9" pageNumber="58">Etymology:</emphasis>
