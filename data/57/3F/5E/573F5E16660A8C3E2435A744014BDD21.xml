<subSubSection pageId="63" pageNumber="64" type="multiple">
<paragraph pageId="63" pageNumber="64">Taxon classification Animalia Hymenoptera Chrysididae</paragraph>
</subSubSection>
<subSubSection pageId="63" pageNumber="64" type="nomenclature">
<paragraph pageId="63" pageNumber="64">
<taxonomicName authority="Linsenmaier, 1959" authorityName="Linsenmaier" authorityYear="1959" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis vanlithi" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="vanlithi">Chrysis vanlithi Linsenmaier, 1959</taxonomicName>
Figs 97, 115, 140, 159
</paragraph>
</subSubSection>
<subSubSection pageId="63" pageNumber="64" type="reference_group">
