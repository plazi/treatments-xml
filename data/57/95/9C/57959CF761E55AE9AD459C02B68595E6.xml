<taxonomicName authorityName="Benson" authorityYear="1856" class="Gastropoda" family="Gastrocoptidae" genus="Hypselostoma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hypselostoma" order="Stylommatophora" pageId="0" pageNumber="1" phylum="Mollusca" rank="genus">
