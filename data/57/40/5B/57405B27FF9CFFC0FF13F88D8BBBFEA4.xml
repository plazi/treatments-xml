<collectingCountry box="[530,686,277,303]" name="South Africa" pageId="44" pageNumber="45">South Africa</collectingCountry>
, 
<collectingCountry box="[699,827,277,303]" name="Zimbabwe" pageId="44" pageNumber="45">Zimbabwe</collectingCountry>
, 
<collectingCountry box="[841,949,277,303]" name="Tanzania" pageId="44" pageNumber="45">Tanzania</collectingCountry>
, and 
<collectingCountry box="[1015,1091,277,303]" name="Ghana" pageId="44" pageNumber="45">Ghana</collectingCountry>
(
<bibRefCitation author="Freeman, P. &amp; Cranston, P. S." pageId="44" pageNumber="45" pagination="175 - 202" refId="ref91087" refString="Freeman, P. &amp; Cranston, P. S. (1980) 11. Family Chironomidae. In: Crosskey, R. W. (Ed.), Catalogue of the Diptera of the Afrotropical Region. Trustees of The British Museum (Natural History), London, pp. 175 - 202." type="book chapter" year="1980">Freeman &amp; Cranston 1980: 195</bibRefCitation>
