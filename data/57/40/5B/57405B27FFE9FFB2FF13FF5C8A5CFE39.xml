hind tibial comb; 
<emphasis bold="true" box="[619,663,1697,1721]" pageId="93" pageNumber="94">E—</emphasis>
hypopygium; 
<emphasis bold="true" box="[815,858,1697,1721]" pageId="93" pageNumber="94">F—</emphasis>
inferior volsella; 
<emphasis bold="true" box="[1046,1094,1697,1721]" pageId="93" pageNumber="94">G—</emphasis>
superior volsella; 
<emphasis bold="true" box="[1289,1336,1697,1721]" pageId="93" pageNumber="94">H—</emphasis>
