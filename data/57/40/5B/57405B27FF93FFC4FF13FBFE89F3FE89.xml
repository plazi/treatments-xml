<emphasis bold="true" box="[1081,1127,1919,1943]" pageId="39" pageNumber="40">C—</emphasis>
foretibial scale; 
<emphasis bold="true" box="[1301,1347,1919,1943]" pageId="39" pageNumber="40">D—</emphasis>
hypopygium; 
<emphasis bold="true" box="[220,264,1954,1978]" pageId="39" pageNumber="40">E—</emphasis>
