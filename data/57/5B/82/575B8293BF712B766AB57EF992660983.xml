<paragraph pageId="0" pageNumber="1">
<taxonomicName class="Insecta" family="Arctiidae" genus="Bertholdia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bertholdia partita" order="Lepidoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="partita">
