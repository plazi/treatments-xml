<taxonomicName baseAuthorityName="Benick" baseAuthorityYear="1952" box="[808,903,1252,1275]" class="Insecta" family="Staphylinidae" genus="Megalopinus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="795" phylum="Arthropoda" rank="species" species="mrazi">
