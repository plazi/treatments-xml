<paragraph blockId="34.[151,1437,151,1008]" box="[199,997,182,207]" pageId="34" pageNumber="435">
<materialsCitation ID-GBIF-Occurrence="1326058619" box="[199,997,182,207]" country="New Caledonia" location="Riviere Bleue National Park" pageId="34" pageNumber="435" specimenCount="1" typeStatus="holotype">
<emphasis bold="true" box="[199,357,182,207]" pageId="34" pageNumber="435">
<typeStatus box="[199,258,182,207]" pageId="34" pageNumber="435">Type</typeStatus>
locality.
</emphasis>
<collectingCountry box="[364,541,182,207]" name="New Caledonia" pageId="34" pageNumber="435">New Caledonia</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:573388304D53FFB7FF2EB7C4FCF0FC6F:DA456FFD4D6EFFB7FD9EB2D1FCE4FF50" box="[551,870,182,207]" country="New Caledonia" name="Riviere Bleue National Park" pageId="34" pageNumber="435">Riviere Bleue National Park</location>
