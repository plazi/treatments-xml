<caption httpUri="https://zenodo.org/record/167186/files/figure.png" pageId="44" pageNumber="445" targetBox="[285,1305,217,1915]" targetPageId="44">
<paragraph blockId="44.[151,1435,1973,2026]" pageId="44" pageNumber="445">
<emphasis bold="true" box="[151,292,1973,1995]" pageId="44" pageNumber="445">FIGURE 27.</emphasis>
<taxonomicName authority="Skuse, 1890" authorityName="Skuse" authorityYear="1890" box="[302,580,1973,1995]" class="Insecta" family="Sciaridae" genus="Sciara" kingdom="Animalia" order="Diptera" pageId="44" pageNumber="445" phylum="Arthropoda" rank="species" species="tryoni">
