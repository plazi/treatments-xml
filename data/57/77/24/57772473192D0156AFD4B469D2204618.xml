<subSubSection pageId="10" pageNumber="17" type="nomenclature">
<paragraph pageId="10" pageNumber="17">
<taxonomicName LSID="urn:lsid:zoobank.org:act:13AB2413-5166-41E5-9DC4-51F3F948FC91" authority="Paulsen &amp; Ocampo" class="Insecta" family="Ochodaeidae" genus="Parochodaeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parochodaeus phoxus" order="Coleoptera" pageId="10" pageNumber="17" phylum="Arthropoda" rank="species" species="phoxus">Parochodaeus phoxus Paulsen &amp; Ocampo</taxonomicName>
