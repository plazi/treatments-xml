, 
<taxonomicName authorityName="Gray &amp; Smith" authorityYear="2002" box="[634,759,1891,1914]" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="vexillum">T. vexillum</taxonomicName>
, 
<taxonomicName authorityName="Gray &amp; Smith" authorityYear="2002" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="lambkinae">T. lambkinae</taxonomicName>
, 
