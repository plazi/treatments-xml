<subSubSection box="[121,493,204,228]" pageId="4" pageNumber="296" type="type_taxon">
<paragraph blockId="4.[121,493,204,228]" box="[121,493,204,228]" pageId="4" pageNumber="296">
<emphasis bold="true" box="[121,263,204,228]" pageId="4" pageNumber="296">
<typeStatus box="[121,177,204,228]" pageId="4" pageNumber="296">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Gray &amp; Smith, 2002" authorityName="Gray &amp; Smith" authorityYear="2002" box="[277,435,205,228]" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="4" pageNumber="296" phylum="Arthropoda" rank="species" species="kiah" status="n.sp.">
