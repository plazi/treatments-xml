<emphasis italics="true" pageId="5" pageNumber="297">
<taxonomicName authorityName="Gray &amp; Smith" authorityYear="2002" box="[364,522,1861,1884]" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="kiah">Therlinya kiah</taxonomicName>
