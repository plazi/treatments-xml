, 
<taxonomicName authorityName="Gray &amp; Smith" authorityYear="2002" box="[668,790,1861,1884]" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="bellinger">T. bellinger</taxonomicName>
, 
<taxonomicName authorityName="Gray &amp; Smith" authorityYear="2002" box="[166,332,1891,1914]" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="horsemanae">T. horsemanae</taxonomicName>
, 
<taxonomicName authorityName="Gray &amp; Smith" authorityYear="2002" box="[347,494,1891,1914]" class="Arachnida" family="Stiphidiidae" genus="Therlinya" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="297" phylum="Arthropoda" rank="species" species="wiangaree">T. wiangaree</taxonomicName>
, 
