<number box="[1346,1360,1213,1237]" pageId="4" pageNumber="5" value="2.0">2</number>
terminal setae. 
<taxonomicName box="[262,336,1251,1275]" class="Maxillopoda" family="Telsidae" genus="Telson" kingdom="Animalia" order="Poecilostomatoida" pageId="4" pageNumber="32" phylum="Arthropoda" rank="genus">Telson</taxonomicName>
