, EL 
<quantity id="4CBE3CC45872FFBAE80CFEB8FB435F4A" box="[1070,1224,286,309]" metricMagnitude="-4" metricUnit="m" metricValue="6.55" metricValueMax="6.8999999999999995" metricValueMin="6.2" pageId="9" pageNumber="466" unit="mm" value="0.655" valueMax="0.69" valueMin="0.62">0.62–0.69 mm</quantity>
, EW 
