, HL 
<quantity id="4CBE3CC45872FFBAEF41FF58FC755F6A" box="[867,1022,254,277]" metricMagnitude="-4" metricUnit="m" metricValue="5.2" metricValueMax="6.0" metricValueMin="4.4" pageId="9" pageNumber="466" unit="mm" value="0.52" valueMax="0.6" valueMin="0.44">0.44–0.60 mm</quantity>
, HW 
<quantity id="4CBE3CC45872FFBAE815FF58FB595F6A" box="[1079,1234,254,277]" metricMagnitude="-4" metricUnit="m" metricValue="4.8" metricValueMax="5.5" metricValueMin="4.1" pageId="9" pageNumber="466" unit="mm" value="0.48" valueMax="0.55" valueMin="0.41">0.41–0.55 mm</quantity>
, PL 
<quantity id="4CBE3CC45872FFBAE8DCFF58FA135F6A" box="[1278,1432,254,277]" metricMagnitude="-4" metricUnit="m" metricValue="4.6499999999999995" metricValueMax="4.9" metricValueMin="4.4" pageId="9" pageNumber="466" unit="mm" value="0.46499999999999997" valueMax="0.49" valueMin="0.44">0.44–0.49 mm</quantity>
, PW 
