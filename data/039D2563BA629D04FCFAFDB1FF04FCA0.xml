<paragraph id="8B8B9475BA629D0BFD54FD15FC35FCD8" blockId="23.[699,1227,656,784]" pageId="23" pageNumber="24">
<treatmentCitationGroup id="AB24B35BBA629D0BFD54FD15FC35FCD8" pageId="23" pageNumber="24">
