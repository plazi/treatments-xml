<emphasis id="04F1CA8DED85E0B006BEC9C368674F2C" bold="true" pageId="0" pageNumber="309">Figures 1-3.</emphasis>
<taxonomicName id="2B46F3C7FB383D0148DA842557391AC1" authorityName="Costa Lima" authorityYear="1927" class="Insecta" family="Scelionidae" genus="Telenomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Telenomus fariai" order="Hymenoptera" pageId="0" pageNumber="309" phylum="Arthropoda" rank="species" species="fariai">
