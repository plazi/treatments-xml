<taxonomicName id="24C44D28FFE02552FF58F9D3FDD1F9D7" authority="Xu, Guo &amp; Wang" authorityName="Xu, Guo &amp; Wang" authorityYear="2019" box="[216,555,1604,1628]" class="Hydrozoa" family="Sugiuridae" genus="Monocanna" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="201" phylum="Cnidaria" rank="genus" status="gen. nov.">
