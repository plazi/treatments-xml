<paragraph id="13B84D6592030BD147469F6ECE23DE2B" pageId="1" pageNumber="92">
<taxonomicName id="F3AC7E39F4BE76893DC063649CD81BB6" class="Insecta" family="Geometridae" genus="Hoplosauris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hoplosauris cinereus" order="Lepidoptera" pageId="1" pageNumber="92" phylum="Arthropoda" rank="species" species="cinereus">Hoplosauris cinereus</taxonomicName>
