<emphasis id="75C74B51FFD3BD62E9A6FAFD1842FAA9" bold="true" box="[151,267,1307,1333]" pageId="10" pageNumber="35">Material.</emphasis>
<collectingCountry id="3FA4D7D3FFD3BD62E825FAFA18D9FAAA" box="[276,400,1308,1334]" name="Venezuela" pageId="10" pageNumber="35">Venezuela</collectingCountry>
