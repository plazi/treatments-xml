<collectingCountry id="3FA4D7D3FFD3BD62EAFAFAFA1D64FAAA" box="[971,1069,1308,1334]" name="Paraguay" pageId="10" pageNumber="35">Paragua</collectingCountry>
, 
<quantity id="804B3AA6FFD3BD62ED08FAFA1DDFFAAA" box="[1081,1174,1308,1334]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="10" pageNumber="35" unit="m" value="1500.0">1500 m</quantity>
