<collectionCode id="21A20F86FFD3BD62EC6FFAA41CD5FAC0" LSID="urn:lsid:biocol.org:col:34838" box="[1374,1436,1346,1372]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="10" pageNumber="35">SMF</collectionCode>
<number id="F17B8564FFD3BD62E9A6FA8F199AFA1F" box="[151,211,1385,1411]" pageId="10" pageNumber="35" value="5142.0">5142</number>
/ 
<typeStatus id="980829E1FFD3BD62E9EDFA8F180FFA1F" box="[220,326,1385,1411]" pageId="10" pageNumber="35">holotype</typeStatus>
