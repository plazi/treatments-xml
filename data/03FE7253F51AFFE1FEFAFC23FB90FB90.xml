<taxonomicName id="4C57B8C6F51AFFE6FB68F81DFABBF82A" authority="Stal" authorityName="Stal" box="[1174,1350,1975,1997]" class="Insecta" family="Naucoridae" genus="Naucoris" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="668" phylum="Arthropoda" rank="species" species="scutellaris">
<emphasis id="B9231F57F51AFFE6FB68F81DFB5BF82A" box="[1174,1190,1977,1997]" italics="true" pageId="3" pageNumber="668">N</emphasis>
