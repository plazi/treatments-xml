<paragraph id="7F3336EB7027FFC1FF23EECD8AC4FF32" blockId="89.[151,1436,151,788]" pageId="89" pageNumber="90">
<emphasis id="4DF8EAF97027FFC1FF23EECD8B76FF75" bold="true" box="[199,381,152,176]" inLineHeading="true" pageId="89" pageNumber="90" reason="1">Measurements.</emphasis>
<emphasis id="4DF8EAF97027FFC1FE6CEECD8BD2FF75" box="[392,473,152,176]" italics="true" pageId="89" pageNumber="90">Worker</emphasis>
(n = 2) 
<emphasis id="4DF8EAF97027FFC1FDD3EECC885BFF75" box="[567,592,153,176]" italics="true" pageId="89" pageNumber="90">—</emphasis>
