<quantity id="4CA788CCE07AFF92C7A1FF0AA96A1080" box="[241,331,224,248]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="15" pageNumber="517" unit="m" value="1100.0">1100 m</quantity>
. 
<date id="FFE103E9E07AFF92C609FF0AA99D108F" box="[345,444,224,248]" pageId="15" pageNumber="517" value="1981-05-08">
<number id="3D97370EE07AFF92C609FF0AA946108F" box="[345,359,224,248]" pageId="15" pageNumber="517" value="8.0">8</number>
