<taxonomicName id="4C23F1BAA7118E0DFF242CA0FDB689AA" authority="Illiger" authorityName="Illiger" box="[151,535,729,754]" class="Insecta" family="Eumenidae" genus="Anterhynchium" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="510" phylum="Arthropoda" rank="species" species="abdominale">
