<number id="3DD524E0FFB4AF7EFEB413E8FE6C123E" box="[332,398,1788,1812]" pageId="33" pageNumber="34" value="167.3">167.3</number>
, pabd ~ 
<number id="3DD524E0FFB4AF7EFE1D13E8FDF8123E" box="[485,538,1788,1812]" pageId="33" pageNumber="34" value="96.4">96.4</number>
