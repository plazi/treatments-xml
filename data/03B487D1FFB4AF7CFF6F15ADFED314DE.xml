</geoCoordinate>
, orbit 
<number id="3DD524E0FFB4AF7EFF2B13CCFEE513DA" box="[211,263,1752,1776]" pageId="33" pageNumber="34" value="24.8">24.8</number>
, uj 
<number id="3DD524E0FFB4AF7EFED713CCFE8113DA" box="[303,355,1752,1776]" pageId="33" pageNumber="34" value="42.4">42.4</number>
, go 
<number id="3DD524E0FFB4AF7EFE6913CCFE2713DA" box="[401,453,1752,1776]" pageId="33" pageNumber="34" value="35.2">35.2</number>
, UPL 63.0, LPL 40.0, bd 
<number id="3DD524E0FFB4AF7EFD1E13CCFCC513DA" box="[742,807,1752,1776]" pageId="33" pageNumber="34" value="115.1">115.1</number>
, bdA 
<number id="3DD524E0FFB4AF7EFC9213CCFC4913DA" box="[874,939,1752,1776]" pageId="33" pageNumber="34" value="114.6">114.6</number>
, preD 
<number id="3DD524E0FFB4AF7EFC0D13CCFBD513DA" box="[1013,1079,1752,1776]" pageId="33" pageNumber="34" value="124.8">124.8</number>
