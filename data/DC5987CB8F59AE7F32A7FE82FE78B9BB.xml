<taxonomicName id="93F04D5E8F59AE7F311CFEA0FDD3BB4A" authority="Brandt, 1990: 97" authorityName="Brandt" authorityPageNumber="97" authorityYear="1990" box="[313,620,318,340]" class="Malacostraca" family="Antarcturidae" genus="Mixarcturus" kingdom="Animalia" order="Isopoda" pageId="34" pageNumber="1838" phylum="Arthropoda" rank="genus">
