<paragraph id="8BFF3689FF93FFC662A1FBFFFE0FFB10" blockId="3.[113,647,1069,1220]" pageId="3" pageNumber="148">
<materialsCitation id="3B283CD4FF93FFC662A1FBFFFE0CFB10" collectingDate="1950-08-26" collectingDateMax="1958-08-28" collectingDateMin="1950-08-26" country="Madagascar" county="Ambodiriana" location="Ambodiriana" municipality="Betampona" pageId="3" pageNumber="148" specimenCount="1" stateProvince="Toamasina" typeStatus="paratype">
