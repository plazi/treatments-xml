<typeStatus id="5266882FFFDA6C0C47B35D4C6B99FDFA" box="[151,212,573,599]" pageId="4" pageNumber="63">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="4ADD4D0EFFDA6C0C46645D4C69C6FDFA" authority="Hagen" authorityName="Hagen" box="[320,651,573,599]" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="4" pageNumber="63" phylum="Arthropoda" rank="species" species="mengeanus">
