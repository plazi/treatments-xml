<taxonomicName id="98912194CB56BAEB4D78072FB260E093" authority="Schulz, 1927" authorityName="Schulz" authorityYear="1927" class="Chromadorea" family="Physalopteridae" genus="Skrjabinoptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Skrjabinoptera" order="Rhabditida" pageId="0" pageNumber="43" phylum="Nematoda" rank="genus">Skrjabinoptera</taxonomicName>
