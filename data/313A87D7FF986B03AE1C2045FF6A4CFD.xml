<heading id="E26481ADFF986B02AE1C2045FC9E4ACC" box="[289,824,1396,1420]" centered="true" fontSize="10" level="2" pageId="44" pageNumber="525" reason="3">
<taxonomicName id="7E934D42FF986B02AE1C2045FD4E4ACC" authority="Lee and Patterson" authorityName="Lee and Patterson" authorityYear="2000" box="[289,744,1396,1420]" class="Imbricatea" family="Thaumatomonadidae" genus="Petalomonas" higherTaxonomySource="GBIF" kingdom="Chromista" order="Thaumatomonadida" pageId="44" pageNumber="525" phylum="Cercozoa" rank="species" species="virgatus" status="n. sp.">
<emphasis id="8BE7EAD3FF986B02AE1C2045FDA94ACC" bold="true" box="[289,527,1396,1420]" italics="true" pageId="44" pageNumber="525">Petalomonas virgatus</emphasis>
Lee and Patterson
</taxonomicName>
, 
<taxonomicNameLabel id="90D457A8FF986B02ADC42044FC9E4ACC" box="[761,824,1397,1420]" pageId="44" pageNumber="525" rank="species">n. sp.</taxonomicNameLabel>
