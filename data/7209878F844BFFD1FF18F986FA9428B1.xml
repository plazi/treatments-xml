<taxonomicName id="3DA04D1A844BFFD1FB53FED2FA072850" authorityName="Sars" authorityYear="1878" box="[1261,1443,359,382]" class="Gastropoda" family="Akiodorididae" genus="Doridunculus" kingdom="Animalia" order="Nudibranchia" pageId="2" pageNumber="103" phylum="Mollusca" rank="species" species="echinulatus">
<emphasis id="C8D4EA8B844BFFD1FB53FED2FA072850" box="[1261,1443,359,382]" italics="true" pageId="2" pageNumber="103">D. echinulatus</emphasis>
</taxonomicName>
