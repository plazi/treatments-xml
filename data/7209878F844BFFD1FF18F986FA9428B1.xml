<taxonomicName id="3DA04D1A844BFFD1FC8EFE3DFC3028B1" authorityName="Martynov &amp; Roginskaya" authorityYear="2005" box="[816,916,392,415]" class="Gastropoda" family="Akiodorididae" genus="Doridunculus" kingdom="Animalia" order="Nudibranchia" pageId="2" pageNumber="103" phylum="Mollusca" rank="species" species="unicus">
<emphasis id="C8D4EA8B844BFFD1FC8EFE3DFC3028B1" box="[816,916,392,415]" italics="true" pageId="2" pageNumber="103">D. unicus</emphasis>
</taxonomicName>
