<heading bold="true" box="[151,531,151,178]" fontSize="11" level="1" pageId="22" pageNumber="323" reason="1">
<emphasis bold="true" box="[151,531,151,178]" pageId="22" pageNumber="323">
<taxonomicName authority="" box="[151,467,151,178]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="subGenus" subGenus="Eutricharaea">
<emphasis bold="true" box="[151,278,151,178]" italics="true" pageId="22" pageNumber="323">Megachile</emphasis>
(
<emphasis bold="true" box="[295,456,151,178]" italics="true" pageId="22" pageNumber="323">Eutricharaea</emphasis>
)
</taxonomicName>
sp. 1
</emphasis>
</heading>
</paragraph>
