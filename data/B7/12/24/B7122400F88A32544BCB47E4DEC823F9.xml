<paragraph pageId="null" pageNumber="248">
Gelbe 
<taxonomicName class="Magnoliopsida" family="Resedaceae" genus="Reseda" kingdom="Plantae" order="Brassicales" pageId="null" pageNumber="248" phylum="Tracheophyta" rank="genus">Reseda</taxonomicName>
</paragraph>
