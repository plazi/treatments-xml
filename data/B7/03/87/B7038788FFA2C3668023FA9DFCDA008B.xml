<quantity box="[563,728,2001,2027]" metricMagnitude="-2" metricUnit="m" metricValue="2.05" metricValueMax="2.68" metricValueMin="1.42" pageId="58" pageNumber="59" unit="mm" value="20.5" valueMax="26.8" valueMin="14.2">14.2–26.8 µm</quantity>
