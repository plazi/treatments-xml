<paragraph blockId="58.[189,1398,1746,1810]" pageId="58" pageNumber="59">
FRUIT. Elliptical; mesocarp 
<quantity box="[520,657,1747,1773]" metricMagnitude="-1" metricUnit="m" metricValue="1.005" metricValueMax="1.29" metricValueMin="0.72" pageId="58" pageNumber="59" unit="mm" value="100.5" valueMax="129.0" valueMin="72.0">72–129 µm</quantity>
thick when dry. Length 
<quantity box="[953,1084,1747,1773]" metricMagnitude="-2" metricUnit="m" metricValue="1.5" metricValueMax="2.0" metricValueMin="1.0" pageId="58" pageNumber="59" unit="mm" value="15.0" valueMax="20.0" valueMin="10.0">10–20 mm</quantity>
, width 
<quantity box="[1174,1276,1746,1773]" metricMagnitude="-3" metricUnit="m" metricValue="7.5" metricValueMax="9.0" metricValueMin="6.0" pageId="58" pageNumber="59" unit="mm" value="7.5" valueMax="9.0" valueMin="6.0">6–9 mm</quantity>
, thickness 4.0– 
