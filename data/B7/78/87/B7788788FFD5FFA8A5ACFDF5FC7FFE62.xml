<taxonomicName authority="Reitter, 1884" authorityName="Reitter" authorityYear="1884" box="[251,515,1948,1970]" class="Insecta" family="Curculionidae" genus="Styphlidius" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="401" phylum="Arthropoda" rank="species" species="corcyreus">
