. (12) 
<taxonomicName authority="(Reitter, 1884) (Kilkis)" authorityName="(Reitter, 1884) (Kilkis" box="[460,807,1783,1805]" class="Insecta" family="Curculionidae" genus="Styphlidius" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="402" phylum="Arthropoda" rank="species" species="corcyreus">
<emphasis box="[460,577,1784,1805]" italics="true" pageId="3" pageNumber="402">S. corcyreus</emphasis>
(Reitter, 1884) (Kilkis)
</taxonomicName>
. (13-15) 
<emphasis box="[894,981,1783,1805]" italics="true" pageId="3" pageNumber="402">
<taxonomicName box="[894,981,1783,1805]" class="Insecta" family="Curculionidae" genus="Styphlidius" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="402" phylum="Arthropoda" rank="species" species="pelops" status="sp. nov.">S. pelops</taxonomicName>
