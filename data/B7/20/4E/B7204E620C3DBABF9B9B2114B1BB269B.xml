<taxonomicName class="Insecta" family="Curculionidae" genus="Styphlus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Styphlus" order="Coleoptera" pageId="257" pageNumber="258" phylum="Arthropoda" rank="genus">Styphlus</taxonomicName>
