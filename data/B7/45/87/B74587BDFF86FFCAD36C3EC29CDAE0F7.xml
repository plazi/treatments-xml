<collectionCode box="[1109,1194,988,1014]" httpUri="http://grbio.org/cool/a5xg-x4h4" name="Museum d'Histoire Naturelle Perpignan" pageId="10" pageNumber="11">MHNP</collectionCode>
is labelled &quot;Leconte [handwritten]/ 
<number box="[587,616,1028,1054]" pageId="10" pageNumber="11" value="34.0">34</number>
[handwritten]/ 
<taxonomicName authority="Putz." authorityName="Putz." box="[833,989,1028,1054]" class="Insecta" family="Carabidae" genus="Ardistomis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="vicina">vicina Putz.</taxonomicName>
[handwritten]/ 
<typeStatus box="[1205,1322,1028,1054]" pageId="10" pageNumber="11">Lectotype</typeStatus>
<taxonomicName authority="Putz." authorityName="Putz." box="[264,553,1068,1094]" class="Insecta" family="Carabidae" genus="Ardistomis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="vicinus">Ardistomis vicinus Putz.</taxonomicName>
des. S.W. Nichols 
<number box="[773,835,1068,1094]" pageId="10" pageNumber="11" value="1984.0">1984</number>
&quot; (
<emphasis box="[860,904,1068,1093]" italics="true" pageId="10" pageNumber="11">fide</emphasis>
<bibRefCitation author="Nichols" box="[910,1085,1068,1094]" lastPageId="10" lastPageNumber="29" pageId="10" pageNumber="28" refString="Nichols, S. W. (1988 a) Systematics and biogeography of West Indian Scaritinae (Coleoptera: Carabidae). Ph. D. thesis, Cornell University, 393 pp." type="book" year="1988" yearSuffix="a">
Nichols 
<number box="[1008,1070,1068,1094]" pageId="10" pageNumber="11" value="1988.0">1988</number>
a
</bibRefCitation>
: 
<number box="[1095,1143,1068,1094]" pageId="10" pageNumber="11" value="197.0">197</number>
). The 
