<taxonomicName authority="Say" authorityName="Say" box="[1104,1323,628,654]" class="Insecta" family="Carabidae" genus="Clivina" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="viridis">Clivina viridis Say</taxonomicName>
design. Lth. / 
<taxonomicName authority="Say" authorityName="Say" box="[459,745,668,694]" class="Insecta" family="Carabidae" genus="Ardistomis" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="viridis">Ardistomis viridis Say</taxonomicName>
det. Lindroth 
<number box="[946,975,668,694]" pageId="10" pageNumber="11" value="68.0">68</number>
/ M.C.Z. 
<typeStatus box="[1125,1224,668,694]" pageId="10" pageNumber="11">Neotype</typeStatus>
