<taxonomicName authority="Putzeys" authorityName="Putzeys" class="Insecta" family="Carabidae" genus="Ardistomis" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="elongatulus">
