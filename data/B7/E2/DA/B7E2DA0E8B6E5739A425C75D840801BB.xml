<paragraph pageId="0" pageNumber="63">
<taxonomicName authority="Dahlbom, 1835" authorityName="Dahlbom" authorityYear="1835" class="Insecta" family="Tenthredinidae" genus="Dineura" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Dineura (Mesoneura) pallipes" order="Hymenoptera" pageId="0" pageNumber="63" phylum="Arthropoda" rank="species" species="pallipes" subGenus="Mesoneura">Dineura (Mesoneura) pallipes</taxonomicName>
