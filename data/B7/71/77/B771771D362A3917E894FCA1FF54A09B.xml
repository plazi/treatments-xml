<taxonomicName authority="Utinomy, 1957" authorityName="Utinomy" authorityYear="1957" box="[968,1320,1710,1736]" class="Anthozoa" family="Alcyoniidae" genus="Eleutherobia" kingdom="Animalia" order="Alcyonacea" pageId="2" pageNumber="3" phylum="Cnidaria" rank="species" species="albiflora">
