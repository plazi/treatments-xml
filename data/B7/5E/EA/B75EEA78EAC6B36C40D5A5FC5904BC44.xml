, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29491" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera inaudax (Santschi)" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="inaudax">inaudax</taxonomicName>
