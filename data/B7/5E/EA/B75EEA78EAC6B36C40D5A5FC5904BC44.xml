, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:298369" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera surda Bolton &amp; Fisher" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="surda">surda</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:298370" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera tecta Bolton &amp; Fisher" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="tecta">tecta</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:29564" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera ursa (Santschi)" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="ursa">ursa</taxonomicName>
