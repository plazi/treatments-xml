<paragraph id="17293C6EFC25CF8C2BBA9460704DCE46" pageId="0" pageNumber="407">
<taxonomicName id="E726F0B4000FD30D5808C88A6C38F442" authorityName="Peters" authorityYear="1875" class="Reptilia" family="Hyperoliidae" genus="Hyperolius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyperolius concolor subsp. guttatus" order="Anura" pageId="0" pageNumber="407" phylum="Chordata" rank="subSpecies" species="concolor" subSpecies="guttatus">
