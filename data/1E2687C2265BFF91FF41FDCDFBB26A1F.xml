<quantity id="51779B31265BFF91FC1CFD40FF306DF7" box="[970,1114,706,732]" metricMagnitude="-3" metricUnit="m" metricValue="4.65" metricValueMax="4.8" metricValueMin="4.5" pageId="6" pageNumber="53" unit="mm" value="4.65" valueMax="4.8" valueMin="4.5">4.5–4.8 mm</quantity>
