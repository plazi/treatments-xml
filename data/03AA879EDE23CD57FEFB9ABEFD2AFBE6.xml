[printed] 
<taxonomicName id="4C034D0BDE23CD57FC2D9994FF66FD6D" authority="Germ." authorityName="Germ." class="Insecta" family="Curculionidae" genus="Sciaphobus" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="782" phylum="Arthropoda" rank="species" species="scitulus">Sciaphobus scitulus Germ.</taxonomicName>
