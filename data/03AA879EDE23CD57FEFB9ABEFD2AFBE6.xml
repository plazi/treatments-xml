<collectionCode id="ED12AE4DDE23CD57FD5899A9FCE5FD6D" box="[742,810,748,767]" country="Germany" httpUri="http://grbio.org/cool/pvva-bewi" name="Martin Luther Universitaet" pageId="37" pageNumber="782">MLUH</collectionCode>
