<collectionCode id="7EB6AE24FFA69602FE62F9C0B4C93656" box="[407,484,1614,1638]" country="0" httpUri="http://grbio.org/cool/42td-7yhx" name="Universidad Nacional de San Agustin, Museo de Historia Natural (Peru)" pageId="5" pageNumber="778">MUSA</collectionCode>
