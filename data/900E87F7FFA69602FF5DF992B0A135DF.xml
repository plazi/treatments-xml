<collectionCode id="7EB6AE24FFA69602FEFEF9E1B47536B7" box="[267,344,1647,1671]" country="0" httpUri="http://grbio.org/cool/42td-7yhx" name="Universidad Nacional de San Agustin, Museo de Historia Natural (Peru)" pageId="5" pageNumber="778">MUSA</collectionCode>
