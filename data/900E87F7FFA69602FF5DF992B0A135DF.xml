<geoCoordinate id="7D935026FFA69602FDB5F9E0B7F936B7" box="[576,724,1646,1671]" degrees="069" direction="west" minutes="36" orientation="longitude" pageId="5" pageNumber="778" precision="15" seconds="36" value="-69.61">069°36′36″W</geoCoordinate>
