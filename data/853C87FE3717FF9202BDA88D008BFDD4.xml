<number id="BB5D24CF3717FF9202BDAA1F03EBF8A7" box="[151,202,1824,1847]" pageId="3" pageNumber="134" value="0.73">0.73</number>
, SI 
<number id="BB5D24CF3717FF9202D0AA1F0204F8A7" box="[250,293,1824,1847]" pageId="3" pageNumber="134" value="108.0">108</number>
, SL 
<number id="BB5D24CF3717FF920377AA1F02B1F8A7" box="[349,400,1824,1847]" pageId="3" pageNumber="134" value="0.98">0.98</number>
