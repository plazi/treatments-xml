is labeled &quot; 
<collectingCountry id="8EBE76685D44FF8932E1FA893460AEEE" box="[1191,1318,1404,1430]" name="Venezuela" pageId="18" pageNumber="19">Venezuela</collectingCountry>
, Guár., 
<quantity id="31519B1D5D44FF89371FFA5130E2AEC6" box="[345,420,1444,1470]" metricMagnitude="4" metricUnit="m" metricValue="1.2" pageId="18" pageNumber="19" unit="km" value="12.0">12 km</quantity>
S 
