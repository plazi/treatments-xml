<taxonomicName id="4C074D03FF91AD4BFE9C8C13E395262B" authorityName="Karny" authorityYear="1937" box="[312,503,850,872]" class="Insecta" family="Haglidae" genus="Phryganogryllacris" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="508" phylum="Arthropoda" rank="genus">
<emphasis id="B973EA92FF91AD4BFE9C8C13E395262B" bold="true" box="[312,503,850,872]" italics="true" pageId="1" pageNumber="508">Phryganogryllacris</emphasis>
</taxonomicName>
