<emphasis id="B9142C7BFFD3AD001BCB4F980275937A" bold="true" box="[199,352,764,789]" pageId="11" pageNumber="502">Distribution.</emphasis>
<collectingCountry id="F377B0F9FFD3AD001A6B4F9802BF937A" box="[359,426,764,789]" name="China" pageId="11" pageNumber="502">China</collectingCountry>
(
<collectingRegion id="49A43E8BFFD3AD001AB54F980105937A" box="[441,528,764,789]" country="China" name="Hainan" pageId="11" pageNumber="502">Hainan</collectingRegion>
