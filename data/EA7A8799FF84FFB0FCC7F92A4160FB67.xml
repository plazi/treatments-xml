<taxonomicName id="A5D34D0CFF85FFB0FAF6FC144037FC0C" authorityName="Wagler" authorityYear="1830" class="Reptilia" family="Gymnophthalmidae" genus="Cercosaura" kingdom="Animalia" order="Squamata" pageId="7" pageNumber="108" phylum="Chordata" rank="genus">Cercosaura</taxonomicName>
