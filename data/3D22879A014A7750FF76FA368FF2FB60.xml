<collectingCountry id="CD9C761C01487750FE48FCA08EF8FCF8" box="[425,485,799,824]" name="India" pageId="3" pageNumber="42">India</collectingCountry>
, 
<collectingRegion id="774FF86E01487750FE10FC9F8D63FCF8" box="[497,638,799,824]" country="India" name="West Bengal" pageId="3" pageNumber="42">
