<taxonomicName id="4C3762D118242275FD61B051FC1E5535" authority="Carter, 1882" authorityName="Carter" authorityYear="1882" box="[689,981,1111,1137]" class="Demospongiae" family="Esperiopsidae" genus="Esperiopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Poecilosclerida" pageId="10" pageNumber="11" phylum="Porifera" rank="genus">
