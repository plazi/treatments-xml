<taxonomicName id="4C2703D8CB66FFB0FEB5FBF8FD41F86C" authority="Schmidt, 1862" authorityName="Schmidt" authorityYear="1862" box="[294,652,1934,1956]" class="Demospongiae" family="Aplysinidae" genus="Aplysina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Verongiida" pageId="4" pageNumber="326" phylum="Porifera" rank="species" species="aerophoba">
