<emphasis id="00C2552F9C35FF83FF4537A4F519F812" bold="true" box="[151,596,764,790]" pageId="17" pageNumber="126">
<taxonomicName id="F5B6F2BE9C35FF83FF4537A4F6F8F812" box="[151,437,764,790]" class="Insecta" family="Cicadellidae" genus="Oncometopia" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="126" phylum="Arthropoda" rank="species" species="acicularis" status="new species">
<emphasis id="00C2552F9C35FF83FF4537A4F6F8F812" bold="true" box="[151,437,764,790]" italics="true" pageId="17" pageNumber="126">Oncometopia acicularis</emphasis>
