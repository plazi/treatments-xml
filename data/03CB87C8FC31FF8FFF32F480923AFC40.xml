<typeStatus id="54D9887CFC3FFF8FFC0DF43A954EFCF4" box="[936,1055,848,874]" pageId="24" pageNumber="25">Paratypes</typeStatus>
: 
<number id="3DAA24F9FC3FFF8FFB8FF43A9568FCF4" box="[1066,1081,848,874]" pageId="24" pageNumber="25" value="1.0">1</number>
