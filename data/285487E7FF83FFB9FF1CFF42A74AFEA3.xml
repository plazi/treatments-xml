<subSubSection id="E8E7657AFF82FFB9FF4CFF0AA722FEFE" pageId="9" pageNumber="596" type="etymology">
<paragraph id="A04236F1FF82FFB9FF4CFF0AA722FEFE" blockId="9.[151,1436,151,320]" pageId="9" pageNumber="596">
<emphasis id="9289EAE3FF82FFB9FF4CFF0AA425FF1B" bold="true" box="[199,334,223,248]" pageId="9" pageNumber="596">Etymology.</emphasis>
