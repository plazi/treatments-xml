<taxonomicName id="4C734D51FFA1174EFC046FD4AE90FA43" box="[991,1164,1461,1486]" class="Arachnida" family="Mideopsidae" genus="Nudomideopsis" kingdom="Animalia" order="Trombidiformes" pageId="41" pageNumber="152" phylum="Arthropoda" rank="genus">
<emphasis id="B907EAC0FFA1174EFC046FD4AE90FA43" box="[991,1164,1461,1486]" italics="true" pageId="41" pageNumber="152">Nudomideopsis</emphasis>
