<emphasis id="6BDDEAA5C501FFA4FF34F8957C79CF23" box="[151,307,1875,1900]" italics="true" pageId="11" pageNumber="38">
<typeStatus id="86128815C501FFA4FF34F8957D84CF23" box="[151,206,1875,1900]" pageId="11" pageNumber="38">Type</typeStatus>
locality.
</emphasis>
<collectingCountry id="21BE7627C501FFA4FEE2F8947CCBCF23" box="[321,385,1874,1900]" name="India" pageId="11" pageNumber="38">India</collectingCountry>
