<heading id="D0D281C9FF99FF8F84A114C0FC12FC95" box="[641,947,755,782]" centered="true" fontSize="11" level="3" pageId="12" pageNumber="13" reason="4">
<taxonomicName id="4C254D26FF99FF8F84A114C0FC12FC95" authority="Simon, 1893" authorityName="Simon" authorityYear="1893" box="[641,947,755,782]" class="Arachnida" family="Pholcidae" genus="Physocyclus" kingdom="Animalia" order="Araneae" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
<emphasis id="B951EAB7FF99FF8F84A114C0FCB4FC95" bold="true" box="[641,789,755,782]" italics="true" pageId="12" pageNumber="13">Physocyclus</emphasis>
<bibRefCitation id="EFB44B54FF99FF8F853C14C0FC12FC95" author="Simon E." box="[796,947,755,782]" journalOrPublisher="Paris" pageId="12" pageNumber="13" pagination="256 - 488" part="1" refString="Simon E. 1893. Histoire Naturelle des Araignees. 2 nd edition., 1 (2): 256 - 488. Paris." title="Histoire Naturelle des Araignees. 2 nd edition." type="journal article" year="1893">Simon, 1893</bibRefCitation>
