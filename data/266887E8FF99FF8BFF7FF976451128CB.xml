<taxonomicName id="69C14D7DFF99FF88FDD4F81044F92FF4" authority="Stal, 1859" authorityName="Stal" authorityYear="1859" box="[572,1007,2003,2027]" class="Insecta" family="Gerridae" genus="Limnogonus" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="34" phylum="Arthropoda" rank="species" species="franciscanus">
