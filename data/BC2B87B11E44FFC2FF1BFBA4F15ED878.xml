<typeStatus id="EB3988051E45FFC2FBDFF9CBF7FBD85D" box="[1107,1205,1591,1616]" pageId="9" pageNumber="478">holotype</typeStatus>
and 
<typeStatus id="EB3988051E45FFC2FB7AF9C5F62AD85C" box="[1270,1380,1593,1617]" pageId="9" pageNumber="478">paratypes</typeStatus>
