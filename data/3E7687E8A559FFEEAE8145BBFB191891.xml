<taxonomicName id="71DF4D7DA559FFECAD50425AFC371C23" authority="Gravenhorst, 1829" authorityName="Gravenhorst" authorityYear="1829" box="[611,938,1094,1119]" class="Insecta" family="Ichneumonidae" genus="Mesostenus" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="1166" phylum="Arthropoda" rank="genus">
<emphasis id="84ABEAECA559FFECAD50425AFD431C23" box="[611,734,1094,1119]" italics="true" pageId="52" pageNumber="1166">Mesostenus</emphasis>
<bibRefCitation id="D24E4B0FA559FFECADD0425AFC371C23" author="Gravenhorst JLC" box="[739,938,1094,1119]" pageId="52" pageNumber="1166" refId="ref21836" refString="Gravenhorst JLC. 1829. Ichneumonologia Europaea. Pars II. Vratislaviae: sumtibus auctoris; 989 pp." type="book" year="1829">Gravenhorst, 1829</bibRefCitation>
