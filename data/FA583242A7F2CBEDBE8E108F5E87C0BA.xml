<taxonomicName id="802EB922C2A63E2A7604E988DC1DDF4A" authorityName="Brusina" authorityYear="1892" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis handmanni" order="Pteropoda" pageId="134" pageNumber="135" phylum="Mollusca" rank="species" species="handmanni">
