<taxonomicName id="B9F3C25EE6C0412D0FA7D8DD1E5DFCF2" authorityName="Brusina" authorityYear="1884" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis fuchsi" order="Pteropoda" pageId="134" pageNumber="135" phylum="Mollusca" rank="species" species="fuchsi">
