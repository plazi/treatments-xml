<treatmentCitationGroup id="AB285E78FFE399738347FAA7FCD6C8CD" box="[264,820,1398,1422]" pageId="10" pageNumber="11">
<taxonomicName id="4C3802D5FFE399738347FAA7FD1CC8CD" authority="Bianchi, 1945: 263" authorityName="Bianchi" authorityPageNumber="263" authorityYear="1945" box="[264,766,1398,1422]" class="Insecta" family="Thripidae" genus="Scirtothrips" kingdom="Animalia" order="Thysanoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="albomaculatus">
<emphasis id="B94CA544FFE399738347FAA7FDC8C8CE" box="[264,554,1399,1422]" italics="true" pageId="10" pageNumber="11">Scirtothrips albomaculatus</emphasis>
