<paragraph id="C60378423841FC790F1D10817585DE91" pageId="2" pageNumber="49">
<taxonomicName id="F1A56856AC0A8C8F12687772FAD1760E" LSID="urn:lsid:zoobank.org:act:8429491E-26FC-469C-B106-8B97C94A3D4B" authority="Maulik, 1926" authorityName="Maulik" authorityYear="1926" class="Insecta" family="Chrysomelidae" genus="Jacobyana" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Jacobyana bezdeki" order="Coleoptera" pageId="2" pageNumber="49" phylum="Arthropoda" rank="species" species="bezdeki">Jacobyana bezdeki</taxonomicName>
