<taxonomicName id="4C3E4D5D8735237CFCDB47DEFB95FE10" authorityName="J. Agardh" authorityYear="1896" box="[871,1141,403,429]" class="Phaeophyceae" family="Cystoseiraceae" genus="Cystoseira" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fucales" pageId="28" pageNumber="229" phylum="Phaeophyta" rank="species" species="brachycarpa">
