<quantity id="6147A7FEFF96FFF8FCC7FED472F8E736" box="[861,953,260,285]" metricMagnitude="-2" metricUnit="m" metricValue="8.15" pageId="11" pageNumber="486" unit="mm" value="81.5">81.5 µm</quantity>
(mean = 74.8± 
<quantity id="6147A7FEFF96FFF8FBFFFED475F4E737" box="[1125,1205,260,284]" metricMagnitude="-3" metricUnit="m" metricValue="3.1" pageId="11" pageNumber="486" unit="mm" value="3.1">3.1 µm</quantity>
), lorica height 43.0– 
<quantity id="6147A7FEFF96FFF8FF0DFEF871B5E76B" box="[151,244,296,320]" metricMagnitude="-2" metricUnit="m" metricValue="4.89" pageId="11" pageNumber="486" unit="mm" value="48.9">48.9 µm</quantity>
(mean = 45.9± 
<quantity id="6147A7FEFF96FFF8FE38FEF770B3E76B" box="[418,498,295,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="11" pageNumber="486" unit="mm" value="1.9">1.9 µm</quantity>
