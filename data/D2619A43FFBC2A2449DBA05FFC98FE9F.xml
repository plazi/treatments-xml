<paragraph id="5A772B55FFBC2A27498BA08CFCC0FAD9" blockId="69.[151,1437,1189,2043]" box="[199,829,1261,1287]" pageId="69" pageNumber="70">
<emphasis id="68BCF747FFBC2A27498BA08CFEFBFAD9" bold="true" box="[199,262,1261,1287]" pageId="69" pageNumber="70">Host.</emphasis>
<taxonomicName id="9DC850D6FFBC2A274841A08CFE52FAD9" box="[269,431,1261,1287]" class="Magnoliopsida" family="Aquifoliaceae" kingdom="Plantae" order="Aquifoliales" pageId="69" pageNumber="70" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
