<paragraph id="1E553684AE61FFD21DC1FC6FFECB39FC" blockId="9.[151,672,943,1003]" box="[151,365,978,1003]" pageId="9" pageNumber="426">
(Figs 4, 10, 
<number id="A82224A3AE61FFD21C4BFC6FFEC339FC" box="[285,357,978,1003]" pageId="9" pageNumber="426" value="37.0" valueMax="38.0" valueMin="36.0">36–38</number>
