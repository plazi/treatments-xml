<taxonomicName id="929A4D79FFDE6654714B3C8FF7B2FB1B" authority="(Gray, 1866)" baseAuthorityName="Gray" baseAuthorityYear="1866" box="[1553,2092,1190,1226]" class="Mammalia" family="Muridae" genus="Mus" kingdom="Animalia" order="Rodentia" pageId="7" pageNumber="152" phylum="Chordata" rank="species" species="macropus">
