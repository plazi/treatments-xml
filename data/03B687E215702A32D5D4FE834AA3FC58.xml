<date id="FFA1103415702A32D6E4FE524ACEFE2C" box="[898,1019,426,446]" pageId="21" pageNumber="748" value="2007-02-14" valueMax="2007-02-16" valueMin="2007-02-14">14-16.II.2007</date>
