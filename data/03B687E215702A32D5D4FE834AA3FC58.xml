<typeStatus id="54A4885615702A32D4C9FE524B35FE2C" box="[431,512,426,446]" pageId="21" pageNumber="748">Holotype</typeStatus>
, 
<collectingRegion id="49DBF81615702A32D74AFE524B51FE2C" box="[556,612,426,446]" country="Malaysia" name="Sabah" pageId="21" pageNumber="748">Sabah</collectingRegion>
