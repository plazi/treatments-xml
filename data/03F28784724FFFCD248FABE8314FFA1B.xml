<taxonomicName id="4C5B4D117248FFCD2545A8C33708FA70" authorityName="Lonsdale &amp; Marshall" authorityYear="2008" baseAuthorityName="Malloch" baseAuthorityYear="1933" box="[349,607,1432,1457]" class="Insecta" family="Clusiidae" genus="Apiochaeta" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="chiloensis">
