<taxonomicName id="4C5B4D117248FFCD27A8A8C33161FA70" authorityName="Czerny" authorityYear="1903" box="[944,1078,1432,1457]" class="Insecta" family="Clusiidae" genus="Apiochaeta" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">
