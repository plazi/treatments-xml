<taxonomicName id="4C5B4D117248FFCD268EA8C3364DFA70" authorityName="Lonsdale &amp; Marshall" authorityYear="2008" baseAuthorityName="Rondani" baseAuthorityYear="1863" box="[662,794,1432,1457]" class="Insecta" family="Clusiidae" genus="Apiochaeta" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="philippii">
