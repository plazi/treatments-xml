<quantity id="4CD09B21A455AE15DDDAF92EFAD6F95B" box="[1275,1363,1766,1789]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="4" pageNumber="23" unit="mm" value="3.5" valueMax="4.0" valueMin="3.0">3–4 mm</quantity>
