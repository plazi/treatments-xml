<quantity id="4CD09B21A455AE15DB96F8AEFCB2F8DB" box="[695,823,1894,1917]" metricMagnitude="-3" metricUnit="m" metricValue="1.95" metricValueMax="2.5" metricValueMin="1.4" pageId="4" pageNumber="23" unit="mm" value="1.95" valueMax="2.5" valueMin="1.4">1.4–2.5 mm</quantity>
