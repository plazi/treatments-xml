<quantity id="4CD09B21A455AE15DA55F8EEFC5FF89B" box="[884,986,1830,1853]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="4" pageNumber="23" unit="mm" value="2.75" valueMax="3.0" valueMin="2.5">2.5-3 mm</quantity>
or 24–28 × 
<quantity id="4CD09B21A455AE15DD7AF8EEFB1FF89B" box="[1115,1178,1830,1853]" metricMagnitude="-3" metricUnit="m" metricValue="9.0" pageId="4" pageNumber="23" unit="mm" value="9.0">9 mm</quantity>
