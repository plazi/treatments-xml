<quantity id="4CA45F92FF99FFAEE7C2D4ABBF526CED" box="[478,555,644,668]" metricMagnitude="3" metricUnit="m" metricValue="7.5" pageId="33" pageNumber="37" unit="km" value="7.5">7.5 km</quantity>
