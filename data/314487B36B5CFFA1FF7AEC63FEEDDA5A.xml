<taxonomicName id="7EED4D266B5BFFA3FEF4EE95FE32DFCA" box="[281,388,1978,1999]" class="Insecta" family="Reduviidae" genus="Emesopsis" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="genus">Emesopsis</taxonomicName>
<taxonomicName id="7EED4D266B5BFFA3FE67EE96FE59DFCA" box="[394,495,1977,1999]" pageId="12" pageNumber="13" rank="species" species="infenestra">infenestra</taxonomicName>
