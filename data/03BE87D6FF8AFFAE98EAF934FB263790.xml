<quantity id="4CEF9B25FF8AFFAE99C4F919FBBC3700" box="[441,509,1771,1797]" metricMagnitude="-1" metricUnit="m" metricValue="9.6" pageId="27" pageNumber="28" unit="cm" value="96.0">96 cm</quantity>
(
<quantity id="4CEF9B25FF8AFFAE9A76F919F81B3703" box="[523,602,1771,1798]" metricMagnitude="-1" metricUnit="m" metricValue="9.6012" pageId="27" pageNumber="28" unit="in" value="37.8">37.8 in</quantity>
) DW in captivity (
<bibRefCitation id="EF864B31FF8AFFAE9B5AF919F9B23700" author="Mollet" box="[807,1011,1771,1797]" etAl="et al." firstAuthor="Mollet" pageId="27" pageNumber="28" pagination="531 - 541" refId="ref302797" refString="Mollet, H. F., Ezcurra, J. M. &amp; O'Sullivan, J. B. (2002) Captive biology of the pelagic stingray, Dasyatis violacea (Bonaparte, 1832). Marine and Freshwater Research, 53, 531 - 541. https: // doi. org. 10.1071 / MF 01074" type="journal article" year="2002">
Mollet 
<emphasis id="B963EAD2FF8AFFAE9B04F919F9E83700" box="[889,937,1771,1797]" italics="true" pageId="27" pageNumber="28">et al</emphasis>
. 2002
</bibRefCitation>
); 
<quantity id="4CEF9B25FF8AFFAE9C78F919FE163700" box="[1029,1111,1771,1797]" metricMagnitude="0" metricUnit="m" metricValue="1.63" pageId="27" pageNumber="28" unit="cm" value="163.0">163 cm</quantity>
(
<quantity id="4CEF9B25FF8AFFAE9C19F919FEF53703" box="[1124,1204,1771,1798]" metricMagnitude="0" metricUnit="m" metricValue="1.63068" pageId="27" pageNumber="28" unit="in" value="64.2">64.2 in</quantity>
