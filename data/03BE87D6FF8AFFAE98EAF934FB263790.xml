<quantity id="4CEF9B25FF8AFFAE9B4BF8A4F9D13777" box="[822,912,1878,1906]" metricMagnitude="2" metricUnit="m" metricValue="3.81" pageId="27" pageNumber="28" unit="ft" value="1250.0">1,250 ft</quantity>
