<quantity id="4CEF9B25FF8AFFAE9B9CF935FE6436E4" box="[993,1061,1735,1761]" metricMagnitude="-1" metricUnit="m" metricValue="8.0" pageId="27" pageNumber="28" unit="cm" value="80.0">80 cm</quantity>
