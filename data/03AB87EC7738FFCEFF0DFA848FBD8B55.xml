<specimenCode id="DBA49E817738FFCEFD0CF9FB88288B55" box="[662,826,1633,1657]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="6" pageNumber="47">CHUNB 1265</specimenCode>
; 
