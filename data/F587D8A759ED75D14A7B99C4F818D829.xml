<taxonomicName id="00366DFAE75A9CCF834F52F7D5C1A181" class="Magnoliopsida" family="Apiaceae" genus="Laserpitium" kingdom="Plantae" order="Apiales" pageId="null" pageNumber="812" phylum="Tracheophyta" rank="species" species="nitidum">
