a
</taxonomicName>
<taxonomicNameLabel id="A213579FFF91FFB1FB99092DFBDFAE37" box="[915,1031,858,882]" pageId="2" pageNumber="611" rank="genus">new genus</taxonomicNameLabel>
, 
<taxonomicName id="4C544D75FF91FFB1FC1B092CFBBEAE37" authority="Šifner, 2010" authorityName="Šifner" authorityYear="2010" box="[1041,1126,859,882]" class="Insecta" family="Scathophagidae" genus="Milania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="611" phylum="Arthropoda" rank="genus" status="gen. nov.">
