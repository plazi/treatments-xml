<emphasis id="B920EAE4FF91FFB1F91D092CFD80AE37" box="[279,600,859,882]" italics="true" pageId="2" pageNumber="611">Norellisoma longiabdominum</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C544D75FF91FFB1FA81092CFC56AE34" authority="Seguy, 1948 a" authorityName="♀, Cotyp., E. Seguy" authorityYear="1948" box="[651,910,858,882]" class="Insecta" family="Scathophagidae" genus="Norellisoma" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="611" phylum="Arthropoda" rank="species" species="agrion" status="new genus">
