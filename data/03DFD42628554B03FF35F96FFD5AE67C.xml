<typeStatus id="54CDDB9228574B03FE3EFEF0FE29E67C" box="[412,510,295,320]" pageId="30" pageNumber="231">holotype</typeStatus>
(in 
<collectionCode id="ED67FDF528574B03FD89FEFFFD56E67C" LSID="urn:lsid:biocol.org:col:34985" box="[555,641,296,320]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34985" name="KwaZulu-Natal Museum" pageId="30" pageNumber="231">NMSA</collectionCode>
