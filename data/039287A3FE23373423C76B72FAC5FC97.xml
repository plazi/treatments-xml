<collectionCode id="ED2AAE70FE203734201E681AFC7AFDBE" box="[846,922,548,572]" pageId="38" pageNumber="439">
<typeStatus id="54808817FE203734201E681AFC7AFDBE" box="[846,922,548,572]" pageId="38" pageNumber="439">TYPE</typeStatus>
</collectionCode>
”, and a white hand written label glued to the rock that says “ 
<taxonomicName id="4C3B4D36FE20373422126879FE46FDE2" box="[322,422,583,608]" class="Insecta" family="Inocelliidae" genus="Inocellia" kingdom="Animalia" order="Raphidioptera" pageId="38" pageNumber="439" phylum="Arthropoda" rank="genus">Inocellia</taxonomicName>
/ 
<taxonomicName id="4C3B4D36FE20373422EC6876FD47FDE2" authority="Ckll &amp; Custer" authorityName="Ckll &amp; Custer" box="[444,679,583,608]" class="Insecta" family="Inocelliidae" genus="Inocellia" kingdom="Animalia" order="Raphidioptera" pageId="38" pageNumber="439" phylum="Arthropoda" rank="species" species="exusta">exusta Ckll &amp; Custer</taxonomicName>
/ 
<collectionCode id="ED2AAE70FE20373421ED6876FCE3FDDD" box="[701,771,584,607]" pageId="38" pageNumber="439">
<typeStatus id="54808817FE20373421ED6876FCE3FDDD" box="[701,771,584,607]" pageId="38" pageNumber="439">TYPE</typeStatus>
