<paragraph id="2A6FB309FF9BFFC6AE8211FFFBF082FA" blockId="0.[151,1087,2008,2030]" box="[151,1087,2008,2030]" pageId="0" pageNumber="245">
<taxonomicName id="EDD0C88AFF9BFFC6AE8211FFFE1282FA" authority="Diakonoff, 1977: 103" authorityName="Diakonoff" authorityPageNumber="103" authorityYear="1977" box="[151,477,2008,2030]" class="Insecta" family="Tortricidae" genus="Cosmetra" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="245" phylum="Arthropoda" rank="genus">
