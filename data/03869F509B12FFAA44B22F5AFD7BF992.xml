<geoCoordinate id="EE1B48819B0DFFAA44062BB1FE87FE78" box="[274,345,432,452]" degrees="16" direction="east" minutes="26" orientation="longitude" pageId="16" pageNumber="713" precision="925" value="16.433332">16°26′E</geoCoordinate>
, sweeping 
<taxonomicName id="4C2F55C59B0DFFAA44AC2BB2FDE1FE78" box="[440,575,434,452]" class="Liliopsida" family="Cyperaceae" genus="Carex" kingdom="Plantae" order="Poales" pageId="16" pageNumber="713" phylum="Tracheophyta" rank="species" species="vesicaria">
