<td id="76B6D81E1A3F002EFBD5D219FBD3FC57" box="[1075,1127,872,898]" gridcol="7" gridrow="1" pageId="27" pageNumber="28">only</td>
<td id="76B6D81E1A3F002EFB89D219FB1BFC57" box="[1135,1199,872,898]" gridcol="8" gridrow="1" pageId="27" pageNumber="28">differ</td>
<td id="76B6D81E1A3F002EFB51D219FB44FC57" box="[1207,1264,872,898]" gridcol="9" gridrow="1" pageId="27" pageNumber="28">from</td>
