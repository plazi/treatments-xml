<taxonomicName id="E4886CBD8CA895279870A47AE47F0B73" authorityName="Zhao &amp; Li" authorityYear="2018" class="Arachnida" family="Telemidae" genus="Pinelema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pinelema pacchanensis" order="Araneae" pageId="0" pageNumber="15" phylum="Arthropoda" rank="species" species="pacchanensis">Pinelema pacchanensis</taxonomicName>
