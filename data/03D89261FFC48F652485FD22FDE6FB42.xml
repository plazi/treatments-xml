<paragraph id="8BCE2377FFC48F6524FEFD44FD18FD25" blockId="20.[144,664,719,741]" box="[144,664,719,741]" pageId="20" pageNumber="63">
<treatmentCitationGroup id="AB610459FFC48F6524FEFD44FD18FD25" box="[144,664,719,741]" pageId="20" pageNumber="63">
<treatmentCitation id="0AD00566FFC48F6524FEFD44FD15FD25" author="Ramme W" box="[144,661,719,741]" page="540" pageId="20" pageNumber="63" year="1933">
<taxonomicName id="4C7158F4FFC48F6524FEFD44FD15FD25" authority="Ramme, 1933: 539 - 540" authorityName="Ramme" authorityPageNumber="539 - 540" authorityYear="1933" box="[144,661,719,741]" class="Insecta" family="Tettigoniidae" genus="Poecilimon" kingdom="Animalia" order="Orthoptera" pageId="20" pageNumber="63" phylum="Arthropoda" rank="species" species="luschani">
<emphasis id="B905FF65FFC48F6524FEFD44FEF7FD24" box="[144,375,719,740]" italics="true" pageId="20" pageNumber="63">Poecilimon luschani</emphasis>
<bibRefCitation id="EFE05E86FFC48F6525EFFD44FD15FD25" author="Ramme W" box="[385,661,719,741]" pageId="20" pageNumber="63" pagination="497 - 575" refId="ref18040" refString="Ramme W. 1933. Revision der Phaneropterinen-Gatung Poecilimon Fisch. (Orth. Tettigon). Mitteilungen aus dem Zoologische Museum Berlin 19: 497 - 575." type="journal article" year="1933">Ramme, 1933: 539–540</bibRefCitation>
</taxonomicName>
</treatmentCitation>
