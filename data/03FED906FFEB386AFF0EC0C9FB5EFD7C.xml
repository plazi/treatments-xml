<taxonomicName id="4C571393FFEB3869FF0EC0C9FD89FD18" authority="(Johannsen)" baseAuthorityName="Johannsen" baseAuthorityYear="1905" box="[151,589,717,743]" class="Insecta" family="Chironomidae" genus="Meropelopia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="flavifrons">
<emphasis id="B923B402FFEB3869FF0EC0C9FD89FD18" bold="true" box="[151,589,717,743]" pageId="9" pageNumber="10">
<emphasis id="B923B402FFEB3869FF0EC0C9FE68FD18" bold="true" box="[151,428,717,743]" italics="true" pageId="9" pageNumber="10">Meropelopia flavifrons</emphasis>
