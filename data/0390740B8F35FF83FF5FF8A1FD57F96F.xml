<subSubSection id="C32396968F36FF82FF0FFF07FBB6FF6B" box="[199,1063,223,248]" pageId="2" pageNumber="583" type="etymology">
<paragraph id="8B86C51D8F36FF82FF0FFF07FBB6FF6B" blockId="2.[151,1436,151,248]" box="[199,1063,223,248]" pageId="2" pageNumber="583">
<emphasis id="B94D190F8F36FF82FF0FFF07FEDFFF6B" bold="true" box="[199,334,223,248]" pageId="2" pageNumber="583">Etymology.</emphasis>
