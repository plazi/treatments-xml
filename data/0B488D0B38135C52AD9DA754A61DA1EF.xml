<geoCoordinate id="770F221534242736D3C2B636913CDD90" degrees="9.1628" direction="north" orientation="latitude" precision="5" value="9.1628">9.1628°N</geoCoordinate>
, 
<geoCoordinate id="6B5B40152D7ACBC2118BD410C2A262F2" degrees="79.8395" direction="west" orientation="longitude" precision="5" value="-79.8395">79.8395°W</geoCoordinate>
