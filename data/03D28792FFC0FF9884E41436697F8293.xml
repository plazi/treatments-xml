<taxonomicName id="4C7B4D07FFC1FF9885A012F06E5E809B" box="[467,599,152,175]" class="Asteroidea" family="Goniasteridae" genus="Ceramaster" kingdom="Animalia" order="Valvatida" pageId="5" pageNumber="6" phylum="Echinodermata" rank="genus">
