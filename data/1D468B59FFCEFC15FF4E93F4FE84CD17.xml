; ♦ = 
<taxonomicName id="52EF41CCFFC9FC14FE92909DFE74CDD9" authorityName="Ueno" authorityYear="2006" box="[326,450,1710,1729]" class="Insecta" family="Carabidae" genus="Sichuanotrechus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="731" phylum="Arthropoda" rank="species" species="albidraconis">
