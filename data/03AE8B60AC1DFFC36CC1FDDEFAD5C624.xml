and notched in 
<taxonomicName id="4C0741F5AC1DFFC36D97FABEFD97C649" authorityName="Barnard &amp; Ingram" authorityYear="1990" box="[449,547,1292,1317]" class="Malacostraca" family="Lysianassidae" genus="Ventiella" kingdom="Animalia" order="Amphipoda" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
