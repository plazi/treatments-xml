<taxonomicName id="66AFFD26EF37C359BBBF1090881163C8" baseAuthorityName="Say" baseAuthorityYear="1825" class="Hexapoda" family="Chrysomelidae" genus="Pachybrachis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="30" pageNumber="125" phylum="Arthropoda" rank="species" species="othonus">
