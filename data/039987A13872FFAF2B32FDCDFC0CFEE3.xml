<taxonomicName id="4C304D343873FFAF2B18FED5FD76FECD" authority="in" authorityName="In" box="[593,746,315,341]" class="Insecta" family="Ptiliidae" genus="Cissidium" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="97" pageNumber="2" phylum="Arthropoda" rank="genus">
