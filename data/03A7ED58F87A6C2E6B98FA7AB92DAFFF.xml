<taxonomicName id="4C0E27CDF87B6C2E6AD5FEDEB909AFDA" box="[474,609,369,392]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="61" pageNumber="62" phylum="Arthropoda" rank="species" species="pitinguensis">
