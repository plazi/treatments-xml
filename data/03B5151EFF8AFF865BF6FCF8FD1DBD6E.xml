L e c t o t y p e (by present designation):, &quot; 
<location id="8EC3F2D3FF8AFF875949FB8CFDBBBB58" LSID="urn:lsid:plazi:treatment:03B5151EFF8AFF865BF6FCF8FD1DBD6E:8EC3F2D3FF8AFF875949FB8CFDBBBB58" box="[552,616,1044,1064]" country="Poland" county="Co-Typus" municipality="Tatra" name="Halicia" pageId="4" pageNumber="599">Halicia</location>
[= 
<location id="8EC3F2D3FF8AFF8759E6FB8CFD00BB58" LSID="urn:lsid:plazi:treatment:03B5151EFF8AFF865BF6FCF8FD1DBD6E:8EC3F2D3FF8AFF8759E6FB8CFD00BB58" box="[647,723,1044,1064]" country="Poland" county="Co-Typus" municipality="Tatra" name="Galizien" pageId="4" pageNumber="599">Galizien</location>
; historical landscape in SE 
<collectingCountry id="F30BE498FF8AFF8758A0FB8CFC2DBB58" box="[961,1022,1044,1064]" name="Poland" pageId="4" pageNumber="599">Poland</collectingCountry>
and W 
<collectingCountry id="F30BE498FF8AFF875B85FBB4FEFFBB30" box="[228,300,1068,1088]" name="Ukraine" pageId="4" pageNumber="599">Ukraine</collectingCountry>
] occ., 
<collectingMunicipality id="6BC73E72FF8AFF875A11FBB4FE4DBB30" box="[368,414,1068,1088]" pageId="4" pageNumber="599">Tatra</collectingMunicipality>
25/7, 
<collectorName id="26E9C1DEFF8AFF875ABEFBB4FD85BB30" box="[479,598,1068,1088]" pageId="4" pageNumber="599">S. Stobiecki.</collectorName>
&quot; [hw?], &quot;tatricus 
<taxonomicNameLabel id="A25BC561FF8AFF87599DFBB4FCE2BB30" box="[764,817,1068,1088]" pageId="4" pageNumber="599" rank="species">n. sp.</taxonomicNameLabel>
&quot; [hw?], &quot; 
<collectingCounty id="62C2DC84FF8AFF8758EFFBB4FC3ABB30" box="[910,1001,1068,1088]" pageId="4" pageNumber="599">Co-Typus</collectingCounty>
&quot; [red, unauthorised curatorial designation], &quot; 
<typeStatus id="54A71AAAFF8AFF875974FBDCFDAABB28" box="[533,633,1092,1112]" pageId="4" pageNumber="599">Lectotypus</typeStatus>
, 
<taxonomicName id="4C1CDF8BFF8AFF8759E5FBDCFC26BB28" authority="Kinel 1949" authorityName="KINEL" authorityYear="1949" box="[644,1013,1092,1112]" class="Insecta" family="Dytiscidae" genus="Hydroporus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="599" phylum="Arthropoda" rank="subSpecies" species="discretus" subSpecies="tatricus">
Hydroporus discretus tatricus 
<bibRefCitation id="EF8DD9F9FF8AFF8758EEFBDCFC26BB28" author="KINEL J" box="[911,1013,1092,1112]" pageId="4" pageNumber="599" pagination="337 - 405" refId="ref4470" refString="KINEL J. (1949): Les Hydradephaga de Pologne et des pays limitrophes. - Polskie Pismo Entomologiczne 18: 337 - 405." type="journal article" year="1949">Kinel 1949</bibRefCitation>
</taxonomicName>
, des. 
<collectorName id="26E9C1DEFF8AFF875BA5FBC4FED4BB00" box="[196,263,1116,1136]" pageId="4" pageNumber="599">H. Fery</collectorName>
2005&quot; [red, printed] (
<collectionCode id="ED0D3CCDFF8AFF875AA3FBC4FE2CBB00" box="[450,511,1116,1136]" country="Austria" httpUri="http://grbio.org/cool/c7yt-hh77" name="Naturhistorisches Museum, Wien" pageId="4" pageNumber="599">NMW</collectionCode>
