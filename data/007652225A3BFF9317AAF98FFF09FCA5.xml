<taxonomicName id="4FDF98B75A3BFF95167FF8B6FC3EF8A7" authority="Kawahara, Nishida and Davis" authorityName="Kawahara, Nishida and Davis" box="[322,969,1833,1858]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="327" phylum="Arthropoda" rank="species" species="tropaeolicola">
