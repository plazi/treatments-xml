<taxonomicName id="9CD24D1372626153FC7DF922FB87F952" authorityName="Kalina" authorityYear="1988" box="[925,1071,1665,1688]" class="Insecta" family="Eupelmidae" genus="Eupelmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="tryapitzini">
<emphasis id="69A6EA8272626153FC7DF922FB87F952" box="[925,1071,1665,1688]" italics="true" pageId="85" pageNumber="86">E. tryapitzini</emphasis>
</taxonomicName>
