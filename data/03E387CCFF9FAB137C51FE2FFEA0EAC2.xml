<subSubSection id="C3506551FF9FAB137E82FAF3FED7EA32" lastPageId="119" lastPageNumber="120" pageId="118" pageNumber="119" type="description">
<paragraph id="8BF536DAFF9FAB127E82FAF3FEA0EE2C" blockId="118.[189,334,1340,1366]" box="[189,334,1340,1366]" pageId="118" pageNumber="119">
<heading id="D0BD81B6FF9FAB127E82FAF3FEA0EE2C" bold="true" box="[189,334,1340,1366]" fontSize="11" level="3" pageId="118" pageNumber="119" reason="3">
<emphasis id="B93EEAC8FF9FAB127E82FAF3FEA0EE2C" bold="true" box="[189,334,1340,1366]" pageId="118" pageNumber="119">Description</emphasis>
</heading>
</paragraph>
<paragraph id="8BF536DAFF9FAB127ED5FAA4FE29EEFF" blockId="118.[189,1258,1386,1453]" box="[234,455,1386,1413]" pageId="118" pageNumber="119">
<emphasis id="B93EEAC8FF9FAB127ED5FAA4FEABEEFF" bold="true" box="[234,325,1387,1413]" pageId="118" pageNumber="119">Female</emphasis>
(
<typeStatus id="54F18878FF9FAB127F6AFAA5FE51EEFF" box="[341,447,1386,1413]" pageId="118" pageNumber="119">holotype</typeStatus>
