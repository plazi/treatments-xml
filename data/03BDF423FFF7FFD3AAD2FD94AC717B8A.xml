<taxonomicName id="4C143EB6FFF7FFD3AAD2FD94AF497D65" authority="(CURTIS, 1826)" baseAuthorityName="CURTIS" baseAuthorityYear="1826" box="[189,571,563,590]" class="Insecta" family="Hystrichopsyllidae" genus="Hystrichopsylla" kingdom="Animalia" order="Siphonaptera" pageId="7" pageNumber="368" phylum="Arthropoda" rank="species" species="talpae">
