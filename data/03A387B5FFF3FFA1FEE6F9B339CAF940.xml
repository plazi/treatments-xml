<paragraph id="8BB536A3FFFCFFA1FED6FCA53F5BFC60" blockId="41.[264,1325,284,1790]" pageId="41" pageNumber="42">
<emphasis id="B97EEAB1FFFCFFA1FED6FCA53BB2FC30" bold="true" box="[312,451,884,910]" pageId="41" pageNumber="42">Type series</emphasis>
. 
<emphasis id="B97EEAB1FFFCFFA1FE3DFCA53820FC30" bold="true" box="[467,593,884,910]" pageId="41" pageNumber="42">
<collectingCountry id="F31D7633FFFCFFA1FE3DFCA5383CFC30" box="[467,589,884,910]" name="Australia" pageId="41" pageNumber="42">Australia</collectingCountry>
