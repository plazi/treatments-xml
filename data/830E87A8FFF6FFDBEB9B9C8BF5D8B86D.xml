(
<bibRefCitation id="6F364B4FFFF6FFDBEAA59C8BF20CBE91" author="Siddiqi" box="[425,592,692,718]" pageId="7" pageNumber="158" refString="Siddiqi, M. R. (1963) Four new species in the subfamily Tylenchinae (Nematoda) from north India. Zeitschrift fur Parasitenkunde, 23, 397 - 404. http: // dx. doi. org / 10.1007 / BF 00331239" type="journal article" year="1963">
