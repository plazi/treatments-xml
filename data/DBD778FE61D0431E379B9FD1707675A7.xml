- 
<geoCoordinate id="620801BA41595CB3B61D259223EA3BCF" degrees="25" direction="north" minutes="29" orientation="latitude" precision="15" seconds="26" value="25.490557">25°29'26&quot;N</geoCoordinate>
, 
<geoCoordinate id="EC3BAC46D484BBFAFB8818856E1173A6" degrees="99" direction="east" minutes="06" orientation="longitude" precision="15" seconds="16" value="99.10445">99°06'16&quot;E</geoCoordinate>
