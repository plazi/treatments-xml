<taxonomicName id="4C7B4D0FB37AFFD9DA6BFC4E9BF4FC01" box="[1314,1429,943,968]" class="Insecta" family="Pseudococcidae" genus="Pelionella" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="202" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B90FEA9EB37AFFD9DA6BFC4E9BF4FC01" box="[1314,1429,943,968]" italics="true" pageId="26" pageNumber="227">Pelionella</emphasis>
</taxonomicName>
, namely 
<taxonomicName id="4C7B4D0FB37AFFD9DFBDFC359AE2FC24" authority="Borchsenius" authorityName="Borchsenius" box="[244,1155,980,1005]" class="Insecta" family="Pseudococcidae" genus="Peliococcus" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="227" phylum="Arthropoda" rank="variety" species="balteatus" subSpecies="cycliger" variety="glandulifer">
