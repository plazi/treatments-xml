(Males of 
<taxonomicName id="1F9A2C9BFFA3FF84FE9CFE0327C97D6B" authority="Tsuneki" authorityName="Tsuneki" authorityYear="1976" box="[266,494,511,537]" class="Insecta" family="Crabronidae" genus="Rhopalum" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="140" phylum="Arthropoda" rank="species" species="peterseni">
