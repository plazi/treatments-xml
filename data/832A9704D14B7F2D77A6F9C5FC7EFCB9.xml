<typeStatus id="D43898B0D14B7F2D77CCF9DBFF61F9BB" box="[199,245,1629,1650]" pageId="37" pageNumber="247">Type</typeStatus>
genus: 
<taxonomicName id="CC835D91D14B7F2D764AF9DBFDD0F9BB" authority="Ritsema, 1869" authorityName="Ritsema" authorityYear="1869" box="[321,580,1629,1650]" class="Insecta" family="Leiodidae" genus="Platypsyllus" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="247" phylum="Arthropoda" rank="genus">
<emphasis id="39F7FA00D14B7F2D764AF9DBFE26F9BB" box="[321,434,1629,1650]" italics="true" pageId="37" pageNumber="247">Platypsyllus</emphasis>
Ritsema, 1869
</taxonomicName>
