<paragraph id="8BD9368A8B3FFF8CFE9FFEFFFCA2FE15" blockId="113.[264,1322,283,430]" pageId="113" pageNumber="114">
<taxonomicName id="4C664D098B3FFF8CFE9FFEFFFDD6FEE6" authorityName="Sun" authorityYear="2003" box="[312,527,324,349]" class="Insecta" family="Tachinidae" genus="Phasia" kingdom="Animalia" order="Diptera" pageId="113" pageNumber="114" phylum="Arthropoda" rank="species" species="yunnanica">
