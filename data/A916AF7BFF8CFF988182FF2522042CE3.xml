. Head, length 
<number id="97770C4AFF8CFF988526FD3C220E2DAF" box="[1075,1106,654,678]" pageId="3" pageNumber="68" value="90.0">90</number>
; width across eyes 
<number id="97770C4AFF8CFF98843EFD3C23042DAF" box="[1323,1368,654,678]" pageId="3" pageNumber="68" value="150.0">150</number>
; ocellar setae III length 
<number id="97770C4AFF8CFF98807FFD0027D52DC3" box="[362,393,690,714]" pageId="3" pageNumber="68" value="15.0">15</number>
. Pronotum, length 
<number id="97770C4AFF8CFF988348FD0024D62DC3" box="[605,650,690,714]" pageId="3" pageNumber="68" value="170.0">170</number>
; maximum width 
<number id="97770C4AFF8CFF988240FD0025DE2DC3" box="[853,898,690,714]" pageId="3" pageNumber="68" value="210.0">210</number>
; posteroangular setae length, inner 
<number id="97770C4AFF8CFF98841BFD0023712DC3" box="[1294,1325,690,714]" pageId="3" pageNumber="68" value="60.0">60</number>
, outer 
<number id="97770C4AFF8CFF98846CFD0023C42DC3" box="[1401,1432,690,714]" pageId="3" pageNumber="68" value="50.0">50</number>
