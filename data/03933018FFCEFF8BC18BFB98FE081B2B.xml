<subSubSection id="C320D285FFCFFFB4C1DAF810FA901D95" pageId="31" pageNumber="32" type="materials_examined">
<paragraph id="8B85810EFFCFFFB4C1DAF810FA901D95" blockId="31.[151,1436,1929,2033]" pageId="31" pageNumber="32">
<emphasis id="B94E5D1CFFCFFFB4C1DAF810FE921DC7" bold="true" box="[198,315,1929,1955]" pageId="31" pageNumber="32">Material.</emphasis>
