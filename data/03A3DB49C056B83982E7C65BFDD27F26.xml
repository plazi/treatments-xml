<taxonomicName id="4C0A11DCC056B83986B1C72BFA3D7F6F" box="[1217,1436,778,803]" class="Elasmobranchii" family="Urolophidae" genus="Urolophus" kingdom="Animalia" order="Myliobatiformes" pageId="19" pageNumber="275" phylum="Chordata" rank="species" species="paucimaculatus">
