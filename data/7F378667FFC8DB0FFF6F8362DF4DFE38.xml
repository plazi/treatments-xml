<emphasis id="C5EAEB63FFC8DB0EFF3F823CDC5FFAEC" bold="true" box="[199,344,1338,1363]" inLineHeading="true" pageId="2" pageNumber="43" reason="1">Description.</emphasis>
<emphasis id="C5EAEB63FFC8DB0EFE99823DDCB2FAEC" box="[353,437,1339,1363]" italics="true" pageId="2" pageNumber="43">Female</emphasis>
(n=1; 
<typeStatus id="282589D3FFC8DB0EFDFA823CDF63FAED" box="[514,612,1338,1362]" pageId="2" pageNumber="43">holotype</typeStatus>
in parentheses): Length 
<quantity id="30669A94FFC8DB0EFC87823CDEE4FAED" box="[895,995,1338,1362]" metricMagnitude="-3" metricUnit="m" metricValue="2.87" pageId="2" pageNumber="43" unit="mm" value="2.87">2.87 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFC0C823CD967FAED" box="[1012,1120,1338,1362]" metricMagnitude="-3" metricUnit="m" metricValue="2.97" pageId="2" pageNumber="43" unit="mm" value="2.97">2.97 mm</quantity>
), maximum width 
<quantity id="30669A94FFC8DB0EFACF823CD89BFAED" box="[1335,1436,1338,1362]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="2" pageNumber="43" unit="mm" value="1.6">1.60 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFF678258DC0FFAC9" box="[159,264,1374,1398]" metricMagnitude="-3" metricUnit="m" metricValue="1.67" pageId="2" pageNumber="43" unit="mm" value="1.67">1.67 mm</quantity>
). 
<emphasis id="C5EAEB63FFC8DB0EFEDB8258DC58FAC9" box="[291,351,1374,1398]" italics="true" pageId="2" pageNumber="43">Head</emphasis>
: Length 
<quantity id="30669A94FFC8DB0EFE3D8258DF2FFAC9" box="[453,552,1374,1398]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="2" pageNumber="43" unit="mm" value="0.6">0.60 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFDCF8258DFA6FAC9" box="[567,673,1374,1398]" metricMagnitude="-4" metricUnit="m" metricValue="6.0" pageId="2" pageNumber="43" unit="mm" value="0.6">0.60 mm</quantity>
), width across eyes 
<quantity id="30669A94FFC8DB0EFC798258DEE3FAC9" box="[897,996,1374,1398]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="2" pageNumber="43" unit="mm" value="1.1">1.10 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFC0C8258D959FAC9" box="[1012,1118,1374,1398]" metricMagnitude="-3" metricUnit="m" metricValue="1.26" pageId="2" pageNumber="43" unit="mm" value="1.26">1.26 mm</quantity>
), interocular width 
<quantity id="30669A94FFC8DB0EFAC08258D89CFAC9" box="[1336,1435,1374,1398]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" pageId="2" pageNumber="43" unit="mm" value="0.65">0.65 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFF668284DC0CFA25" box="[158,267,1410,1434]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" pageId="2" pageNumber="43" unit="mm" value="0.75">0.75 mm</quantity>
). 
<emphasis id="C5EAEB63FFC8DB0EFED48285DC8EFA24" box="[300,393,1411,1435]" italics="true" pageId="2" pageNumber="43">Antenna</emphasis>
: Segment I 
<quantity id="30669A94FFC8DB0EFDE48284DF85FA25" box="[540,642,1410,1434]" metricMagnitude="-4" metricUnit="m" metricValue="1.4" pageId="2" pageNumber="43" unit="mm" value="0.14">0.14 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFD6D8284DE06FA25" box="[661,769,1410,1434]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="2" pageNumber="43" unit="mm" value="0.15">0.15 mm</quantity>
), II 
<quantity id="30669A94FFC8DB0EFCCD8284DE9CFA25" box="[821,923,1410,1434]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="2" pageNumber="43" unit="mm" value="0.5">0.50 mm</quantity>
(
<quantity id="30669A94FFC8DB0EFC568284D91DFA25" box="[942,1050,1410,1434]" metricMagnitude="-4" metricUnit="m" metricValue="5.2" pageId="2" pageNumber="43" unit="mm" value="0.52">0.52 mm</quantity>
