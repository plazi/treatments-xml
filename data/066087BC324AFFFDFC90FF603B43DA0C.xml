<quantity id="49319B4F324AFFFDFAEBFE9C3B70D971" metricMagnitude="3" metricUnit="m" metricValue="1.28" pageId="8" pageNumber="541" unit="m" value="1280.0">1280 m</quantity>
a.s.l., 
<geoCoordinate id="EBFD506D324AFFFDFC3BFEFE3C31D971" box="[902,1027,322,346]" degrees="45.5626" direction="north" orientation="latitude" pageId="8" pageNumber="541" precision="5" value="45.5626">45.5626° N</geoCoordinate>
