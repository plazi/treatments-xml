<geoCoordinate id="EBFD506D324AFFFDFBB0FEFE3CA7D971" box="[1037,1173,322,346]" degrees="024.6093" direction="east" orientation="longitude" pageId="8" pageNumber="541" precision="5" value="24.6093">024.6093° E</geoCoordinate>
