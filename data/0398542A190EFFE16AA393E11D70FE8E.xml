<subSubSection id="C32BB6B7190EFFE26AE094E51D6BFA11" pageId="49" pageNumber="49" type="description">
<paragraph id="8B8EE53C190EFFE26AE094E51D6BFA11" blockId="49.[128,654,1259,1759]" pageId="49" pageNumber="49">
<typeStatus id="548A5B9E190EFFE26AE094E51E32FA87" box="[156,275,1259,1283]" pageId="49" pageNumber="49">SYNTYPES</typeStatus>
: 
<emphasis id="B945392E190EFFE26B4194E21D2AFA87" bold="true" box="[317,523,1260,1281]" pageId="49" pageNumber="49">AMNH 671356</emphasis>
, 
<emphasis id="B945392E190EFFE2684F94E21FF3FA99" bold="true" pageId="49" pageNumber="49">AMNH 671357</emphasis>
, adult males, collected on Mount Elliot, 
<geoCoordinate id="EE0583FB190EFFE26AB095261E38FABB" box="[204,281,1320,1341]" degrees="19.30" direction="south" orientation="latitude" pageId="49" pageNumber="49" precision="555" value="-19.3">19.30S</geoCoordinate>
, 
<geoCoordinate id="EE0583FB190EFFE26B5A95261EA0FABB" box="[294,385,1320,1341]" degrees="147.00" direction="east" orientation="longitude" pageId="49" pageNumber="49" precision="555" value="147.0">147.00E</geoCoordinate>
(Times 
<taxonomicName id="4C319EBF190EFFE26BA395261D3CFAB8" box="[479,541,1320,1342]" class="Mollusca (awaiting allocation)" family="Mollusca (awaiting allocation)" genus="Atlas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca (awaiting allocation)" pageId="49" pageNumber="3" phylum="Mollusca" rank="genus">Atlas</taxonomicName>
