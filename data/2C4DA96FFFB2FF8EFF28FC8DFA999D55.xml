<taxonomicName id="63E463FAFFB2FF8FFA8CFB75FF7F989B" authorityName="Russell" authorityYear="1953" class="Hydrozoa" family="Hydractiniidae" genus="Podocoryne" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anthoathecata" pageId="6" pageNumber="398" phylum="Cnidaria" rank="species" species="minima">
