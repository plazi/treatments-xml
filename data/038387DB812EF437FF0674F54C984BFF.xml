<location id="8EF56016812EF436FCBD74CC4E464F5B" LSID="urn:lsid:plazi:treatment:038387DB812EF437FF0674F54C984BFF:8EF56016812EF436FCBD74CC4E464F5B" box="[819,869,1779,1800]" country="Turkey" county="Sultandagh" municipality="Akscheher" name="Type" pageId="12" pageNumber="30">Type</location>
