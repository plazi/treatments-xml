<taxonomicName id="4C6A72BA9200C9540644F9B0FD8638B0" authority="Lutken, 1856" authorityName="Lutken" authorityYear="1856" box="[283,526,1662,1684]" class="Ophiuroidea" family="Ophiotrichidae" genus="Ophiactis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ophiurida" pageId="44" pageNumber="45" phylum="Echinodermata" rank="genus">
