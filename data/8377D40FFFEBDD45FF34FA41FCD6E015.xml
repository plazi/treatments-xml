<caption id="5FA13591FFE6DD44FF34F836FD7AE5C4" httpUri="https://zenodo.org/record/258254/files/figure.png" pageId="8" pageNumber="537" targetBox="[156,1435,198,1915]" targetPageId="8">
<paragraph id="0B616519FFE6DD44FF34F836FD7AE5C4" blockId="8.[151,1437,1945,2028]" pageId="8" pageNumber="537">
