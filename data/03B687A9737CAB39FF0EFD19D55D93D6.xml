<emphasis id="B96BEAAD737CAB39FF0EFD19D56C94B4" bold="true" box="[151,625,650,676]" pageId="20" pageNumber="21">
Subgenus 
<taxonomicName id="4C1F4D3C737CAB39FE8EFD19D56C94B4" authority="Thor, 1901" authorityName="Thor" authorityYear="1901" box="[279,625,650,676]" class="Arachnida" family="Sperchonidae" genus="Hispidosperchon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
<emphasis id="B96BEAAD737CAB39FE8EFD19D6F994B4" bold="true" box="[279,484,650,676]" italics="true" pageId="20" pageNumber="21">Hispidosperchon</emphasis>
<bibRefCitation id="EF8E4B4E737CAB39FE72FD19D56C94B4" author="Thor" box="[491,625,650,676]" pageId="20" pageNumber="33" refString="Thor, S. (1901) Fjerde bidrag til kundskaben om Norges Hydrachnider. - &quot; Norske Hydrachnider &quot; IV. Archiv for Mathematik og Naturvidenskab, 23, 1 - 56." type="journal article" year="1901">
