<taxonomicName id="4C514D6C9E50D545FC6A59B8A18BFC70" box="[1012,1182,985,1009]" class="Insecta" family="Tephritidae" genus="Anastrepha" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="527" phylum="Arthropoda" rank="species" species="haplacantha">
<emphasis id="B925EAFD9E50D545FC6A59B8A18BFC70" box="[1012,1182,985,1009]" italics="true" pageId="17" pageNumber="527">A. haplacantha</emphasis>
</taxonomicName>
(phallus 
<quantity id="4CA99B0A9E50D545FA9859B8A049FC70" box="[1286,1372,985,1009]" metricMagnitude="-3" metricUnit="m" metricValue="7.2" pageId="17" pageNumber="527" unit="mm" value="7.2">7.2 mm</quantity>
