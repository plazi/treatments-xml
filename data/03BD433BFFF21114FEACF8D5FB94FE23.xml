<taxonomicName id="4C1489AEFFF21115FEACF8D5FDB3F8F1" authority="Wu, 1935" authorityName="Wu" authorityYear="1935" box="[363,596,1848,1873]" class="Insecta" family="Styloperlidae" genus="Styloperla" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="12" phylum="Arthropoda" rank="genus">
