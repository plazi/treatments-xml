<subSubSection id="C37314BC9D099E14FC8EBBB4514DFD3C" pageId="4" pageNumber="671" type="distribution">
<paragraph id="8BD647379D099E14FC8EBBB4514DFD3C" blockId="4.[828,1436,698,1297]" pageId="4" pageNumber="671">
<emphasis id="B91D9B259D099E14FC8EBBB451EAFD1C" bold="true" box="[828,977,698,721]" pageId="4" pageNumber="671">Distribution.</emphasis>
<collectingCountry id="F37E07A79D099E14FC68BBB45672FD1C" box="[986,1097,698,721]" name="Argentina" pageId="4" pageNumber="671">Argentina</collectingCountry>
, 
<collectingCountry id="F37E07A79D099E14FBE5BBB456A1FD1C" box="[1111,1178,698,721]" name="Brazil" pageId="4" pageNumber="671">Brazil</collectingCountry>
(
<collectingRegion id="49AD89D59D099E14FB19BBB45719FD1C" box="[1195,1314,698,721]" country="Brazil" name="Sao Paulo" pageId="4" pageNumber="671">São Paulo</collectingRegion>
