<paragraph id="8BD336B3FFDBF96AFF6BFA49642FF99B" blockId="31.[151,1436,1233,1690]" pageId="31" pageNumber="32">
<emphasis id="B918EAA1FFDBF96AFF6BFA49679BFA0B" bold="true" box="[199,494,1449,1474]" pageId="31" pageNumber="32">Distribution and habitat</emphasis>
. 
<taxonomicName id="4C6C4D30FFDBF96AFE52FA49657CFA0B" box="[510,777,1449,1474]" class="Bivalvia" family="Limidae" genus="Limatula" kingdom="Animalia" order="Limoida" pageId="31" pageNumber="32" phylum="Mollusca" rank="species" species="subauriculata">
<emphasis id="B918EAA1FFDBF96AFE52FA49657CFA0B" box="[510,777,1449,1474]" italics="true" pageId="31" pageNumber="32">Limatula subauriculata</emphasis>
</taxonomicName>
