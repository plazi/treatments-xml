<collectingRegion id="49A8F851FFDBF96AFAB8FA496319FA0B" box="[1300,1388,1449,1474]" country="Mexico" name="Mexico" pageId="31" pageNumber="32">
<collectingCountry id="F37B7623FFDBF96AFAB8FA496319FA0B" box="[1300,1388,1449,1474]" name="Mexico" pageId="31" pageNumber="32">Mexico</collectingCountry>
</collectingRegion>
