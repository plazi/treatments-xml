(
<figureCitation id="1376A99EE25BFFAAC9C44F2A38A3FDFE" box="[159,238,577,601]" captionStart="FIGURES 1–11" captionStartId="3.[151,264,1906,1929]" captionTargetBox="[202,1375,193,1882]" captionTargetId="figure-131@3.[202,1375,193,1882]" captionTargetPageId="3" captionText="FIGURES 1–11. Head, anterior view. 1. Homoneura (Homoneura) baihualingensis sp. nov.; 2. H. (H.) brevituba sp. nov.; 3. H. (H.) conica sp. nov.; 4. H. (H.) cornis sp. nov.; 5. H. (H.) dulongjiangica sp. nov.; 6. H. (H.) ensata sp. nov.; 7. H. (H.) gemmiformis sp. nov.; 8. H. (H.) latizona sp. nov.; 9. H. (H.) longiprocessa sp. nov.; 10. H. (H.) pilifera sp. nov.; 11. H. (H.) trilineata sp. nov.." pageId="10" pageNumber="11">Figs. 3</figureCitation>
, 
<figureCitation id="1376A99EE25BFFAAC9A14F2A3955FDFE" box="[250,280,577,601]" captionStart="FIGURES 12–22" captionStartId="4.[151,264,1877,1900]" captionTargetBox="[185,1402,208,1840]" captionTargetId="figure-133@4.[185,1402,193,1856]" captionTargetPageId="4" captionText="FIGURES 12–22. Thorax, dorsal view. 12. Homoneura (Homoneura) baihualingensis sp. nov.; 13. H. (H.) brevituba sp. nov.; 14. H. (H.) conica sp. nov.; 15. H. (H.) cornis sp. nov.; 16. H. (H.) dulongjiangica sp. nov.; 17. H. (H.) ensata sp. nov.; 18. H. (H.) gemmiformis sp. nov.; 19. H. (H.) latizona sp. nov.; 20. H. (H.) longiprocessa sp. nov.; 21. H. (H.) pilifera sp. nov.; 22. H. (H.) trilineata sp. nov.." pageId="10" pageNumber="11">14</figureCitation>
, 
<figureCitation id="1376A99EE25BFFAAC87E4F2A390EFDFE" box="[293,323,577,601]" captionStart="FIGURES 23–33" captionStartId="5.[151,264,1877,1900]" captionTargetBox="[207,1392,212,1827]" captionTargetId="figure-129@5.[185,1402,193,1856]" captionTargetPageId="5" captionText="FIGURES 23–33. Wing. 23. Homoneura (Homoneura) baihualingensis sp. nov.; 24. H. (H.) brevituba sp. nov.; 25. H. (H.) conica sp. nov.; 26. H. (H.) cornis sp. nov.; 27. H. (H.) dulongjiangica sp. nov.; 28. H. (H.) ensata sp. nov.; 29. H. (H.) gemmiformis sp. nov.; 30. H. (H.) latizona sp. nov.; 31. H. (H.) longiprocessa sp. nov.; 32. H. (H.) pilifera sp. nov.; 33. H. (H.) trilineata sp. nov.." pageId="10" pageNumber="11">25</figureCitation>
, 
<figureCitation id="1376A99EE25BFFAAC8154F2A3921FDFE" box="[334,364,577,601]" captionStart="FIGURES 34–41" captionStartId="6.[151,264,1914,1937]" captionTargetBox="[188,1396,197,1874]" captionTargetId="figure-103@6.[188,1399,193,1893]" captionTargetPageId="6" captionText="FIGURES 34–41. Abdomen, dorsal view. 34. Homoneura (Homoneura) baihualingensis sp. nov.; 35. H. (H.) brevituba sp. nov.; 36. H. (H.) conica sp. nov.; 37. H. (H.) cornis sp. nov.; 38. H. (H.) ensata sp. nov.; 39. H. (H.) latizona sp. nov.; 40. H. (H.) longiprocessa sp. nov.; 41. H. (H.) pilifera sp. nov.." pageId="10" pageNumber="11">36</figureCitation>
, 
<figureCitation id="1376A99EE25BFFAAC8234F2A398DFDFE" box="[376,448,577,601]" captionStart="FIGURES 52–56" captionStartId="11.[151,264,1822,1845]" captionTargetBox="[162,1426,498,1795]" captionTargetId="figure-185@11.[151,1436,483,1801]" captionTargetPageId="11" captionText="FIGURES 52–56. Homoneura (Homoneura) conica sp. nov. Male. 52. epandrium complex, lateral view; 53. epandrial complex, posterior view; 54. protandrium, anterior view; 55. aedeagal complex, ventral view; 56. aedeagal complex, lateral view. Scale 0.5 mm." pageId="10" pageNumber="11">52–56</figureCitation>
