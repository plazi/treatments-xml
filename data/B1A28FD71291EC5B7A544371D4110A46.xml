<taxonomicName id="311B24DFC44B48BC6717D3D17455AB28" class="Actinopterygii" family="Lutjanidae" genus="Lutjanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lutjanus kasmira" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="kasmira">Lutjanus kasmira</taxonomicName>
(
<normalizedToken id="527C5FE4B3E402408865765AEDDD43C0" originalValue="Forsskål">Forsskal</normalizedToken>
