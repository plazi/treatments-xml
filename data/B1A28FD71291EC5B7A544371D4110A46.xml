<taxonomicName id="3D7104401C1BD37A27FE49ECBE670E12" class="Monogenea" family="Ancyrocephalidae" genus="Haliotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Haliotrema chrysotaeniae" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="chrysotaeniae">Haliotrema chrysotaeniae</taxonomicName>
