<taxonomicName id="1ED07C4D6B7B26D793216F94FAEFB425" class="Monogenea" family="Ancyrocephalidae" genus="Euryhaliotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euryhaliotrema chrysotaeniae" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="chrysotaeniae">Euryhaliotrema chrysotaeniae</taxonomicName>
) (
<bibRefCitation id="A23B37BB979F6CEBB20F51E6AF03C40C" author="Li, HY" journalOrPublisher="Journal of Guangxi Normal University" pageId="16" pageNumber="17" pagination="97 - 100" title="Description of two species (including one new species) of genus Haliotrema and one species of genus Euryhaliotrema." volume="25" year="2007">Li and Yan 2007</bibRefCitation>
