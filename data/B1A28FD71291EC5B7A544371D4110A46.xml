<taxonomicName id="472887571BFDC5B825589D32240094D5" class="Actinopterygii" family="Lutjanidae" genus="Lutjanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lutjanus fulvus" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="fulvus">Lutjanus fulvus</taxonomicName>
