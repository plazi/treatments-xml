<taxonomicName id="5DD5595013D22AE0271A3CEAAB489E97" class="Actinopterygii" family="Lutjanidae" genus="Lutjanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lutjanus fulvus" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="fulvus">Lutjanus fulvus</taxonomicName>
