<taxonomicName id="832632A35CA9A273D2897304643ECA13" class="Actinopterygii" family="Lutjanidae" genus="Lutjanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lutjanus fulviflamma" order="Perciformes" pageId="3" pageNumber="4" phylum="Chordata" rank="species" species="fulviflamma">Lutjanus fulviflamma</taxonomicName>
(
<normalizedToken id="D68A6379665FE946318C522C1B670768" originalValue="Forsskål">Forsskal</normalizedToken>
