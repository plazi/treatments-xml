<taxonomicName id="3A18E83C23407C50DFC4E2FFF72D40DB" class="Monogenea" family="Ancyrocephalidae" genus="Euryhaliotrema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Euryhaliotrema chrysotaeniae" order="Dactylogyridea" pageId="3" pageNumber="4" phylum="Platyhelminthes" rank="species" species="chrysotaeniae">Euryhaliotrema chrysotaeniae</taxonomicName>
