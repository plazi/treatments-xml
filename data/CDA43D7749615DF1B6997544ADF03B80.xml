<emphasis id="A143447BA66EEB29F4EC350DEC21C4D0" italics="true" pageId="0" pageNumber="19">Rhamphura subdimota</emphasis>
</taxonomicName>
, 
<taxonomicName id="6D4928355E3024BC45D8E5ACC436A6F7" authority="Landry, 1991" authorityName="Nupponen 1 &amp; Sihvonen" authorityYear="2022" baseAuthorityName="Meyrick" baseAuthorityYear="1931" class="Insecta" family="Scythrididae" genus="Rhamphura" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhamphura depressa" order="Lepidoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="species" species="depressa">
<emphasis id="117AA74646AE70D72BDDF809B08BAF88" italics="true" pageId="0" pageNumber="19">Rhamphura depressa</emphasis>
