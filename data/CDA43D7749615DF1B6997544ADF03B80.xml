<taxonomicName id="4B39F591AC5F4FB730C9B70B977CAB7C" authority="Landry, 1991" authorityName="Nupponen" authorityYear="2022" class="Insecta" family="Scythrididae" genus="Rhamphura" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhamphura spiniuncus" order="Lepidoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="species" species="spiniuncus">
