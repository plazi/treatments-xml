<taxonomicName id="D6894D64FF9B8322FABBF87C2F05F811" authority="(Griffini, 1908)" authorityName="Griffini" authorityYear="1908" baseAuthorityName="Griffini" baseAuthorityYear="1908" class="Insecta" family="Phaneropteridae" genus="Eumecopoda" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="moluccarum">
