<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141671" authority="Forel" authorityName="Forel" class="Hexapoda" family="Formicidae" genus="Euponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Euponera (Pseudoponera) darwini var. africana Forel" lsidName-HNS="Euponera (Pseudoponera) darwini var. africana Forel" order="Hymenoptera" pageNumber="51" phylum="Arthropoda" rank="variety" species="darwini" species.completionEvidence="missing" species.completionRound="1" subGenus="Pseudoponera" variety="africana">Euponera (Pseudoponera) Darwini Forel, var. africana</taxonomicName>
<taxonomicNameLabel rank="variety">n. var.</taxonomicNameLabel>
</paragraph>
