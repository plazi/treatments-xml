<taxonomicName authority="Fabricius, 1792" authorityName="Fabricius" authorityYear="1792" box="[974,1286,256,279]" class="Insecta" family="Dytiscidae" genus="Hydroporus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="68" phylum="Arthropoda" rank="species" species="nigrita">
<emphasis box="[974,1084,256,279]" italics="true" pageId="3" pageNumber="68">H. nigrita</emphasis>
(Fabricius, 
<number box="[1221,1278,256,279]" pageId="3" pageNumber="68" value="1792.0">1792</number>
)
</taxonomicName>
, 
<taxonomicName authority="Aube, 1838" authorityName="Aube" authorityYear="1838" class="Insecta" family="Dytiscidae" genus="Nebrioporus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="68" phylum="Arthropoda" rank="species" species="carinatus" subGenus="Nebrioporus">
<emphasis box="[1301,1436,256,279]" italics="true" pageId="3" pageNumber="68">Nebrioporus</emphasis>
(
<emphasis box="[160,295,290,313]" italics="true" pageId="3" pageNumber="68">Nebrioporus</emphasis>
) 
<emphasis box="[312,413,290,313]" italics="true" pageId="3" pageNumber="68">carinatus</emphasis>
(Aubé, 
<number box="[503,560,290,313]" pageId="3" pageNumber="68" value="1838.0">1838</number>
)
</taxonomicName>
, 
<taxonomicName authority="Fabricius, 1792" authorityName="Fabricius" authorityYear="1792" box="[580,1182,290,313]" class="Insecta" family="Helophoridae" genus="Helophorus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="68" phylum="Arthropoda" rank="species" species="flavipes" subGenus="Ropalhelophorus">
