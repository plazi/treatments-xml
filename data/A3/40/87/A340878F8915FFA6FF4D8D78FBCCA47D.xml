<typeStatus box="[151,255,1402,1427]" pageId="8" pageNumber="257">Holotype</typeStatus>
male, Ado, 
<collectingCountry box="[405,484,1402,1427]" name="Somalia" pageId="8" pageNumber="257">Somali</collectingCountry>
