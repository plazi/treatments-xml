locality at 
<location LSID="urn:lsid:plazi:treatment:A32D87D41C60535A55DEFE082135E7D5:2E5B60191C60535A55FBF80E223FE7D5" box="[178,300,1940,1966]" name="Lake Toba" pageId="43" pageNumber="44" stateProvince="Sumatera Utara">Lake Toba</location>
, 
<collectingRegion box="[310,406,1940,1966]" country="Indonesia" name="Sumatera Utara" pageId="43" pageNumber="44">Sumatra</collectingRegion>
, 
<date box="[418,546,1940,1966]" pageId="43" pageNumber="44" value="1929-04-17">
<collectingDate box="[418,546,1940,1966]" pageId="43" pageNumber="44" value="1929-04-17">17-iv-1929</collectingDate>
</date>
</materialsCitation>
.
</paragraph>
</subSubSection>
