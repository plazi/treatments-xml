) (
<collectionCode box="[569,652,1365,1391]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35009" lsid="urn:lsid:biocol.org:col:35009" name="New York State Museum" pageId="38" pageNumber="39">NYSM</collectionCode>
