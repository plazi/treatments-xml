<taxonomicName authorityName="Felt" authorityYear="1918" class="Insecta" family="Cecidomyiidae" genus="Luzonomyia" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
