. 
<materialsCitation ID-GBIF-Occurrence="3026660446" accessionNumber="JX110976, JX110979" collectionCode="JX" country="China" location="Xiamen" municipality="Oecusee" pageId="59" pageNumber="60" specimenCount="1">
