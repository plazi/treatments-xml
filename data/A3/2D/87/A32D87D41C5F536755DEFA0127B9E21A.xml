, 
<collectingRegion box="[333,385,1647,1673]" country="Indonesia" name="Jawa Timur" pageId="20" pageNumber="21">Java</collectingRegion>
, alt. 
<quantity box="[441,529,1647,1673]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="20" pageNumber="21" unit="m" value="1600.0">
<elevation box="[441,529,1647,1673]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="20" pageNumber="21" unit="m" value="1600.0">1600 m</elevation>
