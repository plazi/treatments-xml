<taxonomicName authority="Cockerell" authorityName="Cockerell" authorityYear="1905" class="Insecta" family="Coccidae" genus="Pulvinaria" kingdom="Animalia" order="Hemiptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="polygonata">
