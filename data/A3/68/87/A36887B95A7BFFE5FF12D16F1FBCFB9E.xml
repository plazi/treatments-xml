<taxonomicName authority="Langerhans, 1879" authorityName="Langerhans" authorityYear="1879" box="[303,640,1901,1923]" class="Polychaeta" family="Syllidae" genus="Syllis" kingdom="Animalia" order="Phyllodocida" pageId="33" pageNumber="234" phylum="Annelida" rank="species" species="rosea">
