<subSubSection type="nomenclature">
<paragraph pageNumber="230">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181693" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" lsidName-HNS="Pheidole seligmanni Wilson" order="Hymenoptera" pageNumber="230" phylum="Arthropoda" rank="species" species="seligmanni" status="new species">Pheidole seligmanni</taxonomicName>
