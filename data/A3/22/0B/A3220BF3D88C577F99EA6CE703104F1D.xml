<taxonomicName genus="P." lsidName="P. jaegerskioeldi" pageId="0" pageNumber="1" rank="species" species="jaegerskioeldi">
<emphasis italics="true" pageId="0" pageNumber="1">P. jaegerskioeldi</emphasis>
</taxonomicName>
