</taxonomicName>
(Harding, 1920); 
<taxonomicName genus="P." lsidName="P. undulata" pageId="0" pageNumber="1" rank="species" species="undulata">
<emphasis italics="true" pageId="0" pageNumber="1">P. undulata</emphasis>
</taxonomicName>
(Harding, 1924); 
<taxonomicName genus="P." lsidName="P. horai" pageId="0" pageNumber="1" rank="species" species="horai">
<emphasis italics="true" pageId="0" pageNumber="1">P. horai</emphasis>
</taxonomicName>
(Baugh, 1960); 
<taxonomicName genus="P." lsidName="P. indica" pageId="0" pageNumber="1" rank="species" species="indica">
<emphasis italics="true" pageId="0" pageNumber="1">P. indica</emphasis>
</taxonomicName>
