<taxonomicName LSID="A3727780-F658-5CD3-A125-4E69C3BF3DF4" authority="Wang &amp; Bourgoin, 2020" authorityName="Wang &amp; Bourgoin" authorityYear="2020" class="Insecta" family="Issidae" genus="Eusarimissus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eusarimissus" order="Hemiptera" pageId="0" pageNumber="13" phylum="Arthropoda" rank="genus" status="gen. nov.">Eusarimissus</taxonomicName>
