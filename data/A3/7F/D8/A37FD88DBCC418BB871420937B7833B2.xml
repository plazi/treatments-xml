<subSubSection pageId="18" pageNumber="49" type="materials_examined">
<paragraph pageId="18" pageNumber="49">
<pageBreakToken pageId="18" pageNumber="49" start="start">Material</pageBreakToken>
examined.
</paragraph>
<paragraph pageId="18" pageNumber="49">
Canada, New Brunswick, York Co., Charters Settlement, 
<geoCoordinate direction="north" orientation="latitude" precision="5" value="45.8395">45.8395°N</geoCoordinate>
, 
<geoCoordinate direction="west" orientation="longitude" precision="5" value="-66.7391">66.7391°W</geoCoordinate>
