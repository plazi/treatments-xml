<taxonomicName baseAuthorityName="Wellnhofer" baseAuthorityYear="1987" class="Reptilia" family="Criorhynchidae" genus="Tropeognathus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pterosauria" pageId="21" pageNumber="22" phylum="Chordata" rank="species" species="mesembrinus">
