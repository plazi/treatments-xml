<taxonomicName baseAuthorityName="Mader and Kellner" baseAuthorityYear="1999" class="Reptilia" family="Anhangueridae" genus="Siroccopteryx" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pterosauria" pageId="21" pageNumber="22" phylum="Chordata" rank="species" species="moroccensis">
