<taxonomicName class="Insecta" family="Staphylinidae" genus="Pselaptus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pselaptus" order="Coleoptera" pageId="100" pageNumber="101" phylum="Arthropoda" rank="genus">Pselaptus</taxonomicName>
