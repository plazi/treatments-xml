<subSubSection pageId="5" pageNumber="128" type="nomenclature">
<paragraph pageId="5" pageNumber="128">
<taxonomicName authority="Yamanaka, 1988" authorityName="Yamanaka" authorityYear="1988" class="Insecta" family="Crambidae" genus="Udea" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Udea stationalis" order="Lepidoptera" pageId="5" pageNumber="128" phylum="Arthropoda" rank="species" species="stationalis">Udea stationalis Yamanaka, 1988</taxonomicName>
