<location id="8C167FDBAC023B65FE21DAAA5E05FCA2" LSID="urn:lsid:plazi:treatment:01609816AC023B65FF1DDA4A5D48FB46:8C167FDBAC023B65FE21DAAA5E05FCA2" box="[496,582,751,771]" name="Yalanzou" pageId="78" pageNumber="471" stateProvince="Nimba">Yalanzou</location>
