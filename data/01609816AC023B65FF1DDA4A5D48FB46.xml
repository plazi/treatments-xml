<collectingDate id="ED33F628AC023B65FCFADAAA5850FCA2" box="[811,1043,751,771]" isEnumeration="true" pageId="78" pageNumber="471">
<date id="FD770FC0AC023B65FCFADAAA5FF7FCA2" box="[811,948,751,771]" pageId="78" pageNumber="471" value="1957-03-27" valueMax="1957-03-30" valueMin="1957-03-27">27-30.III.1957</date>
