<paragraph id="8BC44131FFCBFFF1FF42F90E6769D497" blockId="1.[151,759,1711,1771]" box="[151,307,1746,1771]" pageId="1" pageNumber="96">
(
<figureCitation id="13405DB4FFCBFFF1FF4AF90E66BFD497" box="[159,229,1746,1771]" captionStart="FIGURES 1, 2" captionStartId="2.[151,264,1199,1222]" captionTargetBox="[151,1436,303,1175]" captionTargetId="figure@2.[151,1436,303,1175]" captionTargetPageId="2" captionText="FIGURES 1, 2. Stethantyx spp., ♀, holotype, habitus: 1 — S. erwini sp. nov.; 2 — S. radiata sp. nov." httpUri="https://zenodo.org/record/244939/files/figure.png" pageId="1" pageNumber="96">Figs 1</figureCitation>
, 
<figureCitation id="13405DB4FFCBFFF1FF26F90F6776D497" box="[243,300,1747,1771]" captionStart="FIGURES 7 – 11" captionStartId="4.[151,264,1230,1253]" captionTargetBox="[151,1436,195,1206]" captionTargetId="figure@4.[151,1436,195,1207]" captionTargetPageId="4" captionText="FIGURES 7 – 11. Stethantyx erwini sp. nov., ♀, holotype: 7 — antenna, lateral; 8 — head, ventro-frontal; 9 — mesosoma, posterolateral (arrow indicates notaular wrinkle); 10 — propodeum, dorsal; 11 — apex of ovipositor, lateral." httpUri="https://zenodo.org/record/244941/files/figure.png" pageId="1" pageNumber="96">
7– 
<number id="3DB35316FFCBFFF1FEC7F90F6776D497" box="[274,300,1747,1771]" pageId="1" pageNumber="96" value="11.0">11</number>
</figureCitation>
)
