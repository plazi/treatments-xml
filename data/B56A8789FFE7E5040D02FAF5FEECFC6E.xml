Genus 
<taxonomicName id="FAC34D1CFFE7E5030DBCFAF5FE27FAAE" authority="RoDING, 1798" authorityName="RoDING" authorityYear="1798" box="[199,403,1325,1350]" class="Gastropoda" family="Ampullariidae" genus="Pila" kingdom="Animalia" order="Architaenioglossa" pageId="3" pageNumber="4" phylum="Mollusca" rank="genus">
