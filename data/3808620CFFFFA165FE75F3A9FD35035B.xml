<taxonomicName id="77A1A899FFFDA165FE57F44DFDEB0419" authorityName="Sali &amp; Doğan" authorityYear="2005" box="[400,541,835,859]" class="Arachnida" family="Stigmaeidae" genus="Eustigmaeus" kingdom="Animalia" order="Prostigmata" pageId="18" pageNumber="852" phylum="Arthropoda" rank="species" species="erzincanensis">
