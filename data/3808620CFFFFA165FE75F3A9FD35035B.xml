<typeStatus id="6F1A6DB8FFFDA165FD7BF44DFD1B0419" box="[700,749,835,859]" pageId="18" pageNumber="852">type</typeStatus>
locality, 
<collectingRegion id="72651DF8FFFDA165FC9EF44DFC320419" box="[857,964,835,859]" country="Turkey" name="Erzincan" pageId="18" pageNumber="852">Erzincan</collectingRegion>
