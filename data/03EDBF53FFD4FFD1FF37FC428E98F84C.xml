</taxonomicName>
and 
<taxonomicName id="4C4475C6FFD5FFD1FF37F8118D6FF827" authorityName="Scheuten, Evans" authorityYear="1953" box="[151,317,1932,1957]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="45" pageNumber="46" phylum="Arthropoda" rank="genus">
