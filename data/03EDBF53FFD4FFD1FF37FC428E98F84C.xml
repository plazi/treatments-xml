<location id="8E9B589EFFD4FFD0FC49F8D78823F8E6" LSID="urn:lsid:plazi:treatment:03EDBF53FFD4FFD1FF37FC428E98F84C:8E9B589EFFD4FFD0FC49F8D78823F8E6" box="[1001,1137,1866,1892]" country="Ghana" name="Cape Coast" pageId="44" pageNumber="45">Cape Coast</location>
, 
<collectingCountry id="F3534ED5FFD4FFD0FBDDF8D7889EF8E6" box="[1149,1228,1866,1892]" name="Ghana" pageId="44" pageNumber="45">Ghana</collectingCountry>
