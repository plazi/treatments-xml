<collectionCode id="199EAE37FFABFFF1FACBFF5AFAAA4C8C" box="[1283,1372,152,175]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="60" pageNumber="61">MNHN</collectionCode>
