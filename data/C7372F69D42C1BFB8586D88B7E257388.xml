<taxonomicName id="7511A72172A95C27C3C6BEE9C9018BE6" class="Magnoliopsida" family="Ranunculaceae" genus="Aconitum" kingdom="Plantae" order="Ranunculales" pageId="null" pageNumber="24" phylum="Tracheophyta" rank="species" species="bauhinii">
<italic id="0BC4337C50F20BB0DCDBDDE7AD385BD5" pageId="null" pageNumber="24">A. Bauhinii</italic>
</taxonomicName>
(Nr. 4c)
</td>
</tr>
<tr id="90F4321E2C478225DD891697B5BBCA8E" pageId="null" pageNumber="24">
<td id="F4D044E033E310F59C5DF78593561AE2" align="left" pageId="null" pageNumber="24" valign="top">
3*. Blattzipfel 
<normalizedToken id="64EC4B4570CAD0267B269C4A0513EFD3" originalValue="allmählich" pageId="null" pageNumber="24">allmaehlich</normalizedToken>
zugespitzt; 
<normalizedToken id="466AF5B5CCBF86B0984B70FC5E534648" originalValue="Perigonblätter" pageId="null" pageNumber="24">Perigonblaetter</normalizedToken>
