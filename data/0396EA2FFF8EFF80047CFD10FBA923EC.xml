<taxonomicName id="4C3F20BAFF8EFF8006B9FB8FFB962278" authorityName="Allender and Kraft" authorityYear="1983" box="[932,1085,1120,1144]" class="Phaeophyceae" family="Dictyotaceae" genus="Padina" kingdom="Chromista" order="Dictyotales" pageId="45" pageNumber="3002" phylum="Ochrophyta" rank="species" species="boergesenii">
