<number id="FB76D27077758B26FC8B94DDFC2FE5C6" box="[846,922,413,439]" pageId="12" pageNumber="13" value="17.0" valueMax="18.0" valueMin="16.0">16–18</number>
; VIII, 
<number id="FB76D27077758B26FC2994DDFB8CE5C6" box="[1004,1081,413,439]" pageId="12" pageNumber="13" value="13.0" valueMax="14.0" valueMin="12.0">12–14</number>
. Sternal setae: II, 
<quantity id="8A466DB277758B26FAD494DDFAE8E5C6" box="[1297,1373,413,439]" metricMagnitude="-2" metricUnit="m" metricValue="8.89" metricValueMax="10.16" metricValueMin="7.62" pageId="12" pageNumber="13" unit="in" value="3.5" valueMax="4.0" valueMin="3.0">3–4 in</quantity>
each aster, 
<number id="FB76D27077758B26FF1E9482FE91E5AD" box="[219,292,450,476]" pageId="12" pageNumber="13" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
marginal between asters, 
