<taxonomicName id="4C444D78B433FFF41F1E5DFFFE527E0D" authorityName="Meyrick" authorityYear="1907" box="[336,481,1519,1544]" class="Insecta" family="Gelechiidae" genus="Thyrsostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
<emphasis id="B930EAE9B433FFF41F1E5DFFFE527E0D" box="[336,481,1519,1544]" italics="true" pageId="35" pageNumber="36">Thyrsostoma</emphasis>
</taxonomicName>
