<taxonomicName id="4C444D78B433FFF41CBB5DFFFCC37E0D" authorityName="Pylartis (Meyrick" authorityYear="1908" box="[757,880,1519,1544]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
