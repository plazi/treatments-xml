<taxonomicName id="4C444D78B433FFF41F465E04FE307E28" authorityName="Pylartis (Meyrick" authorityYear="1908" box="[264,387,1556,1581]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
<emphasis id="B930EAE9B433FFF41F465E04FE307E28" box="[264,387,1556,1581]" italics="true" pageId="35" pageNumber="36">Palumbina</emphasis>
</taxonomicName>
