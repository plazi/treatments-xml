<taxonomicName id="6DD35CABE26DFFEB36BC54221312FB99" authorityName=": Benoit" authorityYear="1968" box="[920,1054,1085,1109]" class="Arachnida" family="Selenopidae" genus="Anyphops" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="1384" phylum="Arthropoda" rank="species" species="lawrencei">
