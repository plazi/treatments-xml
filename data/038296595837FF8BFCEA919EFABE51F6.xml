<caption id="DF5477C75834FF8BFF2B9029FD3D5257" ID-DOI="http://doi.org/10.5281/zenodo.5438134" ID-Zenodo-Dep="5438134" httpUri="https://zenodo.org/record/5438134/files/figure.png" pageId="6" pageNumber="569" startId="6.[144,223,1280,1299]" targetBox="[144,763,877,1251]" targetPageId="6">
