<taxonomicName id="4C4A4D3FAA74FFA20867FB21F2FDFB10" authorityName="Sawada" authorityYear="1955" box="[843,950,1159,1180]" class="Insecta" family="Staphylinidae" genus="Halesthenus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="34" phylum="Arthropoda" rank="species" species="nakanei">
