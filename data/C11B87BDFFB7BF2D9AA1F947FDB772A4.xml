<taxonomicName id="8EB24D28FFB4BF2D98BDFB90FDD472F8" authorityName="Hill" authorityYear="1964" box="[438,624,1138,1164]" class="Mammalia" family="Vespertilionidae" genus="Murina" kingdom="Animalia" order="Chiroptera" pageId="22" pageNumber="434" phylum="Chordata" rank="species" species="peninsularis">
