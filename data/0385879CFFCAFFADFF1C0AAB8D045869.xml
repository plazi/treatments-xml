<quantity id="4CD49B6FFFCAFFAEFAC30D9888825FB7" metricMagnitude="-4" metricUnit="m" metricValue="3.5" metricValueMax="4.0" metricValueMin="3.0" pageId="1" pageNumber="73" unit="mm" value="0.35" valueMax="0.4" valueMin="0.3">0.3–0.4 mm</quantity>
), higher 
<emphasis id="B958EA98FFCAFFAEFEA30DFF89735FB7" box="[296,310,1734,1757]" italics="true" pageId="1" pageNumber="73">a</emphasis>
ratio (
<number id="3DE424ADFFCAFFAEFE0B0DFC89825FB7" box="[384,455,1733,1757]" pageId="1" pageNumber="73" value="37.0" valueMax="39.0" valueMin="35.0">35–39</number>
<emphasis id="B958EA98FFCAFFAEFE450DFF89A05FB7" box="[462,485,1734,1757]" italics="true" pageId="1" pageNumber="73">vs</emphasis>
<number id="3DE424ADFFCAFFAEFE670DFC8A705FB7" box="[492,565,1733,1757]" pageId="1" pageNumber="73" value="29.0" valueMax="33.0" valueMin="25.0">25–33</number>
), higher 
<emphasis id="B958EA98FFCAFFAEFD130DFF8AE15FB7" box="[664,676,1734,1757]" italics="true" pageId="1" pageNumber="73">c</emphasis>
ratio (7.0– 
<number id="3DE424ADFFCAFFAEFCAB0DFC8B065FB7" box="[800,835,1733,1757]" pageId="1" pageNumber="73" value="9.2">9.2</number>
<emphasis id="B958EA98FFCAFFAEFCC10DFF8B245FB7" box="[842,865,1734,1757]" italics="true" pageId="1" pageNumber="73">vs</emphasis>
<number id="3DE424ADFFCAFFAEFCE30DFC8BCE5FB7" box="[872,907,1733,1757]" pageId="1" pageNumber="73" value="4.1">4.1</number>
–5.0), posteriorly located vulva (V = 
<number id="3DE424ADFFCAFFAEFAA70DFC8DD95FB7" box="[1324,1436,1733,1757]" pageId="1" pageNumber="73" value="73.65" valueMax="76.5" valueMin="70.8">70.8–76.5</number>
<emphasis id="B958EA98FFCAFFAEFF1C0DD388EB5E6B" box="[151,174,1770,1793]" italics="true" pageId="1" pageNumber="73">vs</emphasis>
<number id="3DE424ADFFCAFFAEFF300DD089465E6B" box="[187,259,1769,1793]" pageId="1" pageNumber="73" value="62.0" valueMax="64.0" valueMin="60.0">60–64</number>
), longer stylet (
<number id="3DE424ADFFCAFFAEFE4F0DD08A4F5E6B" box="[452,522,1769,1793]" pageId="1" pageNumber="73" value="14.5" valueMax="15.0" valueMin="14.0">14–15</number>
<emphasis id="B958EA98FFCAFFAEFD9C0DD38A6B5E6B" box="[535,558,1770,1793]" italics="true" pageId="1" pageNumber="73">vs</emphasis>
6.0– 
<number id="3DE424ADFFCAFFAEFDE00DD08ACB5E6B" box="[619,654,1769,1793]" pageId="1" pageNumber="73" value="6.5">6.5</number>
μm), longer spicules (
<number id="3DE424ADFFCAFFAEFC2B0DD08BA35E6B" box="[928,998,1769,1793]" pageId="1" pageNumber="73" value="19.5" valueMax="21.0" valueMin="18.0">18–21</number>
<emphasis id="B958EA98FFCAFFAEFC780DD38C4F5E6B" box="[1011,1034,1770,1793]" italics="true" pageId="1" pageNumber="73">vs</emphasis>
<number id="3DE424ADFFCAFFAEFB9C0DD08C1E5E6B" box="[1047,1115,1769,1793]" pageId="1" pageNumber="73" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
μm), gubernaculum length (5.0– 
<number id="3DE424ADFFCAFFAEFF5B0C3488B65E4F" box="[208,243,1805,1829]" pageId="1" pageNumber="73" value="6.5">6.5</number>
<emphasis id="B958EA98FFCAFFAEFF710C3789545E4F" box="[250,273,1806,1829]" italics="true" pageId="1" pageNumber="73">vs</emphasis>
3.0– 
<number id="3DE424ADFFCAFFAEFEC20C3489295E4F" box="[329,364,1805,1829]" pageId="1" pageNumber="73" value="4.5">4.5</number>
