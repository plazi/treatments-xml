<quantity id="4CA49B12AF20135CFBB044D8FAABFE22" box="[1140,1297,460,486]" metricMagnitude="3" metricUnit="m" metricValue="6.673133" pageId="13" pageNumber="14" unit="m" value="6673.133">6673, 133 m</quantity>
