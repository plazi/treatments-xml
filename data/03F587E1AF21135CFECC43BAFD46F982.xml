<number id="3D9424D0AF20135CFC2344B0FB9EFE7A" box="[999,1060,420,446]" pageId="13" pageNumber="14" value="1885.0">1885</number>
, st. 
<number id="3D9424D0AF20135CFBA144B0FB2BFE7A" box="[1125,1169,420,446]" pageId="13" pageNumber="14" value="113.0">113</number>
A, 
<quantity id="4CA49B12AF20135CFB7844B0FA97FE7A" box="[1212,1325,420,446]" metricMagnitude="1" metricUnit="m" metricValue="2.9" metricValueMax="4.5" metricValueMin="1.3" pageId="13" pageNumber="14" unit="m" value="29.0" valueMax="45.0" valueMin="13.0">13–45 m</quantity>
(
