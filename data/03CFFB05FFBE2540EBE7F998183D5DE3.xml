<taxonomicName id="4C663190FFBE255FEBE7F9981F255AF3" authority="Hahn, 1835" authorityName="Hahn" authorityYear="1835" box="[151,570,1663,1688]" class="Insecta" family="Mantidae" genus="Stagmomantis" kingdom="Animalia" order="Mantodea" pageId="15" pageNumber="516" phylum="Arthropoda" rank="species" species="limbata">
<emphasis id="B9129601FFBE255FEBE7F9981F255AF3" bold="true" box="[151,570,1663,1688]" pageId="15" pageNumber="516">
<emphasis id="B9129601FFBE255FEBE7F9981C875AF3" bold="true" box="[151,408,1663,1688]" italics="true" pageId="15" pageNumber="516">Stagmomantis limbata</emphasis>
(Hahn, 
<number id="3DAE5834FFBE255FEA89F9981F2D5AF3" box="[505,562,1663,1688]" pageId="15" pageNumber="516" value="1835.0">1835</number>
)
