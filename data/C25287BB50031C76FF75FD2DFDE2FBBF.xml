<taxonomicName id="8DFB4D2E50021C74FEB6F957FDA4F926" authority="Blake." authorityName="Blake." box="[340,633,1669,1691]" class="Insecta" family="Chrysomelidae" genus="Argopistes" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="254" phylum="Arthropoda" rank="species" species="coronatus">
<emphasis id="788FEABF50021C74FEB6F957FDF0F926" box="[340,557,1670,1691]" italics="true" pageId="5" pageNumber="254">Argopistes coronatus</emphasis>
