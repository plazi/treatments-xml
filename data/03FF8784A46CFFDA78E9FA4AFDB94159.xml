<taxonomicName id="4C564D11A46CFFD97954F995FC72462C" authorityName="Illiger" authorityYear="1811" box="[864,988,1627,1650]" class="Mammalia" family="Talpidae" genus="Condylura" kingdom="Animalia" order="Soricomorpha" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46CFFD97954F995FC72462C" box="[864,988,1627,1650]" italics="true" pageId="13" pageNumber="14">Condylura</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A46CFFD979DBF995FB26462C" authorityName="Gunther" authorityYear="1880" box="[1007,1160,1627,1650]" class="Mammalia" family="Talpidae" genus="Neurotrichus" kingdom="Animalia" order="Soricomorpha" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46CFFD979DBF995FB26462C" box="[1007,1160,1627,1650]" italics="true" pageId="13" pageNumber="14">Neurotrichus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A46CFFD97EAFF995FC8846CE" authorityName="True" authorityYear="1894" class="Mammalia" family="Talpidae" genus="Parascalops" kingdom="Animalia" order="Soricomorpha" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46CFFD97EAFF995FC8846CE" italics="true" pageId="13" pageNumber="14">Parascalops</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A46CFFD9790BF9B7FC0646CE" authorityName="E. Geoffroy Saint-Hilaire" authorityYear="1803" box="[831,936,1657,1680]" class="Mammalia" family="Talpidae" genus="Scalopus" kingdom="Animalia" order="Soricomorpha" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A46CFFD9790BF9B7FC0646CE" box="[831,936,1657,1680]" italics="true" pageId="13" pageNumber="14">Scalopus</emphasis>
</taxonomicName>
