<taxonomicName id="4C564D11A46CFFD979F5F9B7FB9E46CE" box="[961,1072,1657,1680]" class="Mammalia" family="Talpidae" genus="Scapanus" kingdom="Animalia" order="Soricomorpha" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">
