<collectingCountry box="[1343,1431,1903,1927]" name="Yemen" pageId="3" pageNumber="291">Socotra</collectingCountry>
, 
<collectingCountry box="[151,231,1940,1964]" name="Yemen" pageId="3" pageNumber="291">Yemen</collectingCountry>
