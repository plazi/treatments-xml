<taxonomicName authority="(Poda, 1761)" authorityName="Poda" authorityYear="1761" baseAuthorityName="Poda" baseAuthorityYear="1761" box="[193,594,977,1002]" class="Insecta" family="Tettigoniidae" genus="Phaneroptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="falcata">
