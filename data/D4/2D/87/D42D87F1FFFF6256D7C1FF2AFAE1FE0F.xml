<taxonomicName authority="(Charpentier, 1825) (Krauss 1879)" authorityName="Charpentier" authorityYear="1825" baseAuthorityName="Charpentier" baseAuthorityYear="1825" box="[341,1010,294,319]" class="Insecta" family="Acrididae" genus="Gomphocerus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="biguttatus">
