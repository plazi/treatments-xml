77. 
<taxonomicName authority="Ramme, 1913" authorityName="Ramme" authorityYear="1913" box="[193,625,1588,1613]" class="Insecta" family="Acrididae" genus="Oedipoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="meridionalis">
<emphasis bold="true" box="[193,455,1588,1613]" italics="true" pageId="65" pageNumber="66">Oedipoda meridionalis</emphasis>
<bibRefCitation author="Ramme, W." box="[462,625,1588,1613]" pageId="65" pageNumber="66" pagination="1 - 20" refId="ref67046" refString="Ramme, W. (1913) Orthopterologische Ergebnisse einer Reise nach Krain und Istrien (1912). Berliner Entomologische Zeitschrift, 58, 1 - 20." type="journal article" year="1913">Ramme, 1913</bibRefCitation>
