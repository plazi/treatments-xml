<taxonomicName authority="Saulcy, 1888" authorityName="Saulcy" authorityYear="1888" box="[179,492,543,568]" class="Insecta" family="Tetrigidae" genus="Tetrix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="kraussi">
