53. 
<taxonomicName authority="(Fabricius, 1793)" authorityName="Fabricius" authorityYear="1793" baseAuthorityName="Fabricius" baseAuthorityYear="1793" box="[193,631,402,427]" class="Insecta" family="Tettigoniidae" genus="Pholidoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="aptera">
