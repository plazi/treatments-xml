51. 
<taxonomicName authority="(Brunner von Wattenwyl, 1861)" authorityName="Brunner von Wattenwyl" authorityYear="1861" baseAuthorityName="Brunner von Wattenwyl" baseAuthorityYear="1861" box="[193,821,1551,1576]" class="Insecta" family="Tettigoniidae" genus="Pachytrachis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="gracilis">
