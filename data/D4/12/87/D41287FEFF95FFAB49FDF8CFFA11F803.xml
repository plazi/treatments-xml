and 
<taxonomicName authority="Wiegmann" authorityName="Wiegmann" box="[610,1060,1982,2007]" class="Reptilia" family="Liolaemidae" genus="Liolaemus" kingdom="Animalia" order="Squamata" pageId="32" pageNumber="33" phylum="Chordata" rank="species" species="nigromaculatus">
<emphasis box="[610,909,1982,2007]" italics="true" pageId="32" pageNumber="33">Liolaemus nigromaculatus</emphasis>
