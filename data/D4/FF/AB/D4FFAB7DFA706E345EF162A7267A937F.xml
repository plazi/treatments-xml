<subSubSection pageId="null" pageNumber="287" type="vernacular_name">
<paragraph pageId="null" pageNumber="287">
<normalizedToken originalValue="Französisches" pageId="null" pageNumber="287">Franzoesisches</normalizedToken>
Raygras, Fromental, Hoher Glatthafer
</paragraph>
