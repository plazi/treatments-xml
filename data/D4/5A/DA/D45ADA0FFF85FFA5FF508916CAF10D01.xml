<number box="[995,1028,623,647]" pageId="8" pageNumber="44" value="36.0">36</number>
</date>
: 
<number box="[1033,1064,623,647]" pageId="8" pageNumber="44" value="14.0">14</number>
: 
<number box="[1068,1083,623,647]" pageId="8" pageNumber="44" value="9.0">9</number>
: 
<number box="[1087,1103,623,647]" pageId="8" pageNumber="44" value="6.0">6</number>
: 
<quantity box="[1107,1167,623,647]" metricMagnitude="-1" metricUnit="m" metricValue="2.54" pageId="8" pageNumber="44" unit="in" value="10.0">10 in</quantity>
