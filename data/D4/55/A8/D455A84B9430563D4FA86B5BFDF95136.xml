<emphasis box="[458,643,267,291]" italics="true" pageId="2" pageNumber="257">K. porphyrocalyx</emphasis>
var. 
<emphasis box="[693,848,267,291]" italics="true" pageId="2" pageNumber="257">porphyrocalyx</emphasis>
</taxonomicName>
; 
<taxonomicName authority="Humbert ex Smith &amp; Figueiredo (2019: 206)" authorityName="Humbert ex Smith &amp; Figueiredo" authorityPageNumber="206" authorityYear="2019" class="Magnoliopsida" family="Crassulaceae" genus="Kalanchoe" kingdom="Plantae" order="Saxifragales" pageId="2" pageNumber="257" phylum="Tracheophyta" rank="variety" species="porphyrocalyx" variety="sambiranensis">
<emphasis box="[861,1046,267,291]" italics="true" pageId="2" pageNumber="257">K. porphyrocalyx</emphasis>
var. 
<emphasis box="[1096,1250,267,291]" italics="true" pageId="2" pageNumber="257">sambiranensis</emphasis>
Humbert ex Smith &amp; Figueiredo (2019: 206)
</taxonomicName>
; and 
<taxonomicName authorityName="Hamet" authorityYear="1910" baseAuthorityName="Stapf" baseAuthorityYear="1908" box="[470,587,303,327]" class="Magnoliopsida" family="Crassulaceae" genus="Kalanchoe" kingdom="Plantae" order="Saxifragales" pageId="2" pageNumber="257" phylum="Tracheophyta" rank="species" species="uniflora">
<emphasis box="[470,587,303,327]" italics="true" pageId="2" pageNumber="257">K. uniflora</emphasis>
