(species included: 
<taxonomicName authorityName="Zherikhin" authorityYear="1987" box="[570,688,1546,1567]" class="Insecta" family="Curculionidae" genus="Niphadomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="155" phylum="Arthropoda" rank="species" species="nigriventris">
<emphasis box="[570,688,1546,1567]" italics="true" pageId="4" pageNumber="155">nigriventris</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Zherikhin" authorityYear="1987" box="[706,759,1546,1567]" class="Insecta" family="Curculionidae" genus="Niphadomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="155" phylum="Arthropoda" rank="species" species="niger">
<emphasis box="[706,759,1546,1567]" italics="true" pageId="4" pageNumber="155">niger</emphasis>
</taxonomicName>
). 
<typeStatus box="[785,837,1546,1567]" pageId="4" pageNumber="155">Type</typeStatus>
species: 
<taxonomicName authority="Zherikhin, 1987" authorityName="Zherikhin" authorityYear="1987" box="[939,1400,1545,1567]" class="Insecta" family="Curculionidae" genus="Niphadomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="155" phylum="Arthropoda" rank="species" species="nigriventris">
