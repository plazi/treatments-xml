<heading bold="true" box="[151,667,367,394]" fontSize="11" level="1" pageId="17" pageNumber="168" reason="1">
<emphasis bold="true" box="[151,667,367,394]" pageId="17" pageNumber="168">
<taxonomicName authority="Zherikhin, 1987" box="[151,337,367,393]" class="Insecta" family="Curculionidae" genus="Niphadomimus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="168" phylum="Arthropoda" rank="genus">
