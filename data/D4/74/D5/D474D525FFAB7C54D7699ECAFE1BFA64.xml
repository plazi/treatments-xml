<subSubSection pageId="48" pageNumber="49" type="distribution">
<paragraph blockId="48.[106,1074,1403,1465]" pageId="48" pageNumber="49">
<emphasis bold="true" box="[106,257,1403,1429]" pageId="48" pageNumber="49">Distribution</emphasis>
: 
<taxonomicName authorityName="Klug" authorityYear="1855" box="[271,413,1403,1429]" class="Insecta" family="Scarabaeidae" genus="Sisyphus" kingdom="Animalia" order="Coleoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="infuscatus">
<emphasis box="[271,413,1403,1429]" italics="true" pageId="48" pageNumber="49">N. infuscatus</emphasis>
</taxonomicName>
