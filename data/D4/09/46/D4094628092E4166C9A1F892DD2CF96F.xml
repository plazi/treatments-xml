<emphasis box="[872,986,950,976]" italics="true" pageId="15" pageNumber="16">frigescens</emphasis>
{1}, 
<taxonomicName box="[1051,1227,950,977]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="melanoxylon">
