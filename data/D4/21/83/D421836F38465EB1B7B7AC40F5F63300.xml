<emphasis italics="true" pageId="5" pageNumber="98">Castanopsis</emphasis>
</taxonomicName>
ssp. (
<taxonomicName class="Dicotyledoneae" family="Fagaceae" genus="Castanopsis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Fagales" pageId="5" pageNumber="98" phylum="Angiospermae" rank="family">Fagaceae</taxonomicName>
), 
<taxonomicName class="Liliopsida" family="Colchicaceae" genus="Disporum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Disporum" order="Liliales" pageId="5" pageNumber="98" phylum="Magnoliophyta" rank="genus">
<emphasis italics="true" pageId="5" pageNumber="98">Disporum</emphasis>
</taxonomicName>
sp. (
<taxonomicName class="Liliopsida" family="Colchicaceae" genus="Disporum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Liliales" pageId="5" pageNumber="98" phylum="Magnoliophyta" rank="family">Colchicaceae</taxonomicName>
), 
<taxonomicName class="Dicotyledoneae" family="Loranthaceae" genus="Elytranthe" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Elytranthe albida" order="Santalales" pageId="5" pageNumber="98" phylum="Angiospermae" rank="species" species="albida">
<emphasis italics="true" pageId="5" pageNumber="98">Elytranthe albida</emphasis>
</taxonomicName>
(Bl.) Bl. (
<taxonomicName class="Dicotyledoneae" family="Loranthaceae" genus="Elytranthe" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Santalales" pageId="5" pageNumber="98" phylum="Angiospermae" rank="family">Loranthaceae</taxonomicName>
), 
<taxonomicName authorityName="T. P. Lin &amp; W. M. Lin" authorityYear="2009" class="Magnoliopsida" family="Orchidaceae" genus="Nervilia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Nervilia tahanshanensis" order="Asparagales" pageId="5" pageNumber="98" phylum="Angiospermae" rank="species" species="tahanshanensis">
<emphasis italics="true" pageId="5" pageNumber="98">Nervilia tahanshanensis</emphasis>
</taxonomicName>
T.P.Lin &amp; W.M.Lin (
<taxonomicName class="Magnoliopsida" family="Orchidaceae" genus="Nervilia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Asparagales" pageId="5" pageNumber="98" phylum="Angiospermae" rank="family">Orchidaceae</taxonomicName>
), 
<taxonomicName class="Dicotyledoneae" family="Rosaceae" genus="Rubus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Rubus" order="Rosales" pageId="5" pageNumber="98" phylum="Angiospermae" rank="genus">
