<emphasis bold="true" box="[140,593,569,596]" pageId="48" pageNumber="59">
IV. 
<emphasis bold="true" box="[180,400,569,596]" italics="true" pageId="48" pageNumber="59">
<taxonomicName authority="Thomson" authorityName=": Moore and Legner" authorityYear="1975" box="[180,290,569,596]" class="Insecta" family="Staphylinidae" genus="Gnypeta" kingdom="Animalia" order="Coleoptera" pageId="48" pageNumber="59" phylum="Arthropoda" rank="genus">Gnypeta</taxonomicName>
