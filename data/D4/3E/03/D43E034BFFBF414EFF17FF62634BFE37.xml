: 
<taxonomicName authority="(C. R. Sahlberg)" baseAuthorityName="C. R. Sahlberg" baseAuthorityYear="1831" box="[348,649,448,475]" class="Insecta" family="Staphylinidae" genus="Gnypeta" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="45" phylum="Arthropoda" rank="species" species="caerulea">
<emphasis box="[348,368,448,474]" italics="true" pageId="34" pageNumber="45">G</emphasis>
. 
<emphasis box="[382,468,448,474]" italics="true" pageId="34" pageNumber="45">caerulea</emphasis>
(C.R. Sahlberg)
</taxonomicName>
and 
<taxonomicName authority="Klimaszewski" authorityName="Klimaszewski &amp; Savard &amp; Pelletier &amp; Webster" authorityYear="2008" box="[705,1015,448,474]" class="Insecta" family="Staphylinidae" genus="Gnypeta" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="45" phylum="Arthropoda" rank="species" species="lohsei" status="sp. nov.">
<emphasis box="[705,859,448,474]" italics="true" pageId="34" pageNumber="45">Gnypeta lohsei</emphasis>
Klimaszewski
</taxonomicName>
