<subSubSection box="[151,1421,223,250]" pageId="56" pageNumber="57" type="type_taxon">
<paragraph blockId="56.[151,1437,223,1078]" box="[151,1421,223,250]" pageId="56" pageNumber="57">
<emphasis bold="true" box="[151,306,223,249]" pageId="56" pageNumber="57">
<typeStatus box="[151,210,223,249]" pageId="56" pageNumber="57">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Kuroda &amp; Sakurai" authorityName="Kuroda &amp; Sakurai" authorityYear="1959" box="[313,705,223,249]" class="Gastropoda" family="Costellariidae" genus="Mitropifex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="56" pageNumber="57" phylum="Mollusca" rank="species" species="isaoi">
<emphasis box="[313,491,223,249]" italics="true" pageId="56" pageNumber="57">Mitropifex isaoi</emphasis>
Kuroda &amp; Sakurai
</taxonomicName>
in 
<bibRefCitation author="Kuroda, T." box="[740,894,223,250]" pageId="56" pageNumber="57" pagination="317 - 335" refId="ref58557" refString="Kuroda, T. (1959) Description of new species of marine shells from Japan. Venus, 20, 317 - 335." type="journal article" year="1959">Kuroda, 1959</bibRefCitation>
