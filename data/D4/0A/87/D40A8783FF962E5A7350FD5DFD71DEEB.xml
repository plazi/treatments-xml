); 
<taxonomicName authority="Parent" authorityName="Parent" box="[659,1072,1404,1430]" class="Insecta" family="Dolichopodidae" genus="Cheiromyia" kingdom="Animalia" order="Diptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="palmaticornis">
