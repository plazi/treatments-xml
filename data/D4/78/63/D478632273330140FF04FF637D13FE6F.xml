<subSubSection lastPageId="27" lastPageNumber="480" pageId="26" pageNumber="479" type="materials_examined">
<paragraph blockId="26.[151,1436,1725,2028]" pageId="26" pageNumber="479">
<emphasis bold="true" box="[151,608,1725,1750]" pageId="26" pageNumber="479">
