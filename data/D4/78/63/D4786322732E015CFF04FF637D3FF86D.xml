<quantity box="[1119,1309,1895,1919]" metricMagnitude="4" metricUnit="m" metricValue="5.26015" metricValueMax="10.510299999999999" metricValueMin="0.01" pageId="7" pageNumber="460" unit="m" value="52601.5" valueMax="105103.0" valueMin="100.0">100–105, 103 m</quantity>
