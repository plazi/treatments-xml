<quantity box="[555,721,1088,1113]" metricMagnitude="-4" metricUnit="m" metricValue="3.7" metricValueMax="3.8" metricValueMin="3.5999999999999996" pageId="17" pageNumber="304" unit="mm" value="0.37" valueMax="0.38" valueMin="0.36">0.36–0.38 mm</quantity>
