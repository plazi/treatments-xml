<subSubSection box="[199,1213,873,900]" pageId="1" pageNumber="436" type="etymology">
<paragraph blockId="1.[151,1437,261,900]" box="[199,1213,873,900]" pageId="1" pageNumber="436">
<emphasis bold="true" box="[199,328,874,900]" pageId="1" pageNumber="436">Etymology</emphasis>
