<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Hymenoptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Hymenoptera" order="Malvales" ordo="Hymenoptera" pageId="0" pageNumber="992" phylum="Tracheophyta" rank="ordo">Hymenoptera</taxonomicName>
