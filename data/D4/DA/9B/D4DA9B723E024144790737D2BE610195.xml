<taxonomicName class="Insecta" family="Ichneumonidae" genus="Zatypota" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Zatypota" order="Hymenoptera" pageId="0" pageNumber="992" phylum="Arthropoda" rank="genus">Zatypota</taxonomicName>
