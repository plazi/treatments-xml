<materialsCitation ID-GBIF-Occurrence="2592189824" collectingDate="2009-08-02" collectingMethod="light trap" collectionCode="INPA" collectorName="J. O. da Silva &amp; J. L. Nessimian &amp; U. G. Neiss" country="Brazil" county="Igarape da Cobra" latitude="0.8761111" location="Amazonas" longLatPrecision="21" longitude="-63.45111" municipality="Serra do Araca" pageId="3" pageNumber="204" specimenCount="1" typeStatus="holotype">
