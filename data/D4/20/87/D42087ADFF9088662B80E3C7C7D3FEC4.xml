<subSubSection pageId="22" pageNumber="23" type="etymology">
<paragraph blockId="22.[151,790,978,1027]" pageId="22" pageNumber="23">
<emphasis box="[151,243,978,999]" italics="true" pageId="22" pageNumber="23">Etymology:</emphasis>
