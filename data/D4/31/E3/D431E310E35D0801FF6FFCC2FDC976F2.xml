<number box="[1388,1436,1342,1363]" pageId="52" pageNumber="53" value="2005.0">2005</number>
</bibRefCitation>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph blockId="52.[151,1436,844,1451]" pageId="52" pageNumber="53">
<keyLead pageId="52" pageNumber="53">
