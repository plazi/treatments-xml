(
<bibRefCitation author="Kuznetzov" box="[1264,1429,1137,1158]" pageId="52" pageNumber="53" refString="Kuznetzov, N. N. (1971) Seven species of the genus Lorryia (Tydeidae) from Crimea and Georgia. Zoologicheskii Zhurnal, 50, 1740 - 1746. [In Russian]" type="journal article" year="1971">
Kuznetzov, 
<number box="[1380,1429,1137,1158]" pageId="52" pageNumber="53" value="1971.0">1971</number>
</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
<paragraph blockId="52.[151,1436,844,1451]" pageId="52" pageNumber="53">
<keyLead pageId="52" pageNumber="53">
