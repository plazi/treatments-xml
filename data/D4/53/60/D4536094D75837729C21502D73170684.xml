sp. (
<bibRefCitation author="Strong, DR" journalOrPublisher="Biotropica" pageId="294" pageNumber="295" pagination="156 - 169" title="Rolled-leaf hispine beetles (Chrysomelidae) and their Zingiberales host plants in Middle America." url="10.2307/2387878" volume="9" year="1977 a">Strong 1977a</bibRefCitation>
); 
<taxonomicName class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia mariae" order="Zingiberales" pageId="203" pageNumber="204" phylum="Tracheophyta" rank="species" species="mariae">Heliconia mariae</taxonomicName>
Hook., 
<taxonomicName class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia pogonantha" order="Zingiberales" pageId="203" pageNumber="204" phylum="Tracheophyta" rank="species" species="pogonantha">Heliconia pogonantha</taxonomicName>
Cufod. (
<taxonomicName genus="Heliconiaceae" lsidName="Heliconiaceae" pageId="203" pageNumber="204" rank="genus">Heliconiaceae</taxonomicName>
) (
<bibRefCitation author="Garcia-Robledo, C" journalOrPublisher="PLoS ONE" pageId="287" pageNumber="288" title="Tropical plant-herbivore networks: Reconstructing species interactions using DNA barcodes." url="10.1371/journal.pone.0052967" volume="8" year="2013 a">
<normalizedToken originalValue="García–Robledo">Garcia-Robledo</normalizedToken>
et al. 2013a
</bibRefCitation>
).
</paragraph>
</subSubSection>
