<number box="[615,673,789,813]" pageId="12" pageNumber="39" value="1998.0">1998</number>
</bibRefCitation>
</taxonomicName>
, p. 
<number box="[713,813,789,813]" pageId="12" pageNumber="39" value="170.0" valueMax="175.0" valueMin="165.0">165–175</number>
, figs. 
<number box="[881,925,789,813]" pageId="12" pageNumber="39" value="3.5" valueMax="6.0" valueMin="1.0">1–6</number>
.
