<taxonomicName id="C7D17DCDBA569F084F4522EEFC4EFD3E" authorityName="Bublichenko" authorityYear="1959" box="[790,951,739,765]" class="Rhynchonellata" family="Spinocyrtiidae" genus="Mediospirifer" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spiriferida" pageId="32" pageNumber="33" phylum="Brachiopoda" rank="genus">
