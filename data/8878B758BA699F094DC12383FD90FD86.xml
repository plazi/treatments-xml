<taxonomicName id="C7D17DCDBA699F374CEE241AFD3DFBF2" authority="Plewes, 1996: 178" authorityName=", Plewes" authorityPageNumber="178" authorityYear="1996" box="[189,708,1047,1073]" class="Demospongiae" family="Clionaidae" genus="Clionolithes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clionaida" pageId="31" pageNumber="32" phylum="Porifera" rank="species" species="bullahirsuta">
