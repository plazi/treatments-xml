<quantity id="62489B17FFD3731CFDFBFEF0F6C96015" box="[591,683,260,287]" metricMagnitude="-1" metricUnit="m" metricValue="1.0" pageId="48" pageNumber="49" unit="mm" value="100.0">100 µm</quantity>
