<quantity id="62489B17FFCC7303FC41F875F0246690" box="[1013,1094,1920,1946]" metricMagnitude="-2" metricUnit="m" metricValue="2.3" pageId="47" pageNumber="48" unit="mm" value="23.0">23 mm</quantity>
