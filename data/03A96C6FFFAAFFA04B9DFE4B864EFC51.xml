<taxonomicName id="4C00A6FAFFAAFFA04F2FFCB881B1FC8C" authorityName="Crawford" authorityYear="1912" box="[1061,1154,836,861]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="fletcheri">
