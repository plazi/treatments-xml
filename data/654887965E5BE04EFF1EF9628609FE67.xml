<taxonomicName id="2AE14D035E5BE04FFF1EF9628787F923" authority="(Macquart, 1843)" authorityName="Macquart" authorityYear="1843" baseAuthorityName="Macquart" baseAuthorityYear="1843" box="[151,651,1741,1768]" class="Insecta" family="Muscidae" genus="Anthomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="375" phylum="Arthropoda" rank="species" species="limbinervis">
