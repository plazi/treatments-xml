<taxonomicName id="4C790D9FFF71D3AFCAD16343CB9C9DF0" authority="Hering" authorityName="Hering" box="[727,939,1407,1432]" class="Arachnida" family="Ascidae" genus="Melichares" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="200" pageNumber="201" phylum="Arthropoda" rank="genus">
