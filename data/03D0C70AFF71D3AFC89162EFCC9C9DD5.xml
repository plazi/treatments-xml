<taxonomicName id="4C790D9FFF71D3AFCCEC6343CD509DF0" box="[1258,1383,1407,1432]" class="Arachnida" family="Ascidae" genus="Orolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="200" pageNumber="201" phylum="Arthropoda" rank="genus">
