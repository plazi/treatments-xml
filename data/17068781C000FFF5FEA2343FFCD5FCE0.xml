<emphasis id="ADDBEA85C002FFF3FE6A34EAFD33F948" box="[448,666,1732,1755]" italics="true" pageId="3" pageNumber="150">
<taxonomicName id="58AF4D14C002FFF3FE6A34EAFD9BF948" box="[448,562,1732,1755]" class="Arachnida" family="Sclerosomatidae" genus="Umbopilio" higherTaxonomySource="CoL" kingdom="Animalia" order="Opiliones" pageId="3" pageNumber="150" phylum="Arthropoda" rank="genus">Umbopilio</taxonomicName>
<taxonomicName id="58AF4D14C002FFF3FD9634EAFD33F948" box="[572,666,1732,1755]" pageId="3" pageNumber="150" rank="species" species="martensi" status="n. sp.">martensi</taxonomicName>
