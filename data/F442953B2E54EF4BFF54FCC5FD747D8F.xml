<taxonomicName id="BBEB5FAE2E54EF4BFDC7FCA7FD577D4F" authority="acus" box="[609,738,847,870]" class="Actinopterygii" family="Belonidae" genus="Strongylura" kingdom="Animalia" order="Beloniformes" pageId="41" pageNumber="42" phylum="Chordata" rank="subSpecies" species="acus" subSpecies="acus">
