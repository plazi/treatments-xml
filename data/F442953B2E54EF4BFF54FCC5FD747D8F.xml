<taxonomicName id="BBEB5FAE2E54EF4BFF54FCA7FE127D4F" authorityName="Collette, B. B. and N. V. Parin" authorityYear="1990" box="[242,423,847,871]" class="Actinopterygii" family="Belonidae" genus="Strongylura" kingdom="Animalia" order="Beloniformes" pageId="41" pageNumber="42" phylum="Chordata" rank="species" species="acus">
