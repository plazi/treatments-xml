<taxonomicName id="4C184D3DFF99FFE1FC823DE27CF7FB3E" authority="Grimaldi, 2018" authorityName="Grimaldi" authorityYear="2018" box="[808,958,1201,1225]" class="Insecta" family="Phoridae" genus="Alonchoptera" kingdom="Animalia" order="Diptera" pageId="31" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis id="B96CEAACFF99FFE1FC823DE27CF7FB3E" bold="true" box="[808,958,1201,1225]" italics="true" pageId="31">Alonchoptera</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A25F57D7FF99FFE1FC663DE17B09FB3D" box="[972,1088,1202,1226]" pageId="31" rank="genus">new genus</taxonomicNameLabel>
