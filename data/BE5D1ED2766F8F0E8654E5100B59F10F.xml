<taxonomicName id="2D3CB30D10C1FDFBB91D02DDC3395C2E" class="Insecta" family="Ichneumonidae" genus="Ethelurgina" kingdom="Animalia" lsidName="Ethelurgina curtorius" order="Hymenoptera" pageId="0" pageNumber="9042" phylum="Arthropoda" rank="species" species="curtorius">curtorius</taxonomicName>
