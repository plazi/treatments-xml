<heading id="D0F2E92EA07CFF8EFF22FC126E3C4AF3" bold="true" box="[151,583,903,930]" fontSize="11" level="1" pageId="2" pageNumber="227" reason="1">
<taxonomicName id="4C0525C1A07CFF8EFF22FC126E3C4AF3" authority="DeLong &amp; Freytag, 1975" authorityName="DeLong &amp; Freytag" authorityYear="1975" box="[151,583,903,930]" class="Insecta" family="Cicadellidae" genus="Regalana" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="227" phylum="Arthropoda" rank="genus">
