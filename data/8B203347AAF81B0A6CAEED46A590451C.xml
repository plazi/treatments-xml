<taxonomicName id="2BC751FF3C8E37CC025D94F7EAE46BB6" class="Actinopterygii" family="Characidae" genus="Cynopotamus" higherTaxonomySource="CoL" kingdom="Animalia" order="Characiformes" pageNumber="60" phylum="Chordata" rank="species" species="kincaidi">C. kincaidi</taxonomicName>
