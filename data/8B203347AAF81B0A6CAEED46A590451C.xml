<taxonomicName id="DC993EF05BE473E2E6D987DD79EA1490" LSID-ZBK="urn:lsid:zoobank.org:act:AB53A1BB-2118-4F39-BCA0-6AE34D999CBF" class="Actinopterygii" family="Characidae" genus="Cynopotamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Cynopotamus bipunctatus Pellegrin 1909:13" order="Characiformes" pageNumber="60" phylum="Chordata" rank="species" species="bipunctatus">C. bipunctatus</taxonomicName>
