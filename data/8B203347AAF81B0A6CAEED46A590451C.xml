<normalizedToken id="9F57E136555BC9E58B8E30F59F16907F" originalValue="Goiás">Goias</normalizedToken>
and 
<normalizedToken id="BF08E8A004A7CBB1CE0BE8C00D6FEE12" originalValue="Pará">Para</normalizedToken>
, Brazil)......................................................................................................... 
<taxonomicName id="D5EEA983B49571A721C2598586210720" LSID-ZBK="urn:lsid:zoobank.org:act:2BE3ABCD-52D0-42B2-BDA4-2BF62BB6EADD" class="Actinopterygii" family="Characidae" genus="Cynopotamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Cynopotamus tocantinensis Menezes 1987:5" order="Characiformes" pageNumber="60" phylum="Chordata" rank="species" species="tocantinensis">C. tocantinensis</taxonomicName>
