<taxonomicName id="1C721E3C58F092BDCAB9861875478164" LSID-ZBK="urn:lsid:zoobank.org:act:9A7000F7-7403-41EA-9CA2-8E858523313B" class="Actinopterygii" family="Characidae" genus="Cynopotamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Cynopotamus gouldingi Menezes 1987:2" order="Characiformes" pageNumber="60" phylum="Chordata" rank="species" species="gouldingi">C. gouldingi</taxonomicName>
