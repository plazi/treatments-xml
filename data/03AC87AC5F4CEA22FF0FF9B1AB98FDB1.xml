,
</emphasis>
<taxonomicNameLabel id="A24257D35F4DEA20FDF4FEBDABF9FE94" box="[620,705,312,335]" pageId="1" pageNumber="56" rank="species">sp. nov.</taxonomicNameLabel>
, and 
<taxonomicName id="4C054D395F4DEA20FD61FEBDAD4EFE94" authority="Ma et Zheng, 1992" authorityName="Ma et Zheng" authorityYear="1992" box="[761,1142,312,335]" class="Insecta" family="Baissogryllidae" genus="Caryanda" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="species" species="rufofemorata">
<emphasis id="B971EAA85F4DEA20FD61FEBDAA9CFE94" box="[761,932,312,335]" italics="true" pageId="1" pageNumber="56">C. rufofemorata</emphasis>
