<taxonomicName id="778D84762CA3493EAEF44B9FD081A28B" class="Dicotyledoneae" family="Apiaceae" genus="Foeniculum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Foeniculum vulgare" order="Umbelliflorae" pageId="0" pageNumber="187" phylum="Angiospermae" rank="species" species="vulgare">
