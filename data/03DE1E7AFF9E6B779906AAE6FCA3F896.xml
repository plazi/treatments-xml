<emphasis id="B903737EFF9E6B779906AAE6FD4AFA61" bold="true" box="[264,665,1444,1470]" pageId="10" pageNumber="37">
<taxonomicName id="4C77D4EFFF9E6B779906AAE6FDC7FA61" box="[264,532,1444,1470]" class="Insecta" family="Miridae" genus="Isometopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="37" phylum="Arthropoda" rank="species" species="sudanicus">
