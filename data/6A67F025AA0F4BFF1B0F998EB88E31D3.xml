<taxonomicName id="4DF90150879546EF9217DB909DEF1C95" class="Chilopoda" family="Lithobiidae" genus="Eupolybothrus" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="9" pageNumber="320" phylum="Arthropoda" rank="species" species="nudicornis">E. nudicornis</taxonomicName>
