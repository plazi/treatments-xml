<taxonomicName id="4C2E4D6E8749FFD9CFA7FD3F8043FD11" box="[1202,1380,655,678]" class="Arachnida" family="Halacaridae" genus="Copidognathus" kingdom="Animalia" order="Trombidiformes" pageId="13" pageNumber="202" phylum="Arthropoda" rank="species" species="adriaticus">
<emphasis id="B95AEAFF8749FFD9CFA7FD3F81E0FD11" box="[1202,1223,655,678]" italics="true" pageId="13" pageNumber="202">C</emphasis>
. 
<emphasis id="B95AEAFF8749FFD9CFF7FD3F8043FD11" box="[1250,1380,655,678]" italics="true" pageId="13" pageNumber="202">adriaticus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C2E4D6E8749FFD9CE6AFD3F8414FD7C" authorityName="Chatterjee, Annapurna &amp; Chang" authorityYear="2004" class="Arachnida" family="Halacaridae" genus="Copidognathus" kingdom="Animalia" order="Trombidiformes" pageId="13" pageNumber="202" phylum="Arthropoda" rank="species" species="andhraensis">
