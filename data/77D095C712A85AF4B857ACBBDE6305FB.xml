<taxonomicName id="47BA6C2EBCA7F91D3A67BFCCDDB60B85" authorityName="Henderson" authorityYear="1888" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Uroptychus" order="Decapoda" pageId="0" pageNumber="54482" phylum="Arthropoda" rank="genus">
