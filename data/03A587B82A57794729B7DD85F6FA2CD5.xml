<taxonomicName id="4C0C4D2D2A57794629B7DC6CF40B2F2D" authority="Sanborn 2004: 365" authorityName="Sanborn" authorityPageNumber="365" authorityYear="2004" box="[151,543,1830,1853]" class="Insecta" family="Cicadidae" genus="Zammara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="olivacea">
<emphasis id="B978EABC2A57794629B7DC6CF7422F2D" box="[151,342,1830,1853]" italics="true" pageId="8" pageNumber="9">Zammara olivacea</emphasis>
