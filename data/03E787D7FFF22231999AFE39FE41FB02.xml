<subSubSection id="C354654AFFF22231999AFDC2FDA8FDB1" box="[170,600,532,557]" pageId="17" pageNumber="247" type="description">
<paragraph id="8BF136C1FFF22231999AFDC2FDA8FDB1" blockId="17.[170,1437,495,1182]" box="[170,600,532,557]" pageId="17" pageNumber="247">
<emphasis id="B93AEAD3FFF22231999AFDC2FF17FDB1" box="[170,231,532,557]" italics="true" pageId="17" pageNumber="247">Blüte</emphasis>
