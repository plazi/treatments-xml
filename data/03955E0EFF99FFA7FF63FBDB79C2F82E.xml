<taxonomicName id="4C3C949BFF99FFA7FC9CF8C97E40F89D" authority="Distant" authorityName="Distant" box="[872,1116,1808,1834]" class="Insecta" family="Cicadidae" genus="Oncotympana" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="20" phylum="Arthropoda" rank="species" species="fuscata" status="comb. nov.">
<emphasis id="B948330AFF99FFA7FC9CF8C9796AF89E" box="[872,886,1808,1833]" italics="true" pageId="6" pageNumber="20">S</emphasis>
