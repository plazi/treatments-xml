<taxonomicName id="4C3C949BFF99FFA7FE17F8C97829F89E" authority="Lee, 2010" authorityName="Lee" authorityYear="2010" box="[483,565,1808,1833]" class="Insecta" family="Cicadidae" genus="Sonata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="20" phylum="Arthropoda" rank="genus">
<emphasis id="B948330AFF99FFA7FE17F8C97829F89E" box="[483,565,1808,1833]" italics="true" pageId="6" pageNumber="20">Sonata</emphasis>
