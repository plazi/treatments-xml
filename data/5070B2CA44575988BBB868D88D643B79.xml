<taxonomicName id="E5BBCF1F9F501E12217C4F82969EEE5E" authorityName="de Niceville" authorityYear="1889" class="Insecta" family="Hesperiidae" genus="Celaenorrhinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Celaenorrhinus pyrrha" order="Lepidoptera" pageId="0" pageNumber="61" phylum="Arthropoda" rank="species" species="pyrrha">
