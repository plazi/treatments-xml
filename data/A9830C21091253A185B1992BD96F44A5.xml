<taxonomicName id="84592B5C881AE0F12F91C34BE9396465" authority="Kriebel, 2016" authorityName="Kriebel" authorityYear="2016" class="Magnoliopsida" family="Melastomataceae" genus="Conostegia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Conostegia attenuata" order="Myrtales" pageId="119" pageNumber="120" phylum="Tracheophyta" rank="species" species="attenuata">Conostegia attenuata</taxonomicName>
