<taxonomicName id="C6986CCEFF91426CFE39425BFD75F81A" box="[506,720,1896,1920]" class="Insecta" family="Formicidae" genus="Pachycondyla" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="481" phylum="Arthropoda" rank="species" species="zoro">
