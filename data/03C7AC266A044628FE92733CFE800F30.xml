<treatmentCitationGroup id="AB7E3A1E6A044629FE92738FFD180D2B" box="[264,751,1022,1046]" pageId="10" pageNumber="11">
<taxonomicName id="4C6E66B36A044629FE92738FFE150D28" box="[264,482,1023,1046]" class="Insecta" family="Tortricidae" genus="Anopinella" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="triquetra">
