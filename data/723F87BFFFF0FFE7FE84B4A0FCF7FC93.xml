<quantity id="3D6E9B4CFFFFFFE7FCBFB355FCAFFDE9" box="[811,889,543,567]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="8" pageNumber="223" unit="mm" value="20.0">20 mm</quantity>
