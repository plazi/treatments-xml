<quantity id="497B73EE1B29FF84E41A3576FB23D989" box="[1123,1220,1656,1682]" metricMagnitude="-3" metricUnit="m" metricValue="5.9" pageId="13" pageNumber="44" unit="mm" value="5.9">5.9 mm</quantity>
