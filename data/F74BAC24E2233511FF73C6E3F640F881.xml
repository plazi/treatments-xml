<quantity id="B81AB0D7E2233511FCE3C44AF172F967" box="[775,864,1647,1671]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" pageId="10" pageNumber="569" unit="mm" value="9.5">9.5 mm</quantity>
