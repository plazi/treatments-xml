<quantity id="4CEF9B25FFA9FF8D99AFFC5DF86C33CC" box="[466,557,943,969]" metricMagnitude="3" metricUnit="m" metricValue="4.268" pageId="56" pageNumber="57" unit="m" value="4268.0">4,268 m</quantity>
(
<quantity id="4CEF9B25FFA9FF8D9A46FC5DF8E333CF" box="[571,674,943,970]" metricMagnitude="3" metricUnit="m" metricValue="4.2668952" pageId="56" pageNumber="57" unit="ft" value="13999.0">13,999 ft</quantity>
