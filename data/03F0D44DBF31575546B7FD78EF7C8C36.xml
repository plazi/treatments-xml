<taxonomicName id="4C591ED8BF3157554741FD0DEE778B16" box="[353,594,763,787]" class="Magnoliopsida" family="Acanthaceae" genus="Avicennia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="8" pageNumber="229" phylum="Tracheophyta" rank="species" species="germinans">
