<taxonomicName id="4C674D3E0C26F80B6C7C9854FE0D744E" authorityName="Skelley" authorityYear="2009" box="[284,486,579,603]" class="Insecta" family="Erotylidae" genus="Notaepytus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="53" phylum="Arthropoda" rank="genus">
