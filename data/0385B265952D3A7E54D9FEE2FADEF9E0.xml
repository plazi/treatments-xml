<bibRefCitation id="EFBD7E82952D3A7E569BFD35FCE6FCEC" author="Kramp" box="[725,784,757,782]" pageId="11" pageNumber="302" refString="Kramp, P. L. (1968) The Hydromedusae of the Pacific and Indian Oceans. Sect. II and III. Dana - Reports, 72, 1 - 200." type="journal article" year="1968">1968</bibRefCitation>
; 
<bibRefCitation id="EFBD7E82952D3A7E5752FD36FC3BFCEC" author="Goy" box="[796,973,757,782]" pageId="11" pageNumber="302" refString="Goy, J., Lakkis, S. &amp; Zeidane, R. (1990) Les meduses de la Mediterranee orientale. Bulletin de l'Institut Oceanographique, 7, 79 - 88." type="journal article" year="1990">
Goy 
<emphasis id="B958DF61952D3A7E571DFD36FC7DFCEC" box="[851,907,757,782]" italics="true" pageId="11" pageNumber="302">et al.</emphasis>
1990
</bibRefCitation>
, 
<bibRefCitation id="EFBD7E82952D3A7E5796FD36FBE5FCEC" author="Goy" box="[984,1043,758,782]" pageId="11" pageNumber="302" refString="Goy, J., Lakkis, S. &amp; Zeidane, R. (1991) Les Meduses (Cnidaria) des eaux libanaises. Annales de l'Institut Oceanographique, 67, 99 - 128." type="journal article" year="1991">1991</bibRefCitation>
; 
<bibRefCitation id="EFBD7E82952D3A7E5051FD35FB38FCEC" author="Cornelius" box="[1055,1230,757,782]" pageId="11" pageNumber="302" refString="Cornelius, P. F. S. (1995) North - west European Thecate Hydroids and Their Medusae (Cnidaria, Leptolida, Leptothecatae). Synopses of the British Fauna, n. s., 50 (1), 1 - 347; 50 (2), 1 - 386." type="journal article" year="1995">
Cornelius 
<number id="3DE41154952D3A7E50DDFD35FB38FCEC" box="[1171,1230,757,782]" pageId="11" pageNumber="302" value="1995.0">1995</number>
</bibRefCitation>
; 
<bibRefCitation id="EFBD7E82952D3A7E5094FD35FA89FCEC" author="Bouillon" box="[1242,1407,757,782]" pageId="11" pageNumber="302" refString="Bouillon, J. (1999) Hydromedusae. In: Boltovskoy, D. (Ed.), South Atlantic Zooplankton. Backhuys Publishers, Leiden, pp. 385 - 465." type="book chapter" year="1999">
Bouillon 
<number id="3DE41154952D3A7E510DFD36FA89FCEC" box="[1347,1407,758,782]" pageId="11" pageNumber="302" value="1999.0">1999</number>
