<heading id="CA6681B8FFA6997FFCC4FAC3FBB5FA67" centered="true" fontSize="10" level="2" pageId="3" reason="2">
<emphasis id="A3E5EAC6FFA6997FFCC4FAC3FBB5FA67" bold="true" pageId="3">
Tribe 
<taxonomicName id="56914D57FFA6997FFC8AFAC3FBC0FA28" authority="Reitter" authorityName="Reitter" authorityYear="1882" box="[871,1090,1400,1421]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="tribe" tribe="Trichonychini">Trichonychini Reitter</taxonomicName>
Subtribe 
<taxonomicName id="56914D57FFA6997FFC99FA28FBD5FA0D" authority="Jeannel" authorityName="Jeannel" authorityYear="1950" box="[884,1111,1427,1448]" class="Insecta" family="Staphylinidae" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="subTribe" subTribe="Panaphantina">Panaphantina Jeannel</taxonomicName>
<taxonomicName id="56914D57FFA6997FFCD8FA16FBB5FA67" authority="Reitter, 1881" authorityName="Reitter" authorityYear="1881" box="[821,1079,1453,1474]" class="Insecta" family="Staphylinidae" genus="Bibloplectus" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="genus">
<emphasis id="A3E5EAC6FFA6997FFCD8FA16FC2CFA67" bold="true" box="[821,942,1453,1474]" italics="true" pageId="3">Bibloplectus</emphasis>
<bibRefCitation id="F5004B25FFA6997FFC58FA16FBB5FA67" author="Reitter" box="[949,1079,1453,1474]" pageId="3" pagination="1 - 529" refId="ref19063" refString="Reitter, E. 1881. Bestimmungs-Tabellen der europaischen Coleoptera. V. Enthaltend die Familien: Paussidae, Clavigeridae, Pselaphidae und Scydmaenidae. Verhandlungen der Kaiserlich-Koniglichen Zoologisch- Botanischen Gesellschaft in Wien 31: 1 - 529." type="journal article" year="1881">Reitter, 1881</bibRefCitation>
