<taxonomicName id="4C6003D8FFD8FF8013D1A5D9FC97FBF4" authority="Li, 2004" authorityName="Li" authorityYear="2004" box="[500,769,1015,1041]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="332" phylum="Arthropoda" rank="species" species="polychaeta">
