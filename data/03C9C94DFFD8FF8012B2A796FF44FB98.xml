<taxonomicName id="4C6003D8FFD8FF8012C7A591FC93FC2C" authority="(Meyrick, 1910) (Meyrick 1910 a)" authorityName="Meyrick" authorityYear="1910" baseAuthorityName="Meyrick" baseAuthorityYear="1910" box="[226,773,943,969]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="332" phylum="Arthropoda" rank="species" species="facunda">
