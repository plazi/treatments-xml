<quantity id="97309B3CFFB2FF8673CD26C3FD99F849" box="[416,558,1988,2014]" metricMagnitude="-3" metricUnit="m" metricValue="3.65" metricValueMax="3.9" metricValueMin="3.4" pageId="8" pageNumber="35" unit="mm" value="3.65" valueMax="3.9" valueMin="3.4">3.4–3.9 mm</quantity>
(
<number id="E60024FEFFB2FF86705326C3FDC4F849" box="[574,627,1988,2014]" pageId="8" pageNumber="35" value="3.72">3.72</number>
± 
<quantity id="97309B3CFFB2FF8670F926C3FCB5F849" box="[660,770,1988,2014]" metricMagnitude="-4" metricUnit="m" metricValue="1.3" pageId="8" pageNumber="35" unit="mm" value="0.13">0.13 mm</quantity>
), TL-H: 
<quantity id="97309B3CFFB2FF86710626C3FBB0F849" box="[875,1031,1988,2014]" metricMagnitude="-3" metricUnit="m" metricValue="3.375" metricValueMax="3.6" metricValueMin="3.15" pageId="8" pageNumber="35" unit="mm" value="3.375" valueMax="3.6" valueMin="3.15">3.15–3.6 mm</quantity>
(
<number id="E60024FEFFB2FF86767526C3FBFAF849" box="[1048,1101,1988,2014]" pageId="8" pageNumber="35" value="3.43">3.43</number>
± 
<quantity id="97309B3CFFB2FF86760326C3FB6CF849" box="[1134,1243,1988,2014]" metricMagnitude="-4" metricUnit="m" metricValue="1.4" pageId="8" pageNumber="35" unit="mm" value="0.14">0.14 mm</quantity>
), MW: 
<quantity id="97309B3CFFB2FF87775B26C3FF72FF25" lastPageId="9" lastPageNumber="36" metricMagnitude="-3" metricUnit="m" metricValue="1.925" metricValueMax="2.1" metricValueMin="1.75" pageId="8" pageNumber="35" unit="mm" value="1.925" valueMax="2.1" valueMin="1.75">1.75–2.1 mm</quantity>
(
<number id="E60024FEFFB3FF8772B9219FFEBFFF25" box="[212,264,152,178]" pageId="9" pageNumber="36" value="1.98">1.98</number>
± 
<quantity id="97309B3CFFB3FF87734A219FFE23FF25" box="[295,404,152,178]" metricMagnitude="-5" metricUnit="m" metricValue="9.0" pageId="9" pageNumber="36" unit="mm" value="0.09">0.09 mm</quantity>
), TL/MW: 
<number id="E60024FEFFB3FF877075219FFD39FF25" box="[536,654,152,178]" pageId="9" pageNumber="36" value="1.9849999999999999" valueMax="2.17" valueMin="1.8">1.80–2.17</number>
(
<number id="E60024FEFFB3FF8770F0219FFD65FF25" box="[669,722,152,178]" pageId="9" pageNumber="36" value="1.88">1.88</number>
± 
