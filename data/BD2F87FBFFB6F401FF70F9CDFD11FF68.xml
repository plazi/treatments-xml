<elevation id="BEABD1DEFFB7F406FEF8F9EBFE9FF9F8" box="[287,378,1544,1570]" metricMagnitude="3" metricUnit="m" metricValue="2.15" pageId="43" pageNumber="44" unit="m" value="2150.0">2150 m</elevation>
