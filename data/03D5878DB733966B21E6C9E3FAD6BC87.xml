<typeStatus id="54C78839B733966B20FDC876FE70B97B" box="[396,500,258,283]" pageId="4" pageNumber="579">Holotype</typeStatus>
♂: 
<collectingCountry id="F36B760BB733966B2355C876FD07B97B" box="[548,643,258,283]" name="Ecuador" pageId="4" pageNumber="579">Ecuador</collectingCountry>
, Puerto 
<taxonomicName id="4C7C4D18B733966B2392C876FCA2B97B" box="[739,806,258,283]" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="580" phylum="Arthropoda" rank="species" species="quito" status="n. sp.">Quito</taxonomicName>
, 
<date id="FFC2105BB733966B2242C877FC16B97B" box="[819,914,259,283]" pageId="4" pageNumber="579" value="2009-06">
VI. 
<number id="3DB424BCB733966B2227C877FC16B97B" box="[854,914,259,283]" pageId="4" pageNumber="579" value="2009.0">2009</number>
</date>
, 
<emphasis id="B908EA89B733966B22EEC877FC79B97B" box="[927,1021,259,283]" italics="true" pageId="4" pageNumber="579">ex larva</emphasis>
, Touroult leg. (
<collectionCode id="ED6DAE5EB733966B25C0C876FB7BB97B" box="[1201,1279,258,283]" httpUri="http://grbio.org/cool/3dkp-keqv" name="Museo Civico di Storia Natural de Carmognola" pageId="4" pageNumber="579">MCCI</collectionCode>
); 
