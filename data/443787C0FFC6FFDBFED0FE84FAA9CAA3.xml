<taxonomicName id="0B9E4D55FFC4FFDCFC6DF99AFBC8CD44" box="[949,1090,1538,1561]" class="Insecta" family="Carabidae" genus="Bembidion" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="34" phylum="Arthropoda" rank="species" species="nigrivestis">
