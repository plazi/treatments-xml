(Rigaud, Quebec); 4, 
<taxonomicName id="0B9E4D55FFC4FFDCFED0F9BAFE19CD64" box="[264,403,1570,1593]" class="Insecta" family="Carabidae" genus="Bembidion" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="34" phylum="Arthropoda" rank="species" species="iridipenne">
