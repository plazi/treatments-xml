<taxonomicName id="ADFA4D2E2E6FFFD9FAA78A0EFA0BFEC3" authorityName="Vieillot" authorityYear="1822" box="[1332,1433,268,292]" class="Aves" family="Caprimulgidae" genus="Asio" kingdom="Animalia" order="Caprimulgiformes" pageId="33" pageNumber="58" phylum="Chordata" rank="species" species="fasciata">
