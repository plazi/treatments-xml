(
<bibRefCitation id="C634358FFF9DDC1BFCCD525316765A3F" author="Du" box="[876,1066,1582,1607]" pageId="8" pageNumber="128" refString="Du Plessis, G. (1907) Etude sur la Cercyra verrucosa nov. Nouvelle triclade marine. Revue Suisse de Zoologie, 15, 129 - 140." type="journal article" year="1907">
<collectorName id="0F502DA8FF9DDC1BFCCD525311B95A3F" box="[876,997,1582,1607]" pageId="8" pageNumber="128">Du Plessis</collectorName>
