<bibRefCitation id="C634358FFF9DDC1BFD2E525310A45A3F" author="Ax" box="[655,760,1582,1607]" pageId="8" pageNumber="128" refString="Ax, P. (1959) Zur Systematik, Okologie und Tiergeographie der Turbellarienfauna in den ponto-kaspischen Brackwassergebieten. Zoologische Jahrbucher fur Systematik, 87, 43 - 184." type="journal article" year="1959">Ax 1959</bibRefCitation>
); 
<materialsCitation id="12CD4223FF9DDC1BFCAE525316215A13" ID-GBIF-Occurrence="1500371100" collectorName="Du Plessis" country="France" location="France" pageId="8" pageNumber="128" specimenCount="1">
