<taxonomicName id="4C170FD9FFC0281CD8AA72169745FA8C" box="[414,578,1284,1309]" class="Insecta" family="Asilidae" genus="Leptogaster" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="55" phylum="Arthropoda" rank="species" species="fumipennis">
