Ƥ 
<number id="41125B1AFFB1FF8EFD12C71D78C14285" box="[733,747,620,644]" pageId="25" pageNumber="26" value="4.0">4</number>
<number id="41125B1AFFB1FF8EFD3AC71D79204285" box="[757,778,620,644]" pageId="25" pageNumber="26" value="3.0">3</number>
, Gusinoye Lake, Baraty, 
<date id="83646FFDFFB1FF8EFBF9C71D7E974285" box="[1078,1213,620,644]" pageId="25" pageNumber="26" value="1984-08-07">
<number id="41125B1AFFB1FF8EFBF9C71D7E6E4285" box="[1078,1092,620,644]" pageId="25" pageNumber="26" value="7.0">7</number>
.VIII.1984
</date>
, 
<date id="83646FFDFFB1FF8EFB03C71D7F724285" box="[1228,1368,620,644]" pageId="25" pageNumber="26" value="2007-07-26">
26.VII. 
<number id="41125B1AFFB1FF8EFAD4C71D7F724285" box="[1307,1368,620,644]" pageId="25" pageNumber="26" value="2007.0">2007</number>
</date>
; 
<number id="41125B1AFFB1FF8EFAA8C71D7F5F4285" box="[1383,1397,620,644]" pageId="25" pageNumber="26" value="3.0">3</number>
<number id="41125B1AFFB1FF8EFA4FC71D7FBF4285" box="[1408,1429,620,644]" pageId="25" pageNumber="26" value="3.0">3</number>
, Naushki, 
<date id="83646FFDFFB1FF8EFECDC7FE7BA042A6" box="[258,394,655,679]" pageId="25" pageNumber="26" value="1984-08-05">
<number id="41125B1AFFB1FF8EFECDC7FE7B2542A6" box="[258,271,655,679]" pageId="25" pageNumber="26" value="5.0">5</number>
.VIII.1984
</date>
