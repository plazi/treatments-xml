<quantity id="8A349B0AB055FF9FFD1BE8481506FA9A" box="[673,776,1293,1312]" metricMagnitude="-3" metricUnit="m" metricValue="1.85" metricValueMax="2.2" metricValueMin="1.5" pageId="6" pageNumber="317" unit="mm" value="1.85" valueMax="2.2" valueMin="1.5">1.5–2.2 mm</quantity>
(mean 
<emphasis id="7FB8EAFDB055FF9FFCFCE8491558FA9B" bold="true" box="[838,854,1292,1313]" italics="true" pageId="6" pageNumber="317">¼</emphasis>
<quantity id="8A349B0AB055FF9FFCE1E84815A7FA9A" box="[859,937,1293,1312]" metricMagnitude="-3" metricUnit="m" metricValue="1.87" pageId="6" pageNumber="317" unit="mm" value="1.87">1.87 mm</quantity>
