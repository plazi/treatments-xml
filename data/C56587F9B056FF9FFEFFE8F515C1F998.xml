<quantity id="8A349B0AB055FF9FFEEBE9D317B6FB13" box="[337,440,1174,1193]" metricMagnitude="-3" metricUnit="m" metricValue="5.15" metricValueMax="6.1" metricValueMin="4.2" pageId="6" pageNumber="317" unit="mm" value="5.15" valueMax="6.1" valueMin="4.2">4.2–6.1 mm</quantity>
(mean 
