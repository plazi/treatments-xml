<quantity id="8A349B0AB055FF9FFF15E84816FDFA9A" box="[175,243,1293,1312]" metricMagnitude="-4" metricUnit="m" metricValue="1.0" pageId="6" pageNumber="317" unit="mm" value="0.1">0.1 mm</quantity>
, seven 
<quantity id="8A349B0AB055FF9FFE89E8481782FA9A" box="[307,396,1293,1312]" metricMagnitude="-4" metricUnit="m" metricValue="1.25" pageId="6" pageNumber="317" unit="mm" value="0.125">0.125 mm</quantity>
; club 
<quantity id="8A349B0AB055FF9FFE7AE8481400FA9A" box="[448,526,1293,1312]" metricMagnitude="-4" metricUnit="m" metricValue="6.5" pageId="6" pageNumber="317" unit="mm" value="0.65">0.65 mm</quantity>
. 
<emphasis id="7FB8EAFDB055FF9FFDADE848145AFA9A" box="[535,596,1293,1312]" italics="true" pageId="6" pageNumber="317">Thorax</emphasis>
