<quantity id="8A349B0AB055FF9FFDB7E9D31452FB13" box="[525,604,1174,1193]" metricMagnitude="-3" metricUnit="m" metricValue="5.17" pageId="6" pageNumber="317" unit="mm" value="5.17">5.17 mm</quantity>
, n 
<emphasis id="7FB8EAFDB055FF9FFDCFE9D0148BFB10" bold="true" box="[629,645,1173,1194]" italics="true" pageId="6" pageNumber="317">¼</emphasis>
