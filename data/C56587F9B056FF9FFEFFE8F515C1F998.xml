<quantity id="8A349B0AB055FF9FFD65E9D31548FB13" box="[735,838,1174,1193]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" metricValueMax="3.2" metricValueMin="2.3" pageId="6" pageNumber="317" unit="mm" value="2.75" valueMax="3.2" valueMin="2.3">2.3–3.2 mm</quantity>
(mean 
<emphasis id="7FB8EAFDB055FF9FFC3FE9D0159BFB10" bold="true" box="[901,917,1173,1194]" italics="true" pageId="6" pageNumber="317">¼</emphasis>
<quantity id="8A349B0AB055FF9FFC20E9D315E7FB13" box="[922,1001,1174,1193]" metricMagnitude="-3" metricUnit="m" metricValue="2.77" pageId="6" pageNumber="317" unit="mm" value="2.77">2.77 mm</quantity>
, n 
<emphasis id="7FB8EAFDB055FF9FFF7AE9E916DEFB7B" bold="true" box="[192,208,1196,1217]" italics="true" pageId="6" pageNumber="317">¼</emphasis>
2). 
<emphasis id="7FB8EAFDB055FF9FFF4CE9EB1733FB7B" box="[246,317,1198,1217]" italics="true" pageId="6" pageNumber="317">Rostrum</emphasis>
: length: 
<quantity id="8A349B0AB055FF9FFE34E9EB17F9FB7B" box="[398,503,1198,1217]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" metricValueMax="5.2" metricValueMin="3.2" pageId="6" pageNumber="317" unit="mm" value="4.2" valueMax="5.2" valueMin="3.2">3.2–5.2 mm</quantity>
