<taxonomicName id="5E7153E912E7820F00F4E248CCAC85FC" authority="Wiedemann, 1824" authorityName="Wiedemann" authorityYear="1824" class="Insecta" family="Syrphidae" genus="Ceratophya" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Ceratophya variegatus" order="Diptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="variegatus">Ceratophya variegatus</taxonomicName>
