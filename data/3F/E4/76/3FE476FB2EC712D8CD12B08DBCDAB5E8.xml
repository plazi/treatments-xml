<taxonomicName LSID-ZBK="urn:lsid:zoobank.org:act:C4304186-4FD4-4672-BDF1-AC51D2C01419" class="Actinopterygii" family="Sisoridae" genus="Glyptothorax" kingdom="Animalia" lsidName-ZBK="Glyptothorax Blyth 1860:154" order="Siluriformes" pageNumber="40" phylum="Chordata" rank="genus">Glyptothorax</taxonomicName>
