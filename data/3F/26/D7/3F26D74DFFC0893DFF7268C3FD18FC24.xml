° 
<number box="[383,416,792,818]" pageId="6" pageNumber="7" value="45.0">45</number>
’N
</geoCoordinate>
, 
<geoCoordinate box="[457,565,792,818]" direction="west" orientation="longitude" pageId="6" pageNumber="7" precision="925" value="-80.25">
<number box="[457,489,792,818]" pageId="6" pageNumber="7" value="80.0">80</number>
° 
