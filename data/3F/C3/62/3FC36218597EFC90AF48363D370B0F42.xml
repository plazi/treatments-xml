<paragraph pageId="2" pageNumber="49">Etymology.</paragraph>
<paragraph pageId="2" pageNumber="49">
<taxonomicName authority="Sendra &amp; Stoev, 2017" authorityName="Sendra &amp; Stoev" authorityYear="2017" class="Diplura" family="Campodeidae" genus="Turkmenocampa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Turkmenocampa" order="Diplura" pageId="2" pageNumber="49" phylum="Arthropoda" rank="genus">Turkmenocampa</taxonomicName>
