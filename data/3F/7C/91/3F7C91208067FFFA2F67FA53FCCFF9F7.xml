<number box="[785,829,689,715]" pageId="3" pageNumber="4" value="137.0">137</number>
µm and 
<number box="[930,974,689,715]" pageId="3" pageNumber="4" value="150.0">150</number>
µm long; spur of middle tibia 
<number box="[264,293,729,755]" pageId="3" pageNumber="4" value="20.0">20</number>
µm; 
<number box="[356,385,729,755]" pageId="3" pageNumber="4" value="35.0">35</number>
µm long, comb with 
<number box="[639,668,729,755]" pageId="3" pageNumber="4" value="22.0">22</number>
teeth, 
<number box="[748,821,729,755]" pageId="3" pageNumber="4" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
µm long; spur of hind tibia 
<number box="[1153,1182,729,755]" pageId="3" pageNumber="4" value="22.0">22</number>
µm; 
<number box="[1245,1274,729,755]" pageId="3" pageNumber="4" value="35.0">35</number>
