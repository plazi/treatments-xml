<td box="[1091,1131,1027,1056]" gridcol="6" gridrow="3" pageId="3" pageNumber="4">110</td>
<td box="[1209,1256,1027,1056]" gridcol="7" gridrow="3" pageId="3" pageNumber="4">0.68</td>
