<emphasis bold="true" box="[151,343,1711,1735]" pageId="19" pageNumber="20">FIGURES 44–50</emphasis>
. 
<taxonomicName authorityName="Beveridge &amp; Shamsi" authorityYear="2009" box="[358,616,1711,1734]" class="Cestoda" family="Anoplocephalidae" genus="Progamotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="19" pageNumber="20" phylum="Platyhelminthes" rank="species" species="dilatata" status="sp. nov.">
<emphasis box="[358,616,1711,1734]" italics="true" pageId="19" pageNumber="20">Progamotaenia dilatata</emphasis>
</taxonomicName>
<emphasis bold="true" box="[623,710,1711,1735]" pageId="19" pageNumber="20">
<taxonomicNameLabel box="[623,710,1711,1735]" pageId="19" pageNumber="20" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
from 
<taxonomicName baseAuthorityName="Desmarest" baseAuthorityYear="1804" box="[776,956,1711,1734]" class="Mammalia" family="Macropodidae" genus="Wallabia" kingdom="Animalia" order="Diprotodontia" pageId="19" pageNumber="20" phylum="Chordata" rank="species" species="bicolor">
<emphasis box="[776,956,1711,1734]" italics="true" pageId="19" pageNumber="20">Wallabia bicolor</emphasis>
</taxonomicName>
. 
<emphasis bold="true" box="[970,997,1711,1735]" pageId="19" pageNumber="20">44</emphasis>
, Entire cestode. 
<emphasis bold="true" box="[1179,1206,1711,1735]" pageId="19" pageNumber="20">45</emphasis>
