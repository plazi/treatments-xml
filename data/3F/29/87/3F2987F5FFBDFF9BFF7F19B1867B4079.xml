<subSubSection pageId="13" pageNumber="438" type="etymology">
<paragraph blockId="13.[151,1437,151,357]" pageId="13" pageNumber="438">
<emphasis bold="true" box="[199,334,188,213]" pageId="13" pageNumber="438">Etymology.</emphasis>
