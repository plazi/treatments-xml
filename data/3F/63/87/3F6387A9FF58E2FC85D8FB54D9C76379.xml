to 
<geoCoordinate box="[398,467,1340,1364]" direction="north" orientation="latitude" pageId="181" pageNumber="182" precision="5555" value="1.1">1.1°N</geoCoordinate>
, 
<geoCoordinate box="[480,567,1340,1364]" direction="west" orientation="longitude" pageId="181" pageNumber="182" precision="5555" value="-77.3">77.3°W</geoCoordinate>
to 
<geoCoordinate box="[605,691,1340,1365]" direction="west" orientation="longitude" pageId="181" pageNumber="182" precision="5555" value="-79.5">79.5°W</geoCoordinate>
(Cely &amp; Chiquillo 1993), 
<collectingCountry box="[994,1048,1341,1364]" name="Peru" pageId="181" pageNumber="182">Peru</collectingCountry>
, from 
<geoCoordinate box="[1124,1186,1340,1365]" direction="south" orientation="latitude" pageId="181" pageNumber="182" precision="5555" value="-6.8">6.8°S</geoCoordinate>
to 17°S (
<collectingRegion box="[1294,1408,1340,1365]" country="Colombia" name="Santander" pageId="181" pageNumber="182">Santander</collectingRegion>
