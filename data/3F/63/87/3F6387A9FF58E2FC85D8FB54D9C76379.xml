1981; 
<collectionCode box="[264,370,1376,1400]" country="Peru" httpUri="http://grbio.org/cool/48mj-v9wp" name="Instituto del Mar del Peru" pageId="181" pageNumber="182">IMARPE</collectionCode>
database); Atlantic Ocean, 
<collectingCountry box="[693,806,1375,1400]" name="Colombia" pageId="181" pageNumber="182">Colombia</collectingCountry>
, at 
<geoCoordinate box="[851,930,1376,1400]" direction="north" orientation="latitude" pageId="181" pageNumber="182" precision="5555" value="11.2">11.2°N</geoCoordinate>
<geoCoordinate box="[940,1028,1376,1400]" direction="west" orientation="longitude" pageId="181" pageNumber="182" precision="5555" value="-74.2">74.2°W</geoCoordinate>
