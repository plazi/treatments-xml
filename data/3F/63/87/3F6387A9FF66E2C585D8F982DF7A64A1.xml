<taxonomicName class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="139" pageNumber="140" phylum="Cnidaria" rank="variety" species="divaricata" variety="subdichotoma">
