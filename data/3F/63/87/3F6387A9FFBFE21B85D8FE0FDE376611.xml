<taxonomicName authority="Hassall, 1848" authorityName="Hassall" authorityYear="1848" box="[165,541,476,501]" class="Hydrozoa" family="Lafoeidae" genus="Filellum" kingdom="Animalia" order="Leptothecata" pageId="82" pageNumber="83" phylum="Cnidaria" rank="species" species="serpens">
