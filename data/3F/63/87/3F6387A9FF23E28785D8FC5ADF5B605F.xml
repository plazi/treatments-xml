<emphasis box="[595,702,977,1002]" italics="true" pageId="206" pageNumber="207">flagellata</emphasis>
Haeckel, 1880
</taxonomicName>
; 
<taxonomicName box="[905,1140,977,1002]" class="Scyphozoa" family="Catostylidae" genus="Catostylus" kingdom="Animalia" order="Rhizostomeae" pageId="206" pageNumber="207" phylum="Cnidaria" rank="species" species="cruciatus">
