<taxonomicName box="[252,448,871,896]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="165" pageNumber="166" phylum="Cnidaria" rank="species" species="fraseri">
<emphasis box="[252,448,871,896]" italics="true" pageId="165" pageNumber="166">Halecium fraseri</emphasis>
</taxonomicName>
, 
<taxonomicName box="[466,657,871,896]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="165" pageNumber="166" phylum="Cnidaria" rank="species" species="beanii">
<emphasis box="[466,657,871,896]" italics="true" pageId="165" pageNumber="166">Halecium beanii</emphasis>
</taxonomicName>
, 
<taxonomicName box="[675,940,871,896]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="165" pageNumber="166" phylum="Cnidaria" rank="species" species="polyzonias">
