<quantity box="[450,582,938,962]" metricMagnitude="2" metricUnit="m" metricValue="1.23" metricValueMax="2.17" metricValueMin="0.29" pageId="96" pageNumber="97" unit="m" value="123.0" valueMax="217.0" valueMin="29.0">29 to 217m</quantity>
