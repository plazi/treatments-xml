America: polyp—Pacific Ocean, 
<collectingCountry box="[812,866,1880,1903]" name="Peru" pageId="155" pageNumber="156">Peru</collectingCountry>
, at 
<geoCoordinate box="[903,979,1879,1904]" direction="south" orientation="latitude" pageId="155" pageNumber="156" precision="555" value="-6.95">6.95°S</geoCoordinate>
<geoCoordinate box="[986,1087,1880,1904]" direction="west" orientation="longitude" pageId="155" pageNumber="156" precision="555" value="-80.7">80.70°W</geoCoordinate>
