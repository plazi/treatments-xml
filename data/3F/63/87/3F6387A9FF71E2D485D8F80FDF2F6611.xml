</taxonomicName>
, 
<taxonomicName box="[279,508,764,789]" class="Hydrozoa" family="Thyroscyphidae" genus="Parascyphus" kingdom="Animalia" order="Leptothecata" pageId="157" pageNumber="158" phylum="Cnidaria" rank="species" species="repens">
