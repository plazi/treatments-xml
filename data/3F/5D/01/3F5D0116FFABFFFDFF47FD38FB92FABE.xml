<taxonomicName authorityName="" authorityYear="1847" box="[597,727,1034,1059]" class="Aves" family="Thraupidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="462" phylum="Chordata" rank="family">Thraupidae</taxonomicName>
