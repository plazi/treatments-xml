<taxonomicName authorityName="Sundevall" authorityYear="1836" box="[1320,1432,998,1023]" class="Aves" family="Ploceidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="462" phylum="Chordata" rank="family">Ploceidae</taxonomicName>
