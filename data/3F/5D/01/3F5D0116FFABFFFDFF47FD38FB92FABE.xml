, 
<taxonomicName authorityName="Vigors" authorityYear="1825" box="[847,956,998,1023]" class="Aves" family="Oriolidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="462" phylum="Chordata" rank="family">Oriolidae</taxonomicName>
, 
<taxonomicName authorityName="Vigors" authorityYear="1825" box="[968,1055,998,1023]" class="Aves" family="Paridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="462" phylum="Chordata" rank="family">Paridae</taxonomicName>
, 
<taxonomicName box="[1067,1176,998,1023]" class="Aves" family="Parulidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="11" pageNumber="462" phylum="Chordata" rank="family">Parulidae</taxonomicName>
