<taxonomicName box="[1150,1271,1070,1095]" class="Aves" higherTaxonomySource="GBIF" kingdom="Animalia" order="Piciformes" pageId="11" pageNumber="462" phylum="Chordata" rank="order">Piciformes</taxonomicName>
(
<taxonomicName authorityName="Vigors" authorityYear="1825" box="[1287,1375,1070,1095]" class="Aves" family="Picidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Piciformes" pageId="11" pageNumber="462" phylum="Chordata" rank="family">Picidae</taxonomicName>
