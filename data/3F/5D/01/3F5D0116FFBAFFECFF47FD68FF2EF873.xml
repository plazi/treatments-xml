<paragraph blockId="26.[151,1437,749,1962]" pageId="26" pageNumber="477">
<collectionCode box="[199,282,1218,1242]" pageId="26" pageNumber="477">MALE</collectionCode>
. Total body length 
<quantity box="[500,572,1217,1242]" metricMagnitude="1" metricUnit="m" metricValue="1.9812" pageId="26" pageNumber="477" unit="in" value="780.0">780 in</quantity>
<specimenCount box="[580,702,1218,1242]" pageId="26" pageNumber="477" type="generic" typeStatus="paratype">1 paratype</specimenCount>
. 
<taxonomicName authorityName="Krefft" authorityYear="1871" box="[713,856,1217,1242]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="26" pageNumber="477" phylum="Nematoda" rank="genus">
<emphasis box="[713,856,1217,1242]" italics="true" pageId="26" pageNumber="477">Gnathosoma</emphasis>
</taxonomicName>
