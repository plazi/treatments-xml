<subSubSection box="[189,964,1212,1237]" pageId="14" pageNumber="209" type="materials_examined">
<paragraph blockId="14.[189,964,1212,1237]" box="[189,964,1212,1237]" pageId="14" pageNumber="209">
<typeStatus box="[189,239,1213,1237]" pageId="14" pageNumber="209">Type</typeStatus>
species: 
<taxonomicName authority="JASCHHOF" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2010" box="[329,719,1212,1236]" class="Insecta" family="Cecidomyiidae" genus="Hintelmannomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="209" phylum="Arthropoda" rank="species" species="aestimata" status="sp. n.">
<emphasis box="[329,611,1212,1236]" italics="true" pageId="14" pageNumber="209">Hintelmannomyia aestimata</emphasis>
JASCHHOF
</taxonomicName>
<taxonomicNameLabel box="[727,784,1213,1236]" pageId="14" pageNumber="209" rank="species">sp. n.</taxonomicNameLabel>
, described below.
</paragraph>
