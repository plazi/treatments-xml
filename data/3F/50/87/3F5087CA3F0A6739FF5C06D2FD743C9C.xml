<elevation box="[151,224,295,321]" metricMagnitude="2" metricUnit="m" metricValue="1.4" pageId="35" pageNumber="36" unit="m" value="140.0">140 m</elevation>
</quantity>
, 
<date box="[236,367,295,321]" pageId="35" pageNumber="36" value="2009-07-21">
<collectingDate box="[236,367,295,321]" pageId="35" pageNumber="36" value="2009-07-21">21 Jul 2009</collectingDate>
</date>
(
<specimenCount box="[385,503,295,321]" pageId="35" pageNumber="36" type="generic" typeStatus="paratype">1 paratype</specimenCount>
, NHMD–865951), 
