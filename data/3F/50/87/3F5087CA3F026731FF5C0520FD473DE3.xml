, and 
<taxonomicName baseAuthorityName="Ushakov" baseAuthorityYear="1972" box="[740,866,328,354]" class="Polychaeta" family="Phyllodocidae" genus="Phyllodoce" kingdom="Animalia" order="Phyllodocida" pageId="44" pageNumber="45" phylum="Annelida" rank="genus">
