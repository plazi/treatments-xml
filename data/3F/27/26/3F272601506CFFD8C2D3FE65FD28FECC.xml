<typeStatus box="[819,872,580,601]" pageId="33" pageNumber="116">Type</typeStatus>
species:
</emphasis>
<taxonomicName authority="Pace (1984 c)" authorityName="Pace" authorityYear="1984" class="Insecta" family="Staphylinidae" genus="Hoplandria" kingdom="Animalia" order="Coleoptera" pageId="33" pageNumber="116" phylum="Arthropoda" rank="species" species="paupercula" subGenus="Ditropandria">
