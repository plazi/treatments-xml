<taxonomicName authority="Cameron" authorityName="Cameron" box="[901,1173,932,953]" class="Insecta" family="Staphylinidae" genus="Omoplandria" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="127" phylum="Arthropoda" rank="species" species="fuscipennis">
<emphasis box="[901,1060,932,953]" italics="true" pageId="44" pageNumber="127">O. fuscipennis</emphasis>
Cameron
</taxonomicName>
and 
<taxonomicName authority="(Sharp)" baseAuthorityName="Sharp" baseAuthorityYear="1888" class="Insecta" family="Staphylinidae" genus="Omoplandria" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="127" phylum="Arthropoda" rank="species" species="gyrophaenula">
<emphasis box="[1235,1422,932,953]" italics="true" pageId="44" pageNumber="127">O. gyrophaenula</emphasis>
