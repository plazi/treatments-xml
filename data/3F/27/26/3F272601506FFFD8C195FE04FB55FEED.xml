<emphasis box="[1012,1117,227,248]" italics="true" pageId="34" pageNumber="117">alternata</emphasis>
Pace
</taxonomicName>
, 
<taxonomicName authority="(Pace)" baseAuthorityName="Pace" class="Insecta" family="Staphylinidae" genus="Ditropandria" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="117" phylum="Arthropoda" rank="species" species="dauphinensis" subGenus="Ditropandria">
