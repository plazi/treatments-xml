<subSubSection pageId="2" pageNumber="3" type="nomenclature">
<paragraph pageId="2" pageNumber="3">
<taxonomicName LSID="http://species-id.net/wiki/Pseudostomella" family="Thaumastodermatidae" genus="Pseudostomella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudostomella" order="Macrodasyida" pageId="2" pageNumber="3" phylum="Gastrotricha" rank="genus">Pseudostomella</taxonomicName>
