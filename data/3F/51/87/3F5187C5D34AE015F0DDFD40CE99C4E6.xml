<taxonomicName authorityName="Andrassy" authorityYear="1981" baseAuthorityName="Butschli" baseAuthorityYear="1973" box="[814,921,548,573]" class="Chromadorea" family="Monhysteridae" genus="Monhystera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monhysterida" pageId="15" pageNumber="91" phylum="Nematoda" rank="species" species="villosa">
