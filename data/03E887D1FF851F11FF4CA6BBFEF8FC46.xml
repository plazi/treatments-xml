</emphasis>
<taxonomicName id="4C414D44FF851F11FE9BA774FDFDFEEF" box="[320,617,301,326]" class="Malacostraca" family="Maeridae" genus="Saurodocus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="61" phylum="Arthropoda" rank="species" species="minimarenus" status="sp. nov.">
