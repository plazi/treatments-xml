<taxonomicName id="4C6C4D3BFFC9FF958795A6CEFC97795F" authority="Franz" authorityName="Franz" box="[469,786,1017,1041]" class="Insecta" family="Staphylinidae" genus="Loeblites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="396" phylum="Arthropoda" rank="species" species="mastigicornis">
