<taxonomicName id="4C104D7A544BFFDB8FB586A98124DEF4" box="[410,589,1428,1451]" class="Insecta" family="Reduviidae" genus="Orbella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="exceptio">
