<quantity id="4C849B14FFDFFFABFE99231C97C1FDF6" box="[333,503,518,545]" metricMagnitude="-3" metricUnit="m" metricValue="4.525" metricValueMax="4.85" metricValueMin="4.2" pageId="13" pageNumber="14" unit="mm" value="4.525" valueMax="4.85" valueMin="4.2">4.20–4.85 mm</quantity>
(
<typeStatus id="54C78853FFDFFFABFDD1231D945CFDF6" box="[517,618,519,545]" pageId="13" pageNumber="14">holotype</typeStatus>
