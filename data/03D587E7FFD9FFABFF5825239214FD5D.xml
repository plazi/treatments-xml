(
<typeStatus id="54C78853FFDFFFABFC3A231D9265FDF6" box="[1006,1107,519,545]" pageId="13" pageNumber="14">holotype</typeStatus>
<quantity id="4C849B14FFDFFFABFF58233096CCFD93" box="[140,250,554,581]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="13" pageNumber="14" unit="mm" value="1.05">1.05 mm</quantity>
