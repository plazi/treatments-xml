<taxonomicName id="4C114D5BB579FEE5C4447098E2A8FA90" authority="PLATNICK, 2000" authorityName="PLATNICK" authorityYear="2000" box="[306,467,1271,1294]" class="Arachnida" family="Lamponidae" genus="Longepi" kingdom="Animalia" order="Araneae" pageId="290" pageNumber="291" phylum="Arthropoda" rank="species" species="boyd" status="new species">
