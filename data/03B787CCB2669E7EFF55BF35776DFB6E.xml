<taxonomicName id="4C1E4D59B2669E7EFE88B8747488FB8A" box="[330,425,1114,1139]" class="Malacostraca" family="Hippolytidae" genus="Lysmata" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="bahia">
<emphasis id="B96AEAC8B2669E7EFE88B8747488FB8A" box="[330,425,1114,1139]" italics="true" pageId="15" pageNumber="16">L. bahia</emphasis>
</taxonomicName>
