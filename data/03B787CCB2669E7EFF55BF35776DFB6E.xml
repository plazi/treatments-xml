(
<bibRefCitation id="EF8F4B2BB2669E7EFC73B81E7168FBB6" author="Gibbes" box="[945,1097,1078,1103]" pageId="15" pageNumber="16" refString="Gibbes, L. R. (1850) On the carcinological collections of the cabinets of natural history in the United States: with an enumeration of the species contained therein and descriptions of new species. American Association for the Advancement of Science, 3, 167 - 201." type="journal article" year="1850">
Gibbes, 
<number id="3DD624FDB2669E7EFBD2B81E7168FBB6" box="[1040,1097,1078,1103]" pageId="15" pageNumber="16" value="1850.0">1850</number>
</bibRefCitation>
)
</taxonomicName>
by 
<bibRefCitation id="EF8F4B2BB2669E7EFBBEB81E7048FBB6" author="Fausto-Filho" box="[1148,1385,1078,1103]" pageId="15" pageNumber="16" refString="Fausto-Filho, J. (1970 a) Quarta contribuicao ao inventario dos crustaceos decapodos marinhos do nordeste brasileiro. Arquivos de Ciencias do Mar, 10, 55 - 60." type="journal article" year="1970" yearSuffix="a">
Fausto-Filho (
<number id="3DD624FDB2669E7EFAD8B81F7075FBB6" box="[1306,1364,1079,1103]" pageId="15" pageNumber="16" value="1970.0">1970</number>
