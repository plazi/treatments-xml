<taxonomicName id="BA82B38FFFC9E9098214F83AC525F7A5" authority="Topsent, 1919" authorityName="Topsent" authorityYear="1919" box="[296,787,1930,1957]" class="Demospongiae" family="Hemiasterellidae" genus="Hemiasterella" kingdom="Animalia" order="Hadromerida" pageId="47" pageNumber="48" phylum="Porifera" rank="species" species="complicata">
