<taxonomicName id="4C614FC1FF80FFA5FE69FB536B00059D" authority="(Gerstaecker, 1874)" authorityName="Gerstaecker" authorityYear="1874" baseAuthorityName="Gerstaecker" baseAuthorityYear="1874" box="[429,800,1197,1223]" class="Insecta" family="Mutillidae" genus="Traumatomutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="hemicycla">
