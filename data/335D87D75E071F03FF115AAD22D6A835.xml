<taxonomicName id="7CF44D425E061F1CFA0559952646ADF0" class="Arachnida" family="Salticidae" genus="Cucudeta" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="gahavisuka">
