<taxonomicName id="DB974D55FFE9FFB1FE386ACFFDFCF797" authorityName="Dumitrica &amp; Zügel" authorityYear="2003" box="[498,590,998,1022]" class="Polycystina" family="Sethoperidae" genus="Turriseiffelus" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="15" pageNumber="20" phylum="Radiozoa" rank="species" species="invalidus">
