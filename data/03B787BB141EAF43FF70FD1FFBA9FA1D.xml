<date id="FFA0106D141EAF43FEC0FB4CFE20FB42" box="[295,434,1213,1237]" pageId="9" pageNumber="10" value="2011-06-07">
<number id="3DD6248A141EAF43FEC0FB4CFEA7FB42" box="[295,309,1214,1237]" pageId="9" pageNumber="10" value="7.0">7</number>
June 
<number id="3DD6248A141EAF43FE90FB4FFE20FB42" box="[375,434,1213,1237]" pageId="9" pageNumber="10" value="2011.0">2011</number>
