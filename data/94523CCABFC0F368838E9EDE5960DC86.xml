(circle) and 
<taxonomicName id="6F13737EEE364ACC8D2D0C78DCD085F9" genus="M." lsidName="M. kirgisica" pageId="3" pageNumber="78" rank="species" species="kirgisica">M. kirgisica</taxonomicName>
