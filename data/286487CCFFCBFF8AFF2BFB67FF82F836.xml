<taxonomicName id="67CD4D59FFCBFF8AFE9DF8EFFDF4F8FE" authorityName="P.A.Saccardo" authorityYear="1888" box="[318,441,1850,1876]" class="Dothideomycetes" family="Roussoellaceae" genus="Roussoella" kingdom="Fungi" order="Pleosporales" pageId="8" pageNumber="9" phylum="Ascomycota" rank="genus">
