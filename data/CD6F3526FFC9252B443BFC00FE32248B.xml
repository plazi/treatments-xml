(= 
<accessionNumber id="5A9519D3FFC8252B46CFFDBAFD8E24B5" box="[456,584,616,641]" httpUri="http://www.ncbi.nlm.nih.gov/protein/DSM17509" pageId="19" pageNumber="20">DSM 17509</accessionNumber>
