<materialsCitation id="74813CCB8A52FFFB22A86D6FFE066B85" ID-GBIF-Occurrence="2240609756" collectingDate="2018-07-08" collectingDateMax="2018-07-21" collectingDateMin="2018-07-08" collectionCode="MT, MCJ" collectorName="Alvdalen &amp; Fulufjallet NP" country="Sweden" location="Goljan valley" pageId="16" pageNumber="297" specimenCount="1" specimenCount-male="1" stateProvince="Dalarna">
