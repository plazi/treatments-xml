: 
<specimenCount id="D2EFFD1F8A52FFFB21316D6FFDF76BA1" box="[514,591,873,899]" pageId="16" pageNumber="297" type="male">1 male</specimenCount>
, 
<collectingRegion id="062DF8748A52FFFB21696D6FFD0E6BA1" box="[602,694,873,899]" country="Sweden" name="Dalarna" pageId="16" pageNumber="297">Dalarna</collectingRegion>
, 
