<taxonomicName id="4C6DE5A20C777309FBC5CA11B8E7DA38" box="[1076,1244,830,853]" class="Actinopterygii" family="Syngnathidae" genus="Hippocampus" kingdom="Animalia" order="Syngnathiformes" pageId="27" pageNumber="28" phylum="Chordata" rank="species" species="curvicuspis">
