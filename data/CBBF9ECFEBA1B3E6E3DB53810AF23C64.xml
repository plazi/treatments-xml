<taxonomicName id="A98860D76F1CCB319435CB6719BE6632" authority="Ochoa &amp; Naskrecki, 1995" authorityName="Ochoa &amp; Naskrecki" authorityYear="1995" class="Arachnida" family="Tarsonemidae" genus="Excelsotarsonemus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Excelsotarsonemus tupi" order="Trombidiformes" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="tupi">Excelsotarsonemus tupi</taxonomicName>
