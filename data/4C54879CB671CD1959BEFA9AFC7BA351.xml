<collectingCountry id="BCEA761AB671CD195A41F932FC48A2AD" box="[872,957,1746,1772]" name="Bolivia" pageId="44" pageNumber="45">Bolivia</collectingCountry>
, 
<collectingCountry id="BCEA761AB671CD195AE0F932FBE5A2AD" box="[969,1040,1746,1772]" name="Brazil" pageId="44" pageNumber="45">Brazil</collectingCountry>
, 
<collectingCountry id="BCEA761AB671CD195D35F932FB30A2AD" box="[1052,1221,1746,1772]" name="French Guiana" pageId="44" pageNumber="45">French Guiana</collectingCountry>
, and 
<collectingCountry id="BCEA761AB671CD195C28F933FAC1A2AC" box="[1281,1332,1747,1773]" name="Peru" pageId="44" pageNumber="45">Peru</collectingCountry>
(
<bibRefCitation id="A06C4B7BB671CD195C6AF932FF2AA351" author="Metcalf, Z. P." pageId="44" pageNumber="45" pagination="1 - 585" refId="ref106666" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 1. Cicadidae. Section I. Tibiceninae. North Carolina State College Contribution, 1502, i-vii + 1 - 585." type="journal article" year="1963">Metcalf 1963a</bibRefCitation>
; 
<bibRefCitation id="A06C4B7BB671CD1959C3F916FE63A351" author="Sanborn, A. F." box="[234,406,1782,1808]" pageId="44" pageNumber="45" pagination="377 - 418" refId="ref107675" refString="Sanborn, A. F. (2011 a) Checklist of the cicadas of French Guiana including new records and the description of nine new species (Insecta, Hemiptera, Cicadoidea, Cicadidae). Zoosystema, 33, 377 - 418. https: // doi. org / 10.5252 / z 2011 n 3 a 6" type="journal article" year="2011">Sanborn 2011a</bibRefCitation>
; 2013; 2019b; 
<bibRefCitation id="A06C4B7BB671CD195B15F917FC76A351" author="Maccagnan, D. H. B. &amp; Sanborn, A. F." box="[572,899,1781,1809]" pageId="44" pageNumber="45" pagination="984 - 986" refId="ref106236" refString="Maccagnan, D. H. B. &amp; Sanborn, A. F. (2015) Orialella aerizulae (Hemiptera: Cicadidae): first record in Brazil. Florida Entomologist, 98, 984 - 986. https: // doi. org / 10.1653 / 024.098.0331" type="journal article" year="2015">Maccagnan &amp; Sanborn 2015</bibRefCitation>
