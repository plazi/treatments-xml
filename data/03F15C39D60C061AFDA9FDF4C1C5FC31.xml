<quantity id="4CA040CAD60C061EFEA4FB0DC481FAD1" box="[343,429,1262,1289]" metricMagnitude="-2" metricUnit="m" metricValue="4.4" pageId="41" pageNumber="42" unit="mm" value="44.0">44 mm</quantity>
