<taxonomicName id="EBB1601F0709A7ED4390B975FDF430CD" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Picea" order="Pinales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="genus">
