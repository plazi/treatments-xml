<taxonomicName id="A590FC9AF710529B5138D32972CD19EA" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus" order="Fagales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="genus">
<emphasis id="9C48B03474E74108984DB025FFDE9B67" italics="true" pageId="0" pageNumber="89">Quercus</emphasis>
