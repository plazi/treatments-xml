<taxonomicName id="3DEDAC613AD7FC4CD101DA4FB58144C7" class="Pinopsida" family="Pinaceae" genus="Abies" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Abies" order="Pinales" pageId="0" pageNumber="89" phylum="Tracheophyta" rank="genus">
<emphasis id="B70840C5EB22283B70130825FE5A74CE" italics="true" pageId="0" pageNumber="89">Abies</emphasis>
