<quantity id="4CD19B501809F32552A17EA2654B99A9" box="[899,1048,1963,1988]" metricMagnitude="2" metricUnit="m" metricValue="3.56" metricValueMax="5.0" metricValueMin="2.12" pageId="30" pageNumber="411" unit="m" value="356.0" valueMax="500.0" valueMin="212.0">212 to 500 m</quantity>
