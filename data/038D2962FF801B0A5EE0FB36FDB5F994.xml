<taxonomicName id="4C24E3F7FF801B0A5D07F9B2FC1AF9B1" box="[875,976,1575,1601]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="64" phylum="Arthropoda" rank="genus">Catocala</taxonomicName>
