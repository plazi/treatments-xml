<taxonomicName id="4C24E3F7FF801B0A5C15F996FD05F9ED" authorityName="Walker" authorityYear="1858" box="[633,719,1539,1565]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="27" pageNumber="64" phylum="Arthropoda" rank="species" species="nuptula">
