<paragraph id="8BF936E4FFAEFFF3FF58FC34FB0BDA80" blockId="1.[151,1207,977,999]" box="[151,1207,977,999]" pageId="1" pageNumber="454">
<taxonomicName id="4C464D67FFAEFFF3FF58FC34FED9DA80" box="[151,357,977,999]" class="Actinopterygii" family="Gobiidae" genus="Didogobius" kingdom="Animalia" order="Perciformes" pageId="1" pageNumber="454" phylum="Chordata" rank="species" species="bentuvii">
<emphasis id="B932EAF6FFAEFFF3FF58FC34FED9DA80" box="[151,357,977,999]" italics="true" pageId="1" pageNumber="454">Didogobius bentuvii</emphasis>
</taxonomicName>
, 
<typeStatus id="54FD8846FFAEFFF3FEBDFC34FE71DA80" box="[370,461,977,999]" pageId="1" pageNumber="454">holotype</typeStatus>
, female, 
<quantity id="4CBE9B01FFAEFFF3FDE7FC34FD3EDA80" box="[552,642,977,999]" metricMagnitude="-2" metricUnit="m" metricValue="3.65" pageId="1" pageNumber="454" unit="mm" value="36.5">36.5 mm</quantity>
