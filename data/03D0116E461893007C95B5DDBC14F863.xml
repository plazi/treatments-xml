<collectingCountry id="F36EE0E8461893007D65B106BFB2F863" box="[359,458,1909,1935]" name="Namibia" pageId="36" pageNumber="37">Namibia</collectingCountry>
, 
<collectingCountry id="F36EE0E8461893007DD4B106BC10F863" box="[470,616,1909,1935]" name="South Africa" pageId="36" pageNumber="37">South Africa</collectingCountry>
