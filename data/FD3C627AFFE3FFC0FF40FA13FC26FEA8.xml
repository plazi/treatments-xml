, 
<collectingCountry id="0D8293FCFFE3FFC3FDE5F9E8FBB3F98F" box="[941,1060,1631,1657]" name="Colombia" pageId="65" pageNumber="66">Colombia</collectingCountry>
, 
<collectingCountry id="0D8293FCFFE3FFC3FA79F9E8FB22F98F" box="[1073,1205,1631,1657]" name="Costa Rica" pageId="65" pageNumber="66">Costa Rica</collectingCountry>
