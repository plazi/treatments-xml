<taxonomicName id="0EE118870404FF87FDB1F99474B71645" baseAuthorityName="Perrier" baseAuthorityYear="1894" box="[533,677,1625,1651]" class="Asteroidea" family="Pseudarchasteridae" genus="Pseudarchaster" kingdom="Animalia" order="Paxillosida" pageId="84" pageNumber="85" phylum="Echinodermata" rank="species" species="annectens">
