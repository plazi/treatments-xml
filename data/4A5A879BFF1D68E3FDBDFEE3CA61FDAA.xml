<taxonomicName id="05F34D0EFF1D68E4FF4AFDAACD65FDD5" authority="Coleman, 1994" authorityName="Coleman" authorityYear="1994" box="[189,626,578,604]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="155" pageNumber="156" phylum="Arthropoda" rank="species" species="annabellae">
