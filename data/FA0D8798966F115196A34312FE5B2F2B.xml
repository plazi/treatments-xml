<typeStatus id="AD1F882C966F114F96E043ADFEA9293A" box="[203,297,510,534]" pageId="15" pageNumber="656">holotype</typeStatus>
of 
<taxonomicName id="B5A44D0D966F114F977A43AEFF342903" authority="(Dollfus, 1960)" baseAuthorityName="Dollfus" baseAuthorityYear="1960" class="Cestoda" family="Tentaculariidae" genus="Nybelinia" kingdom="Animalia" order="Trypanorhyncha" pageId="15" pageNumber="656" phylum="Platyhelminthes" rank="species" species="rougetcampanae">
<emphasis id="40D0EA9C966F114F977A43AEFD9B2938" box="[337,539,509,532]" italics="true" pageId="15" pageNumber="656">H. rougetcampanae</emphasis>
(Dollfus, 1960)
</taxonomicName>
from 
<taxonomicName id="B5A44D0D966F114F972B4044FE952965" authority="(Muller &amp; Troschel, 1848)" baseAuthorityName="Muller &amp; Troschel" baseAuthorityYear="1848" class="Actinopterygii" family="Tetraodontidae" genus="Sphoeroides" kingdom="Animalia" order="Tetraodontiformes" pageId="15" pageNumber="656" phylum="Chordata" rank="species" species="pachygaster">
<emphasis id="40D0EA9C966F114F972B4044FD852903" box="[256,517,535,559]" italics="true" pageId="15" pageNumber="656">Sphoeroides pachygaster</emphasis>
