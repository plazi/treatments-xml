<taxonomicName id="4C5EBDC65B69FFE1D6A0FE7271DFF0C9" authority="Brian, 1906" authorityName="Brian" authorityYear="1906" box="[151,486,439,466]" class="Maxillopoda" family="Pennellidae" genus="Pennella" kingdom="Animalia" order="Siphonostomatoida" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="rubra">
<emphasis id="B92A1A575B69FFE1D6A0FE7271DFF0C9" bold="true" box="[151,486,439,466]" pageId="30" pageNumber="31">
<emphasis id="B92A1A575B69FFE1D6A0FE727176F0CA" bold="true" box="[151,335,439,465]" italics="true" pageId="30" pageNumber="31">Pennella rubra</emphasis>
<bibRefCitation id="EFCFBBB45B69FFE1D762FE7D71DFF0C9" author="Brian" box="[341,486,440,466]" pageId="30" pageNumber="31" refString="Brian, A. (1906) Copepodi parasiti dei Pesci d'Italia. R. Istituto Sordomuti, Geneva, 189 pp." type="book" year="1906">Brian, 1906</bibRefCitation>
