<location id="8EBB30FCFFA1FF9AFEA3D62D70443F67" LSID="urn:lsid:plazi:treatment:03CDD731FFA2FF9DFF47D71171AF3D46:8EBB30FCFFA1FF9AFEA3D62D70443F67" box="[371,560,1547,1573]" country="Australia" latitude="-14.686833" longLatPrecision="13" longitude="145.4415" municipality="Lizard Island" name="Horseshoe Reef" pageId="3" pageNumber="864" stateProvince="Queensland">Horseshoe Reef</location>
, 
<collectingMunicipality id="6BBFFC5DFFA1FF9AFDECD62D70AD3F67" box="[572,729,1547,1573]" pageId="3" pageNumber="864">Lizard Island</collectingMunicipality>
, 
<collectingRegion id="49A0A8C5FFA1FF9AFD36D62D71073F67" box="[742,883,1547,1573]" country="Australia" name="Queensland" pageId="3" pageNumber="864">Queensland</collectingRegion>
, 
<collectingCountry id="F37326B7FFA1FF9AFCAFD62D719E3F67" box="[895,1002,1547,1573]" name="Australia" pageId="3" pageNumber="864">Australia</collectingCountry>
(
<geoCoordinate id="EE5000E0FFA1FF9AFC2AD62D76F43F67" box="[1018,1152,1547,1573]" degrees="14" direction="south" minutes="41.21" orientation="latitude" pageId="3" pageNumber="864" precision="9" value="-14.686833">14°41.21’S</geoCoordinate>
<geoCoordinate id="EE5000E0FFA1FF9AFB57D62D77553F67" box="[1159,1313,1547,1573]" degrees="145" direction="east" minutes="26.49" orientation="longitude" pageId="3" pageNumber="864" precision="9" value="145.4415">145°26.49’E</geoCoordinate>
)
