</taxonomicName>
, 
<taxonomicName id="4C324D2EFFE9246DFEFABAFC084BFD38" authorityName="Cho &amp; Borowiec" authorityYear="2016" box="[351,566,668,691]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="mausonensis" subGenus="Asiphytodecta">
