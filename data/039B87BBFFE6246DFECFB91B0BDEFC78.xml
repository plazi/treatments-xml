<taxonomicName id="4C324D2EFFE9246DFDCABAFC0929FD38" authority="Cho &amp; Borowiec, 2016" authorityName="Cho &amp; Borowiec" authorityYear="2016" box="[623,852,668,691]" class="Insecta" family="Chrysomelidae" genus="Gonioctena" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="763" phylum="Arthropoda" rank="species" species="truncaticornis" status="sp. nov." subGenus="Asiphytodecta">
<emphasis id="B946EABFFFE9246DFDCABAFC08FFFD38" box="[623,642,668,691]" italics="true" pageId="8" pageNumber="763">G</emphasis>
