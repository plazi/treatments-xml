<collectingRegion id="82421CD9FF82C9731E08FB3CFAEE229C" box="[1085,1337,1189,1214]" country="Pakistan" name="Khyber Pakhtunkhwa" pageId="17" pageNumber="18">Khyber Pakhtunkhwa</collectingRegion>
