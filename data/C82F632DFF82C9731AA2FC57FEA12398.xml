<taxonomicName id="8786A9B8FF82C97319D6FB1BFB8F22BB" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[995,1112,1153,1177]" class="Insecta" family="Vespidae" genus="Polistes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="gallicus">
