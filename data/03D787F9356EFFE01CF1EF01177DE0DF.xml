<taxonomicName id="4C7E4D6C356EFFE01F39ECC31099E0FB" box="[863,928,690,715]" class="Gastropoda" family="Lymnaeidae" genus="Radix" kingdom="Animalia" order="Hygrophila" pageId="13" pageNumber="58" phylum="Mollusca" rank="genus">
<emphasis id="B90AEAFD356EFFE01F39ECC31099E0FB" box="[863,928,690,715]" italics="true" pageId="13" pageNumber="58">Radix</emphasis>
