<taxonomicName id="7ED84D58FFE2771CBD0BA08E9D4188EF" box="[812,1071,685,710]" class="Trematoda" genus="Monostomum" higherTaxonomySource="CoL" kingdom="Animalia" pageId="14" pageNumber="15" phylum="Platyhelminthes" rank="species" species="mutabile">
