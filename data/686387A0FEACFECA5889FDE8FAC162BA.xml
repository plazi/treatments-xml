<taxonomicName id="27CA4D35FEACFECA5A9BFCECFC266202" box="[645,917,865,891]" class="Insecta" family="Tachinidae" genus="Schembria" kingdom="Animalia" order="Diptera" pageId="353" pageNumber="354" phylum="Arthropoda" rank="species" species="meridionalis">
