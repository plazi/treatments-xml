<quantity id="0F1C9B0A9A1BFFE7FCF1837160E0E046" box="[844,932,1086,1111]" metricMagnitude="1" metricUnit="m" metricValue="3.8" metricValueMax="6.7" metricValueMin="0.9" pageId="13" pageNumber="14" unit="m" value="38.0" valueMax="67.0" valueMin="9.0">
