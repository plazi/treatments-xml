<taxonomicName id="91D74D133F2CFF87FC0CFF6F813BFF2C" box="[953,1035,225,248]" class="Bivalvia" family="Mactridae" genus="Mactra" kingdom="Animalia" order="Veneroida" pageId="5" pageNumber="52" phylum="Mollusca" rank="genus">
