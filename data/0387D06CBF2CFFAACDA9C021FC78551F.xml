<quantity id="4CD6CC9FBF2CFFA9CC1EC076FEE353B7" box="[288,326,1896,1922]" metricMagnitude="4" metricUnit="kg" metricValue="2.0" pageId="33" pageNumber="34" unit="t" value="20.0">20t</quantity>
