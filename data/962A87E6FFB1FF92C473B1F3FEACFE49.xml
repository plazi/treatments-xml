<paragraph id="1E3C36F0FFB0FF95C443B25AFBEAFAE5" blockId="3.[264,1040,1296,1362]" box="[312,1040,1336,1362]" pageId="3" pageNumber="66">
From “ 
<taxonomicName id="D9834D73FFB0FF95C4F7B25AFE0BFAE6" box="[396,497,1336,1361]" class="Insecta" family="Carabidae" genus="Scaurotrechodes" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="66" phylum="Arthropoda" rank="species" species="capensis">
