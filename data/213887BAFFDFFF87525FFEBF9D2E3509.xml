<number id="1F59248BFFDFFF875703FD049DCD31F9" box="[1108,1229,679,705]" pageId="11" pageNumber="12" value="0.13" valueMax="0.14" valueMin="0.12">0.12–0.14</number>
; 
<collectionCode id="CF80AE69FFDFFF87578EFD049C2631F9" box="[1241,1318,679,705]" pageId="11" pageNumber="12">AWIV</collectionCode>
, 
<number id="1F59248BFFDFFF87525FFD6C988031D1" box="[264,384,719,745]" pageId="11" pageNumber="12" value="0.54" valueMax="0.57" valueMin="0.51">0.51–0.57</number>
