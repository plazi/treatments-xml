</date>
; 
<collectorName id="4850A3A4FFBCFF8F0446FEE7FBE2FED9" box="[891,1103,259,285]" pageId="15" pageNumber="16">M. Claassen. Host</collectorName>
:? 
<taxonomicName id="22A5BDF1FFBCFF8F0355FEE7FAA0FED9" authorityName="Pocock" authorityYear="1901" box="[1128,1293,259,285]" class="Arachnida" family="Theraphosidae" genus="Pamphobeteus" kingdom="Animalia" order="Araneae" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
