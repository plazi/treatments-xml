<materialsCitation id="55CDCC2FFFBCFF8F07AAFEE7FB90FEA1" ID-GBIF-Occurrence="2990869303" collectingDate="2015-02-05" collectorName="M. Claassen. Host &amp; The" country="Colombia" location="Chachag" pageId="15" pageNumber="16" specimenCount="1" stateProvince="Narino">
