, 
<location id="E07A90A9FFBCFF8F051AFEE0FD3AFEDB" LSID="urn:lsid:plazi:treatment:6D0C7764FFBDFF8F07AAF90FFBEFFEA1:E07A90A9FFBCFF8F051AFEE0FD3AFEDB" box="[551,663,260,287]" country="Colombia" name="Chachag" pageId="15" pageNumber="16" stateProvince="Narino">Chachag</location>
̹í; 
