<quantity id="4CF79B33FFCFFFE6C1E2FD01FC47FD00" box="[923,977,728,748]" metricMagnitude="2" metricUnit="m" metricValue="9.85" pageId="32" pageNumber="187" unit="m" value="985.0">985m</quantity>
