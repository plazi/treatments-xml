<collectionCode id="0784B2BA467B5D775CF9FED6FE117A2D" box="[440,492,346,365]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34981" lsid="urn:lsid:biocol.org:col:34981" name="Western Australian Museum" pageId="27" pageNumber="27" type="Museum">WAM</collectionCode>
