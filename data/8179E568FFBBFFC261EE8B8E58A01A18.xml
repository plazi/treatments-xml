<paragraph id="096F547EFFBBFFC361EE8A7659DE1C44" blockId="0.[151,714,1829,1851]" box="[151,714,1829,1851]" pageId="0" pageNumber="441">
<typeStatus id="D66BEADCFFBBFFC361EE8A765BDD1C44" box="[151,201,1830,1851]" pageId="0" pageNumber="441">Type</typeStatus>
species: 
<taxonomicName id="CED02FFDFFBBFFC3605F8A7559D21C44" authority="Berlese, 1886: 19" authorityName="Berlese" authorityPageNumber="19" authorityYear="1886" box="[294,710,1829,1851]" class="Arachnida" family="Camerobiidae" genus="Neophyllobius" kingdom="Animalia" order="Prostigmata" pageId="0" pageNumber="441" phylum="Arthropoda" rank="species" species="elegans">
<emphasis id="3BA4886CFFBBFFC3605F8A75591F1C44" box="[294,523,1829,1851]" italics="true" pageId="0" pageNumber="441">Neophyllobius elegans</emphasis>
<bibRefCitation id="6D41298FFFBBFFC3636B8A7559B51C44" author="Berlese" box="[530,673,1829,1851]" pageId="0" pageNumber="441" refString="Berlese, A. (1886) Acari dannosi alle piante coltivate. Padova, 1 - 31." type="book chapter" year="1886">
Berlese, 
<number id="BF184659FFBBFFC363128A7559B51C44" box="[619,673,1829,1851]" pageId="0" pageNumber="441" value="1886.0">1886</number>
</bibRefCitation>
: 
<number id="BF184659FFBBFFC363D38A7559D21C44" box="[682,710,1829,1851]" pageId="0" pageNumber="441" value="19.0">19</number>
