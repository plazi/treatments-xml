</taxonomicName>
<taxonomicNameLabel id="3AD257BEFF86FFA9D032FB2AFBF1FA8F" box="[1027,1092,1265,1290]" pageId="13" pageNumber="570" rank="species">
<emphasis id="21E1EAC5FF86FFA9D032FB2AFBF1FA8F" bold="true" box="[1027,1092,1265,1290]" pageId="13" pageNumber="570">sp. n.</emphasis>
</taxonomicNameLabel>
, 
<taxonomicName id="D4954D54FF86FFA9D062FB29FB4CFA8F" authority="Laurent &amp; Mccabe, 2016" authorityName="Laurent &amp; Mccabe" authorityYear="2016" box="[1107,1273,1265,1290]" class="Insecta" family="Mimallonidae" genus="Cicinnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="570" phylum="Arthropoda" rank="species" species="bahamensis" status="sp. n.">
