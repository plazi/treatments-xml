<subSubSection id="4F92654BFFDCC25E7CAAFAFB60A3FAA1" box="[151,648,1382,1407]" pageId="43" pageNumber="44" type="materials_examined">
<paragraph id="073736C0FFDCC25E7CAAFAFB60A3FAA1" blockId="43.[151,648,1382,1407]" box="[151,648,1382,1407]" pageId="43" pageNumber="44">
<typeStatus id="D8338862FFDCC25E7CAAFAFB62E2FAA1" box="[151,201,1383,1407]" pageId="43" pageNumber="44">Type</typeStatus>
species: 
<taxonomicName id="C0884D43FFDCC25E7D1BFAFB60AFFAA1" authority="Aube, 1850" authorityName="Aube" authorityYear="1850" box="[294,644,1382,1407]" class="Insecta" family="Staphylinidae" genus="Omalium" kingdom="Animalia" order="Coleoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="striatipenne">
<emphasis id="35FCEAD2FFDCC25E7D1BFAFB602FFAA0" box="[294,516,1383,1407]" italics="true" pageId="43" pageNumber="44">Omalium striatipenne</emphasis>
<bibRefCitation id="63194B31FFDCC25E7E37FAFA60AFFAA1" author="Aube, C." box="[522,644,1382,1407]" pageId="43" pageNumber="44" pagination="299 - 346" refId="ref39660" refString="Aube, C. (1850) Description de quelques insectes coleopteres appartenant a l'Europe et a l'Algerie. Annales de la Societe Entomologique de France, Series 2, 8, 299 - 346." type="journal article" year="1850">Aubé, 1850</bibRefCitation>
</taxonomicName>
