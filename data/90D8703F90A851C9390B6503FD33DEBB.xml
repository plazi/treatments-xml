<taxonomicName id="15A6A877A9F49E40D57247279F459080" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230667" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName="Camponotus variegatus (Smith)" lsidName-HNS="Camponotus variegatus (Smith)" order="Hymenoptera" pageNumber="14" phylum="Arthropoda" rank="variety" species="mitis" variety="variegatus">mitis, var. variegatus</taxonomicName>
