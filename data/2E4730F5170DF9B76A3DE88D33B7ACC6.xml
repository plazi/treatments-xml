<bibRefCitation id="24D3284A9F178C89F8B208E67361AA2D" author="Norenburg, JL" editor="Tunnell, JW" journalOrPublisher="Texas A and M University Press" pageId="20" pageNumber="103" pagination="553 - 560" title="Nemertean of the Gulf of Mexico." volumeTitle="Gulf of Mexico origin, water and biota" year="2009">Norenburg 2009</bibRefCitation>
); 
<normalizedToken id="8DFA3A8B3189C8D68C11B048EAA0FC65" originalValue="Curaçao">Curacao</normalizedToken>
(
<bibRefCitation id="C90545AED5A720A2B95E0606707E0900" author="Schwartz, ML" journalOrPublisher="Caribbean Journal of Science" pageId="20" pageNumber="103" pagination="528 - 543" title="Three New Species of Micrura (Nemertea: Heteronemertea) and a New Type of Heteronemertean Larva from the Caribbean Sea." volume="41" year="2005">Schwartz and Norenburg 2005</bibRefCitation>
