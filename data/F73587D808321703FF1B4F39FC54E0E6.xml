<number id="C95424E908321703FEDE4D30FE5EE11E" box="[338,391,1590,1614]" pageId="7" pageNumber="52" value="0.18">0.18</number>
; OI 22, 21; SL 
<number id="C95424E908321703FDBC4D30FDBDE11E" box="[560,612,1590,1614]" pageId="7" pageNumber="52" value="0.62">0.62</number>
