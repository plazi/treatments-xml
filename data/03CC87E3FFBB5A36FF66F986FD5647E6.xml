<taxonomicName id="4C654D76FFB95A37FF66F814FEAE42C7" authorityName="Andrade" authorityYear="1953" box="[151,353,2007,2028]" class="Insecta" family="Crabronidae" genus="Miscophus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="344" phylum="Arthropoda" rank="species" species="insolitus">
<emphasis id="B911EAE7FFB95A37FF66F814FEAE42C7" box="[151,353,2007,2028]" italics="true" pageId="10" pageNumber="344">Miscophus insolitus</emphasis>
