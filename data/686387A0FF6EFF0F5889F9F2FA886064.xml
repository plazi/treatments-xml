<taxonomicName id="27CA4D35FF6EFF085BEFF8BEFB176634" baseAuthorityName="Evenhuis &amp; Pape" baseAuthorityYear="2020" box="[1009,1188,1843,1869]" class="Insecta" family="Mycetophilidae" kingdom="Animalia" order="Diptera" pageId="163" pageNumber="164" phylum="Arthropoda" rank="family">Mycetophilidae</taxonomicName>
