<taxonomicName id="8FEA4D01D87DFF90FD8970DEFD68F8E7" authority="Shen &amp; Leschen, 2020" authorityName="Shen &amp; Leschen" authorityYear="2020" box="[543,716,1749,1773]" class="Insecta" family="Staphylinidae" genus="Eupines" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="10" phylum="Arthropoda" rank="species" species="pannicula" status="sp. n." subGenus="Byraxis">
<emphasis id="7A9EEA90D87DFF90FD8970DEFD8AF8E7" box="[543,558,1750,1773]" italics="true" pageId="63" pageNumber="64">E</emphasis>
