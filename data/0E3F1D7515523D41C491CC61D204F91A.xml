<subSubSection id="CE8CFFE815523D4EC4C3CF02D64BF94D" pageId="7" pageNumber="24" type="diagnosis">
<paragraph id="8629AC6315523D4EC4C3CF02D64BF94D" blockId="7.[151,1436,1466,2032]" pageId="7" pageNumber="24">
<emphasis id="B4E2707115523D4EC4C3CF02D26AF905" bold="true" box="[197,319,1646,1671]" pageId="7" pageNumber="24">Diagnosis.</emphasis>
