: 
<quantity id="4CC09B2BFFD7BB00FC93805AFC1FFBD4" box="[796,898,1040,1067]" metricMagnitude="-3" metricUnit="m" metricValue="1.63" pageId="17" pageNumber="168" unit="mm" value="1.63">1.63 mm</quantity>
