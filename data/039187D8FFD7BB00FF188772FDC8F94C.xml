<quantity id="4CC09B2BFFD7BB00FEF28716FE3EFC88" box="[381,419,860,887]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="17" pageNumber="168" unit="m" value="5.0">5m</quantity>
</figureCitation>
