<taxonomicName id="4C3951F407147B16C0D5FA66FDEA14E5" authorityName="Meyrick" authorityYear="1911" box="[431,528,1456,1479]" class="Insecta" family="Tortricidae" genus="Herpystis" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="384" phylum="Arthropoda" rank="genus">
