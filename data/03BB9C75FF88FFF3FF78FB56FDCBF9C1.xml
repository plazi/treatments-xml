<taxonomicName id="4C1256E0FF88FFF3FF19FB56FEC7FB4A" authorityName="Samouelle" authorityYear="1819" box="[246,382,1191,1217]" class="Malacostraca" family="Corystidae" kingdom="Animalia" order="Decapoda" pageId="97" pageNumber="98" phylum="Arthropoda" rank="family">Corystidae</taxonomicName>
