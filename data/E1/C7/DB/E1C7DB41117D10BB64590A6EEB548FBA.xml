, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:189143" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole vinelandica Forel" lsidName-HNS="Pheidole vinelandica Forel" order="Hymenoptera" pageNumber="560" phylum="Arthropoda" rank="species" species="vinelandica">vinelandica</taxonomicName>
, 
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34107" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole xerophila Wheeler" lsidName-HNS="Pheidole xerophila Wheeler" order="Hymenoptera" pageNumber="560" phylum="Arthropoda" rank="species" species="xerophila">xerophila</taxonomicName>
