<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33658" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole cerebrosior Wheeler" lsidName-HNS="Pheidole cerebrosior Wheeler" order="Hymenoptera" pageNumber="560" phylum="Arthropoda" rank="species" species="cerebrosior">cerebrosior</taxonomicName>
