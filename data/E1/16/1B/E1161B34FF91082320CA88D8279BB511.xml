<taxonomicNameLabel box="[1259,1352,1688,1712]" pageId="1" pageNumber="29" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName class="Insecta" family="Philopteridae" genus="Bizarrifrons" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="1" pageNumber="29" phylum="Arthropoda" rank="species" species="wecksteini" status="sp. nov.">
