<emphasis box="[1022,1118,1148,1172]" italics="true" pageId="1" pageNumber="29">Brueelia</emphasis>
KÃ©ler, 
<number box="[1214,1273,1148,1172]" pageId="1" pageNumber="29" value="1936.0">1936</number>
</taxonomicName>
; 
<taxonomicName authority="Eichler, 1944" authorityName="Eichler" authorityYear="1944" class="Insecta" family="Philopteridae" genus="Sturnidoecus" kingdom="Animalia" order="Phthiraptera" pageId="1" pageNumber="29" phylum="Arthropoda" rank="genus">
