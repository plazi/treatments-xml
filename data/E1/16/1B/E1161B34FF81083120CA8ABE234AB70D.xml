<number box="[151,266,1124,1148]" pageId="19" pageNumber="47" value="0.52" valueMax="0.54" valueMin="0.5">0.50–0.54</number>
; HL, 
<number box="[329,445,1124,1148]" pageId="19" pageNumber="47" value="0.5449999999999999" valueMax="0.57" valueMin="0.52">0.52–0.57</number>
; DPW, 
<number box="[529,644,1124,1148]" pageId="19" pageNumber="47" value="0.10500000000000001" valueMax="0.11" valueMin="0.1">0.10–0.11</number>
; PW, 
<number box="[709,825,1124,1148]" pageId="19" pageNumber="47" value="0.315" valueMax="0.33" valueMin="0.3">0.30–0.33</number>
; MW, 
<number box="[898,1013,1124,1148]" pageId="19" pageNumber="47" value="0.46499999999999997" valueMax="0.48" valueMin="0.45">0.45–0.48</number>
; AWV, 
<number box="[1100,1215,1124,1148]" pageId="19" pageNumber="47" value="0.6699999999999999" valueMax="0.71" valueMin="0.63">0.63–0.71</number>
; TL, 
<number box="[1275,1390,1124,1148]" pageId="19" pageNumber="47" value="1.775" valueMax="1.89" valueMin="1.66">1.66–1.89</number>
.
</paragraph>
