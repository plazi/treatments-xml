<paragraph blockId="10.[151,1436,1243,1449]" pageId="10" pageNumber="37">
<emphasis bold="true" box="[199,538,1243,1268]" inLineHeading="true" pageId="10" pageNumber="37" reason="1">
Condition of the 
<typeStatus box="[404,455,1244,1268]" pageId="10" pageNumber="37">type</typeStatus>
