<taxonomicName authority="Velez" authorityName="Velez" box="[151,392,403,430]" class="Insecta" family="Ectobiidae" genus="Nyctantonina" kingdom="Animalia" order="Blattodea" pageId="1" pageNumber="80" phylum="Arthropoda" rank="genus">
