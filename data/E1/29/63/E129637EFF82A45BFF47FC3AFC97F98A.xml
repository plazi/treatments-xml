<paragraph blockId="24.[151,1437,1195,1655]" pageId="24" pageNumber="25">
<materialsCitation ID-GBIF-Occurrence="2366496625" country="India" location="Kochi" pageId="24" pageNumber="25" specimenCount="4" typeStatus="Type and type">
<emphasis bold="true" box="[151,419,1195,1222]" pageId="24" pageNumber="25">
<typeStatus box="[151,210,1196,1222]" pageId="24" pageNumber="25">Type</typeStatus>
