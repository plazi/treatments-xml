<taxonomicName baseAuthorityName="Valenciennes" baseAuthorityYear="1847" box="[915,1176,585,611]" class="Actinopterygii" family="Clupeidae" genus="Amblygaster" kingdom="Animalia" order="Clupeiformes" pageId="77" pageNumber="78" phylum="Chordata" rank="species" species="leiogaster">
<emphasis box="[915,1176,585,611]" italics="true" pageId="77" pageNumber="78">Amblygaster leiogaster</emphasis>
