) (
<bibRefCitation author="Lesueur, C. A." box="[537,700,1870,1896]" pageId="77" pageNumber="78" pagination="45 - 46" refId="ref75015" refString="Lesueur, C. A. (1814) Sur une nouvelle espece d'insecte du genre Cymothoa de Fabricius. In: Bulletin des Sciences pars la Societe Philomathique de Paris. Annee 1814. Imprimerie de Plassan, Paris, pp. 45 - 46, pl. II." type="book chapter" year="1814">Lesueur, 1814</bibRefCitation>
; 
<bibRefCitation author="Poore, G. C. B." box="[712,853,1870,1897]" pageId="77" pageNumber="78" refId="ref76639" refString="Poore, G. C. B. (2002) Crustacea: Malacostraca: Syncarida: Peracarida: Isopoda, Tanaidacea, Mictacea, Thermosbaenacea, Spelaeogriphacea. Zoological Catalogue of Australia. Vol. 19.2 A. CSIRO Publishing, Melbourne, 434 pp." type="book" year="2002">Poore, 2002</bibRefCitation>
