</bibRefCitation>
) and 
<taxonomicName authority="strongylura (van Hasselt, 1823) (Rameshkumar et al. 2014 d)" authorityName="van Hasselt" authorityYear="1823" baseAuthorityName="van Hasselt" baseAuthorityYear="1823" box="[272,1100,1762,1789]" class="Actinopterygii" family="Belonidae" genus="Strongylura" kingdom="Animalia" order="Beloniformes" pageId="37" pageNumber="38" phylum="Chordata" rank="species" species="strongylura">
