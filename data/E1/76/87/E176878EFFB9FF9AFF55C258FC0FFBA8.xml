<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[282,348,876,897]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" pageId="4" pageNumber="1199" phylum="Arthropoda" rank="class">Insecta</taxonomicName>
