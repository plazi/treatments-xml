<collectingCountry box="[641,717,1509,1531]" name="Guinea-Bissau" pageId="4" pageNumber="1199">Guinea</collectingCountry>
, 
<collectingRegion box="[732,774,1509,1531]" country="Indonesia" name="Bali" pageId="4" pageNumber="1199">Bali</collectingRegion>
(
<collectingCountry box="[793,897,1509,1531]" name="Indonesia" pageId="4" pageNumber="1199">Indonesia</collectingCountry>
), 
<collectingCountry box="[920,989,1510,1531]" name="Macao" pageId="4" pageNumber="1199">Macao</collectingCountry>
(
<collectingCountry box="[1008,1073,1509,1531]" name="China" pageId="4" pageNumber="1199">China</collectingCountry>
), 
<collectingCountry box="[178,300,1539,1561]" name="Madagascar" pageId="4" pageNumber="1199">Madagascar</collectingCountry>
and 
<collectingCountry box="[352,439,1539,1561]" name="Vietnam" pageId="4" pageNumber="1199">Vietnam</collectingCountry>
(
<bibRefCitation author="SOUTHCOTT R" box="[455,637,1539,1560]" pageId="4" pageNumber="1199" pagination="252 - 264" refId="ref8262" refString="SOUTHCOTT R. V. (1948): Larval Smarididae (Acarina) from Australia and New Guinea. - Proc. Linn. Soc. N, S, W 72: 252 - 264." type="journal article" year="1948">SOUTHCOTT 1948</bibRefCitation>
