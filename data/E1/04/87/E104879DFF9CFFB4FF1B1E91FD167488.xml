<collectingCountry box="[359,430,476,502]" name="China" pageId="4" pageNumber="244">China</collectingCountry>
: 
<collectingRegion box="[441,534,476,502]" country="China" name="Sichuan" pageId="4" pageNumber="244">Sichuan</collectingRegion>
, 
