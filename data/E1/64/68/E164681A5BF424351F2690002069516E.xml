<paragraph pageId="681" pageNumber="682">
<taxonomicName authorityName="Casey" authorityYear="1914" class="Insecta" family="Carabidae" genus="Tachistodes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="681" pageNumber="682" phylum="Arthropoda" rank="species" species="fusciceps">Tachistodes fusciceps</taxonomicName>
