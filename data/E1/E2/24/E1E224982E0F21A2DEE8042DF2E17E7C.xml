<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33662" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole chalca Wheeler" lsidName-HNS="Pheidole chalca Wheeler" order="Hymenoptera" pageNumber="526" phylum="Arthropoda" rank="species" species="chalca">chalca</taxonomicName>
