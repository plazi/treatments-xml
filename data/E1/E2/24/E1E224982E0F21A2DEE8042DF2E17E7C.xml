<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181854" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole caulicola Wilson" lsidName-HNS="Pheidole caulicola Wilson" order="Hymenoptera" pageNumber="526" phylum="Arthropoda" rank="species" species="caulicola">caulicola</taxonomicName>
