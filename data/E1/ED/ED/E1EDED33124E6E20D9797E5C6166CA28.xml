<taxonomicName class="Dicotyledoneae" family="Tiliaceae" genus="Tilia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Malvales" pageId="25" pageNumber="38" phylum="Angiospermae" rank="genus">
