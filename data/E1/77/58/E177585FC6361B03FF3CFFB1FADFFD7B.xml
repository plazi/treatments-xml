<emphasis box="[151,375,366,390]" italics="true" pageId="48" pageNumber="49">
<collectingRegion box="[151,375,366,390]" country="Iran" name="Khuzestan" pageId="48" pageNumber="49">Khuzestan Province</collectingRegion>
