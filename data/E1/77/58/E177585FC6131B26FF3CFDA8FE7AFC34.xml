<taxonomicName authorityName="Sankolli" authorityYear="1965" box="[617,751,869,893]" class="Malacostraca" family="Porcellanidae" genus="Polyonyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="loimicola">
