<collectingCountry box="[777,884,972,996]" name="Tanzania" pageId="22" pageNumber="23">Tanzania</collectingCountry>
), Red Sea, Gulf of 
<collectingRegion box="[1108,1171,972,996]" country="Yemen" name="'Adan" pageId="22" pageNumber="23">Aden</collectingRegion>
, 
<collectingCountry box="[1184,1272,972,996]" name="Yemen" pageId="22" pageNumber="23">Socotra</collectingCountry>
