<collectingCountry box="[239,310,1007,1031]" name="Oman" pageId="22" pageNumber="23">Oman</collectingCountry>
, 
<collectingCountry box="[322,420,1007,1031]" name="Pakistan" pageId="22" pageNumber="23">Pakistan</collectingCountry>
