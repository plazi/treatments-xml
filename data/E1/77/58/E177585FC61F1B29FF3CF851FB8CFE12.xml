<collectingCountry box="[1181,1279,188,212]" name="Pakistan" pageId="26" pageNumber="27">Pakistan</collectingCountry>
, 
<collectingCountry box="[1297,1357,188,212]" name="India" pageId="26" pageNumber="27">India</collectingCountry>
, Indo- Malayan Archipelago, 
<collectingCountry box="[408,513,223,247]" name="Malaysia" pageId="26" pageNumber="27">Malaysia</collectingCountry>
, 
<collectingCountry box="[525,637,223,247]" name="Indonesia" pageId="26" pageNumber="27">Indonesia</collectingCountry>
.
</paragraph>
<paragraph blockId="26.[151,1436,151,464]" box="[199,576,259,284]" pageId="26" pageNumber="27">
