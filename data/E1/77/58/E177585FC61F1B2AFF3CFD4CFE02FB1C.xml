<collectingRegion box="[1162,1225,869,893]" country="Yemen" name="'Adan" pageId="25" pageNumber="26">Aden</collectingRegion>
, 
<collectingCountry box="[1238,1326,869,893]" name="Yemen" pageId="25" pageNumber="26">Socotra</collectingCountry>
, southern 
<collectingCountry box="[151,219,905,929]" name="Oman" pageId="25" pageNumber="26">Oman</collectingCountry>
(
<collectingRegion box="[240,325,905,929]" country="Oman" name="Zufar" pageId="25" pageNumber="26">Dhofar</collectingRegion>
), Persian Gulf, Gulf of 
<collectingCountry box="[617,688,905,929]" name="Oman" pageId="25" pageNumber="26">Oman</collectingCountry>
, 
<collectingCountry box="[705,803,905,929]" name="Pakistan" pageId="25" pageNumber="26">Pakistan</collectingCountry>
, 
<collectingCountry box="[820,880,905,929]" name="India" pageId="25" pageNumber="26">India</collectingCountry>
, Mergui-Archipelago, Andaman Sea, 
<collectingCountry box="[1330,1432,905,929]" name="Thailand" pageId="25" pageNumber="26">Thailand</collectingCountry>
, 
<collectingCountry box="[151,249,941,965]" name="Vietnam" pageId="25" pageNumber="26">Vietnam</collectingCountry>
, 
<collectingCountry box="[259,388,941,965]" name="Philippines" pageId="25" pageNumber="26">Philippines</collectingCountry>
, 
<collectingCountry box="[399,515,941,965]" name="Singapore" pageId="25" pageNumber="26">Singapore</collectingCountry>
, 
<collectingCountry box="[527,639,941,965]" name="Indonesia" pageId="25" pageNumber="26">Indonesia</collectingCountry>
, 
<collectingCountry box="[650,753,941,965]" name="Australia" pageId="25" pageNumber="26">Australia</collectingCountry>
(
