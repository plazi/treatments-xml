<paragraph blockId="43.[151,1436,1038,1314]" pageId="43" pageNumber="44">
<emphasis bold="true" box="[199,451,1110,1135]" inLineHeading="true" pageId="43" pageNumber="44" reason="1">General distribution.</emphasis>
