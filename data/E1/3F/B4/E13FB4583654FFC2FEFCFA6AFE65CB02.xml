<taxonomicName box="[414,609,1822,1845]" class="Insecta" family="Perlodidae" genus="Isoperla" higherTaxonomySource="CoL" kingdom="Animalia" order="Plecoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="maculata">
<emphasis box="[414,609,1822,1845]" italics="true" pageId="17" pageNumber="18">
<taxonomicName box="[414,503,1822,1845]" class="Insecta" family="Perlodidae" genus="Isoperla" higherTaxonomySource="CoL" kingdom="Animalia" order="Plecoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">Isoperla</taxonomicName>
