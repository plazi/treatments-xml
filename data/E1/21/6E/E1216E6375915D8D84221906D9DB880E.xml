<subSubSection pageId="0" pageNumber="89" type="nomenclature">
<paragraph pageId="0" pageNumber="89">
7. 
<taxonomicName LSID="E1216E63-7591-5D8D-8422-1906D9DB880E" authority="Chaudhri, Akbar &amp; Rasool, 1974" authorityName="Chaudhri, Akbar &amp; Rasool" authorityYear="1974" class="Arachnida" family="Tetranychidae" genus="Oligonychus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oligonychus longus" order="Trombidiformes" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="longus">Oligonychus longus Chaudhri, Akbar &amp; Rasool, 1974</taxonomicName>
</paragraph>
</subSubSection>
<subSubSection pageId="0" pageNumber="89" type="reference_group">
<paragraph pageId="0" pageNumber="89">
<taxonomicName authorityName="Chaudhri, Akbar &amp; Rasool" authorityYear="1974" class="Arachnida" family="Tetranychidae" genus="Oligonychus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oligonychus longus" order="Trombidiformes" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="longus">Oligonychus longus</taxonomicName>
