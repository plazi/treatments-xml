<subSubSection pageId="58" pageNumber="59" type="nomenclature">
<paragraph pageId="58" pageNumber="59">
<taxonomicName LSID="http://zoobank.org/A1425B06-B0E0-4412-BB77-EA50BDD80DED" authority="Staines" class="Insecta" family="Chrysomelidae" genus="Cephaloleia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cephaloleia angustacollis" order="Coleoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="angustacollis">Cephaloleia angustacollis Staines</taxonomicName>
