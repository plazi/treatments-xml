ex 
<taxonomicName authority="crispifrons (Blyth, 1855)" authorityName="crispifrons (Blyth" authorityYear="1855" box="[804,1282,1952,1974]" class="Aves" family="Timaliidae" genus="Turdinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="4" pageNumber="405" phylum="Chordata" rank="subSpecies" species="crispifrons" subSpecies="crispifrons">
