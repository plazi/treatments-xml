<quantity id="E84E9B45E1417676FF0F7221FEDFFABC" box="[227,308,1358,1379]" metricMagnitude="-3" metricUnit="m" metricValue="4.1" pageId="17" pageNumber="602" unit="mm" value="4.1">4.1 mm</quantity>
); width 
<quantity id="E84E9B45E1417676FE6A7222FE14FABC" box="[390,511,1357,1379]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" metricValueMax="1.7" metricValueMin="1.5" pageId="17" pageNumber="602" unit="mm" value="1.6" valueMax="1.7" valueMin="1.5">1.5-1.7 mm</quantity>
