<taxonomicName id="4C2C71FEFFC19F2BFF45FD63FE6CAF6C" authorityName="Roxb., Flora Indica" authorityYear="1832" box="[189,417,1062,1089]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="18" pageNumber="19" phylum="Tracheophyta" rank="species" species="gowreephul">
<emphasis id="B958D66FFFC19F2BFF45FD63FE6CAF6C" bold="true" box="[189,417,1062,1089]" italics="true" pageId="18" pageNumber="19">Rubus gowreephul</emphasis>
