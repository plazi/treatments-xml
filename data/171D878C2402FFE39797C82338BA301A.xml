<taxonomicName id="58B44D192402FFE3957BC8BC3F51339F" authority="(KLAUSNITZER 1980)" authorityName="Klausnitzer" authorityYear="2012" baseAuthorityName="KLAUSNITZER" baseAuthorityYear="1980" box="[606,1085,1422,1444]" class="Insecta" family="Scirtidae" genus="Prionocyphon" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="1186" phylum="Arthropoda" rank="species" species="bromelius" status="nov.comb.">
