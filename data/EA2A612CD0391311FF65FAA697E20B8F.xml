<taxonomicName id="A583ABB9D03A1311FD59FC42967C0B59" box="[683,773,1008,1033]" class="Insecta" family="Ephydridae" genus="Scatella" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
<emphasis id="50F70C28D03A1311FD59FC42967C0B59" box="[683,773,1008,1033]" italics="true" pageId="2" pageNumber="303">Scatella</emphasis>
