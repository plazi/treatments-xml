</taxonomicName>
, 
<taxonomicName id="A583ABB9D03A1311FCDCFC7F96C40CB6" box="[814,957,973,998]" class="Insecta" family="Ephydridae" genus="Haloscatella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
<emphasis id="50F70C28D03A1311FCDCFC7F96C40CB6" box="[814,957,973,998]" italics="true" pageId="2" pageNumber="303">Haloscatella</emphasis>
