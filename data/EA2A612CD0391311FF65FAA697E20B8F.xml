</taxonomicName>
, 
<taxonomicName id="A583ABB9D03A1311FB6BFC7F907D0CB6" box="[1177,1284,973,998]" class="Insecta" family="Ephydridae" genus="Limnellia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
<emphasis id="50F70C28D03A1311FB6BFC7F907D0CB6" box="[1177,1284,973,998]" italics="true" pageId="2" pageNumber="303">Limnellia</emphasis>
</taxonomicName>
, 
<taxonomicName id="A583ABB9D03A1311FAE8FC7F90EC0CB6" box="[1306,1429,973,998]" class="Insecta" family="Ephydridae" genus="Philotelma" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
<emphasis id="50F70C28D03A1311FAE8FC7F90EC0CB6" box="[1306,1429,973,998]" italics="true" pageId="2" pageNumber="303">Philotelma</emphasis>
