</taxonomicName>
, 
<taxonomicName id="A583ABB9D03A1311FC6DFC4291590B59" authority="Macquart, 1835" authorityName="Macquart" authorityYear="1835" box="[927,1056,1008,1033]" class="Insecta" family="Ephydridae" genus="Teichomyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="genus">
