<collectionCode id="ED3500B1FF971B1D5DC6FA75FBC3FA0A" box="[938,1033,1504,1530]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="12" pageNumber="49">AMNH</collectionCode>
