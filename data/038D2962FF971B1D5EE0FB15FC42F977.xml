<collectingRegion id="49E05696FF971B1D5E80F9F8FE9DF977" box="[236,343,1645,1671]" country="United States of America" name="Colorado" pageId="12" pageNumber="49">Colorado</collectingRegion>
[
<collectingCountry id="F333D8E4FF971B1D5F09F9F9FE6BF977" box="[357,417,1644,1671]" name="United States of America" pageId="12" pageNumber="49">USA</collectingCountry>
