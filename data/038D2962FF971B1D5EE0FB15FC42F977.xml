<taxonomicName id="4C24E3F7FF971B1D5F1CF9DFFE1FF994" box="[368,469,1610,1636]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="49" phylum="Arthropoda" rank="genus">Catocala</taxonomicName>
