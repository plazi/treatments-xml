<taxonomicName id="24F550E9475AFFDCC54AE1F0FD5FF8D0" box="[586,650,1772,1795]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="11" pageNumber="412" phylum="Tracheophyta" rank="genus">
