<subSubSection id="C312BF983732F4642D82FC7AFF2BFC32" pageId="24" pageNumber="25" type="etymology">
<paragraph id="8BB7EC133732F4642D82FC7AFF2BFC32" blockId="24.[151,1437,152,1002]" pageId="24" pageNumber="25">
<emphasis id="B97C30013732F4642D82FC7AFE86FC69" bold="true" box="[196,334,901,927]" pageId="24" pageNumber="25">Etymology</emphasis>
