, 
<taxonomicName id="4C4A4D43FFA4FFD8D838F051E07E0CDF" authority="(Ferussac, 1821)" baseAuthorityName="Ferussac" baseAuthorityYear="1821" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" kingdom="Animalia" order="Stylommatophora" pageId="8" pageNumber="65" phylum="Mollusca" rank="species" species="virgulatus" subGenus="Mesembrinus">
