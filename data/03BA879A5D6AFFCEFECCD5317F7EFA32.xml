<taxonomicName id="4C134D0F5D6AFFCFFC72D21D79EFF9CB" authority="Bates" authorityName="Bates" box="[950,1204,1584,1607]" class="Insecta" family="Carabidae" genus="Aspasiola" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="ignea">
