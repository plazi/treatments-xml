<taxonomicName id="47B2F798FFFBFFC040A6322E01358B7D" box="[666,802,512,536]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="138" phylum="Arthropoda" rank="species" species="deemingi">
