<collectionCode id="ED7EAE6E460E324AFD6CFDD8DDDED3E5" box="[725,746,632,657]" country="Finland" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15618" lsid="urn:lsid:biocol.org:col:15618" name="University of Helsinki" pageId="19" pageNumber="20" type="Herbarium">H</collectionCode>
</emphasis>
. 
<emphasis id="B91BEAB9460E324AFD40FDD8DC57D3E5" box="[761,867,632,657]" italics="true" pageId="19" pageNumber="20">pygmaea</emphasis>
</taxonomicName>
(type species of 
<taxonomicName id="4C6F4D28460E324AFB95FDD8DBBDD3E5" authorityName="Burmeister" authorityYear="1855" box="[1068,1161,632,657]" class="Insecta" family="Coleoptera (awaiting allocation)" genus="Hercitis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">
<emphasis id="B91BEAB9460E324AFB95FDD8DBBDD3E5" box="[1068,1161,632,657]" italics="true" pageId="19" pageNumber="20">Hercitis</emphasis>
</taxonomicName>
) with 
<taxonomicName id="4C6F4D28460E324AFB6EFDD8DA07D3E5" authorityName="Blanchard" authorityYear="1850" box="[1239,1331,632,658]" class="Insecta" family="Scarabaeidae" genus="Barybas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="nana">
