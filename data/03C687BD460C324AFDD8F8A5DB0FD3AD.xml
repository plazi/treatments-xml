<collectionCode id="ED7EAE6E460E324AFE86FD1FDE60D3AC" box="[319,340,703,728]" country="Finland" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15618" name="University of Helsinki" pageId="19" pageNumber="20" type="Herbarium">H</collectionCode>
</emphasis>
