<collectionCode id="ED7EAE6E460E324AFB6EFDD8DBDDD3E5" box="[1239,1257,632,657]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15534" lsid="urn:lsid:biocol.org:col:15534" name="Botanischer Garten und Botanisches Museum Berlin-Dahlem, Zentraleinrichtung der Freien Universitaet" pageId="19" pageNumber="20" type="Herbarium">B</collectionCode>
</emphasis>
