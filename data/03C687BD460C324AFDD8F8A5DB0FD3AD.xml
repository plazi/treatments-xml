of 
<taxonomicName id="4C6F4D28460E324AFE86FD1FDEF9D3AC" authorityName="Burmesiter" authorityYear="1855" box="[319,461,703,729]" class="Insecta" family="Coleoptera (awaiting allocation)" genus="Hercitis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="pygmaea">
