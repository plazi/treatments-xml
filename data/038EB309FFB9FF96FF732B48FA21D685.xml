<caption id="DF585297FFB4FF97FF732CFBFC63D190" httpUri="https://zenodo.org/record/1042950/files/figure.png" pageId="24" pageNumber="225" targetBox="[163,1394,206,1394]" targetPageId="24">
<paragraph id="8B98021FFFB4FF97FF732CFBFC63D190" blockId="24.[151,1436,1457,1509]" pageId="24" pageNumber="225">
<emphasis id="B953DE0DFFB4FF97FF732CFBFE87D1B3" bold="true" box="[151,292,1457,1479]" pageId="24" pageNumber="225">FIGURE 12.</emphasis>
<taxonomicName id="4C27799CFFB4FF97FECB2CFBFD90D1B2" authority="Mironov &amp; Chandler, 2017" authorityName="Mironov &amp; Chandler" authorityYear="2017" box="[303,563,1457,1479]" class="Arachnida" family="Proctophyllodidae" genus="Amerodectes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="24" pageNumber="225" phylum="Arthropoda" rank="species" species="helmitheros" status="sp. n.">
<emphasis id="B953DE0DFFB4FF97FECB2CFBFD90D1B2" box="[303,563,1457,1479]" italics="true" pageId="24" pageNumber="225">Amerodectes helmitheros</emphasis>
