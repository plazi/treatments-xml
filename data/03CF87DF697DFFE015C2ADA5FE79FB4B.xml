<quantity id="4C9E9B2C697CFFE016A0ABF5FB3DFBB4" box="[1013,1179,1017,1043]" metricMagnitude="-4" metricUnit="m" metricValue="3.15" metricValueMax="3.3" metricValueMin="3.0" pageId="3" unit="mm" value="0.315" valueMax="0.33" valueMin="0.3">0.30–0.33 mm</quantity>
(mean 
