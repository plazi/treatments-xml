<taxonomicName id="B2D34D30CD79731B9EACFA8EFCC34788" authorityName="Lelej &amp; Terine &amp; Kumar" authorityYear="2021" baseAuthorityName="Lelej &amp; Ullah" baseAuthorityYear="2007" box="[708,821,1398,1420]" class="Insecta" family="Mutillidae" genus="Smicromyrmilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="576" phylum="Arthropoda" rank="species" species="margalla">
