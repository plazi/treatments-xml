<taxonomicName id="8D858BAE147EFFC1FBA33D4FFB6BFB35" box="[1081,1248,1120,1145]" class="Bivalvia" family="Solenidae" genus="Solen" higherTaxonomySource="CoL" kingdom="Animalia" order="Adapedonta" pageId="2" pageNumber="63" phylum="Mollusca" rank="species" species="corneus">
