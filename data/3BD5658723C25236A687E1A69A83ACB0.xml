<geoCoordinate id="806C7B1AE23A1D0419039BAE6D43D98A" degrees="24" direction="north" minutes="33" orientation="latitude" precision="1" seconds="36.57" value="24.560158">24°33'36.57&quot;N</geoCoordinate>
