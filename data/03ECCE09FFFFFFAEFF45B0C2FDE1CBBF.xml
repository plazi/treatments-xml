Wings. 1, 
<taxonomicName id="4C45049CFFFEFFA9FE75B43FFDAECD1C" authority="Zhao &amp; Yan &amp; Liu, 2013" authorityName="Zhao &amp; Yan &amp; Liu" authorityYear="2013" box="[423,626,1856,1878]" class="Insecta" family="Hemerobiidae" genus="Neuronema" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="560" phylum="Arthropoda" rank="species" species="ypsilum" status="sp. nov.">
<emphasis id="B931A30DFFFEFFA9FE75B43FFDAECD1C" box="[423,626,1856,1878]" italics="true" pageId="3" pageNumber="560">Neuronema ypsilum</emphasis>
</taxonomicName>
<emphasis id="B931A30DFFFEFFA9FDAAB43EFD14CD1F" bold="true" box="[632,712,1856,1877]" pageId="3" pageNumber="560">
