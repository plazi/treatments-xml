<emphasis id="B93E4031510E3203FF04CA1AF42FF8E0" bold="true" box="[151,618,1838,1864]" pageId="1" pageNumber="2">
Genus 
<taxonomicName id="4C4AE7A0510E3203FF7DCA1AF42FF8E0" authority="Kieffer, 1919" authorityName="Kieffer" authorityYear="1919" box="[238,618,1838,1864]" class="Insecta" family="Chironomidae" genus="Stenochironomus" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis id="B93E4031510E3203FF7DCA1AF787F8E0" bold="true" box="[238,450,1838,1864]" italics="true" pageId="1" pageNumber="2">Stenochironomus</emphasis>
<bibRefCitation id="EFDBE1D2510E3203FE5ACA1AF42FF8E0" author="Kieffer" box="[457,618,1838,1864]" pageId="1" pageNumber="2" refString="Kieffer, J. J. (1919) Chironomiden der Nordlichen Polarregion. Entomologische Mitteilungen, 8, 40 - 48." type="journal article" year="1919">
Kieffer, 
<number id="3D828E04510E3203FDBCCA1AF42FF8E0" box="[559,618,1838,1864]" pageId="1" pageNumber="2" value="1919.0">1919</number>
</bibRefCitation>
