<quantity id="4CF09B4DCD11FFE5FD1DFBE5780FFB6E" box="[703,846,1149,1174]" metricMagnitude="2" metricUnit="m" metricValue="3.3528" metricValueMax="4.572" metricValueMin="2.1336" pageId="21" pageNumber="266" unit="ft" value="1100.0" valueMax="1500.0" valueMin="700.0">700–1500 ft</quantity>
