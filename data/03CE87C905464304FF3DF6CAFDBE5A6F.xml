<taxonomicName id="4C674D5C05454302FE1AF720FD865C74" box="[432,547,1888,1911]" class="Insecta" family="Buprestidae" genus="Brachelytrium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="17" phylum="Arthropoda" rank="species" species="niehuisi" status="sp. nov.">
