<taxonomicName id="4C674D5C05454302FB57F7C0FEF75CB5" authority="Elytrobrachium" authorityName="Elytrobrachium" class="Insecta" family="Buprestidae" genus="Brachelytrium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="17" phylum="Arthropoda" rank="genus">
