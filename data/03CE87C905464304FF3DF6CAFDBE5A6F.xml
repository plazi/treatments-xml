<taxonomicName id="4C674D5C05454302FB62F760FA935C34" box="[1224,1334,1824,1847]" class="Insecta" family="Buprestidae" genus="Brachelytrium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="17" phylum="Arthropoda" rank="species" species="nigrum" status="sp. nov.">
