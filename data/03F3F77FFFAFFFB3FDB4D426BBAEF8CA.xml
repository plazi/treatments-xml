</emphasis>
. 
<taxonomicName id="4C5A3DEAFFAFFFB3FB04D570BBAEFAB8" authorityName="Temminck" authorityYear="1840" box="[1157,1253,1282,1306]" class="Mammalia" family="Vespertilionidae" genus="Vespertilio" kingdom="Animalia" order="Chiroptera" pageId="49" pageNumber="50" phylum="Chordata" rank="species" species="pachypus">
