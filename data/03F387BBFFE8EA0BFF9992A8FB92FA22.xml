<taxonomicName id="4C5A4D2EFFEBEA0BFD5A928BFCF1FA02" box="[698,814,1408,1431]" class="Bacilli" family="Lactobacillaceae" genus="Latilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="6" pageNumber="7" phylum="Firmicutes" rank="species" species="curvatus">
