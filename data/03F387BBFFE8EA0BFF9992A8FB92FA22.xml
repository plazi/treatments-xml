<taxonomicName id="4C5A4D2EFFEBEA0BFDF1928BFD59FA02" box="[529,646,1408,1431]" class="Bacilli" family="Lactobacillaceae" genus="Latilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="6" pageNumber="7" phylum="Firmicutes" rank="species" species="graminis">
<emphasis id="B92EEABFFFEBEA0BFDF1928BFD59FA02" box="[529,646,1408,1431]" italics="true" pageId="6" pageNumber="7">L. graminis</emphasis>
