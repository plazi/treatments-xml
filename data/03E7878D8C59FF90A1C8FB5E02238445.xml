<subSubSection id="C35465108C5AFF90A198FEB302238445" box="[199,1114,332,357]" pageId="14" pageNumber="215" type="etymology">
<paragraph id="8BF1369B8C5AFF90A198FEB302238445" blockId="14.[151,1436,151,357]" box="[199,1114,332,357]" pageId="14" pageNumber="215">
<emphasis id="B93AEA898C5AFF90A198FEB307378445" bold="true" box="[199,334,332,357]" pageId="14" pageNumber="215">Etymology.</emphasis>
