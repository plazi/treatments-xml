<location id="8EFE79D5FFFA5553FA9E4011FEB6FE7F" LSID="urn:lsid:plazi:treatment:03889E18FFFB5553FF5C4737FACBF830:8EFE79D5FFFA5553FA9E4011FEB6FE7F" country="Spain" county="Guipuzcoa province" latitude="43.1425" longLatPrecision="19" longitude="-2.1258333" municipality="Albistur" name="Cueva de Mendicute" pageId="69" pageNumber="70" stateProvince="Basque Country">
<collectorName id="26D44AD8FFFA5553FA9E4011FEB6FE7F" pageId="69" pageNumber="70">Cueva de Mendicute</collectorName>
</location>
(
<geoCoordinate id="EE1549C9FFFA5553FEF440F5FE40FE7F" box="[319,454,404,429]" direction="north" orientation="latitude" pageId="69" pageNumber="70" precision="15" value="43.1425">43°08′33″N</geoCoordinate>
, 
<geoCoordinate id="EE1549C9FFFA5553FE1A40F5FDDAFE7F" box="[465,604,404,429]" direction="west" orientation="longitude" pageId="69" pageNumber="70" precision="15" value="-2.1258333">02°07′33″W</geoCoordinate>
; 
<elevation id="000CC83DFFFA5553FDA340F5FD6CFE7F" box="[616,746,404,429]" metricMagnitude="2" metricUnit="m" metricValue="6.58" pageId="69" pageNumber="70" unit="m" value="658.0">
<quantity id="4CD982EBFFFA5553FDA340F5FD29FE7E" box="[616,687,404,429]" metricMagnitude="2" metricUnit="m" metricValue="6.58" pageId="69" pageNumber="70" unit="m" value="658.0">658 m</quantity>
a.s.l.
</elevation>
), without collecting date, leg. 
<collectorName id="26D44AD8FFFA5553FB8B40F5FB3DFE7F" box="[1088,1211,404,429]" pageId="69" pageNumber="70">C. Bolivar.</collectorName>
