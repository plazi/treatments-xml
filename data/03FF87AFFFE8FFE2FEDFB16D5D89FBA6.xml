<quantity id="4CAE9B5CFFEBFFE2FE1BB7255C21FC4E" box="[460,564,604,630]" metricMagnitude="-3" metricUnit="m" metricValue="3.18" pageId="22" pageNumber="23" unit="mm" value="3.18">3.18 mm</quantity>
