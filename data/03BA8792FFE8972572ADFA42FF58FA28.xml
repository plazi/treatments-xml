<taxonomicName id="4C134D07FFE8973B70ADFAC5FBAFFB4B" authority="Pace, 2008" authorityName="Pace" authorityYear="2008" box="[753,1067,1357,1385]" class="Insecta" family="Staphylinidae" genus="Placusa" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="570" phylum="Arthropoda" rank="species" species="madecassa" status="n. sp." subGenus="Calpusa">
