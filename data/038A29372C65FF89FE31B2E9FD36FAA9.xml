and 
<taxonomicName id="4C23E3A22C65FF89FE20B6F8FD8AFB48" box="[444,552,1226,1249]" class="Insecta" family="Anthicidae" genus="Notoxus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="663" phylum="Arthropoda" rank="species" species="brincki">
