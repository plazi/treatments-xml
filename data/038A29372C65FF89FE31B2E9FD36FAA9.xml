<taxonomicName id="4C23E3A22C65FF89FF75B6F8FE25FB48" authorityName="Bonadona" authorityYear="1986" box="[233,391,1226,1249]" class="Insecta" family="Anthicidae" genus="Notoxus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="663" phylum="Arthropoda" rank="species" species="brinckianus">
