<taxonomicNameLabel id="F7DF57EBFF8CFFAF7DD1F9E38A20F9CD" box="[1245,1341,1556,1581]" pageId="4" rank="species">
<emphasis id="ECECEA90FF8CFFAF7DD1F9E38A20F9CD" bold="true" box="[1245,1341,1556,1581]" pageId="4">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="19984D01FF8CFFAF7C8DF9E38E33F9B0" class="Insecta" family="Coccinellidae" genus="Oryssomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" phylum="Arthropoda" rank="species" species="unimaculatus">
