<taxonomicName id="19984D01FF8CFFAF7A45F9E38CB4F9CC" box="[841,937,1556,1580]" class="Insecta" family="Coccinellidae" genus="Oryssomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" phylum="Arthropoda" rank="species" species="calix" status="sp. nov.">
<emphasis id="ECECEA90FF8CFFAF7A45F9E38CB4F9CC" box="[841,937,1556,1580]" italics="true" pageId="4">O. calix</emphasis>
</taxonomicName>
<emphasis id="ECECEA90FF8CFFAF7ABAF9E38B00F9CD" bold="true" box="[950,1053,1556,1581]" pageId="4">
<taxonomicNameLabel id="F7DF57EBFF8CFFAF7ABAF9E38B0AF9CD" box="[950,1047,1556,1581]" pageId="4" rank="species">sp. nov.</taxonomicNameLabel>
,
</emphasis>
<taxonomicName id="19984D01FF8CFFAF7D26F9E38BD2F9CC" box="[1066,1231,1556,1580]" class="Insecta" family="Coccinellidae" genus="Oryssomus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" phylum="Arthropoda" rank="species" species="guyanensis" status="sp. nov.">
