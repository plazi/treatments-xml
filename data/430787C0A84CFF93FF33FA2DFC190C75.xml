<collectionCode id="ADBFAE13A84CFF93FCA5FBE4FC2D0D5C" box="[816,912,1073,1094]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="84" pageNumber="84">AMNH</collectionCode>
