<specimenCode id="9B089EADA84CFF93FD0EFB5FFCE00D85" box="[667,861,1161,1183]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="84" pageNumber="84">AMNH 110257</specimenCode>
(12948, 441237), 
<specimenCode id="9B089EADA84CFF93FBD9FB5FFD500DA6" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="84" pageNumber="84">AMNH 113040</specimenCode>
(12947, 441240), 
<specimenCode id="9B089EADA84CFF93FC71FB72FB140DA6" box="[996,1193,1191,1212]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="84" pageNumber="84">AMNH 113042</specimenCode>
