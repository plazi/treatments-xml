<collectionCode id="ADBFAE13A84CFF93FBD9FAD5FB170C0F" box="[1100,1194,1280,1301]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="84" pageNumber="84">AMNH</collectionCode>
