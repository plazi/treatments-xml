<collectionCode id="ADBFAE13A84CFF93FC47FBB9FB8D0D9B" box="[978,1072,1132,1153]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="84" pageNumber="84">AMNH</collectionCode>
