<quantity id="4C8E9B677D7AD105FE1C5539FDC20453" box="[426,512,1619,1641]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="98" pageNumber="98" unit="mm" value="1.1">1.1 mm</quantity>
