<taxonomicName id="D094D8E0FFDB6404DDB8FBAA09184A20" authorityName="Triebel" authorityYear="1941" box="[980,1136,1072,1098]" class="Ostracoda" family="Healdiidae" genus="Ogmoconcha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="55" pageNumber="56" phylum="Arthropoda" rank="genus">
