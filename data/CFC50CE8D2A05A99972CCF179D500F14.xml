<taxonomicName id="611755251A3517ECC585D23F29A19612" baseAuthorityName="Rudman" baseAuthorityYear="1979" class="Gastropoda" family="Trinchesiidae" genus="Phestilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phestilla poritophages" order="Nudibranchia" pageId="0" pageNumber="1" phylum="Mollusca" rank="species" species="poritophages">
