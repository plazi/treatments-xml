<taxonomicName id="8FEE4D41FFB9FF92FBE0F94D6462CC6F" authority="in Conil (1877" authorityName="in Conil" authorityYear="1877" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="80" pageNumber="81" phylum="Arthropoda" rank="species" species="argentinae">
