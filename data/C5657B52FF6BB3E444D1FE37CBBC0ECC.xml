<location id="48139C9FFF6BB3E44103FB26CD7C0E2B" LSID="urn:lsid:plazi:treatment:C5657B52FF6BB3E444D1FE37CBBC0ECC:48139C9FFF6BB3E44103FB26CD7C0E2B" box="[1349,1402,1263,1287]" name="Gulf" pageId="247" pageNumber="248" stateProvince="San Felipe, Baja California">Gulf</location>
