<quantity id="4CCA8D91FF9DFFE0FD6AD1801D4963BC" box="[757,820,919,945]" metricMagnitude="0" metricUnit="m" metricValue="9.0" pageId="16" pageNumber="45" unit="m" value="9.0">9 m</quantity>
