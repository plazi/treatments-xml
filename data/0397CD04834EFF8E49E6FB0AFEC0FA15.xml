<taxonomicName id="4C3E0791834EFF8E4A1DFB63FBD0FACE" authority="Nobuchi, 1981" authorityName="Nobuchi" authorityYear="1981" box="[876,1146,1261,1283]" class="Insecta" family="Curculionidae" genus="Xyleborus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="394" phylum="Arthropoda" rank="species" species="bispinus">
