<taxonomicName id="6DF6E099FFB4FFDB0A4B2F5295FBB16F" authorityName="Dendy &amp; Row" authorityYear="1913" box="[289,423,513,538]" class="Calcarea" family="Leucaltidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clathrinida" pageId="23" pageNumber="354" phylum="Porifera" rank="family">Leucaltidae</taxonomicName>
