<emphasis bold="true" box="[197,370,475,500]" pageId="19" pageNumber="20">
<typeStatus box="[197,254,476,500]" pageId="19" pageNumber="20">Type</typeStatus>
material.
</emphasis>
<typeStatus box="[381,485,476,500]" pageId="19" pageNumber="20">Holotype</typeStatus>
<number box="[493,514,476,500]" pageId="19" pageNumber="20" value="3.0">3</number>
(
<collectionCode box="[531,600,476,500]" httpUri="http://grbio.org/cool/rm88-499z" name="Instituto Nacional de Pesquisas da Amazonia" pageId="19" pageNumber="20">INPA</collectionCode>
), 
<collectingCountry box="[620,723,476,500]" name="Brazil" pageId="19" pageNumber="20">BRASIL</collectingCountry>
, 
