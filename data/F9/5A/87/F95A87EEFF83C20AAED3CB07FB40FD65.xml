<subSubSection pageId="11" pageNumber="12" type="etymology">
<paragraph blockId="11.[151,1437,151,1365]" box="[197,1300,1340,1365]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[197,332,1340,1364]" pageId="11" pageNumber="12">Etymology.</emphasis>
