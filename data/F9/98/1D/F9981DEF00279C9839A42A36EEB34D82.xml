<subSubSection pageId="0" pageNumber="1" type="type material">
<paragraph pageId="0" pageNumber="1">Type material.</paragraph>
<paragraph pageId="0" pageNumber="1">
<emphasis bold="true" italics="true" pageId="0" pageNumber="1">Holotype</emphasis>
<taxonomicName authorityName="Eggers" authorityYear="1923" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus fornicatior" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="fornicatior">
<emphasis italics="true" pageId="0" pageNumber="1">Xyleborus fornicatior</emphasis>
</taxonomicName>
(NMNH). 
<emphasis bold="true" italics="true" pageId="0" pageNumber="1">Lectotype</emphasis>
<taxonomicName authorityName="Schedl" authorityYear="1951" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus schultzei" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="schultzei">
