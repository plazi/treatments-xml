<taxonomicName class="Sordariomycetes" family="Nectriaceae" genus="Nectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Nectria" order="Hypocreales" pageId="0" pageNumber="49" phylum="Ascomycota" rank="genus">Nectria</taxonomicName>
