<subSubSection pageId="0" pageNumber="49" type="notes">
<paragraph pageId="0" pageNumber="49">Note.</paragraph>
<paragraph pageId="0" pageNumber="49">
<taxonomicName class="Sordariomycetes" family="Nectriaceae" genus="Nectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Nectria dematiosa" order="Hypocreales" pageId="0" pageNumber="49" phylum="Ascomycota" rank="species" species="dematiosa">Nectria dematiosa</taxonomicName>
