</taxonomicName>
<taxonomicNameLabel box="[378,427,1644,1661]" pageId="8" pageNumber="727" rank="species">n. sp.</taxonomicNameLabel>
; 
<emphasis bold="true" box="[434,447,1644,1661]" pageId="8" pageNumber="727">A</emphasis>
, male body, dorsal view; 
<emphasis bold="true" box="[658,671,1644,1661]" pageId="8" pageNumber="727">B</emphasis>
