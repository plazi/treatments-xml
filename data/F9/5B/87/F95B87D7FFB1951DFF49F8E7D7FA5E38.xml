<taxonomicName class="Cycadopsida" family="Zamiaceae" genus="Ceratozamia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Cycadales" pageId="5" pageNumber="156" phylum="Tracheophyta" rank="species" species="tenuis">
