) with 
<taxonomicName authorityName="Tang, Skelley &amp; Perez-Farrera" authorityYear="2018" box="[699,1033,800,824]" class="Cycadopsida" family="Zamiaceae" genus="Ceratophila" higherTaxonomySource="GBIF" kingdom="Plantae" order="Cycadales" pageId="20" pageNumber="171" phylum="Tracheophyta" rank="genus" species="picipennis" subGenus="Ceratophila">
