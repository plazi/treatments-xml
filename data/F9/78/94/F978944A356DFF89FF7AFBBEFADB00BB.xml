<collectingCountry box="[804,933,1158,1183]" name="Costa Rica" pageId="9" pageNumber="142">Costa Rica</collectingCountry>
, 
<collectingCountry box="[950,1060,1158,1183]" name="Colombia" pageId="9" pageNumber="142">Colombia</collectingCountry>
(
<bibRefCitation author="Kraepelin, K." box="[1080,1263,1158,1183]" pageId="9" pageNumber="142" pagination="1 - 276" refId="ref17651" refString="Kraepelin, K. (1903) Revision der Scolopendriden. Mitteilungen aus dem Naturhistorischen Museum in Hamburg, 20, 1 - 276." type="journal article" year="1903">Kraepelin 1903</bibRefCitation>
