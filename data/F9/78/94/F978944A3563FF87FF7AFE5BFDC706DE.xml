<taxonomicName baseAuthorityName="Humbert &amp; Saussure" baseAuthorityYear="1869" box="[314,488,548,572]" class="Chilopoda" family="Scolopendridae" genus="Rhysida" kingdom="Animalia" order="Scolopendromorpha" pageId="7" pageNumber="140" phylum="Arthropoda" rank="species" species="celeris">
