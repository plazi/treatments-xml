<collectingCountry box="[834,952,2008,2033]" name="Venezuela" pageId="6" pageNumber="139">Venezuela</collectingCountry>
, 
<collectingCountry box="[966,1079,2008,2033]" name="Colombia" pageId="6" pageNumber="139">Colombia</collectingCountry>
, 
<collectingCountry box="[1093,1147,2009,2032]" name="Peru" pageId="6" pageNumber="139">Peru</collectingCountry>
, 
<collectingCountry box="[1162,1226,2008,2033]" name="Chile" pageId="6" pageNumber="139">Chile</collectingCountry>
, 
<collectingCountry box="[1239,1354,2008,2033]" name="Argentina" pageId="6" pageNumber="139">Argentina</collectingCountry>
, 
<collectingCountry box="[1367,1435,2008,2033]" name="Brazil" pageId="6" pageNumber="139">Brazil</collectingCountry>
and 
<collectingCountry box="[197,295,152,176]" name="Uruguay" pageId="7" pageNumber="140">Uruguay</collectingCountry>
(
<bibRefCitation author="Kraepelin, K." box="[309,487,151,176]" pageId="7" pageNumber="140" pagination="1 - 276" refId="ref17651" refString="Kraepelin, K. (1903) Revision der Scolopendriden. Mitteilungen aus dem Naturhistorischen Museum in Hamburg, 20, 1 - 276." type="journal article" year="1903">Kraepelin 1903</bibRefCitation>
