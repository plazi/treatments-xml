<typeStatus box="[455,560,1749,1773]" pageId="4" pageNumber="66">holotype</typeStatus>
): Total length 
<number box="[727,779,1749,1773]" pageId="4" pageNumber="66" value="2.08">2.08</number>
; prosoma 
<number box="[899,948,1749,1773]" pageId="4" pageNumber="66" value="1.1">1.10</number>
long, 
<number box="[1024,1073,1749,1773]" pageId="4" pageNumber="66" value="0.82">0.82</number>
wide. Leg measurements: leg I 
<number box="[151,200,1785,1809]" pageId="4" pageNumber="66" value="3.36">3.36</number>
(
<number box="[217,270,1785,1809]" pageId="4" pageNumber="66" value="0.88">0.88</number>
, 
<number box="[284,336,1785,1809]" pageId="4" pageNumber="66" value="0.38">0.38</number>
, 
<number box="[350,402,1785,1809]" pageId="4" pageNumber="66" value="0.74">0.74</number>
, 
<number box="[416,468,1785,1809]" pageId="4" pageNumber="66" value="0.8">0.80</number>
, 
<number box="[481,534,1785,1809]" pageId="4" pageNumber="66" value="0.56">0.56</number>
); leg II 3.00 (
<number box="[695,748,1785,1809]" pageId="4" pageNumber="66" value="0.76">0.76</number>
, 
<number box="[762,814,1785,1809]" pageId="4" pageNumber="66" value="0.36">0.36</number>
, 
<number box="[828,880,1785,1809]" pageId="4" pageNumber="66" value="0.54">0.54</number>
, 
<number box="[893,945,1785,1809]" pageId="4" pageNumber="66" value="0.84">0.84</number>
, 
<number box="[958,1012,1785,1809]" pageId="4" pageNumber="66" value="0.5">0.50</number>
); leg III 
<number box="[1116,1165,1785,1809]" pageId="4" pageNumber="66" value="2.98">2.98</number>
(
<number box="[1182,1235,1785,1809]" pageId="4" pageNumber="66" value="0.8">0.80</number>
, 
<number box="[1249,1300,1785,1809]" pageId="4" pageNumber="66" value="0.36">0.36</number>
, 
<number box="[1314,1366,1785,1809]" pageId="4" pageNumber="66" value="0.52">0.52</number>
, 
<number box="[1380,1432,1785,1809]" pageId="4" pageNumber="66" value="0.76">0.76</number>
, 
<number box="[151,205,1821,1845]" pageId="4" pageNumber="66" value="0.54">0.54</number>
); leg IV 
<number box="[302,351,1821,1845]" pageId="4" pageNumber="66" value="4.54">4.54</number>
(
<number box="[366,418,1821,1845]" pageId="4" pageNumber="66" value="1.18">1.18</number>
, 
<number box="[430,482,1821,1845]" pageId="4" pageNumber="66" value="0.42">0.42</number>
, 1.00, 
<number box="[556,605,1821,1845]" pageId="4" pageNumber="66" value="1.3">1.30</number>
, 
<number box="[619,673,1821,1845]" pageId="4" pageNumber="66" value="0.64">0.64</number>
). Eyes: ALE 
<number box="[822,875,1821,1845]" pageId="4" pageNumber="66" value="0.12">0.12</number>
; other eyes 
<number box="[1007,1060,1821,1845]" pageId="4" pageNumber="66" value="0.06">0.06</number>
