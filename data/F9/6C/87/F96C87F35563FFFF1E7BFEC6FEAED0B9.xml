<quantity box="[475,645,444,470]" metricMagnitude="1" metricUnit="m" metricValue="3.9195" metricValueMax="4.239" metricValueMin="3.6" pageId="6" pageNumber="7" unit="m" value="39.195" valueMax="42.39" valueMin="36.0">36–42, 39 m</quantity>
