<taxonomicName authority="Townsend, 1921" authorityName="Townsend" authorityYear="1921" class="Insecta" family="Muscidae" genus="Paleotachina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paleotachina smithii" order="Diptera" pageId="2" pageNumber="29" phylum="Arthropoda" rank="species" species="smithii">Paleotachina smithii</taxonomicName>
