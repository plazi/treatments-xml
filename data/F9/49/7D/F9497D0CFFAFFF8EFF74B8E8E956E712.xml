<emphasis box="[1037,1293,587,613]" italics="true" pageId="1" pageNumber="117">Monachosorum henryi</emphasis>
<bibRefCitation author="Christ, H." pageId="1" pageNumber="117" pagination="956 - 994" refId="ref1431" refString="Christ, H. (1898) Fougeres de Mengtze, Yunnan Meridional (Chine). Bulletin de l'Herbier Boissier 6: 956 - 994." type="journal article" year="1898">Christ (1898: 869)</bibRefCitation>
</taxonomicName>
</materialsCitation>
. 
<materialsCitation box="[200,579,623,650]" collectionCode="A" country="Indonesia" pageId="1" pageNumber="117" specimenCount="1" stateProvince="Jawa Timur" typeStatus="neotype">
<collectionCode box="[200,220,624,650]" country="USA" lsid="urn:lsid:biocol.org:col:15406" name="Harvard University - Arnold Arboretum" pageId="1" pageNumber="117" type="Herbarium">A</collectionCode>
<typeStatus box="[226,315,624,650]" pageId="1" pageNumber="117">neotype</typeStatus>
of 
<taxonomicName box="[351,464,623,650]" class="Polypodiopsida" family="Pteridaceae" genus="Adiantum" kingdom="Plantae" order="Polypodiales" pageId="1" pageNumber="117" phylum="Tracheophyta" rank="species" species="pallens">
