<paragraph blockId="10.[151,1437,925,2030]" pageId="10" pageNumber="151">
<materialsCitation ID-GBIF-Occurrence="1324562834" collectorName="Remarks. De Zoysa" country="Sri Lanka" location="Sri Lanka" pageId="10" pageNumber="151" specimenCount="1" typeStatus="holotype">
