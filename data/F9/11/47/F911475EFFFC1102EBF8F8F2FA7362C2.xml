<subSubSection pageId="6" pageNumber="347" type="description">
<paragraph blockId="6.[151,1437,151,392]" box="[199,804,295,320]" pageId="6" pageNumber="347">
<emphasis bold="true" box="[199,337,295,320]" pageId="6" pageNumber="347">Description</emphasis>
