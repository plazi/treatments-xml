22–24— 
<taxonomicName baseAuthorityName="Fairmaire" baseAuthorityYear="1894" box="[435,737,1527,1551]" class="Insecta" family="Staphylinidae" genus="Rhyncocheilus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="106" phylum="Arthropoda" rank="species" species="griseosericans">
<emphasis box="[435,737,1527,1551]" italics="true" pageId="7" pageNumber="106">Rhyncocheilus griseosericans</emphasis>
</taxonomicName>
; 25–27— 
<taxonomicName authorityName="Tang, Schillhammer &amp; Zhao" authorityYear="2021" box="[839,1014,1527,1551]" class="Insecta" family="Staphylinidae" genus="Rhyncocheilus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="106" phylum="Arthropoda" rank="species" species="gonggashanus">
<emphasis box="[839,1014,1527,1551]" italics="true" pageId="7" pageNumber="106">R. gonggashanus</emphasis>
</taxonomicName>
; 28, 29— 
<taxonomicName authorityName="Tang &amp; Schillhammer &amp; Zhao" authorityYear="2021" box="[1117,1296,1527,1551]" class="Insecta" family="Staphylinidae" genus="Rhyncocheilus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="106" phylum="Arthropoda" rank="species" species="yangxiaodongi">
<emphasis box="[1117,1296,1527,1551]" italics="true" pageId="7" pageNumber="106">R. yangxiaodongi</emphasis>
</taxonomicName>
; 22, 25, 28— male head; 23, 26—female head; 24, 27, 29—Labrum. Scale bars: 
<quantity box="[829,891,1563,1587]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="7" pageNumber="106" unit="mm" value="1.0">1 mm</quantity>
.
