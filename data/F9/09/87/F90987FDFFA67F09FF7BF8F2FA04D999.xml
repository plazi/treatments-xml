<taxonomicName authorityName=", Schillhammer" authorityYear="2004" box="[873,1078,223,248]" class="Insecta" family="Staphylinidae" genus="Rhyncocheilus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="111" phylum="Arthropoda" rank="species" species="sommersgutteri">
<emphasis box="[873,1078,223,248]" italics="true" pageId="12" pageNumber="111">R. sommersgutteri</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Tang, Schillhammer &amp; Zhao" authorityYear="2021" box="[1093,1286,223,249]" class="Insecta" family="Staphylinidae" genus="Rhyncocheilus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="111" phylum="Arthropoda" rank="species" species="gonggashanus">
<emphasis box="[1093,1286,223,249]" italics="true" pageId="12" pageNumber="111">R. gonggashanus</emphasis>
