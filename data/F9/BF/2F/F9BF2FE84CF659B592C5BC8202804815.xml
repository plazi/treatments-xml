<paragraph pageId="449" pageNumber="450">
<taxonomicName authorityName="Casey" authorityYear="1918" class="Hexapoda" family="Carabidae" genus="Tachys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="449" pageNumber="450" phylum="Arthropoda" rank="species" species="subtropicus">Tachys subtropicus</taxonomicName>
