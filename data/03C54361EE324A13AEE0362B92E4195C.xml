<location id="8EB3A4ACEE324A13ACB137B2934B1ECD" LSID="urn:lsid:plazi:treatment:03C54361EE324A13AEE0362B92E4195C:8EB3A4ACEE324A13ACB137B2934B1ECD" box="[710,787,908,934]" country="China" county="Huanjiang County" name="Mulun" pageId="25" pageNumber="428" stateProvince="Guangxi">Mulun</location>
