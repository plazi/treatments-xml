<taxonomicName id="FD0BE96DEF5A883D0EC2B79B12D987F9" class="Diplopoda" family="Rhachodesmidae" genus="Tancitares" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tancitares" order="Polydesmida" pageId="1" pageNumber="28" phylum="Arthropoda" rank="genus">Tancitares</taxonomicName>
