</taxonomicName>
(formerly 
<taxonomicName id="4C1D989DFFE3F01F152AFD8ADE5E9FBA" authorityName="Haacke" authorityYear="1886" box="[260,373,1977,2000]" class="Cubozoa" family="Carybdeidae" genus="Carybdea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Carybdeida" pageId="13" pageNumber="528" phylum="Cnidaria" rank="species" species="rastonii">
