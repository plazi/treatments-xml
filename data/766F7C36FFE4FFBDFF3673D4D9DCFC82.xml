<quantity id="393E60C5FFE7FFBDFED077CED9E7FC82" box="[347,415,745,769]" metricMagnitude="2" metricUnit="m" metricValue="7.5" pageId="86" pageNumber="99" unit="m" value="750.0">750 m</quantity>
