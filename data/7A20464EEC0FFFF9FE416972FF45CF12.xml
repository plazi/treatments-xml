<caption id="A6F6A7D0EC09FFF9FF266903FCA4C99D" ID-DOI="http://doi.org/10.5281/zenodo.5250826" ID-Zenodo-Dep="5250826" httpUri="https://zenodo.org/record/5250826/files/figure.png" pageId="37" pageNumber="2626" startId="37.[154,170,879,901]" targetBox="[316,952,173,842]" targetPageId="37">
<paragraph id="F236F758EC09FFF9FF266903FCA4C99D" blockId="37.[154,1111,879,927]" pageId="37" pageNumber="2626">
