<emphasis id="C0FD2B4AEC09FFF9FF436903FED8C986" box="[255,380,879,900]" italics="true" pageId="37" pageNumber="2626">
<taxonomicName id="35898CDBEC09FFF9FF436903FED3C986" authorityName="Loven" authorityYear="1846" box="[255,375,879,900]" class="Bivalvia" family="Limidae" genus="Limea" kingdom="Animalia" order="Limida" pageId="37" pageNumber="2626" phylum="Mollusca" rank="species" species="sarsi">Limea sarsi</taxonomicName>
.
