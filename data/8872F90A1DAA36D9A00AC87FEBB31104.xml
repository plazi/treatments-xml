[[ 
<taxonomicName id="865C2A62325ABA1A2ABE47A893B3CAFE" LSID-ZBK="urn:lsid:zoobank.org:act:3885C1A0-95D6-4FDB-8674-45214D2C4EE6" class="Actinopterygii" family="Siluridae" genus="Pterocryptis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Pterocryptis Peters 1861:712" order="Siluriformes" pageNumber="51" phylum="Chordata" rank="genus">Pterocryptis Peters</taxonomicName>
