<taxonomicName id="4C374D59FF93C823FD1348B335C1DFF2" authorityName="Braun" authorityYear="1917" box="[767,902,690,715]" class="Insecta" family="Nepticulidae" genus="Glaucolepis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="502" phylum="Arthropoda" rank="genus">
