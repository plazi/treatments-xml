<taxonomicName id="11A2AED4E42C026F883138D9B7F4BE5D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33915" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole paiute Gregg" lsidName-HNS="Pheidole paiute Gregg" order="Hymenoptera" pageNumber="584" phylum="Arthropoda" rank="species" species="paiute">paiute</taxonomicName>
