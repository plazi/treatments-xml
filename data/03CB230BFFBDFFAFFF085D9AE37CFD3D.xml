<taxonomicName id="4C62E99EFFBEFFAFFBCF5AC8E3F0FE5D" box="[1104,1174,404,428]" class="Maxillopoda" family="Telsidae" genus="Telson" kingdom="Animalia" order="Poecilostomatoida" pageId="11" pageNumber="12" phylum="Arthropoda" rank="genus">
<emphasis id="B9164E0FFFBEFFAFFBCF5AC8E3F0FE5D" box="[1104,1174,404,428]" italics="true" pageId="11" pageNumber="12">Telson</emphasis>
</taxonomicName>
