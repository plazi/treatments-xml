<materialsCitation id="87D94D78A94DFFA82F04C57BFCC2FB97" ID-GBIF-Occurrence="2331574330" collectionCode="MGUH" pageId="77" pageNumber="78" specimenCode="MGUH 33122" specimenCount="2" typeStatus="paratype">
