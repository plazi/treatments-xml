<location id="F7240EADFF8D5016FC8FFDCEFCB3A22F" LSID="urn:lsid:plazi:treatment:7A52E960FF8D5016FF67F9D7FBB9A38F:F7240EADFF8D5016FC8FFDCEFCB3A22F" box="[853,888,522,545]" country="Vietnam" municipality="Phuc-Son" name="We" pageId="11" pageNumber="340" stateProvince="Ho Chi Minh City">We</location>
