<collectingCountry id="B997BA9FE356FF907132FAA035DCFE79" box="[574,679,433,457]" name="Germany" pageId="22" pageNumber="23">Germany</collectingCountry>
, 
<collectingCountry id="B997BA9FE356FF9071B8FAA03479FE79" box="[692,770,433,457]" name="Poland" pageId="22" pageNumber="23">Poland</collectingCountry>
(
<bibRefCitation id="A51187FEE356FF90701DFAA034D6FE79" author="Jansson" box="[785,941,433,457]" firstAuthor="Jansson" pageId="22" pageNumber="23" pagination="65 - 70" refId="ref61321" refString="Jansson, A. (1983) Three new palaearctic species of Sigara (Subsigara) (Heteroptera, Corixidae). Annales Entomologici Fennici, 49, 65 - 70." type="journal article" year="1983">Jansson 1983</bibRefCitation>
), 
<collectingCountry id="B997BA9FE356FF9070CEFAA03308FE79" box="[962,1139,433,457]" name="Netherlands" pageId="22" pageNumber="23">the Netherlands</collectingCountry>
(
<bibRefCitation id="A51187FEE356FF90778FFAA03265FE79" author="Cuppen" box="[1155,1310,433,457]" firstAuthor="Cuppen" pageId="22" pageNumber="23" pagination="94 - 96" refId="ref57082" refString="Cuppen, J. G. M. (1988) Sigara iactans new for the Netherlands (Heteroptera: Corixidae). Entomologische Berichten (Amsterdam), 48, 94 - 96." type="journal article" year="1988">Cuppen 1988</bibRefCitation>
), 
<collectingCountry id="B997BA9FE356FF907638FAA032E7FE79" box="[1332,1436,433,457]" name="Denmark" pageId="22" pageNumber="23">Denmark</collectingCountry>
(
<bibRefCitation id="A51187FEE356FF907392FAC436B7FE5D" author="Damgaard &amp; Mahler" box="[158,460,469,493]" firstAuthor="Damgaard" pageId="22" pageNumber="23" pagination="101 - 105" refId="ref57564" refString="Damgaard, J. &amp; Mahler, V. (1993) To nye danske vandtaeger (Heteroptera, Nepomorpha). (Two new Danish waterbugs (Heteroptera, Nepomorpha )). Entomologisk Meddelelser, 63, 101 - 105 (in Danish, Eniglish abstract)." type="journal article" year="1993">Damgaard &amp; Mahler 1993</bibRefCitation>
), 
<collectingCountry id="B997BA9FE356FF9072ECFAC43543FE5D" box="[480,568,469,493]" name="Sweden" pageId="22" pageNumber="23">Sweden</collectingCountry>
(Polhemus 
<emphasis id="F3F4261DE356FF9071B3FAC7358CFE5D" box="[703,759,469,493]" italics="true" pageId="22" pageNumber="23">et al.</emphasis>
1995), 
<collectingCountry id="B997BA9FE356FF907042FAC434D5FE5D" box="[846,942,469,493]" name="Belgium" pageId="22" pageNumber="23">Belgium</collectingCountry>
(
<bibRefCitation id="A51187FEE356FF9070B1FAC433FCFE5D" author="Vercauteren" box="[957,1159,469,493]" firstAuthor="Vercauteren" pageId="22" pageNumber="23" pagination="97 - 105" refId="ref71921" refString="Vercauteren, T. (1997) Sigara (Subsigara) iactans, a new species for Belgium (Heteroptera: Corixidae). Phegea, 25, 97 - 105" type="journal article" year="1997">Vercauteren 1997</bibRefCitation>
), 
<collectingCountry id="B997BA9FE356FF907797FAC43236FE5D" box="[1179,1357,469,493]" name="Czech Republic" pageId="22" pageNumber="23">Czech Republic</collectingCountry>
(
<bibRefCitation id="A51187FEE356FF907650FAC43631FDA1" author="Bryja &amp; Kment" firstAuthor="Bryja" pageId="22" pageNumber="23" pagination="1 - 36" refId="ref56043" refString="Bryja, J. &amp; Kment, P. (2001) The present state of knowledge of the true bugs (Heteroptera) in the PLA of Poodri (Czech Republic). Klapalekiana, 37, 1 - 36." type="journal article" year="2001">Bryja &amp; Kment 2001</bibRefCitation>
