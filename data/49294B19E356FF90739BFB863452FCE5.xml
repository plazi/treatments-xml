<collectingCountry id="B997BA9FE356FF90739BF9503792FDE9" box="[151,233,577,601]" name="Greece" pageId="22" pageNumber="23">Greece</collectingCountry>
, 
<collectingCountry id="B997BA9FE356FF9073FAF950360EFDE9" box="[246,373,577,601]" name="Macedonia" pageId="22" pageNumber="23">Macedonia</collectingCountry>
, Turkish Thrace, 
