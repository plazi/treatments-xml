<typeStatus id="816623E3FFEBC44D4C6BF8F6338EF8BA" box="[634,682,1796,1820]" pageId="4" pageNumber="248">type</typeStatus>
specimens of 
<taxonomicName id="99DDE6C2FFEBC44D4D42F8F632C3F8BA" box="[851,999,1796,1820]" class="Insecta" family="Formicidae" genus="Cataglyphis" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="248" phylum="Arthropoda" rank="species" species="bellicosus">
