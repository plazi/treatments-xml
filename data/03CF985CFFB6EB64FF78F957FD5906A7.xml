(
<figureCitation id="135D35CFFFB6EB66FF70F8B4FF0C030C" box="[159,228,1815,1840]" captionStart="FIGURE 2" captionStartId="6.[151,250,1863,1885]" captionTargetBox="[151,1436,193,1841]" captionTargetId="figure@6.[151,1436,193,1842]" captionTargetPageId="6" captionText="FIGURE 2. Wing of male (Scale bars = 1 mm): A. Leucophenga ornata; B. L. saigusai; C. L. sculpta; D. L. todai; E. L. apunctata sp. nov.; F. L. arcuata sp. nov.; G. L. brevifoliacea sp. nov.; H. L. euryphylla sp. nov.; I. L. falcata sp. nov.; J. L. fuscinotata sp. nov." httpUri="https://zenodo.org/record/249342/files/figure.png" pageId="25" pageNumber="126">
Figs 
<number id="3DAE3B6DFFB6EB66FF38F8B4FF0C030C" box="[215,228,1815,1839]" pageId="25" pageNumber="126" value="2.0">2</number>
</figureCitation>
G, 
<number id="3DAE3B6DFFB6EB66FEEBF8B4FEFA0313" box="[260,274,1815,1840]" pageId="25" pageNumber="126" value="5.0">5</number>
C, 
<number id="3DAE3B6DFFB6EB66FEDCF8B4FEA90313" box="[307,321,1815,1840]" pageId="25" pageNumber="126" value="5.0">5</number>
D, 
<number id="3DAE3B6DFFB6EB66FE8CF8B4FE87030C" box="[355,367,1815,1839]" pageId="25" pageNumber="126" value="8.0">8</number>
G, 
<number id="3DAE3B6DFFB6EB66FE7FF8B4FE75030C" box="[400,413,1815,1839]" pageId="25" pageNumber="126" value="8.0">8</number>
H, 
