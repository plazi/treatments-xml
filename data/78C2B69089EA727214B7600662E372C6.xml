<taxonomicName id="88B349D1D15EC0A0E219FD3788657C50" family="Megachilidae" lsidName="" pageId="15" pageNumber="138" rank="family">Megachilidae</taxonomicName>
) (
<bibRefCitation id="1A6BC5ADAA464A22F3207975C1D4DF5E" author="Peck, O" journalOrPublisher="Canadian Entomologist" pageId="23" pageNumber="146" pagination="418 - 422" title="Chalcidoid (Hymenoptera) parasites of the alfalfa leaf-cutter bee, Megachilerotundata in Canada." url="https://doi.org/10.4039/Ent101418-4" volume="101" year="1969">Peck 1969</bibRefCitation>
) and 
<taxonomicName id="A58629C7208BC563E97A34DBD3C8F102" class="Insecta" family="Arctiidae" genus="Hyphantria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hyphantria cunea" order="Lepidoptera" pageId="15" pageNumber="138" phylum="Arthropoda" rank="species" species="cunea">Hyphantria cunea</taxonomicName>
(Drury) (
<taxonomicName id="5C14DBE440A058DA8C3CD4A281DE8CC8" authority="" authorityName="" class="Insecta" family="Liviidae" genus="Noctuoidea" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Noctuoidea" order="Lepidoptera" pageId="15" pageNumber="138" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName id="F8C781E0A0FF2C47C53FAA67E4548439" family="Erebidae" lsidName="" pageId="15" pageNumber="138" rank="family">Erebidae</taxonomicName>
) (
<bibRefCitation id="2E200FC9C8313C7E4349087F9F5BD662" author="Dzhanokmen, KA" journalOrPublisher="Opredeliteli Nasekomikh Evropeyskoy Chasti SSR" pageId="22" pageNumber="145" pagination="57 - 228" title="Hymenoptera III. Chalcidoidea 5. Pteromalidae." volume="3" year="1978">Dzhanokmen 1978</bibRefCitation>
