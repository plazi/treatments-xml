<paragraph id="C24C368DFFC3683AFF4AFC27CEE3FB15" blockId="69.[189,1399,974,1181]" pageId="69" pageNumber="70">
<taxonomicName id="05F34D0EFFC3683AFF4AFC27CE83FC61" authority="Verheye, 2017" authorityName="d’Acoz &amp; Verheye" authorityYear="2017" box="[189,404,974,1000]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="atalanta" status="sp. nov.">
<emphasis id="F087EA9FFFC3683AFF4AFC27CE83FC61" box="[189,404,974,1000]" italics="true" pageId="69" pageNumber="70">Epimeria atalanta</emphasis>
</taxonomicName>
<taxonomicNameLabel id="EBB457E4FFC3683AFE6CFC26CEE1FC61" box="[411,502,974,1000]" pageId="69" pageNumber="70" rank="species">sp. nov.</taxonomicNameLabel>
