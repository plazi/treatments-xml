<taxonomicName id="05F34D0EFFC3683AFBAAFBFFCBF7FBB9" authority="Verheye, 2017" authorityName="d’Acoz &amp; Verheye" authorityYear="2017" box="[1117,1248,1046,1072]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="atalanta" status="sp. nov.">
<emphasis id="F087EA9FFFC3683AFBAAFBFFCBF7FBB9" box="[1117,1248,1046,1072]" italics="true" pageId="69" pageNumber="70">E. atalanta</emphasis>
