</taxonomicName>
(A), 
<taxonomicName id="4C220CFBFFD79657F4E6F844FBF8F83A" authorityName="Kamiński &amp; Schoeman" authorityYear="2018" box="[986,1133,1970,1992]" class="Insecta" family="Tenebrionidae" genus="Anaxius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="474" phylum="Arthropoda" rank="species" species="campbellae">
<emphasis id="B956AB6AFFD79657F4E6F844FBF8F83A" box="[986,1133,1970,1992]" italics="true" pageId="3" pageNumber="474">A. campbellae</emphasis>
</taxonomicName>
(B), 
<taxonomicName id="4C220CFBFFD79657F394F844FAD9F83A" authorityName="Kamiński &amp; Schoeman" authorityYear="2018" box="[1192,1356,1970,1992]" class="Insecta" family="Tenebrionidae" genus="Anaxius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="474" phylum="Arthropoda" rank="species" species="limpopoensis">
<emphasis id="B956AB6AFFD79657F394F844FAD9F83A" box="[1192,1356,1970,1992]" italics="true" pageId="3" pageNumber="474">A. limpopoensis</emphasis>
