<subSubSection id="4791653FFFEEFFE6FF6DF912FBE84EC1" box="[198,1120,1733,1759]" pageId="1" pageNumber="435" type="etymology">
<paragraph id="0F3436B4FFEEFFE6FF6DF912FBE84EC1" blockId="1.[151,1436,1462,2030]" box="[198,1120,1733,1759]" pageId="1" pageNumber="435">
<emphasis id="3DFFEAA6FFEEFFE6FF6DF912FEDB4EC1" bold="true" box="[198,339,1733,1759]" pageId="1" pageNumber="435">Etymology.</emphasis>
