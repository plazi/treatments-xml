<quantity id="4CA6A7FBFF84FFD782A2FAD7FBC1FA8E" box="[989,1059,1292,1313]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" pageId="5" pageNumber="424" unit="mm" value="18.0">18 mm</quantity>
