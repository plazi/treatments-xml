<taxonomicName id="4C094D3FFF93B10A39525212FD97697A" box="[275,588,585,611]" class="Insecta" family="Aphididae" genus="Trichosiphonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="15" pageNumber="590" phylum="Arthropoda" rank="species" species="polygoni">
