<number id="F770BF20392DDC23FB2F703997D0FAD9" box="[1165,1194,1326,1350]" pageId="14" pageNumber="15" value="26.0">26</number>
% [
<number id="F770BF20392DDC23FB7370399664FAD9" box="[1233,1310,1326,1350]" pageId="14" pageNumber="15" value="26.0" valueMax="27.0" valueMin="25.0">25–27</number>
%], and 
<taxonomicName id="86B8D684392DDC23FADF70279259FAF6" class="Trematoda" family="Mesocoeliidae" genus="Mesocoelium" kingdom="Animalia" order="Plagiorchiida" pageId="14" pageNumber="15" phylum="Platyhelminthes" rank="species" species="americanum">
