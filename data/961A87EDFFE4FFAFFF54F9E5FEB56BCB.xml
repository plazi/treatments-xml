<number id="A87B24DCFFE7FFAFFDE2FDC7FDC16BED" box="[574,588,618,642]" pageId="6" pageNumber="155" value="4.0">4</number>
) 
<number id="A87B24DCFFE7FFAFFD87FDC7FDE56BED" box="[603,616,618,642]" pageId="6" pageNumber="155" value="4.0">4</number>
