<taxonomicName id="4C230FBBFFDCA467FD56FEC3FCFB7E32" box="[764,883,332,357]" class="Entognatha" family="Paronellidae" genus="Trogolaphysa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Collembola" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="balteata" status="sp. nov.">
