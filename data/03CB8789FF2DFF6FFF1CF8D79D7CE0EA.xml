<taxonomicName id="4C624D1CFF2CFF6FFDC3FE889BA4E0CC" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[584,685,376,401]" class="Insecta" family="Diptera (awaiting allocation)" genus="Thelipus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="157" pageNumber="158" phylum="Arthropoda" rank="genus">
<emphasis id="B916EA8DFF2CFF6FFDC3FE889BA4E0CC" box="[584,685,376,401]" italics="true" pageId="157" pageNumber="158">Thelipus</emphasis>
</taxonomicName>
in synonymy with 
<taxonomicName id="4C624D1CFF2CFF6FFC2BFE889AD6E0CC" authorityName="Robineau-Desvoidy" authorityYear="1830" box="[928,991,376,401]" class="Insecta" family="Heleomyzidae" genus="Leria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="157" pageNumber="158" phylum="Arthropoda" rank="genus">
