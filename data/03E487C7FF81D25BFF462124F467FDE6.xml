<caption id="DF326659FF83D255FF462012F3DDF8AA" httpUri="https://zenodo.org/record/164277/files/figure.png" pageId="55" pageNumber="56" targetBox="[161,1378,198,1772]" targetPageId="55">
<paragraph id="8BF236D1FF83D255FF462012F3DDF8AA" blockId="55.[151,1436,1792,1876]" pageId="55" pageNumber="56">
