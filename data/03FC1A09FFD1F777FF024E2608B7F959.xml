<caption id="DF2AFB97FFCFF777FF014B2F0ACBFB90" ID-DOI="http://doi.org/10.5281/zenodo.5352454" ID-Zenodo-Dep="5352454" box="[195,898,1101,1123]" httpUri="https://zenodo.org/record/5352454/files/figure.png" pageId="16" pageNumber="15" startId="16.[195,279,1101,1123]" targetBox="[205,1430,201,1071]" targetPageId="16">
