<taxonomicName id="4C7B4D7AFF8EE464A9FB569D4323FAEB" box="[393,544,1265,1290]" class="Magnoliopsida" family="Fabaceae" genus="Baptisia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="46" pageNumber="47" phylum="Tracheophyta" rank="species" species="alba">
