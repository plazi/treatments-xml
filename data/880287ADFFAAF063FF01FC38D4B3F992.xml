<taxonomicName id="C7AB4D38FFAAF063FEE5FC4CD702FBC1" box="[374,551,1001,1024]" class="Liliopsida" family="Orchidaceae" genus="Dracula" kingdom="Plantae" order="Asparagales" pageId="0" pageNumber="59" phylum="Tracheophyta" rank="species" species="radiosa">
