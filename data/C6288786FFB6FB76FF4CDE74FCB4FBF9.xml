<taxonomicName id="89814D13FFB6FB76FCCEDEA8FBEFFC00" authority="Hubner, 1819" authorityName="Hubner" authorityYear="1819" box="[789,1040,977,1002]" class="Insecta" family="Megachilidae" genus="Hoplitis" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="123" phylum="Arthropoda" rank="genus">
<emphasis id="7CF5EA82FFB6FB76FCCEDEA8FC90FC03" box="[789,879,977,1002]" italics="true" pageId="22" pageNumber="123">Hoplitis</emphasis>
