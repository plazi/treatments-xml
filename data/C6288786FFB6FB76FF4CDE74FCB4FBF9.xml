<number id="F84924B7FFB6FB76FC03DEA8FBEFFC00" box="[984,1040,977,1001]" pageId="22" pageNumber="123" value="1819.0">1819</number>
</taxonomicName>
(
<taxonomicName id="89814D13FFB6FB76FBC5DEA8FB52FC03" box="[1054,1197,977,1002]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="22" pageNumber="123" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
