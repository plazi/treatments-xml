<taxonomicName id="89814D13FFB6FB76FBB3DED3FA9DFC2A" authority="Berlese, 1916" authorityName="Berlese" authorityYear="1916" box="[1128,1378,938,963]" class="Insecta" family="Megachilidae" genus="Hoplitis" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="123" phylum="Arthropoda" rank="genus">
<emphasis id="7CF5EA82FFB6FB76FBB3DED3FB3DFC2A" box="[1128,1218,938,963]" italics="true" pageId="22" pageNumber="123">Hoplitis</emphasis>
