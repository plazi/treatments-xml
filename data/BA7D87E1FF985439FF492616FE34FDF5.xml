<taxonomicName id="F5D44D74FF975438FEFE233CFDE3F8C3" box="[288,533,1885,1908]" class="Insecta" family="Eriococcidae" genus="Sangicoccus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="morrisoni">
