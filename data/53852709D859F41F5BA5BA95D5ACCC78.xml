, 
<taxonomicName id="4434CD5E58FF67B90EE1874B802E0096" baseAuthorityName="Reeve" baseAuthorityYear="1849" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="138" pageNumber="139" phylum="Mollusca" rank="species" species="nigropileatus">
