<treatmentCitationGroup id="AB701183FFA7FFD2FE8E972391D99D20" box="[264,722,1308,1331]" pageId="9" pageNumber="44">
<treatmentCitation id="0AC110BCFFA7FFD2FE8E972391719D20" author="Aurivillius, C. W. S." box="[264,634,1308,1331]" page="133" pageId="9" pageNumber="44" year="1892">
