<taxonomicName id="4C0E4D69FFFBFFD0E2E0FAE5FD512674" authority="(Walker)" baseAuthorityName="Walker" box="[470,707,1341,1368]" class="Insecta" family="Tortricidae" genus="Isotenes" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="miserana">
