<taxonomicName id="FD029AC7318459B4E2F4F61576AD3EE6" class="Insecta" family="Cerambycidae" genus="Sciades" kingdom="Animalia" order="Coleoptera" pageNumber="31" phylum="Arthropoda" rank="species" species="guatemalensis">S. guatemalensis</taxonomicName>
, 
