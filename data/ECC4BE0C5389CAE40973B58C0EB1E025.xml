<taxonomicName id="C986C20F594F455152A69EB9F2242431" LSID-ZBK="urn:lsid:zoobank.org:act:4A7D104C-F38B-4AFB-8C4A-A3838C172BB0" class="Actinopterygii" family="Ariidae" genus="Brustiarius" kingdom="Animalia" lsidName-ZBK="Brustiarius Arius Herre 1935:388" order="Siluriformes" pageNumber="31" phylum="Chordata" rank="genus">Brustiarius</taxonomicName>
