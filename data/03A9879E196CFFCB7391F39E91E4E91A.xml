<number id="3DC824AF196DFFCC7706F1D89529EA5C" box="[1111,1124,237,261]" pageId="3" pageNumber="219" value="1.0">1</number>
(
<number id="3DC824AF196DFFCC7722F1D895C3EA5C" box="[1139,1166,237,261]" pageId="3" pageNumber="219" value="31.0">31</number>
