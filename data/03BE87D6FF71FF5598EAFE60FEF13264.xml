<quantity id="4CEF9B25FF71FF559C33FE60FED131A8" box="[1102,1168,402,429]" metricMagnitude="-1" metricUnit="m" metricValue="5.2" pageId="224" pageNumber="225" unit="cm" value="52.0">52 cm</quantity>
