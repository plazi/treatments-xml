<quantity id="4CEF9B25FF71FF559CE0FE60FEA831AB" box="[1181,1257,402,430]" metricMagnitude="-1" metricUnit="m" metricValue="5.207" pageId="224" pageNumber="225" unit="in" value="20.5">20.5 in</quantity>
