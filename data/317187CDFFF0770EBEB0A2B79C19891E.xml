<taxonomicName id="7ED84D58FFF0770EBC34A2FF9A598ADE" authority="Linnaeus" authorityName="Linnaeus" box="[531,823,222,247]" class="Aves" family="Scolopacidae" genus="Tringa" kingdom="Animalia" order="Charadriiformes" pageId="28" pageNumber="29" phylum="Chordata" rank="species" species="totanus">
