<taxonomicName id="019D8EB2FFF32121009F29F9EDDC4DC4" box="[763,924,698,721]" class="Malacostraca" family="Palaemonidae" genus="Stegopontonia" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="463" phylum="Arthropoda" rank="genus">
