<emphasis id="78F0F50EFFB2F32C7112FA1FD554FA54" box="[318,608,1480,1505]" italics="true" pageId="64" pageNumber="65">
<collectingMunicipality id="AA5FB366FFB2F32C7112FA1FD6A6FA54" box="[318,402,1480,1504]" pageId="64" pageNumber="65">Lengths</collectingMunicipality>
