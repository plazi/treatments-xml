<collectorName id="00307CA8FF85FFCCFE53BD3EFE72FEF6" box="[427,469,332,356]" pageId="31" pageNumber="32">Sge</collectorName>
<specimenCode id="FD63B105FF85FFCCFE26BD3EFDBFFEF6" box="[478,536,332,356]" collectionCode="II" pageId="31" pageNumber="32">II 20</specimenCode>
