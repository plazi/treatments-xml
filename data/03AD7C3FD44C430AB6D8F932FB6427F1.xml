<emphasis id="B970113BD44C4304B6D8F932FD4D24AA" bold="true" box="[151,654,1665,1691]" pageId="38" pageNumber="339">
<taxonomicName id="4C04B6AAD44C4304B6D8F932FD8724AA" box="[151,580,1665,1691]" class="Insecta" family="Ichneumonidae" genus="Netelia" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="339" phylum="Arthropoda" rank="species" species="yakushimensis" status="n. sp." subGenus="Bessobates">
<emphasis id="B970113BD44C4304B6D8F932FF2E24AA" bold="true" box="[151,237,1665,1691]" italics="true" pageId="38" pageNumber="339">Netelia</emphasis>
(
<emphasis id="B970113BD44C4304B6B1F932FE4124AA" bold="true" box="[254,386,1665,1691]" italics="true" pageId="38" pageNumber="339">Bessobates</emphasis>
