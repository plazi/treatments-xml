(Subgenus 
<taxonomicName id="95A84D6CFFF12617FCA2FEC9FC456B1C" box="[864,948,266,292]" class="Liliopsida" family="Zingiberaceae" genus="Globba" kingdom="Plantae" order="Zingiberales" pageId="16" pageNumber="155" phylum="Tracheophyta" rank="genus">
