<taxonomicName id="95A84D6CFFF12617FED3F9D9FE216C0C" authorityName="Sangvir. &amp; M. F. Newman" authorityYear="2021" box="[273,464,1562,1588]" class="Liliopsida" family="Zingiberaceae" genus="Globba" kingdom="Plantae" order="Zingiberales" pageId="16" pageNumber="155" phylum="Tracheophyta" rank="species" species="rupestris">
