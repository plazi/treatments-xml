<emphasis id="FED4EAE32F1EFF97D4155841FCC4FABC" box="[867,892,1352,1376]" italics="true" pageId="10" pageNumber="311">
z 
<number id="7A6824D62F1EFF97D4185841FCC4FABC" box="[878,892,1352,1376]" pageId="10" pageNumber="311" value="4.0">4</number>
</emphasis>
(
<number id="7A6824D62F1EFF97D4F9584EFC14FABC" box="[911,940,1351,1376]" pageId="10" pageNumber="311" value="53.0">53</number>
