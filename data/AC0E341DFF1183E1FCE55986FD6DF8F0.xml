<collectionCode id="42B61DCEFF1183E1FE0A5CC3FE70FB40" box="[462,480,1187,1211]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="172" pageNumber="172" type="Herbarium">G</collectionCode>
