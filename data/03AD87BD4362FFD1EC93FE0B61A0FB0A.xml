<taxonomicName id="4C044D284362FFD1EE1BFB2C61DEFB78" box="[543,748,1153,1178]" class="Malacostraca" family="Aeglidae" genus="Aegla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="23" phylum="Arthropoda" rank="species" species="intercalata">
