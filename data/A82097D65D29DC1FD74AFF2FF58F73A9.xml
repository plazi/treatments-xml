<geoCoordinate id="805B71E90B1110972A01A768B42F2758" direction="north" orientation="latitude" precision="5" value="47.8134">47.8134°N</geoCoordinate>
, 
<geoCoordinate id="002A55FD2201FF7295176B4FCDF7B817" direction="west" orientation="longitude" precision="5" value="-66.7579">66.7579°W</geoCoordinate>
