<caption id="DF7EE18E77E1FF1AFF14A640935DFAE4" httpUri="https://zenodo.org/record/273064/files/figure.png" pageId="182" pageNumber="183" targetBox="[151,1436,193,1301]" targetPageId="182">
<paragraph id="8BBEB10677E1FF1AFF14A640935DFAE4" blockId="182.[151,1436,1323,1406]" pageId="182" pageNumber="183">
