<caption id="DF7A2EDEBF23FFDD79F3F764F783D465" ID-DOI="http://doi.org/10.5281/zenodo.6331197" ID-Zenodo-Dep="6331197" httpUri="https://zenodo.org/record/6331197/files/figure.png" inLine="true" pageId="20" pageNumber="431" targetBox="[1408,1996,2186,2600]" targetPageId="20">
