<taxonomicName id="4C4C98F42277FFF76E9EF94E6A885281" authorityName="T. Scott" authorityYear="1894" box="[659,916,1694,1720]" class="Maxillopoda" family="Ancorabolidae" genus="Laophontodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Harpacticoida" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="typicus">
