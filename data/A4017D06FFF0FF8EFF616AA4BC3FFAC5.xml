. OAD 
<quantity id="EB5061F5FFF3FF8EFF616DF6B965FE6D" box="[151,316,441,467]" metricMagnitude="-4" metricUnit="m" metricValue="4.05" metricValueMax="4.9" metricValueMin="3.2" pageId="39" pageNumber="40" unit="mm" value="0.405" valueMax="0.49" valueMin="0.32">0.32–0.49 mm</quantity>
. IOD 
<quantity id="EB5061F5FFF3FF8EFE8A6DF6BA78FE6D" box="[380,545,441,467]" metricMagnitude="-4" metricUnit="m" metricValue="1.9" metricValueMax="2.1" metricValueMin="1.7" pageId="39" pageNumber="40" unit="mm" value="0.19" valueMax="0.21" valueMin="0.17">0.17–0.21 mm</quantity>
. OOD 
<quantity id="EB5061F5FFF3FF8EFD9B6DF7BB48FE6D" box="[621,785,440,467]" metricMagnitude="-4" metricUnit="m" metricValue="2.05" metricValueMax="2.5" metricValueMin="1.6" pageId="39" pageNumber="40" unit="mm" value="0.205" valueMax="0.25" valueMin="0.16">0.16–0.25 mm</quantity>
. GW 
<quantity id="EB5061F5FFF3FF8EFCB86DF6BBBCFE6D" box="[846,997,441,467]" metricMagnitude="-4" metricUnit="m" metricValue="2.1" metricValueMax="3.2" metricValueMin="1.0" pageId="39" pageNumber="40" unit="mm" value="0.21" valueMax="0.32" valueMin="0.1">0.1–0.32 mm</quantity>
