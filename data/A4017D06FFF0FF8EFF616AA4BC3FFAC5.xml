<quantity id="EB5061F5FFF2FF8FFCAE6AACBBE5F943" box="[856,956,1763,1789]" metricMagnitude="-3" metricUnit="m" metricValue="4.99" pageId="38" pageNumber="39" unit="mm" value="4.99">4.99 mm</quantity>
