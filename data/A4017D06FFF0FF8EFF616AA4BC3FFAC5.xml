. ML 
<quantity id="EB5061F5FFF3FF8EFF316D92B935FE49" box="[199,364,477,503]" metricMagnitude="-4" metricUnit="m" metricValue="1.35" metricValueMax="1.6" metricValueMin="1.1" pageId="39" pageNumber="40" unit="mm" value="0.135" valueMax="0.16" valueMin="0.11">0.11–0.16 mm</quantity>
