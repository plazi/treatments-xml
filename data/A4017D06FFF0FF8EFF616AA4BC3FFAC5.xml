<quantity id="EB5061F5FFF3FF8EFE6E68BBB9ADFAB1" box="[408,500,1268,1295]" metricMagnitude="3" metricUnit="m" metricValue="1.1949999999999998" pageId="39" pageNumber="40" unit="m" value="1195.0">1,195 m</quantity>
