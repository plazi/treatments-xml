<quantity id="EB5061F5FFF2FF8FFB7C6AACBCA8F943" box="[1162,1265,1763,1789]" metricMagnitude="-4" metricUnit="m" metricValue="3.3" pageId="38" pageNumber="39" unit="mm" value="0.33">0.33 mm</quantity>
