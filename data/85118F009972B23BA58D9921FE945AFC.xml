<quantity id="CA4093F39971B23BA01D9F6CFAB35D37" box="[1287,1365,536,559]" metricMagnitude="-3" metricUnit="m" metricValue="8.2" pageId="2" pageNumber="67" unit="mm" value="8.2">8.2mm</quantity>
