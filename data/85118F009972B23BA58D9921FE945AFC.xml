<typeStatus id="D20380B49971B23BA7B19F6CFCE15D37" box="[683,775,536,559]" pageId="2" pageNumber="67">paratype</typeStatus>
<specimenCount id="1BBEF59F9971B23BA6149F63FCC65D31" box="[782,800,535,553]" pageId="2" pageNumber="67" type="male">♂</specimenCount>
