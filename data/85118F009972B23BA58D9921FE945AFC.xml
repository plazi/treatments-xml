<quantity id="CA4093F39971B23BA66D9F6CFC235D37" box="[887,965,536,559]" metricMagnitude="-3" metricUnit="m" metricValue="6.1" pageId="2" pageNumber="67" unit="mm" value="6.1">6.1mm</quantity>
; 
<typeStatus id="D20380B49971B23BA6CA9F6CFBD05D37" box="[976,1078,536,559]" pageId="2" pageNumber="67">paratypes</typeStatus>
<specimenCount id="1BBEF59F9971B23BA1279F63FBA95D31" box="[1085,1103,535,553]" pageId="2" pageNumber="67" type="female">♀</specimenCount>
