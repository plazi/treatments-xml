<quantity id="CA4093F39971B23BA4AC9F4CFDE25D57" box="[438,516,568,591]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="2" pageNumber="67" unit="mm" value="4.4">4.4mm</quantity>
