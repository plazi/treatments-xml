<collectingCountry id="BAA5764F9E201971FECCFBFEFE1CFBDC" box="[359,426,1015,1040]" name="China" pageId="51" pageNumber="52">China</collectingCountry>
(
<collectingRegion id="0076F83D9E201971FE13FBF1FDA7FBDC" box="[440,529,1016,1040]" country="China" name="Yunnan" pageId="51" pageNumber="52">Yunnan</collectingRegion>
) (
<figureCitation id="5A892A5A9E201971FD83FBFEFDDBFBDC" box="[552,621,1015,1040]" captionStart="FIGURE 3" captionStartId="7.[151,250,1701,1723]" captionTargetBox="[340,1247,483,1661]" captionTargetId="figure@7.[340,1247,483,1679]" captionTargetPageId="7" captionText="FIGURE 3. Distribution map of Eupolyphaga species only found in Yunnan (larger circles indicate the type localities)." httpUri="https://zenodo.org/record/2606726/files/figure.png" pageId="51" pageNumber="52">Fig. 3</figureCitation>
