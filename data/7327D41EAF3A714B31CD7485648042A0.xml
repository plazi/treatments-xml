<taxonomicName id="DF5F4747DB962C0CD64D8D5802ACDBEB" class="Insecta" family="Tettigoniidae" genus="Saga" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Saga rhodiensis" order="Orthoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="rhodiensis">Saga rhodiensis</taxonomicName>
