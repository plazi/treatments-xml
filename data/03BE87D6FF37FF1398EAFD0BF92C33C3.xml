<quantity id="4CEF9B25FF37FF139915FC5EFBFC33C2" box="[360,445,940,967]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="2.0" metricValueMin="0.1" pageId="166" pageNumber="167" unit="m" value="10.5" valueMax="20.0" valueMin="1.0">1–20 m</quantity>
(
<quantity id="4CEF9B25FF37FF1399B1FC5EF86133C2" box="[460,544,940,967]" metricMagnitude="1" metricUnit="m" metricValue="1.05156" metricValueMax="2.01168" metricValueMin="0.09144" pageId="166" pageNumber="167" unit="ft" value="34.5" valueMax="66.0" valueMin="3.0">3–66 ft</quantity>
) (
<bibRefCitation id="EF864B31FF37FF139A48FC5EF92033C3" author="Robertson, D. R. &amp; Allen, G. R." box="[565,865,940,966]" pageId="166" pageNumber="167" refId="ref316713" refString="Robertson, D. R. &amp; Allen, G. R. (2002) Shorefishes of the tropical eastern Pacific: an information system. CD-ROM. Smithsonian Tropical Research Institute, Balboa, Panama." type="book" year="2002">Robertson and Allen 2002</bibRefCitation>
