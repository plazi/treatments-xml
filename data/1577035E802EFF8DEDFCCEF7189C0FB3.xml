<taxonomicName id="5ADEC9CB802EFF8DECF6CF0F1BD00F43" authorityName="White, Last &amp; Stevens" authorityYear="2007" box="[413,653,294,319]" class="Elasmobranchii" family="Squalidae" genus="Squalus" kingdom="Animalia" order="Squaliformes" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="edmundsi">
