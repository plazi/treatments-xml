<geoCoordinate id="AE5767FF01DFE06970DAA39704ADBCC6" direction="north" orientation="latitude" precision="1" value="34.07509">34.07509°N</geoCoordinate>
, 
<geoCoordinate id="3D63468ECB75505BAD17FA484318B6A1" direction="west" orientation="longitude" precision="1" value="-118.95508">118.95508°W</geoCoordinate>
