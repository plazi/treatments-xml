93. 
<taxonomicName id="49B6FAC323772CCE8C5528E53116D2F4" LSID="urn:lsid:zoobank.org:act:AD26DAEC-6157-4ECA-9B7D-2B6E4968C677" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus uniformis" order="Coleoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="uniformis">Trigonopterus uniformis Riedel</taxonomicName>
<taxonomicNameLabel id="686A0A6CC9797F614EBCBA89B66DC292" pageId="81" pageNumber="82">sp. n.</taxonomicNameLabel>
