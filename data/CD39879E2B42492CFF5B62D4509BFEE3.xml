<collectingRegion id="8754F86A2B42492EFD2967FA5064F810" box="[741,804,2005,2031]" country="Yemen" name="'Adan" pageId="41" pageNumber="42">Aden</collectingRegion>
, 
<collectingCountry id="3D8776182B42492EFCE267FA50D3F810" box="[814,915,2005,2031]" name="Armenia" pageId="41" pageNumber="42">Armenia</collectingCountry>
, 
<collectingCountry id="3D8776182B42492EFC5367FA50A5F810" box="[927,997,2005,2031]" name="Egypt" pageId="41" pageNumber="42">Egypt</collectingCountry>
, 
<collectingCountry id="3D8776182B42492EFC2367FA5770F810" box="[1007,1072,2005,2031]" name="Israel" pageId="41" pageNumber="42">Israel</collectingCountry>
, 
<collectingCountry id="3D8776182B42492EFBF067FA57F4F810" box="[1084,1204,2005,2031]" name="Zimbabwe" pageId="41" pageNumber="42">Zimbabwe</collectingCountry>
(
<collectingCountry id="3D8776182B42492EFB0E67FA566EF80F" box="[1218,1326,2005,2032]" name="Zimbabwe" pageId="41" pageNumber="42">Rhodesia</collectingCountry>
