</taxonomicName>
(as 
<taxonomicName id="4C034D26FFC43503FB8AFB5F236ADBF8" box="[1124,1385,1158,1183]" class="Liliopsida" family="Poaceae" genus="Pentameris" kingdom="Plantae" order="Poales" pageId="2" pageNumber="303" phylum="Tracheophyta" rank="species" species="macrantha">
