<taxonomicName id="4C7E3B83FFC8FF8FFAB8366B9E2D5689" authority="Morawitz, 1876" authorityName="Morawitz" authorityYear="1876" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="aulica">
