<taxonomicName id="4C7E3B83FFC8FF8FFEB336A39EFA56F5" authorityName="Morawitz" authorityYear="1876" box="[313,414,1977,2004]" class="Insecta" family="Andrenidae" genus="Andrena" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="aulica">
<emphasis id="B90A9C12FFC8FF8FFEB336A39EFA56F5" box="[313,414,1977,2004]" italics="true" pageId="38" pageNumber="39">A. aulica</emphasis>
</taxonomicName>
