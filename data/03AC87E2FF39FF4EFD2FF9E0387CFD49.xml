<specimenCount id="9D03FD7DFF38FF4EFD8DFDE73996FD30" pageId="131" pageNumber="131" type="generic" typeStatus="paratypes">26 paratypes</specimenCount>
in AMNH: 
<emphasis id="B971EAE6FF38FF4EFE45FDF939B5FD15" bold="true" pageId="131" pageNumber="131">AMNH 830294– 830319</emphasis>
, 
<specimenCount id="9D03FD7DFF38FF4EFF1AFD1B3835FD16" box="[229,338,668,690]" pageId="131" pageNumber="131" type="male">15 males</specimenCount>
, 
<specimenCount id="9D03FD7DFF38FF4EFE9CFD1B389CFD16" box="[355,507,668,690]" pageId="131" pageNumber="131" type="female">four females</specimenCount>
