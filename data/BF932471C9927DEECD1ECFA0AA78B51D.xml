3. 
<taxonomicName id="F31A4A7F04A03A303EB8941F16B9EF79" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:144901" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="TETRAMORIUM" kingdom="Animalia" lsidName-HNS="Tetramorium (Xiphomyrmex) bessonii Forel" order="Hymenoptera" pageNumber="156" phylum="Arthropoda" rank="species" species="bessonii" status="n. sp." subGenus="XIPHOMYRMEX">TETRAMORIUM (XIPHOMYRMEX) BESSONII</taxonomicName>
, 
<taxonomicNameLabel id="4DD821713B0BE254E88957ED68BE9199" rank="species">n. sp.</taxonomicNameLabel>
</paragraph>
