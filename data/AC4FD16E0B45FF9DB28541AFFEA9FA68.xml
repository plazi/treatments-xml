<quantity id="E31ECD9D0B45FF9DB1424567FDC8FBE9" box="[592,635,1039,1063]" metricMagnitude="0" metricUnit="m" metricValue="5.0" pageId="8" pageNumber="1808" unit="m" value="5.0">5 m</quantity>
