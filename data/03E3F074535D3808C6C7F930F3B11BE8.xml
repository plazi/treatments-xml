<paragraph id="8BF5416253433809C718FB40F6691E84" pageId="16" pageNumber="317">
<table id="F94AB3C25343C7E6C6CFFB40F7791E84" box="[159,1397,1174,1464]" gridcols="4" gridrows="6" pageId="16" pageNumber="317">
<tr id="357A43205343C7E6C6CFFB40F7791F90" box="[159,1397,1174,1196]" gridrow="0" pageId="16" pageNumber="317" rowspan-0="1">
<th id="76AB2A5C5343C7E6C718FB40F0161F90" box="[328,538,1174,1196]" gridcol="1" gridrow="0" pageId="16" pageNumber="317">Fristedt, 1887</th>
<th id="76AB2A5C5343C7E6C415FB40F1D01F90" box="[581,988,1174,1196]" gridcol="2" gridrow="0" pageId="16" pageNumber="317">Koltun, 1966</th>
<th id="76AB2A5C5343C7E6C251FB40F7791F90" box="[1025,1397,1174,1196]" gridcol="3" gridrow="0" pageId="16" pageNumber="317">This study (CMNI 2018-0183)</th>
</tr>
<tr id="357A43205343C7E6C6CFFB16F7791FEA" box="[159,1397,1216,1238]" gridrow="1" pageId="16" pageNumber="317">
<th id="76AB2A5C5343C7E6C6CFFB16F3171FEA" box="[159,283,1216,1238]" gridcol="0" gridrow="1" pageId="16" pageNumber="317">Large oxeas</th>
<td id="76AB2A5C5343C7E6C718FB16F0161FEA" box="[328,538,1216,1238]" gridcol="1" gridrow="1" pageId="16" pageNumber="317">3500–5000</td>
<td id="76AB2A5C5343C7E6C415FB16F1D01FEA" box="[581,988,1216,1238]" gridcol="2" gridrow="1" pageId="16" pageNumber="317">2000–6000 x 32–50</td>
<td id="76AB2A5C5343C7E6C251FB16F7791FEA" box="[1025,1397,1216,1238]" gridcol="3" gridrow="1" pageId="16" pageNumber="317">1784–2811–4080 x 28–37–54</td>
</tr>
<tr id="357A43205343C7E6C6CFFB3FF7791FC3" box="[159,1397,1257,1279]" gridrow="2" pageId="16" pageNumber="317">
<th id="76AB2A5C5343C7E6C6CFFB3FF3171FC3" box="[159,283,1257,1279]" gridcol="0" gridrow="2" pageId="16" pageNumber="317">Small oxeas</th>
