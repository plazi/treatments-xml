<number id="3D86F2185D53FF5CFBA0FF43FB72FF22" box="[1122,1214,189,215]" pageId="11" pageNumber="12" value="38.198">38.198</number>
’ N
</geoCoordinate>
<geoCoordinate id="EE7A86F85D53FF5CFB2BFF43FA54FF22" box="[1257,1432,189,215]" direction="west" orientation="longitude" pageId="11" pageNumber="12" precision="1" value="-83.74841">
<number id="3D86F2185D53FF5CFB2BFF43FACBFF22" box="[1257,1287,189,215]" pageId="11" pageNumber="12" value="83.0">83</number>
° 
<number id="3D86F2185D53FF5CFAD3FF43FAA1FF22" box="[1297,1389,189,215]" pageId="11" pageNumber="12" value="44.905">44.905</number>
’ W
</geoCoordinate>
, 
<quantity id="4CB64DDA5D53FF5CFF55FF1CFF13FF09" box="[151,223,226,252]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="11" pageNumber="12" unit="m" value="500.0">500m</quantity>
, under limestone rock, Gimmel M. 
<date id="FFF0C6FF5D53FF5CFD51FF1CFC83FF09" box="[659,847,226,252]" pageId="11" pageNumber="12" value="2008-03-20">
<number id="3D86F2185D53FF5CFD51FF1CFD7DFF09" box="[659,689,226,252]" pageId="11" pageNumber="12" value="20.0">20</number>
March 
