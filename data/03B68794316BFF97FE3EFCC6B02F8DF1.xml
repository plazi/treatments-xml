<materialsCitation id="3B773CDF316BFF95FF1DFACBB0B38C20" box="[234,945,1306,1330]" collectionCode="PIN" pageId="9" pageNumber="21" specimenCode="PIN 4042" specimenCount="1" typeStatus="paratype">
