<specimenCode id="DBB99EF9316BFF95FD64FA8AB00A8C61" box="[659,776,1371,1395]" collectionCode="PIN" country="Russia" httpUri="http://grbio.org/cool/kjjc-ziy5" name="Paleontological Institute, Russian Academy of Sciences" pageId="9" pageNumber="21">PIN 3015</specimenCode>
