<subSubSection id="33FA550DF4677FC32138DE5ED5E36ECF" pageId="null" pageNumber="204" type="nomenclature">
<paragraph id="A6CEBC3244E268709F34F986E8B1FD4C" pageId="null" pageNumber="204">
<taxonomicName id="4822408532E759E62EB234BC0202BFBB" authority="L." class="Liliopsida" family="Potamogetonaceae" genus="Potamogeton" kingdom="Plantae" order="Alismatales" pageId="null" pageNumber="204" phylum="Tracheophyta" rank="species" species="pusillus">
<pageBreakToken id="F5162842C43D2D01A568FE4F058EC3CE" pageId="null" pageNumber="204" start="start">Potamogeton</pageBreakToken>
<normalizedToken id="0D438A9DC490F041E68D22FB927D5E41" originalValue="pusíllus" pageId="null" pageNumber="204">pusillus</normalizedToken>
<authorityName id="6E3B1A39C7941E80812F0F4970910832" pageId="null" pageNumber="204">L.</authorityName>
</taxonomicName>
