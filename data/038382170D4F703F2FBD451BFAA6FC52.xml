<subSubSection id="C330608A0D4E703F2FED43F8FCE2FC76" box="[199,857,942,969]" pageId="15" pageNumber="292" type="biology_ecology">
<paragraph id="8B9533010D4E703F2FED43F8FCE2FC76" blockId="15.[151,1437,151,1005]" box="[199,857,942,969]" pageId="15" pageNumber="292">
