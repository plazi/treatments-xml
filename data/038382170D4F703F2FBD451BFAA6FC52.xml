<collectingCountry id="F33D73910D4E703F2E4D43DDFE15FC1A" box="[359,430,907,933]" name="China" pageId="15" pageNumber="292">China</collectingCountry>
: 
<collectingRegion id="49EEFDE30D4E703F2E9243DDFDAAFC1A" box="[440,529,907,933]" country="China" name="Yunnan" pageId="15" pageNumber="292">Yunnan</collectingRegion>
