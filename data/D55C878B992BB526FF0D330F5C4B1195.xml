<quantity id="9A0D9B78992CB526FD8C36845D4912C6" box="[534,619,476,500]" metricMagnitude="3" metricUnit="m" metricValue="2.6" pageId="12" pageNumber="441" unit="m" value="2600.0">2600 m</quantity>
