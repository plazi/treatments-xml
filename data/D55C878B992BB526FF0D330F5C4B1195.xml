<quantity id="9A0D9B78992BB521FEAE320A5EAC1658" box="[308,398,1362,1386]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="11" pageNumber="440" unit="m" value="2300.0">2300 m</quantity>
(Schönwieskopf), Caricetum at 
<quantity id="9A0D9B78992BB521FC95320A5C4B1658" box="[783,873,1362,1386]" metricMagnitude="3" metricUnit="m" metricValue="2.6" pageId="11" pageNumber="440" unit="m" value="2600.0">2600 m</quantity>
(Hohe Mut), Androsacetum alpinae at 
<quantity id="9A0D9B78992BB521FADB320A5ABE1658" box="[1345,1436,1362,1386]" metricMagnitude="3" metricUnit="m" metricValue="2.9" pageId="11" pageNumber="440" unit="m" value="2900.0">2900 m</quantity>
(Liebener Rippe); previous studies: 
<quantity id="9A0D9B78992BB521FDB0322E5DE716BC" box="[554,709,1398,1422]" metricMagnitude="3" metricUnit="m" metricValue="1.97" metricValueMax="1.98" metricValueMin="1.96" pageId="11" pageNumber="440" unit="m" value="1970.0" valueMax="1980.0" valueMin="1960.0">1960–1980 m</quantity>
(hay meadows), pine forest at 
<quantity id="9A0D9B78992BB521FBBA322E5B5716BC" box="[1056,1141,1398,1422]" metricMagnitude="3" metricUnit="m" metricValue="2.05" pageId="11" pageNumber="440" unit="m" value="2050.0">2050 m</quantity>
(Zirbenwald), 
<quantity id="9A0D9B78992BB521FA84322E5F8F1680" metricMagnitude="3" metricUnit="m" metricValue="2.145" metricValueMax="2.19" metricValueMin="2.1" pageId="11" pageNumber="440" unit="m" value="2145.0" valueMax="2190.0" valueMin="2100.0">2100–2190 m</quantity>
(dwarf shrub community), 
<quantity id="9A0D9B78992BB521FE6B32C25DAD1680" box="[497,655,1434,1458]" metricMagnitude="3" metricUnit="m" metricValue="2.295" metricValueMax="2.34" metricValueMin="2.25" pageId="11" pageNumber="440" unit="m" value="2295.0" valueMax="2340.0" valueMin="2250.0">2250–2340 m</quantity>
(alpine meadows with lichen communities), 
<quantity id="9A0D9B78992BB521FB0532C25BDA1680" box="[1183,1272,1434,1458]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="11" pageNumber="440" unit="m" value="2500.0">2500 m</quantity>
(scree slope), 
<quantity id="9A0D9B78992BB521FF0D32E65E1016E4" box="[151,306,1470,1494]" metricMagnitude="3" metricUnit="m" metricValue="2.6" metricValueMax="2.65" metricValueMin="2.55" pageId="11" pageNumber="440" unit="m" value="2600.0" valueMax="2650.0" valueMin="2550.0">2550–2650 m</quantity>
(alpine meadows), 
<quantity id="9A0D9B78992BB521FD9432E65D8B16E4" box="[526,681,1470,1494]" metricMagnitude="3" metricUnit="m" metricValue="2.95" metricValueMax="3.1" metricValueMin="2.8" pageId="11" pageNumber="440" unit="m" value="2950.0" valueMax="3100.0" valueMin="2800.0">2800–3100 m</quantity>
