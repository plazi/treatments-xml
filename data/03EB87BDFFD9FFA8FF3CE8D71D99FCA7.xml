<quantity id="4CBA9B4EFFD8FFA8FC15EC86181EFD52" box="[958,1049,692,717]" metricMagnitude="3" metricUnit="m" metricValue="9.4" pageId="3" pageNumber="572" unit="km" value="9.4">9.4 km</quantity>
