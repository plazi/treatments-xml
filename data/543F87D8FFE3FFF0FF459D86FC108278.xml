<taxonomicName id="1B964D4DFFE3FFF0FF6E9D86FD6680B9" authority="Rao &amp; Kurian, 1950" authorityName="Rao &amp; Kurian" authorityYear="1950" box="[188,719,1240,1266]" class="Insecta" family="Braconidae" genus="Microplitis" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="435" phylum="Arthropoda" rank="species" species="ajmerensis">
<emphasis id="EEE2EADCFFE3FFF0FF6E9D86FE6380B9" bold="true" box="[188,458,1240,1266]" italics="true" pageId="6" pageNumber="435">Microplitis ajmerensis</emphasis>
Rao &amp; Kurian, 
<number id="6A5E24E9FFE3FFF0FD469D86FD6680B9" box="[660,719,1240,1266]" pageId="6" pageNumber="435" value="1950.0">1950</number>
