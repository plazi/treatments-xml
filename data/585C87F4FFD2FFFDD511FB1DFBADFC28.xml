; 
<materialsCitation id="609D3CBFFFD5FFFDD584FD7DFADAFCE8" collectingDate="2010-10-30" collectingDateMax="2017-07-12" collectingDateMin="2010-10-30" collectionCode="OR" country="Paraguay" location="Aquidaban River" municipality="Bella Vista del Norte" pageId="4" pageNumber="5" specimenCount="1" stateProvince="Amambay">
