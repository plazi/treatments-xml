<date id="A44B1022FFD5FFFDD2CDFD3DFA2BFC88" box="[1192,1484,762,787]" pageId="4" pageNumber="5" value="2017-04-26" valueMax="2017-07-12" valueMin="2017-04-26">
<collectingDate id="B40FE9CAFFD5FFFDD2CDFD3DFA2BFC88" box="[1192,1484,762,787]" pageId="4" pageNumber="5" value="2017-04-26" valueMax="2017-07-12" valueMin="2017-04-26">26 April and 12 July 2017</collectingDate>
