(
<figureCitation id="132DE7DB656DFF98FF16FE33FEC0FE2A" box="[159,297,428,454]" captionStart="FIGURE 1" captionStartId="4.[151,250,1972,1996]" captionTargetBox="[151,1435,561,1949]" captionTargetId="figure-192@4.[151,1435,561,1949]" captionTargetPageId="4" captionText="FIGURE 1. Eotrogaspidia spp., ♂, habitus. A, E. E. adhabar sp. nov., holotype; B, F. E. amans (André); C, G. E. auroguttata (Smith); D, H. E. buddha sp. nov., paratype; A–D. dorsal view; E–H. lateral view." figureDoi="http://doi.org/10.5281/zenodo.4471592" httpUri="https://zenodo.org/record/4471592/files/figure.png" pageId="3" pageNumber="59">Figs 1A, 1E</figureCitation>
, 
<figureCitation id="132DE7DB656DFF98FEBCFE33FEB3FE2B" box="[309,346,428,455]" captionStart="FIGURE 5" captionStartId="11.[151,250,1498,1522]" captionTargetBox="[151,1435,181,1473]" captionTargetId="figure-17@11.[151,1435,181,1473]" captionTargetPageId="11" captionText="FIGURE 5. Eotrogaspidia spp., ♂, face view. A. E. adhabar sp. nov., holotype; B. E. amans (André); C. E. auroguttata (Smith); D. E. buddha sp. nov., paratype." figureDoi="http://doi.org/10.5281/zenodo.4471602" httpUri="https://zenodo.org/record/4471602/files/figure.png" pageId="3" pageNumber="59">5A</figureCitation>
, 
<figureCitation id="132DE7DB656DFF98FEECFE33FE63FE2A" box="[357,394,428,454]" captionStart="FIGURE 7" captionStartId="16.[151,250,1738,1762]" captionTargetBox="[151,1435,181,1711]" captionTargetId="figure-17@16.[151,1435,181,1714]" captionTargetPageId="16" captionText="FIGURE 7. Eotrogaspidia and Trogaspidia spp., ♂, metacoxa. A. E. adhabar sp. nov., paratype; B. E. amans (André); C. E. auroguttata (Smith); D. E. buddha sp. nov., paratype; E. E. hauseri sp. nov., paratype; F. E. oryzae (Pagden); G. E. saussurei (Lelej); H. T. mackieae (Cockerell)." figureDoi="http://doi.org/10.5281/zenodo.4471608" httpUri="https://zenodo.org/record/4471608/files/figure.png" pageId="3" pageNumber="59">7A</figureCitation>
