<taxonomicName id="4C7FB0BDFFA9DE3DFECAF9934230F300" authority="Hendel" authorityName="Hendel" box="[365,617,1581,1607]" class="Insecta" family="Tephritidae" genus="Acroceratitis" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="417" phylum="Arthropoda" rank="genus">
