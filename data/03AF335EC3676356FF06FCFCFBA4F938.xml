<quantity id="4CFE2FADC3676356FB9DF9E4FB61F960" box="[1036,1202,1649,1675]" metricMagnitude="-3" metricUnit="m" metricValue="1.815" metricValueMax="1.84" metricValueMin="1.79" pageId="3" pageNumber="344" unit="mm" value="1.815" valueMax="1.84" valueMin="1.79">1.79–1.84 mm</quantity>
