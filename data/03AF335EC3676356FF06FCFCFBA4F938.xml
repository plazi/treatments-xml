<quantity id="4CFE2FADC3676356FE20F9E5FD9BF960" box="[433,584,1648,1675]" metricMagnitude="-2" metricUnit="m" metricValue="1.035" metricValueMax="1.1199999999999999" metricValueMin="0.95" pageId="3" pageNumber="344" unit="mm" value="10.35" valueMax="11.2" valueMin="9.5">9.5–11.2 mm</quantity>
, EL 
<quantity id="4CFE2FADC3676356FDEEF9E4FCDAF960" box="[639,777,1649,1675]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" metricValueMax="8.2" metricValueMin="7.8" pageId="3" pageNumber="344" unit="mm" value="8.0" valueMax="8.2" valueMin="7.8">7.8–8.2 mm</quantity>
, WH 
<quantity id="4CFE2FADC3676356FCDCF9E5FC04F960" box="[845,983,1648,1675]" metricMagnitude="-3" metricUnit="m" metricValue="3.4" metricValueMax="3.5" metricValueMin="3.3" pageId="3" pageNumber="344" unit="mm" value="3.4" valueMax="3.5" valueMin="3.3">3.3–3.5 mm</quantity>
