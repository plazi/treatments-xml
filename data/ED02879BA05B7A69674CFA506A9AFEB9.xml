<collectingCountry id="1DBC761DA0547A686733F9FA6906F90B" box="[288,359,1656,1680]" name="China" pageId="40" pageNumber="303">China</collectingCountry>
(
<collectingRegion id="A76FF86FA0547A68676AF9FA69BFF90B" box="[377,478,1656,1680]" country="China" name="Yunnan" pageId="40" pageNumber="303">Yunnan</collectingRegion>
