<taxonomicName id="DCE24D39A540FFEFFF4FF8798D7A65D2" authority="Zheng &amp; Xie" authorityName="Zheng &amp; Xie" box="[151,540,2013,2036]" class="Insecta" family="Baissogryllidae" genus="Caryandoides" kingdom="Animalia" order="Orthoptera" pageId="11" pageNumber="50" phylum="Arthropoda" rank="species" species="maguas">
