<taxonomicName id="4C554D06FFC76F7D84D7FAFBC4841855" authority="Spanish" authorityName="Spanish" box="[437,641,1364,1390]" class="Insecta" family="Glossosomatidae" genus="Culoptila" kingdom="Animalia" order="Trichoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="cascada">
