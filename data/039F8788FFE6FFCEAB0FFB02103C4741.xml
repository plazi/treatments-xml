<emphasis id="B942EA8CFFE6FFCEA8E3F97F134246FE" box="[503,631,1676,1702]" italics="true" pageId="6" pageNumber="7">
<taxonomicName id="4C364D1DFFE6FFCEA8E3F97F135F46FE" baseAuthorityName="Dreyfus" baseAuthorityYear="1889" box="[503,618,1676,1702]" class="Insecta" family="Adelgidae" genus="Pineus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="orientalis">orientalis</taxonomicName>
*
</emphasis>
, 
<taxonomicName id="4C364D1DFFE6FFCEAB91F97E13D246FE" box="[645,743,1677,1702]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="pungens">
<emphasis id="B942EA8CFFE6FFCEAB91F97E13D246FE" box="[645,743,1677,1702]" italics="true" pageId="6" pageNumber="7">pungens</emphasis>
