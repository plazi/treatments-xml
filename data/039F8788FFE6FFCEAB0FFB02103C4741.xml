<paragraph id="8B89369EFFE6FFCEA9A9F97F157346FE" blockId="6.[189,1094,1676,1703]" box="[189,1094,1676,1703]" pageId="6" pageNumber="7">
<taxonomicName id="4C364D1DFFE6FFCEA9A9F97F101F46FE" box="[189,298,1676,1702]" class="Pinopsida" family="Pinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
