<taxonomicName id="4C364D1DFFE6FFCEAAF9F97F151E46FE" box="[1005,1067,1676,1702]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="abies">
<emphasis id="B942EA8CFFE6FFCEAAF9F97F151E46FE" box="[1005,1067,1676,1702]" italics="true" pageId="6" pageNumber="7">abies</emphasis>
</taxonomicName>
