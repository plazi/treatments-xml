<taxonomicName id="4C364D1DFFE6FFCEABE2F97E127146FE" authorityName="Koehne" authorityYear="2010" baseAuthorityName="Siebold ex K. Koch" box="[758,836,1677,1702]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="torano">
<emphasis id="B942EA8CFFE6FFCEABE2F97E127146FE" box="[758,836,1677,1702]" italics="true" pageId="6" pageNumber="7">torano</emphasis>
</taxonomicName>
