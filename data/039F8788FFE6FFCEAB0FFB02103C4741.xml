<taxonomicName id="4C364D1DFFE6FFCEA821F97E10DC46FE" box="[309,489,1677,1702]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="jezoensis">
<emphasis id="B942EA8CFFE6FFCEA821F97E10DC46FE" box="[309,489,1677,1702]" italics="true" pageId="6" pageNumber="7">Picea jezoensis</emphasis>
</taxonomicName>
