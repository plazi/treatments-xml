<paragraph id="2114F6FDD1F865206E161C65873EB5B6" pageId="null" pageNumber="138">
Artengruppe der 
<taxonomicName id="4E5B4CAD426115C3147AFB42B8E0A32C" class="Magnoliopsida" family="Lamiaceae" genus="Satureja" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="138" phylum="Tracheophyta" rank="genus">Satureja</taxonomicName>
