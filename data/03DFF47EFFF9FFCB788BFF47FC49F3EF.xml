<taxonomicName id="4C763EEBFFF9FFCB7C3EFCB6FB34F3CB" authorityName="Ribaga" authorityYear="1904" box="[1058,1154,870,896]" class="Arachnida" family="Phytoseiidae" genus="Phytoseius" kingdom="Animalia" order="Mesostigmata" pageId="10" pageNumber="223" phylum="Arthropoda" rank="species" species="horridus">
