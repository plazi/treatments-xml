<typeStatus id="4E6566D8235AFFDFB79C5CDB127CFE82" box="[487,584,253,275]" pageId="22" pageNumber="1531">Holotype</typeStatus>
: 
<specimenCount id="87D813F3235AFFDFB4215D26125FFE84" box="[602,619,256,277]" pageId="22" pageNumber="1531" type="female">♀</specimenCount>
