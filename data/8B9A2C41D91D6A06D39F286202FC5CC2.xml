<taxonomicName id="CE0B5C0E32A4C416CB960CBE60B81EE8" authority="Fabricius, 1775" authorityName="Fabricius" authorityYear="1775" class="Insecta" family="Meloidae" genus="Apalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Apalus" order="Coleoptera" pageId="192" pageNumber="193" phylum="Arthropoda" rank="genus">Apalus</taxonomicName>
