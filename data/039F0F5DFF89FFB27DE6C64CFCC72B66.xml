(n = 
<number id="3DFEAC6CFF89FFB57CC1C722FE5F2DB4" box="[432,449,1914,1938]" pageId="19" pageNumber="20" value="9.0">9</number>
): HB = 
<number id="3DFEAC6CFF89FFB57F6AC722FDFA2DB4" box="[539,612,1914,1938]" pageId="19" pageNumber="20" value="74.5" valueMax="82.0" valueMin="67.0">67–82</number>
, T = 
<number id="3DFEAC6CFF89FFB57FEFC722FD6D2DB4" box="[670,755,1914,1938]" pageId="19" pageNumber="20" value="102.5" valueMax="112.0" valueMin="93.0">93–112</number>
, HF = 
<number id="3DFEAC6CFF89FFB57E31C722FC172DB4" box="[832,905,1914,1938]" pageId="19" pageNumber="20" value="21.5" valueMax="23.0" valueMin="20.0">20–23</number>
, E = 
<number id="3DFEAC6CFF89FFB57EB2C722FB952DB4" box="[963,1035,1914,1938]" pageId="19" pageNumber="20" value="16.0" valueMax="17.0" valueMin="15.0">15–17</number>
, W = 
<number id="3DFEAC6CFF89FFB5793EC722FB072DB4" box="[1103,1177,1914,1938]" pageId="19" pageNumber="20" value="17.0" valueMax="22.0" valueMin="12.0">12–22</number>
