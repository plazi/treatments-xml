<taxonomicName id="4C2C86C01548FF9BFC1ED34FFAA28916" authority="Moore, 1868" authorityName="Moore" authorityYear="1868" box="[940,1310,1032,1054]" class="Insecta" family="Geometridae" genus="Anisodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="260" phylum="Arthropoda" rank="species" species="sanguinaria">
