<collectionCode id="0699AE3FD7429D5AFC55F79610585E78" box="[1001,1052,2067,2091]" pageId="6" pageNumber="797">SVL</collectionCode>
<quantity id="A7709B1FD7429D5AFB97F796103A5E78" box="[1067,1150,2067,2091]" metricMagnitude="-2" metricUnit="m" metricValue="3.6" pageId="6" pageNumber="797" unit="mm" value="36.0">36 mm</quantity>
