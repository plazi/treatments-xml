<taxonomicName id="01C65574B1F074903ED3A110D47E4D3E" class="Demospongiae" family="Ianthellidae" genus="Hexadella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hexadella racovitzai" order="Verongiida" pageId="45" pageNumber="46" phylum="Porifera" rank="species" species="racovitzai">Hexadella racovitzai</taxonomicName>
