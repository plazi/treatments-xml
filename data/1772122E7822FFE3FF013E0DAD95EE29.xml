<taxonomicName id="58DBD8BB7822FFE3FEA33E97AC39EBAF" authority="de Saussure &amp; Zehntner, 1897" authorityName="de Saussure &amp; Zehntner" authorityYear="1897" box="[302,864,483,510]" class="Diplopoda" family="Spirobolidae" genus="Spirobolus" kingdom="Animalia" order="Spirobolida" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="rubripes">
