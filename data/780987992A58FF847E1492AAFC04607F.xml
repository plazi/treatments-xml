<taxonomicName id="37A04D0C2A58FF847FAC93FDFE0E64EC" authority="Gistel, 1848" authorityName="Gistel" authorityYear="1848" box="[191,453,1964,1987]" class="Insecta" family="Chrysomelidae" genus="Haplosonyx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="183" phylum="Arthropoda" rank="genus">
