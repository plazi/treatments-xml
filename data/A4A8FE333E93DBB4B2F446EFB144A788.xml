<paragraph id="29A8F782A24C6742B3BE11C1AEEFE705">
<taxonomicName id="6EE999A4D2ACDB0D0C3B73D96F9AF43A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145149" class="Insecta" family="Formicidae" genus="Tetramorium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetramorium (Xiphomyrmex) ranarum Forel" lsidName-HNS="Tetramorium (Xiphomyrmex) ranarum Forel" order="Hymenoptera" phylum="Arthropoda" rank="species" species="ranarum" subGenus="Xiphomyrmex">Tetramorium (Xiphomyrmex) ranarum Forel</taxonomicName>
