<taxonomicName id="15647DB1C10554951F2F4C58E174417A" class="Insecta" family="Leiodidae" genus="Oryotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oryotus" order="Coleoptera" pageId="94" pageNumber="95" phylum="Arthropoda" rank="genus">Oryotus</taxonomicName>
