<taxonomicName id="9C92ED9BFF9FFFDA4FACF8AE7FB2F8DC" box="[151,683,1886,1912]" class="Insecta" family="Braconidae" genus="Heterospilus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="205" phylum="Arthropoda" rank="species" species="alternicoloratus" status="sp. nov." subGenus="Heterospilus">
