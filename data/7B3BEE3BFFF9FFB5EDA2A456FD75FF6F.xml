<treatmentCitationGroup id="D3827803FFF9FFBAEDA2A4C3FDBBFA44" box="[151,597,1512,1534]" pageId="7" pageNumber="480">
<taxonomicName id="349224AEFFF9FFBAEDA2A4C3FDEAFA44" authority="Baker, 1924: 69" authorityName="Baker" authorityPageNumber="69" authorityYear="1924" box="[151,516,1512,1534]" class="Insecta" family="Cicadellidae" genus="Makilingia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="480" phylum="Arthropoda" rank="species" species="costalis">
