<taxonomicName id="81D24D4CFFBEFFAA00B9FC7BFB3BFC6C" authorityName="Henderson &amp; N. Q. Dung" authorityYear="2013" box="[971,1189,924,950]" class="Actinopterygii" family="Sparidae" genus="Calamus" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="21" phylum="Chordata" rank="species" species="quangngaiensis">C. quangngaiensis</taxonomicName>
</emphasis>
