<taxonomicName id="81D24D4CFFBEFFAA07CFFC7BFA02FC6C" authorityName="Henderson &amp; N. Q. Dung" authorityYear="2013" box="[1213,1436,924,950]" class="Actinopterygii" family="Sparidae" genus="Calamus" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="21" phylum="Chordata" rank="species" species="batoensis">
