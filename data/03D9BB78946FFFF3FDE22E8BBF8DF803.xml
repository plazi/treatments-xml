<taxonomicName id="4C7071ED946FFFF3FD8A2FF4BB1EFC3A" authority=", Lendenfeld" authorityName="Lendenfeld" box="[522,1343,949,976]" class="Demospongiae" family="Callyspongiidae" genus="Siphonochalina" kingdom="Animalia" order="Haplosclerida" pageId="58" pageNumber="479" phylum="Porifera" rank="species" species="confoederata">
<emphasis id="B904D67C946FFFF3FD8A2FF4BA0AFC3A" box="[522,1067,949,976]" italics="true" pageId="58" pageNumber="479">Siphonochalina confoederata</emphasis>
