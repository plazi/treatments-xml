<taxonomicName id="4C2F4D026322FF82A007F859FE7FF825" authority="Neto &amp; García Aldrete &amp; Rafael, 2018" authorityName="Neto &amp; García Aldrete &amp; Rafael" authorityYear="2018" box="[332,511,1963,1985]" class="Insecta" family="Ptiloneuridae" genus="Brasineura" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Psocodea" pageId="3" pageNumber="550" phylum="Arthropoda" rank="species" species="jiboia" status="sp.n.">
