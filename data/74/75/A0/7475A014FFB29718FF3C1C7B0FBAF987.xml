(
<taxonomicName authority="Dejean, 1833" authorityName="Dejean" authorityYear="1833" box="[408,536,1937,1958]" class="Insecta" family="Staphylinidae" genus="Microsaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
