<subSubSection type="reference_group">
<paragraph pageId="11" pageNumber="194" type="mainText">
<taxonomicName authority=", Goode" authorityName=", Goode" class="Actinopterygii" family="Paralichthyidae" genus="Citharichthys" kingdom="Animalia" order="Pleuronectiformes" pageId="11" pageNumber="194" phylum="Chordata" rank="species" species="arctifrons">
