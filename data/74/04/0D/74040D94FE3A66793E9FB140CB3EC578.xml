<taxonomicName class="Arachnida" family="Achipteriidae" genus="Parachipteria" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageNumber="357" phylum="Arthropoda" rank="genus">Parachipteria</taxonomicName>
; eine 
<normalizedToken originalValue="Begründung">Begruendung</normalizedToken>
<normalizedToken originalValue="dafür">dafuer</normalizedToken>
