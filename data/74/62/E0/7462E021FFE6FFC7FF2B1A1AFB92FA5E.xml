<taxonomicName authority="(Kunth)" baseAuthorityName="Kunth" box="[478,797,1091,1115]" class="Magnoliopsida" family="Siparunaceae" genus="Siparuna" kingdom="Plantae" order="Laurales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="lepidota">
