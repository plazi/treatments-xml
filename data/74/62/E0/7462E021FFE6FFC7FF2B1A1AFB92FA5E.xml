<taxonomicName authority="Juss. ex Lam." authorityName="Lam." box="[776,1202,1156,1180]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="laurifolia">
<emphasis box="[776,1029,1156,1180]" italics="true" pageId="8" pageNumber="7">Euphorbia laurifolia</emphasis>
Juss. ex Lam.
</taxonomicName>
(
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[1216,1400,1157,1181]" class="Magnoliopsida" family="Euphorbiaceae" kingdom="Plantae" order="Malpighiales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
