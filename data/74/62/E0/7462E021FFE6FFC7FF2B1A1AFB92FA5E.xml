<taxonomicName authority="(Ruiz and Pav.) Pers." authorityName="Pers." baseAuthorityName="Ruiz and Pav." box="[255,750,1189,1213]" class="Magnoliopsida" family="Asteraceae" genus="Baccharis" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="latifolia">
<emphasis box="[255,481,1189,1213]" italics="true" pageId="8" pageNumber="7">Baccharis latifolia</emphasis>
(Ruiz and Pav.) Pers.
</taxonomicName>
(
<taxonomicName authorityName="A.M.C.Dumeril" authorityYear="1805" box="[766,902,1189,1213]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
) (n = 1), and 
<taxonomicName authorityName="Hoerold" authorityYear="1909" box="[1067,1296,1189,1213]" class="Magnoliopsida" family="Ericaceae" genus="Psammisia" kingdom="Plantae" order="Ericales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="sodiroi">
<emphasis box="[1067,1296,1189,1213]" italics="true" pageId="8" pageNumber="7">Psammisia sodiroi</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Durande" authorityYear="1782" box="[1312,1432,1189,1213]" class="Magnoliopsida" family="Ericaceae" kingdom="Plantae" order="Ericales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Ericaceae</taxonomicName>
) (n = 1). 
<taxonomicName authority="Razowski and Pelz, 2004" authorityName="Razowski and Pelz" authorityYear="2004" box="[284,802,1222,1246]" class="Insecta" family="Tortricidae" genus="Sisurcana" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="topina">
<emphasis box="[284,494,1222,1245]" italics="true" pageId="8" pageNumber="7">Sisurcana topina</emphasis>
Razowski and Pelz, 2004
</taxonomicName>
