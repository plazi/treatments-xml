<taxonomicName authorityName="Schodde" authorityYear="1970" baseAuthorityName="A.DC." box="[895,1061,1091,1115]" class="Magnoliopsida" family="Siparunaceae" kingdom="Plantae" order="Laurales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Siparunaceae</taxonomicName>
