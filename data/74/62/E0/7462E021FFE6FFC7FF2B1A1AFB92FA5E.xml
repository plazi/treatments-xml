<taxonomicName authority="Razowski and Pelz, 2007" authorityName="Razowski and Pelz" authorityYear="2007" box="[886,1440,1026,1050]" class="Insecta" family="Tortricidae" genus="Sisurcana" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="fasciana">
<emphasis box="[886,1122,1026,1050]" italics="true" pageId="8" pageNumber="7">Sisurcana fasciana</emphasis>
Razowski and Pelz, 2007
</taxonomicName>
was reared from 
<taxonomicName box="[400,598,1058,1082]" class="Magnoliopsida" family="Piperaceae" genus="Piper" kingdom="Plantae" order="Piperales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="baezanum">
<emphasis box="[400,598,1058,1082]" italics="true" pageId="8" pageNumber="7">Piper baezanum</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Giseke" authorityYear="1792" box="[614,750,1059,1083]" class="Magnoliopsida" family="Piperaceae" kingdom="Plantae" order="Piperales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Piperaceae</taxonomicName>
) (n = 1), 
<taxonomicName authorityName="H.W.Schott" authorityYear="1829" box="[861,998,1058,1082]" class="Liliopsida" family="Araceae" genus="Anthurium" kingdom="Plantae" order="Alismatales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="genus">
<emphasis box="[861,998,1058,1082]" italics="true" pageId="8" pageNumber="7">Anthurium</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[1056,1156,1059,1083]" class="Liliopsida" family="Araceae" kingdom="Plantae" order="Alismatales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Araceae</taxonomicName>
) (n = 1), 
<taxonomicName authorityName="Aublet" authorityYear="1775" box="[1267,1398,1058,1082]" class="Magnoliopsida" family="Rubiaceae" genus="Palicourea" kingdom="Plantae" order="Gentianales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="genus">
<emphasis box="[1267,1398,1058,1082]" italics="true" pageId="8" pageNumber="7">Palicourea</emphasis>
</taxonomicName>
sp. (
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[200,327,1091,1115]" class="Magnoliopsida" family="Rubiaceae" kingdom="Plantae" order="Gentianales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Rubiaceae</taxonomicName>
