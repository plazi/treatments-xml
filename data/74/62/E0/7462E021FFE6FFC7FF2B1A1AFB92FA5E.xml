<taxonomicName class="Gastropoda" family="Triviidae" genus="Erato" kingdom="Animalia" order="Littorinimorpha" pageId="8" pageNumber="7" phylum="Mollusca" rank="species" species="polymnioides">
<emphasis italics="true" pageId="8" pageNumber="7">Erato polymnioides</emphasis>
</taxonomicName>
DC. (
<taxonomicName authorityName="A.M.C.Dumeril" authorityYear="1805" box="[422,558,1255,1279]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
) (n = 1), 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" box="[665,730,1255,1278]" class="Magnoliopsida" family="Piperaceae" genus="Piper" kingdom="Plantae" order="Piperales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="genus">
<emphasis box="[665,730,1255,1278]" italics="true" pageId="8" pageNumber="7">Piper</emphasis>
</taxonomicName>
sp. (
<taxonomicName authorityName="Giseke" authorityYear="1792" box="[787,923,1254,1278]" class="Magnoliopsida" family="Piperaceae" kingdom="Plantae" order="Piperales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Piperaceae</taxonomicName>
) (n = 1), 
<taxonomicName box="[1029,1227,1254,1278]" class="Magnoliopsida" family="Piperaceae" genus="Piper" kingdom="Plantae" order="Piperales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="species" species="baezanum">
<emphasis box="[1029,1227,1254,1278]" italics="true" pageId="8" pageNumber="7">Piper baezanum</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Giseke" authorityYear="1792" box="[1242,1378,1254,1278]" class="Magnoliopsida" family="Piperaceae" kingdom="Plantae" order="Piperales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Piperaceae</taxonomicName>
) (n = 1), and 
<taxonomicName authorityName="O.Swartz" authorityYear="1788" box="[284,397,1287,1311]" class="Magnoliopsida" family="Malvaceae" genus="Ochroma" kingdom="Plantae" order="Malvales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="genus">
<emphasis box="[284,397,1287,1311]" italics="true" pageId="8" pageNumber="7">Ochroma</emphasis>
</taxonomicName>
sp. (
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[455,587,1287,1311]" class="Magnoliopsida" family="Malvaceae" kingdom="Plantae" order="Malvales" pageId="8" pageNumber="7" phylum="Tracheophyta" rank="family">Malvaceae</taxonomicName>
) (n = 1). 
<taxonomicName authority="(Meyrick, 1926)" baseAuthorityName="Meyrick" baseAuthorityYear="1926" box="[698,1169,1287,1311]" class="Insecta" family="Tortricidae" genus="Sisurcana" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="umbellifera">
