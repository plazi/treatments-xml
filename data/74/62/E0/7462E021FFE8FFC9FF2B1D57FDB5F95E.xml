<taxonomicName authority="Cuatrec." authorityName="Cuatrec." box="[591,931,1541,1566]" class="Magnoliopsida" family="Aquifoliaceae" genus="Ilex" kingdom="Plantae" order="Aquifoliales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="yurumanguinis">
<emphasis box="[591,824,1541,1565]" italics="true" pageId="6" pageNumber="5">Ilex yurumanguinis</emphasis>
Cuatrec.
</taxonomicName>
(
<taxonomicName authorityName="Richard" authorityYear="1820" box="[943,1105,1541,1565]" class="Magnoliopsida" family="Aquifoliaceae" kingdom="Plantae" order="Aquifoliales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
