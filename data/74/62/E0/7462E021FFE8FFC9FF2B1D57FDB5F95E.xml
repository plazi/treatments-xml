, 
<taxonomicName authority="Mez and Donn. Sm." authorityName="Mez and Donn. Sm." box="[268,633,1309,1333]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="tenera">
<emphasis box="[268,382,1309,1332]" italics="true" pageId="6" pageNumber="5">O. tenera</emphasis>
Mez and Donn. Sm.
</taxonomicName>
, 
<taxonomicName authority="Woodson" authorityName="Woodson" box="[645,880,1309,1333]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="whitei">
<emphasis box="[645,758,1309,1333]" italics="true" pageId="6" pageNumber="5">O. whitei</emphasis>
Woodson
</taxonomicName>
, 
<taxonomicName authority="Mill." authorityName="Mill." box="[891,1182,1309,1333]" class="Magnoliopsida" family="Lauraceae" genus="Persea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="americana">
