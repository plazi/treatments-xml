<taxonomicName authorityName="P.J.Bergius" authorityYear="1767" box="[296,425,1508,1532]" class="Magnoliopsida" family="Lauraceae" genus="Nectandra" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="genus">
<emphasis box="[296,425,1508,1532]" italics="true" pageId="6" pageNumber="5">Nectandra</emphasis>
</taxonomicName>
sp. (
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[483,612,1508,1532]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
) (n = 1); 
<taxonomicName authority="Razowski and Wojtusiak, 2010" authorityName="Razowski and Wojtusiak" authorityYear="2010" box="[723,1274,1508,1532]" class="Insecta" family="Tortricidae" genus="Orthocomotis" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="parandina">
<emphasis box="[723,889,1508,1532]" italics="true" pageId="6" pageNumber="5">O. parandina</emphasis>
<bibRefCitation author="Razowski, J. &amp; J. Wojtusiak" box="[897,1274,1508,1532]" pageId="6" pageNumber="5" pagination="585 - 603" refId="ref8894" refString="Razowski, J., and J. Wojtusiak. 2010. Some Tortricidae from the East Cordillera in Ecuador reared from larvae in Yanayacu Biological Station in Ecuador (Insecta: Lepidoptera). Genus 21: 585 - 603." type="journal article" year="2010">Razowski and Wojtusiak, 2010</bibRefCitation>
