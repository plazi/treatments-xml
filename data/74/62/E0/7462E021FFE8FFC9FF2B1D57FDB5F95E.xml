<taxonomicName authority="(Busck, 1920)" baseAuthorityName="Busck" baseAuthorityYear="1920" class="Insecta" family="Tortricidae" genus="Orthocomotis" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="herbaria">
