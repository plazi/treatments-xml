<taxonomicName authority="Mez and Donn. Sm." authorityName="Mez and Donn. Sm." class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="atirrensis">
<emphasis italics="true" pageId="6" pageNumber="5">O. atirrensis</emphasis>
Mez and Donn. Sm.
</taxonomicName>
, 
<taxonomicName authority="(Nees) Mez" authorityName="Mez" authorityYear="1889" baseAuthorityName="Nees" box="[588,865,1276,1300]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="cernua">
<emphasis box="[588,711,1276,1299]" italics="true" pageId="6" pageNumber="5">O. cernua</emphasis>
