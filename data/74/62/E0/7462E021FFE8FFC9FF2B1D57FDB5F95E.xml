(all 
<taxonomicName authorityName="de Jussieu" authorityYear="1789" box="[646,783,1342,1366]" class="Magnoliopsida" family="Lauraceae" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Lauraceae</taxonomicName>
