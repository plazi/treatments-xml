, 
<taxonomicName authority="(Sw.) Mez" authorityName="Mez" authorityYear="1889" baseAuthorityName="Sw." box="[877,1185,1275,1300]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="floribunda">
<emphasis box="[877,1047,1275,1299]" italics="true" pageId="6" pageNumber="5">O. floribunda</emphasis>
(Sw.) Mez
</taxonomicName>
, 
<taxonomicName authority="(Rich.) Nees" authorityName="Nees" authorityYear="1836" baseAuthorityName="Rich." class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="puberula">
<emphasis box="[1197,1348,1275,1299]" italics="true" pageId="6" pageNumber="5">O. puberula</emphasis>
(Rich.) Nees
