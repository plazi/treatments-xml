<taxonomicName authority="Kunth" authorityName="Kunth" authorityYear="1822" class="Liliopsida" family="Poaceae" genus="Chusquea" kingdom="Plantae" order="Poales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="species" species="scandens">
<emphasis italics="true" pageId="6" pageNumber="5">Chusquea scandens</emphasis>
Kunth
</taxonomicName>
(
<taxonomicName authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[396,494,1542,1566]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
