<taxonomicName authority="(Meyrick, 1912)" baseAuthorityName="Meyrick" baseAuthorityYear="1912" box="[802,1106,647,671]" class="Insecta" family="Tortricidae" genus="Inape" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="iantha">
<emphasis box="[802,908,647,671]" italics="true" pageId="6" pageNumber="5">I. iantha</emphasis>
(Meyrick, 1912)
</taxonomicName>
from 
<taxonomicName authorityName="Ruiz &amp; Pavon" authorityYear="1794" box="[1176,1276,647,670]" class="Magnoliopsida" family="Melastomataceae" genus="Miconia" kingdom="Plantae" order="Myrtales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="genus">
<emphasis box="[1176,1276,647,670]" italics="true" pageId="6" pageNumber="5">Miconia</emphasis>
</taxonomicName>
sp. (
<taxonomicName authorityName="de Jussieu" authorityYear="1789" class="Magnoliopsida" family="Melastomataceae" kingdom="Plantae" order="Myrtales" pageId="6" pageNumber="5" phylum="Tracheophyta" rank="family">Melastomataceae</taxonomicName>
) (n = 1); 
<taxonomicName authority="Razowski and Pelz, 2006" authorityName="Razowski and Pelz" authorityYear="2006" box="[416,837,680,704]" class="Insecta" family="Tortricidae" genus="Inape" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="sororia">
