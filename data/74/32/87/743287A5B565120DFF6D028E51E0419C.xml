(
<number box="[1277,1291,367,391]" pageId="23" pageNumber="24" value="5.0">5</number>
exceptional, median 
<number box="[240,271,404,428]" pageId="23" pageNumber="24" value="12.0">12</number>
); pereopod 3, 1– 
<number box="[458,474,404,428]" pageId="23" pageNumber="24" value="4.0">4</number>
(median 
<number box="[579,594,404,428]" pageId="23" pageNumber="24" value="3.0">3</number>
); pereopod 
<number box="[729,745,404,428]" pageId="23" pageNumber="24" value="4.0">4</number>
, 
<number box="[756,798,404,428]" pageId="23" pageNumber="24" value="1.5" valueMax="3.0" valueMin="0.0">0–3</number>
(median 
<number box="[904,921,404,428]" pageId="23" pageNumber="24" value="2.0">2</number>
).
