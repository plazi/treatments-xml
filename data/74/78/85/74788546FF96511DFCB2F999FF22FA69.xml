<taxonomicName authority="(Jurine, 1820)" baseAuthorityName="Jurine" baseAuthorityYear="1820" box="[1043,1386,945,970]" class="Ostracoda" family="Cyprididae" genus="Eucypris" kingdom="Animalia" order="Podocopida" pageId="33" pageNumber="286" phylum="Arthropoda" rank="species" species="virens">
