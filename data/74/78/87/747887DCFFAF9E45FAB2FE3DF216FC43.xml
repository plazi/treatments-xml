<subSubSection pageId="6" pageNumber="167" type="diagnosis">
<paragraph blockId="6.[812,1403,288,381]" pageId="6" pageNumber="167">
<emphasis bold="true" box="[812,938,322,346]" pageId="6" pageNumber="167">Diagnosis.</emphasis>
