<subSubSection box="[184,1245,288,381]" pageId="6" pageNumber="167" type="etymology">
<paragraph blockId="6.[184,775,288,381]" box="[184,775,357,381]" pageId="6" pageNumber="167">
<emphasis bold="true" box="[184,318,357,381]" pageId="6" pageNumber="167">Etymology.</emphasis>
