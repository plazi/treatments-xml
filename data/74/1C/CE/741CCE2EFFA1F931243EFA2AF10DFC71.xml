<subSubSection box="[264,643,1673,1699]" pageId="18" pageNumber="19" type="description">
<paragraph blockId="18.[264,1324,1673,1899]" box="[264,643,1673,1699]" pageId="18" pageNumber="19">
<emphasis bold="true" box="[264,472,1673,1699]" pageId="18" pageNumber="19">Live appearance</emphasis>
