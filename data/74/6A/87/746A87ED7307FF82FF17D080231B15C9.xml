<subSubSection box="[199,1096,404,429]" pageId="17" pageNumber="80" type="etymology">
<paragraph blockId="17.[151,1436,151,429]" box="[199,1096,404,429]" pageId="17" pageNumber="80">
<emphasis bold="true" box="[199,334,404,429]" pageId="17" pageNumber="80">Etymology.</emphasis>
