<taxonomicName authorityName="Macquart" authorityYear="1848" box="[606,698,404,429]" class="Insecta" family="Tabanidae" genus="Tabanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="80" phylum="Arthropoda" rank="species" species="callosus">
<emphasis box="[606,698,404,429]" italics="true" pageId="17" pageNumber="80">callosus</emphasis>
