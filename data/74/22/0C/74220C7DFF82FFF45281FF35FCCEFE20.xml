<taxonomicName authorityName="Watts &amp; Cooper &amp; Saint" authorityYear="2017" box="[777,914,326,349]" class="Insecta" family="Scirtidae" genus="Scirtes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="527" phylum="Arthropoda" rank="species" species="rutai">
