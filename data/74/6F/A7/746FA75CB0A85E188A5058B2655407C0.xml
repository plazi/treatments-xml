<geoCoordinate degrees="27" direction="north" minutes="27.38" orientation="latitude" precision="9" value="27.456333">27°27.38'N</geoCoordinate>
, 
<geoCoordinate degrees="97" direction="east" minutes="13.65" orientation="longitude" precision="9" value="97.2275">97°13.65'E</geoCoordinate>
