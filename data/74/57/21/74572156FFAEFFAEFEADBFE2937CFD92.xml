.......................................................................................... 
<taxonomicName box="[1138,1226,324,349]" class="Insecta" family="Chrysomelidae" genus="Longitarsus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="24" phylum="Arthropoda" rank="species" species="igori" status="sp. nov.">L. igori</taxonomicName>
