<keyLead pageId="4" pageNumber="23">
<number box="[264,295,1724,1750]" pageId="4" pageNumber="23" value="13.0">13</number>
(
<number box="[305,335,1724,1750]" pageId="4" pageNumber="23" value="11.0">11</number>
