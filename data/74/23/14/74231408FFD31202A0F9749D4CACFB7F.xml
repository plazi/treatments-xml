<collectingCountry box="[359,480,1128,1152]" name="Democratic Republic of the Congo" pageId="5" pageNumber="456">DR Congo</collectingCountry>
(
<collectingCountry box="[494,602,1128,1152]" name="Ecuador" pageId="5" pageNumber="456">Equateur</collectingCountry>
