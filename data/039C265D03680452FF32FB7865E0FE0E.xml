<taxonomicName id="4C35ECC803770451FD47FF1663CFFF5D" box="[738,947,188,212]" class="Adenophorea" family="Plectidae" genus="Plectus" kingdom="Animalia" order="Araeolaimida" pageId="17" pageNumber="18" phylum="Nematoda" rank="species" species="magadani">
