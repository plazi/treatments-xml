<paragraph id="39607F30FFE8EA5E01F4FD8CFE4C1C50" blockId="13.[151,460,511,573]" box="[199,446,548,573]" pageId="13" pageNumber="90">
<collectingCountry id="41C83FA0FFE8EA5E01F4FD8CFEE21C51" box="[199,272,548,573]" name="Brazil" pageId="13" pageNumber="90">Brazil</collectingCountry>
