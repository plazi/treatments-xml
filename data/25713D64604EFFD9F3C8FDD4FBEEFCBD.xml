<taxonomicName id="6AD8F7F1604EFFD9F3C8FDD4FD5FFDC3" authority="KASPARYAN &amp; KOPELKE, 2009" authorityName="KASPARYAN &amp; KOPELKE" authorityYear="2009" box="[178,689,568,590]" class="Insecta" family="Ichneumonidae" genus="Saotis" kingdom="Animalia" order="Hymenoptera" pageId="21" pageNumber="1468" phylum="Arthropoda" rank="species" species="pygidiator">
