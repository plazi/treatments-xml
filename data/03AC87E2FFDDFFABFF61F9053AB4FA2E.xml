<location id="8EDA602FFFDDFFABFD64FD123B92FD0E" LSID="urn:lsid:plazi:treatment:03AC87E2FFDDFFABFF61F9053AB4FA2E:8EDA602FFFDDFFABFD64FD123B92FD0E" box="[667,757,661,682]" country="Australia" county="Fitzroy River" municipality="Mount Anderson" name="Rogers'" pageId="102" pageNumber="102">Rogers’</location>
label and 
<location id="8EDA602FFFDDFFABFC84FD123A81FD0F" LSID="urn:lsid:plazi:treatment:03AC87E2FFDDFFABFF61F9053AB4FA2E:8EDA602FFFDDFFABFC84FD123A81FD0F" box="[891,998,661,683]" country="Australia" county="Fitzroy River" municipality="Mount Anderson" name="Mathews" pageId="102" pageNumber="102">Mathews</location>
