(
<emphasis bold="true" box="[267,367,331,356]" italics="true" pageId="7" pageNumber="208">Coranus</emphasis>
) 
<emphasis bold="true" box="[382,442,331,356]" italics="true" pageId="7" pageNumber="208">niger</emphasis>
