<subSubSection pageId="null" pageNumber="347" type="reference_group">
<paragraph pageId="null" pageNumber="347">
(
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Alchemilla" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="347" phylum="Tracheophyta" rank="species" species="pratensis">
