<typeStatus box="[199,258,1073,1098]" pageId="1" pageNumber="343">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName box="[361,585,1073,1097]" class="Malacostraca" family="Leptocheliidae" genus="Brunarus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Tanaidacea" pageId="1" pageNumber="343" phylum="Arthropoda" rank="species" species="colekanus" status="sp. nov.">
<emphasis box="[361,585,1073,1097]" italics="true" pageId="1" pageNumber="343">Brunarus colekanus</emphasis>
