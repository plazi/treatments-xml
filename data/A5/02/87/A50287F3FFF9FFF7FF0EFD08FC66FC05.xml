</collectionCode>
</collectingCountry>
, 
<collectingCountry box="[382,472,762,787]" name="Uganda" pageId="20" pageNumber="21">Uganda</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:A50287F3FFF9FFF7FF0EFD08FC66FC05:2874603EFFF9FFF7FE7AFD7CFD75FCB5" box="[483,656,762,787]" country="Cameroon, CAR" municipality="western Kenya. The" name="southern Sudan" pageId="20" pageNumber="21" stateProvince="Sahel">
