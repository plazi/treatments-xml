<collectingCountry box="[1347,1436,726,751]" name="Senegal" pageId="20" pageNumber="21">Senegal</collectingCountry>
to 
<collectingCountry box="[180,370,763,787]" isEnumeration="true" pageId="20" pageNumber="21">
<collectingCountry box="[180,299,763,787]" name="Cameroon" pageId="20" pageNumber="21">Cameroon</collectingCountry>
