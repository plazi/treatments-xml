<collectingRegion box="[556,632,1335,1360]" country="Guinea-Bissau" name="Bafata" pageId="47" pageNumber="48">Bafatá</collectingRegion>
, 
<collectingRegion box="[644,689,1335,1360]" country="Guinea-Bissau" name="Oio" pageId="47" pageNumber="48">Oio</collectingRegion>
. 
