(
<bibRefCitation author="Fabricius" box="[448,625,546,571]" pageId="19" pageNumber="20" refString="Fabricius, J. C. (1775) Systema Entomologiae. Officina Libraria Kortii, Flensburgi (Flensburg - Germany) &amp; Lipsiae (Leipzig - Germany), [xxxii] + 832 pp." type="book" year="1775">Fabricius, 1775</bibRefCitation>
