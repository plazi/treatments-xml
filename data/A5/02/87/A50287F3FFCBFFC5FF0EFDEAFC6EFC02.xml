<collectingRegion box="[556,649,836,861]" country="Guinea-Bissau" name="Biombo" pageId="38" pageNumber="39">Biombo</collectingRegion>
, 
<collectingRegion box="[661,754,836,861]" country="Guinea-Bissau" name="Tombali" pageId="38" pageNumber="39">Tombali</collectingRegion>
. 
