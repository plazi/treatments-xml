WL: 
<quantity box="[257,332,1628,1652]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="24" pageNumber="25" unit="mm" value="13.0">13mm</quantity>
. 
<taxonomicName box="[343,459,1628,1653]" class="Insecta" family="Lycaenidae" genus="Anthene" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="larydas">
