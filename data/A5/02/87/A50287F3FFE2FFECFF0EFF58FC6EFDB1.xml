<location LSID="urn:lsid:plazi:treatment:A50287F3FFE2FFECFF0EFF58FC6EFDB1:2874603EFFE2FFECFB06FE85FB55FEBC" box="[1183,1200,259,282]" country="Senegal" name="It" pageId="15" pageNumber="16">It</location>
