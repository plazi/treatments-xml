<collectingCountry box="[225,295,332,357]" name="Sudan" pageId="48" pageNumber="49">Sudan</collectingCountry>
</collectingCountry>
and 
<collectingCountry box="[356,432,333,357]" name="Kenya" pageId="48" pageNumber="49">Kenya</collectingCountry>
