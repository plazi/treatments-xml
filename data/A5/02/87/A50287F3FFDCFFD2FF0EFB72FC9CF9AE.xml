<collectingCountry box="[1116,1197,1303,1328]" name="Guinea-Bissau" pageId="49" pageNumber="50">Guinea</collectingCountry>
savanna. 
<materialsCitation ID-GBIF-Occurrence="1423853029" country="Senegal" location="south Sudan." pageId="49" pageNumber="50" specimenCount="1" stateProvince="A">
It is known from 
<collectingCountry box="[214,303,1340,1365]" name="Senegal" pageId="49" pageNumber="50">Senegal</collectingCountry>
to 
<collectingCountry box="[342,427,1340,1365]" name="Angola" pageId="49" pageNumber="50">Angola</collectingCountry>
, eastwards to western 
<collectingCountry box="[686,918,1340,1365]" isEnumeration="true" pageId="49" pageNumber="50">
<collectingCountry box="[686,759,1341,1365]" name="Kenya" pageId="49" pageNumber="50">Kenya</collectingCountry>
and 
<collectingCountry box="[817,918,1340,1365]" name="Tanzania" pageId="49" pageNumber="50">Tanzania</collectingCountry>
</collectingCountry>
and 
<location LSID="urn:lsid:plazi:treatment:A50287F3FFDCFFD2FF0EFB72FC9CF9AE:2874603EFFDCFFD2FC49FABAFB86FAF3" box="[976,1123,1340,1365]" country="Senegal" name="south Sudan." pageId="49" pageNumber="50" stateProvince="A">
<collectingCountry box="[976,1119,1340,1365]" name="South Sudan" pageId="49" pageNumber="50">south Sudan</collectingCountry>
.
</location>
<collectingRegion box="[1131,1151,1340,1364]" country="Nigeria" name="Rivers" pageId="49" pageNumber="50">A</collectingRegion>
second subspecies
</materialsCitation>
, 
<materialsCitation ID-GBIF-Occurrence="1423853027" country="Ethiopia, Kenya" location="Rift valley" pageId="49" pageNumber="50" specimenCount="1" stateProvince="A">
<emphasis italics="true" pageId="49" pageNumber="50">
