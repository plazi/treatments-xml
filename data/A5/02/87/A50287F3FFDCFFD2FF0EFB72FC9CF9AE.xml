<collectingCountry box="[386,572,1375,1400]" isEnumeration="true" pageId="49" pageNumber="50">
<collectingCountry box="[386,484,1375,1400]" name="Ethiopia" pageId="49" pageNumber="50">Ethiopia</collectingCountry>
, 
<collectingCountry box="[496,572,1376,1400]" name="Kenya" pageId="49" pageNumber="50">Kenya</collectingCountry>
</collectingCountry>
, 
<collectingCountry box="[584,685,1375,1400]" name="Tanzania" pageId="49" pageNumber="50">Tanzania</collectingCountry>
