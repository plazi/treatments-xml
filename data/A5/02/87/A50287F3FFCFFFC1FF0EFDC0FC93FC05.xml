<collectingCountry box="[416,715,654,679]" isEnumeration="true" pageId="34" pageNumber="35">
<collectingCountry box="[416,535,655,679]" name="Cameroon" pageId="34" pageNumber="35">Cameroon</collectingCountry>
, 
<collectingRegion box="[548,625,655,679]" country="Republic of the Congo" name="Bas-Congo" pageId="34" pageNumber="35">
<collectingCountry box="[548,625,655,679]" name="Democratic Republic of the Congo" pageId="34" pageNumber="35">Congo</collectingCountry>
</collectingRegion>
, 
<collectingCountry box="[638,715,654,679]" name="Gabon" pageId="34" pageNumber="35">Gabon</collectingCountry>
</collectingCountry>
, 
<collectingCountry box="[727,859,655,679]" isEnumeration="true" pageId="34" pageNumber="35">
<collectionCode box="[727,787,655,679]" country="Venezuela" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13632" name="Museo de Historia Natural La Salle" pageId="34" pageNumber="35">
<collectingCountry box="[727,787,655,679]" name="Central African Republic" pageId="34" pageNumber="35">CAR</collectingCountry>
</collectionCode>
, 
<collectionCode box="[799,859,655,679]" pageId="34" pageNumber="35">
<collectingCountry box="[799,859,655,679]" name="Democratic Republic of the Congo" pageId="34" pageNumber="35">DRC</collectingCountry>
</collectionCode>
</collectingCountry>
, 
<collectingCountry box="[872,959,654,679]" name="Uganda" pageId="34" pageNumber="35">Uganda</collectingCountry>
and 
<location LSID="urn:lsid:plazi:treatment:A50287F3FFCFFFC1FF0EFDC0FC93FC05:2874603EFFCFFFC1FC6EFD08FB05FD01" box="[1015,1248,654,679]" country="Nigeria" name="southern Sudan. The" pageId="34" pageNumber="35" stateProvince="West">
