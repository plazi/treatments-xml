<collectionCode box="[228,288,656,680]" pageId="33" pageNumber="34">
<collectingCountry box="[228,288,656,680]" name="Democratic Republic of the Congo" pageId="33" pageNumber="34">DRC</collectingCountry>
</collectionCode>
, 
<collectingCountry box="[305,378,655,680]" name="Sudan" pageId="33" pageNumber="34">Sudan</collectingCountry>
, 
<collectingCountry box="[394,484,655,680]" name="Uganda" pageId="33" pageNumber="34">Uganda</collectingCountry>
