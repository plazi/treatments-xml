, 
<collectingRegion box="[669,746,975,1000]" country="Guinea-Bissau" name="Bissau" pageId="47" pageNumber="48">Bissau</collectingRegion>
, 
<collectingRegion box="[764,809,975,1000]" country="Guinea-Bissau" name="Oio" pageId="47" pageNumber="48">Oio</collectingRegion>
