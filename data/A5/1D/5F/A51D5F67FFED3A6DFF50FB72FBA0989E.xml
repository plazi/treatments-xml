<taxonomicName authorityName="Miranda &amp; Skevington &amp; Marshall" authorityYear="2020" baseAuthorityName="Macquart" baseAuthorityYear="1842" box="[1251,1365,1807,1833]" class="Insecta" family="Syrphidae" genus="Ocyptamus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="154" phylum="Arthropoda" rank="species" species="lepidus">
