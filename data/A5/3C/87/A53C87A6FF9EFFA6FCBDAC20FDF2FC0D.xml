<subSubSection pageId="8" pageNumber="677" type="discussion">
<paragraph blockId="8.[167,780,160,975]" pageId="8" pageNumber="677">
<emphasis box="[168,589,816,841]" inLineHeading="true" pageId="8" pageNumber="677" reason="1">
<emphasis bold="true" box="[168,279,817,841]" pageId="8" pageNumber="677">Remarks.</emphasis>
Two members of the genus
</emphasis>
<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Mammalia" family="Canidae" genus="Canis" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="677" phylum="Chordata" rank="genus">
<emphasis box="[598,778,816,840]" inLineHeading="true" pageId="8" pageNumber="677" reason="1">
<emphasis box="[598,661,817,840]" italics="true" pageId="8" pageNumber="677">Canis</emphasis>
Linnaeus,
</emphasis>
1758
</taxonomicName>
(
<taxonomicName baseAuthorityName="Say" baseAuthorityYear="1823" box="[230,338,850,874]" class="Mammalia" family="Canidae" genus="Canis" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="677" phylum="Chordata" rank="species" species="latrans">
<emphasis box="[230,338,850,874]" italics="true" pageId="8" pageNumber="677">C. latrans</emphasis>
</taxonomicName>
and Gray Wolf, 
<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[512,769,850,874]" class="Mammalia" family="Canidae" genus="Canis" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="677" phylum="Chordata" rank="species" species="lupus">
