<quantity box="[370,477,404,428]" metricMagnitude="1" metricUnit="m" metricValue="2.0" metricValueMax="4.0" metricValueMin="0.0" pageId="10" pageNumber="11" unit="m" value="20.0" valueMax="40.0" valueMin="0.0">0 to 40 m</quantity>
