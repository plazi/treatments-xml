</taxonomicName>
, 
<taxonomicName id="4C1741A1EB71FFC2FD9AEF7AFC8FFAFF" authorityName="Rossmassler" authorityYear="1837" box="[633,867,1355,1379]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="15" pageNumber="358" phylum="Mollusca" rank="species" species="elata" subGenus="Turricula">
<emphasis id="B963E630EB71FFC2FD9AEF7AFD5CFAFF" box="[633,688,1355,1379]" italics="true" pageId="15" pageNumber="358">Helix</emphasis>
(
