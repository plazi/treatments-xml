<taxonomicName id="4C1741A1EB71FFC2FC7EEF7AFAD7FAFF" box="[925,1339,1355,1379]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="15" pageNumber="358" phylum="Mollusca" rank="variety" species="elata" subGenus="Tropidocochlis" variety="elevata">
<emphasis id="B963E630EB71FFC2FC7EEF7AFC38FAFF" box="[925,980,1355,1379]" italics="true" pageId="15" pageNumber="358">Helix</emphasis>
(
<emphasis id="B963E630EB71FFC2FC07EF7AFB97FAFF" box="[996,1147,1355,1379]" italics="true" pageId="15" pageNumber="358">Tropidocochlis</emphasis>
