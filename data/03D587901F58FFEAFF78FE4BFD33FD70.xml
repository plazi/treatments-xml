<taxonomicName id="4C7C4D051F58FFEAFE0EFD2FFC45FD16" authority="GIORDANI SOIKA." box="[469,936,650,672]" class="Insecta" family="Eumenidae" genus="Microdynerus" kingdom="Animalia" order="Hymenoptera" pageId="51" pageNumber="56" phylum="Arthropoda" rank="species" species="appenninicus">
