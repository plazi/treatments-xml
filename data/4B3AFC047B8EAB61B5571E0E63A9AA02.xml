2. 
<taxonomicName id="E3CF9FEC5185A4E24AC64D29EEA76E23" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:32697" class="Insecta" family="Formicidae" genus="Myrmicaria" kingdom="Animalia" lsidName-HNS="Myrmicaria vidua Smith" order="Hymenoptera" pageId="141" pageNumber="141" phylum="Arthropoda" rank="species" species="vidua">Myrmicaria vidua</taxonomicName>
