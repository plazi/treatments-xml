<geoCoordinate id="7FE56FA2FFE9FFACDFDF26FDFEEA1509" degrees="29" direction="south" minutes="09" orientation="latitude" pageId="23" pageNumber="438" precision="15" seconds="24" value="-29.156666">29° 09′24′′S</geoCoordinate>
