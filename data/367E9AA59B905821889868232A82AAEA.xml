<collectingCountry id="A790223498254234C0BE625EF7E97582" name="Indonesia">Indonesia</collectingCountry>
: 
<collectingRegion id="482865E5B518D2B3453E63411326082E" country="Indonesia" name="Papua Barat">Papua Province</collectingRegion>
. 
<collectingMunicipality id="B5936A338BA32782B20B570D469A0553">This</collectingMunicipality>
