<emphasis id="D59CDE6AFFE48B2B4CE687A756C1E465" box="[202,467,1004,1024]" italics="true" pageId="2" pageNumber="95">
<taxonomicName id="20E879FBFFE48B2B4CE687A7564FE465" authorityName="CANU" authorityYear="1927" box="[202,349,1004,1024]" class="Gymnolaemata" family="Romancheinidae" genus="Hippopleurifera" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="95" phylum="Bryozoa" rank="genus">Hippopleurifera</taxonomicName>
