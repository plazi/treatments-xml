(=similar), referring to the similarity of the new species to 
<taxonomicName id="4C6D4D5EED463822FF4DFA74FF2FFA12" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[136,234,1465,1491]" class="Maxillopoda" family="Notodelphyidae" genus="Loboixys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="535" pageNumber="536" phylum="Arthropoda" rank="species" species="pilosa" status="sp. nov.">
<emphasis id="B919EACFED463822FF4DFA74FF2FFA12" box="[136,234,1465,1491]" italics="true" pageId="535" pageNumber="536">L. pilosa</emphasis>
</taxonomicName>
<emphasis id="B919EACFED463822FF34FA74FE8EFA15" bold="true" box="[241,331,1466,1492]" pageId="535" pageNumber="536">
<taxonomicNameLabel id="A22A57B4ED463822FF34FA74FE8EFA15" box="[241,331,1466,1492]" pageId="535" pageNumber="536" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C6D4D5EED463822FE44FA74FDD2FA12" authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[385,535,1465,1491]" class="Maxillopoda" family="Notodelphyidae" genus="Loboixys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="535" pageNumber="536" phylum="Arthropoda" rank="species" species="palauensis" status="sp. nov.">
<emphasis id="B919EACFED463822FE44FA74FDD2FA12" box="[385,535,1465,1491]" italics="true" pageId="535" pageNumber="536">L. palauensis</emphasis>
