<treatmentCitationGroup id="F9F2118D0122370576E5FB98EE4000D8" pageId="18" pageNumber="19">
<treatmentCitation id="584310B20122370576E5FB98EDF000FB" author="Walker, F." box="[151,422,1134,1159]" page="50" pageId="18" pageNumber="19" year="1846">
<taxonomicName id="1EE24D200122370576E5FB98EDF000FB" authority="Walker, 1846: 50" authorityName="Walker" authorityPageNumber="50" authorityYear="1846" box="[151,422,1134,1159]" class="Insecta" family="Mymaridae" genus="Arescon" kingdom="Animalia" order="Hymenoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus">
<emphasis id="EB96EAB10122370576E5FB98ECBF00FB" box="[151,233,1135,1158]" italics="true" pageId="18" pageNumber="19">Arescon</emphasis>
<bibRefCitation id="BD734B52012237057680FB98EDF000FB" author="Walker, F." box="[242,422,1134,1159]" pageId="18" pageNumber="19" pagination="49 - 54" refId="ref57247" refString="Walker, F. (1846) VIII. - Descriptions of the Mymaridae. The Annals and Magazine of Natural History, 18, 49 - 54 + Errata and Addenda, viii. https: // doi. org / 10.1080 / 037454809494390" type="journal article" year="1846">Walker, 1846: 50</bibRefCitation>
