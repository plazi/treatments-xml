<taxonomicName id="D2C74D73BC5FC963FD52F92EFC46F909" baseAuthorityName="Gertsch &amp; Soleglad" baseAuthorityYear="1972" box="[731,989,1750,1775]" class="Arachnida" family="Vaejovidae" genus="Uroctonites" kingdom="Animalia" order="Scorpiones" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="huachuca">
