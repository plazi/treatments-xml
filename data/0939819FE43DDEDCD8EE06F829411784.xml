<emphasis id="5B3C820C34DF8B221AA3A20C6915F3B3" italics="true" pageId="0" pageNumber="1">Cyclorhipidion</emphasis>
</taxonomicName>
species near 
<taxonomicName id="68E1F447F23D06B73FA3FD6F7D786E74" genus="C." lsidName="C. pelliculosum" pageId="0" pageNumber="1" rank="species" species="pelliculosum">
