<taxonomicName id="4C5A4D498B77FFD81BEF96898A2CFF7B" box="[446,547,223,248]" class="Insecta" family="Crambidae" genus="Nymphicula" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="407" phylum="Arthropoda" rank="species" species="edwardsi">
