<collectingCountry id="F308762F7363AB26FB74FA5FD25093F6" box="[1261,1357,1484,1510]" name="Sweden" pageId="11" pageNumber="12">Sweden</collectingCountry>
(
<collectionCode id="ED0EAE7A7363AB26FAC7FA5FD28093F6" LSID="urn:lsid:biocol.org:col:34838" box="[1374,1437,1484,1510]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="11" pageNumber="12">SMF</collectionCode>
