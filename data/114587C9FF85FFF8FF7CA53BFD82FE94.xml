<taxonomicName id="5EEC4D5CFF84FFF8FE31A683FDAAFF70" box="[474,580,223,248]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="tupinikim">
