<geoCoordinate id="EE50EA94831FFFCBB924F920FD73A860" box="[580,715,1782,1807]" degrees="04" direction="north" minutes="39.415" orientation="latitude" pageId="11" pageNumber="1066" precision="1" value="4.6569166">04°39.415′N</geoCoordinate>
