<geoCoordinate id="EE50EA94831FFFCBB9B8F920FEB0A85E" degrees="101" direction="east" minutes="16.333" orientation="longitude" pageId="11" pageNumber="1066" precision="1" value="101.27222">101° 16.333′E</geoCoordinate>
