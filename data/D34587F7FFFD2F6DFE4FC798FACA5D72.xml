<taxonomicName id="9CEC4D62FFFD2F6DFBA2C63EFACE5D72" authority="(Otto, 1999)" baseAuthorityName="Otto" baseAuthorityYear="1999" box="[1030,1322,1823,1848]" class="Arachnida" family="Anystidae" genus="Erythracarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="921" phylum="Arthropoda" rank="species" species="pyrrholeucus">
