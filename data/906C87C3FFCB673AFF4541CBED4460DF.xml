<emphasis id="2AB1EAC7FFCC673BFBE4415CEA3F6510" bold="true" box="[1044,1068,1095,1116]" pageId="44" pageNumber="311">86</emphasis>
<taxonomicName id="DFC54D56FFCC673BFBC54153EA896511" box="[1077,1178,1096,1117]" class="Crustacea (awaiting allocation)" family="Crustacea (awaiting allocation)" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea (awaiting allocation)" pageId="44" pageNumber="351" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
