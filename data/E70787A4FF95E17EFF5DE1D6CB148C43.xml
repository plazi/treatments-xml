<taxonomicName id="A8AE4D31FF95E17EFF5DE1D6CE0C8888" authority="Nielson, 2011" authorityName="Nielson" authorityYear="2011" box="[151,371,151,177]" class="Insecta" family="Cicadellidae" genus="Docalidia" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="caudata" status="sp. nov.">
<emphasis id="5DDAEAA0FF95E17EFF5DE1D6CE0C8888" bold="true" box="[151,371,151,177]" italics="true" pageId="24" pageNumber="25">Docalidia caudata</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="46E957DBFF95E17EFE48E1D6CEA08888" box="[386,479,151,177]" pageId="24" pageNumber="25" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
