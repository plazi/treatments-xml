<taxonomicName id="ACF180A4FFB1857EBE36FC8E29A68737" authority="Johnson" authorityName="Johnson" class="Insecta" family="Chrysomelidae" genus="Acanthoscelides" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="buenaventura">
