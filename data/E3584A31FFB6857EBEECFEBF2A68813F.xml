<taxonomicName id="ACF180A4FFB1857EBF19FB872C2E805D" box="[815,1055,1060,1079]" class="Insecta" family="Chrysomelidae" genus="Acanthoscelides" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="65" phylum="Arthropoda" rank="species" species="machiques">
