<caption id="30D6159B473FFFA3F9B3F966FB8E0B85" ID-DOI="http://doi.org/10.5281/zenodo.5305031" ID-Zenodo-Dep="5305031" box="[151,1132,1738,1762]" httpUri="https://zenodo.org/record/5305031/files/figure.png" pageId="11" pageNumber="12" startId="11.[151,255,1738,1762]" targetBox="[223,1335,192,1702]" targetPageId="11">
