<taxonomicName id="5BA496E60449F774CE8132EEE61C4AA4" class="Insecta" family="Arctiidae" genus="Paracles" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Paracles fusca" order="Lepidoptera" pageId="14" pageNumber="79" phylum="Arthropoda" rank="species" species="fusca">Paracles fusca</taxonomicName>
