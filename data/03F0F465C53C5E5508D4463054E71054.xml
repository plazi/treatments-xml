<emphasis id="B92D9961C53D5E550A8340F5531D139C" box="[704,782,441,467]" italics="true" pageId="109" pageNumber="110">
<taxonomicName id="4C593EF0C53D5E550A8340F55319139C" box="[704,778,441,467]" class="Insecta" family="Elachistidae" genus="Elachista" kingdom="Animalia" order="Lepidoptera" pageId="109" pageNumber="110" phylum="Arthropoda" rank="species" species="poella">poella</taxonomicName>
