<quantity id="E5519B444543FFECFDF8FE9AFD2DD775" box="[564,674,284,310]" metricMagnitude="3" metricUnit="m" metricValue="1.524" pageId="2" pageNumber="31" unit="ft" value="5000.0">5000 feet</quantity>
