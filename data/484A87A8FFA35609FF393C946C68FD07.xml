<taxonomicName id="07E34D3DFFAC5608FEB43C1A6D4FFBFA" authority="Ho &amp; Tighe, 2018" authorityName="Ho &amp; Tighe" authorityYear="2018" box="[282,541,1087,1109]" class="Actinopterygii" family="Synaphobranchidae" genus="Dysomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="8" phylum="Chordata" rank="species" species="brachygnathos" status="sp. nov.">
