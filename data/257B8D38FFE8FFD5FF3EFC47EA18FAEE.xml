<taxonomicName id="6AD247ADFFE8FFD5FEBBFBD7EB0EFBD7" baseAuthorityName="Stal" baseAuthorityYear="1872" box="[274,533,1051,1077]" class="Insecta" family="Pentatomidae" genus="Chinavia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="impicticornis">
