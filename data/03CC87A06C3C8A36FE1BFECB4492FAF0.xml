<paragraph id="8BDA36B66C3D8A36FF45FB674587FB4C" blockId="17.[189,1399,1191,1254]" pageId="17" pageNumber="18">
<taxonomicName id="4C654D356C3D8A36FF45FB6746C9FB68" baseAuthorityName="Gude" baseAuthorityYear="1897" box="[189,448,1191,1217]" class="Gastropoda" family="Plectopylidae" genus="Chersaecia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="17" pageNumber="18" phylum="Mollusca" rank="species" species="smithiana">
