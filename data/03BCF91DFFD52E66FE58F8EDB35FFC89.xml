<taxonomicName id="4C153388FFD52E61FE58F8EDB5C6F80D" authority="Agassiz, 1838" authorityName="Cope" authorityYear="1876" box="[395,696,1908,1938]" class="Sarcopterygii" family="Ceratodontidae" genus="Ceratodus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ceratodontiformes" pageId="11" pageNumber="259" phylum="Chordata" rank="species" species="eruciferus" status="sp. nov.">Ceratodus eruciferus</taxonomicName>
, 
<taxonomicNameLabel id="A2522962FFD52E61FD1EF8EDB44FF80E" box="[717,817,1908,1938]" pageId="11" pageNumber="259" rank="species">sp. nov.</taxonomicNameLabel>
