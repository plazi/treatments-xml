<taxonomicName id="549C4D25FFE1FFDBFF33C392FE6DE7F2" authority="Neto &amp; Passos, 2020" authorityName="Neto &amp; Passos" authorityYear="2020" box="[151,385,151,178]" class="Insecta" family="Hydroptilidae" genus="Neotrichia" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="184" phylum="Arthropoda" rank="species" species="mucajai" status="sp. nov.">
<emphasis id="A1E8EAB4FFE1FFDBFF33C392FE6DE7F2" bold="true" box="[151,385,151,178]" italics="true" pageId="5" pageNumber="184">Neotrichia mucajai</emphasis>
</taxonomicName>
