(
<taxonomicName id="91E0E43074421D07F172605931BF9BA7" authority="Koch, L." authorityName="Koch, L." class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="540" phylum="Tracheophyta" rank="variety" species="corniculatus" variety="hirsutus">
