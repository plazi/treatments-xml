<quantity id="796A9B33FFF8FFD2B94CFDD9FC13FDA7" box="[811,910,552,576]" metricMagnitude="-3" metricUnit="m" metricValue="3.84" pageId="76" pageNumber="1563" unit="mm" value="3.84">3.84 mm</quantity>
