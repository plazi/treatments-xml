(deposited in 
<specimenCode id="DBBD9EA3FFC5FFDF075DFBFDC0133C38" box="[628,772,1029,1053]" collectionCode="MNHN" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="5" pageNumber="6">MNHN 1502</specimenCode>
