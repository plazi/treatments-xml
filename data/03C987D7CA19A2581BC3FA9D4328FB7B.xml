<taxonomicName id="4C604D42CA19A25819D9F9F74310FB64" box="[653,794,1559,1584]" class="Aves" family="Tinamidae" genus="Nothocercus" kingdom="Animalia" order="Tinamiformes" pageId="5" pageNumber="132" phylum="Chordata" rank="genus">
