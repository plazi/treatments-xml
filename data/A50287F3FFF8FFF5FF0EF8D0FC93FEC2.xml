<emphasis id="1FDFEAF7FFF8FFF6FF0EF8D0FF5FF8C9" bold="true" box="[151,186,1878,1903]" pageId="21" pageNumber="22">62.</emphasis>
<taxonomicName id="EAAB4D66FFF8FFF6FF58F8D0FCE1F8C9" authority="Fabricius, 1793" authorityName="Fabricius" authorityYear="1793" box="[193,772,1878,1903]" class="Insecta" family="Lycaenidae" genus="Hypolycaena" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="subSpecies" species="philippus" subSpecies="philippus">
