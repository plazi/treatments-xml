<subSubSection id="C3776556ED3D3859FF4DFAC1FE67FAAC" pageId="620" pageNumber="621" type="materials_examined">
<paragraph id="8BD236DDED3D3859FF4DFAC1FE67FAAC" blockId="620.[136,778,1295,2070]" pageId="620" pageNumber="621">
<emphasis id="B919EACFED3D3859FF4DFAC1FEF7FAE8" bold="true" box="[136,306,1295,1321]" pageId="620" pageNumber="621">Type material</emphasis>
. 
<typeStatus id="54D6887FED3D3859FE86FAC1FE6EFAE8" box="[323,427,1295,1321]" pageId="620" pageNumber="621">Holotype</typeStatus>
<specimenCount id="9D6BFD54ED3D3859FE73FADEFE0EFAEB" box="[438,459,1296,1322]" pageId="620" pageNumber="621">♀</specimenCount>
