<taxonomicName id="4C5C4D6D46D9C7BADB40FB57F436C2CA" authority="WESMAEL." box="[308,614,1195,1215]" class="Insecta" family="Ichneumonidae" genus="Platylabus" kingdom="Animalia" order="Hymenoptera" pageId="205" pageNumber="1522" phylum="Arthropoda" rank="species" species="variegatus">
