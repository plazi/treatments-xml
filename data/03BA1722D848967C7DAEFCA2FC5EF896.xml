<subSubSection id="C309F5BFD848967C7DAEF9A2FA20F9F6" box="[192,1441,1635,1659]" pageId="60" pageNumber="59" type="etymology">
<paragraph id="8BACA634D848967C7DAEF9A2FA20F9F6" blockId="60.[192,1441,1635,1659]" box="[192,1441,1635,1659]" pageId="60" pageNumber="59">
<emphasis id="B9677A26D848967C7DAEF9A2FED6F9F6" bold="true" box="[192,343,1635,1659]" pageId="60" pageNumber="59">Etymology.</emphasis>
