<taxonomicName id="F0934D75FFFCFFC2FEF7F8771828F867" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[318,534,1946,1970]" class="Insecta" family="Carabidae" genus="Cylindera" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="99" phylum="Arthropoda" rank="species" species="germanica">
