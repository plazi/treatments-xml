<taxonomicName id="666DA43CD4A4880B2A03A8B2B0BB2576" LSID-IPNI="urn:lsid:ipni.org:names:649237-1" authority="L." authorityName="Linnaeus" authorityYear="1753" class="Liliopsida" family="Orchidaceae" genus="Orchis" higherTaxonomySource="CoL" kingdom="Plantae" order="Asparagales" pageId="35" pageNumber="628" phylum="Tracheophyta" rank="species" species="ustulata">
