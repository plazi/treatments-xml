<quantity id="8A3467A1FFB9B33645F6FEA6CA250AAB" box="[432,547,367,391]" metricMagnitude="2" metricUnit="m" metricValue="6.18" metricValueMax="12.36" metricValueMin="0.0" pageId="37" pageNumber="38" unit="m" value="618.0" valueMax="1236.0" valueMin="0.0">0–1236 m</quantity>
, usually at 
<quantity id="8A3467A1FFB9B33646E4FEA6CB250AAB" box="[674,803,367,391]" metricMagnitude="2" metricUnit="m" metricValue="4.75" metricValueMax="8.5" metricValueMin="1.0" pageId="37" pageNumber="38" unit="m" value="475.0" valueMax="850.0" valueMin="100.0">100–850 m</quantity>
