<collectingCountry id="E1BD7986FFF8FF89FDE2310968340926" box="[564,633,1034,1059]" name="China" pageId="23" pageNumber="24">China</collectingCountry>
, 
<collectingCountry id="E1BD7986FFF8FF89FD5F310968810924" box="[649,716,1034,1057]" name="Japan" pageId="23" pageNumber="24">Japan</collectingCountry>
, 
<collectingCountry id="E1BD7986FFF8FF89FD0C3109696C0924" box="[730,801,1034,1057]" name="South Korea" pageId="23" pageNumber="24">Korea</collectingCountry>
, 
