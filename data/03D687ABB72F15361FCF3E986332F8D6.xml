<materialsCitation id="3B173CE0B72F15331E143F7460F0F8E8" ID-GBIF-Occurrence="2985802301" box="[332,698,1837,1863]" collectionCode="NIGP" pageId="1" pageNumber="77" specimenCode="NIGP173462" specimenCount="1" typeStatus="holotype">
