<paragraph id="8BBA751BFFB70332FF110DA7FE33FF43" blockId="1.[151,1437,1780,2023]" lastBlockId="2.[151,1437,151,501]" lastPageId="2" lastPageNumber="559" pageId="1" pageNumber="558">
Reported from 
<collectingCountry id="F312358BFFB70331FEA30DA7FE53F870" box="[373,494,1996,2022]" name="Guatemala" pageId="1" pageNumber="558">Guatemala</collectingCountry>
(Quirigua, Cayuga and Queriflia), 
<collectingCountry id="F312358BFFB70331FCA80DA7FC69F870" box="[894,980,1996,2022]" name="Mexico" pageId="1" pageNumber="558">Mexico</collectingCountry>
(Cordoba and Sinaola) and 
<collectingCountry id="F312358BFFB70331FAC30DA7FAEDF870" box="[1301,1360,1996,2022]" name="United States of America" pageId="1" pageNumber="558">USA</collectingCountry>
