<subSubSection id="712F5623F0A0D220227218E713DB40D3" pageId="5" pageNumber="6" type="type locality">
<paragraph id="5CA0958614F735E67A26DAB024F88EFE" pageId="5" pageNumber="6">Type locality.</paragraph>
<paragraph id="201B50BF379860DE530D5E9818BA2D25" pageId="5" pageNumber="6">
