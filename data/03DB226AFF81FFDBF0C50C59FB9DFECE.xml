<taxonomicName id="4C72E8FFFF80FFDBF28B0B2CFB98FECE" authority="Howell 1995" authorityName="Howell" authorityYear="1995" box="[729,1073,295,319]" class="Trilobita" family="Odontopleuridae" genus="Primaspis" kingdom="Animalia" order="Lichida" pageId="1" pageNumber="300" phylum="Arthropoda" rank="species" species="tippinsi">
