<taxonomicName id="4C72E8FFFF81FFDAF29D0C86FC97F96B" box="[719,830,1666,1690]" class="Insecta" kingdom="Animalia" pageId="0" pageNumber="299" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
