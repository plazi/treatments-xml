<materialsCitation id="ADD93CA1FFFBFFA6FF01D21BE791FE9A" ID-GBIF-Occurrence="3387337345" country="India" latitude="17.725" location="Visakhapatnam" longLatPrecision="21" longitude="83.34167" pageId="56" pageNumber="292" specimenCount="1" stateProvince="Andhra Pradesh" typeStatus="holotype">
