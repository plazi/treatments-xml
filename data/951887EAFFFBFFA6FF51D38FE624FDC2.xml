<typeStatus id="C20A885EFFFBFFA6FF01D21BE542FEC6" box="[199,258,259,285]" pageId="56" pageNumber="292">Type</typeStatus>
locality.
</emphasis>
<collectingCountry id="65A6766CFFFBFFA6FEB6D21BE5ECFEC6" box="[368,428,259,285]" name="India" pageId="56" pageNumber="292">India</collectingCountry>
, 
<collectingRegion id="DF75F81EFFFBFFA6FE71D21BE630FEC6" box="[439,624,259,285]" country="India" name="Andhra Pradesh" pageId="56" pageNumber="292">Andhra Pradesh</collectingRegion>
, 
<location id="186E6027FFFBFFA6FDBAD21BE769FEC6" LSID="urn:lsid:plazi:treatment:951887EAFFFBFFA6FF51D38FE624FDC2:186E6027FFFBFFA6FDBAD21BE769FEC6" box="[636,809,259,285]" country="India" latitude="17.725" longLatPrecision="21" longitude="83.34167" name="Visakhapatnam" pageId="56" pageNumber="292" stateProvince="Andhra Pradesh">Visakhapatnam</location>
(formerly Waltair), 
<geoCoordinate id="7885503BFFFBFFA6FBC9D21BE0D8FEC6" box="[1039,1176,259,285]" degrees="17" direction="north" minutes="43" orientation="latitude" pageId="56" pageNumber="292" precision="15" seconds="30" value="17.725">17°43’30”N</geoCoordinate>
