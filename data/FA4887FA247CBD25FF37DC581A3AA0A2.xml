<date id="065F102C247CBD25FAB3DC031EA7A159" box="[1299,1432,577,600]" pageId="98" pageNumber="99" value="1930-03">March 1930</date>
, 
<emphasis id="4095EAFE247CBD25FF67DC201A43A179" box="[199,380,609,633]" italics="true" pageId="98" pageNumber="99">L. Williams 7836</emphasis>
