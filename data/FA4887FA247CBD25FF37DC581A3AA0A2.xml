<taxonomicName id="B5E14D6F247CBD25FD05DCC3182AA199" authorityName="Kunth" authorityYear="1841" baseAuthorityName="Poiteau" box="[677,789,641,664]" class="Liliopsida" family="Arecaceae" genus="Geonoma" kingdom="Plantae" order="Arecales" pageId="98" pageNumber="99" phylum="Tracheophyta" rank="species" species="deversa">
