<quantity id="B7739B6BFFC0FF80FCB0FE294FF4FD9A" box="[835,933,496,523]" metricMagnitude="3" metricUnit="m" metricValue="2.3" pageId="13" pageNumber="1058" unit="m" value="2300.0">2300 m</quantity>
.
