(as 
<taxonomicName id="4C1D4D6FC247526EFAE38063CDF9FBBA" baseAuthorityName="Meigen" baseAuthorityYear="1830" box="[1328,1436,1085,1109]" class="Insecta" family="Simuliidae" genus="Prosimulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="204" phylum="Arthropoda" rank="species" species="rufipes">
