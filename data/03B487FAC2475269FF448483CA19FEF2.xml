- 
<taxonomicName id="4C1D4D6FC247526EFB7C81BBCD60FA11" baseAuthorityName="Fries" baseAuthorityYear="1824" box="[1199,1285,1510,1534]" class="Insecta" family="Simuliidae" genus="Prosimulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="204" phylum="Arthropoda" rank="species" species="hirtipes">hirtipes</taxonomicName>
