<collectionCode id="ED01AE1B267C931AFB92CFA000ED2BEE" box="[1131,1199,911,932]" pageId="3" pageNumber="242">EAST</collectionCode>
<collectingCountry id="F307764E267C931AFB43CFA001062BEE" box="[1210,1348,911,932]" name="Malaysia" pageId="3" pageNumber="242">MALAYSIA</collectingCountry>
: 
