<materialsCitation id="3B783C83267C931AFCC3CFC300252C75" ID-GBIF-Occurrence="3325218303" collectingDate="1999-06-06" collectionCode="EAST" country="Malaysia" location="Danum Valley" pageId="3" pageNumber="242" specimenCount="6" specimenCount-male="6" stateProvince="Sabah" typeStatus="paratype">
