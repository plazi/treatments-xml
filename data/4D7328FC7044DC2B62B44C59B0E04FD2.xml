<taxonomicName id="B1A4E464205A67DFEB84F374EF178F1C" authority="Walckenaer, 1805" authorityName="Walckenaer" authorityYear="1805" class="Arachnida" family="Araneidae" genus="Epeira" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Epeira phalerata" order="Araneae" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="phalerata">Epeira phalerata</taxonomicName>
