<taxonomicName id="4C56099CEE46251EFBB1FF63FB45FF00" authority="HANSSON &amp; LASALLE, 2003" authorityName="HANSSON &amp; LASALLE" authorityYear="2003" box="[1111,1182,188,209]" class="Insecta" family="Eulophidae" genus="Cabeza" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="707" phylum="Arthropoda" rank="species" species="ugaldei" status="sp. n.">
