<taxonomicName id="4C56099CEE46251EFBB7FE3CFB47FE29" authority="HANSSON &amp; LASALLE, 2003" authorityName="HANSSON &amp; LASALLE" authorityYear="2003" box="[1105,1180,483,504]" class="Insecta" family="Eulophidae" genus="Cabeza" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="707" phylum="Arthropoda" rank="species" species="laticeps" status="sp. n.">
