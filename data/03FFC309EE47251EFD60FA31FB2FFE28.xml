<taxonomicName id="4C56099CEE47251FFBC1F958FB44F94D" authority="HANSSON &amp; LASALLE, 2003" authorityName="HANSSON &amp; LASALLE" authorityYear="2003" box="[1063,1183,1671,1692]" class="Insecta" family="Eulophidae" genus="Cabeza" kingdom="Animalia" order="Hymenoptera" pageId="10" pageNumber="706" phylum="Arthropoda" rank="species" species="canaliculata" status="sp. n.">
