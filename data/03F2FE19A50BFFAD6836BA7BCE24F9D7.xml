<heading id="D0ACF863A50BFFAC6836BA7BCF39FBEA" box="[840,1387,1070,1092]" centered="true" fontSize="9" level="2" pageId="30" pageNumber="129" reason="4">
<taxonomicName id="4C5B348CA50BFFAC6836BA7BCEADFBEA" authority="THOMPSON" authorityName="THOMPSON" authorityYear="2012" box="[840,1279,1070,1092]" class="Insecta" family="Syrphidae" genus="Citrogramma" kingdom="Animalia" order="Diptera" pageId="30" pageNumber="129" phylum="Arthropoda" rank="species" species="australe" status="SP. NOV.">
