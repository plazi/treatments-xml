<paragraph id="9C9363CA9C8104ECA1CB717E145D0F5B" pageId="9" pageNumber="202">
<taxonomicName id="99AF1B10E65BC8A165754196ED6DF807" authorityName="**" baseAuthorityName="Kirby" baseAuthorityYear="1837" class="Insecta" family="Coccinellidae" genus="Macronaemia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="9" pageNumber="202" phylum="Arthropoda" rank="species" species="episcopalis">
<emphasis id="93B3111974FBCAAC47228F681AD9BA36" italics="true" pageId="9" pageNumber="202">Macronaemia episcopalis</emphasis>
</taxonomicName>
