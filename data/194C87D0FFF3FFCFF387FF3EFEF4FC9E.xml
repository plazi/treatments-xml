</taxonomicName>
, 
<taxonomicName id="56E54D45FFF3FFCFF142FDC0FCE7FDD1" box="[594,782,613,638]" class="Arachnida" family="Tenuipalpidae" genus="Crossipalpus" kingdom="Animalia" order="Prostigmata" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="muellerianae">
<emphasis id="A391EAD4FFF3FFCFF142FDC0FCE7FDD1" box="[594,782,613,638]" italics="true" pageId="21" pageNumber="22">Cr. muellerianae</emphasis>
</taxonomicName>
, 
<taxonomicName id="56E54D45FFF3FFCFF00CFDC0FC62FDD2" box="[796,907,614,637]" class="Arachnida" family="Tenuipalpidae" genus="Crossipalpus" kingdom="Animalia" order="Prostigmata" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="raveni">
