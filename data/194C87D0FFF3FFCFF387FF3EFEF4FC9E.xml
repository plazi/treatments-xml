</taxonomicName>
, 
<taxonomicName id="56E54D45FFF3FFCFF08AFDC0FBA3FDD1" box="[922,1098,613,638]" class="Arachnida" family="Tenuipalpidae" genus="Crossipalpus" kingdom="Animalia" order="Prostigmata" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="verticillatae">
