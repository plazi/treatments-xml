<taxonomicName id="4C154D34FFE5FFF2D6EAFD55FEB5FD3C" authorityName="Jałoszyński" authorityYear="2017" box="[151,370,654,678]" class="Insecta" family="Scydmaenidae" genus="Euconnus" kingdom="Animalia" order="Coleoptera" pageId="7" phylum="Arthropoda" rank="species" species="rhombopanamensis">
