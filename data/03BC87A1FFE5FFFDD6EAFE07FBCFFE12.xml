; HL 
<quantity id="4CED9B52FFE5FFF2D2C3F930FAA5F898" box="[1214,1378,1771,1795]" metricMagnitude="-4" metricUnit="m" metricValue="4.55" metricValueMax="4.8" metricValueMin="4.3" pageId="7" unit="mm" value="0.45499999999999996" valueMax="0.48" valueMin="0.43">0.43–0.48 mm</quantity>
, HW 
<quantity id="4CED9B52FFE5FFF2D6EAF8D5FEFCF8BC" box="[151,315,1806,1831]" metricMagnitude="-4" metricUnit="m" metricValue="3.55" metricValueMax="3.5999999999999996" metricValueMin="3.5" pageId="7" unit="mm" value="0.355" valueMax="0.36" valueMin="0.35">0.35–0.36 mm</quantity>
, AnL 
<quantity id="4CED9B52FFE5FFF2D7FDF8D5FDE1F8BC" box="[384,550,1806,1831]" metricMagnitude="-3" metricUnit="m" metricValue="1.225" metricValueMax="1.25" metricValueMin="1.2" pageId="7" unit="mm" value="1.225" valueMax="1.25" valueMin="1.2">1.20–1.25 mm</quantity>
; PL 
<quantity id="4CED9B52FFE5FFF2D425F8D5FD3BF8BC" box="[600,764,1806,1831]" metricMagnitude="-4" metricUnit="m" metricValue="4.4" metricValueMax="4.5" metricValueMin="4.3" pageId="7" unit="mm" value="0.44" valueMax="0.45" valueMin="0.43">0.43–0.45 mm</quantity>
