<taxonomicName id="20B94D318F5FF53C611DF82BFA790083" box="[1366,1435,1928,1950]" class="Insecta" family="Formicidae" genus="Lasius" kingdom="Animalia" order="Hymenoptera" pageId="8" phylum="Arthropoda" rank="species" species="alienus">
<emphasis id="D5CDEAA08F5FF53C611DF82BFA790083" box="[1366,1435,1928,1950]" italics="true" pageId="8">alienus</emphasis>
