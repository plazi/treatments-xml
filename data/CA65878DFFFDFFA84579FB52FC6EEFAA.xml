<collectionCode id="24DDAE5EFFFDFFA84671FA99FDCEEF71" box="[530,592,1285,1309]" country="0" httpUri="http://grbio.org/cool/r9n2-w98n" name="Bayerische Staatssammlung fuer Palaeontologie und Geologie" pageId="8" pageNumber="419">BSPG</collectionCode>
