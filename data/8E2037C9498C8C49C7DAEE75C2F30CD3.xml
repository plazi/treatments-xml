<geoCoordinate id="A1C081B89E15E598DEF3BBF2E92AEFB6" direction="north" orientation="latitude" precision="15" value="24.151943">24°09'07&quot;N</geoCoordinate>
, 
<geoCoordinate id="D50A9205F05A14AC26937977D037ECA5" direction="east" orientation="longitude" precision="15" value="110.20805">110°12'29&quot;E</geoCoordinate>
