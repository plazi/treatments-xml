<taxonomicName id="4C044D495215FFCE1093FD55FC8CFD19" authority="Turedrilus" authorityName="Turedrilus" box="[491,781,678,702]" class="Clitellata" family="Glossoscolecidae" genus="Andiorrhinus" kingdom="Animalia" order="Haplotaxida" pageId="5" pageNumber="108" phylum="Annelida" rank="genus">
