<taxonomicName id="87A774BCFFEDFFDCFF59FA360E42FB77" authorityName="Boulenger" authorityYear="1895" box="[187,336,1136,1161]" class="Reptilia" family="Colubridae" genus="Stegonotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="42" pageNumber="43" phylum="Chordata" rank="species" species="reticulatus">
