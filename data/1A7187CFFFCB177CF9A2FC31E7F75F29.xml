<taxonomicName id="55D84D5AFFCB177CFABEFC03E7E7599A" box="[1284,1439,957,992]" class="Reptilia" family="Dermatemydidae" genus="Basilemys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Testudines" pageId="33" pageNumber="350" phylum="Chordata" rank="genus">
