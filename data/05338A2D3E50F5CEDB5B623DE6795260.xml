<pageBreakToken id="956F8E41E15D7CB6B5EB68F66E0EB277" pageId="null" pageNumber="726" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="C1D545C4887A6163B93F0D4F1D254687" authority="(L.) Miller" class="Magnoliopsida" family="Cistaceae" genus="Helianthemum" kingdom="Plantae" order="Malvales" pageId="null" pageNumber="726" phylum="Tracheophyta" rank="species" species="nummularium">
