<taxonomicName id="9BDD1FB0FF957C6AD6179FEEFE37FC13" authorityName="Montreuil" authorityYear="2015" box="[276,437,949,975]" class="Insecta" family="Scarabaeidae" genus="Sisyphus" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="perissinottoi">
<emphasis id="6EA9B821FF957C6AD6179FEEFE37FC13" box="[276,437,949,975]" italics="true" pageId="14" pageNumber="15">S. perissinottoi</emphasis>
