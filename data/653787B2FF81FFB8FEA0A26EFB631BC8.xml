<taxonomicName id="2A9E4D27FF81FFBFFE1CA2F3FCD61CF0" authority="Thomson, 1899" authorityName="Thomson" authorityYear="1899" box="[436,843,948,974]" class="Insecta" family="Carabidae" genus="Stratiotes" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="setosus">
<emphasis id="DFEAEAB6FF81FFBFFE1CA2F3FD1E1CF3" box="[436,643,948,973]" italics="true" pageId="3" pageNumber="4">Stratiotes setosus</emphasis>
<bibRefCitation id="890F4B55FF81FFBFFD25A2F3FCD61CF0" author="Thomson" box="[653,843,948,974]" pageId="3" pageNumber="42" refString="Thomson, G. M. (1899) A revision of the Crustacea Anomura of New Zealand. Transactions and Proceedings of the New Zealand Institute, Zoology, 31, 169 - 197." type="journal article" year="1899">
Thomson, 
<number id="5B562483FF81FFBFFCA6A2F3FCD61CF0" box="[782,843,948,974]" pageId="3" pageNumber="4" value="1899.0">1899</number>
</bibRefCitation>
