<taxonomicName id="8FEE4D41FF8FFFA4FF57FEA56701CA5A" authority="Koch, 1844 a" authorityName="Koch" authorityYear="1844" box="[199,578,258,285]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="102" pageNumber="103" phylum="Arthropoda" rank="species" species="latum">
