<collectingCountry id="CFC564F0FA4AFFC5FC47FE08FC0EFDF2" box="[952,999,508,534]" name="Iran" pageId="3" pageNumber="4">Iran</collectingCountry>
(
<collectingCountry id="CFC564F0FA4AFFC5FC08FE08FBACFDF2" box="[1015,1093,508,534]" name="Iran" pageId="3" pageNumber="4">Persia</collectingCountry>
