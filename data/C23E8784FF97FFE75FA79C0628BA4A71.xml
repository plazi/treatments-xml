<taxonomicName id="8D974D11FF97FFE75E339B3628F74B48" authority="P. Browne ex Gaertn." authorityName="Gaertn." box="[340,685,1053,1080]" class="Magnoliopsida" family="Myrtaceae" genus="Syzygium" higherTaxonomySource="IPNI" kingdom="Plantae" order="Myrtales" pageId="70" pageNumber="69" phylum="Magnoliophyta" rank="genus">
