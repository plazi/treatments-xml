<quantity id="563C5CD48B12FFA019AFFBF7FB6FFBC0" box="[1158,1232,1087,1112]" metricMagnitude="2" metricUnit="m" metricValue="1.46" pageId="3" pageNumber="144" unit="m" value="146.0">146 m</quantity>
