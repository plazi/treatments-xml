<caption id="BBBA51D3FFA5FFCEFF32F80BFEC13685" ID-DOI="http://doi.org/10.5281/zenodo.5127472" ID-Zenodo-Dep="5127472" box="[151,1437,1993,2017]" httpUri="https://zenodo.org/record/5127472/files/figure.png" pageId="55" pageNumber="56" startId="55.[151,255,1993,2017]" targetBox="[180,1433,208,1963]" targetPageId="55">
<paragraph id="EF7A015BFFA5FFCEFF32F80BFEC13685" blockId="55.[151,1437,1993,2017]" box="[151,1437,1993,2017]" pageId="55" pageNumber="56">
<emphasis id="DDB1DD49FFA5FFCEFF32F80BFA7A3684" bold="true" box="[151,294,1993,2017]" pageId="55" pageNumber="56">FIGURE 27.</emphasis>
<taxonomicName id="28C57AD8FFA5FFCEFE89F808FAFE3684" authority="Edgar, 2008" authorityName="Edgar" authorityYear="2008" box="[300,418,1994,2017]" class="Malacostraca" family="Tanaididae" genus="Zeuxo" kingdom="Animalia" order="Tanaidacea" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="belli" status="n. sp.">
<emphasis id="DDB1DD49FFA5FFCEFE89F808FAFE3684" box="[300,418,1994,2017]" italics="true" pageId="55" pageNumber="56">Zeuxo belli</emphasis>
</taxonomicName>
<emphasis id="DDB1DD49FFA5FFCEFE0CF80BFAB83684" bold="true" box="[425,484,1993,2017]" pageId="55" pageNumber="56">
<taxonomicNameLabel id="C6826032FFA5FFCEFE0CF80BFAB83684" box="[425,484,1993,2017]" pageId="55" pageNumber="56" rank="species">n. sp.</taxonomicNameLabel>
