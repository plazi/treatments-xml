<taxonomicName id="4C5ABBE21F0EFFD5BC30F9F4154A97DF" authority="Meyrick" authorityName="Meyrick" box="[1132,1341,1658,1680]" class="Insecta" family="Lecithoceridae" genus="Pacificulla" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="70" phylum="Arthropoda" rank="species" species="miltina">
