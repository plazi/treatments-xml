<taxonomicName id="4C61D8C1D027FFC7FD139800B02AF889" box="[681,849,1803,1827]" class="Insecta" family="Cicadellidae" genus="Busonia" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="409" phylum="Arthropoda" rank="species" species="albilateralis">
