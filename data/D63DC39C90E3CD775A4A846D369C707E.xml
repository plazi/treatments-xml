<taxonomicName id="973908100B95C77F83888009E8F0B6C1" class="Magnoliopsida" family="Solanaceae" genus="Lycium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lycium ruthenicum" order="Solanales" pageId="4" pageNumber="21" phylum="Tracheophyta" rank="species" species="ruthenicum">Lycium ruthenicum</taxonomicName>
Murray (
<taxonomicName id="A5794C25225759AB19FC59CDFC5607F9" genus="Solanaceae" lsidName="Solanaceae" pageId="4" pageNumber="21" rank="genus">Solanaceae</taxonomicName>
