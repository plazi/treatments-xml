<paragraph id="8F194C0D3FB48824619A7D1577061A7E" pageId="3" pageNumber="20">
<taxonomicName id="2DD36620061E65BCD2B91A3D16C4B3D5" LSID="http://zoobank.org/C54984F9-3B58-4756-A422-81683CE5C3A2" class="Arachnida" family="Eriophyidae" genus="Echinacrus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Echinacrus ruthenicus" order="Trombidiformes" pageId="3" pageNumber="20" phylum="Arthropoda" rank="species" species="ruthenicus">Echinacrus ruthenicus</taxonomicName>
