<quantity id="4CEF9B25FF52FF769B48FAAAF93D3576" box="[821,892,1368,1395]" metricMagnitude="-1" metricUnit="m" metricValue="1.6" pageId="195" pageNumber="196" unit="cm" value="16.0">16 cm</quantity>
