<subSubSection id="FB85655D7C549B03FF67F6529E90FB75" pageId="39" pageNumber="40" type="diagnosis">
<paragraph id="B32036D67C549B03FF67F6529E90FB75" blockId="39.[151,1436,1133,1302]" pageId="39" pageNumber="40">
<emphasis id="81EBEAC47C549B03FF67F6529E6FFB59" bold="true" box="[151,266,1133,1158]" pageId="39" pageNumber="40">Diagnosis</emphasis>
