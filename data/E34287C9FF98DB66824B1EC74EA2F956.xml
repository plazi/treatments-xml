FAMILY 
<taxonomicName id="ACEB4D5CFF98DB6782E11EC74F7EFA42" authority="FILIPJEV, 1918" authorityName="Filipjev" authorityYear="1918" box="[967,1350,1421,1442]" class="Adenophorea" family="Draconematidae" kingdom="Animalia" order="Desmodorida" pageId="6" pageNumber="517" phylum="Nematoda" rank="family">DRACONEMATIDAE FILIPJEV, 1918</taxonomicName>
