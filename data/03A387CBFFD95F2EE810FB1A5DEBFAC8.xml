<taxonomicName id="4C0A4D5EFFD95F2EE84EFABA5C80FAE3" baseAuthorityName="LeConte" baseAuthorityYear="1873" box="[631,730,1283,1304]" class="Insecta" family="Carabidae" genus="Poecilus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="382" phylum="Arthropoda" rank="species" species="corvus">
