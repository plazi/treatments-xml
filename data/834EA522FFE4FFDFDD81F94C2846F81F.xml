<quantity id="CC1FB9D1FFE9FFDFDE3AFECD2AA4FEB3" box="[812,900,332,356]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="8" pageNumber="238" unit="mm" value="2.1">2.1 mm</quantity>
