, 
<taxonomicName id="D0ACDA9D6B52D30EDA4B7551FC8D7E61" box="[635,861,1858,1883]" class="Agaricomycetes" family="Boletaceae" genus="Leccinum" kingdom="Fungi" order="Boletales" pageId="11" pageNumber="62" phylum="Basidiomycota" rank="species" species="versipelle">
