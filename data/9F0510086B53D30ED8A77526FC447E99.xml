<taxonomicName id="D0ACDA9D6B52D30EDBA57551FBC47E61" box="[917,1044,1858,1883]" class="Agaricomycetes" family="Boletaceae" genus="Leccinum" kingdom="Fungi" order="Boletales" pageId="11" pageNumber="62" phylum="Basidiomycota" rank="species" species="scabrum">
