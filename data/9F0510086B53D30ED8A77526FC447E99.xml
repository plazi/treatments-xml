<taxonomicName id="D0ACDA9D6B52D30EDCE27557FA847E61" box="[1234,1364,1858,1883]" class="Agaricomycetes" family="Boletaceae" kingdom="Fungi" order="Boletales" pageId="11" pageNumber="62" phylum="Basidiomycota" rank="family">Boletaceae</taxonomicName>
