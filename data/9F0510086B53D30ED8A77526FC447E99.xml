<number id="A164B3396B52D30EDAA774EEFD007E29" box="[663,720,1787,1811]" pageId="11" pageNumber="62" value="1983.0">1983</number>
</bibRefCitation>
) and 
<collectingCountry id="6FBBE18E6B52D30EDB3E74EFFC537E29" box="[782,899,1786,1811]" name="Greenland" pageId="11" pageNumber="62">Greenland</collectingCountry>
(
<bibRefCitation id="733DDCEF6B52D30EDBA174EFFBE57E29" author="Griffiths" box="[913,1077,1786,1811]" pageId="11" pageNumber="62" refString="Griffiths, G. C. D. (1983) Anthomyiidae [part]. In: Griffiths, G. C. D. (Ed.), Flies of the Nearctic Region, 8 (2), 2. E. Schweizerbart, Stuttgart, pp. 161 - 288." type="journal article" year="1983">
