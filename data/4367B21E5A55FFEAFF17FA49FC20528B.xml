<emphasis id="F9BADF1A5A5BFFEAFB7EFE78FAC254FA" box="[1278,1397,403,429]" italics="true" pageId="9" pageNumber="537">A. emilyae</emphasis>
</taxonomicName>
, 
<taxonomicName id="0CCE788B5A5BFFEAFA04FE78FF495486" authorityName="Huang" authorityYear="1975" class="Reptilia" family="Xenodermatidae" genus="Achalinus" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="537" phylum="Chordata" rank="species" species="hainanus">
<emphasis id="F9BADF1A5A5BFFEAFA04FE78FF495486" italics="true" pageId="9" pageNumber="537">A. hainanus</emphasis>
</taxonomicName>
and 
<taxonomicName id="0CCE788B5A5BFFEAFEB4FE5CFE055486" box="[308,434,439,465]" class="Reptilia" family="Xenodermatidae" genus="Achalinus" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="537" phylum="Chordata" rank="species" species="rufecens">
