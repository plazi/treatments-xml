<taxonomicName id="0CCE788B5A5BFFEAFEF1FE78FDA254FA" authorityName="Boulenger" authorityYear="1908" box="[369,533,403,429]" class="Reptilia" family="Xenodermatidae" genus="Achalinus" kingdom="Animalia" order="Squamata" pageId="9" pageNumber="537" phylum="Chordata" rank="species" species="formosanus">
