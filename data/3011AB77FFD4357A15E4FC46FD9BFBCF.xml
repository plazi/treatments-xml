</taxonomicName>
, which is based on 
<taxonomicName id="7FB861E2FFD4357A1169FBD4FACEFBBB" authorityName="Leveille" authorityYear="1912" box="[1050,1307,1027,1052]" class="Magnoliopsida" family="Gesneriaceae" genus="Didymocarpus" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="mairei">
<emphasis id="8ACCC673FFD4357A1169FBD4FACEFBBB" box="[1050,1307,1027,1052]" italics="true" pageId="22" pageNumber="23">Didymocarpus mairei</emphasis>
</taxonomicName>
, described in the same publication as 
<taxonomicName id="7FB861E2FFD4357A14ACFBFEFD6EFBE5" authorityName="Leveille" authorityYear="1912" box="[479,699,1065,1090]" class="Insecta" family="Curculionidae" genus="Oreocharis" kingdom="Plantae" order="Coleoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="mairei">
<emphasis id="8ACCC673FFD4357A14ACFBFEFD6EFBE5" box="[479,699,1065,1090]" italics="true" pageId="22" pageNumber="23">Oreocharis mairei</emphasis>
</taxonomicName>
. 
<taxonomicName id="7FB861E2FFD4357A17BDFBFEFCAFFBE5" authorityName="Wallich" authorityYear="1819" box="[718,890,1065,1090]" class="Magnoliopsida" family="Gesneriaceae" genus="Didymocarpus" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
<emphasis id="8ACCC673FFD4357A17BDFBFEFCAFFBE5" box="[718,890,1065,1090]" italics="true" pageId="22" pageNumber="23">Didymocarpus</emphasis>
</taxonomicName>
/ 
<taxonomicName id="7FB861E2FFD4357A16F1FBFEFBABFBE5" authority="Craib, 1919" authorityName="Craib" authorityYear="1920" baseAuthorityName="H. Lev." box="[898,1150,1065,1090]" class="Magnoliopsida" family="Gesneriaceae" genus="Ancylostemon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="mairei">
<emphasis id="8ACCC673FFD4357A16F1FBFEFBABFBE5" box="[898,1150,1065,1090]" italics="true" pageId="22" pageNumber="23">Ancylostemon mairei</emphasis>
</taxonomicName>
