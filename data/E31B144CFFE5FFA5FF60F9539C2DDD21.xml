<taxonomicName id="ACB2DED9FFE5FFA4FF60F9539B83DCBB" authority="Quaintance and Baker 1917" authorityName="Quaintance and Baker" authorityYear="1917" box="[192,916,1731,1755]" class="Insecta" family="Aleyrodidae" genus="Aleuroplatus" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="berbericolus">
