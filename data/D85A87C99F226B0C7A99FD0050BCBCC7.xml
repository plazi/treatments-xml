<typeStatus id="8F48887D9F226B0C7ADEFC715018B998" box="[677,779,997,1020]" pageId="19" pageNumber="168">PARATYPES</typeStatus>
. — 
<specimenCount id="46F5FD569F226B0C7B4CFC7150A1B99F" box="[823,946,997,1020]" pageId="19" pageNumber="168" type="immature">7 immatures</specimenCount>
