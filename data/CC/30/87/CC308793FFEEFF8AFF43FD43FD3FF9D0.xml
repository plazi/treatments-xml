, 
<collectingCountry box="[272,375,1519,1545]" name="Lebanon" pageId="9" pageNumber="448">Lebanon</collectingCountry>
&amp; 
<collectingCountry box="[409,555,1519,1545]" name="Afghanistan" pageId="9" pageNumber="448">Afghanistan</collectingCountry>
