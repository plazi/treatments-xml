<paragraph blockId="35.[264,740,799,865]" box="[312,740,839,865]" pageId="35" pageNumber="36">
<taxonomicName box="[312,374,839,864]" class="Scaphopoda" family="Gadilidae" genus="Cadulus" kingdom="Animalia" order="Gadilida" pageId="35" pageNumber="36" phylum="Mollusca" rank="species" species="nerta">
