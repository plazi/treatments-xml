<bibRefCitation author="Illiger" box="[1309,1429,1562,1583]" pageId="4" pageNumber="155" refString="Illiger, K. (1801) Neue Insekten. Magazin fur Insektenkunde, 1, 163 - 208." type="journal article" year="1801">
Illiger, 
<number box="[1380,1429,1562,1583]" pageId="4" pageNumber="155" value="1801.0">1801</number>
</bibRefCitation>
)
</taxonomicName>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="4" pageNumber="155">
<paragraph blockId="4.[151,1436,1122,2023]" pageId="4" pageNumber="155">
<keyLead pageId="4" pageNumber="155">
<number box="[151,163,1591,1612]" pageId="4" pageNumber="155" value="5.0">5</number>
