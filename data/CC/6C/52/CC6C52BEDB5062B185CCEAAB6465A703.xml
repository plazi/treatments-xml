<taxonomicName class="Arachnida" family="Theraphosidae" genus="Ybyrapora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ybyrapora" order="Araneae" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">Ybyrapora</taxonomicName>
