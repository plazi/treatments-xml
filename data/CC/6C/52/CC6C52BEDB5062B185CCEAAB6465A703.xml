<taxonomicName class="Arachnida" family="Theraphosidae" genus="Stromatopelma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Stromatopelma" order="Araneae" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">Stromatopelma</taxonomicName>
