<taxonomicName class="Arachnida" family="Theraphosidae" genus="Psalmopoeus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psalmopoeus" order="Araneae" pageId="19" pageNumber="20" phylum="Arthropoda" rank="genus">Psalmopoeus</taxonomicName>
