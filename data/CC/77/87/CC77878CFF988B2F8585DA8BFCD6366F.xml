<collectingCountry box="[217,348,1607,1632]" name="Uzbekistan" pageId="7" pageNumber="208">Uzbekistan</collectingCountry>
, Kirghizstan, 
<collectingCountry box="[502,614,1607,1632]" name="Tajikistan" pageId="7" pageNumber="208">Tajikistan</collectingCountry>
