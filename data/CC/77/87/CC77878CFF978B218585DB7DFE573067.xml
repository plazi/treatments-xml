<collectingCountry box="[357,435,1905,1930]" name="Russia" pageId="8" pageNumber="209">Russia</collectingCountry>
; 
<collectingCountry box="[446,530,1905,1930]" name="Turkey" pageId="8" pageNumber="209">Turkey</collectingCountry>
