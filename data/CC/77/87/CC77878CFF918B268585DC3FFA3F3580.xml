<taxonomicName authorityName="Costa" authorityYear="1878" box="[1066,1200,1035,1059]" class="Insecta" family="Syrphidae" genus="Paragus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="nitidissimus">
