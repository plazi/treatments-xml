<taxonomicName authorityName="Macquart" authorityYear="1850" box="[906,1046,1071,1094]" class="Insecta" family="Syrphidae" genus="Paragus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="215" phylum="Arthropoda" rank="species" species="aegyptius">
