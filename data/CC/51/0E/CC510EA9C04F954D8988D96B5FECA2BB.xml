<paragraph pageId="143" pageNumber="244">Remarks.</paragraph>
<paragraph pageId="143" pageNumber="244">
<taxonomicName authorityName="Burmeister" authorityYear="1847" class="Insecta" family="Scarabaeidae" genus="Peltonotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Peltonotus morio" order="Coleoptera" pageId="143" pageNumber="244" phylum="Arthropoda" rank="species" species="morio">
