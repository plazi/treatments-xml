<typeStatus box="[417,513,2053,2077]" pageId="33" pageNumber="330">Holotype</typeStatus>
, NSMT-Co 1738, Colahan Seamount, 
<quantity box="[978,1089,1792,1816]" metricMagnitude="2" metricUnit="m" metricValue="6.97" metricValueMax="7.12" metricValueMin="6.82" pageId="33" pageNumber="330" unit="m" value="697.0" valueMax="712.0" valueMin="682.0">682–712 m</quantity>
