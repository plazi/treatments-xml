<paragraph blockId="6.[151,692,1567,1627]" box="[151,344,1602,1627]" pageId="6" pageNumber="157">
(
<figureCitation box="[158,336,1602,1627]" captionStart-0="FIGURES 87 – 91" captionStart-1="FIGURES 92 &amp; 93" captionStart-2="FIGURES 94 – 98" captionStart-3="FIGURES 99 – 101" captionStart-4="FIGURES 102 – 106" captionStartId-0="26.[151,264,1145,1168]" captionStartId-1="27.[151,264,1239,1262]" captionStartId-2="27.[151,264,1962,1985]" captionStartId-3="28.[151,264,1082,1105]" captionStartId-4="29.[151,264,1030,1053]" captionTargetBox-0="[151,1436,191,1126]" captionTargetBox-1="[151,1434,183,1224]" captionTargetBox-2="[150,1434,1298,1942]" captionTargetBox-3="[319,1254,193,1058]" captionTargetBox-4="[151,1435,193,1009]" captionTargetId-0="figure@26.[151,1436,191,1126]" captionTargetId-1="figure@27.[151,1435,183,1225]" captionTargetId-2="figure@27.[149,1434,1298,1943]" captionTargetId-3="figure@28.[319,1254,193,1059]" captionTargetId-4="figure@29.[151,1436,193,1010]" captionTargetPageId-0="26" captionTargetPageId-1="27" captionTargetPageId-2="27" captionTargetPageId-3="28" captionTargetPageId-4="29" captionText-0="FIGURES 87 – 91. Euseboides reni sp. nov. holotype, male. 87. dorsal view; 88. ventral view; 89. lateral view; 90. mandibles and palps; 91. frons. Scale bars A = 5 mm, B = 1 mm." captionText-1="FIGURES 92 &amp; 93. Euseboides reni sp. nov. holotype, male. Elytra, ventral view. Scale bars = 1 mm." captionText-2="FIGURES 94 – 98. Euseboides reni sp. nov. paratype, male. 94. &amp; 95. terminalia: 94. dorsal view, 95. ventral view; 96 – 98. aedeagus: tegmen, 96. dorsal view, 97. ventral view, 98. lateral view. Scale bars = 1 mm." captionText-3="FIGURES 99 – 101. Euseboides reni sp. nov. paratype, male. Aedeagus: penis, 99. dorsal view, 100. ventral view, 101. lateral view. Scale bars = 1 mm." captionText-4="FIGURES 102 – 106. Euseboides reni sp. nov. paratype, female. 102. dorsal view; 103. ventral view; 104. lateral view; 105. mandibles and palps; 106. frons. Scale bars A = 5 mm, B = 1 mm." httpUri-0="https://zenodo.org/record/253857/files/figure.png" httpUri-1="https://zenodo.org/record/253858/files/figure.png" httpUri-2="https://zenodo.org/record/253859/files/figure.png" httpUri-3="https://zenodo.org/record/253860/files/figure.png" httpUri-4="https://zenodo.org/record/253861/files/figure.png" pageId="6" pageNumber="157">
Figures 
<number box="[251,336,1602,1627]" pageId="6" pageNumber="157" value="96.5" valueMax="106.0" valueMin="87.0">87–106</number>
