<taxonomicName authority="Huang, Chen &amp; Li" authorityName="Huang, Chen &amp; Li" box="[151,673,921,947]" class="Insecta" family="Cerambycidae" genus="Euseboides" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="155" phylum="Arthropoda" rank="species" species="motuoensis" status="sp. nov.">
<emphasis bold="true" box="[151,432,921,947]" italics="true" pageId="4" pageNumber="155">Euseboides motuoensis</emphasis>
Huang, Chen &amp; Li
</taxonomicName>
, 
<taxonomicNameLabel box="[686,780,921,947]" pageId="4" pageNumber="155" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="4.[151,780,921,981]" box="[151,331,956,981]" pageId="4" pageNumber="155">
(
<figureCitation box="[158,322,956,981]" captionStart-0="FIGURES 46 – 50" captionStart-1="FIGURES 51 &amp; 52" captionStart-2="FIGURES 53 – 57" captionStart-3="FIGURES 58 – 60" captionStart-4="FIGURES 61 – 65" captionStartId-0="18.[151,264,1233,1256]" captionStartId-1="19.[151,264,1471,1494]" captionStartId-2="20.[151,264,1227,1250]" captionStartId-3="21.[151,264,927,950]" captionStartId-4="21.[151,264,1912,1935]" captionTargetBox-0="[153,1436,387,1210]" captionTargetBox-1="[152,1436,329,1456]" captionTargetBox-2="[150,1436,535,1206]" captionTargetBox-3="[406,1213,193,906]" captionTargetBox-4="[151,1435,1030,1891]" captionTargetId-0="figure@18.[153,1438,387,1211]" captionTargetId-1="figure@19.[152,1438,329,1456]" captionTargetId-2="figure@20.[150,1436,535,1207]" captionTargetId-3="figure@21.[406,1213,193,907]" captionTargetId-4="figure@21.[151,1436,1030,1891]" captionTargetPageId-0="18" captionTargetPageId-1="19" captionTargetPageId-2="20" captionTargetPageId-3="21" captionTargetPageId-4="21" captionText-0="FIGURES 46 – 50. Euseboides motuoensis sp. nov. holotype, male. 46. dorsal view; 47. ventral view; 48. lateral view; 49. mandibles and palps; 50. frons. Scale bars A = 5 mm, B = 1 mm." captionText-1="FIGURES 51 &amp; 52. Euseboides motuoensis sp. nov. holotype, male. Elytra, ventral view. Scale bars = 1 mm." captionText-2="FIGURES 53 – 57. Euseboides motuoensis sp. nov. holotype, male. 53. &amp; 54. terminalia: 53. dorsal view, 54. ventral view; 55 – 57. aedeagus: tegmen, 55. dorsal view, 56. ventral view, 57. lateral view. Scale bars = 1 mm." captionText-3="FIGURES 58 – 60. Euseboides motuoensis sp. nov. holotype, male. Aedeagus: penis, 58. dorsal view, 59. ventral view, 60. lateral view. Scale bars = 1 mm." captionText-4="FIGURES 61 – 65. Euseboides motuoensis sp. nov. paratype, female. 61. dorsal view; 62. ventral view; 63. lateral view; 64. mandibles and palps; 65. frons. Scale bars A = 5 mm, B = 1 mm." httpUri-0="https://zenodo.org/record/253847/files/figure.png" httpUri-1="https://zenodo.org/record/253848/files/figure.png" httpUri-2="https://zenodo.org/record/253849/files/figure.png" httpUri-3="https://zenodo.org/record/253850/files/figure.png" httpUri-4="https://zenodo.org/record/253851/files/figure.png" pageId="4" pageNumber="155">
Figures 
<number box="[251,322,956,981]" pageId="4" pageNumber="155" value="55.5" valueMax="65.0" valueMin="46.0">46–65</number>
</figureCitation>
)
