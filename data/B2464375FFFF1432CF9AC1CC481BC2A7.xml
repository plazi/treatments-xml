</taxonomicName>
; E–F: hemelytron; E: 
<taxonomicName id="FDEF89E0FFFC1435CB8AC75C4F20C619" authority="Kim &amp; Damken &amp; Ahmad Sah &amp; Jung, 2019" authorityName="Kim &amp; Damken &amp; Ahmad Sah &amp; Jung" authorityYear="2019" box="[1159,1285,1365,1389]" class="Insecta" family="Miridae" genus="Kosmiomiris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="379" phylum="Arthropoda" rank="species" species="carvalhoi" status="sp. nov.">
<emphasis id="089B2E71FFFC1435CB8AC75C4F20C619" box="[1159,1285,1365,1389]" italics="true" pageId="2" pageNumber="379">K. carvalhoi</emphasis>
</taxonomicName>
<emphasis id="089B2E71FFFC1435CA06C75C4F79C619" bold="true" box="[1291,1372,1365,1389]" pageId="2" pageNumber="379">
<taxonomicNameLabel id="13A8930AFFFC1435CA06C75C4F79C619" box="[1291,1372,1365,1389]" pageId="2" pageNumber="379" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; F: 
<taxonomicName id="FDEF89E0FFFC1435CA88C75C4B38C6E5" authorityName="Kirkaldy" authorityYear="1902" class="Insecta" family="Miridae" genus="Kosmiomiris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="379" phylum="Arthropoda" rank="species" species="rubroornatus">
