<subSubSection id="C3535BDAFF92FFBEFF4BFF0BFCF1F8B7" box="[151,790,1826,1850]" pageId="1" pageNumber="33" type="materials_examined">
<paragraph id="8BF60851FF92FFBEFF4BFF0BFCF1F8B7" blockId="1.[151,790,1826,1850]" box="[151,790,1826,1850]" pageId="1" pageNumber="33">
