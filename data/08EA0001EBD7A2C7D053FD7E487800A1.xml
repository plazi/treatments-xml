<geoCoordinate id="ADDE433245D1FAED62F25E9CBBEAA0A4" direction="north" orientation="latitude" precision="55" value="33.439">33.439N</geoCoordinate>
, 
<geoCoordinate id="2306C83AFEFF06F8983C09B135DF08DE" direction="east" orientation="longitude" precision="55" value="133.086">133.086E</geoCoordinate>
