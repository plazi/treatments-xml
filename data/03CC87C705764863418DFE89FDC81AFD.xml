species:
</emphasis>
<taxonomicName id="4C654D52057648634036FE71FD4D1E8D" authority="Stal 1870 c: 737" authorityName="Stal" authorityPageNumber="737" authorityYear="1870" box="[300,687,436,461]" class="Insecta" family="Cicadellidae" genus="Hecalus" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="269" phylum="Arthropoda" rank="species" species="kirschbaumii">
