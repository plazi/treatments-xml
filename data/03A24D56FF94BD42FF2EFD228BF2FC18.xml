<taxonomicName id="4C0B87C3FF94BD42FBBAFCCE8820FCE5" class="Insecta" family="Staphylinidae" genus="Neosclerus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="1552" phylum="Arthropoda" rank="species" species="praeacutus">
