<taxonomicName id="4C0B87C3FF94BD42FEEAFC888B06FC18" box="[374,516,888,909]" class="Insecta" family="Staphylinidae" genus="Neosclerus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="1552" phylum="Arthropoda" rank="species" species="praeacutus">
