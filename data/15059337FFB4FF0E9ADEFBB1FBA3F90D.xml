<taxonomicName id="5AAC59A2FFB4FF0E9ADEF9F5FE4CF9D6" box="[151,441,1587,1612]" class="Actinopterygii" family="Macrouridae" genus="Ventrifossa" kingdom="Animalia" order="Gadiformes" pageId="13" pageNumber="38" phylum="Chordata" rank="species" species="macropogon">
