<collectingCountry id="E5BB62B1FFB4FF0E98A9F9F4FCB5F9D6" box="[736,832,1586,1612]" name="Jamaica" pageId="13" pageNumber="38">Jamaica</collectingCountry>
(
<geoCoordinate id="F89844E6FFB4FF0E991BF9F4FC31F9D6" box="[850,964,1586,1612]" direction="north" orientation="latitude" pageId="13" pageNumber="38" precision="925" value="16.583334">
