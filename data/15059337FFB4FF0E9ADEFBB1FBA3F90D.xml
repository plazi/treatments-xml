<number id="2B643006FFB4FF0E9934F9F4FC6AF9D6" box="[893,927,1586,1612]" pageId="13" pageNumber="38" value="35.0">35</number>
’ N
</geoCoordinate>
, 
<geoCoordinate id="F89844E6FFB4FF0E999BF9F4FBB9F9D6" box="[978,1100,1586,1612]" direction="west" orientation="longitude" pageId="13" pageNumber="38" precision="925" value="-80.166664">
