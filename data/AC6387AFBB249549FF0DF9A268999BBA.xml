<taxonomicName id="E3CA4D3ABB209549FE2CFDB16A789A4F" authority="Dillon &amp; Dillon, 1946" authorityName="Dillon &amp; Dillon" authorityYear="1946" box="[438,797,548,573]" class="Insecta" family="Cerambycidae" genus="Onocephala" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="345" phylum="Arthropoda" rank="species" species="tepahi">
<emphasis id="16BEEAABBB209549FE2CFDB16B7B9A4F" box="[438,542,548,573]" italics="true" pageId="6" pageNumber="345">O. tepahi</emphasis>
Dillon &amp; Dillon, 
<number id="9202249EBB209549FD7FFDB06A789A4F" box="[741,797,548,573]" pageId="6" pageNumber="345" value="1946.0">1946</number>
</taxonomicName>
