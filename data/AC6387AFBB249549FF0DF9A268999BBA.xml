<taxonomicName id="E3CA4D3ABB209549FA1BFDDC699B9AF7" class="Insecta" family="Cerambycidae" genus="Onocephala" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="345" phylum="Arthropoda" rank="species" species="thomsoni">
