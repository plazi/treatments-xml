<taxonomicName id="E3CA4D3ABB209549FBD6FDDC6C119A12" authority="Bates, 1887" authorityName="Bates" authorityYear="1887" box="[1100,1396,583,608]" class="Insecta" family="Cerambycidae" genus="Onocephala" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="345" phylum="Arthropoda" rank="species" species="suturalis">
