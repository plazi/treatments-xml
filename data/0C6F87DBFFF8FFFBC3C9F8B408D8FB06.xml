<caption id="D0B96645FFFFFFF9C3C9F81F0982F877" ID-DOI="http://doi.org/10.5281/zenodo.5321155" ID-Zenodo-Dep="5321155" httpUri="https://zenodo.org/record/5321155/files/figure.png" pageId="20" pageNumber="240" startId="20.[151,255,1972,1996]" targetBox="[162,1433,354,1913]" targetPageId="20">
