<collectionCode id="F8CB878FFFBCF51CFF19FC88FEEE462C" box="[244,324,839,862]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="11" pageNumber="412" type="Museum">RMNH</collectionCode>
