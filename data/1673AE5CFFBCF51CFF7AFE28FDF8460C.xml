<specimenCount id="88DCD4C3FFBCF51CFC69FD09FC1647AC" box="[900,956,708,734]" pageId="11" pageNumber="412" type="female">34 ♀</specimenCount>
<typeStatus id="4161A1E8FFBCF51CFC28FD08FB8147AD" box="[965,1067,711,735]" pageId="11" pageNumber="412">paratypes</typeStatus>
