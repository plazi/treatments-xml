: 
<location id="9B054991FFBCF51CFCAFFD89FC64472C" LSID="urn:lsid:plazi:treatment:1673AE5CFFBCF51CFF7AFE28FDF8460C:9B054991FFBCF51CFCAFFD89FC64472C" box="[834,974,582,606]" municipality="Canavese" name="St. Elisabetta" pageId="11" pageNumber="412" stateProvince="Piemonte">St. Elisabetta</location>
, 
<quantity id="5922B2AFFFBCF51CFC36FD89FB81472C" box="[987,1067,582,606]" metricMagnitude="3" metricUnit="m" metricValue="1.35" pageId="11" pageNumber="412" unit="m" value="1350.0">
