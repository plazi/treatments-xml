<collectionCode id="F8CB878FFFBCF51CFD6BFCE9FD7D464C" box="[646,727,806,830]" country="Switzerland" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="11" pageNumber="412" type="Museum">MHNG</collectionCode>
