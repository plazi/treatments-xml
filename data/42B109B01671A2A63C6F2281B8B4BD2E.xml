<taxonomicName id="FACABBD881D3C540DF5B917442E32988" class="Gastropoda" family="Hydrobiidae" genus="Pyrgula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pyrgula (Oxypyrgula) simplex" order="Littorinimorpha" pageId="61" pageNumber="92" phylum="Mollusca" rank="species" species="simplex" subGenus="Oxypyrgula">Pyrgula (Oxypyrgula) simplex</taxonomicName>
