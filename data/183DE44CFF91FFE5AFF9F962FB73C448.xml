, 
<elevation id="1BB9B269FF93FFE5AE42FD8DFD95C592" box="[379,560,635,661]" metricMagnitude="2" metricUnit="m" metricValue="1.8" pageId="19" pageNumber="18" unit="m" value="180.0">
