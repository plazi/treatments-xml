<collectingCountry id="E88315CAFF93FFE5AE67FDAEFE06C575" box="[350,419,600,626]" name="Brazil" pageId="19" pageNumber="18">Brazil</collectingCountry>
: 
<collectingRegion id="52509BB8FF93FFE5AE92FDAEFDB9C575" box="[427,540,600,626]" country="Brazil" name="Rondonia" pageId="19" pageNumber="18">Rondônia</collectingRegion>
