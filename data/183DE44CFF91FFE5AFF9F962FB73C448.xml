<quantity id="576CF8BFFF93FFE5AEDEFD8DFD95C592" box="[487,560,635,661]" metricMagnitude="2" metricUnit="m" metricValue="1.8" pageId="19" pageNumber="18" unit="m" value="180.0">180 m</quantity>
