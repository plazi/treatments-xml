<quantity id="8D149B56FFD24F65FCBBFCAA44CDFDFA" box="[892,948,505,530]" metricMagnitude="1" metricUnit="m" metricValue="4.706619999999999" pageId="52" pageNumber="53" unit="in" value="1853.0">1853</quantity>
