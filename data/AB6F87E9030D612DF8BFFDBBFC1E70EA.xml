<taxonomicName id="E4C64D7C030C612AF8C2FCFCFED27210" authorityName="Macaulay &amp; Schmidt" authorityYear="2009" box="[241,379,779,800]" class="Insecta" family="Arctiidae" genus="Dodia" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="82" phylum="Arthropoda" rank="species" species="tarandus">
