<taxonomicName id="4C654D356C088A03FF45FDD146F9FD82" baseAuthorityName="Ehrmann" baseAuthorityYear="1922" box="[189,496,529,555]" class="Gastropoda" family="Plectopylidae" genus="Chersaecia" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="36" pageNumber="37" phylum="Mollusca" rank="species" species="goniobathmos">
<emphasis id="B911EAA46C088A03FF45FDD146F9FD82" box="[189,496,529,555]" italics="true" pageId="36" pageNumber="37">Chersaecia goniobathmos</emphasis>
