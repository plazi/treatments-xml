<materialsCitation id="CBB93CCAFF81FFC15A81FE1DE50CFE4D" box="[408,576,419,443]" collectionCode="MNHU" country="Brazil" location="Pebas" pageId="32" pageNumber="31" specimenCount="1" typeStatus="lectotype">
