<typeStatus id="6D5275D1FF9BA7168287FCDC287DFCE8" box="[746,798,820,841]" pageId="15" pageNumber="216">Type</typeStatus>
species: 
<taxonomicName id="75E9B0F0FF9BA71683E3FCDB2E73FCE8" authority="Fallen, 1826" authorityName="Fallen" authorityYear="1826" box="[910,1296,819,841]" class="Insecta" family="Muscidae" genus="Cordilura" kingdom="Animalia" order="Diptera" pageId="15" pageNumber="216" phylum="Arthropoda" rank="species" species="loxocerata">
