<quantity box="[272,357,778,802]" metricMagnitude="-2" metricUnit="m" metricValue="1.75" metricValueMax="2.0" metricValueMin="1.5" pageId="3" pageNumber="170" unit="cm" value="1.75" valueMax="2.0" valueMin="1.5">1,5-2 cm</quantity>
