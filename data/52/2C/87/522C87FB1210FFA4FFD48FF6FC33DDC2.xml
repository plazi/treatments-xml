<quantity metricMagnitude="-2" metricUnit="m" metricValue="1.15" metricValueMax="1.5" metricValueMin="0.8" pageId="3" pageNumber="170" unit="cm" value="1.15" valueMax="1.5" valueMin="0.8">0,8-1,5 cm</quantity>
foliis, 
<quantity box="[214,313,804,828]" metricMagnitude="-1" metricUnit="m" metricValue="3.5" metricValueMax="4.5" metricValueMin="2.5" pageId="3" pageNumber="170" unit="cm" value="35.0" valueMax="45.0" valueMin="25.0">25-45 cm</quantity>
