<taxonomicName authorityName="Chen &amp; Zhang &amp; Chen &amp; Freyhof" authorityYear="2019" box="[1320,1435,838,859]" class="Actinopterygii" family="Nemacheilidae" genus="Barbatula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cypriniformes" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="emuensis">
