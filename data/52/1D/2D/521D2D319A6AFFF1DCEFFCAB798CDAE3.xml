<subSubSection box="[199,1373,692,717]" pageId="2" pageNumber="534" type="etymology">
<paragraph blockId="2.[151,1437,151,938]" box="[199,1373,692,717]" pageId="2" pageNumber="534">
<emphasis bold="true" box="[199,334,692,717]" pageId="2" pageNumber="534">Etymology.</emphasis>
