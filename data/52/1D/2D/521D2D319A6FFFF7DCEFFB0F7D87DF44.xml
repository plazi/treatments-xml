<subSubSection pageId="4" pageNumber="536" type="materials_examined">
<paragraph blockId="4.[151,1436,1344,1549]" pageId="4" pageNumber="536">
<emphasis bold="true" box="[199,446,1416,1441]" pageId="4" pageNumber="536">Material examined.</emphasis>
Western 
<collectingCountry box="[578,626,1417,1440]" name="Iran" pageId="4" pageNumber="536">Iran</collectingCountry>
, 
<collectingRegion box="[651,872,1416,1441]" country="Iran" name="Lorestan" pageId="4" pageNumber="536">Lorestan Province</collectingRegion>
, Borujerd city, Fial village (
<geoCoordinate box="[1254,1431,1416,1441]" direction="north" orientation="latitude" pageId="4" pageNumber="536" precision="1" value="33.89753">33°53'51.1&quot; N</geoCoordinate>
