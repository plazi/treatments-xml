<taxonomicName box="[954,1051,1453,1476]" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="536" phylum="Arthropoda" rank="genus">
