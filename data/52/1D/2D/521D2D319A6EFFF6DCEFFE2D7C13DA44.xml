<taxonomicName authority="Latr." authorityName="Latr." box="[1010,1260,542,565]" class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="537" phylum="Arthropoda" rank="species" species="gagates">
