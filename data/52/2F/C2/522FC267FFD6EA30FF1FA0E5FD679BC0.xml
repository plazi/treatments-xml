<elevation box="[1038,1192,1402,1427]" metricMagnitude="3" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="2" pageNumber="577" unit="m" value="1400.0" valueMax="1500.0" valueMin="1300.0">
<quantity box="[1038,1192,1402,1427]" metricMagnitude="3" metricUnit="m" metricValue="1.4" metricValueMax="1.5" metricValueMin="1.3" pageId="2" pageNumber="577" unit="m" value="1400.0" valueMax="1500.0" valueMin="1300.0">
