<taxonomicName authorityName="Mulsant, Gordon" authorityYear="1971" box="[1001,1110,1044,1069]" class="Insecta" family="Coccinellidae" genus="Coccinella" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="48" phylum="Arthropoda" rank="species" species="areata">
<emphasis box="[1001,1110,1044,1069]" italics="true" pageId="35" pageNumber="48">C. areata</emphasis>
</taxonomicName>
(= 
<taxonomicName authorityName="González &amp; Vandenberg" authorityYear="2006" baseAuthorityName="González &amp; Vandenberg" baseAuthorityYear="2006" box="[1144,1224,1044,1069]" class="Insecta" family="Coccinellidae" genus="Cycloneda" kingdom="Animalia" order="Coleoptera" pageId="35" pageNumber="48" phylum="Arthropoda" rank="species" species="sicardi">
<emphasis box="[1144,1224,1044,1069]" italics="true" pageId="35" pageNumber="48">sicardi</emphasis>
</taxonomicName>
).
</paragraph>
</subSubSection>
