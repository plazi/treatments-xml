<subSubSection pageId="35" pageNumber="48" type="discussion">
<paragraph blockId="35.[264,1325,284,1230]" pageId="35" pageNumber="48">
<emphasis bold="true" box="[312,435,1004,1030]" pageId="35" pageNumber="48">Remarks:</emphasis>
Mulsant (
<number box="[562,623,1004,1030]" pageId="35" pageNumber="48" value="1866.0">1866</number>
