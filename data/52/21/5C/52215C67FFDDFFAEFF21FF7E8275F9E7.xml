; 
<taxonomicName authority="Platonova, 1971" authorityName="Platonova" authorityYear="1971" box="[1048,1388,366,391]" class="Chromadorea" family="Chromadoridae" genus="Spilophorella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="44" pageNumber="45" phylum="Nematoda" rank="species" species="unidentata">
