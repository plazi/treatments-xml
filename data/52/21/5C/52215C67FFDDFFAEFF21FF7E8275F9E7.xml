</taxonomicName>
; 
<taxonomicName authority="Gerlach, 1955" authorityName="Gerlach" authorityYear="1955" box="[759,1092,330,355]" class="Chromadorea" family="Chromadoridae" genus="Spilophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chromadorida" pageId="44" pageNumber="45" phylum="Nematoda" rank="species" species="meyerabichi">
<emphasis box="[759,924,330,355]" italics="true" pageId="44" pageNumber="45">S. meyerabichi</emphasis>
