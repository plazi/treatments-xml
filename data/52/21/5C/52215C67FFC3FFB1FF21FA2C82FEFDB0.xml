<emphasis bold="true" box="[151,501,1806,1831]" pageId="50" pageNumber="51">
Genus 
<taxonomicName authority="Kreis, 1929" authorityName="Kreis" authorityYear="1929" box="[234,501,1806,1831]" class="Chromadorea" family="Chromadoridae" genus="Deltanema" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="50" pageNumber="51" phylum="Nematoda" rank="genus">
<emphasis bold="true" box="[234,360,1806,1831]" italics="true" pageId="50" pageNumber="51">Deltanema</emphasis>
