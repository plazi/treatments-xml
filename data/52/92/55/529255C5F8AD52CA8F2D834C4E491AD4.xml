<paragraph pageId="0" pageNumber="127">
<taxonomicName authority="Weber, 1801" authorityName="Weber" authorityYear="1801" class="Insecta" family="Endomychidae" genus="Eumorphus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Eumorphus satanus" order="Coleoptera" pageId="0" pageNumber="127" phylum="Arthropoda" rank="species" species="satanus">
