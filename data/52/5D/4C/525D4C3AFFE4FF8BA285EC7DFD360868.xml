<typeStatus box="[519,677,649,675]" pageId="14" pageNumber="15">paralectotype</typeStatus>
of 
<taxonomicName authority="Nutting, 1904" authorityName="Nutting" authorityYear="1904" box="[716,1183,649,675]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="14" pageNumber="15" phylum="Cnidaria" rank="species" species="meridionalis">
