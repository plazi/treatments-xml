<taxonomicName box="[1142,1274,310,331]" pageId="8" pageNumber="41" rank="subFamily" subFamily="Pachyseiinae">
<emphasis bold="true" box="[1142,1274,310,331]" pageId="8" pageNumber="9">Pachyseiinae</emphasis>
</taxonomicName>
