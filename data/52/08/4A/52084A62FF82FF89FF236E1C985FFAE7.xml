<typeStatus box="[475,525,1158,1179]" pageId="48" pageNumber="49">Type</typeStatus>
species 
<taxonomicName authority="Berlese, 1916" authorityName="Berlese" authorityYear="1916" box="[612,1127,1157,1179]" class="Arachnida" family="Ameroseiidae" genus="Epicriopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="terrificans" subGenus="Actinoseius">
