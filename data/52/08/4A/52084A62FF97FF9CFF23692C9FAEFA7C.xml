species 
<taxonomicName authority="Berlese, 1910" authorityName="Berlese" authorityYear="1910" box="[287,866,884,906]" class="Arachnida" family="Pachylaelapidae" genus="Pachylaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="fenestratus" subGenus="Elaphrolaelaps">
