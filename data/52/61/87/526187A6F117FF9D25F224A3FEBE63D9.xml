<number box="[387,396,417,431]" pageId="2" pageNumber="242" value="2.0">2</number>
; d-cell with shortened base of M 
<number box="[764,771,417,431]" pageId="2" pageNumber="242" value="3.0">3</number>
+ 
<number box="[780,788,417,431]" pageId="2" pageNumber="242" value="4.0">4</number>
