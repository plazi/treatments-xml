<number box="[554,562,1032,1046]" pageId="5" pageNumber="378" value="2.0">2</number>
, M 
<number box="[601,609,1032,1046]" pageId="5" pageNumber="378" value="4.0">4</number>
) and 
<number box="[673,687,1018,1041]" pageId="5" pageNumber="378" value="7.0">7</number>
sutural (S0, S 
<number box="[840,848,1032,1046]" pageId="5" pageNumber="378" value="2.0">2</number>
