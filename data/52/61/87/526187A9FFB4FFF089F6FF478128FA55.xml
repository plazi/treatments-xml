p, m 
<number box="[175,190,977,1001]" pageId="7" pageNumber="380" value="4.0">4</number>
, a 
<number box="[214,228,976,1001]" pageId="7" pageNumber="380" value="5.0">5</number>
, m 
<number box="[265,278,976,1001]" pageId="7" pageNumber="380" value="5.0">5</number>
, rarely a 
<number box="[375,389,976,1001]" pageId="7" pageNumber="380" value="6.0">6</number>
) macrochaetae. Abd. II with 
<number box="[715,729,977,1001]" pageId="7" pageNumber="380" value="4.0">4</number>
central (a 
<number box="[841,856,977,1001]" pageId="7" pageNumber="380" value="2.0">2</number>
, a 
<number box="[882,896,977,1001]" pageId="7" pageNumber="380" value="3.0">3</number>
, m 
<number box="[933,946,977,1001]" pageId="7" pageNumber="380" value="3.0">3</number>
, m 
<number box="[981,993,977,1001]" pageId="7" pageNumber="380" value="3.0">3</number>
e) and 
<number box="[1068,1082,977,1001]" pageId="7" pageNumber="380" value="2.0">2</number>
