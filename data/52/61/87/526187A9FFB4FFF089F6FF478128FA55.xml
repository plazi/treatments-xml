<number box="[605,613,595,609]" pageId="7" pageNumber="380" value="4.0">4</number>
) and 
<number box="[678,692,581,604]" pageId="7" pageNumber="380" value="7.0">7</number>
sutural (S0, S 
<number box="[847,855,595,609]" pageId="7" pageNumber="380" value="2.0">2</number>
, S 
<number box="[885,893,595,609]" pageId="7" pageNumber="380" value="3.0">3</number>
, S 
<number box="[923,931,595,609]" pageId="7" pageNumber="380" value="4.0">4</number>
, S 
