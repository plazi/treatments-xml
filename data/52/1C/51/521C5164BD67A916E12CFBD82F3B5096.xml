<paragraph pageId="0" pageNumber="89">
<taxonomicName class="Insecta" family="Mordellidae" genus="Mordellistena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mordellistena irritans" order="Coleoptera" pageId="0" pageNumber="89" phylum="Arthropoda" rank="species" species="irritans">Mordellistena irritans</taxonomicName>
