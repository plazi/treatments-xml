<number box="[927,990,804,830]" pageId="12" pageNumber="19" value="1990.0">1990</number>
b: 
<number box="[1017,1048,804,830]" pageId="12" pageNumber="19" value="32.0">32</number>
)
</bibRefCitation>
and Roselaar (
<quantity box="[1233,1323,804,830]" metricMagnitude="1" metricUnit="m" metricValue="5.06222" pageId="12" pageNumber="19" unit="in" value="1993.0">1993 in</quantity>
Cramp &amp; Perrins 
