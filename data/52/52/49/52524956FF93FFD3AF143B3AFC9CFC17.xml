</taxonomicName>
BUTURLIN, 
<number box="[643,701,1284,1310]" pageId="16" pageNumber="23" value="1911.0">1911</number>
[Krasnoyarsk]; 
<taxonomicName box="[892,1173,1284,1309]" class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="16" pageNumber="23" phylum="Chordata" rank="subSpecies" species="cyanus" subSpecies="tianschanicus">
<emphasis box="[892,1173,1284,1309]" italics="true" pageId="16" pageNumber="23">P. cyanus tianschanicus</emphasis>
</taxonomicName>
(MENZBIER, 
<number box="[264,324,1324,1350]" pageId="16" pageNumber="23" value="1884.0">1884</number>
) [loc. typ. restr. Tian Shan, syn. 
<emphasis box="[704,810,1324,1349]" italics="true" pageId="16" pageNumber="23">apeliotes</emphasis>
<bibRefCitation author="MEISE" box="[817,968,1324,1350]" pageId="16" pageNumber="51" refString="Meise, W. (1934) Die Vogelwelt der Mandschurei. Abhandlungen und Berichte der Museen fur Tierkunde und Volkerkunde Dresden XVIII (2), 86 pp." type="book" year="1934">
