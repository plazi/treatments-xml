(DEMENTIEW &amp; HEPTNER, 
<number box="[781,841,1244,1270]" pageId="16" pageNumber="23" value="1932.0">1932</number>
) [Kustanai, Turgai/Kirgisan steppe]; 
<taxonomicName class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="16" pageNumber="23" phylum="Chordata" rank="subSpecies" species="cyanus" subSpecies="yenisseensis">
