</taxonomicName>
PALLAS, 
<number box="[1015,1074,1204,1230]" pageId="16" pageNumber="23" value="1770.0">1770</number>
[Volga]; 
<taxonomicName class="Aves" family="Paridae" genus="Parus" kingdom="Animalia" order="Passeriformes" pageId="16" pageNumber="23" phylum="Chordata" rank="subSpecies" species="cyanus" subSpecies="hyperriphaeus">
