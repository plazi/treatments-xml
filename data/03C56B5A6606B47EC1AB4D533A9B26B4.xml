<taxonomicName id="4C6CA1CF6606B47EC0544CD839B22690" box="[360,424,1960,1985]" class="Magnoliopsida" family="Betulaceae" genus="Alnus" kingdom="Plantae" order="Fagales" pageId="83" pageNumber="84" phylum="Tracheophyta" rank="genus">
