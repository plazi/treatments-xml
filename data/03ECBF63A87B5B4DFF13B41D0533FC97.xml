<treatmentCitationGroup id="AB55295BA87B5B4DFF13B44802AEFD6A" box="[151,1231,626,649]" pageId="1" pageNumber="54">
<taxonomicName id="4C4575F6A87B5B4DFF13B4480470FD6A" authority="Suarez 1965a: 570" authorityName="Suarez" authorityPageNumber="570" authorityYear="1965" box="[151,529,626,649]" class="Insecta" family="Mutillidae" genus="Smicromyrmilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="54" phylum="Arthropoda" rank="genus">
<emphasis id="B931D267A87B5B4DFF13B4480723FD6A" box="[151,322,626,649]" italics="true" pageId="1" pageNumber="54">Smicromyrmilla</emphasis>
<treatmentCitation id="0AE42864A87B5B4DFECCB4480470FD6A" author="Suarez" box="[328,529,626,649]" page="570" pageId="1" pageNumber="54" year="1965">
<bibRefCitation id="EFD47384A87B5B4DFECCB44807BCFD6A" author="Suarez" box="[328,477,626,649]" pageId="1" pageNumber="60" refString="Suarez, F. J. (1965 a) Datos preliminares al estudio de los mutilidos Ibericos (Hymenoptera). Eos, Madrid, 40 (3 / 4), 569 - 586." type="journal article" year="1965" yearSuffix="a">
Suárez 
<number id="3D8D1C52A87B5B4DFE13B44807B1FD6A" box="[407,464,626,649]" pageId="1" pageNumber="54" value="1965.0">1965</number>
a
</bibRefCitation>
: 
<number id="3D8D1C52A87B5B4DFE63B4480470FD6A" box="[487,529,626,649]" pageId="1" pageNumber="54" value="570.0">570</number>
</treatmentCitation>
</taxonomicName>
, 
<typeStatus id="54FEB0D7A87B5B4DFD98B448042BFD6A" box="[540,586,626,649]" pageId="1" pageNumber="54">type</typeStatus>
species: 
<taxonomicName id="4C4575F6A87B5B4DFD28B44805A7FD6A" authority="Andre 1896" authorityName="Andre" authorityYear="1896" box="[684,966,626,649]" class="Insecta" family="Mutillidae" genus="Mutilla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="54" phylum="Arthropoda" rank="species" species="ariasi">
<emphasis id="B931D267A87B5B4DFD28B448055EFD6A" box="[684,831,626,649]" italics="true" pageId="1" pageNumber="54">Mutilla ariasi</emphasis>
<bibRefCitation id="EFD47384A87B5B4DFCC1B44805A7FD6A" author="Andre" box="[837,966,626,649]" pageId="1" pageNumber="59" refString="Andre, E. (1896) Mutillides nouveaux ou imparfaitement connus faisant partie des collections du Musee National de Hongrie. Termeszetrajzi Fuzetek, 19 (1), 9 - 25." type="journal article" year="1896">
André 
<number id="3D8D1C52A87B5B4DFC0AB44805A7FD6A" box="[910,966,626,649]" pageId="1" pageNumber="54" value="1896.0">1896</number>
</bibRefCitation>
</taxonomicName>
