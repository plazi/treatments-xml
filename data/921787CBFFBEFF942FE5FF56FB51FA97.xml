<taxonomicName id="DDBE4D5EFFBEFF942EE9FE73FDBCFE60" baseAuthorityName="Ueno" baseAuthorityYear="1954" box="[411,529,434,459]" class="Insecta" family="Carabidae" genus="Cillenus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="tsutsuii">
