<taxonomicName id="4C754D64337B4474EA1EFA2EFD1681C5" authority="Nakanishi" authorityName="Nakanishi" box="[331,702,1412,1437]" class="Insecta" family="Ichneumonidae" genus="Sussaba" kingdom="Animalia" order="Hymenoptera" pageId="8" phylum="Arthropoda" rank="species" species="dajsetsuzana">
