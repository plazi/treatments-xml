<emphasis id="B924AD2CFF9FFFC9FBEDFF32991EFEA6" bold="true" box="[1101,1112,231,257]" pageId="11" pageNumber="50">I</emphasis>
(
<emphasis id="B924AD2CFF9FFFC9FBCCFF3D99CFFEA6" box="[1132,1161,232,257]" italics="true" pageId="11" pageNumber="50">
<number id="3D986319FF9FFFC9FBCCFF3D993FFEA6" box="[1132,1145,232,257]" pageId="11" pageNumber="50" value="1.0">1</number>
b
</emphasis>
) 
<number id="3D986319FF9FFFC9FB3DFF3D99FCFEA5" box="[1181,1210,232,258]" pageId="11" pageNumber="50" value="20.0">20</number>
(
<number id="3D986319FF9FFFC9FB6DFF3D985DFEA5" box="[1229,1307,232,258]" pageId="11" pageNumber="50" value="19.0" valueMax="22.0" valueMin="16.0">16–22</number>
), proximal setae on basal coxisternum 
<emphasis id="B924AD2CFF9FFFC9FE79FEDA9CA2FE8E" bold="true" box="[473,484,271,297]" pageId="11" pageNumber="50">I</emphasis>
(
<emphasis id="B924AD2CFF9FFFC9FE55FEC59F54FE8E" box="[501,530,272,297]" italics="true" pageId="11" pageNumber="50">
