) wide, 
<number id="3D986319FF9EFFC8FB01F8ED998BF8F5" box="[1185,1229,1848,1874]" pageId="10" pageNumber="49" value="105.0">105</number>
(
<number id="3D986319FF9EFFC8FB7FF8ED9801F8F5" box="[1247,1351,1848,1874]" pageId="10" pageNumber="49" value="106.5" valueMax="113.0" valueMin="100.0">100–113</number>
) thick; light yellow. Gnathosoma 
<number id="3D986319FF9EFFC8FE70F8B59CABF8DD" box="[464,493,1888,1914]" pageId="10" pageNumber="49" value="70.0">70</number>
(
<number id="3D986319FF9EFFC8FE5FF8B59F0BF8DD" box="[511,589,1888,1914]" pageId="10" pageNumber="49" value="71.0" valueMax="75.0" valueMin="67.0">67–75</number>
), pedipalp coxal setae (
<emphasis id="B924AD2CFF9EFFC8FCC6F8B49EC4F8DD" box="[870,898,1889,1914]" italics="true" pageId="10" pageNumber="49">ep</emphasis>
) 
<number id="3D986319FF9EFFC8FC34F8B59EE5F8DD" box="[916,931,1888,1914]" pageId="10" pageNumber="49" value="2.0">2</number>
(
<number id="3D986319FF9EFFC8FC15F8B59EA2F8DD" box="[949,996,1888,1914]" pageId="10" pageNumber="49" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis id="B924AD2CFF9EFFC8FAFDF8B4982DF8DD" box="[1373,1387,1889,1914]" italics="true" pageId="10" pageNumber="49">d</emphasis>
) 
<number id="3D986319FF9EFFC8FADEF8B598DDF8DD" box="[1406,1435,1888,1914]" pageId="10" pageNumber="49" value="15.0">15</number>
(
<number id="3D986319FF9EFFC8FF00F85D9DABF805" box="[160,237,1928,1954]" pageId="10" pageNumber="49" value="14.0" valueMax="15.0" valueMin="13.0">13–15</number>
), gnathosoma projecting downwards. 
<emphasis id="B924AD2CFF9EFFC8FD0BF85D9E31F805" bold="true" box="[683,887,1928,1954]" pageId="10" pageNumber="49">Prodorsal shield</emphasis>
<number id="3D986319FF9EFFC8FCDEF85D9EDDF805" box="[894,923,1928,1954]" pageId="10" pageNumber="49" value="43.0">43</number>
(
<number id="3D986319FF9EFFC8FC0CF85D9EBCF805" box="[940,1018,1928,1954]" pageId="10" pageNumber="49" value="44.0" valueMax="45.0" valueMin="43.0">43–45</number>
), 
<number id="3D986319FF9EFFC8FBAFF85D996AF805" box="[1039,1068,1928,1954]" pageId="10" pageNumber="49" value="80.0">80</number>
(
<number id="3D986319FF9EFFC8FB9CF85D99CEF805" box="[1084,1160,1928,1954]" pageId="10" pageNumber="49" value="77.5" valueMax="80.0" valueMin="75.0">75–80</number>
