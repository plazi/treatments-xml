<taxonomicName id="4C714D14FF90FFDAFF714B8CFD15FA2C" authority="Zhang &amp; Zhang 2000: 153" authorityName="Zhang &amp; Zhang" authorityPageNumber="153" authorityYear="2000" box="[243,667,1490,1513]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="92" pageNumber="93" phylum="Arthropoda" rank="species" species="triangulatus">
