<subSubSection id="C32DEFF7E1100D50DFD94A83FA32FA79" box="[199,1435,1414,1441]" pageId="2" pageNumber="79" type="etymology">
<paragraph id="8B88BC7CE1100D50DFD94A83FA32FA79" blockId="2.[151,1437,981,1441]" box="[199,1435,1414,1441]" pageId="2" pageNumber="79">
<emphasis id="B943606EE1100D50DFD94A83FEE7FA78" bold="true" box="[199,334,1414,1440]" pageId="2" pageNumber="79">Etymology.</emphasis>
