<taxonomicName id="4C37C7FFE1100D50DB064A82FBCFFA79" box="[1048,1126,1415,1441]" class="Magnoliopsida" family="Fabaceae" genus="Acacia" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="79" phylum="Tracheophyta" rank="genus">
