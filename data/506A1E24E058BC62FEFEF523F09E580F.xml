<taxonomicName id="1FC3D4B1E05ABC62FCF2F052F6A45807" authorityName="Londt" authorityYear="1983" box="[805,972,534,569]" class="Insecta" family="Asilidae" genus="Remotomyia" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="306" phylum="Arthropoda" rank="genus">
