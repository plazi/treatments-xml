<taxonomicName id="1FC3D4B1E05BBC63FEB1FB59F7FE5300" authority="Londt, 1983" authorityName="Londt" authorityYear="1983" box="[358,662,2333,2366]" class="Insecta" family="Asilidae" genus="Remotomyia" kingdom="Animalia" order="Diptera" pageId="22" pageNumber="305" phylum="Arthropoda" rank="species" species="longipalpus" status="spec. nov .">
