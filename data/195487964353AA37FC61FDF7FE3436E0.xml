<taxonomicName id="56FD4D034353AA36FD2BF866FC8834BD" baseAuthorityName="Harrison" baseAuthorityYear="1976" box="[676,820,1978,1999]" class="Insecta" family="Canacidae" genus="Zalea" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="53" phylum="Arthropoda" rank="species" species="horningi">
