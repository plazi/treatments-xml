</taxonomicName>
; 
<taxonomicName id="243D3B06A581372DAD8F512171CE67DE" class="Insecta" family="Curculionidae" genus="Synergus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synergus dubiosus" order="Hymenoptera" pageId="0" pageNumber="68558" phylum="Arthropoda" rank="species" species="dubiosus">
<emphasis id="574658F45D375F3CCD093BCE5041CC79" italics="true" pageId="0" pageNumber="68558">Synergus dubiosus</emphasis>
