<taxonomicName id="4E0FCBAFDC5C1CF86721825E7FFFB986" genus="Q." lsidName="Q. agrifolia" pageId="0" pageNumber="68558" rank="species" species="agrifolia">
<emphasis id="12A7905E43139583DE660CC136DA8F6A" italics="true" pageId="0" pageNumber="68558">Q. agrifolia</emphasis>
</taxonomicName>
