<taxonomicName id="E073D6D3883D332980F2C020AF80AB27" authority="Arrow, 1920" authorityName="Arrow" authorityYear="1920" class="Insecta" family="Endomychidae" genus="Brachytrycherus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Brachytrycherus curviantennae" order="Coleoptera" pageId="0" pageNumber="85" phylum="Arthropoda" rank="species" species="curviantennae">Brachytrycherus curviantennae</taxonomicName>
