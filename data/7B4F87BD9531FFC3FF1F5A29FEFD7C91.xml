<collectingRegion id="3122F849953FFFC3FB405F86FAF27CC1" box="[1224,1318,392,418]" country="United States of America" name="Georgia" pageId="41" pageNumber="42">Georgia</collectingRegion>
(
<collectingCountry id="8BF1763B953FFFC3FABF5F86FAA17CC1" box="[1335,1397,392,418]" name="United States of America" pageId="41" pageNumber="42">USA</collectingCountry>
) to 
