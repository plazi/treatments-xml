<taxonomicName id="4C631BDD6F42E06BFE4DF8CCFD9CF8EA" box="[489,608,1821,1846]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="488" phylum="Arthropoda" rank="species" species="hanseni">
