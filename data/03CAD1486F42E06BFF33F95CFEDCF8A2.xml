<taxonomicName id="4C631BDD6F42E06BFD9DF890FD51F886" box="[569,685,1857,1882]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="5" pageNumber="488" phylum="Arthropoda" rank="species" species="hanseni">
<emphasis id="B917BC4C6F42E06BFD9DF890FD51F886" box="[569,685,1857,1882]" italics="true" pageId="5" pageNumber="488">L. hanseni</emphasis>
</taxonomicName>
in 
<collectingCountry id="F37420CE6F42E06BFD75F893FCCCF886" box="[721,816,1857,1882]" name="Vietnam" pageId="5" pageNumber="488">Vietnam</collectingCountry>
