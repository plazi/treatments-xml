female. 
<taxonomicName id="4D8D4D2AFFFDFFBFFA87F90AFA2AF8E7" authorityName="Ausserer" authorityYear="1871" box="[1332,1436,1771,1797]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="32" pageNumber="240" phylum="Arthropoda" rank="genus">Idiosoma</taxonomicName>
