<taxonomicName id="4D8D4D2AFFFDFFBFFE4DF8D2FDD3F8AF" authorityName="Ausserer" authorityYear="1871" box="[510,613,1843,1869]" class="Arachnida" family="Idiopidae" genus="Idiosoma" kingdom="Animalia" order="Araneae" pageId="32" pageNumber="240" phylum="Arthropoda" rank="genus">idiosoma</taxonomicName>
