<quantity id="4D759B4CFFFDFFBFFD3DF8EFFCBBF8CB" box="[654,781,1806,1833]" metricMagnitude="0" metricUnit="m" metricValue="3.7845999999999997" metricValueMax="3.8354" metricValueMin="3.7338" pageId="32" pageNumber="233" unit="in" value="149.0" valueMax="151.0" valueMin="147.0">147–151 in</quantity>
female. 
<taxonomicName id="4D8D4D2AFFFDFFBFFCDFF8EEFC4CF8CB" authorityName="Krefft" authorityYear="1871" box="[876,1018,1807,1833]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="32" pageNumber="203" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
width 60 (
<taxonomicName id="4D8D4D2AFFFDFFBFFBC2F8EEFB4CF8CB" authorityName="Krefft" authorityYear="1871" box="[1137,1274,1807,1833]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="32" pageNumber="203" phylum="Nematoda" rank="genus">gnathosoma</taxonomicName>
