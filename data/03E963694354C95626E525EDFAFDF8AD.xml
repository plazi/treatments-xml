<quantity id="4CB87F9A4354C95625F826E9FC78F8AD" box="[906,996,1779,1804]" metricMagnitude="3" metricUnit="m" metricValue="1.6" pageId="51" pageNumber="52" unit="m" value="1600.0">1600 m</quantity>
