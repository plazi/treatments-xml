<taxonomicName id="4C7E4D6C3570FFFE1891EC6E16ACE007" baseAuthorityName="Germain" baseAuthorityYear="1918" box="[1271,1429,542,567]" class="Gastropoda" family="Planorbidae" genus="Armiger" kingdom="Animalia" order="Hygrophila" pageId="19" pageNumber="64" phylum="Mollusca" rank="species" species="annandalei">
<emphasis id="B90AEAFD3570FFFE1891EC6E16ACE007" box="[1271,1429,542,567]" italics="true" pageId="19" pageNumber="64">A. annandalei</emphasis>
</taxonomicName>
