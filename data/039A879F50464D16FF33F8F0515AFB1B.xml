<taxonomicName id="4C334D0A50474D16FE23FD8E574DFEB8" box="[391,585,635,660]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="1" pageNumber="56" phylum="Tracheophyta" rank="species" species="campestris">
