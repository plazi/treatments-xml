</collectingRegion>
to 
<collectingCountry id="10F5899EFFACDF3EFF1E93E1FF7428A7" box="[151,246,404,429]" name="Ecuador" pageId="5" pageNumber="194">Ecuador</collectingCountry>
, 
<collectingCountry id="10F5899EFFACDF3EFE8893E1FEE128A7" box="[257,355,404,429]" name="Trinidad and Tobago" pageId="5" pageNumber="194">Trinidad</collectingCountry>
, 
<collectingCountry id="10F5899EFFACDF3EFEE793E1FE6328A7" box="[366,481,404,429]" name="Venezuela" pageId="5" pageNumber="194">Venezuela</collectingCountry>
and northeastern 
