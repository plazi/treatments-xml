<number id="072824F0D173FFE687C15D67FD22FDBB" box="[733,765,578,602]" pageId="6" pageNumber="69" value="12.0">12</number>
</figureCitation>
): 
<emphasis id="8394EAC5D173FFE6860E5D67FC54FDBA" bold="true" box="[786,907,578,603]" pageId="6" pageNumber="69">
