<taxonomicName id="4C03DED40F70FFF805B757B6B87EF90D" box="[937,1079,1679,1705]" class="Malacostraca" family="Alpheidae" genus="Leptalpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="687" phylum="Arthropoda" rank="species" species="pacificus">
