<geoCoordinate id="DBF37CBD6C69092726C085041DE17799" direction="south" orientation="latitude" precision="9" value="-34.678165">34°40.69'S</geoCoordinate>
, 
<geoCoordinate id="FADC432C4FC186B66386F34EBB49DA04" direction="east" orientation="longitude" precision="1" value="117.85408">117°51.245'E</geoCoordinate>
, 6 VIII 2004, karri forest (
<taxonomicName id="7D2E687267526771430FDC3A72A0ADF5" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eucalyptus diversicolor" order="Myrtales" pageId="7" pageNumber="118" phylum="Tracheophyta" rank="species" species="diversicolor">Eucalyptus diversicolor</taxonomicName>
