<taxonomicName id="1189CFA60B8AC110DB9E35B42F21C432" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eucalyptus" order="Myrtales" pageId="7" pageNumber="118" phylum="Tracheophyta" rank="genus">Eucalyptus</taxonomicName>
