<geoCoordinate id="C9EBE4597DAE5810A6AA5C841BC9DF89" degrees="29.4283" direction="north" orientation="latitude" precision="5" value="29.4283">29.4283N</geoCoordinate>
, 
<geoCoordinate id="AE33FAF5772324921EF68DED6A404EAA" degrees="95.0498" direction="east" orientation="longitude" precision="5" value="95.0498">95.0498E</geoCoordinate>
