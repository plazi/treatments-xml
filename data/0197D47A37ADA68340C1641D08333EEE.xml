<taxonomicName id="8C6140E709D0C2154BBE36EDA8FBD9FC" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:142415" class="Insecta" family="Formicidae" genus="Myrmica" kingdom="Animalia" lsidName="Myrmica suspiciosa Smith" lsidName-HNS="Myrmica suspiciosa Smith" order="Hymenoptera" pageNumber="148" phylum="Arthropoda" rank="species" species="suspiciosa">Myrmica suspiciosa</taxonomicName>
