<taxonomicName id="AAD88588B75DA06F9ADFFA6EFC9A4DE9" authority="Nonaka &amp; Okajima, 1991: 110" authorityName="Nonaka &amp; Okajima" authorityPageNumber="110" authorityYear="1991" box="[151,771,1444,1469]" class="Insecta" family="Thripidae" genus="Dendrothrips" kingdom="Animalia" order="Thysanoptera" pageId="9" pageNumber="240" phylum="Arthropoda" rank="species" species="maltimaculatus">
