<quantity id="4CC5D18EFFDBBA1B82CE4857FEE1B1EF" box="[211,273,372,395]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" pageId="11" pageNumber="194" unit="mm" value="5.0">5 mm</quantity>
