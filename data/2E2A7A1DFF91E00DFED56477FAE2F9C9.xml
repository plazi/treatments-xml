<quantity id="617B66EEFF92E00DFD2C65AEFC8EFB61" box="[753,839,1164,1190]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="66" pageNumber="67" unit="m" value="700.0">700 m</quantity>
, P[arque] N[acional] Braulio Carrillo, Prov[incia] S[an] José, 
<collectingCountry id="DE948B9BFF92E00DFDFD6596FD19FB09" box="[544,720,1204,1230]" name="Costa Rica" pageId="66" pageNumber="67">
<collectionCode id="C09253CEFF92E00DFDFD6596FDB7FB09" box="[544,638,1204,1230]" pageId="66" pageNumber="67">COSTA</collectionCode>
