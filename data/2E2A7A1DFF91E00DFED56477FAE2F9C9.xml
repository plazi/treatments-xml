<number id="104BD92CFF92E00DFED565FEFEAFFB31" box="[264,358,1244,1270]" pageId="66" pageNumber="67" value="541800.0">541800</number>
”, “ 
<collectionCode id="C09253CEFF92E00DFE5A65FEFE1EFB31" box="[391,471,1244,1270]" country="Costa Rica" httpUri="http://grbio.org/cool/931n-s1te" name="National Biodiversity Institute, Costa Rica" pageId="66" pageNumber="67">INBio</collectionCode>
