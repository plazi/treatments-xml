<taxonomicName id="6183B088FF92E00DFD6C64D6FD3BF9CA" box="[689,754,1524,1549]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="66" pageNumber="67" phylum="Arthropoda" rank="species" species="lanae">
