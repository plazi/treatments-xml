<quantity id="4CF846BDFFC6FFF0047EFF77DD26BBCD" box="[782,882,223,249]" metricMagnitude="-3" metricUnit="m" metricValue="1.89" pageId="3" pageNumber="132" unit="mm" value="1.89">1.89 mm</quantity>
(
<typeStatus id="54BB55FAFFC6FFF004F0FF77DDBDBBCE" box="[896,1001,223,250]" pageId="3" pageNumber="132">holotype</typeStatus>
