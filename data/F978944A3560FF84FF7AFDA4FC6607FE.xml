<emphasis id="43A5F94E3560FF84FF2AFD32FE960696" bold="true" box="[199,352,690,715]" inLineHeading="true" pageId="4" pageNumber="137" reason="1">Distribution.</emphasis>
<taxonomicName id="B6D15EDF3560FF84FE84FD33FD2D0696" authorityName="Chamberlin" authorityYear="1914" box="[361,731,691,715]" class="Chilopoda" family="Scolopendridae" genus="Scolopendra" kingdom="Animalia" order="Scolopendromorpha" pageId="4" pageNumber="137" phylum="Arthropoda" rank="subSpecies" species="angulata" subSpecies="explorans">
<emphasis id="43A5F94E3560FF84FE84FD33FD2D0696" box="[361,731,691,715]" italics="true" pageId="4" pageNumber="137">Scolopendra angulata explorans</emphasis>
</taxonomicName>
