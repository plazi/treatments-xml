<taxonomicName id="2EAC2E03B807AE2BD0A38D02FD0A396C" authority="Martin, 2008" authorityName="Martin" authorityYear="2008" class="Insecta" family="Aleyrodidae" genus="Aleuroctarthrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aleuroctarthrus destructor" order="Hemiptera" pageId="0" pageNumber="73" phylum="Arthropoda" rank="species" species="destructor">
<emphasis id="F36C5ECF40B1379DBEFE5562CF23FE38" italics="true" pageId="0" pageNumber="73">Aleuroctarthrus destructor</emphasis>
</taxonomicName>
(Mackie) (
<taxonomicName id="F23555DB8D0CF84BDA47033DE5DB38E1" genus="Platygastroidea" lsidName="Platygastroidea" order="Hemiptera" pageId="0" pageNumber="73" rank="order">Hemiptera</taxonomicName>
