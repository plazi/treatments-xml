<taxonomicName id="410E54C30A604187644A16B5DB33585F" class="Liliopsida" family="Asparagaceae" genus="Cordyline" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cordyline stricta" order="Asparagales" pageId="0" pageNumber="73" phylum="Tracheophyta" rank="species" species="stricta">
<emphasis id="0F788C44B4977A327681F03282A820D4" italics="true" pageId="0" pageNumber="73">Cordyline stricta</emphasis>
