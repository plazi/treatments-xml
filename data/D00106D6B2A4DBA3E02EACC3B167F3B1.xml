<taxonomicName id="FCF35D28529A784B102AA2EDC84CA7CD" authorityName="Pujade-Villar &amp; Restrepo-Ortiz" authorityYear="2011" class="Hexapoda" family="Figitidae" genus="Mikeius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="4" pageNumber="25" phylum="Arthropoda" rank="species" species="clavatus">
