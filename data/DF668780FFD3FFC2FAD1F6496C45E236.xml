<collectingRegion id="950BF874FFD3FFC3F7FAF4DB6671EB5B" box="[2158,2361,2848,2873]" country="Brazil" name="Rio de Janeiro" pageId="6" pageNumber="311">Rio de Janeiro</collectingRegion>
