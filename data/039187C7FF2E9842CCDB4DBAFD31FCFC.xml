<subSubSection id="C322655AFF2E9842CDB34E6DFD1EFE88" lastPageId="5" lastPageNumber="1604" pageId="2" pageNumber="1601" type="reference_group">
<paragraph id="8B8736D1FF2E9845CDB34E6DFD73F9E2" blockId="2.[160,675,1599,1624]" box="[160,675,1599,1624]" pageId="2" pageNumber="1601">
<treatmentCitationGroup id="AB2811FFFF2E9845CDB34E6DFD73F9E2" box="[160,675,1599,1624]" pageId="2" pageNumber="1601">
<treatmentCitation id="0A9910C0FF2E9845CDB34E6DFD73F9E2" author="Priesner H" box="[160,675,1599,1624]" page="313" pageId="2" pageNumber="1601" year="1935">
