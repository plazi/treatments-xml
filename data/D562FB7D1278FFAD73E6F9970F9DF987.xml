<taxonomicName id="9ACB31E8127BFFAD7209FC390C43FC21" authority="Platnick &amp; Dupérré, 2011" authorityName="Platnick &amp; Dupérré" authorityYear="2011" box="[328,535,974,994]" class="Arachnida" family="Oonopidae" genus="Brignolia" kingdom="Animalia" order="Araneae" pageId="106" pageNumber="107" phylum="Arthropoda" rank="species" species="chumphae" status="new species">
