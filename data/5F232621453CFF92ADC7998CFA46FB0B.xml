<taxonomicName id="108AECB4453FFF92A91D9E09FA41FE9B" authority="Shi, Ren &amp; Merkl, 2007" authorityName="Shi, Ren &amp; Merkl" authorityYear="2007" box="[1101,1436,298,319]" class="Insecta" family="Tenebrionidae" genus="Gnaptorina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="464" phylum="Arthropoda" rank="species" species="pilifera">
