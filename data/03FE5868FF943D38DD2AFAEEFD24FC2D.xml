<materialsCitation id="3B3FE323FF973D39DFC3FE1FFA94FE66" ID-GBIF-Occurrence="3700114347" box="[189,1399,412,439]" collectionCode="MB" collectorName="Neben &amp; Krueger" country="Germany" county="Oderberg" location="Oderberg" municipality="Ordovician" pageId="98" pageNumber="99" specimenCode="MB.C.30561" specimenCount="1" stateProvince="Brandenburg">
