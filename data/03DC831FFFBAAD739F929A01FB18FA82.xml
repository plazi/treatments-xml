</taxonomicName>
, 
<taxonomicName id="4C75498AFFB5AD739CB29D09FBE4FDAE" box="[951,1081,512,535]" class="Diplopoda" family="Chelodesmidae" genus="Rhacophorus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="rhodopus">
<emphasis id="B901EE1BFFB5AD739CB29D09FBE4FDAE" box="[951,1081,512,535]" italics="true" pageId="8" pageNumber="9">R. rhodopus</emphasis>
