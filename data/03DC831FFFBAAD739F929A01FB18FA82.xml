<taxonomicName id="4C75498AFFB5AD739D9D9D09FC74FDAE" box="[664,937,512,535]" class="Diplopoda" family="Chelodesmidae" genus="Rhacophorus" kingdom="Animalia" order="Polydesmida" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="bipunctatus">
