<taxonomicName id="3B64A5E71087BFD463A10264AB4CFD76" class="Insecta" family="Histeridae" genus="Operclipygus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Operclipygus marginipennis" order="Coleoptera" pageId="276" pageNumber="277" phylum="Arthropoda" rank="species" species="marginipennis">Operclipygus marginipennis</taxonomicName>
