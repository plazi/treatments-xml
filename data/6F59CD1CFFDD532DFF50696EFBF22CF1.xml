<paragraph id="E74F7C0AFFDC532DFF006FAAFC772CB8" blockId="11.[151,1436,150,249]" box="[199,962,150,177]" pageId="11" pageNumber="400">
<emphasis id="D584A018FFDC532DFF006FAAFE1F2CB8" bold="true" box="[199,426,150,176]" pageId="11" pageNumber="400">Other distribution.</emphasis>
<collectingCountry id="9FE73C9AFFDC532DFE766FAAFE582CB8" box="[433,493,150,176]" name="India" pageId="11" pageNumber="400">India</collectingCountry>
, 
<collectingCountry id="9FE73C9AFFDC532DFE3F6FAAFDEB2CB8" box="[504,606,150,176]" name="Thailand" pageId="11" pageNumber="400">Thailand</collectingCountry>
, 
