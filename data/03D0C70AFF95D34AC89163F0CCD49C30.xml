<paragraph id="8BC6761CFF95D34BC89163F0C9469D8D" blockId="44.[151,369,1484,1509]" box="[151,369,1484,1509]" pageId="44" pageNumber="45">
<heading id="D08EC170FF95D34BC89163F0C9469D8D" bold="true" box="[151,369,1484,1509]" fontSize="10" level="2" pageId="44" pageNumber="45" reason="2">
