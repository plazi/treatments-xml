<taxonomicName id="4C23A283711F43382B4BFD0F331EFD40" box="[1280,1376,647,668]" class="Insecta" family="Mycteridae" genus="Thisiomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="305" phylum="Arthropoda" rank="species" species="festivus" status="n. sp.">
