<taxonomicName id="4C3186FDBB57F572E92AFB12BF5092F7" authority="Illiger (Khaustov &amp; Trach 2014)" authorityName="Illiger (Khaustov &amp; Trach" authorityYear="2014" box="[284,854,1241,1266]" class="Insecta" family="Trogidae" genus="Trox" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="432" phylum="Arthropoda" rank="species" species="cadaverinus">
