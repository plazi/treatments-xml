<taxonomicName id="D9DD4D77FFAFFFACFC499F9CFEC2FE70" box="[915,978,404,429]" class="Insecta" family="Geometridae" genus="Talca" kingdom="Animalia" order="Lepidoptera" pageId="63" pageNumber="49" phylum="Arthropoda" rank="genus">Talca</taxonomicName>
, 
<collectingCountry id="66CA7664FFAFFFACFC049F9CF924FE71" box="[990,1076,404,428]" name="Chile" pageId="63" pageNumber="64">CHILE</collectingCountry>
