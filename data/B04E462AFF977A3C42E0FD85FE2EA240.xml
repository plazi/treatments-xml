<typeStatus id="E75C499EFF977A3B42E0FDBFFF02A502" box="[160,214,567,591]" pageId="2" pageNumber="2712">Type</typeStatus>
species: 
<taxonomicName id="FFE78CBFFF977A3B437DFDBFFCF4A502" authority="Ament and Brown. Refs" authorityName="Ament and Brown. Refs" box="[317,800,567,591]" class="Insecta" family="Phoridae" genus="Caledophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="2712" phylum="Arthropoda" rank="species" species="irwini">
