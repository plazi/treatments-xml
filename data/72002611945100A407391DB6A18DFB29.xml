<taxonomicName id="AD2568EEE7CDDDE1AB69F047D3A8D577" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="509" phylum="Tracheophyta" rank="species" species="dubium">
<italic id="31974137A9BA9D414274F90D7E3B26CE" pageId="null" pageNumber="509">T. dubium</italic>
