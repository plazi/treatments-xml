<taxonomicName id="16FEC82E284BEB86702482B75B67EF21" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="509" phylum="Tracheophyta" rank="species" species="campestre">
<italic id="06ACEC9CB36927FC16C5816B79505ECC" pageId="null" pageNumber="509">T. campestre</italic>
</taxonomicName>
(Nr. 3c)
</td>
</tr>
<tr id="4CE334CC1EFC2D0B27DD8B0F060ED240" pageId="null" pageNumber="509">
<td id="9C010012730D1D38A5B1FA8E1D805F97" align="left" pageId="null" pageNumber="509" valign="top">
1*. 
<normalizedToken id="0BF1079477FDE6976B7C98A4AEC23E18" originalValue="Blütenstand" pageId="null" pageNumber="509">Bluetenstand</normalizedToken>
2-15 
<normalizedToken id="AC2AA87A751BE6E69C168DC39B5D7F1B" originalValue="blütig" pageId="null" pageNumber="509">bluetig</normalizedToken>
, 0,6-0,8 cm im Durchmesser; Stengel niederliegend; Krone 2,5 bis 4 mm lang, 4-5mal so lang wie die 
<normalizedToken id="A95E91BC75483F9B9BAB3163CD1C4463" originalValue="Kelchröhre" pageId="null" pageNumber="509">Kelchroehre</normalizedToken>
.
</td>
</tr>
<tr id="FD46FC9E471F3AE14768EDF3159DB1E8" pageId="null" pageNumber="509">
<td id="4236994FE3698EAFB58856E433FCC973" align="left" pageId="null" pageNumber="509" valign="top">
4. 
<normalizedToken id="0F59633CC1C4FF680736FC9FCE2B1699" originalValue="Blütenstiele" pageId="null" pageNumber="509">Bluetenstiele</normalizedToken>
<normalizedToken id="35D2A305E8C3DFC5BD1B806184D891E7" originalValue="kürzer" pageId="null" pageNumber="509">kuerzer</normalizedToken>
