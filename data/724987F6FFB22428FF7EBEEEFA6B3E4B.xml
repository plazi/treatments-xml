<taxonomicName id="3DE04D63FFB32428FD1DBB33FC043C48" authority="Zhang, 2019" authorityName="Zhang" authorityYear="2019" box="[756,995,1354,1376]" class="Gastropoda" family="Tateidae" genus="Conondalia" higherTaxonomySource="CoL" kingdom="Animalia" order="Littorinimorpha" pageId="53" pageNumber="54" phylum="Mollusca" rank="species" species="buzwilsoni" status="n. gen. and n. sp." subClass="Caenogastropoda" superFamily="Truncatelloidea">
