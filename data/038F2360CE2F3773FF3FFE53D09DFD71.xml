<typeStatus id="549D2CD4CE2F3773FF6FFD8ED317FD55" box="[199,258,618,643]" pageId="34" pageNumber="135">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="4C26E9F5CE2F3773FEC1FD8ED091FD55" authority="Just, 2019" authorityName="Just" authorityYear="2019" box="[361,644,618,643]" class="Malacostraca" family="Ischyroceridae" genus="Pararhinoecetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="bicornis" status="sp. nov.">
<emphasis id="B9524E64CE2F3773FEC1FD8ED091FD55" box="[361,644,618,643]" italics="true" pageId="34" pageNumber="135">Pararhinoecetes bicornis</emphasis>
</taxonomicName>
<emphasis id="B9524E64CE2F3773FD23FD8ED0F0FD55" bold="true" box="[651,741,618,643]" pageId="34" pageNumber="135">
