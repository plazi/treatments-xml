<collectingCountry id="F32376071932324C8C4CFB591BE4B1DC" box="[1099,1166,1171,1197]" name="Japan" pageId="8" pageNumber="94">Japan</collectingCountry>
, southwest Pacific, Sulawesi, 
<collectingCountry id="F32376071932324C88EAFB7D1EA0B1A0" box="[237,458,1207,1233]" name="Papua New Guinea" pageId="8" pageNumber="94">Papua New Guinea</collectingCountry>
, 
<collectingCountry id="F32376071932324C89D3FB7D1D51B1A0" box="[468,571,1207,1233]" name="Australia" pageId="8" pageNumber="94">Australia</collectingCountry>
and 
<collectingCountry id="F32376071932324C8A75FB7D1DB1B1A0" box="[626,731,1207,1233]" name="Malaysia" pageId="8" pageNumber="94">Malaysia</collectingCountry>
, 
<collectingCountry id="F32376071932324C8AE0FB7D1C3DB1A0" box="[743,855,1207,1233]" name="Indonesia" pageId="8" pageNumber="94">Indonesia</collectingCountry>
, Marshall, 
<collectingCountry id="F32376071932324C8BD3FB7D1B43B1A0" box="[980,1065,1207,1233]" name="Taiwan" pageId="8" pageNumber="94">Taiwan</collectingCountry>
