<typeStatus id="54C688477903FFA242929BD4D301FC18" box="[963,1059,907,931]" pageId="11" pageNumber="275">holotype</typeStatus>
and 
<typeStatus id="54C688477903FFA245329BD3D3E1FC1F" box="[1123,1219,908,932]" pageId="11" pageNumber="275">paratype</typeStatus>
: AW 59, 57; 
<materialsCitation id="3B153CB87903FFA244129BD3D299FC18" ID-GBIF-Occurrence="3032987301" box="[1347,1467,907,931]" collectionCode="PW" pageId="11" pageNumber="275" specimenCode="PW 65, 60" specimenCount="1">
<specimenCode id="DBDB9E9E7903FFA244129BD3D2B2FC18" box="[1347,1424,907,931]" collectionCode="PW" country="Thailand" name="Paleontological Collections" pageId="11" pageNumber="275">PW 65</specimenCode>
