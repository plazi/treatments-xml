<quantity id="4CAD45F80944C1233E4DFEF0FD322894" box="[566,702,365,389]" metricMagnitude="6" metricUnit="kg" metricValue="4.3" pageId="45" pageNumber="2257" unit="t" value="4300.0">4300 tonnes</quantity>
