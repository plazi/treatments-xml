<taxonomicName id="AFD9A596124AFFC7FBB4FDB7FEFCFD46" authority="(Linnaeus, 1767)" authorityName="Linnaeus" authorityYear="1767" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" class="Gastropoda" family="Truncatellidae" genus="Truncatella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="48" pageNumber="49" phylum="Mollusca" rank="species" species="subcylindrica">
