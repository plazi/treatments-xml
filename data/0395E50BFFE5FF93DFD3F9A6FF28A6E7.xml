<taxonomicName id="4C3C2F9EFFFAFF92DE0BFAC4FE16A76B" box="[352,463,1352,1373]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="112" pageNumber="112" phylum="Tracheophyta" rank="genus">
