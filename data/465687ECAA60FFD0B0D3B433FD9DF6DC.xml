<collectingCounty id="27214E76AA60FFD0B15FB4B3FE69F49C" box="[332,393,1166,1190]" pageId="7" pageNumber="6">Male</collectingCounty>
imago, 
<collectingMunicipality id="2E24AC80AA60FFD0B1F9B4B3FDB5F49C" box="[490,597,1166,1190]" pageId="7" pageNumber="6">Province</collectingMunicipality>
