<taxonomicName id="ABA40EB33833FF8EFE7AFB889E12FBF9" box="[457,639,1113,1137]" class="Insecta" family="Phaneropteridae" genus="Arantia" kingdom="Animalia" order="Orthoptera" pageId="23" pageNumber="324" phylum="Arthropoda" rank="species" species="fasciata">
