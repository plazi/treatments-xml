<quantity id="4C86D581FFA4FFDCFBBC32DCFB66FDCF" box="[1080,1191,489,515]" metricMagnitude="-3" metricUnit="m" metricValue="1.3" pageId="36" pageNumber="37" unit="mm" value="1.3">1.30 mm</quantity>
); LAED = 0.77 ± 
<quantity id="4C86D581FFA4FFDCFF393138FEE4FDE4" box="[189,293,525,552]" metricMagnitude="-5" metricUnit="m" metricValue="2.0" pageId="36" pageNumber="37" unit="mm" value="0.02">0.02 mm</quantity>
(0.74 Ĺ LAED Ĺ 
<quantity id="4C86D581FFA4FFDCFE6B3138FD9AFDE4" box="[495,603,525,552]" metricMagnitude="-4" metricUnit="m" metricValue="7.9" pageId="36" pageNumber="37" unit="mm" value="0.79">0.79 mm</quantity>
); LB = 2.05 ± 
<quantity id="4C86D581FFA4FFDCFC853138FCA9FDE4" box="[769,872,525,552]" metricMagnitude="-4" metricUnit="m" metricValue="1.9" pageId="36" pageNumber="37" unit="mm" value="0.19">0.19 mm</quantity>
