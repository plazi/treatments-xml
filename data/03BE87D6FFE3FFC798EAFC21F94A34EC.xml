<taxonomicName id="4C174D43FFE3FFC798EAFC21F8C733E8" authority="(Pallas, 1810)" baseAuthorityName="Pallas" baseAuthorityYear="1810" box="[151,646,978,1005]" class="Actinopterygii" family="Hexagrammidae" genus="Hexagrammos" kingdom="Animalia" order="Scorpaeniformes" pageId="114" pageNumber="115" phylum="Chordata" rank="species" species="lagocephalus">
<emphasis id="B963EAD2FFE3FFC798EAFC21FBA133E8" bold="true" box="[151,480,978,1005]" italics="true" pageId="114" pageNumber="115">Hexagrammos lagocephalus</emphasis>
