<taxonomicName id="27CA4D35FEDEFEB85CD4FDE6FEEF63D0" authority="Walker, 1859" authorityName="Walker" authorityYear="1859" class="Insecta" family="Bombyliidae" genus="Volucella" kingdom="Animalia" order="Diptera" pageId="275" pageNumber="276" phylum="Arthropoda" rank="species" species="decorata">
