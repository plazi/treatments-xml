<taxonomicName id="4C594D632A03FF9DB136F97AFC9E269E" authority="(Ohlin, 1901)" baseAuthorityName="Ohlin" baseAuthorityYear="1901" box="[601,871,1719,1746]" class="Malacostraca" family="Mysidae" genus="Michthyops" kingdom="Animalia" order="Mysida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="theeli">
