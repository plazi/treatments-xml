<taxonomicName id="4C594D632A03FF9DB2EBF919FDE126B9" authority="Wittmann, 2020" authorityName="Wittmann" authorityYear="2020" box="[388,536,1755,1781]" class="Malacostraca" family="Mysidae" genus="Abyssomysis" kingdom="Animalia" order="Mysida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis id="B92DEAF22A03FF9DB2EBF919FDE126B9" box="[388,536,1755,1781]" italics="true" pageId="18" pageNumber="19">Abyssomysis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A21E57892A03FF9DB170F919FD7326B9" box="[543,650,1755,1781]" pageId="18" pageNumber="19" rank="genus">gen. nov.</taxonomicNameLabel>
with its only species 
<taxonomicName id="4C594D632A03FF9DB0E8F919FBFA26B9" authority="Wittmann, 2020" authorityName="Wittmann" authorityYear="2020" box="[903,1027,1755,1781]" class="Malacostraca" family="Mysidae" genus="Abyssomysis" kingdom="Animalia" order="Mysida" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="cornuta" status="gen. et sp. nov.">
