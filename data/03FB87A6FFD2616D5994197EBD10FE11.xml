<subSubSection id="C348653BFFD1616D59C61AACB9E3FF42" pageId="6" pageNumber="53" type="etymology">
<paragraph id="8BED36B0FFD1616D59C61AACB9E3FF42" blockId="6.[151,1436,151,392]" pageId="6" pageNumber="53">
<emphasis id="B926EAA2FFD1616D59C61AACB92CFF39" box="[197,331,152,176]" inLineHeading="true" pageId="6" pageNumber="53" reason="1">
<emphasis id="B926EAA2FFD1616D59C61AACB923FF26" bold="true" box="[197,324,152,176]" pageId="6" pageNumber="53">Etymology</emphasis>
.
</emphasis>
