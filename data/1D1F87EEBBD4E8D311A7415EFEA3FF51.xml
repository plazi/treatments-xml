<subSubSection id="DDAC6573BBD4E8D211A741EFFCFFF86C" box="[151,892,1964,1989]" pageId="252" pageNumber="253" type="description">
<paragraph id="950936F8BBD4E8D211A741EFFCFFF86C" blockId="252.[151,1436,1964,2024]" box="[151,892,1964,1989]" pageId="252" pageNumber="253">
<collectingCountry id="EDA17668BBD4E8D211A741EFFE85F86C" box="[151,262,1964,1989]" name="Argentina" pageId="252" pageNumber="253">Argentina</collectingCountry>
