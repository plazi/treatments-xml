<paragraph pageId="31" pageNumber="32">
<taxonomicName class="Insecta" family="Formicidae" genus="Formica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Formica lusatica" order="Hymenoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="lusatica">Formica lusatica</taxonomicName>
