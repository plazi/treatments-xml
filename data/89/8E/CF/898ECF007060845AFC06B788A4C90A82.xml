<paragraph pageId="0" pageNumber="45">
<taxonomicName class="Magnoliopsida" family="Rubiaceae" genus="Remijia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Remijia involucrata" order="Gentianales" pageId="0" pageNumber="45" phylum="Tracheophyta" rank="species" species="involucrata">Remijia involucrata</taxonomicName>
