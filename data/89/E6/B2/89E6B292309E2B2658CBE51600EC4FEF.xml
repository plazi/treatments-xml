<taxonomicName class="Magnoliopsida" family="Bignoniaceae" genus="Tecoma" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Tecoma stans" order="Lamiales" pageId="0" pageNumber="83" phylum="Tracheophyta" rank="species" species="stans">Tecoma stans</taxonomicName>
