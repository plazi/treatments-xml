</emphasis>
<taxonomicName authority="Broun, 1915: 282" authorityName="Broun" authorityPageNumber="282" authorityYear="1915" box="[313,720,654,679]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="552" phylum="Arthropoda" rank="species" species="setiventris">
