(OMNH-Ar-10597) and 
<typeStatus box="[1040,1139,691,715]" pageId="9" pageNumber="469">paratype</typeStatus>
, male, 
<quantity box="[1223,1324,691,715]" metricMagnitude="-2" metricUnit="m" metricValue="1.1199999999999999" pageId="9" pageNumber="469" unit="mm" value="11.2">11.2 mm</quantity>
