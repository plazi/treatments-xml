<emphasis italics="true" pageId="0" pageNumber="115">Phyllanthus reticulatus</emphasis>
</taxonomicName>
Poir. and 
<taxonomicName authorityName="Corner" authorityYear="1962" baseAuthorityName="Bur." class="Dicotyledoneae" family="Moraceae" genus="Streblus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Streblus indicus" order="Urticales" pageId="0" pageNumber="115" phylum="Angiospermae" rank="species" species="indicus">
