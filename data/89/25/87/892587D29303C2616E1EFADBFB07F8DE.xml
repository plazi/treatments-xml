, Loreto, Boqueron Abad 
<date box="[1181,1318,1473,1499]" pageId="4" pageNumber="5" value="1962-09-26">
<number box="[1181,1213,1473,1499]" pageId="4" pageNumber="5" value="26.0">26</number>
.IX. 
<number box="[1255,1318,1473,1499]" pageId="4" pageNumber="5" value="1962.0">1962</number>
</date>
, M. J. Schunke leg. (
<collectionCode box="[517,612,1513,1539]" pageId="4" pageNumber="5">BMNH</collectionCode>
). 
<typeStatus box="[636,738,1513,1539]" pageId="4" pageNumber="5">Paratype</typeStatus>
male: 
<collectingCountry box="[828,885,1513,1539]" name="Peru" pageId="4" pageNumber="5">Peru</collectingCountry>
, Dept. of Loreto, Iquitos area, Allpahuayo, 
<date box="[378,607,1553,1579]" pageId="4" pageNumber="5" value="2000-07-16" valueMax="2000-08-02" valueMin="2000-07-16">
<number box="[378,406,1553,1579]" pageId="4" pageNumber="5" value="16.0">16</number>
.VII­ 
<number box="[464,479,1553,1579]" pageId="4" pageNumber="5" value="2.0">2</number>
.VIII. 
<number box="[542,607,1553,1579]" pageId="4" pageNumber="5" value="2000.0">2000</number>
