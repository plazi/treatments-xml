<taxonomicName class="Magnoliopsida" family="Primulaceae" genus="Soldanella" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Soldanella carpatica" order="Ericales" pageId="5" pageNumber="136" phylum="Tracheophyta" rank="species" species="carpatica">Soldanella carpatica</taxonomicName>
Vierh. (
<taxonomicName family="Primulaceae" lsidName="" pageId="5" pageNumber="136" rank="family">Primulaceae</taxonomicName>
), 
<taxonomicName class="Aves" family="Scolopacidae" genus="Arenaria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Arenaria tenella" order="Charadriiformes" pageId="5" pageNumber="136" phylum="Chordata" rank="species" species="tenella">Arenaria tenella</taxonomicName>
Kit., 
<taxonomicName class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dianthus nitidus" order="Caryophyllales" pageId="5" pageNumber="136" phylum="Tracheophyta" rank="species" species="nitidus">Dianthus nitidus</taxonomicName>
