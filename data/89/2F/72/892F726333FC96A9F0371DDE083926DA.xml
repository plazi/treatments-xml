<taxonomicName family="Caryophyllaceae" lsidName="" pageId="5" pageNumber="136" rank="family">Caryophyllaceae</taxonomicName>
), 
<taxonomicName class="Magnoliopsida" family="Orobanchaceae" genus="Bartsia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Bartsia alpina" order="Lamiales" pageId="5" pageNumber="136" phylum="Tracheophyta" rank="species" species="alpina">Bartsia alpina</taxonomicName>
L., 
<taxonomicName class="Magnoliopsida" family="Orobanchaceae" genus="Pedicularis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pedicularis oederi" order="Lamiales" pageId="5" pageNumber="136" phylum="Tracheophyta" rank="species" species="oederi">Pedicularis oederi</taxonomicName>
