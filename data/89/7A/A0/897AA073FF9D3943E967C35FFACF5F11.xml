<subSubSection lastPageId="53" lastPageNumber="54" pageId="52" pageNumber="53" type="description">
<paragraph blockId="52.[227,565,302,328]" box="[227,565,302,328]" pageId="52" pageNumber="53">
<heading bold="true" box="[227,565,302,328]" fontSize="11" level="3" pageId="52" pageNumber="53" reason="3">
<emphasis bold="true" box="[227,565,302,328]" pageId="52" pageNumber="53">
Dimensions of the 
<typeStatus box="[457,565,302,328]" pageId="52" pageNumber="53">holotype</typeStatus>
</emphasis>
</heading>
</paragraph>
<paragraph blockId="52.[227,938,349,376]" box="[227,938,349,376]" pageId="52" pageNumber="53">
Length = 
<quantity box="[341,449,349,376]" metricMagnitude="-3" metricUnit="m" metricValue="2.75" pageId="52" pageNumber="53" unit="mm" value="2.75">2.75 mm</quantity>
; height = 
