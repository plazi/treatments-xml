<subSubSection pageId="38" pageNumber="39" type="description">
<paragraph blockId="38.[227,565,1178,1204]" box="[227,565,1178,1204]" pageId="38" pageNumber="39">
<heading bold="true" box="[227,565,1178,1204]" fontSize="11" level="3" pageId="38" pageNumber="39" reason="3">
