<paragraph blockId="53.[227,341,1808,1834]" box="[227,341,1808,1834]" pageId="53" pageNumber="54">
<heading bold="true" box="[227,341,1808,1834]" fontSize="11" level="3" pageId="53" pageNumber="54" reason="3">
