<collectionCode box="[240,303,1823,1847]" httpUri="http://grbio.org/cool/r82x-h8re" name="Atlantic Reference Centre" pageId="5" pageNumber="92">ARC</collectionCode>
), 
<collectingCountry box="[323,394,1823,1847]" name="Egypt" pageId="5" pageNumber="92">Egypt</collectingCountry>
