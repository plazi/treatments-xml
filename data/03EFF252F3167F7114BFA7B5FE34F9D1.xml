<taxonomicName id="4C4638C7F3167F711788A47EFACEF999" authority="Basilewsky, 1946" authorityName="Basilewsky" authorityYear="1946" box="[928,1341,1549,1574]" class="Insecta" family="Carabidae" genus="Heterohyparpalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="219" phylum="Arthropoda" rank="genus">
