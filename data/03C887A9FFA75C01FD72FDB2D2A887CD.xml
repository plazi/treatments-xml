<taxonomicName id="4C614D3CFFA75C02FEABF962D0C283B5" authorityName="Schurhoff" authorityYear="1935" box="[315,503,1686,1704]" class="Insecta" family="Cetoniidae" genus="Macrocranius" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="372" phylum="Arthropoda" rank="species" species="similis">
