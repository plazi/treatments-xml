<subSubSection id="3D86C1C3963FA94059AB16A9DC010A36" pageId="null" pageNumber="178" type="nomenclature">
<paragraph id="E07581590844E656E96FBBA8F25ACC2A" pageId="null" pageNumber="178">
<taxonomicName id="BDEDA8A435C69CB92344171F99123A95" class="Pinopsida" family="Cupressaceae" genus="Juniperus" kingdom="Plantae" order="Pinales" pageId="null" pageNumber="178" phylum="Tracheophyta" rank="genus">Juniperus</taxonomicName>
