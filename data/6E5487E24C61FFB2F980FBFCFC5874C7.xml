<number id="503524D34C61FFB1F9C1FB91FF187147" box="[214,229,1112,1138]" pageId="4" pageNumber="5" value="2.0">2</number>
</figureCitation>
A, 
<number id="503524D34C61FFB1F81EFB91FEE57147" box="[265,280,1112,1138]" pageId="4" pageNumber="5" value="2.0">2</number>
B)
