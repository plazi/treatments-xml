<paragraph id="8BC336F1FFC8FFBCFF6D23E695DEFCCF" blockId="26.[140,1107,659,1038]" box="[185,1000,764,792]" pageId="26" pageNumber="27">
<emphasis id="B908EAE3FFC8FFBCFF6D23E696C6FCC1" bold="true" box="[185,240,764,790]" pageId="26" pageNumber="27">
<emphasis id="B908EAE3FFC8FFBCFF6D23E696DDFCC1" box="[185,235,764,790]" inLineHeading="true" pageId="26" pageNumber="27" reason="1">Size</emphasis>
