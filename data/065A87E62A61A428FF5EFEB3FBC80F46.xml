<subSubSection id="C6E9657B2A61A428FF5EFEE9FE8609C8" pageId="18" pageNumber="19" type="materials_examined">
<paragraph id="8E4C36F02A61A428FF5EFEE9FBD00B82" blockId="18.[151,1436,372,428]" pageId="18" pageNumber="19">
<emphasis id="BC87EAE22A61A428FF5EFEE9FF590BA2" bold="true" box="[151,207,372,396]" pageId="18" pageNumber="19">Type</emphasis>
