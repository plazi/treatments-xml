<location id="8B2C602B2A61A428FE69FEE8FDB00BA2" LSID="urn:lsid:plazi:treatment:065A87E62A61A428FF5EFEB3FBC80F46:8B2C602B2A61A428FE69FEE8FDB00BA2" box="[416,550,373,396]" country="Macedonia" municipality="Bay of Stenje" name="Lake Prespa" pageId="18" pageNumber="19">Lake Prespa</location>
, 
<collectingMunicipality id="6E28AC8A2A61A428FDF9FEE8FD540BA2" box="[560,706,373,396]" pageId="18" pageNumber="19">Bay of Stenje</collectingMunicipality>
