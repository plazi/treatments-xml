<materialsCitation id="2EB21517FFB3F512FF39F9CDFDEC4429" collectorName="Nadig &amp; There" country="Italy" county="Swiss" elevation="60600" lastPageId="5" lastPageNumber="406" location="Although" municipality="Meride" pageId="4" pageNumber="405" specimenCount="4" specimenCount-female="1" typeStatus="holotype">
Ƌ 
<typeStatus id="4161A1E8FFB3F513FF1EF9CCFEFC4369" box="[243,342,1539,1563]" pageId="4" pageNumber="405">Holotype</typeStatus>
with labels: “Meride (TI.) “Camponaria”, 16.VII. 
<quantity id="5922B2AFFFB3F513FC96F9CCFC414369" box="[891,1003,1539,1563]" metricMagnitude="4" metricUnit="m" metricValue="6.06" pageId="4" pageNumber="405" unit="m" value="60600.0">
