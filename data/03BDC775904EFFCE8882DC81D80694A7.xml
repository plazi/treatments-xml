<emphasis id="B960AA71904EFFCD8A22DFD5D93090CE" box="[567,594,1921,1944]" italics="true" pageId="5" pageNumber="6">av</emphasis>
<number id="3DDC6444904EFFCD8A4DDFD4D90590CE" box="[600,615,1920,1944]" pageId="5" pageNumber="6" value="1.0">1</number>
; mid coxa with 
<emphasis id="B960AA71904EFFCD8B0EDFD5D84B90CE" box="[795,809,1921,1944]" italics="true" pageId="5" pageNumber="6">a</emphasis>
<number id="3DDC6444904EFFCD8B25DFD4D82390CE" box="[816,833,1920,1944]" pageId="5" pageNumber="6" value="2.0">2</number>
