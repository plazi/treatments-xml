<collectingCountry id="1382763BC627FFFAF66166DC734F735A" box="[524,597,1017,1043]" name="Brazil" pageId="28" pageNumber="29">Brazil</collectingCountry>
: 
<collectingRegion id="A951F849C627FFFAF63266DC73BA735A" box="[607,672,1017,1043]" country="Brazil" name="Bahia" pageId="28" pageNumber="29">Bahia</collectingRegion>
