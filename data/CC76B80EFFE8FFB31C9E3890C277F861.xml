<taxonomicName id="83DF729BFFE8FFB31C9E3890C2B9FAF9" authority="Laporte, 1833" authorityName="Laporte" authorityYear="1833" box="[151,467,225,252]" class="Insecta" family="Scutelleridae" genus="Eurygaster" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="503" phylum="Arthropoda" rank="genus">
