<taxonomicName id="12343919117F874B55389EB84EB46E75" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:226218" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Uploaded - Camponotus (Myrmoturba) maculatus subsp. radamoides var. cataractae" lsidName-HNS="Uploaded - Camponotus (Myrmoturba) maculatus subsp. radamoides var. cataractae" order="Hymenoptera" phylum="Arthropoda" rank="variety" species="maculatus" subGenus="Myrmoturba" subSpecies="radamoides" variety="cataractae">Camponotus (Myrmoturba) maculatus For. M. radamoides For. var. cataractae</taxonomicName>
