<collectingCountry id="F336763CFF8EFFD3FCCA2182FCF53087" box="[820,895,1949,1973]" name="Belize" pageId="1" pageNumber="32">Belize</collectingCountry>
; 
<taxonomicName id="4C214D2FFF8EFFD3FC742182FC783087" box="[906,1010,1949,1973]" class="Insecta" family="Psephenidae" genus="Belicinus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="38" phylum="Arthropoda" rank="genus" status="gen. nov.">
