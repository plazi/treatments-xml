<location id="4768FCD4366E2261FE54FEF2859DFED6" LSID="urn:lsid:plazi:treatment:CA1E1B1936712261FE62FA038655FD89:4768FCD4366E2261FE54FEF2859DFED6" box="[393,502,296,320]" country="Kenya" municipality="The" name="Recorded" pageId="16" pageNumber="1558" stateProvince="Taita/Taveta">Recorded</location>
