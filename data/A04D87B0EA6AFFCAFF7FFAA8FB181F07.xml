<taxonomicName id="EFE44D25EA6BFFC9FEB9F8E5FD761A0A" box="[337,603,1892,1916]" class="Insecta" family="Thripidae" genus="Asprothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="497" phylum="Arthropoda" rank="species" species="bimaculatus" status="sp. n.">
