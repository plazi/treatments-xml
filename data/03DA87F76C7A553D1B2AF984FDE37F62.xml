<number id="3DBB24C66C7A5522194BF8F4FC217952" box="[873,937,1876,1902]" pageId="15" pageNumber="16" value="2000.0">2000</number>
</bibRefCitation>
; 
<bibRefCitation id="EFE24B106C7A55221996F8F4FC7B7952" author="Bamber" box="[948,1011,1876,1902]" pageId="15" pageNumber="26" refString="Bamber, R. N. (2004) Pycnogonids (Arthropoda: Pycnogonida) from New Caledonia and Fiji: new records and new species. In: Marshall, B. &amp; Richer de Forges, B. (eds), Tropical Deep Sea Benthos, Volume 23. Memoires du Museum National d'Histoire Naturelle, Paris, 191, 73 - 83." type="journal article" year="2004">
<number id="3DBB24C66C7A55221996F8F4FC7B7952" box="[948,1011,1876,1902]" pageId="15" pageNumber="16" value="2004.0">2004</number>
</bibRefCitation>
), from 
<quantity id="4C8B9B046C7A55221E6AF8F4FB7C7952" box="[1096,1268,1876,1902]" metricMagnitude="3" metricUnit="m" metricValue="1.48" metricValueMax="2.11" metricValueMin="0.85" pageId="15" pageNumber="16" unit="m" value="1480.0" valueMax="2110.0" valueMin="850.0">850 to 2110 m</quantity>
