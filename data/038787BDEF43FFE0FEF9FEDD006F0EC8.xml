<taxonomicName id="4C2E4D28EF43FFE0FB73FE4E05700EC8" authority="Austen" authorityName="Austen" class="Insecta" family="Bombyliidae" genus="Spogostylum" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="arenivagum">
