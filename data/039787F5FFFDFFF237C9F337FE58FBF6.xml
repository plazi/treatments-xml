(1,08-1,30;n=11) lange, 
<quantity id="4CC69B06FFFDFFF237C9F0F8FEA6FD33" box="[178,268,680,702]" metricMagnitude="-4" metricUnit="m" metricValue="3.5999999999999996" pageId="19" pageNumber="606" unit="mm" value="0.36">0,36 mm</quantity>
