<taxonomicName id="4C6F4D52FFF7E466FE7A98DA2120FA52" box="[425,627,1455,1481]" class="Insecta" family="Aleyrodidae" genus="Acanthaleyrodes" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="35" phylum="Arthropoda" rank="genus">
