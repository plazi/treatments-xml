, and 
<taxonomicName id="4C0F18A6323CFF8BFF25771AFEE9507C" box="[239,312,1360,1386]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="genus">
