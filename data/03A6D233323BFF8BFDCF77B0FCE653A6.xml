<collectionCode id="ED1EFBE0323CFF8BFB53768EFB7F51C8" box="[1177,1198,1220,1246]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13092" name="Nanjing University" pageId="5" pageNumber="6" type="Herbarium">N</collectionCode>
</geoCoordinate>
- 
<geoCoordinate id="EE3B05E2323CFF8BFB167689FA9751C8" box="[1244,1350,1219,1246]" degrees="2" direction="west" minutes="39" orientation="longitude" pageId="5" pageNumber="6" precision="925" value="-2.65">
2°39’ 
<collectionCode id="ED1EFBE0323CFF8BFAED768EFA9751C8" box="[1319,1350,1220,1246]" country="Austria" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="5" pageNumber="6" type="Herbarium">W</collectionCode>
</geoCoordinate>
), in association with pine forest; and the province of Huesca (northern Iberian Peninsula), road from Somport to Huesca (
<geoCoordinate id="EE3B05E2323CFF8BFE8C7740FE655032" box="[326,436,1290,1316]" degrees="42" direction="north" minutes="34" orientation="latitude" pageId="5" pageNumber="6" precision="925" value="42.566666">
42°34’ 
<collectionCode id="ED1EFBE0323CFF8BFE557740FE655032" box="[415,436,1290,1316]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13092" name="Nanjing University" pageId="5" pageNumber="6" type="Herbarium">N</collectionCode>
</geoCoordinate>
- 
<geoCoordinate id="EE3B05E2323CFF8BFE187740FDB95032" box="[466,616,1290,1317]" degrees="0" direction="west" minutes="32" orientation="longitude" pageId="5" pageNumber="6" precision="15" seconds="58" value="-0.5494445">
0°32’58’’ 
<collectionCode id="ED1EFBE0323CFF8BFD837740FDB95032" box="[585,616,1290,1316]" country="Austria" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15588" name="Naturhistorisches Museum Wien" pageId="5" pageNumber="6" type="Herbarium">W</collectionCode>
