<taxonomicName id="4C0F18A6323CFF8BFF777764FDB3505E" authority="Linnaeus, 1753" authorityName="Linnaeus" authorityYear="1753" box="[189,610,1325,1352]" class="Magnoliopsida" family="Buxaceae" genus="Buxus" kingdom="Plantae" order="Buxales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="sempervirens">
<emphasis id="B97BBF37323CFF8BFF777764FE775051" box="[189,422,1326,1351]" italics="true" pageId="5" pageNumber="6">Buxus sempervirens</emphasis>
Linnaeus, 1753
</taxonomicName>
, 
<taxonomicName id="4C0F18A6323CFF8BFDA77764FBCB505E" authority="Linnaeus, 1753" authorityName="Linnaeus" authorityYear="1753" box="[621,1050,1325,1352]" class="Pinopsida" family="Cupressaceae" genus="Juniperus" kingdom="Plantae" order="Pinales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="oxycedrus">
<emphasis id="B97BBF37323CFF8BFDA77764FC8F5051" box="[621,862,1325,1351]" italics="true" pageId="5" pageNumber="6">Juniperus oxycedrus</emphasis>
Linnaeus, 1753
</taxonomicName>
, 
<taxonomicName id="4C0F18A6323CFF8BFBEF7764FAA0505E" authority="Linnaeus, 1753" authorityName="Linnaeus" authorityYear="1753" box="[1061,1393,1325,1352]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="species" species="canina">
