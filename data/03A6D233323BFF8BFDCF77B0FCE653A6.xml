<geoCoordinate id="EE3B05E2323CFF8BFBF57689FB7F51C8" box="[1087,1198,1219,1246]" degrees="38" direction="north" minutes="17" orientation="latitude" pageId="5" pageNumber="6" precision="925" value="38.283333">
