<subSubSection id="C5BE19ECE478FFA6FFC49292CCEDF937" box="[199,1430,1744,1769]" pageId="20" pageNumber="471" type="etymology">
<paragraph id="8D1B4A67E478FFA6FFC49292CCEDF937" blockId="20.[151,1436,1744,2021]" box="[199,1430,1744,1769]" pageId="20" pageNumber="471">
<emphasis id="BFD09675E478FFA6FFC49292C835F937" bold="true" box="[199,334,1744,1769]" pageId="20" pageNumber="471">Etymology.</emphasis>
