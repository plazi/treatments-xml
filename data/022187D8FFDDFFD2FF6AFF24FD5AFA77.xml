<taxonomicName id="4D884D4DFFDDFFD0FE86FDBEFDE1FD8E" box="[379,536,514,537]" class="Arachnida" family="Araneidae" genus="Micrathena" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="65" phylum="Arthropoda" rank="species" species="cyanospina">
