<taxonomicName id="4D884D4DFFDEFFD3FECAF8CEFDE5F810" box="[311,540,1906,1927]" class="Arachnida" family="Araneidae" genus="Micrathena" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="66" phylum="Arthropoda" rank="species" species="cyanospina">Micrathena cyanospina</taxonomicName>
