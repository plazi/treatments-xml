</geoCoordinate>
<geoCoordinate id="EFBC5009FFDFFFD2FBAFFB45FB4CFA87" box="[1106,1205,1273,1296]" direction="west" orientation="longitude" pageId="3" pageNumber="67" precision="925" value="-59.733334">
