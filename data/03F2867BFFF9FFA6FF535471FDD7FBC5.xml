<taxonomicName id="4C5B4CEEFFF9FFA9FC7C5062FB59F8B2" authority="(LANE)" authorityName="Mohrig &amp; Menzel" authorityYear="2014" baseAuthorityName="LANE" baseAuthorityYear="1960" box="[914,1153,1888,1910]" class="Insecta" family="Sciaridae" genus="Cratyna" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="142" phylum="Arthropoda" rank="species" species="cucarisi">
