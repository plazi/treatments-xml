<heading id="D0A581F5FFF3D57BFF3FF8B39CC5DA48" bold="true" box="[151,435,1891,1918]" fontSize="11" level="1" pageId="1" pageNumber="46" reason="1">
<taxonomicName id="4C524D1AFFF3D57BFF3FF8B39CC5DA48" authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" box="[151,435,1891,1918]" class="Insecta" family="Dryinidae" genus="Dryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="46" phylum="Arthropoda" rank="genus">
