<paragraph id="8BBA369E4779AE2AFF02FCBD0AC23FA1" blockId="1.[151,1437,573,948]" pageId="1" pageNumber="43">
<treatmentCitationGroup id="AB1511B04779AE2AFF02FCBD0AC23FA1" pageId="1" pageNumber="43">
<taxonomicName id="4C054D1D4779AE2AFF02FCBD0AC43F61" authority="Wolf 1970: 52" authorityName="Wolf" authorityPageNumber="52" authorityYear="1970" box="[151,492,861,884]" class="Insecta" family="Pompilidae" genus="Carinevagetes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="43" phylum="Arthropoda" rank="genus">
<emphasis id="B971EA8C4779AE2AFF02FCBD0A193F61" box="[151,305,861,884]" italics="true" pageId="1" pageNumber="43">Carinevagetes</emphasis>
<treatmentCitation id="0AA4108F4779AE2AFED5FCBD0AC43F61" author="Wolf" box="[320,492,861,884]" page="52" pageId="1" pageNumber="43" year="1970">
<bibRefCitation id="EF944B6F4779AE2AFED5FCBD0A953F61" author="Wolf" box="[320,445,861,884]" pageId="1" pageNumber="50" refString="Wolf, H. (1970) Zur Kenntnis der Gattung Evagetes Lepeletier, 1845 (Hymenoptera: Pompilidae). Beitrage zur Entomologie, 20 (1 / 2), 51 - 83." type="journal article" year="1970">
Wolf 
<number id="3DCD24B94779AE2AFE11FCBD0A953F61" box="[388,445,861,884]" pageId="1" pageNumber="43" value="1970.0">1970</number>
</bibRefCitation>
: 
<number id="3DCD24B94779AE2AFE44FCBD0AC43F61" box="[465,492,861,884]" pageId="1" pageNumber="43" value="52.0">52</number>
</treatmentCitation>
</taxonomicName>
(as subgenus of 
<taxonomicName id="4C054D1D4779AE2AFD57FCBD08CE3F61" authority="Lepeletier 1845" authorityName="Lepeletier" authorityYear="1845" box="[706,998,861,884]" class="Insecta" family="Pompilidae" genus="Evagetes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="43" phylum="Arthropoda" rank="genus">
