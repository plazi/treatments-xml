<taxonomicName id="65F14D6A406BF37D90EAFDCD11DA3E3E" authority=": Fallen (1810: 6)" authorityName="Fallen" authorityPageNumber="6" authorityYear="1810" box="[151,434,570,595]" class="Insecta" family="Scathophagidae" genus="Cordylura" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="472" phylum="Arthropoda" rank="genus">
<emphasis id="9085EAFB406BF37D90EAFDCD11683E3F" box="[151,256,570,594]" italics="true" pageId="1" pageNumber="472">Cordylura</emphasis>
: 
<bibRefCitation id="C6604B18406BF37D9170FDCD11DA3E3E" author="Fallen, C. F." box="[269,434,570,595]" pageId="1" pageNumber="472" refId="ref6582" refString="Fallen, C. F. (1810) Specimen entomologicum novam Diptera disponendi methodum exhibens. Litteris Berlingianis, Lundae, 26 pp." type="book" year="1810">Fallén (1810: 6)</bibRefCitation>
</taxonomicName>
</treatmentCitation>
, 
<treatmentCitation id="235010F8406BF37D91C0FDCD12E33E3E" author="Meigen, J. W." box="[445,651,570,595]" page="229" pageId="1" pageNumber="472" year="1826">
