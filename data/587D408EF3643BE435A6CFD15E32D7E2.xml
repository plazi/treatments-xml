<taxonomicName id="1617AD3C32FBB34141BEBBB6504EA6DD" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:233580" class="Insecta" family="Formicidae" genus="Ectatomma" kingdom="Animalia" lsidName="Ectatomma (Rhytidoponera) metallicum var. cristulatum Forel" lsidName-HNS="Ectatomma metallicum var. cristulatum" order="Hymenoptera" pageNumber="59" phylum="Arthropoda" rank="subSpecies" species="metallicum" variety="cristulatum">cristulatum</taxonomicName>
