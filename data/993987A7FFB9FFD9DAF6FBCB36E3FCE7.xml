<heading id="4A6781DDFFB9FFDBDAF6FBCB34C3FBAA" bold="true" box="[151,963,1079,1105]" fontSize="11" level="1" pageId="12" pageNumber="41" reason="1">
<emphasis id="23E4EAA3FFB9FFDBDAF6FBCB34C3FBAA" bold="true" box="[151,963,1079,1105]" pageId="12" pageNumber="41">
<taxonomicName id="D6904D32FFB9FFDBDAF6FBCB36A2FBAA" authorityName="Dominguez, Molineri &amp; Mariano" authorityYear="2009" box="[151,418,1079,1105]" class="Insecta" family="Leptophlebiidae" genus="Hagenulopsis" kingdom="Animalia" order="Ephemeroptera" pageId="12" pageNumber="41" phylum="Arthropoda" rank="species" species="zunigae" status="sp. nov.">
<emphasis id="23E4EAA3FFB9FFDBDAF6FBCB36A2FBAA" bold="true" box="[151,418,1079,1105]" italics="true" pageId="12" pageNumber="41">Hagenulopsis zunigae</emphasis>
</taxonomicName>
<taxonomicNameLabel id="38D757D8FFB9FFDBDBCBFBCB3507FBAA" box="[426,519,1079,1105]" pageId="12" pageNumber="41" rank="species">sp. nov.</taxonomicNameLabel>
