<subSubSection id="CCAA6536FFF7296AFF2EA4F5B21BFCB0" pageId="17" pageNumber="348" type="discussion">
<paragraph id="840F36BDFFF7296AFF2EA4F5B21BFCB0" blockId="17.[151,1437,151,790]" pageId="17" pageNumber="348">
<emphasis id="B6C4EAAFFFF7296AFF2EA4F5B005FD68" bold="true" box="[199,308,691,717]" pageId="17" pageNumber="348">Remarks</emphasis>
. 
<bibRefCitation id="E0214B4CFFF7296AFEAAA4F5B0C9FD6B" author="Schubert, K." box="[323,504,691,718]" pageId="17" pageNumber="348" pagination="378 - 383" refId="ref15499" refString="Schubert, K. (1906) Einige neue palaalrktische Staphyliniden aus Kashmir. Deutsche Entomologische Zeitschrift, 1906, 378 - 383. https: // doi. org / 10.1002 / mmnd. 48019060108" type="journal article" year="1906">Schubert (1906)</bibRefCitation>
described 
<taxonomicName id="43B04D3EFFF7296AFD9BA4F5B2F6FD68" authorityName="Schubert" authorityYear="1906" box="[626,967,691,717]" class="Insecta" family="Staphylinidae" genus="Gyrophaena" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="348" phylum="Arthropoda" rank="variety" species="permutaria" variety="puncticolle">
<emphasis id="B6C4EAAFFFF7296AFD9BA4F5B222FD69" box="[626,787,691,717]" italics="true" pageId="17" pageNumber="348">G. permutaria</emphasis>
var. 
<emphasis id="B6C4EAAFFFF7296AFCA2A4F5B2F6FD68" box="[843,967,691,717]" italics="true" pageId="17" pageNumber="348">puncticolle</emphasis>
</taxonomicName>
based on the single specimen, which distinguished from 
<taxonomicName id="43B04D3EFFF7296AFEB9A491B0DEFD55" authorityName="Schubert" authorityYear="1906" box="[336,495,727,753]" class="Insecta" family="Staphylinidae" genus="Gyrophaena" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="348" phylum="Arthropoda" rank="species" species="permutaria">
<emphasis id="B6C4EAAFFFF7296AFEB9A491B055FD54" box="[336,356,727,753]" italics="true" pageId="17" pageNumber="348">G</emphasis>
. 
<emphasis id="B6C4EAAFFFF7296AFE98A491B0DEFD55" box="[369,495,727,752]" italics="true" pageId="17" pageNumber="348">permutaria</emphasis>
</taxonomicName>
