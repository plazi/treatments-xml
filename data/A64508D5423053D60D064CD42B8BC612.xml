<taxonomicName id="AB0972AEFD7E4F4D227B525E875047B9" authorityName="LeConte" authorityYear="1844" class="Insecta" family="Carabidae" genus="Rembus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="627" pageNumber="628" phylum="Arthropoda" rank="species" species="striatopunctatus">Rembus striatopunctatus</taxonomicName>
