: 
<taxonomicName id="F6C04D2C3634FC16C515D869FDDBB764" authority="PEYERIMHOFF" authorityName="Peyerimhoff" authorityYear="1908" box="[293,628,1366,1386]" class="Insecta" family="Staphylinidae" genus="Oxypoda" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="378" phylum="Arthropoda" rank="species" species="transgressa">
