<paragraph id="06660F50C5547F38C6CFE7FAFE69165A" blockId="5.[151,411,1008,1034]" box="[151,411,1008,1034]" pageId="5" pageNumber="35">
<heading id="5D2EB83CC5547F38C6CFE7FAFE69165A" bold="true" box="[151,411,1008,1034]" fontSize="11" level="1" pageId="5" pageNumber="35" reason="1">
<emphasis id="34ADD342C5547F38C6CFE7FAFE69165A" bold="true" box="[151,411,1008,1034]" pageId="5" pageNumber="35">
Genus 
<taxonomicName id="C1D974D3C5547F38C6B6E7FAFE69165A" box="[238,411,1008,1034]" class="Malacostraca" family="Kalliapseudidae" genus="Acutihumerus" kingdom="Animalia" order="Tanaidacea" pageId="5" pageNumber="35" phylum="Arthropoda" rank="genus">
<emphasis id="34ADD342C5547F38C6B6E7FAFE69165A" bold="true" box="[238,411,1008,1034]" italics="true" pageId="5" pageNumber="35">Acutihumerus</emphasis>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
