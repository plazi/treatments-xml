<taxonomicName id="4C424D7AFF9EFFE30D811940FE16F964" authority="R. D. Stone" authorityName="R. D. Stone" authorityYear="2006" box="[204,474,1657,1674]" class="Magnoliopsida" family="Melastomataceae" genus="Warneckea" kingdom="Plantae" order="Myrtales" pageId="19" pageNumber="356" phylum="Tracheophyta" rank="species" species="masoalae">
