<subSubSection id="C3676547FFB2D22D0F6774148C1990CE" pageId="71" pageNumber="72" type="description">
<paragraph id="8BC236CCFFB2D22D0F6774148C1990CE" blockId="71.[264,1321,1093,1244]" pageId="71" pageNumber="72">
<emphasis id="B909EADEFFB2D22D0F6774148B02903F" box="[264,524,1093,1118]" italics="true" pageId="71" pageNumber="72">Diagnostic characters</emphasis>
