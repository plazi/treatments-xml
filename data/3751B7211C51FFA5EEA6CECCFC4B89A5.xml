<taxonomicName id="78F87DB41C51FFA5ECC4CC1CFCC789ED" box="[757,892,873,899]" class="Insecta" family="Noctuidae" genus="Cidariplura" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="491" phylum="Arthropoda" rank="species" species="shanmeii">
