<subSubSection id="C34F65695B67856BFF106BFC8E1E7B47" pageId="5" pageNumber="463" type="distribution">
<paragraph id="8BEA36E25B67856BFF106BFC8A487AD6" blockId="5.[151,1437,151,1401]" pageId="5" pageNumber="463">
<emphasis id="B921EAF05B67856BFF106BFC8A4B7AB3" bold="true" box="[199,352,1159,1184]" pageId="5" pageNumber="463">Distribution.</emphasis>
<taxonomicName id="4C554D615B67856BFEBC6BF289457AB3" box="[363,622,1160,1184]" class="Insecta" family="Staphylinidae" genus="Protocypus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="463" phylum="Arthropoda" rank="species" species="taibaiensis" status="sp. nov.">
<emphasis id="B921EAF05B67856BFEBC6BF289457AB3" box="[363,622,1160,1184]" italics="true" pageId="5" pageNumber="463">Protocypus taibaiensis</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A212578B5B67856BFDAD6BFC89F37AB3" box="[634,728,1159,1184]" pageId="5" pageNumber="463" rank="species">
<emphasis id="B921EAF05B67856BFDAD6BFC89F37AB3" bold="true" box="[634,728,1159,1184]" pageId="5" pageNumber="463">sp. nov.</emphasis>
