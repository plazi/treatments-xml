<taxonomicName id="04B211ABC945FF8E5FD6FCA4FDC51946" authority="(Bleeker)" baseAuthorityName="Bleeker" baseAuthorityYear="1870" box="[222,556,859,884]" class="Actinopterygii" family="Mastacembelidae" genus="Sinobdella" kingdom="Animalia" order="Synbranchiformes" pageId="39" pageNumber="40" phylum="Chordata" rank="species" species="sinensis">
