<taxonomicName id="47E4F667B488E04E03E9A2E9C9CB1CEC" class="Dicotyledoneae" family="Tiliaceae" genus="Tilia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Malvales" pageId="25" pageNumber="38" phylum="Angiospermae" rank="genus">
<emphasis id="61802AA496C4BB4F1464F97EDCF27B4F" italics="true" pageId="25" pageNumber="38">Tilia</emphasis>
