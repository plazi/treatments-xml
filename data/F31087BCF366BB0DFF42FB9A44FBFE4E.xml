<heading id="204E81C6F366BB0AFF42FB9A44E8FBFD" bold="true" box="[151,741,1027,1054]" fontSize="11" level="1" pageId="42" pageNumber="493" reason="1">
<taxonomicName id="BCB94D29F366BB0AFF42FB9A44E8FBFD" authority="Edwards, 1941" authorityName="Edwards. After Edwards" authorityYear="1941" box="[151,741,1027,1054]" class="Insecta" family="Culicidae" genus="Culex" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="493" phylum="Arthropoda" rank="species" species="subaequalis" subGenus="Culiciomyia">
<emphasis id="49CDEAB8F366BB0AFF42FB9A44E8FBFD" bold="true" box="[151,741,1027,1054]" pageId="42" pageNumber="493">
<emphasis id="49CDEAB8F366BB0AFF42FB9A46D2FBFD" bold="true" box="[151,223,1027,1054]" italics="true" pageId="42" pageNumber="493">Culex</emphasis>
(
<emphasis id="49CDEAB8F366BB0AFF25FB9A478EFBFD" bold="true" box="[240,387,1027,1054]" italics="true" pageId="42" pageNumber="493">Culiciomyia</emphasis>
) 
<emphasis id="49CDEAB8F366BB0AFE41FB9A442EFBFD" bold="true" box="[404,547,1027,1054]" italics="true" pageId="42" pageNumber="493">subaequalis</emphasis>
<bibRefCitation id="1F284B5BF366BB0AFDFEFB9D44E8FBFD" author="Edwards, F. W." box="[555,741,1028,1054]" pageId="42" pageNumber="493" refId="ref24576" refString="Edwards, F. W. (1941) Mosquitoes of the Ethiopean Region III. - Culicine adults and pupae. Printed by order of the Trustees, British Museum (Natural History), London, viii + 499 pp., 4 pls." type="book" year="1941">Edwards, 1941</bibRefCitation>
</emphasis>
</taxonomicName>
