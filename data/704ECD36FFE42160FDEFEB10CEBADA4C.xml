<collectionCode id="9EF6E4E5FFE42166FE50EBADCE3ADEC7" box="[471,521,1423,1446]" country="China" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35160" lsid="urn:lsid:biocol.org:col:35160" name="Institute of Hydrobiology, Chinese Academy of Sciences" pageId="19" pageNumber="287">IHB</collectionCode>
<specimenCode id="A841D45BFFE42166FD95EBACCE89DEC7" box="[530,698,1422,1446]" collectionCode="HANA" pageId="19" pageNumber="287">HANA200009</specimenCode>
