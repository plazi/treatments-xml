nymphal 
<typeStatus id="54EE63845957FF88FB4731D3FABCE3CD" box="[1177,1284,1673,1697]" pageId="2" pageNumber="63">paratypes</typeStatus>
in CEUNES, 
<number id="3D9DCF015957FF88FF4931F6FF1DE3A8" box="[151,165,1708,1732]" pageId="2" pageNumber="63" value="2.0">2</number>
nymphal 
<typeStatus id="54EE63845957FF88FECB31F7FE38E3A9" box="[277,384,1709,1733]" pageId="2" pageNumber="63">paratypes</typeStatus>
