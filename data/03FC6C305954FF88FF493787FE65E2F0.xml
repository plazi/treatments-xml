<collectionCode id="ED4445E35957FF88FD9831D2FD1EE3CC" box="[582,678,1672,1696]" pageId="2" pageNumber="63">CEBUC</collectionCode>
, 
<number id="3D9DCF015957FF88FD6F31D2FD07E3CC" box="[689,703,1672,1696]" pageId="2" pageNumber="63" value="2.0">2</number>
