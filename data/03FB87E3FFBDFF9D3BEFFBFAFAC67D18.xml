<taxonomicName id="4C524D76FFBDFF9E3BBDFA81FEF1792E" box="[197,282,1339,1362]" class="Scyphozoa" family="Cyaneidae" genus="Cyanea" kingdom="Animalia" order="Semaeostomeae" pageId="29" pageNumber="30" phylum="Cnidaria" rank="genus">
