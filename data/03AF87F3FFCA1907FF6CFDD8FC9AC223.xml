<quantity id="4CFE9B00FFCA1907FC48FDA4FC57C34E" box="[918,987,599,620]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="3" pageNumber="1154" unit="m" value="2000.0">2000 m</quantity>
