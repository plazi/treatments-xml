(
<emphasis id="84ABEAECA55EFFEBACC646E8FBA21971" box="[1013,1087,244,269]" italics="true" pageId="51" pageNumber="1165">Pigeria</emphasis>
) 
<emphasis id="84ABEAECA55EFFEBAB7F46E8FB1E1971" box="[1100,1155,244,269]" italics="true" pageId="51" pageNumber="1165">piger</emphasis>
(
<bibRefCitation id="D24E4B0FA55EFFEBAF95470BFECF194C" author="Wesmael C" box="[166,338,279,304]" pageId="51" pageNumber="1165" pagination="1 - 166" refId="ref25252" refString="Wesmael C. 1838. Monographie des Braconides de Belgique. 4. Nouveaux Mem Acad R Sci et Belleslettres Bruxelles. 11: 1 - 166." type="journal article" year="1838">Wesmael, 1838</bibRefCitation>
)
</taxonomicName>
