</taxonomicName>
with 
<taxonomicName id="71DF4D7DA55EFFEBACAE46E8FEC6194C" authority="(Wesmael, 1838)" baseAuthorityName="Wesmael" baseAuthorityYear="1838" class="Insecta" family="Braconidae" genus="Bracon" kingdom="Animalia" order="Hymenoptera" pageId="51" pageNumber="1165" phylum="Arthropoda" rank="species" species="piger" subGenus="Pigeria">
