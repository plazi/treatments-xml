(
<bibRefCitation id="753569CEFFBCFFA5FDB543CB155AFE4B" author="Endrodi" box="[612,771,404,429]" pageId="34" pageNumber="485" refString="Endrodi, S. (1969) Monographie der Dynastinae. 4. Tribus: Pentodontini (Coleoptera, Lamellicornia). Entomologische Abhandlungen Museum fur Tierkunde, Dresden, 37, 1 - 145." type="journal article" year="1969">
