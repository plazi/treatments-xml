<keyLead id="308B95EEFF83EC0F4DE12637FA10FC8F" pageId="2" pageNumber="291">
<number id="3DB224FCFF83EC0F4DE12637FF29FCA2" box="[151,165,772,794]" pageId="2" pageNumber="291" value="2.0">2</number>
