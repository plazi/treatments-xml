<location id="8ED760642973FFBBFDA5C5D09FDFFEC7" LSID="urn:lsid:plazi:treatment:03A187A92973FFBBFF03C5B59EE4FD11:8ED760642973FFBBFDA5C5D09FDFFEC7" box="[561,651,253,278]" county="Material" municipality="Late Eocene" name="Europe" pageId="74" pageNumber="75" stateProvince="Rovno">Europe</location>
