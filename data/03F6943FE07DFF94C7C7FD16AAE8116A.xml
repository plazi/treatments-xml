<quantity id="4CA788CCE07DFF95C5CCFA04AAD41671" box="[668,757,1518,1543]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="8" pageNumber="510" unit="mm" value="3.6">3.6 mm</quantity>
