<taxonomicName id="4C054D77FF8AFFFCFF40FA523BEDFA4D" authority="(Mathews, 1914)" baseAuthorityName="Mathews" baseAuthorityYear="1914" box="[191,650,1493,1513]" class="Aves" family="Meliphagidae" genus="Glycichaera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="49" pageNumber="49" phylum="Chordata" rank="subSpecies" species="fallax" subSpecies="claudi">
