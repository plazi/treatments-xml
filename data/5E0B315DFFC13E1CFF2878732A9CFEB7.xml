<heading id="8D553727FFC13E1DFF2878732A15F962" bold="true" box="[151,568,1656,1683]" fontSize="11" level="1" pageId="16" pageNumber="17" reason="1">
<emphasis id="E4D65C59FFC13E1DFF2878732A15F962" bold="true" box="[151,568,1656,1683]" pageId="16" pageNumber="17">
Genus 
<taxonomicName id="11A2FBC8FFC13E1DFF5178722A15F962" authority="Panning, 1949" authorityName="Panning" authorityYear="1949" box="[238,568,1656,1683]" class="Holothuroidea" family="Cucumariidae" genus="Pseudocnus" kingdom="Animalia" order="Dendrochirotida" pageId="16" pageNumber="17" phylum="Echinodermata" rank="genus">
<emphasis id="E4D65C59FFC13E1DFF5178722952F963" bold="true" box="[238,383,1657,1683]" italics="true" pageId="16" pageNumber="17">Pseudocnus</emphasis>
<bibRefCitation id="B233FDBAFFC13E1DFE3978732A15F962" author="Panning" box="[390,568,1656,1682]" pageId="16" pageNumber="17" refString="Panning, A. (1949) Versuch einer Neuordnung der Familie Cucumariidae (Holothuroidea: Dendrochirota). Zoologische Jahrbucher, 78, 404 - 470." type="journal article" year="1949">
Panning, 
<number id="606A926CFFC13E1DFE4278732A15F962" box="[509,568,1656,1682]" pageId="16" pageNumber="17" value="1949.0">1949</number>
</bibRefCitation>
