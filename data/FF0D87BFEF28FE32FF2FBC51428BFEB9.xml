<taxonomicName id="B0A44D2AEF28FE2BFCABBDC9457AFA47" box="[787,860,1411,1434]" class="Malacostraca" family="Epimeriidae" genus="Epimeria" kingdom="Animalia" order="Amphipoda" pageId="9" pageNumber="443" phylum="Arthropoda" rank="species" species="emma" status="sp. nov.">Emma</taxonomicName>
