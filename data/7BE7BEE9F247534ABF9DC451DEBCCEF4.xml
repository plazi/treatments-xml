<quantity id="7481C64DB409D1BF469DFDC4545B9EF7" metricMagnitude="-2" metricUnit="m" metricValue="2.85" metricValueMax="3.2" metricValueMin="2.5" unit="mm" value="28.5" valueMax="32.0" valueMin="25.0">25-32 mm</quantity>
