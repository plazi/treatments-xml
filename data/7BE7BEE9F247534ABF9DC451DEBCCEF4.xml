<subSubSection id="FD65D7A9ACA58D1817539AF7BD47A56A" pageId="0" pageNumber="73" type="ecology">
<paragraph id="3B01146C1921E5513526907D924B2F89" pageId="0" pageNumber="73">Ecology.</paragraph>
<paragraph id="4357195ADC02385517E785CD54B59BDD" pageId="0" pageNumber="73">From shallow soft sediments to sandy areas along the reef edge. Rarely in deeper soft sediment habitats beyond the reef edge. Depth 0.5-11 m.</paragraph>
</subSubSection>
<subSubSection id="438E46FFE4E7AAD18E9C38E020D5B028" pageId="0" pageNumber="73" type="distribution">
<paragraph id="EC3A6516047258C95AF16D44D5454324" pageId="0" pageNumber="73">Distribution.</paragraph>
<paragraph id="8BD26890D4158C040864F69B1591DE9E" pageId="0" pageNumber="73">
<taxonomicName id="77081052DC9BE5ED1D007C9E979B6DB0" authorityName="van Hasselt" authorityYear="1824" class="Gastropoda" family="Plakobranchidae" genus="Plakobranchus" kingdom="Animalia" lsidName="Plakobranchus ocellatus" order="Sacoglossa" pageId="0" pageNumber="73" phylum="Chlorophyta" rank="species" species="ocellatus">
<emphasis id="A4424119BD4E4358D4F4920CDDF8BA24" italics="true" pageId="0" pageNumber="73">Plakobranchus ocellatus</emphasis>
</taxonomicName>
and 
<taxonomicName id="5C0B2B232EC3D02EAF436A3E32915E99" genus="P." lsidName="P. ocellatus" pageId="0" pageNumber="73" rank="species" species="ocellatus">P. cf. ocellatus</taxonomicName>
