<taxonomicName id="4C5907F64678FFA0FF55B969B6B8F970" authority="(E. A. Miyasato &amp; S. D. L. Imenes)" authorityName="E.A.Miyasato &amp; S.D.L.Imenes" box="[151,719,1302,1327]" class="Liliopsida" family="Poaceae" genus="Bambusa" kingdom="Plantae" order="Poales" pageId="1" pageNumber="392" phylum="Tracheophyta" rank="species" species="gracilis">
