species. 
<taxonomicName id="4C5470EBF52FC50AFED9F6B8FD5C139D" authority="Bolivar, 1898" authorityName="Bolivar" authorityYear="1898" box="[348,673,1419,1443]" class="Insecta" family="Tetrigidae" genus="Gestroa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="144" phylum="Arthropoda" rank="species" species="discoidea">
