<taxonomicName id="4C2E4D4DEF1F734340D7FCA771C01E60" box="[751,935,775,800]" class="Insecta" family="Staphylinidae" genus="Psammostiba" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="comparabilis">
