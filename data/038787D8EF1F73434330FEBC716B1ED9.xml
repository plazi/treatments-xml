<taxonomicName id="4C2E4D4DEF1F734341D8FCA776221E60" box="[992,1093,775,800]" class="Insecta" family="Staphylinidae" genus="Psammostiba" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="kenaii">
