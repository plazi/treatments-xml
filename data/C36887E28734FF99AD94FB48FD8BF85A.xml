<paragraph id="4B7E36F48734FF99AD94FAFEFBEFFAFF" blockId="17.[151,1435,1274,1361]" box="[151,1029,1338,1361]" pageId="17" pageNumber="18">
<treatmentCitationGroup id="6BD111DA8734FF99AD94FAFEFBEFFAFF" box="[151,1029,1338,1361]" pageId="17" pageNumber="18">
