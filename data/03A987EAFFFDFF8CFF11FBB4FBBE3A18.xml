<taxonomicName id="4C004D7FFFFDFF8CFEBCFB15FD393A75" authority="(WESMAEL, 1849)" baseAuthorityName="WESMAEL" baseAuthorityYear="1849" box="[280,719,1261,1285]" class="Insecta" family="Ichneumonidae" genus="Camposcopus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="254" phylum="Arthropoda" rank="species" species="nigricornis">
