<taxonomicName id="4C0D09AFFFD0C316FD8F65275AA7F8C3" authority="Van Duzee" authorityName="Van Duzee" box="[591,905,1836,1859]" class="Insecta" family="Cicadellidae" genus="Agallia" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="35" phylum="Arthropoda" rank="species" species="constricta">
