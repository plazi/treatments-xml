<tr id="353D706EFFD03CEDFED0676A5C33FA3D" box="[272,1309,1377,1469]" gridrow="1" pageId="4" pageNumber="35">
<th id="76EC1912FFD03CEDFED0676A5B58FA3D" box="[272,630,1377,1469]" gridcol="0" gridrow="1" pageId="4" pageNumber="35">Distance between ocelli</th>
<td id="76EC1912FFD03CEDFD4A676A5AE5FA3D" box="[650,971,1377,1469]" gridcol="1" gridrow="1" pageId="4" pageNumber="35">greater than distance from ocellus to compound eye</td>
