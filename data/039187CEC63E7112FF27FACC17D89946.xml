<taxonomicName id="4C384D5BC63E7112FE93FACB16CA9EBB" authority="Saussure, 1878" authorityName="Saussure" authorityYear="1878" box="[341,682,1395,1422]" class="Insecta" family="Gryllidae" kingdom="Animalia" order="Orthoptera" pageId="31" pageNumber="436" phylum="Arthropoda" rank="subFamily" subFamily="Podoscirtinae">
PODOSCIRTINAE 
<bibRefCitation id="EFA94B29C63E7112FDCFFACC16CA9EBB" author="SAUSSURE H. &amp; DE" box="[521,682,1395,1421]" pageId="31" pageNumber="436" pagination="369 - 702" refId="ref36461" refString="SAUSSURE H. DE 1878. - Melanges orthopterologiques. VIe fascicule. Gryllides (2 e partie). Memoires de la Societe de Physique et d'Histoire naturelle de Geneve 25: 369 - 702." type="journal article" year="1878">Saussure, 1878</bibRefCitation>
