<taxonomicName id="63F76183BEDE8F60F75B5202BF942BB9" genus="Aspilapterix" lsidName="Aspilapterix spectabilis" pageId="3" pageNumber="148" rank="species" species="spectabilis">Aspilapterix spectabilis</taxonomicName>
, paratype, male genitalia; Austria, Osttirol, Virgental, Venedigergruppe, 
<normalizedToken id="1AAA223752330B591E95879F8BEEB153" originalValue="Sajatmähder">Sajatmaehder</normalizedToken>
