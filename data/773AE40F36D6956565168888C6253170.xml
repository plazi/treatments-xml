<paragraph id="F5EC00383CF787618CDC58C1C5B071D8" pageId="2" pageNumber="147">
<taxonomicName id="29F5FBF9D8BBBC66F63CA1C7FBB863F7" LSID="http://zoobank.org/4B34364D-EDD2-4E73-A2A8-903EC332015C" authority="Huemer, Lopez-Vaamonde &amp; Triberti" authorityName="Huemer, Lopez-Vaamonde &amp; Triberti" authorityYear="2016" class="Insecta" family="Gracillariidae" genus="Mercantouria" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Mercantouria" order="Lepidoptera" pageId="2" pageNumber="147" phylum="Arthropoda" rank="genus">Mercantouria Huemer, Lopez-Vaamonde &amp; Triberti</taxonomicName>
<taxonomicNameLabel id="A6591F524FB3E982EA09E3A475952641" pageId="2" pageNumber="147">gen. n.</taxonomicNameLabel>
