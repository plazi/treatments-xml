<number id="3B43C143F659FFE5FB94FA08FB01C6B4" box="[1143,1212,1416,1440]" pageId="3" pageNumber="566" value="12.0" valueMax="13.0" valueMin="11.0">11–13</number>
setae; R 
<number id="3B43C143F659FFE5FAFFFA17FA99C6B1" box="[1308,1316,1431,1445]" pageId="3" pageNumber="566" value="1.0">1</number>
with 
<number id="3B43C143F659FFE5FA87FA08FA21C6B4" box="[1380,1436,1416,1440]" pageId="3" pageNumber="566" value="10.0" valueMax="12.0" valueMin="8.0">8–12</number>
