<number id="3B43C143F659FFE5FBB6FA2FFB20C6DC" box="[1109,1181,1455,1480]" pageId="3" pageNumber="566" value="76.5" valueMax="77.0" valueMin="76.0">76–77</number>
, m 
<number id="3B43C143F659FFE5FB5CFA3EFB79C6D8" box="[1215,1220,1470,1484]" pageId="3" pageNumber="566" value="1.0">1</number>
+ 
<number id="3B43C143F659FFE5FB2CFA3EFB65C6D8" box="[1231,1240,1470,1484]" pageId="3" pageNumber="566" value="2.0">2</number>
with 
<number id="3B43C143F659FFE5FAFAFA2FFADCC6D3" box="[1305,1377,1455,1479]" pageId="3" pageNumber="566" value="40.5" valueMax="41.0" valueMin="40.0">40–41</number>
