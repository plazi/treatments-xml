with 
<number id="3B43C143F659FFE5FEA8FA2FFE2CC6D3" box="[331,401,1455,1479]" pageId="3" pageNumber="566" value="22.5" valueMax="28.0" valueMin="17.0">17–28</number>
setae; M 
<number id="3B43C143F659FFE5FE19FA3EFE42C6D8" box="[506,511,1470,1484]" pageId="3" pageNumber="566" value="1.0">1</number>
+ 
<number id="3B43C143F659FFE5FDEAFA3EFDAFC6D8" box="[521,530,1470,1484]" pageId="3" pageNumber="566" value="2.0">2</number>
with 
<number id="3B43C143F659FFE5FDB0FA2FFD27C6D3" box="[595,666,1455,1479]" pageId="3" pageNumber="566" value="25.5" valueMax="28.0" valueMin="23.0">23–28</number>
setae; Cu 
<number id="3B43C143F659FFE5FCE9FA3EFCAFC6D8" box="[778,786,1470,1484]" pageId="3" pageNumber="566" value="1.0">1</number>
with 
<number id="3B43C143F659FFE5FCB0FA2FFCDCC6D3" box="[851,865,1455,1479]" pageId="3" pageNumber="566" value="9.0">9</number>
setae. Cells r 
<number id="3B43C143F659FFE5FC18FA3EFBBEC6D8" box="[1019,1027,1470,1484]" pageId="3" pageNumber="566" value="4.0">4</number>
+ 
