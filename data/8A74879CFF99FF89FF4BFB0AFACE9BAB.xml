<taxonomicName id="C5DD4D09FF99FF89FA83F9F4FF3D9BE1" class="Insecta" family="Therevidae" genus="Ruppellia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="36" phylum="Arthropoda" rank="species" species="semiflava">
