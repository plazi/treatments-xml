<taxonomicName id="C5DD4D09FF99FF89FE6CF92CFD879BC2" authority="Krober" authorityName="Krober" box="[432,629,1679,1704]" class="Insecta" family="Therevidae" genus="Ruppellia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="36" phylum="Arthropoda" rank="species" species="pallida">
