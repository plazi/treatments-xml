<taxonomicName id="499F42CAFF97725DFE12559FFD18849C" authorityName=": Gunther" authorityYear="1938" box="[497,661,404,429]" class="Insecta" family="Tetrigidae" genus="Indoscelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="genus">
