<taxonomicName id="499F42CAFF94725EFEC9537FFD3482BB" authority="Bolivar, 1887" authorityName=": Bolivar" authorityYear="1887" box="[298,697,1908,1930]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="sanguinolenta">
