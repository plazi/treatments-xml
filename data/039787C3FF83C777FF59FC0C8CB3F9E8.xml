</taxonomicName>
and the 
<taxonomicName id="4C3E4D56FF83C777FF0EF9E78C0EF9E9" authorityName="Walker" authorityYear="1855" box="[192,324,1555,1578]" class="Insecta" family="Erebidae" genus="Agrisius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="131" phylum="Arthropoda" rank="species" species="guttivitta">
