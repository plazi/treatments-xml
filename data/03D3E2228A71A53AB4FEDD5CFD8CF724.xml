<taxonomicName id="4C7A28B78A6EA538B544DCFBFE55F699" box="[301,503,1960,1983]" class="Insecta" family="Hemiptera" genus="Valentia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="hoffmanni">
