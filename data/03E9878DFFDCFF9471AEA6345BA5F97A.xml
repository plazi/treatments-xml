<taxonomicName id="4C404D18FFDDFF94759AA60A5A0EF997" authorityName="Iredale" authorityYear="1939" box="[1168,1329,1605,1626]" class="Gastropoda" family="Camaenidae" genus="Torresitrachia" kingdom="Animalia" order="Stylommatophora" pageId="7" pageNumber="696" phylum="Mollusca" rank="genus">
