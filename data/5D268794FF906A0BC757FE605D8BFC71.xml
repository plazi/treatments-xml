<taxonomicName id="128F4D01FF906A0BC0C9FD265C1FFD78" authorityName="Rusconi" authorityYear="1933" box="[1263,1395,628,649]" class="Reptilia" family="Uruguaysuchidae" genus="Uruguaysuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crocodylia" pageId="3" phylum="Chordata" rank="species" species="aznarezi">
