<taxonomicName id="08B04D74FFEBFFFBFC1085E3735AF95A" baseAuthorityName="Nakane" baseAuthorityYear="1963" box="[974,1131,1727,1753]" class="Insecta" family="Tenebrionidae" genus="Dioedus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="133" phylum="Arthropoda" rank="species" species="miyakensis">
