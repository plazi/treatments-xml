<date id="FFB3104AFFD27621FB67566AFAACF8D9" box="[1238,1326,1868,1891]" pageId="0" pageNumber="595" value="2011-10-11">
<number id="3DC524ADFFD27621FB67566AFB72F8D9" box="[1238,1264,1868,1891]" pageId="0" pageNumber="595" value="11.0">11</number>
x 
<number id="3DC524ADFFD27621FAA5566AFAACF8D9" box="[1300,1326,1868,1891]" pageId="0" pageNumber="595" value="11.0">11</number>
</date>
