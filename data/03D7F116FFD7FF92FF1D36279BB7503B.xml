– 
<specimenCount id="9D788B89FFD4FF93FE2336949EB0568E" box="[425,468,1933,1960]" pageId="58" pageNumber="59" type="female">1 ♀</specimenCount>
, 
<collectingCountry id="F3690090FFD4FF93FE6936979D4D568E" box="[483,553,1934,1960]" name="Egypt" pageId="58" pageNumber="59">Egypt</collectingCountry>
