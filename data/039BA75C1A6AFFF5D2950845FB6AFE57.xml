<taxonomicName id="4C326DC91A6AFFF5D34F0845FDC6FEC2" authority="Karsch, 1891" authorityName="Karsch" authorityYear="1891" box="[278,597,300,324]" class="Insecta" family="Acrididae" genus="Orthochtha" kingdom="Animalia" order="Orthoptera" pageId="52" pageNumber="363" phylum="Arthropoda" rank="genus">
<emphasis id="B946CA581A6AFFF5D34F0845FE50FEC5" bold="true" box="[278,451,300,323]" italics="true" pageId="52" pageNumber="363">ORTHOCHTHA</emphasis>
