<quantity id="3E45129FF27A142572F8EFED56652DC5" box="[474,576,1449,1475]" metricMagnitude="-4" metricUnit="m" metricValue="8.0" pageId="19" pageNumber="20" unit="mm" value="0.8">0.80 mm</quantity>
, 
<geoCoordinate id="9C89D9BDF27A14257169EFED56B82DC5" box="[587,669,1449,1475]" direction="west" orientation="longitude" pageId="19" pageNumber="20" precision="555" value="-0.13">
