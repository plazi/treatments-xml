<taxonomicName id="91B85BCD5710FFAE55B8F8DFFBACDB63" authority="Hummon, Balsamo &amp; Todaro, 1992" authorityName="Hummon, Balsamo &amp; Todaro" authorityYear="1992" box="[151,1068,1831,1858]" family="Chaetonotidae" genus="Chaetonotus" kingdom="Animalia" order="Chaetonotida" pageId="5" pageNumber="337" phylum="Gastrotricha" rank="species" species="apolemmus" subGenus="Marinochaetus">
<emphasis id="64CCFC5C5710FFAE55B8F8DFFBACDB63" bold="true" box="[151,1068,1831,1858]" pageId="5" pageNumber="337">
<emphasis id="64CCFC5C5710FFAE55B8F8DFFEA9DB60" bold="true" box="[151,297,1831,1858]" italics="true" pageId="5" pageNumber="337">Chaetonotus</emphasis>
(
<emphasis id="64CCFC5C5710FFAE5415F8DFFE6ADB60" bold="true" box="[314,490,1831,1858]" italics="true" pageId="5" pageNumber="337">Marinochaetus</emphasis>
