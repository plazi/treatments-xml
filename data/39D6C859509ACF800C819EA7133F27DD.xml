<taxonomicName id="60113BD3AB3E5FDF934BC1914BA26F2E" authority="Weigel" class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="298" phylum="Tracheophyta" rank="species" species="helveticum">
