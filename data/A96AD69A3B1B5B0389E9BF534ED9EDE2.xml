<taxonomicName id="6B95DFE1320860E8D037F83435AB8C75" baseAuthorityName="Brusina" baseAuthorityYear="1903" class="Gastropoda" family="Melanopsidae" genus="Microcolpia" kingdom="Animalia" lsidName="Microcolpia parreyssii subsp. sikorai" order="Sorbeoconcha" pageId="68" pageNumber="69" phylum="Mollusca" rank="subSpecies" species="parreyssii" subSpecies="sikorai">
