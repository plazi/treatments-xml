<taxonomicName id="D0C24ECB38BF2873AB4017D399AD5036" authorityName="Gassies" authorityYear="1861" class="Gastropoda" family="Melanopsidae" genus="Melanopsis" kingdom="Animalia" lsidName="Melanopsis carinata" order="Pteropoda" pageId="68" pageNumber="69" phylum="Mollusca" rank="species" species="carinata">
