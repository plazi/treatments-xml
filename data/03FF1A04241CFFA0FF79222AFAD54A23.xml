<quantity id="4CAE06F7241CFFA0FB6C22CEFB414E2A" box="[1192,1251,493,520]" metricMagnitude="-2" metricUnit="m" metricValue="3.0" pageId="10" pageNumber="146" unit="cm" value="3.0">3 cm</quantity>
