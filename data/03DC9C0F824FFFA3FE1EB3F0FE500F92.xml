species: 
<taxonomicName id="4C75569A824FFFA2FF4EB3B4FDE509F6" authority="Pascoe 1863: 551" authorityName="Pascoe. Since" authorityPageNumber="551" authorityYear="1863" box="[234,606,1489,1511]" class="Insecta" family="Cerambycidae" genus="Phoracantha" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="1691" phylum="Arthropoda" rank="species" species="scitula">
