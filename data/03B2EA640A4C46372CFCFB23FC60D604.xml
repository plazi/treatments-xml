<taxonomicName id="4C1B20F10A4C46312EC9FB7CFC77D24E" authority="Hempel" authorityName="Hempel. Macropterous" authorityYear="1900" box="[674,956,1264,1289]" class="Insecta" family="Eriococcidae" genus="Tectococcus" kingdom="Animalia" order="Hemiptera" pageId="99" pageNumber="100" phylum="Arthropoda" rank="species" species="ovatus">
