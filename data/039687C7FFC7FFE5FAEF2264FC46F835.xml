<taxonomicName id="4C3F4D52FFC7FFE5FE1D26D6FE93F9BB" authority="Waters, 1899" authorityName="Waters" authorityYear="1899" class="Gymnolaemata" genus="Lepralia" kingdom="Animalia" order="Cheilostomatida" pageId="21" pageNumber="22" phylum="Bryozoa" rank="species" species="mucronelliformis">
