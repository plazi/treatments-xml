<taxonomicName id="4C011396D23A0274FC0EF920546E35F3" authority="Tohno et al. 2017" authorityName="Tohno" authorityYear="2017" box="[935,1388,1748,1773]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="53" pageNumber="2835" phylum="Firmicutes" rank="species" species="pentosiphilus">
