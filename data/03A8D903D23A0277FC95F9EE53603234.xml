<taxonomicName id="4C011396D23A0274FC03F953553035A1" box="[938,1074,1703,1727]" class="Bacilli" family="Lactobacillaceae" genus="Secundilactobacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="53" pageNumber="2835" phylum="Firmicutes" rank="species" species="pentosiphilus">
