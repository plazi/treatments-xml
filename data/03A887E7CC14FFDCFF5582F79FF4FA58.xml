<taxonomicName id="4C014D72CC14FFE3FD8981539EBDF983" authority="Range" authorityName="Range" box="[541,667,1642,1663]" class="Insecta" family="Tachinidae" genus="Dolichocolon" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="575" phylum="Arthropoda" rank="species" species="rex">Rex Range</taxonomicName>
