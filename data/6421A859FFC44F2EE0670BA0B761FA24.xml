<taxonomicName id="2B8862CCFFC44F28E3E40831B209F9AF" authority="d'Orbigny, 1850" authorityName="d'Orbigny" authorityYear="1850" box="[706,1028,1596,1620]" class="Bivalvia" family="Cardiidae" genus="Cardium" kingdom="Animalia" order="Cardiida" pageId="43" pageNumber="60" phylum="Mollusca" rank="species" species="peresii">
