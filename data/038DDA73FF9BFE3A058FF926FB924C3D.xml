<treatmentCitationGroup id="AB344C4BFF9BFE3A05B1F8A3FDAE48E6" box="[132,599,1906,1930]" pageId="15" pageNumber="418">
<taxonomicName id="4C2410E6FF9BFE3A05B1F8A3FEB948E6" authorityName="Haeckel" authorityYear="1862" box="[132,320,1906,1930]" class="Polycystina" higherTaxonomySource="GBIF" kingdom="Chromista" order="Spumellaria" pageId="15" pageNumber="568" phylum="Radiozoa" rank="superFamily" superFamily="Spongosphaeroidea">Spongosphaeroidea</taxonomicName>
