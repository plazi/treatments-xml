<taxonomicName id="4C084D4BFFCCFFCF63E1F8B707EEF066" authority="Makhan." authorityName="Makhan." box="[738,945,1863,1889]" class="Insecta" family="Hydrochidae" genus="Hydrochus" kingdom="Animalia" order="Coleoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="piroei">
<emphasis id="B97CEADAFFCCFFCF63E1F8B70718F067" box="[738,839,1863,1888]" italics="true" pageId="48" pageNumber="49">H. piroei</emphasis>
