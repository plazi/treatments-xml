<taxonomicName id="4C014D1CFF9F996DF01AF8FCFDACF885" authority="Liu &amp; Chen" authorityName="Liu &amp; Chen" authorityYear="2020" box="[282,612,1879,1904]" class="Insecta" family="Braconidae" genus="Apanteles" kingdom="Animalia" order="Hymenoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="ambultor" status="sp. nov.">
