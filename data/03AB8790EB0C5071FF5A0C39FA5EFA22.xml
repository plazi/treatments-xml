<taxonomicName id="4C024D05EB0C5071FEE20C84FE71FC60" box="[303,435,980,1004]" class="Reptilia" family="Typhlopidae" genus="Typhlops" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="144" phylum="Chordata" rank="species" species="schwartzi">
