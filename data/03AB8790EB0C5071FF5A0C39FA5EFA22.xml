<taxonomicName id="4C024D05EB0C5071FE090C84FDA7FC60" box="[452,613,980,1004]" class="Reptilia" family="Typhlopidae" genus="Typhlops" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="144" phylum="Chordata" rank="species" species="tetrathyreus">
