<taxonomicName id="4C024D05EB0C5071FA4A0CE0FEE3FC60" class="Reptilia" family="Typhlopidae" genus="Typhlops" kingdom="Animalia" order="Squamata" pageId="8" pageNumber="144" phylum="Chordata" rank="species" species="pachyrhinus">
