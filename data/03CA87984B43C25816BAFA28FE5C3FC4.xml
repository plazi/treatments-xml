<taxonomicName id="4C634D0D4B44C2591731F8FCFD173BD2" authority="Rayment, 1933 b" authorityName="Rayment" authorityYear="1933" box="[284,734,1918,1943]" class="Insecta" family="Mutillidae" genus="Ephutomorpha" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="213" phylum="Arthropoda" rank="species" species="subelegans">
