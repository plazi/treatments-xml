<taxonomicName id="4C634D0D4B45C2581583FCC1FC273F19" authorityName="Jurine in Spinola" authorityYear="1808" box="[942,1006,835,860]" class="Insecta" family="Crabronidae" genus="Pison" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="214" phylum="Arthropoda" rank="genus">
<emphasis id="B917EA9C4B45C2581583FCC1FC273F19" box="[942,1006,835,860]" italics="true" pageId="13" pageNumber="214">Pison</emphasis>
</taxonomicName>
