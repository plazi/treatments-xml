<taxonomicName id="4C634D0D4B45C25816D7FCE4FE433FC4" authorityName="Latreille" authorityYear="1802" box="[250,394,870,897]" class="Insecta" family="Crabronidae" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="214" phylum="Arthropoda" rank="family">Crabronidae</taxonomicName>
