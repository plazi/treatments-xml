<taxonomicName id="A9895CE4FF8C050EFC6DB6CCFBC267D4" box="[1021,1130,1735,1759]" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="50" phylum="Arthropoda" rank="species" species="decora">
