<taxonomicName id="32A14D1DEE407A7DFD0E62645F25FEFA" box="[710,852,352,376]" class="Insecta" family="Hymenoptera" genus="Xenapates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="145" phylum="Arthropoda" rank="species" species="nigrifrons">
