<taxonomicName id="3A9F4D4DFFABFFB919F77F68FAB7F881" box="[1185,1324,1875,1900]" class="Insecta" family="Zopheridae" genus="Phellopsis" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="chinensis">
