<taxonomicName id="3A9F4D4DFFABFFB91DEE7F40FEF9F879" box="[184,354,1915,1940]" class="Insecta" family="Zopheridae" genus="Phellopsis" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="yulongensis" status="new species">
