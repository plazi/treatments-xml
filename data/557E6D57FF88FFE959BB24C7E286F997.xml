<collectingRegion id="1F1312A3FF88FFE958432384E6E5FA34" box="[312,451,1410,1434]" country="United States of America" name="Mississippi" pageId="6" pageNumber="5">Mississippi</collectingRegion>
River from southern 
<collectingRegion id="1F1312A3FF88FFE95BB02384E407FA34" box="[715,801,1410,1434]" country="United States of America" name="Illinois" pageId="6" pageNumber="5">Illinois</collectingRegion>
to southern 
<collectingRegion id="1F1312A3FF88FFE95AC52384E318FA37" box="[958,1086,1410,1433]" country="United States of America" name="Tennessee" pageId="6" pageNumber="5">Tennessee</collectingRegion>
and northeastern 
<collectingRegion id="1F1312A3FF88FFE95C582384E2BFFA34" box="[1315,1433,1410,1434]" country="United States of America" name="Arkansas" pageId="6" pageNumber="5">Arkansas</collectingRegion>
......................................................................................................... 
<taxonomicName id="1AD7A7C2FF88FFE95D3523A7E286FA17" authority="Causey 1974" authorityName="Causey" authorityYear="1974" box="[1102,1440,1441,1465]" pageId="6" pageNumber="5" rank="subFamily" subFamily="Ptyoiulinae">
