<taxonomicName id="4C114D20C427FF8FFBB93D0213C54217" box="[1058,1278,332,357]" class="Polypodiopsida" family="Nephrolepidaceae" genus="Arthropteris" kingdom="Plantae" order="Polypodiales" pageId="4" pageNumber="573" phylum="Tracheophyta" rank="species" species="tenella">
