<taxonomicName id="27AA4D08FF94FF8C96F7FACED6B4720C" authority="Moure &amp; Castro, 2001" authorityName="Moure &amp; Castro" authorityYear="2001" box="[189,711,1315,1341]" class="Insecta" family="Apidae" genus="Centris" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="xanthomelaena">
