<taxonomicName id="9CEC4D62FFFB2F6BFD93C58AFD0E5EE6" box="[567,746,1173,1196]" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="923" phylum="Tracheophyta" rank="species" species="communis">
