<taxonomicName id="9CEC4D62FFFB2F6BFCDAC56DFBA55EC3" box="[894,1089,1137,1161]" class="Magnoliopsida" family="Rosaceae" genus="Prunus" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="923" phylum="Tracheophyta" rank="species" species="domestica">
