<td id="FB105DDDC640005FFC2E117028D5F9FC" box="[896,909,1626,1647]" gridcol="4" gridrow="5" pageId="13" pageNumber="1259">1</td>
<td id="FB105DDDC640005FFC05117028A9F9FC" box="[939,1009,1626,1647]" gridcol="5" gridrow="5" pageId="13" pageNumber="1259">1</td>
<td id="FB105DDDC640005FFBA111702F43F9FC" box="[1039,1051,1626,1647]" gridcol="6" gridrow="5" pageId="13" pageNumber="1259">1</td>
</tr>
<tr id="B8C134A1C640005FFE56115E2F43F919" box="[504,1051,1652,1674]" gridrow="6" pageId="13" pageNumber="1259">
<td id="FB105DDDC640005FFE56115E290FF919" box="[504,599,1652,1674]" gridcol="0" gridrow="6" pageId="13" pageNumber="1259">XIII</td>
<td id="FB105DDDC640005FFD27115E29CDF919" box="[649,661,1652,1674]" gridcol="1" gridrow="6" pageId="13" pageNumber="1259">1</td>
<td id="FB105DDDC640005FFD1F115E29A4F919" box="[689,764,1652,1674]" gridcol="2" gridrow="6" pageId="13" pageNumber="1259">1</td>
<td id="FB105DDDC640005FFCB7115E287DF919" box="[793,805,1652,1674]" gridcol="3" gridrow="6" pageId="13" pageNumber="1259">–</td>
<td id="FB105DDDC640005FFC2E115E28D5F919" box="[896,909,1652,1674]" gridcol="4" gridrow="6" pageId="13" pageNumber="1259">1</td>
<td id="FB105DDDC640005FFC05115E28A9F919" box="[939,1009,1652,1674]" gridcol="5" gridrow="6" pageId="13" pageNumber="1259">1</td>
<td id="FB105DDDC640005FFBA1115E2F43F919" box="[1039,1051,1652,1674]" gridcol="6" gridrow="6" pageId="13" pageNumber="1259">1</td>
