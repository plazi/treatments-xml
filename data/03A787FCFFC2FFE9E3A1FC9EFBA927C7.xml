<collectingCountry id="F319767AFFC2FFE9E0ACFBC6FB8A271B" box="[922,1048,1053,1079]" name="Costa Rica" pageId="43" pageNumber="44">Costa Rica</collectingCountry>
, 
<collectingCountry id="F319767AFFC2FFE9E712FBC6FB13271B" box="[1060,1153,1053,1079]" name="Jamaica" pageId="43" pageNumber="44">Jamaica</collectingCountry>
