<taxonomicName id="1CBE43ECDE249CDC0864D832DFE9DB85" class="Insecta" family="Tipulidae" genus="Tipula" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tipula (V.) gongdangensis" order="Diptera" pageId="0" pageNumber="33" phylum="Arthropoda" rank="species" species="gongdangensis" subGenus="V.">Tipula (V.) gongdangensis</taxonomicName>
