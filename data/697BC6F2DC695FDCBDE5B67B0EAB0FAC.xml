, 
<location id="4D2CC8954333F4CDDFBEAB39B89CCA88" LSID="urn:lsid:plazi:treatment:697BC6F2DC695FDCBDE5B67B0EAB0FAC:4D2CC8954333F4CDDFBEAB39B89CCA88" country="China" county="Lingshui County" name="Diaoluoshan Natural Reserve" stateProvince="Hainan Province">Diaoluoshan Natural Reserve</location>
, on fallen leaves of 
<taxonomicName id="BB52301C0F5505FA1CAC40B646E8D0CF" class="Magnoliopsida" family="Theaceae" genus="Polyspora" kingdom="Plantae" lsidName="Polyspora hainanensis" order="Ericales" pageId="0" pageNumber="47" phylum="Tracheophyta" rank="species" species="hainanensis">
