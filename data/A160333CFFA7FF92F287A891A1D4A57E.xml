<taxonomicName id="EEC9F9A9FFA7FF92F287AF2BA078A708" authority="Lea, 1905: 223" authorityName="Lea" authorityPageNumber="223" authorityYear="1905" box="[151,552,1110,1133]" class="Insecta" family="Curculionidae" genus="Melanterius" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="costipennis">
<emphasis id="1BBD5E38FFA7FF92F287AF2BA3D9A709" box="[151,393,1110,1133]" italics="true" pageId="15" pageNumber="16">Melanterius costipennis</emphasis>
