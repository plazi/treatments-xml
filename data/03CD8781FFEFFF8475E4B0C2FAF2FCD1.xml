<paragraph id="8BDB3697FFEFFF8475E4B155FD6FFE04" blockId="12.[151,1436,329,413]" pageId="12" pageNumber="13">
<taxonomicName id="4C644D14FFEFFF8475E4B155FD80FEC7" authority="Girault, 1911: 400" authorityName="Girault" authorityPageNumber="400" authorityYear="1911" box="[151,544,329,351]" class="Insecta" family="Pteromalidae" genus="Catolaccus" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="cyaneus">
