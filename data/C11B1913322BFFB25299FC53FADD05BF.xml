<taxonomicName id="8EB2D386322BFFB1502DFADBFD3A052B" authorityName="White &amp; Thomas" authorityYear="2009" box="[547,640,1313,1338]" class="Magnoliopsida" family="Ericaceae" genus="Leucothoe" kingdom="Plantae" order="Ericales" pageId="4" pageNumber="498" phylum="Tracheophyta" rank="species" species="adelphe">
