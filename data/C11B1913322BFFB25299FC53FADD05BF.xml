<subSubSection id="01A8FB8E322BFFB152C8FADAFB65052A" box="[198,1247,1312,1339]" pageId="4" pageNumber="498" type="etymology">
<paragraph id="490DA805322BFFB152C8FADAFB65052A" blockId="4.[151,1436,1042,2035]" box="[198,1247,1312,1339]" pageId="4" pageNumber="498">
<emphasis id="7BC67417322BFFB152C8FADAFEE9052B" bold="true" box="[198,339,1312,1338]" pageId="4" pageNumber="498">Etymology.</emphasis>
