</taxonomicName>
, 
<collectingCountry id="F314760CFFE0206FFBF18A05FB33FDAE" box="[1107,1209,538,563]" name="Thailand" pageId="5" pageNumber="532">Thailand</collectingCountry>
, 
<collectingCountry id="F314760CFFE0206FFB658A05FAA3FDAE" box="[1223,1321,538,563]" name="Vietnam" pageId="5" pageNumber="532">Vietnam</collectingCountry>
, 
<collectingCountry id="F314760CFFE0206FFA978A05FA16FDAE" box="[1333,1436,538,563]" name="Malaysia" pageId="5" pageNumber="532">Malaysia</collectingCountry>
