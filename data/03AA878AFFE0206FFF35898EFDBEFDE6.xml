<taxonomicName id="4C034D1FFFE0206FFBAB8A05FBCFFDAE" box="[1033,1093,538,563]" class="Insecta" family="Tetrigidae" genus="Scelimena" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="546" phylum="Arthropoda" rank="species" species="india">
