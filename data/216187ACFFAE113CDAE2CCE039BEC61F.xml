<taxonomicName id="6EC84D39FFAE113CD963CF883E4AC61F" authorityName="Laporte" authorityYear="1833" box="[790,890,762,788]" class="Insecta" family="Lampyridae" genus="Photinus" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="81" phylum="Arthropoda" rank="genus">
