<taxonomicName id="4C264D1DFF98FFF0DF59FA1BFA31260F" authorityName="Schweigger" authorityYear="1812" box="[1280,1444,332,357]" class="Reptilia" family="Emydidae" genus="Emys" kingdom="Animalia" order="Testudines" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="cayennensis">
<emphasis id="B952EA8CFF98FFF0DF59FA1BFA31260F" box="[1280,1444,332,357]" italics="true" pageId="14" pageNumber="15">E. cayennensis</emphasis>
</taxonomicName>
