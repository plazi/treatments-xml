<taxonomicName id="4C264D1DFF98FFF0DE4BFADBFB0C26CF" authorityName="Schweigger" authorityYear="1812" box="[1042,1177,396,421]" class="Reptilia" family="Emydidae" genus="Emys" kingdom="Animalia" order="Testudines" pageId="14" pageNumber="15" phylum="Chordata" rank="species" species="cayennensis">
<emphasis id="B952EA8CFF98FFF0DE4BFADBFB0C26CF" box="[1042,1177,396,421]" italics="true" pageId="14" pageNumber="15">cayennensis</emphasis>
</taxonomicName>
