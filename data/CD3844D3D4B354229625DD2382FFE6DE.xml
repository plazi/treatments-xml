<taxonomicName id="1FC7DD18F080F580AF09A20C2D74F93C" authority="" authorityName="" class="Insecta" family="Ichneumonidae" genus="Henicospilus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Henicospilus maruyamanus" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="maruyamanus">
