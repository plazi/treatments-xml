<subSubSection id="EFFD4C08D7B6E15F1FF3338CCC56E9F6" pageId="0" pageNumber="1" type="chromosome number">
<paragraph id="FB3C7C4B7761A110387F7AC57F92A747" pageId="0" pageNumber="1">Chromosome number.</paragraph>
<paragraph id="3A6FD3F3A1FFF49552AD0D596C1447C3" pageId="0" pageNumber="1">
2n = 24 (
<bibRefCitation id="FA6B3A428792278C6DB950C13F454D26" DOI="https://doi.org/10.1007/BF02907353" author="Williams, D" journalOrPublisher="Economic Botany" pageId="0" pageNumber="1" pagination="387 - 400" refId="B118" refString="Williams, D, 1993. Lycianthes moziniana (Solanaceae): An underutilized Mexican food plant with 'new' crop potential. Economic Botany 47 (4): 387 - 400, DOI: https://doi.org/10.1007/BF02907353" title="Lycianthes moziniana (Solanaceae): An underutilized Mexican food plant with ' new' crop potential." url="https://doi.org/10.1007/BF02907353" volume="47" year="1993">Williams 1993</bibRefCitation>
); 2n = 24, 
<emphasis id="42C08F403CED5A29AE5D1ABBDC9BD369" italics="true" pageId="0" pageNumber="1">Dean 300, 306</emphasis>
