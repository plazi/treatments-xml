<quantity id="4C929B01FFA4FF91FE304A35543BFA83" box="[508,579,1303,1328]" metricMagnitude="2" metricUnit="m" metricValue="1.3699999999999999" pageId="40" pageNumber="41" unit="m" value="137.0">137 m</quantity>
