<taxonomicName id="4C467BAAFFB3FF9FFCEAFA3E699CFA73" baseAuthorityName="Westwood" baseAuthorityYear="1837" box="[835,984,1453,1479]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="luteipennis">
