<location id="8E9956F2FFB2FF9EFC3BFAD56E56FAD4" LSID="urn:lsid:plazi:treatment:03EFB13FFFB2FF9FFFC3FC0B68B3FA5E:8E9956F2FFB2FF9EFC3BFAD56E56FAD4" box="[914,1042,1350,1376]" country="Brazil" municipality="Gonocoxites" name="Thickening" pageId="26" pageNumber="27" stateProvince="Go">Thickening</location>
