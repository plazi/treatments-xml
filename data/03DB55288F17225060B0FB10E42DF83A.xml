<taxonomicName id="4C729FBD8F1722506535F870E26BF81E" authority="Morino &amp; Miyamoto, 1988" authorityName="Morino &amp; Miyamoto" authorityYear="1988" class="Malacostraca" family="Talitridae" genus="Talorchestia" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="353" phylum="Arthropoda" rank="species" species="palawanensis">
<emphasis id="B906382C8F1722506535F870E133F81E" italics="true" pageId="6" pageNumber="353">Talorchestia palawanensis</emphasis>
<bibRefCitation id="EFE399CF8F1722506111F85CE26BF81E" author="Morino" box="[310,616,1976,2000]" pageId="6" pageNumber="353" refString="Morino, H. &amp; Miyamoto, H. (1988) Redefinition of Talorchestia (Amphipoda: Talitridae) with description of a new species from the tropical West Pacific. Journal of Crustacean Biology, 8, 91 - 98. http: // dx. doi. org / 10.2307 / 1548434" type="journal article" year="1988">
Morino &amp; Miyamoto, 
<number id="3DBAF6198F1722506217F85CE26BF81E" box="[560,616,1976,2000]" pageId="6" pageNumber="353" value="1988.0">1988</number>
</bibRefCitation>
</taxonomicName>
with 
<taxonomicName id="4C729FBD8F172250628FF85DE406F81E" authority="Haswell, 1880" authorityName="Haswell" authorityYear="1880" box="[680,1029,1976,2000]" class="Malacostraca" family="Talitridae" genus="Talorchestia" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="353" phylum="Arthropoda" rank="species" species="terraereginae">
<emphasis id="B906382C8F172250628FF85DE35FF81E" box="[680,860,1977,2000]" italics="true" pageId="6" pageNumber="353">T. terraereginae</emphasis>
<bibRefCitation id="EFE399CF8F1722506344F85CE406F81E" author="Haswell" box="[867,1029,1976,2000]" pageId="6" pageNumber="353" refString="Haswell, W. A. (1880) On some new amphipods from Australia and Tasmania. Proceedings of the Linnean Society of New South Wales, 5, 97 - 105, pls 5 - 7." type="journal article" year="1880">
Haswell, 
<number id="3DBAF6198F17225063EAF85CE406F81E" box="[973,1029,1976,2000]" pageId="6" pageNumber="353" value="1880.0">1880</number>
</bibRefCitation>
</taxonomicName>
and two species, 
<taxonomicName id="4C729FBD8F17225064ECF85DE16CF83A" authority="Lowry &amp; Springthorpe, 2009" authorityName="Lowry &amp; Springthorpe" authorityYear="2009" class="Malacostraca" family="Talitridae" genus="Talorchestia" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="353" phylum="Arthropoda" rank="species" species="brucei">
<emphasis id="B906382C8F17225064ECF85DE52DF81E" box="[1227,1326,1976,2000]" italics="true" pageId="6" pageNumber="353">T. brucei</emphasis>
<bibRefCitation id="EFE399CF8F1722506513F85DE16CF83A" author="Lowry" pageId="6" pageNumber="353" refString="Lowry, J. K. &amp; Springthorpe, R. T. (2009) Talorchestia brucei sp. nov. (Amphipoda, Talitridae), the first talitrid from the Northern Territory, Australia. Crustaceana, 82 (7), 897 - 912. http: // dx. doi. org / 10.1163 / 156854009 X 427423" type="journal article" year="2009">
Lowry &amp; Springthorpe, 
<number id="3DBAF6198F1722506110F838E16CF83A" box="[311,367,2012,2036]" pageId="6" pageNumber="353" value="2009.0">2009</number>
</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="4C729FBD8F1722506182F839E33BF83A" authority="Othman &amp; Azman, 2007" authorityName="Othman &amp; Azman" authorityYear="2007" box="[421,824,2012,2036]" class="Malacostraca" family="Talitridae" genus="Talorchestia" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="353" phylum="Arthropoda" rank="species" species="morinoi">
<emphasis id="B906382C8F1722506182F839E219F83A" box="[421,538,2013,2036]" italics="true" pageId="6" pageNumber="353">T. morinoi</emphasis>
<bibRefCitation id="EFE399CF8F1722506206F838E33BF83A" author="Othman" box="[545,824,2012,2036]" pageId="6" pageNumber="353" refString="Othman, B. H. R. &amp; Azman, B. A. R. (2007) A new species of Talitridae (Amphipoda: Gammaridea) from Tioman Island, Malaysia. Zootaxa, 1454, 59 - 68." type="journal article" year="2007">
Othman &amp; Azman, 
<number id="3DBAF6198F17225062DAF838E33BF83A" box="[765,824,2012,2036]" pageId="6" pageNumber="353" value="2007.0">2007</number>
</bibRefCitation>
</taxonomicName>
, have been described.
</paragraph>
