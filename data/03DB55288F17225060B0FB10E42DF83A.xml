<taxonomicName id="4C729FBD8F1722506238F9EDE37CF9D1" authority="Haswell, 1880" authorityName="Haswell" authorityYear="1880" box="[543,895,1543,1568]" class="Malacostraca" family="Talitridae" genus="Talorchestia" kingdom="Animalia" order="Amphipoda" pageId="6" pageNumber="353" phylum="Arthropoda" rank="species" species="terraereginae">
