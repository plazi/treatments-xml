<taxonomicName id="B4A30D2F8DBCA7CE39DA1CE689FE45B7" LSID="http://zoobank.org/E941914F-A912-4E89-9C78-C125CC234D4D" authority="Peng &amp; L. - Z. Li" class="Insecta" family="Staphylinidae" genus="Deinopteroloma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Deinopteroloma songi" order="Coleoptera" pageId="2" pageNumber="57" phylum="Arthropoda" rank="species" species="songi">Deinopteroloma songi Peng &amp; L.-Z. Li</taxonomicName>
