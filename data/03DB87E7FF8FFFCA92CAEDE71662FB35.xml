10. 
<taxonomicName id="4C724D72FF8FFFCA92F0EDE7148AFC68" authority="(Linnaeus 1753: 753) Bentham (1837: 56)" authorityName="Bentham" authorityPageNumber="56" authorityYear="1837" baseAuthorityName="Linnaeus" baseAuthorityPageNumber="753" baseAuthorityYear="1753" box="[178,951,914,941]" class="Magnoliopsida" family="Fabaceae" genus="Centrosema" kingdom="Plantae" order="Fabales" pageId="9" pageNumber="254" phylum="Tracheophyta" rank="species" species="virginianum">
