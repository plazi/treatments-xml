to 
<collectingCountry id="7F9D765B06710969FE4EFECCFDF4FE80" box="[488,552,295,320]" name="Chile" pageId="100" pageNumber="101">Chile</collectingCountry>
, intertidal to 
<specimenCount id="118CFD4206710969FD1AFEC3FD20FEFF" box="[700,764,296,320]" pageId="100" pageNumber="101" type="generic">
