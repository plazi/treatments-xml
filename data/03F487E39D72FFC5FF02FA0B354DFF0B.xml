<collectionCode id="ED4CAE309D72FFC5FDE5F94E335EF966" box="[591,613,1696,1719]" country="Norway" lsid="urn:lsid:biocol.org:col:13093" name="Botanical Museum - University of Oslo" pageId="14" pageNumber="665" type="Herbarium">O</collectionCode>
