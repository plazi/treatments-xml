<taxonomicName id="6ACB4D39FF822710FAB0F895E05E8402" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" box="[463,623,1798,1822]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="challaensis">
<emphasis id="9FBFEAA8FF822710FAB0F895E05E8402" box="[463,623,1798,1822]" italics="true" pageId="71" pageNumber="72">L. challaensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FF822710F9FEF895E0EF8402" authorityName="Watts &amp; Humphreys" authorityYear="2006" box="[641,734,1798,1822]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="exilis">
<emphasis id="9FBFEAA8FF822710F9FEF895E0EF8402" box="[641,734,1798,1822]" italics="true" pageId="71" pageNumber="72">L. exilis</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FF822710F98FF895E18D8402" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2001" box="[752,956,1798,1822]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="fridaywellensis">
<emphasis id="9FBFEAA8FF822710F98FF895E18D8402" box="[752,956,1798,1822]" italics="true" pageId="71" pageNumber="72">L. fridaywellensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FF822710F8B1F895E6B18402" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2004" box="[974,1152,1798,1822]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="hillviewensis">
<emphasis id="9FBFEAA8FF822710F8B1F895E6B18402" box="[974,1152,1798,1822]" italics="true" pageId="71" pageNumber="72">L. hillviewensis</emphasis>
</taxonomicName>
, 
<taxonomicName id="6ACB4D39FF822710FFECF895E73A8402" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2000" box="[1171,1291,1798,1822]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="hinkleri">
<emphasis id="9FBFEAA8FF822710FFECF895E73A8402" box="[1171,1291,1798,1822]" italics="true" pageId="71" pageNumber="72">L. hinkleri</emphasis>
</taxonomicName>
