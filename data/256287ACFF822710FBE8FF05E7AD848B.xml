<taxonomicName id="6ACB4D39FF822710FE61F895E7438402" authorityName="Watts &amp; Humphreys" authorityYear="2006" box="[1310,1394,1798,1822]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="leysi">
