from 
<collectingCountry id="73B019C3070CFFC71FDF244B6E5BC27A" box="[216,296,264,290]" name="France" pageId="26" pageNumber="27">France</collectingCountry>
(
<bibRefCitation id="6F3624A2070CFFC71E30244B6ED2C27A" author="Yu" box="[311,417,264,290]" pageId="26" pageNumber="37" refString="Yu, Y. - X. (2008) Notes of a new species of Dasyhelea and new record of Stilobezzia (Diptera: Ceratopogonidae). Sichuan Journal of Zoology, 27, 165 - 166." type="journal article" year="2008">
