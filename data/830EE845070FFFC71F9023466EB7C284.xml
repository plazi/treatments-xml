<taxonomicName id="CCA722D0070CFFC71EFC25A16D32C3A4" box="[507,577,226,252]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="major">major</taxonomicName>
differences between 
<taxonomicName id="CCA722D0070CFFC71C4525A16CBDC3A3" box="[834,974,226,252]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="turficola">
