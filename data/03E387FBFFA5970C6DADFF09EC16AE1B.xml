<emphasis id="B93EEAFFFFA5970C6DADFF63EC7FAB10" box="[151,292,253,276]" italics="true" pageId="41" pageNumber="42">Trypanaresta</emphasis>
Hering 1940
</taxonomicName>
(
<typeStatus id="54F1884FFFA5970C6C84FF63ECB6AB10" box="[446,493,253,276]" pageId="41" pageNumber="42">type</typeStatus>
species 
<taxonomicName id="4C4A4D6EFFA5970C6F72FF63EE38AB10" authority="Hering" authorityName="Hering" authorityYear="1938" box="[584,867,253,276]" class="Insecta" family="Tephritidae" genus="Trypanea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="imitatrix">
