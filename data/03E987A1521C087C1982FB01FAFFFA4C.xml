<taxonomicName id="4C404D34521F087C1982FC52FEA7FC44" box="[151,284,929,953]" class="Hexapoda" family="Figitidae" genus="Penteucoila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="genus">
