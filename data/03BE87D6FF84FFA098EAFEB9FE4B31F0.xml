<quantity id="4CEF9B25FF84FFA09976FE29FB2631F0" box="[267,359,475,501]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="21" pageNumber="22" unit="m" value="2000.0">2,000 m</quantity>
