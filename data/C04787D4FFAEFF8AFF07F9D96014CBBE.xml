<taxonomicName id="8FEE4D41FFA1FF8AFB18FF796192CBBF" authorityName="Guglielmone &amp; Petney &amp; Robbins" authorityYear="2020" box="[1160,1233,222,248]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="72" pageNumber="73" phylum="Arthropoda" rank="genus">
