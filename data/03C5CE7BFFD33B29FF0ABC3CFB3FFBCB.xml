<number id="3DA46D4AFFD33B29FE8ABE20FE9FFCB4" box="[279,341,837,863]" pageId="10" pageNumber="39" value="1870.0">1870</number>
</date>
] at &quot;Monterico&quot; [= Monterrico, 
<collectingCountry id="F37B3FFDFFD33B29FD51BE20FCC2FCB4" box="[716,776,837,863]" name="Peru" pageId="10" pageNumber="39">Peru</collectingCountry>
