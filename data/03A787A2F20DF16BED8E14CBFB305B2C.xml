<collectionCode id="ED1FAE71F20DF16BEDE712DCFC9B59B6" box="[701,794,1200,1221]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="57" pageNumber="57">AMNH</collectionCode>
