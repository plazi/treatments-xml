<collectingCountry id="E1949A8AFFD3FF9862C436AF585D978E" box="[693,783,644,665]" name="Australia" pageId="12" pageNumber="237">Australia</collectingCountry>
, S. 
<collectingCountry id="E1949A8AFFD3FF98634336AF58DE978E" box="[818,908,644,665]" name="Australia" pageId="12" pageNumber="237">Australia</collectingCountry>
