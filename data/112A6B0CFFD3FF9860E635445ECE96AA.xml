<taxonomicName id="5E83A199FFD3FF9864CE36625EC79749" authority="Carter" authorityName="Carter" box="[1215,1429,585,606]" class="Insecta" family="Buprestidae" genus="Neobuprestis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="237" phylum="Arthropoda" rank="species" species="albosparsa">
