</emphasis>
<taxonomicName id="5CDAC2E9FFD5FFA9C75D96A822AEF393" authority="Busk, 1856" authorityName="Busk" authorityYear="1856" box="[361,686,471,495]" class="Gymnolaemata" genus="Lepralia" kingdom="Animalia" order="Cheilostomatida" pageId="15" pageNumber="16" phylum="Bryozoa" rank="species" species="ringens">
