<taxonomicName id="4C23219DEF375C46AC646A610FEB911E" authorityName="Latreille" authorityYear="1802" box="[852,998,258,286]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="91" phylum="Arthropoda" rank="family">CARABIDAE</taxonomicName>
