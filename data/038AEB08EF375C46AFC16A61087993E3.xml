<emphasis id="B957860CEF375C46AFC16A610D91911E" bold="true" box="[241,412,258,286]" pageId="0" pageNumber="91">SYNONYMY IN</emphasis>
<taxonomicName id="4C23219DEF375C46AE966A610E89911E" authority="HABU" authorityName="HABU" box="[422,644,258,286]" class="Insecta" family="Carabidae" genus="Sugimotoa" kingdom="Animalia" order="Coleoptera" pageId="0" pageNumber="91" phylum="Arthropoda" rank="genus">
<emphasis id="B957860CEF375C46AE966A610E3C911E" bold="true" box="[422,561,258,286]" italics="true" pageId="0" pageNumber="91">SUGIMOTOA</emphasis>
<emphasis id="B957860CEF375C46AD0B6A610E89911E" bold="true" box="[571,644,258,286]" pageId="0" pageNumber="91">HABU</emphasis>
