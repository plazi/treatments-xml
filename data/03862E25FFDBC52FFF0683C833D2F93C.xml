<collectingMunicipality id="6BF40549FFDBC52FFC3381BC336FFC50" pageId="2" pageNumber="3">Institute of Vertebrate Paleontology</collectingMunicipality>
and Paleoanthropology, 
<location id="8EF0C9E8FFDBC52FFB8581A2338FFC32" LSID="urn:lsid:plazi:treatment:03862E25FFDBC52FFF0683C833D2F93C:8EF0C9E8FFDBC52FFB8581A2338FFC32" municipality="Institute of Vertebrate Paleontology" name="Chinese Academy of Sciences" pageId="2" pageNumber="3" stateProvince="Beijing">Chinese Academy of Sciences</location>
, 
<collectingRegion id="49EB51D1FFDBC52FFC488180342EFC32" box="[959,1042,956,980]" country="China" name="Beijing" pageId="2" pageNumber="3">Beijing</collectingRegion>
</materialsCitation>
