<taxonomicName id="4C2FE4B0FFDBC52FFB9380F234B8FD00" authorityName="Bi &amp; Meng &amp; Wu" authorityYear="2008" box="[1124,1156,718,742]" class="Mammalia" family="Muridae" genus="Megacricetodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="2" pageNumber="3" phylum="Chordata" rank="species" species="yei">
