<quantity id="4C889B385E2EFFDA1B3B546BFEDDFC2E" box="[316,381,880,905]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="22" pageNumber="177" unit="mm" value="1.0">1 mm</quantity>
