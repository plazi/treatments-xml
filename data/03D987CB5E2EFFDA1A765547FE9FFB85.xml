<quantity id="4C889B385E2EFFDA1B57544AFE05FCCD" box="[336,421,849,874]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="22" pageNumber="177" unit="mm" value="3.5">3.5 mm</quantity>
