<quantity id="4C889B385E2EFFDA1AF755EEFEF0FCA9" box="[240,336,757,782]" metricMagnitude="-1" metricUnit="m" metricValue="1.35" pageId="22" pageNumber="177" unit="mm" value="135.0">135 mm</quantity>
