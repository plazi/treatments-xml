<quantity id="4C889B385E2EFFDA1BE35429FD86FCEC" box="[484,550,818,843]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="22" pageNumber="177" unit="mm" value="1.0">1 mm</quantity>
