<quantity id="4C889B385E2EFFDA1AAB540FFF4EFC8A" box="[172,238,788,813]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" pageId="22" pageNumber="177" unit="mm" value="8.0">8 mm</quantity>
