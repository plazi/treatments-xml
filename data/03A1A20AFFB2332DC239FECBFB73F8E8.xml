<subSubSection id="C3124097FFB2332DC209FE56FB14FDC9" box="[312,1227,507,534]" pageId="7" pageNumber="8" type="etymology">
<paragraph id="8BB7131CFFB2332DC209FE56FB14FDC9" blockId="7.[264,1324,427,1254]" box="[312,1227,507,534]" pageId="7" pageNumber="8">
<emphasis id="B97CCF0EFFB2332DC209FE56FE1BFDCA" bold="true" box="[312,452,507,533]" pageId="7" pageNumber="8">Etymology.</emphasis>
