<typeStatus id="54A7BF8B6B57FFFA1776FE24AE39FE5D" box="[809,863,449,470]" pageId="10" pageNumber="333">Type</typeStatus>
species: 
<taxonomicName id="4C1C7AAA6B57FFFA178CFE25A865FE5D" authority="Kise &amp; Montenegro &amp; Santos &amp; Hoeksema &amp; Ekins &amp; Ise &amp; Higashiji &amp; Fernandez-Silva &amp; Reimer, 2022" authorityName="Kise &amp; Montenegro &amp; Santos &amp; Hoeksema &amp; Ekins &amp; Ise &amp; Higashiji &amp; Fernandez-Silva &amp; Reimer" authorityYear="2022" box="[979,1283,448,470]" class="Anthozoa" family="Parazoanthidae" genus="Churabana" kingdom="Animalia" order="Zoantharia" pageId="10" pageNumber="333" phylum="Cnidaria" rank="species" species="kuroshioae" status="sp. nov.">
