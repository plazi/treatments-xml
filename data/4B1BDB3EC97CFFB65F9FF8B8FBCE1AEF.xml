<taxonomicName id="04B211ABC97DFFB65D44FF3AFBCA1AEF" authority="Boulenger, 1894" authorityName="Boulenger" authorityYear="1894" box="[588,1059,197,221]" class="Actinopterygii" family="Balitoridae" genus="Homaloptera" kingdom="Animalia" order="Cypriniformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="formosana">
