<taxonomicName id="04B211ABC97DFFB65D6FFF61FBDF1A84" authority="Boulenger 1894" authorityName="Boulenger" authorityYear="1894" box="[615,1078,158,182]" class="Actinopterygii" family="Balitoridae" genus="Homaloptera" kingdom="Animalia" order="Cypriniformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="formosana">
