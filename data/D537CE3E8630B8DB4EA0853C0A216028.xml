<taxonomicName id="85F01B82FD23A76E2BA8F48302D9E0EE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:150198" class="Insecta" family="Formicidae" genus="Leptothorax" kingdom="Animalia" lsidName-HNS="Leptothorax nevadensis rudis Wheeler" order="Hymenoptera" pageNumber="19" phylum="Arthropoda" rank="subSpecies" species="nevadensis" subSpecies="rudis">Leptothorax nevadensis subsp. rudis Wheeler</taxonomicName>
1917a: 508. 
<materialsCitation id="74E824D610C675508CB9DDD9EBD2F3C8" ID-GBIF-Occurrence="923893279" collectionCode="MCZC" country="U. S. A." location="Camp Curry, Yosemite" stateProvince="California" typeStatus="syntype">
Six 
<typeStatus id="632E2244B36C99B2971CBB5D9A563DC9">syntype</typeStatus>
workers, 
<location id="F894D7C76E715C4F9B30B79914BBD30A" LSID="urn:lsid:plazi:treatment:D537CE3E8630B8DB4EA0853C0A216028:F894D7C76E715C4F9B30B79914BBD30A" name="Camp Curry, Yosemite" stateProvince="California">Camp Curry, Yosemite</location>
, 
