<paragraph id="8B8E369BFF81FF83C7D18512FDA6FEF6" blockId="3.[151,542,290,350]" box="[151,542,290,316]" pageId="3" pageNumber="60">
<heading id="D0C681F7FF81FF83C7D18512FDA6FEF6" bold="true" box="[151,542,290,316]" fontSize="11" level="1" pageId="3" pageNumber="60" reason="1">
<emphasis id="B945EA89FF81FF83C7D18512FDA6FEF6" bold="true" box="[151,542,290,316]" pageId="3" pageNumber="60">
Genus 
<taxonomicName id="4C314D18FF81FF83C7A88512FE7DFEF6" box="[238,453,290,316]" class="Hydrozoa" family="Plumulariidae" genus="Schizoplumularia" kingdom="Animalia" order="Leptothecata" pageId="3" pageNumber="60" phylum="Cnidaria" rank="genus" status="n. gen.">
<emphasis id="B945EA89FF81FF83C7A88512FE7DFEF6" bold="true" box="[238,453,290,316]" italics="true" pageId="3" pageNumber="60">Schizoplumularia</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27657F2FF81FF83C68A8512FDA6FEF6" box="[460,542,290,316]" pageId="3" pageNumber="60" rank="genus">n. gen.</taxonomicNameLabel>
