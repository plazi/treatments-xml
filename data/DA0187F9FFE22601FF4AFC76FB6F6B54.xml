(Subgenus 
<taxonomicName id="95A84D6CFFE22604FCF6FC76FC7969F7" box="[820,904,949,975]" class="Liliopsida" family="Zingiberaceae" genus="Globba" kingdom="Plantae" order="Zingiberales" pageId="3" pageNumber="142" phylum="Tracheophyta" rank="genus">
