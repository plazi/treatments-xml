<taxonomicName id="D6944D7AFFC0FF99D4C7FBF6FC9D2867" box="[692,836,1047,1074]" class="Insecta" family="Cicindelidae" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="127" phylum="Arthropoda" rank="family">Cicindelidae</taxonomicName>
