<quantity id="4CBA83D1E561FFD65D1DFA4BFD62169F" box="[558,651,1436,1458]" metricMagnitude="-2" metricUnit="m" metricValue="2.45" pageId="0" pageNumber="327" unit="mm" value="24.5">24,5mm</quantity>
