<quantity id="4CBA83D1E561FFD65C73FA4BFC72169F" box="[832,923,1436,1458]" metricMagnitude="-2" metricUnit="m" metricValue="1.17" pageId="0" pageNumber="327" unit="mm" value="11.7">11,7mm</quantity>
