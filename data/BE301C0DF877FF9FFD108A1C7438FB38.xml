, 
<collectingCountry id="4E8EED8BF870FF9FFEEC88D9709AFE5C" box="[358,469,434,460]" name="Malaysia" pageId="20" pageNumber="21">Malaysia</collectingCountry>
, 
<collectingCountry id="4E8EED8BF870FF9FFE6B88D97327FE5C" box="[481,616,434,460]" name="Philippines" pageId="20" pageNumber="21">Philippines</collectingCountry>
