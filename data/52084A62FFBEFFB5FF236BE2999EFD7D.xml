<taxonomicName id="1DA180F7FFBEFFB5FE10688F9F3CFD51" authority="Berlese" authorityName="Berlese" box="[420,808,685,710]" class="Arachnida" family="Pachylaelapidae" genus="Pachylaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="brachiosus">
