<quantity id="1DCB7EF7B7B8FB6AFA62859B47874AE7" metricMagnitude="-2" metricUnit="m" metricValue="3.95" metricValueMax="4.0" metricValueMin="3.9" pageId="0" pageNumber="65" unit="mm" value="39.5" valueMax="40.0" valueMin="39.0">39-40 mm</quantity>
lang ♂, 
<quantity id="E6EAE03A4720A621A69EC910EE387932" metricMagnitude="-3" metricUnit="m" metricValue="3.5" pageId="0" pageNumber="65" unit="mm" value="3.0">3,5 mm</quantity>
