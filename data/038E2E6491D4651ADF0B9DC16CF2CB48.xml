<taxonomicName id="0E4B56FA4784B3881E404822AF35BE5C" authority="Latzel" authorityName="Latzel" class="Chilopoda" family="Lithobiidae" genus="Lithobius" higherTaxonomySource="CoL" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="65" phylum="Arthropoda" rank="subGenus" subGenus="Polybothrus">
