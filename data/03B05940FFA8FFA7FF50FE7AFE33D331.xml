<location id="8EC6BE8DFFA8FFA7FE08FCFAFCA6D40E" LSID="urn:lsid:plazi:treatment:03B05940FFA8FFA7FF50FE7AFE33D331:8EC6BE8DFFA8FFA7FE08FCFAFCA6D40E" box="[463,874,823,848]" country="China" county="Pingbian Miao Autonomous County" municipality="Honghe Hani" name="Daweishan National Nature Reserve" pageId="4" pageNumber="252" stateProvince="Yunnan">Daweishan National Nature Reserve</location>
, stream, 
<collectingDate id="EFE3377EFFA8FFA7FC09FCFAFBF7D411" box="[974,1083,823,848]" pageId="4" pageNumber="252" value="2017-06-04">
