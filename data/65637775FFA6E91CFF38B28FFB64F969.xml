; HW: 
<quantity id="2A326B86FFA6E91BFD67B39AFCA2FBFF" box="[712,822,386,412]" metricMagnitude="-4" metricUnit="m" metricValue="6.8999999999999995" pageId="3" pageNumber="32" unit="mm" value="0.69">0.69 mm</quantity>
