<taxonomicName id="9DBBECB12A50D856FEF27DC31D74B2C7" authority="Chaudoir." authorityName="Chaudoir Lectotypus" authorityYear="1872" box="[347,631,1228,1250]" class="Insecta" family="Carabidae" genus="Calleida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="lativittis">
<emphasis id="68CF4B202A50D856FEF27DC31D0FB2C7" bold="true" box="[347,524,1228,1250]" italics="true" pageId="17" pageNumber="18">Calleida lativittis</emphasis>
Chaudoir.
</taxonomicName>
<emphasis id="68CF4B202A50D856FDD47DC31D95B2C7" bold="true" box="[637,662,1228,1250]" pageId="17" pageNumber="18">30</emphasis>
habitus and labels of lectotype (female, &quot;Deccan&quot;, MNHN), scale bar = 2.0 mm; 
<emphasis id="68CF4B202A50D856FF627DE41FE6B324" bold="true" box="[203,229,1259,1281]" pageId="17" pageNumber="18">31</emphasis>
