</taxonomicName>
(
<taxonomicName box="[1144,1239,1426,1447]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="382" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
); 
<collectingCountry box="[1255,1312,1426,1447]" name="China" pageId="7" pageNumber="382">China</collectingCountry>
......................................................................................... 
<taxonomicName box="[1173,1339,1455,1476]" class="Insecta" family="Aphididae" genus="Hybothoracaphis" kingdom="Animalia" order="Aphidomorpha" pageId="7" pageNumber="382" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis box="[1173,1339,1455,1476]" italics="true" pageId="7" pageNumber="382">Hybothoracaphis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1346,1435,1455,1477]" pageId="7" pageNumber="382" rank="genus">
<emphasis bold="true" box="[1346,1435,1455,1477]" pageId="7" pageNumber="382">gen. nov.</emphasis>
</taxonomicNameLabel>
</keyLead>
</paragraph>
<paragraph blockId="7.[151,1436,1397,2034]" pageId="7" pageNumber="382">
<keyLead pageId="7" pageNumber="382">- Antennae 3- or 4-segmented; body without a conspicuous median dorsal ridge; central axis of dorsal prosoma without pustule clusters.............................................................................................. 2</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="7" pageNumber="382">
<paragraph blockId="7.[151,1436,1397,2034]" pageId="7" pageNumber="382">
<keyLead pageId="7" pageNumber="382">
