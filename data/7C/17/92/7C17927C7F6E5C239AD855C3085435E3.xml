<taxonomicName class="Magnoliopsida" family="Musaceae" genus="Musa" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Musa" order="Zingiberales" pageId="0" pageNumber="29" phylum="Angiospermae" rank="genus">
