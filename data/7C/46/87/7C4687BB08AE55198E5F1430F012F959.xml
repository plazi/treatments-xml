<paragraph pageId="0" pageNumber="31">
<taxonomicName LSID="7C4687BB-08AE-5519-8E5F-1430F012F959" authority="Perez, 1895" authorityName="Pérez" authorityYear="1895" class="Insecta" family="Andrenidae" genus="Andrena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Andrena (Cryptandrena) rotundata" order="Hymenoptera" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="rotundata" subGenus="Cryptandrena">
