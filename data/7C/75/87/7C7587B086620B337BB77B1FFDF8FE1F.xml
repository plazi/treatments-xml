<collectingCountry box="[739,809,1770,1796]" name="Japan" pageId="9" pageNumber="27">Japan</collectingCountry>
, 
<collectingCountry box="[826,979,1770,1796]" name="South Korea" pageId="9" pageNumber="27">South Korea</collectingCountry>
and the 
<collectingCountry box="[1097,1263,1770,1796]" name="United States of America" pageId="9" pageNumber="27">United States</collectingCountry>
