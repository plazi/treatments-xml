<taxonomicName baseAuthorityName="Banks" baseAuthorityYear="1946" class="Hexapoda" family="Pompilidae" genus="Chirodamus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chirodamus argentinicus" order="Hymenoptera" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="argentinicus">
<emphasis italics="true" pageId="3" pageNumber="92">Chirodamus argentinicus</emphasis>
</taxonomicName>
