<taxonomicName baseAuthorityName="Banks" baseAuthorityYear="1946" class="Hexapoda" family="Pompilidae" genus="Aimatocare" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aimatocare argentinica" order="Hymenoptera" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="argentinica">
<emphasis italics="true" pageId="3" pageNumber="92">Aimatocare argentinica</emphasis>
</taxonomicName>
(Banks), 
<emphasis bold="true" pageId="3" pageNumber="92">comb. n</emphasis>
.; 
<taxonomicName baseAuthorityName="Evans" baseAuthorityYear="1968" class="Hexapoda" family="Pompilidae" genus="Aimatocare" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aimatocare imitator" order="Hymenoptera" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="imitator">
<emphasis italics="true" pageId="3" pageNumber="92">Aimatocare imitator</emphasis>
</taxonomicName>
(Evans), 
<emphasis bold="true" pageId="3" pageNumber="92">comb. n</emphasis>
.; 
<taxonomicName baseAuthorityName="Banks" baseAuthorityYear="1946" class="Hexapoda" family="Pompilidae" genus="Aimatocare" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Aimatocare longula" order="Hymenoptera" pageId="3" pageNumber="92" phylum="Arthropoda" rank="species" species="longula">
<emphasis italics="true" pageId="3" pageNumber="92">Aimatocare longula</emphasis>
</taxonomicName>
