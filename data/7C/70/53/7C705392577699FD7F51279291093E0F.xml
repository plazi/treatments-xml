<taxonomicName authority="Mayr" authorityName="Mayr" class="Hexapoda" family="Formicidae" genus="Bothroponera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="105" phylum="Arthropoda" rank="genus">P. wasmannii-group</taxonomicName>
