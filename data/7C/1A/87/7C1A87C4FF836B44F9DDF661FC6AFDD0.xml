<subSubSection pageId="35" pageNumber="68" type="etymology">
<paragraph blockId="35.[1280,2263,2642,2727]" pageId="35" pageNumber="68">Etymology. Named for the presence of eyes, unique among the family.</paragraph>
</subSubSection>
<subSubSection box="[1280,2034,2793,2832]" pageId="35" pageNumber="68" type="type_taxon">
<paragraph blockId="35.[1280,2034,2793,2832]" box="[1280,2034,2793,2832]" pageId="35" pageNumber="68">
<emphasis bold="true" box="[1280,1367,2793,2832]" pageId="35" pageNumber="68">
<typeStatus box="[1280,1367,2793,2832]" pageId="35" pageNumber="68">Type</typeStatus>
</emphasis>
species. 
<taxonomicName authority="Bradbury &amp; Williams, 1996" authorityName="Bradbury &amp; Williams" authorityYear="1996" box="[1552,1931,2793,2832]" class="Malacostraca" family="Bogidiellidae" genus="Bogidomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="35" pageNumber="68" phylum="Arthropoda" rank="species" species="australis" status="n.sp.">
<emphasis box="[1552,1931,2793,2832]" italics="true" pageId="35" pageNumber="68">Bogidomma australis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1958,2034,2793,2832]" pageId="35" pageNumber="68" rank="species">n.sp.</taxonomicNameLabel>
