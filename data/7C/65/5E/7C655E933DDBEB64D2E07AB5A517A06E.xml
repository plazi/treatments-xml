, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="101.21661">101°12.997'E</geoCoordinate>
, elevation ca 744 m, 1.-15.06.2007 primary tropical seasonal rain forest; 
<geoCoordinate direction="north" orientation="latitude" precision="1" value="21.9238">21°55.428'N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="101.27402">101°16.441'E</geoCoordinate>
, elevation ca 598 m, 16.-31.07.2007, secondary tropical seasonal rain forest; 1♀, 
<geoCoordinate direction="north" orientation="latitude" precision="1" value="21.910767">21°54.646'N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="101.27095">101°16.257'E</geoCoordinate>
