´
<number id="501D9E07FFE9A0719CA6FCDDFB20FC95" box="[1179,1233,805,829]" pageId="4" pageNumber="67" value="42.9">42.9</number>
´´N
</geoCoordinate>
/ 
<geoCoordinate id="83E1EAE7FFE9A0719CC2FCDDFA69FC95" box="[1279,1432,805,829]" direction="west" orientation="longitude" pageId="4" pageNumber="67" precision="1" value="-2.6865833">
