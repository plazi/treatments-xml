<quantity id="5A261FAD80D2FF71EEA6CDFD1D7F0D90" box="[973,1058,980,1004]" metricMagnitude="1" metricUnit="m" metricValue="1.0" metricValueMax="2.0" metricValueMin="0.0" pageId="214" pageNumber="215" unit="m" value="10.0" valueMax="20.0" valueMin="0.0">0–20 m</quantity>
