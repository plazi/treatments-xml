<taxonomicName id="5ADEC9CB80D2FF71ED95CDFD18340D97" authorityName="Oken, 1815-" authorityYear="1815" box="[254,361,980,1003]" class="Anthozoa" family="Acroporidae" genus="Acropora" kingdom="Animalia" order="Scleractinia" pageId="214" pageNumber="215" phylum="Cnidaria" rank="genus">
