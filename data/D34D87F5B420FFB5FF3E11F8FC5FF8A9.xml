<taxonomicName id="9CE44D60B420FFB5FF1D11F8FEB0FA57" box="[180,500,1477,1503]" class="Insecta" family="Staphylinidae" genus="Batriscenellus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="47" phylum="Arthropoda" rank="species" species="transformis">
