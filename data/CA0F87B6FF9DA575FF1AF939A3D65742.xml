<quantity id="855E9B45FF9CA575FA8EFF43A30C5457" box="[1283,1350,297,319]" metricMagnitude="-2" metricUnit="m" metricValue="6.223" pageId="7" pageNumber="440" unit="in" value="2.45">2.45 in</quantity>
