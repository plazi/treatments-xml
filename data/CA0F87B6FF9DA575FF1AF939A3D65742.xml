<taxonomicName id="85A64D23FF9CA575FBB4FFD7A3D654BA" authority="Heiss &amp; Banar, 2012" authorityName="Heiss &amp; Banar" authorityYear="2012" box="[1081,1436,444,466]" class="Insecta" family="Aradidae" genus="Comorocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="440" phylum="Arthropoda" rank="species" species="inexpectatus">
