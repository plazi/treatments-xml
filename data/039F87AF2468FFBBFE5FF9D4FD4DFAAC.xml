<paragraph id="8B8936B92456FFBBFF0AFBC0FE48FB1A" blockId="33.[188,1398,1145,1248]" pageId="33" pageNumber="34">
<collectingCountry id="F32176292456FFBBFF0AFBC0FEC5FB69" box="[189,293,1145,1171]" name="Vietnam" pageId="33" pageNumber="34">Vietnam</collectingCountry>
