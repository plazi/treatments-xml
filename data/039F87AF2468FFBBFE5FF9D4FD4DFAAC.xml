<collectingCountry id="F32176292456FFBBFF0BFB19FF0EFB40" box="[188,238,1184,1210]" name="Iran" pageId="33" pageNumber="34">Iran</collectingCountry>
, 
<collectingCountry id="F32176292456FFBBFF4EFB26FEB1FB40" box="[249,337,1183,1210]" name="Turkey" pageId="33" pageNumber="34">Turkey</collectingCountry>
), through South Asia (
<collectingCountry id="F32176292456FFBBFDEFFB26FD0CFB43" box="[600,748,1183,1209]" name="Afghanistan" pageId="33" pageNumber="34">Afghanistan</collectingCountry>
, 
<collectingCountry id="F32176292456FFBBFD4FFB26FC8EFB43" box="[760,878,1183,1209]" name="Sri Lanka" pageId="33" pageNumber="34">Sri Lanka</collectingCountry>
, 
