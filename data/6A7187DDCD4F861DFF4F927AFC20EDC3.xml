</bibRefCitation>
: 
<taxonomicName id="25D84D48CD4F861DFD34942EFC9BEC73" authorityName="Lugo-Ortiz &amp; McCafferty" authorityYear="1996" box="[748,874,1072,1096]" class="Insecta" family="Baetidae" genus="Crassabwa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
