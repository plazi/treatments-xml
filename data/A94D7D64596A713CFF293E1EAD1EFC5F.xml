<taxonomicName id="E6E4B7F1596A713CFEEA3FC5A8FEFC5F" box="[340,430,996,1020]" class="Maxillopoda" family="Cyclopidae" genus="Cyclops" kingdom="Animalia" order="Cyclopoida" pageId="31" pageNumber="332" phylum="Arthropoda" rank="genus">
