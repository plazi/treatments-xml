<taxonomicName id="4C580784FF83E559FB725198FA69E9FB" authority="Pop, 1938" authorityName="Pop" authorityYear="1938" box="[1178,1436,1715,1739]" class="Clitellata" family="Lumbricidae" genus="Dendrobaena" kingdom="Animalia" order="Crassiclitellata" pageId="4" pageNumber="151" phylum="Annelida" rank="species" species="clujensis">
