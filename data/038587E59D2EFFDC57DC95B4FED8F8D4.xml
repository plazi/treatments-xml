<subSubSection id="C33665789D2EFFDC578C938BFC4DF944" box="[199,923,1701,1728]" pageId="53" pageNumber="54" type="etymology">
<paragraph id="8B9336F39D2EFFDC578C938BFC4DF944" blockId="53.[151,1437,261,1872]" box="[199,923,1701,1728]" pageId="53" pageNumber="54">
<emphasis id="B958EAE19D2EFFDC578C938BFE98F944" bold="true" box="[199,334,1702,1728]" pageId="53" pageNumber="54">Etymology.</emphasis>
