<taxonomicName id="4C6640C10046FFD69442F9FBFB151549" box="[1067,1235,1638,1663]" class="Magnoliopsida" family="Passifloraceae" genus="Passiflora" kingdom="Plantae" order="Malpighiales" pageId="9" pageNumber="10" phylum="Tracheophyta" rank="species" species="rhamnifolia">P. rhamnifolia</taxonomicName>
