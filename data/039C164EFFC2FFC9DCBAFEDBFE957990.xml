<taxonomicName id="4C35DCDBFFC2FFCEDCBAFEDBFE0D79FF" box="[136,407,972,995]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="scalptus" status="new species">
<emphasis id="B9417B4AFFC2FFCEDCBAFEDBFE0D79FF" bold="true" box="[136,407,972,995]" italics="true" pageId="67" pageNumber="68">Paramblynotus scalptus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFC2FFCEDD9AFED9FDB579FF" box="[424,559,973,995]" pageId="67" pageNumber="68" rank="species">new species</taxonomicNameLabel>
