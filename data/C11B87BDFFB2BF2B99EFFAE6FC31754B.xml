<taxonomicName id="8EB24D28FFB2BF2B9C0AFD5CFA2974AC" baseAuthorityName="E. Geoffroy" baseAuthorityYear="1813" box="[1281,1421,702,728]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="16" pageNumber="428" phylum="Chordata" rank="species" species="diadema">
<emphasis id="7BC6EAB9FFB2BF2B9C0AFD5CFA2974AC" box="[1281,1421,702,728]" italics="true" pageId="16" pageNumber="428">H. diadema</emphasis>
</taxonomicName>
