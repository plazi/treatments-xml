<taxonomicName id="8EB24D28FFB2BF2B9A3BFCE1FC1D7569" baseAuthorityName="E. Geoffroy" baseAuthorityYear="1813" box="[816,953,771,797]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="16" pageNumber="428" phylum="Chordata" rank="species" species="diadema">
<emphasis id="7BC6EAB9FFB2BF2B9A3BFCE1FC1D7569" box="[816,953,771,797]" italics="true" pageId="16" pageNumber="428">H. diadema</emphasis>
</taxonomicName>
