material
</emphasis>
(
<specimenCount id="989B3990421E256434EBD0CFFDB4FF3A" box="[393,531,152,178]" pageId="7" pageNumber="40" type="generic">
<number id="3855E03E421E256434EBD0CFFE31FF3A" box="[393,406,152,178]" pageId="7" pageNumber="40" value="1.0">1</number>
