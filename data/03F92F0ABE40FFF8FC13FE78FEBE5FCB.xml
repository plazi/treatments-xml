<taxonomicName id="4C50E59FBE40FFF9FBC8FDD7FB225F21" authorityName="Euzet" authorityYear="1959" baseAuthorityName="Molin" baseAuthorityYear="1858" box="[1115,1257,590,610]" class="Cestoda" family="Phyllobothriidae" genus="Crossobothrium" kingdom="Animalia" order="Phyllobothriidea" pageId="14" pageNumber="1350" phylum="Platyhelminthes" rank="species" species="longicolle">
