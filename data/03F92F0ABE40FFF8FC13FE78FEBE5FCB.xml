<taxonomicName id="4C50E59FBE40FFF9FC47FDB5FBE65F20" authorityName="Blainville" authorityYear="1828" class="Cestoda" family="Tetrabothriidae" genus="Tetrabothrium" kingdom="Animalia" order="Tetrabothriidea" pageId="14" pageNumber="1350" phylum="Platyhelminthes" rank="species" species="lewini">
