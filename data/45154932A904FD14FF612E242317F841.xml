<number id="7B74EA03A907FD14FBC729D12257FF48" box="[1073,1109,152,178]" pageId="15" pageNumber="43" value="4.0" valueMax="7.0" valueMin="1.0">1-7</number>
- 
<number id="7B74EA03A907FD14FBA929D122A5FF48" box="[1119,1191,152,178]" pageId="15" pageNumber="43" value="10.5" valueMax="11.0" valueMin="10.0">10-11</number>
</date>
