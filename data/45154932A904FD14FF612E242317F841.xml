<emphasis id="FFC82436A906FD15FCB52E8C229EF824" box="[835,1180,1989,2014]" italics="true" pageId="14" pageNumber="42">
<taxonomicName id="0ABC83A7A906FD15FCB52E8C25E4F824" box="[835,998,1989,2014]" class="Insecta" family="Curculionidae" genus="Kyklioacalles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="42" phylum="Arthropoda" rank="genus">Kyklioacalles</taxonomicName>
