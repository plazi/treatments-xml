<taxonomicName id="4C00A6FAFFA5FFAF480EFA3086D8FA35" authorityName="Burckhardt &amp; Sharma &amp; Raman" authorityYear="2018" baseAuthorityName="Kandasamy" baseAuthorityYear="1986" box="[772,1003,1484,1509]" class="Insecta" family="Psyllidae" genus="Psylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="ternstroemiae">
