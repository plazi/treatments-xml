<taxonomicName id="A2844D1031128B3F9FDDFB1BFBEFFB6A" authorityName="Engelmann" authorityYear="1987" box="[915,1120,1227,1251]" class="Mammalia" family="Cyclopedidae" genus="Pseudoglyptodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pilosa" pageId="2" pageNumber="3" phylum="Chordata" rank="genus">
<emphasis id="57F0EA8131128B3F9FDDFB1BFBEFFB6A" box="[915,1120,1227,1251]" italics="true" pageId="2" pageNumber="3">PSEUDOGLYPTODON</emphasis>
</taxonomicName>
: 
<taxonomicName id="A2844D1031128B3F98CCFB1BFB9DFA88" authority="Engelmann, 1987: 217" authorityName="Engelmann" authorityPageNumber="217" authorityYear="1987" class="Mammalia" family="Cyclopedidae" genus="Pseudoglyptodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pilosa" pageId="2" pageNumber="3" phylum="Chordata" rank="species" species="sallaensis">
<emphasis id="57F0EA8131128B3F98CCFB1BFC89FA89" italics="true" pageId="2" pageNumber="3">P. sallaensis</emphasis>
<bibRefCitation id="01154B6231128B3F9F41FB38FB9DFA88" author="Engelmann, G. F." box="[783,1042,1256,1280]" pageId="2" pageNumber="3" pagination="217 - 223" refId="ref10821" refString="Engelmann, G. F. 1987. A new Deseadan sloth (Mammalia: Xenarthra) from Salla, Bolivia, and its implications for the primitive condition of the dentition in edentates. Journal of Vertebrate Paleontology 7 (2): 217 - 223, figs. 1, 2." type="book chapter" year="1987">Engelmann, 1987: 217</bibRefCitation>
</taxonomicName>
. 
<typeStatus id="BA3F883131128B3F9850FB38FB06FA88" box="[1054,1161,1256,1280]" pageId="2" pageNumber="3">Holotype</typeStatus>
of 
<taxonomicName id="A2844D1031128B3F98FCFB39FCD4FA95" authority=", PU" authorityName="PU" class="Mammalia" family="Cyclopedidae" genus="Pseudoglyptodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pilosa" pageId="2" pageNumber="3" phylum="Chordata" rank="species" species="sallaensis">
<emphasis id="57F0EA8131128B3F98FCFB39FCAAFA96" italics="true" pageId="2" pageNumber="3">P. sallaensis</emphasis>
, PU
</taxonomicName>
20552, collected from Branisa’s locality V-12, lower Salla beds, Salla, 
<collectingCountry id="1D93760331128B3F983DFAF3FB47FAB3" box="[1139,1224,1315,1339]" name="Bolivia" pageId="2" pageNumber="3">Bolivia</collectingCountry>
