<taxonomicName id="D6D15DF66560854ED0C9215DFCED9BF6" baseAuthorityName="Moullade" baseAuthorityYear="1974" box="[505,789,1140,1166]" class="Globothalamea" family="Hedbergellidae" genus="Microhedbergella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Rotaliida" pageId="53" pageNumber="54" phylum="Foraminifera" rank="species" species="rischi">
