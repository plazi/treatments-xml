</taxonomicName>
(≡ 
<taxonomicName authorityName="Hook fil" authorityYear="1871" class="Magnoliopsida" family="Melastomataceae" genus="Dissotis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dissotis barteri" order="Myrtales" pageId="2" pageNumber="91" phylum="Tracheophyta" rank="species" species="barteri">
