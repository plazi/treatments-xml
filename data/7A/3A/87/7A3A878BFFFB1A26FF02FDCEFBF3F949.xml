<collectionCode box="[140,183,799,825]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14240" lsid="urn:lsid:biocol.org:col:14240" name="Tullie House Museum" pageId="13" pageNumber="38" type="Herbarium">CLÉ</collectionCode>
