<number box="[908,967,362,387]" pageId="22" pageNumber="152" value="1958.1965">1958</number>
</bibRefCitation>
, 
<bibRefCitation author="Beier" box="[983,1043,362,387]" pageId="22" pageNumber="152" refString="Beier, M. (1965) Die Mantodeen Neu-Guineas. Pacific Insects, 7 (3), 473 - 502." type="journal article" year="1965">1965</bibRefCitation>
