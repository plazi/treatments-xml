<taxonomicName authority="Giglio-Tos, 1907" authorityName="Giglio-Tos" authorityYear="1907" class="Insecta" family="Mantidae" genus="Tenodera" kingdom="Animalia" order="Mantodea" pageId="22" pageNumber="152" phylum="Arthropoda" rank="species" species="bokiana">
<emphasis italics="true" pageId="22" pageNumber="152">T. bokiana</emphasis>
