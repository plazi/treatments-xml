<taxonomicName authority="Jager, 2012" authorityName="Jager" authorityYear="2012" box="[988,1251,325,348]" class="Arachnida" family="Ctenidae" genus="Ctenus" kingdom="Animalia" order="Araneae" pageId="1" pageNumber="92" phylum="Arthropoda" rank="species" species="cladarus">
