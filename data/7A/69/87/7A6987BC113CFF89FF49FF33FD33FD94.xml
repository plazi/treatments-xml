<subSubSection pageId="18" pageNumber="263" type="biology_ecology">
<paragraph blockId="18.[140,1108,166,1672]" pageId="18" pageNumber="263">
<emphasis bold="true" box="[185,283,1539,1565]" pageId="18" pageNumber="263">
<emphasis box="[185,278,1539,1565]" inLineHeading="true" pageId="18" pageNumber="263" reason="1">Biology</emphasis>
.
</emphasis>
