<subSubSection lastPageId="4" lastPageNumber="223" pageId="3" pageNumber="222" type="description">
<paragraph blockId="3.[151,1436,1626,2011]" box="[199,1267,1662,1687]" pageId="3" pageNumber="222">
<emphasis bold="true" box="[199,453,1662,1687]" pageId="3" pageNumber="222">Measurements (mm).</emphasis>
