<paragraph pageId="2" pageNumber="282" type="mainText">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2414" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus Mayr" order="Hymenoptera" pageId="2" pageNumber="282" phylum="Arthropoda" rank="genus">Camponotus</taxonomicName>
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:193289" class="Insecta" family="Formicidae" genus="Camponotus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Camponotus Cressoni Andr?©" order="Hymenoptera" pageId="2" pageNumber="282" phylum="Arthropoda" rank="species" species="Cressoni">Cressoni</taxonomicName>
<taxonomicNameLabel pageId="2" pageNumber="282" rank="species">nov. sp.</taxonomicNameLabel>
