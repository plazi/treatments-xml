<subSubSection lastPageId="11" lastPageNumber="12" pageId="8" pageNumber="9" type="description">
<paragraph blockId="8.[869,1134,538,563]" box="[869,1134,538,563]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[869,1012,538,563]" pageId="8" pageNumber="9">Body width:</emphasis>
