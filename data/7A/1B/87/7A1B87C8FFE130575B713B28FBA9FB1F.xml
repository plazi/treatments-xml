<taxonomicName box="[363,607,2074,2099]" class="Insecta" family="Dynastidae" genus="Podischnus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="undetermined">
<emphasis bold="true" box="[363,452,2074,2099]" italics="true" pageId="8" pageNumber="9">Podischnus</emphasis>
Burmeister species
</taxonomicName>
in ventral view.(A) 
<taxonomicName authority="(Olivier)" baseAuthorityName="Olivier" box="[765,908,2074,2099]" class="Insecta" family="Scarabaeidae" genus="Scarabaeus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="agenor">
<emphasis bold="true" box="[765,836,2074,2099]" italics="true" pageId="8" pageNumber="9">P.agenor</emphasis>
(Olivier)
</taxonomicName>
.(B) 
<taxonomicName authority="(Taschenberg)" baseAuthorityName="Taschenberg" baseAuthorityYear="1870" box="[942,1175,2074,2099]" class="Insecta" family="Dynastidae" genus="Podischnus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="sexdentatus">
<emphasis bold="true" box="[942,1052,2074,2099]" italics="true" pageId="8" pageNumber="9">P.sexdentatus</emphasis>
(Taschenberg)
</taxonomicName>
.(C) 
<taxonomicName authority="Sternberg." authorityName="Sternberg." box="[1207,1387,2074,2099]" class="Insecta" family="Dynastidae" genus="Podischnus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="oberthuri">
<emphasis bold="true" box="[1207,1297,2074,2099]" italics="true" pageId="8" pageNumber="9">P.oberthuri</emphasis>
Sternberg.
