Alert; 
<geoCoordinate box="[370,500,1726,1751]" degrees="82" direction="north" minutes="28" orientation="latitude" pageId="33" pageNumber="214" precision="15" seconds="04" value="82.46778">82°28′04″N</geoCoordinate>
, 
<geoCoordinate box="[509,655,1726,1751]" degrees="062" direction="west" minutes="51" orientation="longitude" pageId="33" pageNumber="214" precision="15" seconds="28" value="-62.857777">062°51′28″W</geoCoordinate>
; 
<quantity box="[663,715,1727,1751]" metricMagnitude="1" metricUnit="m" metricValue="3.1" pageId="33" pageNumber="214" unit="m" value="31.0">31 m</quantity>
a.s.l.; 
<date box="[167,285,1761,1785]" pageId="33" pageNumber="214" value="2019-07-09">9 Jul. 2019</date>
; habitat: xeric in a slope, with till and rocks as substrates, dominated by 
