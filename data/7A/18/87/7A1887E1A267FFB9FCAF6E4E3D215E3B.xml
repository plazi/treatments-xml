, 
<taxonomicName box="[658,771,161,184]" class="Magnoliopsida" family="Brassicaceae" genus="Draba" kingdom="Plantae" order="Brassicales" pageId="12" pageNumber="193" phylum="Tracheophyta" rank="species" species="cinerea">
<emphasis box="[658,771,161,184]" italics="true" pageId="12" pageNumber="193">D. cinerea</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[214,372,194,218]" class="Magnoliopsida" family="Brassicaceae" genus="Draba" kingdom="Plantae" order="Brassicales" pageId="12" pageNumber="193" phylum="Tracheophyta" rank="species" species="subcapitata">
