and 
<taxonomicName class="Magnoliopsida" family="Leguminosae" genus="Caesalpinia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Caesalpinia vesicaria" order="Fabales" pageId="33" pageNumber="34" phylum="Tracheophyta" rank="species" species="vesicaria">
