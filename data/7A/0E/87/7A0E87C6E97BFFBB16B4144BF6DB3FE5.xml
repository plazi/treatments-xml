<emphasis box="[1195,1357,1839,1861]" italics="true" pageId="13" pageNumber="360">
A. 
<taxonomicName box="[1229,1357,1839,1861]" pageId="13" pageNumber="360" rank="species" species="diminuatana">diminuatana</taxonomicName>
</emphasis>
</taxonomicName>
(Ohio, TMG412). 85, 
<taxonomicName box="[304,407,1869,1891]" class="Insecta" family="Tortricidae" genus="Ancylis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="360" phylum="Arthropoda" rank="species" species="saliana">
<emphasis box="[304,407,1869,1891]" italics="true" pageId="13" pageNumber="360">
A. 
<taxonomicName box="[332,407,1869,1891]" pageId="13" pageNumber="360" rank="species" species="saliana">saliana</taxonomicName>
</emphasis>
</taxonomicName>
(Florida, TMG629). 86, 
<taxonomicName box="[660,808,1869,1891]" class="Insecta" family="Tortricidae" genus="Ancylis" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="360" phylum="Arthropoda" rank="species" species="subarcuana">
<emphasis box="[660,808,1869,1891]" italics="true" pageId="13" pageNumber="360">A. subarcuana</emphasis>
