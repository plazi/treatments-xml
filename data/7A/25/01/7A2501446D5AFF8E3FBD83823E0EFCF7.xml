<subSubSection pageId="8" pageNumber="415" type="biology_ecology">
<paragraph blockId="8.[151,1436,732,867]" pageId="8" pageNumber="415">
<emphasis bold="true" box="[199,295,732,758]" pageId="8" pageNumber="415">Biology.</emphasis>
