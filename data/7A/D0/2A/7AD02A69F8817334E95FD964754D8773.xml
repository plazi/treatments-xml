<taxonomicName authority="Guérin, 1837" authorityName="Guérin" authorityYear="1837" class="Insecta" family="Tenebrionidae" genus="Calognathus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Calognathus" order="Coleoptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="genus">Calognathus</taxonomicName>
