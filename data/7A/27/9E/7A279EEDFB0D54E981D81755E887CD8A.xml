<emphasis italics="true" pageId="7" pageNumber="30">Eriophorum angustifolium</emphasis>
</taxonomicName>
Honck., 
<taxonomicName class="Liliopsida" family="Juncaginaceae" genus="Eriophorum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Eriophorum triste" order="Poales" pageId="7" pageNumber="30" phylum="Tracheophyta" rank="species" species="triste">
