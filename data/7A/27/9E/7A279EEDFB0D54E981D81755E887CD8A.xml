Boott, 
<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex fuliginosa subsp. misandra" order="Poales" pageId="7" pageNumber="30" phylum="Tracheophyta" rank="subSpecies" species="fuliginosa" subSpecies="misandra">Carex fuliginosa subsp. misandra</taxonomicName>
(R. Br.) Nyman, 
<taxonomicName class="Liliopsida" family="Cyperaceae" genus="Carex" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Carex scirpoidea" order="Poales" pageId="7" pageNumber="30" phylum="Tracheophyta" rank="species" species="scirpoidea">
