<taxonomicName authority="" box="[273,526,1787,1814]" class="Insecta" family="Lycidae" genus="Lyropaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="439" phylum="Arthropoda" rank="subGenus" subGenus="Lyroneces">
