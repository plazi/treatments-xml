<emphasis bold="true" box="[199,315,470,496]" pageId="3" pageNumber="440">Remarks.</emphasis>
<taxonomicName authorityName="Kazantsev" authorityYear="1998" box="[322,668,470,496]" class="Insecta" family="Lycidae" genus="Lyropaeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="440" phylum="Arthropoda" rank="species" species="orcinus" subGenus="Lyroneces">
