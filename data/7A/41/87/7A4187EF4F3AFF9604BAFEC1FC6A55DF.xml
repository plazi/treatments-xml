<taxonomicName authority="(Cumberlidge, 1999)" authorityName="Cumberlidge &amp; Daniels" authorityYear="2022" baseAuthorityName="Cumberlidge" baseAuthorityYear="1999" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="species" species="reidi" status="comb. nov.">
<emphasis box="[450,761,1550,1572]" italics="true" pageId="23" pageNumber="1291">Longipotamonautes reidi</emphasis>
(Cumberlidge, 1999)
</taxonomicName>
<taxonomicNameLabel box="[394,517,1581,1603]" pageId="23" pageNumber="1291" rank="species">comb. nov.</taxonomicNameLabel>
, 
<taxonomicName authority="(Balss, 1936)" authorityName="Cumberlidge &amp; Daniels" authorityYear="2022" baseAuthorityName="Balss" baseAuthorityYear="1936" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="species" species="schubotzi" status="comb.nov.">
<emphasis italics="true" pageId="23" pageNumber="1291">Longipotamonautes schubotzi</emphasis>
(
<bibRefCitation author="Balss H" box="[258,378,1612,1634]" pageId="23" pageNumber="1291" pagination="165 - 204" refId="ref29128" refString="Balss H. 1936. Beitrage zur Kenntnis der Potamonidae (Susswasserkrabben) des Kongogebietes. Revue Zoologique et Botanique d'Afrique 28: 165 - 204." type="journal article" year="1936">Balss,1936</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel box="[389,497,1612,1634]" pageId="23" pageNumber="1291" rank="species">comb.nov.</taxonomicNameLabel>
and 
<taxonomicName authority="(Bott, 1955)" authorityName="Cumberlidge &amp; Daniels" authorityYear="2022" baseAuthorityName="Bott" baseAuthorityYear="1955" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="species" species="semilunaris" status="comb. nov.">
<emphasis italics="true" pageId="23" pageNumber="1291">Longipotamonautes semilunaris</emphasis>
(
<bibRefCitation author="Bott R" box="[290,403,1642,1664]" pageId="23" pageNumber="1291" pagination="209 - 352" refId="ref29250" refString="Bott R. 1955. Die Susswasserkrabben von Africa und ihre Stammesgeschichte. Annales du Musee Royal du Congo Belge serie 3: 209 - 352." type="journal article" year="1955">Bott, 1955</bibRefCitation>
)
</taxonomicName>
<taxonomicNameLabel box="[417,531,1642,1664]" pageId="23" pageNumber="1291" rank="species">comb. nov.</taxonomicNameLabel>
</paragraph>
</subSubSection>
<subSubSection pageId="23" pageNumber="1291" type="distribution">
<paragraph blockId="23.[145,762,1705,1911]" pageId="23" pageNumber="1291">
<emphasis box="[145,291,1705,1726]" italics="true" pageId="23" pageNumber="1291">Distribution:</emphasis>
<taxonomicName box="[309,625,1705,1727]" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="variety" species="acristatus" variety="adentatus">
<emphasis box="[309,460,1706,1727]" italics="true" pageId="23" pageNumber="1291">Lo. acristatus</emphasis>
, 
<emphasis box="[471,500,1706,1727]" italics="true" pageId="23" pageNumber="1291">Lo</emphasis>
. 
<emphasis box="[512,625,1705,1726]" italics="true" pageId="23" pageNumber="1291">adentatus</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Cumberlidge &amp; Daniels" authorityYear="2022" baseAuthorityName="A. Milne-Edwards" baseAuthorityYear="1886" box="[636,755,1705,1727]" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="species" species="ballayi">
<emphasis box="[636,665,1706,1727]" italics="true" pageId="23" pageNumber="1291">Lo</emphasis>
. 
<emphasis box="[676,755,1705,1726]" italics="true" pageId="23" pageNumber="1291">ballayi</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Cumberlidge &amp; Daniels" authorityYear="2022" baseAuthorityName="A. Milne-Edwards" baseAuthorityYear="1886" box="[145,272,1736,1757]" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="species" species="paecilei">
<emphasis box="[145,174,1736,1757]" italics="true" pageId="23" pageNumber="1291">Lo</emphasis>
. 
<emphasis box="[186,272,1736,1757]" italics="true" pageId="23" pageNumber="1291">paecilei</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Cumberlidge &amp; Daniels" authorityYear="2022" baseAuthorityName="Bott" baseAuthorityYear="1955" box="[328,482,1736,1757]" class="Malacostraca" family="Potamonautidae" genus="Longipotamonautes" kingdom="Animalia" order="Decapoda" pageId="23" pageNumber="1291" phylum="Arthropoda" rank="species" species="punctatus">
