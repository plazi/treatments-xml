<taxonomicName id="63B14D28FFE8FFF27FEA8C68FAC2FEB3" authority="GRUVEL, 1905" authorityName="GRUVEL" authorityYear="1905" box="[1020,1305,345,370]" class="Maxillopoda" family="Lauridae" kingdom="Animalia" order="Laurida" pageId="34" pageNumber="823" phylum="Arthropoda" rank="family">LAURIDAE GRUVEL, 1905</taxonomicName>
</heading>
