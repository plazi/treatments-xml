<number id="3DAB24DE70628945FBF4FCF84C5C6A54" box="[764,774,844,866]" pageId="64" pageNumber="65" value="1.0">1</number>
f, figs 
<number id="3DAB24DE70628945FA42FCF84C246A54" box="[842,894,844,866]" pageId="64" pageNumber="65" value="3.11">3, 11</number>
, 
<number id="3DAB24DE70628945FA81FCF84C926A54" box="[905,968,844,866]" pageId="64" pageNumber="65" value="56.0" valueMax="65.0" valueMin="47.0">47–65</number>
