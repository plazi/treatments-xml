<subSubSection id="C31B92E8FFD310198005116893AA12DE" pageId="5" pageNumber="6" type="distribution">
<paragraph id="8BBEC163FFD310198005116893AA12DE" blockId="5.[264,1323,1087,1193]" pageId="5" pageNumber="6">
<emphasis id="B9751D71FFD3101980051168968E122E" bold="true" box="[312,472,1087,1113]" pageId="5" pageNumber="6">Distribution.</emphasis>
Gulf of 
<collectingRegion id="49C50F81FFD310198303116895F6122E" box="[574,672,1087,1113]" country="Panama" name="Chiriqui" pageId="5" pageNumber="6">Chiriqui</collectingRegion>
