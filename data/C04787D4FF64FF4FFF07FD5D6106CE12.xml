<taxonomicName id="8FEE4D41FF64FF4FFF57FD5C6602C852" authority="Marx, 1892 in Curtice (1892)" authorityName="Marx, 1892 in Curtice" authorityYear="1892" box="[199,833,762,789]" class="Arachnida" family="Ixodidae" genus="Dermacentor" kingdom="Animalia" order="Ixodida" pageId="141" pageNumber="142" phylum="Arthropoda" rank="species" species="occidentalis">
