<taxonomicName id="51FDE5B8FFEC80670887FA2EFE42FA0C" authority="Shi &amp; Xu, 2016" authorityName="Shi &amp; Xu" authorityYear="2016" box="[151,446,1490,1516]" class="Enoplea" family="Thoracostomopsidae" genus="Epacanthion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Enoplida" pageId="9" pageNumber="566" phylum="Nematoda" rank="species" species="sparsisetae" status="sp. nov.">
