<emphasis id="C1EAEAC3FF9D0B261880FD293C7BFD69" bold="true" box="[151,267,655,680]" pageId="3" pageNumber="284">Remarks.</emphasis>
<taxonomicName id="349E4D52FF9D0B26190FFD363F3FFD69" box="[280,591,656,680]" class="Insecta" family="Perlidae" genus="Largusoperla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="284" phylum="Arthropoda" rank="species" species="charliewattsi">
