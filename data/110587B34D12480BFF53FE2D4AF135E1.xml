<collectingCountry id="E1BB76354D124808FEA3FA294ACC30E0" box="[359,445,1502,1528]" name="Mexico" pageId="25" pageNumber="26">Mexico</collectingCountry>
(
<bibRefCitation id="FD3D4B544D124808FE08FA2949EE30E1" author="Ugalde, D. &amp; Gomez, P. &amp; Simoes, N." box="[460,671,1502,1529]" pageId="25" pageNumber="26" pagination="151 - 183" refId="ref82484" refString="Ugalde, D., Gomez, P. &amp; Simoes, N. (2015) Marine sponges (Porifera: Demospongiae) from the Gulf of Mexico, new records and redescription of Erylus trisphaerus (de Laubenfels, 1953). Zootaxa, 3911 (2), 151 - 183. https: // doi. org / 10.11646 / zootaxa. 3911.2.1" type="journal article" year="2015">
Ugalde 
<emphasis id="ABD8EAB74D124808FDE1FA28492C30E0" box="[549,605,1502,1528]" italics="true" pageId="25" pageNumber="26">et al.</emphasis>
2015
</bibRefCitation>
, current records), 
<taxonomicName id="5EAC4D264D124808FCAEFA294F0E30E0" authority="(Rutzler 1974)" baseAuthorityName="Rutzler" baseAuthorityYear="1974" box="[874,1151,1502,1528]" class="Demospongiae" family="Chondropsidae" genus="Strongylacidon" kingdom="Animalia" order="Poecilosclerida" pageId="25" pageNumber="26" phylum="Porifera" rank="species" species="bermuda">
Bermuda (
<bibRefCitation id="FD3D4B544D124808FC1BFA294F0630E0" author="Rutzler, K." box="[991,1143,1502,1528]" pageId="25" pageNumber="26" pagination="32" refId="ref80398" refString="Rutzler, K. (1974) The Burrowing Sponges of Bermuda. Smithsonian Contributions to Zoology, 165, 32. https: // doi. org / 10.5479 / si. 00810282.165" type="journal article" year="1974">Rützler 1974</bibRefCitation>
