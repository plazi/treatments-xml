<taxonomicName id="4C2C3BA69D52FFA2C818FB51FD3F7BC3" authority="(Kom.) Kitag., Neolineamenta Florae Manshuricae" authorityName="Kitag., Neolineamenta Florae Manshuricae" baseAuthorityName="Kom." class="Magnoliopsida" family="Betulaceae" form="brevituba" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="29" pageNumber="30" phylum="Tracheophyta" rank="form" species="mandshurica">
