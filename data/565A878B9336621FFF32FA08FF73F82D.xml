<taxonomicName id="19F34D1E9336621FFE02F8F0FD7CF801" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" box="[423,757,1916,1941]" class="Insecta" family="Heleomyzidae" genus="Blepharoptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
