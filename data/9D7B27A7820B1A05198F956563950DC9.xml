<taxonomicName id="31DBDC19AADA73E635C13113B3376FD0" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33735" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole fallax Mayr" lsidName-HNS="Pheidole fallax Mayr" order="Hymenoptera" pageNumber="262" phylum="Arthropoda" rank="species" species="fallax">fallax</taxonomicName>
