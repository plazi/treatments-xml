<taxonomicName id="4C41D1E7DC07FFA00FF9FA5CFCC66EA2" authorityName="Sullivan" authorityYear="2010" baseAuthorityName="Guenee" baseAuthorityYear="1852" box="[662,771,1469,1496]" class="Insecta" family="Noctuidae" genus="Neadysgonia" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="89" phylum="Arthropoda" rank="species" species="similis">
