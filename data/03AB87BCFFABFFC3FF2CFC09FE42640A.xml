<taxonomicName id="4C024D29FFABFFC0FF2CFC09FD9061EB" authority="Kuhlmann" authorityName="Kuhlmann" box="[151,522,977,1003]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="90" phylum="Arthropoda" rank="species" species="tibestensis" status="sp. nov.">
<emphasis id="B976EAB8FFABFFC0FF2CFC09FEE061EB" bold="true" box="[151,378,977,1003]" italics="true" pageId="9" pageNumber="90">Colletes tibestensis</emphasis>
