<taxonomicName id="4C7E4D3E6C1AFFFAFB64CD3EAE64BE33" box="[1169,1316,1581,1606]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="28" pageNumber="29" phylum="Cnidaria" rank="species" species="diaphana">
