<taxonomicName id="4C7E4D3E6C1AFFFAFF4CCD3EAA0BBE33" box="[185,331,1581,1606]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="28" pageNumber="29" phylum="Cnidaria" rank="species" species="diaphana">
