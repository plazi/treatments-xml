<taxonomicName id="4C547EADFFEDFFDDCC59FA98FD785D42" authorityName="Niu, Zhu &amp; Kuhlmann" authorityYear="2013" box="[546,708,1359,1385]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="xizangensis">
