<taxonomicName id="4C547EADFFEDFFDDCF68FA98FE1A5D42" authorityName="Niu, Zhu &amp; Kuhlmann" authorityYear="2013" box="[275,422,1359,1385]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="xuezhongi">
