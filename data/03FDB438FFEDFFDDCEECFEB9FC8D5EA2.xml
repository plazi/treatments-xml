<taxonomicName id="4C547EADFFEDFFDDCFE5FAA4FDFC5DA6" authorityName="Niu, Zhu &amp; Kuhlmann" authorityYear="2013" box="[414,576,1395,1421]" class="Insecta" family="Colletidae" genus="Colletes" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="xizangensis">
