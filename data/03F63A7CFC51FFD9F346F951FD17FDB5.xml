<paragraph id="8BE08B6AFC51FFD8F346F951FDF8F958" blockId="16.[340,541,1703,1725]" box="[340,541,1703,1725]" pageId="16" pageNumber="1025">
<heading id="D0A83C06FC51FFD8F346F951FDF8F958" box="[340,541,1703,1725]" fontSize="8" level="2" pageId="16" pageNumber="1025" reason="2">
<emphasis id="B92B5778FC51FFD8F346F951FDF8F958" bold="true" box="[340,541,1703,1725]" pageId="16" pageNumber="1025">
<taxonomicName id="4C5FF0E9FC51FFD8F346F951FE59F958" authorityName="Malicky &amp; Chantaramongkol" authorityYear="2007" box="[340,444,1703,1725]" class="Insecta" family="Hydroptilidae" genus="Huayptila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="16" pageNumber="1025" phylum="Arthropoda" rank="genus" status="nov.gen.">
<emphasis id="B92B5778FC51FFD8F346F951FE59F958" bold="true" box="[340,444,1703,1725]" italics="true" pageId="16" pageNumber="1025">Huayptila</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A218EA03FC51FFD8F3D0F951FDF8F958" box="[450,541,1703,1725]" pageId="16" pageNumber="1025" rank="genus">nov.gen.</taxonomicNameLabel>
