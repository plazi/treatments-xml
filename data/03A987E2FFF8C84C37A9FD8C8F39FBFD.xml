<quantity id="4CF89B11FFF8C84F3421FA6E88D3FA7F" box="[799,972,1413,1439]" metricMagnitude="-4" metricUnit="m" metricValue="6.25" metricValueMax="7.5" metricValueMin="5.0" pageId="16" pageNumber="17" unit="mm" value="0.625" valueMax="0.75" valueMin="0.5">0.50â€“0.75 mm</quantity>
