Now 
<taxonomicName id="0CAE4D55A825FFFAFF29FCF9FF6A0A40" authority="(Wied, 1821)" baseAuthorityName="Wied" baseAuthorityYear="1821" class="Aves" family="Thraupidae" genus="Schistochlamys" kingdom="Animalia" order="Passeriformes" pageId="61" pageNumber="61" phylum="Chordata" rank="subSpecies" species="ruficapillus" subSpecies="capistrata">
