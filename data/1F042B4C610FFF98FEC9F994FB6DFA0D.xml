</emphasis>
<taxonomicName id="50ADE1D9610EFF9BFEDEF9E8FD83F909" authorityName="Hitchcock" authorityYear="1974" box="[338,528,1648,1672]" class="Insecta" family="Leuctridae" genus="Leuctra" kingdom="Animalia" order="Plecoptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="species" species="duplicata">
