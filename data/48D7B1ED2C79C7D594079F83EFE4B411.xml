<taxonomicName id="6A7E10429C56B5A54C0710178F6BD8D3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:141038" authority="Emery" authorityName="Emery" class="Insecta" family="Formicidae" genus="Sifolinia" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Sifolinia laurae Emery" order="Hymenoptera" pageNumber="58" phylum="Arthropoda" rank="species" species="laurae">laurae Emery</taxonomicName>
