</taxonomicName>
, 
<taxonomicName id="4C0518E38E71FECCFC1FFC1643EA3CAB" authorityName="Moenkhaus" authorityYear="1898" box="[945,1020,942,965]" class="Arachnida" family="Pholcidae" genus="Litoporus" kingdom="Animalia" order="Araneae" pageId="293" pageNumber="294" phylum="Arthropoda" rank="species" species="fulvus">
<emphasis id="B971BF728E71FECCFC1FFC1643EA3CAB" box="[945,1020,942,965]" italics="true" pageId="293" pageNumber="294">fulvus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C0518E38E71FECCFBBAFC1644833CAB" authorityName="Keyserling" authorityYear="1891" box="[1044,1173,942,965]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="293" pageNumber="294" phylum="Arthropoda" rank="species" species="imbecillus">
