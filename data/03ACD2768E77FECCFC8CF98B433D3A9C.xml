</taxonomicName>
; 
<taxonomicName id="4C0518E38E71FECCFCBDFBBE438E3B73" authorityName="Mello-Leitao" authorityYear="1922" box="[787,920,1030,1053]" class="Arachnida" family="Pholcidae" genus="Litoporus" kingdom="Animalia" order="Araneae" pageId="293" pageNumber="294" phylum="Arthropoda" rank="species" species="agricola">
