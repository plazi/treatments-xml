. 
<emphasis id="B971BF728E71FECCFD6FFC5043333C91" box="[705,805,1000,1023]" italics="true" pageId="293" pageNumber="294">genitalis</emphasis>
</taxonomicName>
to 
<taxonomicName id="4C0518E38E71FECCFCE3FC5043AA3C91" box="[845,956,1000,1023]" class="Arachnida" family="Pholcidae" genus="Carapoia" kingdom="Animalia" order="Araneae" pageId="293" pageNumber="294" phylum="Arthropoda" rank="genus">
<emphasis id="B971BF728E71FECCFCE3FC5043AA3C91" box="[845,956,1000,1023]" italics="true" pageId="293" pageNumber="294">Carapoia</emphasis>
</taxonomicName>
; 
<taxonomicName id="4C0518E38E71FECCFC60FC5044973C91" box="[974,1153,1000,1023]" class="Arachnida" family="Pholcidae" genus="Litoporus" kingdom="Animalia" order="Araneae" pageId="293" pageNumber="294" phylum="Arthropoda" rank="species" species="iguassuensis">
