<taxonomicName id="4C0518E38E71FECCFB87FC7344B63C8C" authorityName="HUBER" authorityYear="2000" box="[1065,1184,971,994]" class="Arachnida" family="Pholcidae" genus="Chibchea" kingdom="Animalia" order="Araneae" pageId="293" pageNumber="294" phylum="Arthropoda" rank="genus">
