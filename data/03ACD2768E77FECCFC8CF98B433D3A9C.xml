<paragraph id="8BBA63608E77FECAFD6FF9DA432039C2" blockId="291.[705,1233,1634,1708]" pageId="291" pageNumber="292">
<taxonomicName id="4C0518E38E77FECAFD6FF9DA441D3919" authority="Simon, 1893 b: 483" authorityName="Simon" authorityPageNumber="483" authorityYear="1893" box="[705,1035,1634,1655]" class="Arachnida" family="Pholcidae" genus="Litoporus" kingdom="Animalia" order="Araneae" pageId="291" pageNumber="292" phylum="Arthropoda" rank="genus">
