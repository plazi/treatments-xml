<taxonomicName id="4C2F02C9FFDC2A252E554CA5FBC1FF49" box="[886,1064,188,212]" class="Insecta" family="Hybotidae" genus="Hybos" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="172" phylum="Arthropoda" rank="species" species="wudanganus">
