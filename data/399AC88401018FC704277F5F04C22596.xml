<geoCoordinate id="55C47EFFFDC8099720A1848A6184ABB8" direction="north" orientation="latitude" precision="15" value="31.390278">31°23'25&quot;N</geoCoordinate>
, 
<geoCoordinate id="38842746E8DDFE57256F36A737005338" direction="east" orientation="longitude" precision="15" value="121.369446">121°22'10&quot;E</geoCoordinate>
, alt. 5 m, 7.v.2017, Song, Peng, Hu, Wang &amp; Liu leg. / HOLOTYPE [red], 
<taxonomicName id="2BF16732B6A76A014B7076193051FDEF" class="Insecta" family="Carabidae" genus="Eustra" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eustra shanghaiensis" order="Coleoptera" pageId="2" pageNumber="47" phylum="Arthropoda" rank="species" species="shanghaiensis">Eustra shanghaiensis</taxonomicName>
