<geoCoordinate id="FC9850624D024818FBF0FD784FB837B1" box="[1076,1225,655,681]" degrees="91.87944" direction="west" orientation="longitude" pageId="9" pageNumber="10" precision="1" value="-91.87944">91.87944°W</geoCoordinate>
), 
<quantity id="5E549B404D024818FB19FD784E6637B1" box="[1245,1303,655,681]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="10" unit="m" value="20.0">20 m</quantity>
depth, coll. Oscar Bocardo, 
