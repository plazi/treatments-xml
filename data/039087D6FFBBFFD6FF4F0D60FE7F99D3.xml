<taxonomicName id="4C394D43FFBEFFD6FD8A0A5CFD3D993F" box="[594,743,335,359]" class="Insecta" family="Ceratopogonidae" genus="Culicoides" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="177" phylum="Arthropoda" rank="species" species="jacobsoni">
