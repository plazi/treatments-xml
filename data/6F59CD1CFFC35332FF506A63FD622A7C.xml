<taxonomicName id="20F00789FFC35332FBFC692FFB0A2A25" box="[1083,1215,1555,1581]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="409" phylum="Tracheophyta" rank="species" species="undetermined">
