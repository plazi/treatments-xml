</taxonomicName>
(
<taxonomicName id="20F00789FFC35332FF25692EFEE32A24" box="[226,342,1554,1580]" class="Magnoliopsida" family="Moraceae" kingdom="Plantae" order="Rosales" pageId="20" pageNumber="409" phylum="Tracheophyta" rank="family">Moraceae</taxonomicName>
), and of the synonym, 
<taxonomicName id="20F00789FFC35332FD95692FFC8D2A25" authority="Fursov" authorityName="Fursov" box="[594,824,1554,1581]" class="Insecta" family="Bostrichidae" genus="Lyctus" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="409" phylum="Arthropoda" rank="species" species="shestakovi">
<emphasis id="D584A018FFC35332FD95692FFD512A24" box="[594,740,1554,1580]" italics="true" pageId="20" pageNumber="409">L. shestakovi</emphasis>
Fursov
</taxonomicName>
