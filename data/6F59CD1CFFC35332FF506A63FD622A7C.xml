</taxonomicName>
(
<taxonomicName id="20F00789FFC35332FC4E690AFC422A58" authorityName="Dumortier" authorityYear="1829" box="[905,1015,1590,1616]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="409" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
