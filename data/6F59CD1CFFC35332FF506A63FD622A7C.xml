</taxonomicName>
) (
<taxonomicName id="20F00789FFC35332FB11692EFAF72A24" authorityName="Dumortier" authorityYear="1829" box="[1238,1346,1554,1580]" class="Magnoliopsida" family="Fagaceae" kingdom="Plantae" order="Fagales" pageId="20" pageNumber="409" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
