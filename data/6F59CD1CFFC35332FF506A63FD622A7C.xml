<taxonomicName id="20F00789FFC35332FF506966FED32A7C" authority="Mill." authorityName="Mill." box="[151,358,1626,1652]" class="Magnoliopsida" family="Malvaceae" genus="Tilia" kingdom="Plantae" order="Malvales" pageId="20" pageNumber="409" phylum="Tracheophyta" rank="species" species="cordata">
