<taxonomicName id="20F00789FFC35332FA946AD3FF612A24" class="Magnoliopsida" family="Moraceae" genus="Morus" kingdom="Plantae" order="Rosales" pageId="20" pageNumber="409" phylum="Tracheophyta" rank="species" species="nigra">
