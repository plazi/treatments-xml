<quantity id="4CC0D7D20B498675FBF3FD9D09A99348" box="[1047,1182,599,622]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="2.2" metricValueMin="1.3" pageId="17" pageNumber="65" unit="mm" value="1.75" valueMax="2.2" valueMin="1.3">1.3–2.2 mm</quantity>
