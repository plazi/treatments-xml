<date id="D75110769E5AFF92FEC474CFFDC316A7" box="[375,565,212,234]" pageId="94" pageNumber="94" value="1906-01-14">
<collectingDate id="C715E99E9E5AFF92FEC474CFFDC316A7" box="[375,565,212,234]" pageId="94" pageNumber="94" value="1906-01-14">14 January 1906</collectingDate>
</date>
