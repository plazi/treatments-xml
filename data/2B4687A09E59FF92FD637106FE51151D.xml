<collectorName id="0E1A53609E5AFF92FF4A757DFEAF1731" box="[249,345,359,380]" pageId="94" pageNumber="94">Ansorge</collectorName>
and 
<collectorName id="0E1A53609E5AFF92FE12757DFF7C17D7" pageId="94" pageNumber="94">C.H. Pemberton. The</collectorName>
following 
<specimenCount id="B5E9FD3F9E5AFF92FEA6759EFE4417D7" box="[277,434,388,410]" pageId="94" pageNumber="94" type="generic">12 specimens</specimenCount>
are 
<typeStatus id="7C5488149E5AFF92FE46759EFD9117D4" box="[501,615,388,409]" pageId="94" pageNumber="94">paratypes</typeStatus>
: N’gungo, 
<emphasis id="919BEAA49E5AFF92FF6B75BBFE6217FB" bold="true" box="[216,404,417,438]" pageId="94" pageNumber="94">
<specimenCode id="F3499ECD9E5AFF92FF6B75BBFE6217FB" box="[216,404,417,438]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="94" pageNumber="94">AMNH 451749</specimenCode>
</emphasis>
