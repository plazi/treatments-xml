<date id="D75110769E5AFF92FE4A75BBFF60179E" pageId="94" pageNumber="94" value="1901-08-09">
<collectingDate id="C715E99E9E5AFF92FE4A75BBFF60179E" pageId="94" pageNumber="94" value="1901-08-09">9 August 1901</collectingDate>
</date>
, Pemberton
</materialsCitation>
; 
