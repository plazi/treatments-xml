<specimenCount id="9D6B64DA8630FF8DDC6FFB23FD2FFB30" box="[627,726,1153,1178]" pageId="1" pageNumber="386" type="male">
<number id="3DA5BD748630FF8DDC6FFB23FD76FB30" box="[627,655,1154,1178]" pageId="1" pageNumber="386" value="14.0">14</number>
