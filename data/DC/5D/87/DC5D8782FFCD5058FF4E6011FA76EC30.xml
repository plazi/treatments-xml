<quantity box="[486,557,637,659]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" metricValueMax="9.0" metricValueMin="1.0" pageId="10" pageNumber="128" unit="cm" value="5.0" valueMax="9.0" valueMin="1.0">1–9 cm</quantity>
