<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33670" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole clementensis Gregg" lsidName-HNS="Pheidole clementensis Gregg" order="Hymenoptera" pageNumber="572" phylum="Arthropoda" rank="species" species="clementensis">Pheidole clementensis Gregg</taxonomicName>
