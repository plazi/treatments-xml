<subSubSection pageId="0" pageNumber="1" type="reference_group">
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Maas" authorityYear="1986" class="Magnoliopsida" family="Annonaceae" genus="Cremastosperma" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cremastosperma panamense" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="panamense">Cremastosperma panamense</taxonomicName>
