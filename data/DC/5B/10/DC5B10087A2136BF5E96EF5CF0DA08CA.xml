<taxonomicName baseAuthorityName="Dejean" baseAuthorityYear="1826" class="Hexapoda" family="Carabidae" genus="Chlaenius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="613" pageNumber="614" phylum="Arthropoda" rank="species" species="rufipes">
