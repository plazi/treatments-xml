<taxonomicName authority="Tsuneki" authorityName="Tsuneki" authorityYear="1989" box="[450,814,1463,1488]" class="Insecta" family="Pompilidae" genus="Morochares" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="498" phylum="Arthropoda" rank="species" species="nigripennis">
