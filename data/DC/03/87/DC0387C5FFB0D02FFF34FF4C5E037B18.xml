<collectingCountry box="[1117,1246,724,749]" name="Philippines" pageId="1" pageNumber="556">Philippines</collectingCountry>
, 
<collectingCountry box="[1262,1367,724,749]" name="Australia" pageId="1" pageNumber="556">Australia</collectingCountry>
, 
<collectingCountry name="New Zealand" pageId="1" pageNumber="556">New Zealand</collectingCountry>
, 
<collectingCountry box="[258,329,760,784]" name="Guam" pageId="1" pageNumber="556">Guam</collectingCountry>
, 
<collectingCountry box="[343,415,760,784]" name="Tonga" pageId="1" pageNumber="556">Tonga</collectingCountry>
, Hawaiian Islands, the 
<collectingRegion box="[686,807,760,783]" country="French Polynesia" name="Iles Marquises" pageId="1" pageNumber="556">Marquesas</collectingRegion>
and the Austral Islands; 
<quantity box="[1099,1243,759,784]" metricMagnitude="3" metricUnit="m" metricValue="1.0595" metricValueMax="1.519" metricValueMin="0.6" pageId="1" pageNumber="556" unit="m" value="1059.5" valueMax="1519.0" valueMin="600.0">600–1519 m</quantity>
