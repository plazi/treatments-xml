</paragraph>
<paragraph blockId="14.[151,1437,1312,1978]" box="[198,688,1592,1618]" pageId="14" pageNumber="15">
Pereiopods 
<number box="[336,351,1592,1618]" pageId="14" pageNumber="15" value="4.0">4</number>
and 
<number box="[407,422,1592,1618]" pageId="14" pageNumber="15" value="5.0">5</number>
