<taxonomicName authorityName="Bousfield &amp; Hendrycks" authorityYear="1994" box="[794,900,1663,1688]" class="Malacostraca" family="Pleustidae" genus="Eosymtes" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="548" phylum="Arthropoda" rank="genus">
