<taxonomicName authorityName="Xue, Wang, Song &amp; Hong" authorityYear="2009" box="[656,760,827,852]" class="Arachnida" family="Eriophyidae" genus="Neovittacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="shitaicus">
