<subSubSection pageId="58" pageNumber="59" type="etymology">
<paragraph blockId="58.[151,1436,158,894]" pageId="58" pageNumber="59">
<emphasis bold="true" box="[210,350,826,852]" pageId="58" pageNumber="59">Etymology.</emphasis>
