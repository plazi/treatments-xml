<emphasis box="[377,508,1932,1953]" italics="true" pageId="143" pageNumber="144">Gonatocerus</emphasis>
</taxonomicName>
(
<taxonomicName authority="Howard, 1908" authorityName="Howard" authorityYear="1908" box="[525,694,1931,1953]" class="Insecta" family="Mymaridae" genus="Cosmocomoidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="genus">
<emphasis box="[525,694,1931,1953]" italics="true" pageId="143" pageNumber="144">Cosmocomoidea</emphasis>
</taxonomicName>
)? 
<taxonomicName box="[723,827,1931,1953]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="kodaianus">
<emphasis box="[723,827,1931,1953]" italics="true" pageId="143" pageNumber="144">kodaianus</emphasis>
