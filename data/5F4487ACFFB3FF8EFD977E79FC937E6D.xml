<taxonomicName id="10ED4D39FFB3FFB1FE8F7C50FCDF793B" authority="(Brunnich, 1764)" baseAuthorityName="Brunnich" baseAuthorityYear="1764" box="[345,769,836,863]" class="Aves" family="Stercorariidae" genus="Stercorarius" kingdom="Animalia" order="Charadriiformes" pageId="31" pageNumber="32" phylum="Chordata" rank="species" species="skua">
