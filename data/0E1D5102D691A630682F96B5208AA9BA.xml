<taxonomicName id="0719AAF0C325A3922668A60EBC4A1325" class="Insecta" family="Curculionidae" genus="Platygaster" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Platygaster haloxylonomyiae" order="Coleoptera" pageId="3" pageNumber="102" phylum="Arthropoda" rank="species" species="haloxylonomyiae">Platygaster haloxylonomyiae</taxonomicName>
