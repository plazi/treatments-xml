<collectingCountry id="F334D8B12C57FFBBFEB3B329FE2BFE9A" box="[303,393,283,307]" name="Vietnam" pageId="22" pageNumber="649">Vietnam</collectingCountry>
(
<bibRefCitation id="EFB2E5D02C57FFBBFE04B32EFE58FE9A" author="PIC M." box="[408,506,283,308]" pageId="22" pageNumber="649" pagination="1 - 32" refId="ref24611" refString="PIC M. 1926: Nouveautes diverses. Melanges Exotico-Entomologiques 45: 1 - 32." type="journal article" year="1926">PIC 1926</bibRefCitation>
), 
<collectingCountry id="F334D8B12C57FFBBFD91B32EFDE1FE9A" box="[525,579,284,307]" name="Laos" pageId="22" pageNumber="649">Laos</collectingCountry>
, 
<collectingCountry id="F334D8B12C57FFBBFDD2B329FD12FE9A" box="[590,688,283,307]" name="Thailand" pageId="22" pageNumber="649">Thailand</collectingCountry>
