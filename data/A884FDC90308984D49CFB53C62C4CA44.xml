<taxonomicName id="6D46C5ADC233D97ED080B007FF38607E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:133634" class="Insecta" family="Formicidae" genus="Anochetus" kingdom="Animalia" lsidName-HNS="Anochetus estus Wheeler" order="Hymenoptera" pageNumber="98" phylum="Arthropoda" rank="species" species="estus">
