<bibRefCitation id="EFABD4AE665AE854C0F359BBFDF51F16" author="China" box="[450,604,495,521]" pageId="22" pageNumber="26" refString="China, W. E. (1938) Die Arthropodenfauna von Madeira nach den Ergebnissen der reise von Prof. Dr. O. Lundblad. Juli - August 1935. III. Terrestrial Hemiptera (Hemiptera and Homoptera Auchenorrhyncha). Arkiv for Zoologi, 30 A (2), 1 - 69." type="journal article" year="1938">
<collectingCountry id="F32DE9CF665AE854C0F359BBFDA51F16" box="[450,524,495,521]" name="China" pageId="22" pageNumber="23">China</collectingCountry>
, 
<number id="3DF2BB78665AE854C32E59BBFDF51F16" box="[543,604,495,521]" pageId="22" pageNumber="23" value="1938.0">1938</number>
</bibRefCitation>
, no localities indicated; 
<bibRefCitation id="EFABD4AE665AE854C2A259BBFBFB1F16" author="Lindberg" box="[915,1106,495,521]" pageId="22" pageNumber="27" refString="Lindberg, H. (1941) Die Hemipteren der Azorischen Inseln. Nebst anhang: Zur Kenntis der Hemipterenfauna von Madeira. Commentationes Biologicae, 8 (8), 1 - 32." type="journal article" year="1941">
Lindberg, 
<number id="3DF2BB78665AE854C52559BBFBFB1F16" box="[1044,1106,495,521]" pageId="22" pageNumber="23" value="1941.0">1941</number>
</bibRefCitation>
; 
<bibRefCitation id="EFABD4AE665AE854C55559BBFA8A1F16" author="Lindberg" box="[1124,1315,495,521]" pageId="22" pageNumber="27" refString="Lindberg, H. (1961) Hemiptera Insularum Madeirensium. Commentationes Biologicae, 24 (1), 1 - 82." type="journal article" year="1961">
Lindberg, 
<number id="3DF2BB78665AE854C5D559BBFA8A1F16" box="[1252,1315,495,521]" pageId="22" pageNumber="23" value="1961.0">1961</number>
</bibRefCitation>
). 
<collectionCode id="ED2B319A665AE854C40F59BBFA351F16" box="[1342,1436,495,521]" pageId="22" pageNumber="23">PORTO</collectionCode>
<collectionCode id="ED2B319A665AE854C1C65A41FEF41F30" box="[247,349,533,559]" pageId="22" pageNumber="23">SANTO</collectionCode>
