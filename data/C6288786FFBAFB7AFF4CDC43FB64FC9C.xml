<number id="F84924B7FFBAFB7AFA86DF0BFA67FD62" box="[1373,1432,626,651]" pageId="26" pageNumber="127" value="1768.0">1768</number>
</bibRefCitation>
</taxonomicName>
. 
<taxonomicName id="89814D13FFBAFB7AFF39DFE3FDB7FD5B" authority="Koch, 1835" authorityName="Koch" authorityYear="1835" box="[226,584,665,690]" class="Insecta" family="Crambidae" genus="Notaspis" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="obscurus">
