<taxonomicName id="89814D13FFBAFB7AFAE1DFB8FE11FD16" authority="Koch, 1839" authorityName="Koch" authorityYear="1839" class="Insecta" family="Crambidae" genus="Notaspis" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="marginatus">
