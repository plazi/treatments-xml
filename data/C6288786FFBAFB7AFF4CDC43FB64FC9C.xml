<taxonomicName id="89814D13FFBAFB7AFEB0DF5DFD29FDD4" authority="Oudemans, 1936" authorityName="Oudemans" authorityYear="1936" box="[363,726,548,573]" class="Arachnida" family="Trematuridae" genus="Pseuduropoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="127" phylum="Arthropoda" rank="genus">
<emphasis id="7CF5EA82FFBAFB7AFEB0DF5DFDF1FDD4" box="[363,526,548,573]" italics="true" pageId="26" pageNumber="127">Pseuduropoda</emphasis>
<bibRefCitation id="2A104B61FFBAFB7AFDCEDF5DFD29FDD4" author="Oudemans" box="[533,726,548,573]" pageId="26" pageNumber="127" refString="Oudemans, A. C. (1936) Kritisch Historisch Overzicht der Acarologie, Deerde Gedeelte, 1805 - 1850. Band A. E. J. Brill, Leiden, 430 pp." type="book chapter" year="1936">
