<bibRefCitation id="2A104B61FFBAFB7AFB6EDFE3FA9CFD5B" author="De" box="[1205,1379,665,690]" pageId="26" pageNumber="127" refString="De Geer, C. (1768) Djur som med en strang i andan aro faste vid andra lefvande djur. Kongliga Vetenskaps Academiens Handlingar, 29, 176 - 183. [not seen, information from Oudemans, 1929]" type="journal article" year="1768">
<collectingCountry id="36967600FFBAFB7AFB6EDFE3FB29FD58" box="[1205,1238,666,689]" name="South Georgia and the South Sandwich Islands" pageId="26" pageNumber="127">De</collectingCountry>
Geer, 
<number id="F84924B7FFBAFB7AFAF0DFE0FA9CFD5B" box="[1323,1379,665,690]" pageId="26" pageNumber="127" value="1768.0">1768</number>
</bibRefCitation>
</taxonomicName>
(not 
<taxonomicName id="89814D13FFBAFB7AFF39DFB8FDB2FD30" authority="Latreille, 1806" authorityName="Latreille" authorityYear="1806" box="[226,589,704,729]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="vegetans">
<emphasis id="7CF5EA82FFBAFB7AFF39DFB8FE63FD31" box="[226,412,705,728]" italics="true" pageId="26" pageNumber="127">Acarus vegetans</emphasis>
<bibRefCitation id="2A104B61FFBAFB7AFE78DFB9FDB2FD30" author="Latreille" box="[419,589,704,729]" pageId="26" pageNumber="127" refString="Latreille, P. A. (1806) Genera Crustaceorum et Insectorum secundum ordinem naturalem in famlias disposita, iconibus exemplisque plurimus explicita. Vol. 1. Konig, Paris, xviii + 302 pp. + 16 plates. http: // dx. doi. org / 10.5962 / bhl. title. 65741" type="book" year="1806">
Latreille, 
<number id="F84924B7FFBAFB7AFDCADFB9FDB2FD30" box="[529,589,704,729]" pageId="26" pageNumber="127" value="1806.0">1806</number>
</bibRefCitation>
</taxonomicName>
). 
<taxonomicName id="89814D13FFBAFB7AFDBADFB8FC38FD30" authority="De Geer, 1768" authorityName="De Geer" authorityYear="1768" box="[609,967,704,729]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="vegetans">
<emphasis id="7CF5EA82FFBAFB7AFDBADFB8FCE5FD31" box="[609,794,705,728]" italics="true" pageId="26" pageNumber="127">Acarus vegetans</emphasis>
<bibRefCitation id="2A104B61FFBAFB7AFCF9DFB8FC38FD30" author="De" box="[802,967,704,729]" pageId="26" pageNumber="127" refString="De Geer, C. (1768) Djur som med en strang i andan aro faste vid andra lefvande djur. Kongliga Vetenskaps Academiens Handlingar, 29, 176 - 183. [not seen, information from Oudemans, 1929]" type="journal article" year="1768">
<collectingCountry id="36967600FFBAFB7AFCF9DFB8FCBCFD31" box="[802,835,705,728]" name="South Georgia and the South Sandwich Islands" pageId="26" pageNumber="127">De</collectingCountry>
Geer, 
<number id="F84924B7FFBAFB7AFC54DFB9FC38FD30" box="[911,967,704,729]" pageId="26" pageNumber="127" value="1768.0">1768</number>
</bibRefCitation>
