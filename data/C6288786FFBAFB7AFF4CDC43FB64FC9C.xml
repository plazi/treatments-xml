<taxonomicName id="89814D13FFBAFB7AFC37DFE3FA9CFD5B" authority="De Geer, 1768" authorityName="De Geer" authorityYear="1768" box="[1004,1379,665,690]" class="Insecta" family="Blaberidae" genus="Acarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Blattodea" pageId="26" pageNumber="127" phylum="Arthropoda" rank="species" species="vegetans">
