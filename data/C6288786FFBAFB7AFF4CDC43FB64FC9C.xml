<taxonomicName id="89814D13FFBAFB7AFBCBDC87FACFFDFF" authority="Hermann, 1804" authorityName="Hermann" authorityYear="1804" box="[1040,1328,510,534]" class="Insecta" family="Crambidae" genus="Notaspis" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="127" phylum="Arthropoda" rank="genus">
<emphasis id="7CF5EA82FFBAFB7AFBCBDC87FB8DFDFC" box="[1040,1138,510,533]" italics="true" pageId="26" pageNumber="127">Notaspis</emphasis>
<bibRefCitation id="2A104B61FFBAFB7AFBA7DC87FACFFDFF" author="Hermann" box="[1148,1328,510,534]" pageId="26" pageNumber="127" refString="Hermann, J. - F. (1804) Memoire Apterologique. Levrault, Strasbourg, 136 pp. + plates I - IX." type="book" year="1804">
