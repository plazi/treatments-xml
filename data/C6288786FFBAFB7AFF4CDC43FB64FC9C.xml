<taxonomicName id="89814D13FFBAFB7AFBC4DF32FA66FD8D" authority="Oudemans, 1936" authorityName="Oudemans" authorityYear="1936" box="[1055,1433,587,612]" class="Arachnida" family="Trematuridae" genus="Pseuduropoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="127" phylum="Arthropoda" rank="genus">
<emphasis id="7CF5EA82FFBAFB7AFBC4DF32FB3CFD8D" box="[1055,1219,587,612]" italics="true" pageId="26" pageNumber="127">Pseuduropoda</emphasis>
<bibRefCitation id="2A104B61FFBAFB7AFB0BDF32FA66FD8D" author="Oudemans" box="[1232,1433,587,612]" pageId="26" pageNumber="127" refString="Oudemans, A. C. (1936) Kritisch Historisch Overzicht der Acarologie, Deerde Gedeelte, 1805 - 1850. Band A. E. J. Brill, Leiden, 430 pp." type="book chapter" year="1936">
