<typeStatus id="913A8832FFBAFB7AFD1DDF0AFD09FD62" box="[710,758,627,651]" pageId="26" pageNumber="127">type</typeStatus>
species of 
<taxonomicName id="89814D13FFBAFB7AFCAADF0BFBEBFD62" box="[881,1044,626,651]" class="Arachnida" family="Trematuridae" genus="Pseuduropoda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="26" pageNumber="127" phylum="Arthropoda" rank="genus">
