<emphasis id="B924EA9F320CFFCFFF2DFD04FF337FDB" box="[144,254,681,702]" italics="true" pageId="65" pageNumber="157">Eolambia</emphasis>
</taxonomicName>
<materialsCitation id="3B383CD0320CFFCFFEB8FD04FEA77E3D" country="United States of America" county="The" elevation="8" location="Mussentuchit Member" municipality="lower Cenomanian" pageId="65" pageNumber="157" specimenCount="1" stateProvince="Utah">
