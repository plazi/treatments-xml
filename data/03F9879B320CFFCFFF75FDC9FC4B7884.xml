<location id="8E8F6056320CFFCFFDE7FD65FF3C7F98" LSID="urn:lsid:plazi:treatment:03F9879B320CFFCFFF75FDC9FC4B7884:8E8F6056320CFFCFFDE7FD65FF3C7F98" country="United States of America" county="The" municipality="lower Cenomanian" name="Mussentuchit Member" pageId="65" pageNumber="157" stateProvince="Utah">Mussentuchit Member</location>
of the 
<location id="8E8F6056320CFFCFFEECFD4AFD6D7F98" LSID="urn:lsid:plazi:treatment:03F9879B320CFFCFFF75FDC9FC4B7884:8E8F6056320CFFCFFEECFD4AFD6D7F98" box="[337,672,743,765]" country="United States of America" county="The" municipality="lower Cenomanian" name="Cedar Mountain Formation" pageId="65" pageNumber="157" stateProvince="Utah">
<location id="8E8F6056320CFFCFFEECFD4AFDDA7F98" LSID="urn:lsid:plazi:treatment:03F9879B320CFFCFFF75FDC9FC4B7884:8E8F6056320CFFCFFEECFD4AFDDA7F98" box="[337,535,743,765]" country="United States of America" county="The" municipality="lower Cenomanian" name="Cedar Mountain" pageId="65" pageNumber="157" stateProvince="Utah">
Cedar 
<location id="8E8F6056320CFFCFFE19FD4AFDDA7F98" LSID="urn:lsid:plazi:treatment:03F9879B320CFFCFFF75FDC9FC4B7884:8E8F6056320CFFCFFE19FD4AFDDA7F98" box="[420,535,743,765]" country="United States of America" county="The" municipality="lower Cenomanian" name="Mountain Formation" pageId="65" pageNumber="157" stateProvince="Utah">Mountain</location>
</location>
Formation
</location>
(
<collectingMunicipality id="6B8BACF7320CFFCFFD09FD4AFED37E7E" pageId="65" pageNumber="157">lower Cenomanian</collectingMunicipality>
