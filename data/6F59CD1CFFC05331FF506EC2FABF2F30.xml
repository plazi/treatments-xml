</taxonomicName>
(
<taxonomicName id="20F00789FFC05331FB7D6DC6FAD12F1D" box="[1210,1380,762,789]" class="Magnoliopsida" family="Euphorbiaceae" kingdom="Plantae" order="Malpighiales" pageId="23" pageNumber="412" phylum="Tracheophyta" rank="family">Euphorbiaceae</taxonomicName>
