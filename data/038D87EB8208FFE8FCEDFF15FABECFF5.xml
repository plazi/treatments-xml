<taxonomicName id="4C244D7E8208FFE8FC0AFBD3FAAACF4D" authorityName="Hirayama" authorityYear="1988" box="[970,1283,1105,1129]" class="Malacostraca" family="Caprellidae" genus="Quadrisegmentum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="236" phylum="Arthropoda" rank="species" species="triangulum">
<emphasis id="B950EAEF8208FFE8FC0AFBD3FAAACF4D" box="[970,1283,1105,1129]" italics="true" pageId="5" pageNumber="236">Quadrisegmentum triangulum</emphasis>
</taxonomicName>
