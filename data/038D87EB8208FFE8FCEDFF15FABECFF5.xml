<taxonomicName id="4C244D7E8208FFE8FB6DFBB6FAB7CF6F" authorityName="Hughes &amp; Takeuchi" authorityYear="2016" box="[1197,1310,1076,1099]" class="Malacostraca" family="Caprellidae" genus="Quadrisegmentum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="5" pageNumber="236" phylum="Arthropoda" rank="species" species="yirrgay">
