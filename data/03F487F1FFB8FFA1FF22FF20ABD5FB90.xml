<taxonomicName id="4C5D4D64FFB8FFA1FF22F8DCAEB6F87F" authorityName="Meigen" authorityYear="1835" box="[151,255,1899,1924]" class="Insecta" family="Chironomidae" genus="Diamesa" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="221" phylum="Arthropoda" rank="genus">
