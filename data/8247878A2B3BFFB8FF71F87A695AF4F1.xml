<paragraph id="0A51369C2B3AFFB8FF23FC8C695AF4F1" blockId="11.[151,1436,479,986]" pageId="11" pageNumber="12">
<taxonomicName id="CDEE4D1F2B3AFFB8FF23FC8C6BDFF4A2" box="[197,476,880,905]" class="Rhabditophora" family="Typhloplanidae" genus="Strongylosma" kingdom="Animalia" order="Rhabdocoela" pageId="11" pageNumber="12" phylum="Platyhelminthes" rank="species" species="radiatum">
