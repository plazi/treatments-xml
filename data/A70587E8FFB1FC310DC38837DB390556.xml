<quantity id="E8549B1BFFB1FC310CE98994DB4806F5" box="[965,1028,1420,1446]" metricMagnitude="-1" metricUnit="m" metricValue="2.6" pageId="3" pageNumber="860" unit="cm" value="26.0">26cm</quantity>
