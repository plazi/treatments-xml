<taxonomicName id="8BF94D60D60A4D45FC7F653830D5FB89" box="[1317,1425,1125,1150]" class="Insecta" family="Lonchaeidae" genus="Silba" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="533" phylum="Arthropoda" rank="species" species="garamba" status="sp. nov.">Garamba</taxonomicName>
), II/gd/9, 
<date id="38471023D60A4D45F9AB65D4341AFB55" box="[241,350,1161,1186]" pageId="7" pageNumber="522" value="1951-11-08">
