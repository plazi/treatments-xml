<geoCoordinate id="EE235007FF16FF32994BFBE9FBEA3430" box="[310,427,1051,1077]" degrees="178" direction="west" minutes="42" orientation="longitude" pageId="135" pageNumber="136" precision="925" value="-178.7">178°42’W</geoCoordinate>
