<geoCoordinate id="EE235007FF16FF3298BBFBE9FB683430" box="[198,297,1051,1077]" degrees="60" direction="north" minutes="49" orientation="latitude" pageId="135" pageNumber="136" precision="925" value="60.816666">60°49’N</geoCoordinate>
