<taxonomicName id="8EF04D64FFE0FFB0FDB5B85CFDF151E3" authority="Kramina &amp; D. D. Sokoloff" authorityName="Kramina &amp; D. D. Sokoloff" class="Magnoliopsida" family="Fabaceae" genus="Lotus" kingdom="Plantae" order="Fabales" pageId="3" pageNumber="192" phylum="Tracheophyta" rank="section" section="Chamaelotus">
