<quantity id="8A349B0AB041FF8BFD23EC2F150FFEC7" box="[665,769,362,381]" metricMagnitude="-3" metricUnit="m" metricValue="3.8499999999999996" metricValueMax="4.3" metricValueMin="3.4" pageId="18" pageNumber="329" unit="mm" value="3.8499999999999996" valueMax="4.3" valueMin="3.4">3.4–4.3 mm</quantity>
(mean 
<emphasis id="7FB8EAFDB041FF8BFCFBEC2C155FFEC4" bold="true" box="[833,849,361,382]" italics="true" pageId="18" pageNumber="329">¼</emphasis>
<quantity id="8A349B0AB041FF8BFCECEC2F15A8FEC7" box="[854,934,362,381]" metricMagnitude="-3" metricUnit="m" metricValue="3.85" pageId="18" pageNumber="329" unit="mm" value="3.85">3.85 mm</quantity>
