<quantity id="8A349B0AB041FF8BFC17EC1716DEFEC7" metricMagnitude="-3" metricUnit="m" metricValue="3.8499999999999996" metricValueMax="4.3" metricValueMin="3.4" pageId="18" pageNumber="329" unit="mm" value="3.8499999999999996" valueMax="4.3" valueMin="3.4">3.4–4.3 mm</quantity>
