<subSubSection id="EBCD655EFFFBE6034FCCFA5072C5BF16" box="[199,748,1518,1545]" pageId="17" pageNumber="18" type="distribution">
<paragraph id="A36836D5FFFBE6034FCCFA5072C5BF16" blockId="17.[151,1437,150,1905]" box="[199,748,1518,1545]" pageId="17" pageNumber="18">
<emphasis id="91A3EAC7FFFBE6034FCCFA507148BF17" bold="true" box="[199,353,1518,1544]" pageId="17" pageNumber="18">Distribution.</emphasis>
<collectingRegion id="6113F837FFFBE6034E6CFA51721BBF16" box="[359,562,1519,1545]" country="Australia" name="Western Australia" pageId="17" pageNumber="18">Western Australia</collectingRegion>
, from 
<quantity id="642F9B30FFFBE6034D71FA5172C1BF16" box="[634,744,1518,1545]" metricMagnitude="1" metricUnit="m" metricValue="2.65" metricValueMax="4.5" metricValueMin="0.8" pageId="17" pageNumber="18" unit="m" value="26.5" valueMax="45.0" valueMin="8.0">
8 to 
<specimenCount id="B5D1FD5CFFFBE6034DA7FA5072C1BF16" box="[684,744,1518,1545]" pageId="17" pageNumber="18" type="generic">45 m</specimenCount>
