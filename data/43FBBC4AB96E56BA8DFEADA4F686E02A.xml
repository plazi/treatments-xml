<taxonomicName id="3BCF2A0AC1B39FA12D72CD7603FACA26" authority="Fleutiaux, 1903" authorityName="Fleutiaux" authorityYear="1903" class="Insecta" family="Elateridae" genus="Tropidiplus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tropidiplus tellinii" order="Coleoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="tellinii">Tropidiplus tellinii</taxonomicName>
