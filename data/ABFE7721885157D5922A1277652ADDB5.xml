<paragraph id="0AD948A1A9B8BEAE90D152B07AECAD49" pageId="11" pageNumber="12">
<taxonomicName id="1400EFB3681DAE599C47947FC76D1A5E" authorityName="J. R. I. Wood &amp; R. Degen" authorityYear="2017" class="Magnoliopsida" family="Convolvulaceae" genus="Ipomoea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Ipomoea dolichopoda" order="Solanales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="dolichopoda">
<emphasis id="420A222CAE625A1D9F076CB1C12D3478" italics="true" pageId="11" pageNumber="12">Ipomoea dolichopoda</emphasis>
</taxonomicName>
