<taxonomicName id="CA1B5FE7BD337104FC9848305FCF7E00" genus="I." lsidName="I. attenuata" pageId="11" pageNumber="12" rank="species" species="attenuata">
<emphasis id="8ABA7601881C08F01711D4990BCB38E8" italics="true" pageId="11" pageNumber="12">I. attenuata</emphasis>
</taxonomicName>
