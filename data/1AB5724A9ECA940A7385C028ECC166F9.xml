<taxonomicName id="8E216D52673AFA4D624FC656F21912F4" class="Polychaeta" family="Lumbrineridae" genus="Lumbrinerides" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lumbrinerides acuta" order="Eunicida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="acuta">Lumbrinerides acuta</taxonomicName>
