<taxonomicName id="4C0B35E30600FFDDF5F35FF26EC17845" box="[178,410,248,270]" class="Magnoliopsida" family="Crassulaceae" genus="Phedimus" kingdom="Plantae" order="Saxifragales" pageId="16" pageNumber="357" phylum="Tracheophyta" rank="species" species="sarmentosus">
