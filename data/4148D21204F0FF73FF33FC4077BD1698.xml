<quantity id="0E19CEE104F0FF73FE85FAC577D81514" box="[289,458,1288,1315]" metricMagnitude="0" metricUnit="m" metricValue="3.235" metricValueMax="4.85" metricValueMin="1.62" pageId="160" pageNumber="161" unit="m" value="3.235" valueMax="4.85" valueMin="1.62">1,620–4,850 m</quantity>
