acute tip, except in 
<taxonomicName id="C6F965BC1344FFAEFDF6F818FCFDF86F" box="[598,775,1993,2017]" class="Secernentea" family="Cephalobidae" genus="Pseudacrobeles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="7" pageNumber="207" phylum="Nematoda" rank="species" species="bostromi" subGenus="Bunobus">
