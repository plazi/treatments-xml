<taxonomicName id="4C7FFFC0CC330D7B5190FDEDBBC36742" authorityName="ZEVINA" authorityYear="1978" box="[949,1160,564,585]" class="Maxillopoda" family="Scalpellidae" genus="Amigdoscalpellum" kingdom="Animalia" order="Pedunculata" pageId="34" pageNumber="300" phylum="Arthropoda" rank="genus">
