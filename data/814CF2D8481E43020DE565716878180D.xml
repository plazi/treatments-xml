<taxonomicName id="F7CD6327A9DEECD24A57BBDBA6F1F2D4" class="Magnoliopsida" family="Primulaceae" genus="Embelia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Embelia" order="Ericales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Embelia</taxonomicName>
