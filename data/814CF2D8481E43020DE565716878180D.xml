<taxonomicName id="61E2C9A02FC5E64EF79551729BD1C0F0" class="Insecta" family="Geometridae" genus="Collix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Collix ghosha" order="Lepidoptera" pageId="0" pageNumber="7938" phylum="Arthropoda" rank="species" species="ghosha">Collix ghosha</taxonomicName>
