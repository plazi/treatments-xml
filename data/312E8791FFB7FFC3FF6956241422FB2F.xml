<taxonomicName id="7E874D04FFB7FFC3FE2755CB120AFD43" box="[473,605,675,699]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="10" pageNumber="158" phylum="Cnidaria" rank="species" species="tenellum">
