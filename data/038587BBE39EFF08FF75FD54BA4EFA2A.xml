<treatmentCitationGroup id="AB3C1183E39EFF08FF75FC9DBB6EFCCB" box="[151,594,824,846]" pageId="153" pageNumber="154">
<taxonomicName id="4C2C4D2EE39EFF08FF75FC9DBB72FCCB" authority="Fairmaire 1898 b: 469" authorityName="Fairmaire" authorityPageNumber="469" authorityYear="1898" box="[151,590,824,846]" class="Insecta" family="Hydraenidae" genus="Hydraena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="discicollis">
<emphasis id="B958EABFE39EFF08FF75FC9DB855FCCB" box="[151,361,824,846]" italics="true" pageId="153" pageNumber="154">Hydraena discicollis</emphasis>
