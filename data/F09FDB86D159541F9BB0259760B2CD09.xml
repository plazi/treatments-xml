<paragraph id="59C1EA2E4331DED96DE8828590B320F2" pageId="0" pageNumber="1">
<taxonomicName id="EC51BB1883A96DF1D9E28B4C91FB3272" baseAuthorityName="J.A.G.Rehn" baseAuthorityYear="1904" class="Insecta" family="Diapheromeridae" genus="Micadina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Micadina phluctainoides" order="Phasmatodea" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="phluctainoides">
<emphasis id="31EC340CA129D7FE219FE33BB2F5B865" italics="true" pageId="0" pageNumber="1">Micadina phluctainoides</emphasis>
</taxonomicName>
(Rehn, 1904) (
<taxonomicName id="321D9FC073BFA8E38DEAB30C77B8D76F" class="Insecta" family="Diapheromeridae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Phasmatodea" pageId="0" pageNumber="1" phylum="Arthropoda" rank="family">Diapheromeridae</taxonomicName>
