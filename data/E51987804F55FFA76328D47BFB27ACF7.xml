<collectingRegion id="AF74F8744F56FFA76056D62CFD07AC17" box="[630,760,1056,1082]" country="Dominican Republic" name="Pedernales" pageId="18" pageNumber="19">Pedernales</collectingRegion>
, and 
<collectingRegion id="AF74F8744F56FFA76115D62CFC11AC17" box="[821,1006,1056,1082]" country="Dominican Republic" name="Santo Domingo" pageId="18" pageNumber="19">Santo Domingo</collectingRegion>
