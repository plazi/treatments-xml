<emphasis id="7E8BEA8CBB626133FECFF43A11FDFC42" box="[264,472,885,907]" italics="true" pageId="13" pageNumber="494">
<taxonomicName id="8BFF4D1DBB626133FECFF43A11A3FC42" authorityName="Thomson" authorityYear="1915" box="[264,390,885,907]" class="Rhynchonellata" family="Thecidellinidae" genus="Thecidellina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thecideida" pageId="13" pageNumber="494" phylum="Brachiopoda" rank="genus">Thecidellina</taxonomicName>
