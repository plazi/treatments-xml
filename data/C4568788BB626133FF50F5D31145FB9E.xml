<taxonomicName id="8BFF4D1DBB626133FECFF40311A3FCAB" authorityName="Thomson" authorityYear="1915" box="[264,390,844,866]" class="Rhynchonellata" family="Thecidellinidae" genus="Thecidellina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thecideida" pageId="13" pageNumber="494" phylum="Brachiopoda" rank="genus">Thecidellina</taxonomicName>
