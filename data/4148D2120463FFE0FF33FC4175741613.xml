<quantity id="0E19CEE10463FFE0FD02FAC3755D151F" box="[678,847,1294,1320]" metricMagnitude="0" metricUnit="m" metricValue="1.6764999999999999" metricValueMax="1.727" metricValueMin="1.626" pageId="51" pageNumber="52" unit="m" value="1.6764999999999999" valueMax="1.727" valueMin="1.626">
