<taxonomicName id="DEE1C6F6345F477EFE0AFCF5A564B057" box="[435,578,824,848]" class="Insecta" family="Rhyparochromidae" genus="Dieuches" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="109" phylum="Arthropoda" rank="species" species="jacobsoni">
