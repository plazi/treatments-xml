<taxonomicName id="AB40A59F8DFDCA369A598F18729D8147" authorityName="Munir" authorityYear="1979" class="Magnoliopsida" family="Lamiaceae" genus="Pityrodia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pityrodia augustensis" order="Lamiales" pageId="0" pageNumber="45" phylum="Tracheophyta" rank="species" species="augustensis">
