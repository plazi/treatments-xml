</paragraph>
<paragraph id="8BEC369AFFCEFF8E10E7DF0EED1AFED9" blockId="7.[192,1441,1763,1915]" lastBlockId="8.[192,1441,195,891]" lastPageId="8" lastPageNumber="7" pageId="7" pageNumber="6">
<collectingCountry id="F344760AFFCEFF8110E7DF0EE9BBF899" box="[192,418,1859,1883]" name="US Virgin Islands" pageId="7" pageNumber="6">US Virgin Islands</collectingCountry>
