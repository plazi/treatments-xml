<taxonomicName id="4C534D19FFCEFF81133FDF4EEBA0F8D9" baseAuthorityName="Latreille" baseAuthorityYear="1804" box="[792,953,1795,1819]" class="Insecta" family="Lathiceridae" genus="Scapteriscus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="didactylus">
