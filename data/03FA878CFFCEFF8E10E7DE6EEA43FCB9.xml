<collectingCountry id="F344760AFFCEFF8112C9DF6EEB4AF8F9" box="[750,851,1827,1851]" name="Jamaica" pageId="7" pageNumber="6">Jamaica</collectingCountry>
as 
<taxonomicName id="4C534D19FFCEFF81135EDF6EEC3FF8F9" box="[889,1062,1827,1851]" class="Insecta" family="Lathiceridae" genus="Scapteriscus" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="abbreviatus">
