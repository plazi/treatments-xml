<collectingCountry id="F344760AFFCEFF811331DF29EBB0F8B9" box="[790,937,1891,1915]" name="Puerto Rico" pageId="7" pageNumber="6">Puerto Rico</collectingCountry>
<emphasis id="B927EA88FFCEFF811393DF2EEBA5F8B9" bold="true" box="[948,956,1891,1915]" pageId="7" pageNumber="6">!</emphasis>
