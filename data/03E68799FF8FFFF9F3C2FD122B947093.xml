<taxonomicName id="4C4F4D0CFF8FFFF9F752FBA02A7B704D" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1031,1139,1117,1138]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="wallacei" status="sp. nov.">
