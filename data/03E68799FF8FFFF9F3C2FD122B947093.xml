</taxonomicName>
(
<taxonomicName id="4C4F4D0CFF8FFFF9F784FBBD2B12706A" authorityName="Zelazny" authorityYear="1981" baseAuthorityName="Bierman" baseAuthorityYear="1910" box="[1233,1306,1088,1109]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="pavo">
