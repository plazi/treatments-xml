<taxonomicName id="9BB84D4A044614C1692307D39FB2D575" class="Insecta" family="Cerambycidae" genus="Demonax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Demonax brachychona" order="Coleoptera" pageId="0" pageNumber="20997" phylum="Arthropoda" rank="species" species="brachychona">Demonax brachychona</taxonomicName>
