<taxonomicName id="4C194D78FFEAFFBAFE6AFC28FDC6DB71" box="[420,611,1020,1043]" class="Demospongiae" family="Acarnidae" genus="Iophon" kingdom="Animalia" order="Poecilosclerida" pageId="35" pageNumber="503" phylum="Porifera" rank="subSpecies" species="situ" subSpecies="appearance">
