<taxonomicName id="CCC18FF5FF94FFEE16BAFE770D32EECD" box="[729,827,444,467]" class="Insecta" family="Philopotamidae" genus="Alterosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="12" phylum="Arthropoda" rank="species" species="graciosa">
