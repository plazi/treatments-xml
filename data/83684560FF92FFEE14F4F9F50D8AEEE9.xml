<subSubSection id="43DBA7FDFF94FFEE14A4FE700D8AEEE9" pageId="12" type="etymology">
<paragraph id="0B7EF476FF94FFEE14A4FE700D8AEEE9" blockId="12.[151,1436,151,504]" pageId="12">
<emphasis id="39B52864FF94FFEE14A4FE700F58EECD" bold="true" box="[199,337,443,467]" pageId="12">Etymology:</emphasis>
