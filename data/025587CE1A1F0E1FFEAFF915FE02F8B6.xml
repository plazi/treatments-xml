<taxonomicName id="4DFC4D5B1A180E1EFE10FB54FDCAFAEC" box="[439,628,1277,1300]" class="Insecta" family="Miridae" genus="Isometopus" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="mirus" status="sp. nov.">
