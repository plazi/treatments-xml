<subSubSection id="A7D9657DFFA3B14D23C2FABD46F20B3D" box="[199,1278,1374,1399]" pageId="7" pageNumber="36" type="etymology">
<paragraph id="EF7C36F6FFA3B14D23C2FABD46F20B3D" blockId="7.[151,1437,762,1435]" box="[199,1278,1374,1399]" pageId="7" pageNumber="36">
<emphasis id="DDB7EAE4FFA3B14D23C2FABD43420B3D" bold="true" box="[199,334,1374,1399]" pageId="7" pageNumber="36">Etymology.</emphasis>
<emphasis id="DDB7EAE4FFA3B14D2251FABC43910B3C" box="[340,413,1375,1398]" italics="true" pageId="7" pageNumber="36">Laevis</emphasis>
