<taxonomicName id="4C494D6F6644FFC6FB7E0C8A7B772E8F" authority="C.L. Koch, 1844 in Berlese (1888)" authorityName="C.L. Koch, 1844 in Berlese" authorityYear="1888" class="Arachnida" family="Oribatulidae" genus="Oribates" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="62" pageNumber="63" phylum="Arthropoda" rank="species" species="latipes">
