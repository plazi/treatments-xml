</taxonomicName>
<emphasis id="B933EA8D3B4DA1626EE7FBD4AAF6FB33" bold="true" box="[1180,1269,1129,1155]" pageId="18" pageNumber="19">
<taxonomicNameLabel id="A20057F63B4DA1626EE7FBD4AAF6FB33" box="[1180,1269,1129,1155]" pageId="18" pageNumber="19" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName id="4C474D1C3B4DA1626F52FBD7AB9EFB33" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[1321,1437,1129,1155]" class="Arachnida" family="Prodidomidae" genus="Eleleis" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="luderitz" status="sp. nov.">
