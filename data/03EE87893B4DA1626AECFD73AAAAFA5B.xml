<taxonomicName id="4C474D1C3B4DA162695DFBD7ADA3FB33" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[806,928,1129,1155]" class="Arachnida" family="Prodidomidae" genus="Eleleis" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="limpopo" status="sp. nov.">
<emphasis id="B933EA8D3B4DA162695DFBD7ADA3FB33" box="[806,928,1129,1155]" italics="true" pageId="18" pageNumber="19">E. limpopo</emphasis>
</taxonomicName>
<emphasis id="B933EA8D3B4DA16269DDFBD4AA05FB33" bold="true" box="[934,1030,1129,1155]" pageId="18" pageNumber="19">
<taxonomicNameLabel id="A20057F63B4DA16269DDFBD4AA03FB33" box="[934,1024,1129,1155]" pageId="18" pageNumber="19" rank="species">sp. nov.</taxonomicNameLabel>
,
</emphasis>
<taxonomicName id="4C474D1C3B4DA1626E70FBD7AA95FB33" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[1035,1174,1129,1155]" class="Arachnida" family="Prodidomidae" genus="Eleleis" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="okavango" status="sp. nov.">
