<emphasis id="F697EAF34454E039FE8E9542FE46F9D5" box="[278,440,1631,1652]" italics="true" pageId="1" pageNumber="434">
<taxonomicName id="03E34D624454E039FE8E9542FE4AF9D5" authorityName="Kolbe" authorityYear="1910" box="[278,436,1631,1652]" class="Insecta" family="Dynastidae" genus="Hemiphileurus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="434" phylum="Arthropoda" rank="genus">Hemiphileurus</taxonomicName>
