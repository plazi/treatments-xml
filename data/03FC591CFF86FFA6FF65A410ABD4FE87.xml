<taxonomicName id="4C559389FF87FFA4FEBBA449AB02F87E" authorityName="Dow" authorityYear="2010" box="[329,552,1918,1941]" class="Insecta" family="Platystictidae" genus="Drepanosticta" kingdom="Animalia" order="Odonata" pageId="1" pageNumber="64" phylum="Arthropoda" rank="species" species="sbong">
