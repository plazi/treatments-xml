<taxonomicName id="84E6C7E85A1DFFB14ACFFF234EFAFF62" authorityName="Xue &amp; Zhang" authorityYear="2008" box="[640,731,192,217]" class="Arachnida" family="Eriophyidae" genus="Nameriophyes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="sapidae">
