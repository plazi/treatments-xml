<quantity id="FD3D9B46FFB43B1AFCE1F91E8DC6C2CA" box="[796,934,1743,1768]" metricMagnitude="-3" metricUnit="m" metricValue="9.35" metricValueMax="9.5" metricValueMin="9.2" pageId="2" pageNumber="357" unit="mm" value="9.35" valueMax="9.5" valueMin="9.2">9.2–9.5 mm</quantity>
