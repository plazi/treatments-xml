<taxonomicName id="4C051AAAFFEE0C16FF39F9ADE1B1F9F7" authority="Assing, 2010" authorityName="Assing" authorityYear="2010" box="[219,454,1572,1588]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="15" pageNumber="316" phylum="Arthropoda" rank="species" species="houhuanicum" status="sp. n.">
<emphasis id="B971BD3BFFEE0C16FF39F9ADE1B1F9F7" box="[219,454,1572,1588]" italics="true" pageId="15" pageNumber="316">Lathrobium houhuanicum</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A2420040FFEE0C16FE2EF9ADE277F9FA" box="[460,512,1572,1593]" pageId="15" pageNumber="316" rank="species">sp. n.</taxonomicNameLabel>
: habitus (
<emphasis id="B971BD3BFFEE0C16FDB9F9ADE205F9F6" bold="true" box="[603,626,1572,1589]" pageId="15" pageNumber="316">49</emphasis>
); forebody (
<emphasis id="B971BD3BFFEE0C16FD05F9ADE28AF9F6" bold="true" box="[743,765,1572,1589]" pageId="15" pageNumber="316">50</emphasis>
); male sternite VII (
<emphasis id="B971BD3BFFEE0C16FC5EF9ADE3A6F9F6" bold="true" box="[956,977,1572,1589]" pageId="15" pageNumber="316">51</emphasis>
); male sternite VIII (
<emphasis id="B971BD3BFFEE0C16FF2EF9C8E095F991" bold="true" box="[204,226,1601,1618]" pageId="15" pageNumber="316">52</emphasis>
); aedeagus in lateral view (
<emphasis id="B971BD3BFFEE0C16FE02F9C8E260F991" bold="true" box="[480,535,1601,1618]" pageId="15" pageNumber="316">53-54</emphasis>
); female tergite VIII (
<emphasis id="B971BD3BFFEE0C16FD0AF9C8E289F991" bold="true" box="[744,766,1601,1618]" pageId="15" pageNumber="316">55</emphasis>
