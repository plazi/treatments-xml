<number id="3D9A917CFFA1FFE25F8BFA468A8BFA66" box="[151,192,1474,1498]" pageId="8" pageNumber="557" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
weak pd and 
<number id="3D9A917CFFA1FFE25E46FA468B23FA66" box="[346,360,1474,1498]" pageId="8" pageNumber="557" value="3.0">3</number>
weak av; apically with 
<number id="3D9A917CFFA1FFE25D6DFA468834FA66" box="[625,639,1474,1498]" pageId="8" pageNumber="557" value="1.0">1</number>
ad and 
<number id="3D9A917CFFA1FFE25DC9FA4688A8FA66" box="[725,739,1474,1498]" pageId="8" pageNumber="557" value="1.0">1</number>
av. Fore tarsomere 
<number id="3D9A917CFFA1FFE25CA3FA468986FA66" box="[959,973,1474,1498]" pageId="8" pageNumber="557" value="1.0">1</number>
