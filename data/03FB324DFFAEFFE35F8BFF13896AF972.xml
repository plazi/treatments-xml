tarsomere 
<number id="3D9A917CFFA1FFE25E52F98E8B17F99E" box="[334,348,1546,1570]" pageId="8" pageNumber="557" value="1.0">1</number>
with 
<number id="3D9A917CFFA1FFE25E81F98E8BE0F99E" box="[413,427,1546,1570]" pageId="8" pageNumber="557" value="1.0">1</number>
