<quantity id="8D6B9B35FF80FFEBFD69FEB81F53FEC0" box="[751,853,296,320]" metricMagnitude="1" metricUnit="m" metricValue="2.5" metricValueMax="3.0" metricValueMin="2.0" pageId="51" pageNumber="52" unit="m" value="25.0" valueMax="30.0" valueMin="20.0">
