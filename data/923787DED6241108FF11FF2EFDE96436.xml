(
<bibRefCitation id="7E0F4B39D6231108FF18FF05FE9C669B" author="Sorokina" box="[158,375,188,212]" pageId="4" pageNumber="487" refString="Sorokina, V. S. (2009 a) A key to Siberian flies of the genus Coenosia Meigen (Diptera, Muscidae) with the descriptions of three new species. Zootaxa, 2308, 1 - 28." type="journal article" year="2009" yearSuffix="a">
Sorokina, 
<number id="AC5624EFD6231108FE95FF05FEA4669B" box="[275,335,188,212]" pageId="4" pageNumber="487" value="2009.0">2009</number>
a: 
<number id="AC5624EFD6231108FEE0FF05FE9C669B" box="[358,375,188,212]" pageId="4" pageNumber="487" value="6.0">6</number>
</bibRefCitation>
)
</taxonomicName>
:
</paragraph>
<paragraph id="1A2136C8D6231108FF11FEBAFDE66787" blockId="4.[151,1436,259,633]" pageId="4" pageNumber="487">
