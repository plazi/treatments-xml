<taxonomicName id="17B04D74FF81FF90235F5A2EFD07FEB8" authority="Bravard, 1835" authorityName="Bravard" authorityYear="1835" box="[295,746,275,297]" class="Mammalia" family="Cainotheriidae" genus="Cainotherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Artiodactyla" pageId="18" pageNumber="163" phylum="Chordata" rank="species" species="commune">
