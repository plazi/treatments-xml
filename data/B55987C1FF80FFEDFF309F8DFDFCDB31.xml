, as 
<taxonomicName id="FAF04D54FF80FFEDFDA89D55FD63DB63" authorityName="Odhner" authorityYear="1925" box="[527,683,782,806]" class="Malacostraca" family="Xanthidae" genus="Xantho" kingdom="Animalia" order="Decapoda" pageId="102" pageNumber="103" phylum="Arthropoda" rank="species" species="danae">
<emphasis id="0F84EAC5FF80FFEDFDA89D55FD63DB63" box="[527,683,782,806]" italics="true" pageId="102" pageNumber="103">Xantho danae</emphasis>
</taxonomicName>
), 
<bibRefCitation id="59614B26FF80FFEDFD649D55FC06DB63" author="Forest &amp; Guinot" box="[707,974,782,806]" firstAuthor="Forest" pageId="102" pageNumber="103" pagination="1 - 18" refId="ref99816" refString="Forest, J. &amp; Guinot, D. (1961) Crustaces decapodes brachyoures de Tahiti et des Tuamotu. In: Expedition francaise sur les Recifs coralliens de la Nouvelle-Caledonie. Volume preliminaire. Fondation Singer-Polignac, Paris, xi + 195 pp., pls. 1 - 18." type="book chapter" year="1961">Forest &amp; Guinot (1961)</bibRefCitation>
