<taxonomicName id="216992033817FE60D4A37FFA98A01804" class="Hexapoda" family="Carabidae" genus="Pterostichus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="549" pageNumber="550" phylum="Arthropoda" rank="subSpecies" species="ventricosus" subSpecies="paludosus">
