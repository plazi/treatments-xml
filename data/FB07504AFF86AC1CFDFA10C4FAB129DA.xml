Genus 
<taxonomicName id="B4AE9ADFFF86AC1DFD0C10C5FC1F2C67" authority="Dalman" authorityName="Dalman" authorityYear="1823" box="[685,985,1518,1545]" class="Insecta" family="Figitidae" genus="Anacharis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus">
