<taxonomicName id="DEB734A4A5721C46301FAC0D055015E9" class="Pinopsida" family="Cupressaceae" genus="Juniperus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Juniperus" order="Pinales" pageId="8" pageNumber="43" phylum="Tracheophyta" rank="genus">Juniperus</taxonomicName>
