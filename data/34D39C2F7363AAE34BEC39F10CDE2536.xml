<normalizedToken id="AC500DC14BF26BD44935FC921ABEE0DE" originalValue="“Alm”">&quot;Alm&quot;</normalizedToken>
Pakora SE Ganto La, 
<geoCoordinate id="7A552A3738EA92852D54975BE45AFA01" direction="north" orientation="latitude" precision="925" value="35.683334">35°41'N</geoCoordinate>
, 
<geoCoordinate id="64A8DCD3ADB45F6C78FE22E3B57E7D34" direction="east" orientation="longitude" precision="925" value="75.35">75°21'E</geoCoordinate>
