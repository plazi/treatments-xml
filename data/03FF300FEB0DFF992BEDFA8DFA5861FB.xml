<taxonomicName id="4C56FA9AEB0DFF982FD4F906FA5867CB" authority="Evenhuis &amp; Baez" authorityName="Evenhuis &amp; Baez" authorityYear="2001" box="[1198,1436,1764,1786]" class="Insecta" family="Bombyliidae" genus="Reissa" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="597" phylum="Arthropoda" rank="genus">
