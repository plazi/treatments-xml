<emphasis id="B938D442A02F462DFF3060B3FE29AE86" bold="true" box="[160,401,718,744]" italics="true" pageId="14" pageNumber="748">
<taxonomicName id="4C4C73D3A02F462DFF3060B3FEB6AE86" authorityName="Cameron" authorityYear="1884" box="[160,270,718,744]" class="Insecta" family="Eucharitidae" genus="Orasema" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="748" phylum="Arthropoda" rank="genus">Orasema</taxonomicName>
wayqecha
