<paragraph id="338F036271D34D7881B538C0FC2173EA" pageId="42" pageNumber="43">
<taxonomicName id="E622919B9F5DF1DED252B9BCD7595CE3" class="Insecta" family="Scarabaeidae" genus="Canthon" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Canthon obscuriellus" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="obscuriellus">Canthon obscuriellus</taxonomicName>
