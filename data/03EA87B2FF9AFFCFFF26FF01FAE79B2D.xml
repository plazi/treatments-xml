<taxonomicName id="4C434D27FF9AFFCFFEECFF2AFDC79A9D" box="[349,620,188,213]" class="Entognatha" family="Entomobryidae" genus="Lepidocyrtus" kingdom="Animalia" order="Collembola" pageId="36" pageNumber="337" phylum="Arthropoda" rank="species" species="curvicollis">
