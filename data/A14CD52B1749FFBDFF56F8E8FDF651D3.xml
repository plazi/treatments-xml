<taxonomicName id="EEE51FBE174AFFBDFF56FC0EFE90519B" box="[151,260,908,932]" class="Trilobita" family="Styginidae" genus="Scutellum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Corynexochida" pageId="14" pageNumber="24" phylum="Arthropoda" rank="genus">scutellum</taxonomicName>
