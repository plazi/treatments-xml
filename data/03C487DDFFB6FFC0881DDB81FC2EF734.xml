<paragraph id="8BD236CBFFB6FFC089D0DBD1FDF6F49E" blockId="8.[151,631,1511,1536]" box="[151,631,1511,1536]" pageId="8" pageNumber="149">
<emphasis id="B919EAD9FFB6FFC089D0DBD1FEACF49F" bold="true" box="[151,301,1511,1536]" pageId="8" pageNumber="149">Type species:</emphasis>
<taxonomicName id="4C6D4D48FFB6FFC08873DBDEFD9CF49E" authority="Tanasevitch, 2017" authorityName="Tanasevitch" authorityYear="2017" box="[308,541,1511,1535]" class="Arachnida" family="Linyphiidae" genus="Tegulinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="8" pageNumber="149" phylum="Arthropoda" rank="species" species="sumatranus" status="sp. nov.">
<emphasis id="B919EAD9FFB6FFC08873DBDEFD9CF49E" box="[308,541,1511,1535]" italics="true" pageId="8" pageNumber="149">Tegulinus sumatranus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A22A57A2FFB6FFC08B63DBD1FDF6F49E" box="[548,631,1512,1535]" pageId="8" pageNumber="149" rank="species">sp. nov.</taxonomicNameLabel>
