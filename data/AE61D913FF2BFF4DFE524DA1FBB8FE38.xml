<taxonomicName id="E1C81386FF2AFF4DFEBE4A68FED1FE8C" box="[275,348,314,339]" class="Magnoliopsida" family="Fagaceae" genus="Fagus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="139" pageNumber="140" phylum="Tracheophyta" rank="genus">
