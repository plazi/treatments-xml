<heading id="9F1E81FA8A4BFFE223A46A8FFD976C81" bold="true" box="[151,559,1161,1188]" fontSize="11" level="1" pageId="9" pageNumber="290" reason="1">
<emphasis id="F69DEA848A4BFFE223A46A8FFD976C81" bold="true" box="[151,559,1161,1188]" pageId="9" pageNumber="290">
<taxonomicName id="03E94D158A4BFFE223A46A8FFE736C86" authority="Jaschhof &amp; Jaschhof, 2019" authorityName="Jaschhof &amp; Jaschhof" authorityYear="2019" box="[151,459,1161,1188]" class="Insecta" family="Cecidomyiidae" genus="Camptomyia" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="290" phylum="Arthropoda" rank="species" species="oldhammeri" status="sp. nov.">
<emphasis id="F69DEA848A4BFFE223A46A8FFE736C86" bold="true" box="[151,459,1161,1188]" italics="true" pageId="9" pageNumber="290">Camptomyia oldhammeri</emphasis>
