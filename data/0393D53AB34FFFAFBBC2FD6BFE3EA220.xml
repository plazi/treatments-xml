<heading id="D0CDD340B34FFFAFBBC2FD6BFDDDA440" bold="true" box="[151,606,764,790]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<taxonomicName id="4C3A1FAFB34FFFAFBBC2FD6BFDDDA440" authority="Tapia, 1953: 77" authorityName="Tapia" authorityPageNumber="77" authorityYear="1953" box="[151,606,764,790]" class="Insecta" family="Aeolothripidae" genus="Dactuliothrips" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="malloi">
