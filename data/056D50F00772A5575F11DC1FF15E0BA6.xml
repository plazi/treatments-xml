<geoCoordinate id="45C237809084864C0D285F792E38D6BD" direction="north" orientation="latitude" precision="925" value="16.1">16°06'N</geoCoordinate>
, 
<geoCoordinate id="75F815F52493DD8B9FF914468A240275" direction="east" orientation="longitude" precision="925" value="99.71667">99°43'E</geoCoordinate>
