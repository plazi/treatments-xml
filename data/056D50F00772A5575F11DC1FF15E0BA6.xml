<taxonomicName id="1ED6A96049CF3FC2C38528D5CE5A1031" class="Magnoliopsida" family="Fabaceae" genus="Acacia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Acacia erubescens" order="Fabales" pageId="11" pageNumber="76" phylum="Tracheophyta" rank="species" species="erubescens">Acacia erubescens</taxonomicName>
