<taxonomicName id="7748F5DAE6CD05039C94CAE023F3D18D" class="Magnoliopsida" family="Fabaceae" genus="Dichrostachys" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Dichrostachys cinerea" order="Fabales" pageId="11" pageNumber="76" phylum="Tracheophyta" rank="species" species="cinerea">Dichrostachys cinerea</taxonomicName>
) and Umbrella thorn trees (
<taxonomicName id="75BD15C05FDE3485564DFDFDAF778115" class="Magnoliopsida" family="Fabaceae" genus="Vachellia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vachellia tortilis" order="Fabales" pageId="11" pageNumber="76" phylum="Tracheophyta" rank="species" species="tortilis">Vachellia tortilis</taxonomicName>
