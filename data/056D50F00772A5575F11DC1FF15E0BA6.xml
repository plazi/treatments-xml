<geoCoordinate id="7F68917FD69B21343EC14C8FE4B6588D" direction="south" orientation="latitude" precision="9" value="-22.271166">22°16.27'S</geoCoordinate>
, 
<geoCoordinate id="C69614F9A5B32A223F69FEEF9E31C925" direction="east" orientation="longitude" precision="9" value="29.817">29°49.02'E</geoCoordinate>
