<taxonomicName id="5EEC4D5CFF99FFE5FCF3A045FBB4F9B8" authority="Kieffer, 1910" authorityName="Kieffer" authorityYear="1910" box="[792,1114,1559,1584]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="brasiliensis">
