<taxonomicName id="4C564D11A467FFD27888FF0FFCFB4086" authorityName="Gunther" authorityYear="1880" box="[700,853,193,216]" class="Mammalia" family="Talpidae" genus="Neurotrichus" kingdom="Animalia" order="Soricomorpha" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">
