<emphasis id="B922EA80A467FFD2795AFF0FFC514086" box="[878,1023,193,216]" italics="true" pageId="6" pageNumber="7">Parascalops</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A467FFD27E2CFF0FFB2F4086" authorityName="E. Geoffroy Saint-Hilaire" authorityYear="1803" box="[1048,1153,193,216]" class="Mammalia" family="Talpidae" genus="Scalopus" kingdom="Animalia" order="Soricomorpha" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A467FFD27E2CFF0FFB2F4086" box="[1048,1153,193,216]" italics="true" pageId="6" pageNumber="7">Scalopus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C564D11A467FFD27EADFF0FFCAF40A8" class="Mammalia" family="Talpidae" genus="Scapanus" kingdom="Animalia" order="Soricomorpha" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">
<emphasis id="B922EA80A467FFD27EADFF0FFCAF40A8" italics="true" pageId="6" pageNumber="7">Scapanus</emphasis>
</taxonomicName>
