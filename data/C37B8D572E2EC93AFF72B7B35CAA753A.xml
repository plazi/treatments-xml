<taxonomicName id="8CD247C22E2EC93BFF72B66A5CB6732F" authority="Bellier, 1861" authorityName="Bellier" authorityYear="1861" box="[151,403,1330,1352]" class="Insecta" family="Geometridae" genus="Hylaea" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="481" phylum="Arthropoda" rank="species" species="pinicolaria">
<emphasis id="79A6E0532E2EC93BFF72B66A5C2D732F" box="[151,264,1330,1352]" italics="true" pageId="12" pageNumber="481">pinicolaria</emphasis>
<materialsCitation id="FBBA361C2E2EC93BFEEBB66A5CB6732F" ID-GBIF-Occurrence="3312254304" box="[270,403,1330,1352]" collectorName="C. Oberthur" country="United Kingdom" location="UK" pageId="12" pageNumber="481" specimenCount="1" typeStatus="syntype">
<bibRefCitation id="2F4341B02E2EC93BFEEBB66A5CB6732F" author="Bellier, C." box="[270,403,1330,1352]" pageId="12" pageNumber="481" pagination="29 - 30" refId="ref10615" refString="Bellier, C. (1861) Description de trois lepidopteres nouveaux de l'ile de Corse. Annales de la Societ entomologique de France, 1, 29 - 30." type="journal article" year="1861">Bellier, 1861</bibRefCitation>
