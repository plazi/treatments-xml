<taxonomicName id="4C014C9DFFAEFFCB169AF6F9E4C5FB1F" authorityName=", Eyles and Schuh" authorityYear="2003" class="Insecta" family="Miridae" genus="Polyozus" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="galbanus">
