<taxonomicName id="4C014C9DFFAEFFCB163DF69AE291FB22" authorityName="WEIRAUCH" authorityYear="2007" box="[1068,1187,1195,1219]" class="Insecta" family="Miridae" genus="Pygovepres" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="manilla">
