<taxonomicName id="4C014C9DFFAEFFCB118DF6F9E275FB01" baseAuthorityName="Carvalho" baseAuthorityYear="1965" box="[924,1095,1224,1248]" class="Insecta" family="Miridae" genus="Orthotylus" kingdom="Animalia" order="Hemiptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="australianus">
