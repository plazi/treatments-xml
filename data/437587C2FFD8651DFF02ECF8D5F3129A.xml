<taxonomicName id="0CDC4D57FFD8651DFB4CED9DD1A111BA" authorityName="Reeve" authorityYear="1852" box="[1241,1419,1474,1500]" class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" order="Stylommatophora" pageId="28" pageNumber="29" phylum="Mollusca" rank="species" species="barbatula">
