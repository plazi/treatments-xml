<subSubSection id="5985CB94C52FFFAA6DD79CF3FEA259BF" pageId="2" pageNumber="135" type="etymology">
<paragraph id="1120981FC52FFFAA6DD79CF3FEA259BF" blockId="2.[175,1004,1006,1399]" pageId="2" pageNumber="135">
<emphasis id="23EB440DC52FFFAA6DD79CF3FEDF59A0" bold="true" box="[199,315,1165,1186]" pageId="2" pageNumber="135">Etymology.</emphasis>
