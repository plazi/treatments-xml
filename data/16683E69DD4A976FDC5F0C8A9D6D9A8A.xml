<paragraph id="12A803B634799D81D3048738EC87DA5D" pageId="8" pageNumber="99">
<taxonomicName id="904345B632F565F07E1866AF733709B6" class="Insecta" family="Liviidae" genus="Notophorina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Notophorina fusca" order="Hemiptera" pageId="8" pageNumber="99" phylum="Arthropoda" rank="species" species="fusca">Notophorina fusca</taxonomicName>
