<quantity id="9E569B15FFFC3F28C08C958AC3B2FDCB" box="[417,490,506,532]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="27" pageNumber="80" unit="m" value="100.0">100 m</quantity>
; coll. 
<collectionCode id="3FBFAE35FFFC3F28C300958AC02AFDCB" box="[557,626,506,532]" country="0" httpUri="http://grbio.org/cool/1q0g-mdtw" name="University of Massachusetts, Museum of Zoology" pageId="27" pageNumber="80">UMA</collectionCode>
