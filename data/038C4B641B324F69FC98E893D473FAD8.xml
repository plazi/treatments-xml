<subSubSection id="C33FA9F91B324F69FC98E893D4C9FC6C" box="[833,1196,966,986]" pageId="8" pageNumber="51" type="nomenclature">
<paragraph id="8B9AFA721B324F69FC98E893D4C9FC6C" blockId="8.[833,1196,966,986]" box="[833,1196,966,986]" pageId="8" pageNumber="51">
<heading id="D0D24D1E1B324F69FC98E893D4C7FC6C" box="[833,1186,966,986]" pageId="8" pageNumber="51">
<taxonomicName id="4C2581F11B324F69FC98E893D4C7FC6C" authority="(Pereira, 1954)" baseAuthorityName="Pereira" baseAuthorityYear="1954" box="[833,1186,966,986]" class="Insecta" family="Scarabaeidae" genus="Dichotomius" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="51" phylum="Arthropoda" rank="species" species="ribeiroi">
<emphasis id="B95126601B324F69FC98E893D3D9FC6C" box="[833,956,966,986]" italics="true" pageId="8" pageNumber="51">Dichotomius</emphasis>
<emphasis id="B95126601B324F69FC18E893D46DFC6C" box="[961,1032,966,986]" italics="true" pageId="8" pageNumber="51">ribeiroi</emphasis>
(
<bibRefCitation id="EFB487831B324F69FBCCE892D4FEFC6C" author="Pereira, F." box="[1045,1179,967,986]" journalOrPublisher="Arq. Mus. Nal." pageId="8" pageNumber="51" pagination="462 - 466" part="42" refId="ref10484" refString="Pereira, F., 1954. Especies novas de Scarabaeidae Brasileiros. Arq. Mus. Nal. 42, 462 - 466." title="Especies novas de Scarabaeidae Brasileiros" type="journal article" year="1954">Pereira, 1954</bibRefCitation>
