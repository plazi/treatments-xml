<number id="A8015F23FFC92D76FB63DAFC218B0EC0" box="[1211,1241,1966,1990]" pageId="4" pageNumber="163" value="31.0">31</number>
, collecting event 
<number id="A8015F23FFC92D76FF4FDA8325910EEC" box="[151,195,2001,2026]" pageId="4" pageNumber="163" value="515.0">515</number>
) (in 
<collectionCode id="78D8D5C1FFC92D76FED3DA8024090EEC" box="[267,347,2002,2026]" httpUri="http://grbio.org/cool/rsbi-vwmq" name="Swedish Museum of Natural History, Entomology Collections" pageId="4" pageNumber="163">NHRS</collectionCode>
