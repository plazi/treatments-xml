</emphasis>
<taxonomicName id="4C27E3B77E50FFDCFE81709FDC03FC37" authority="Hewitson, [1866]" authorityName="Hewitson" authorityYear="1866" box="[382,797,948,972]" class="Insecta" family="Hesperiidae" genus="Hesperia" kingdom="Animalia" order="Lepidoptera" pageId="34" pageNumber="33" phylum="Arthropoda" rank="species" species="coryna">
