<number id="3DA5695CD563FFDDFEE156F8977C2BCD" box="[356,382,1249,1270]" pageId="124" pageNumber="125" value="30.0">30</number>
%; frontal margin wrinkled, otherwise flat; ventral surface with usual ornamentation but no spinous processes.................................................................................. 
<taxonomicName id="4C6D00F8D563FFDDFB3956E7934B2A28" box="[1212,1353,1278,1299]" class="Maxillopoda" family="Monstrillidae" genus="Cymbasoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Monstrilloida" pageId="124" pageNumber="125" phylum="Arthropoda" rank="species" species="annulocolle" status="sp. nov.">
<emphasis id="B919A769D563FFDDFB3956E7934B2A28" box="[1212,1353,1278,1299]" italics="true" pageId="124" pageNumber="125">C. annulocolle</emphasis>
