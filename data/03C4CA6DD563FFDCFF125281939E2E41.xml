<keyLead id="309CD84ED563FFDDFF12572093992A75" box="[151,1435,1337,1358]" pageId="124" pageNumber="125">
<number id="3DA5695CD563FFDDFF12572096B32A75" box="[151,177,1337,1358]" pageId="124" pageNumber="125" value="10.0">10</number>
