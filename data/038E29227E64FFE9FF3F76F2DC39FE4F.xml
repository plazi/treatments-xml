<taxonomicName id="4C27E3B77E65FFE9FD8F7220DB31FED9" authority="Wallengren, 1857" authorityName="Wallengren" authorityYear="1857" box="[624,1071,266,290]" class="Insecta" family="Hesperiidae" genus="Hesperia" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="mohozutza">
<emphasis id="B95344267E65FFE9FD8F7220DC45FED9" box="[624,859,266,290]" italics="true" pageId="23" pageNumber="22">Hesperia mohozutza</emphasis>
Wallengren, 1857
</taxonomicName>
, 
<taxonomicName id="4C27E3B77E65FFE9FBC77220DA85FED9" authority="Butler, 1901" authorityName="Butler" authorityYear="1901" box="[1080,1435,266,290]" class="Insecta" family="Hesperiidae" genus="Kedestes" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="protensa">
<emphasis id="B95344267E65FFE9FBC77220DA1AFEDA" box="[1080,1284,266,290]" italics="true" pageId="23" pageNumber="22">Kedestes protensa</emphasis>
Butler, 1901
</taxonomicName>
, 
<taxonomicName id="4C27E3B77E65FFE9FF3F7201DEE0FEB8" authority="Trimen, 1873" authorityName="Trimen" authorityYear="1873" box="[192,510,298,323]" class="Insecta" family="Hesperiidae" genus="Pyrgus" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="22" phylum="Arthropoda" rank="species" species="chaca">
<emphasis id="B95344267E65FFE9FF3F7201DE45FEB9" box="[192,347,298,322]" italics="true" pageId="23" pageNumber="22">Pyrgus chaca</emphasis>
