<taxonomicName id="7B98FBFFFFA9FF9FFDE4C090FD21F930" box="[547,729,1727,1750]" class="Insecta" family="Mycetophilidae" genus="Paracycloneura" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">
