<collectorName id="2B17530F5406FF99FCE08A37FC04FDC1" box="[857,912,542,564]" pageId="1" pageNumber="692">Funde</collectorName>
aus 
<collectingRegion id="4426F83B5406FF99FC0E8A37FBAEFDC1" box="[951,1082,542,564]" country="Austria" name="Oberoesterreich" pageId="1" pageNumber="692">Oberösterreich</collectingRegion>
</materialsCitation>
