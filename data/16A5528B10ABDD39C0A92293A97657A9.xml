<paragraph id="B958758237135B6851B03C2918710464" pageId="null" pageNumber="452">
(
<taxonomicName id="5ED6DC3FCC0C7C548EC90CDCC0AA6DD2" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="452" phylum="Tracheophyta" rank="species" species="amara">
<emphasis id="A8BA8FD54BBE85B3F82E3CF872D33EB9" italics="true" pageId="null" pageNumber="452">C. amara</emphasis>
