<taxonomicName id="4C7A4D1FB789FF3C64007942DD9AFB11" box="[340,490,1136,1162]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Laboulbenia" kingdom="Fungi" order="Laboulbeniales" pageId="216" pageNumber="217" phylum="Ascomycota" rank="genus">
