<taxonomicName id="4C494D25C607FFA5BE2BFA4CFE4BFA26" box="[295,473,1508,1528]" class="Insecta" family="Staphylinidae" genus="Alevonota" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="242" phylum="Arthropoda" rank="species" species="hepatica">
