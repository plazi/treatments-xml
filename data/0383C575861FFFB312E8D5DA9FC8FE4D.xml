. Eastern Atlantic from 
<collectingCountry id="F33D34F3861FFFB01002D7D79EB8F842" box="[637,760,1929,1954]" name="Mauritania" pageId="28" pageNumber="179">Mauritania</collectingCountry>
(present work) to Namibian coasts (
<bibRefCitation id="EFBB0992861FFFB016C6D7D799CFF842" author="Macpherson" box="[1209,1423,1929,1954]" pageId="28" pageNumber="179" refString="Macpherson, E. (1983) Crustaceos decapodos capturados en las costas de Namibia. Resultados Expediciones Cientificas, 11, 3 - 80." type="journal article" year="1983">
Macpherson 
<number id="3DE26644861FFFB0172BD7D499CFF842" box="[1364,1423,1930,1954]" pageId="28" pageNumber="179" value="1983.0">1983</number>
</bibRefCitation>
), between 
<quantity id="4CD2D986861FFFB01284D7F39D02F825" box="[251,322,1965,1990]" metricMagnitude="2" metricUnit="m" metricValue="1.45" pageId="28" pageNumber="179" unit="m" value="145.0">145 m</quantity>
(
<bibRefCitation id="EFBB0992861FFFB0132FD7F09D98F826" author="Forest" box="[336,472,1965,1990]" pageId="28" pageNumber="179" refString="Forest, J. (1955) Crustaces Decapodes, Pagurides. Expedition Oceanographique Belge dans les Eaux Cotieres Africaines de L'Atlantique Sud (1948 - 1949). Resultats Scientifiques. Institut Royal des Sciences Naturelles de Belge, 3 (4), 23 - 147." type="journal article" year="1955">
Forest 
<number id="3DE26644861FFFB013E0D7F39D98F826" box="[415,472,1965,1990]" pageId="28" pageNumber="179" value="1955.0">1955</number>
</bibRefCitation>
) and 
<quantity id="4CD2D986861FFFB01068D7F39ED6F825" box="[535,662,1965,1990]" metricMagnitude="2" metricUnit="m" metricValue="3.63" metricValueMax="3.6399999999999997" metricValueMin="3.62" pageId="28" pageNumber="179" unit="m" value="363.0" valueMax="364.0" valueMin="362.0">362–364 m</quantity>
