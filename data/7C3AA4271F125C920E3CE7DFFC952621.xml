<taxonomicName id="4C42EC5CA19FA7CBEBFEEE6E7490C6CB" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182025" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole cuprina Wilson" lsidName-HNS="Pheidole cuprina Wilson" order="Hymenoptera" pageNumber="678" phylum="Arthropoda" rank="species" species="cuprina">Pheidole cuprina</taxonomicName>
