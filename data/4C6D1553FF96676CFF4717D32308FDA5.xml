<paragraph id="C47BA445FF96676BFF4717F02123F9FE" blockId="10.[151,920,1492,1551]" box="[151,381,1527,1551]" pageId="10" pageNumber="29">
(
<figureCitation id="5CFFB8C0FF96676BFF4F17F0217FF9FE" box="[159,289,1527,1551]" captionStart="FIGURES 38 – 42" captionStartId="12.[151,264,1967,1990]" captionTargetBox="[189,1419,217,1945]" captionTargetId="figure@12.[167,1419,193,1946]" captionTargetPageId="12" captionText="FIGURES 38 – 42. Andersonaltica valens. 38, habitus, dorsal view. 39, habitus, lateral view. 40, head. 41, median lobe of aedeagus, ventral and lateral views, holotype. 42, pronotum." httpUri="https://zenodo.org/record/246181/files/figure.png" pageId="10" pageNumber="29">
Figs 
<number id="720CB662FF96676BFF0717F0217FF9FE" box="[215,289,1527,1551]" pageId="10" pageNumber="29" value="40.0" valueMax="42.0" valueMin="38.0">38–42</number>
</figureCitation>
, Map 
<number id="720CB662FF96676BFEB617F0212CF9FE" box="[358,370,1527,1551]" pageId="10" pageNumber="29" value="1.0">1</number>
