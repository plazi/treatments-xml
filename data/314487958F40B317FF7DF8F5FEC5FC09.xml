<taxonomicName id="7EED4D008F43B317FD53FC68FC40FC2D" box="[697,900,943,968]" class="Gastropoda" family="Cyclophoridae" genus="Vargapupa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Architaenioglossa" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="oharai" status="n. sp.">
