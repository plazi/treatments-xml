<quantity id="7E159B668F43B317FED5FC55FE43FC40" box="[319,391,908,933]" metricMagnitude="4" metricUnit="m" metricValue="6.0" pageId="42" pageNumber="43" unit="km" value="60.0">60 km</quantity>
