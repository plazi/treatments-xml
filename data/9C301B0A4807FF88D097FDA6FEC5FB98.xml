<taxonomicName id="D399D19F4807FF88D51EFD72FE08FCB0" authority="(Linnaeus)" authorityName="Linnaeus" baseAuthorityName="Linnaeus" baseAuthorityYear="1771" class="Insecta" family="Dynastidae" genus="Allomyrina" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="487" phylum="Arthropoda" rank="species" species="dichotoma">
<emphasis id="26ED760E4807FF88D51EFD72FE88FCB1" italics="true" pageId="2" pageNumber="487">Allomyrina dichotoma</emphasis>
