<quantity id="4CAD36E1FF84FFAA1AFBF8B1FCD0F889" box="[732,825,1824,1850]" metricMagnitude="-3" metricUnit="m" metricValue="2.6" pageId="10" pageNumber="32" unit="mm" value="2.6">2.6 mm</quantity>
