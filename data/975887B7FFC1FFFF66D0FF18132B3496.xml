<taxonomicName id="D8F14D22FFC1FFFF6395F9BE135734B6" authorityName="Santos-Silva &amp; Heffern &amp; Matsuda" authorityYear="2010" class="Insecta" family="Cerambycidae" genus="Komiyandra" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="17" phylum="Arthropoda" rank="species" species="luzonica">
<emphasis id="2D85EAB3FFC1FFFF6395F9BE135734B6" italics="true" pageId="20" pageNumber="17">K. luzonica</emphasis>
