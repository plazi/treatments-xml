<taxonomicName id="D8F14D22FFC1FFFF65F8F99E160E3476" authorityName="Santos-Silva &amp; Heffern &amp; Matsuda" authorityYear="2010" box="[1000,1137,1603,1627]" class="Insecta" family="Cerambycidae" genus="Komiyandra" kingdom="Animalia" order="Coleoptera" pageId="20" pageNumber="17" phylum="Arthropoda" rank="species" species="luzonica">
<emphasis id="2D85EAB3FFC1FFFF65F8F99E160E3476" box="[1000,1137,1603,1627]" italics="true" pageId="20" pageNumber="17">K. luzonica</emphasis>
</taxonomicName>
