<taxonomicName id="12914D4AFFE88F38EAE9FB9AFDD6FB10" baseAuthorityName="Silvestri" baseAuthorityYear="1927" box="[407,520,1143,1166]" class="Insecta" family="Aphelinidae" genus="Encarsia" kingdom="Animalia" order="Hymenoptera" pageId="97" pageNumber="2194" phylum="Arthropoda" rank="species" species="strenua">E. strenua</taxonomicName>
</emphasis>
group.
