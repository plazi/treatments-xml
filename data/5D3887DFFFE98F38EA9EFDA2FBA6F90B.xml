<paragraph id="D52E36C9FFE98F38EBCBFD3CFBD2FF7F" blockId="96.[138,1185,591,777]" lastBlockId="97.[125,1172,137,291]" lastPageId="97" lastPageNumber="2194" pageId="96" pageNumber="2193">
<typeStatus id="0A2A886BFFE98F39EBCBFD3CFE82FD77" box="[181,348,721,745]" pageId="96" pageNumber="2193">Paralectotypes</typeStatus>
<specimenCount id="C397FD40FFE98F39EA1BFD3CFE5FFD77" box="[357,385,720,745]" pageId="96" pageNumber="2193" type="female">2♀</specimenCount>
