<taxonomicName id="77E44D584135FFD6FCFB66109E303A3C" box="[890,1045,837,862]" class="Insecta" family="Drosophilidae" genus="Drosophila" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="61" phylum="Arthropoda" rank="species" species="conformis">
