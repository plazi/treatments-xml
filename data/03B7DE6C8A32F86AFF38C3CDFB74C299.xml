<number id="3DD67D5D8A32F86AFAA2C60EFAE0C16D" box="[1293,1321,1065,1089]" pageId="29" pageNumber="30" value="19.0">19</number>
</figureCitation>
E), 
<number id="3DD67D5D8A32F86AFAFBC60EFF1CC149" pageId="29" pageNumber="30" value="100.0" valueMax="117.5" valueMin="82.5">82.5 – 117.5</number>
x 
<number id="3DD67D5D8A32F86AFF55C66AFED4C149" box="[250,285,1101,1125]" pageId="29" pageNumber="30" value="2.5">2.5</number>
