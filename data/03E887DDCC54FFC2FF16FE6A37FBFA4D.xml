</paragraph>
<paragraph id="8BFE36CBCC54FFC2FF16FE2D3183FDE6" blockId="1.[151,1180,508,533]" box="[151,1180,508,533]" pageId="1" pageNumber="182">
<taxonomicName id="4C414D48CC54FFC2FF16FE2D3480FDE6" authority="Townes, 1946: 58" authorityName="Townes" authorityPageNumber="58" authorityYear="1946" box="[151,415,508,533]" class="Insecta" family="Ichneumonidae" genus="Mnioes" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="182" phylum="Arthropoda" rank="genus">
