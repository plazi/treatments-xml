<emphasis id="B94CEACD360DFFB36199F8CFD67C9120" bold="true" box="[151,290,1827,1849]" pageId="20" pageNumber="81">FIGURE 12.</emphasis>
<taxonomicName id="4C384D5C360DFFB36024F8CFD55C9121" authorityName="Rioja" authorityYear="1947" box="[298,514,1827,1849]" class="Polychaeta" family="Chrysopetalidae" genus="Paleanotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="20" pageNumber="81" phylum="Annelida" rank="species" species="purpurea">
