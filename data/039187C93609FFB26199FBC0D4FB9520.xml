<taxonomicName id="4C384D5C360AFFB4602BF890D68B918A" baseAuthorityName="Johnson" baseAuthorityYear="1897" box="[293,469,1916,1938]" class="Polychaeta" family="Chrysopetalidae" genus="Paleanotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="19" pageNumber="80" phylum="Annelida" rank="species" species="bellis">
