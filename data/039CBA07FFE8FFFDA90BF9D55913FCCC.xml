<taxonomicName id="4C357092FFEFFFFDA960FCD35B4CFCE9" authorityName="Krommelbein" authorityYear="1967" box="[576,789,825,852]" class="Ostracoda" family="Krithidae" genus="Aracajuia" kingdom="Animalia" order="Podocopida" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="benderi">
<emphasis id="B941D703FFEFFFFDA960FCD35B4CFCE9" box="[576,789,825,852]" italics="true" pageId="20" pageNumber="21">Aracajuia benderi</emphasis>
</taxonomicName>
