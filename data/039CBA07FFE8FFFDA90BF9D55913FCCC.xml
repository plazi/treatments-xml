<taxonomicName id="4C357092FFEFFFFDA8F0FCD05CE1FCE9" box="[976,1208,825,851]" class="Ostracoda" family="Trachyleberididae" genus="Sergipella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="viviersae">
<emphasis id="B941D703FFEFFFFDA8F0FCD05CE1FCE9" box="[976,1208,825,851]" italics="true" pageId="20" pageNumber="21">Sergipella viviersae</emphasis>
