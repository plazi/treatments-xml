<taxonomicName id="B88F4D71FFC5FF9FFEEDF8CEFE484B01" authorityName="Bologna &amp; Amore &amp; Pitzalis" authorityYear="2018" box="[293,446,1804,1826]" class="Insecta" family="Meloidae" genus="Hycleus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="aridus">
