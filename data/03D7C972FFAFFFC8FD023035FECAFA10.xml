<paragraph id="8BC17864FFB0FFC8FF39374CFDB1FB16" blockId="48.[189,1399,1145,1242]" pageId="48" pageNumber="49">
<typeStatus id="54C5C6C6FFB0FFC8FF39374CFEEEFB5F" box="[189,303,1145,1171]" pageId="48" pageNumber="49">Paratypes</typeStatus>
