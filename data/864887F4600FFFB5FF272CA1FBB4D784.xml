<collectingCountry id="76F67672600FFFB4FEDC2B6CFE71D7FA" box="[364,457,1904,1930]" name="Panama" pageId="10" pageNumber="11">Panama</collectingCountry>
and 
<collectingCountry id="76F67672600FFFB4FDB22B6CFDC6D7FA" box="[514,638,1904,1930]" name="Venezuela" pageId="10" pageNumber="11">Venezuela</collectingCountry>
