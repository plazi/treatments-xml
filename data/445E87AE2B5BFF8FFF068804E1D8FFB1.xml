<quantity id="0B0F9B5D2B5BFF8FFC6B889DE054FD9E" box="[941,1068,1422,1449]" metricMagnitude="-2" metricUnit="m" metricValue="2.9" metricValueMax="3.6" metricValueMin="2.2" pageId="14" pageNumber="13" unit="mm" value="29.0" valueMax="36.0" valueMin="22.0">22–36 mm</quantity>
