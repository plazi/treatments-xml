<typeStatus id="54EE15BDFFD9F761FF024B0609B4FB8F" box="[192,253,1124,1148]" pageId="6" pageNumber="5">Type</typeStatus>
species: 
<taxonomicName id="4C55D09CFFD9F761FEA74B060A43FB88" authority="Fabricius 1775" authorityName="Fabricius" authorityYear="1775" box="[357,778,1123,1147]" class="Insecta" family="Geotrupidae" genus="Geotrupes" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="sylvanus">
