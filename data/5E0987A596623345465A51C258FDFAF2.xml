, 
<quantity id="11589B5696623345452552E45E10F90E" box="[542,626,1667,1688]" metricMagnitude="3" metricUnit="m" metricValue="1.0" pageId="4" pageNumber="667" unit="m" value="1000.0">1000 m</quantity>
