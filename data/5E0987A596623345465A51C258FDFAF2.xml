<collectionCode id="B0B1AE76966233454655537B5DC4F8A7" box="[366,422,1820,1841]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="4" pageNumber="667" type="Museum">MCZ</collectionCode>
DNA102051
<emphasis id="E4D4EAA1966233454502537C5E21F8A7" bold="true" box="[569,579,1819,1841]" pageId="4" pageNumber="667">)</emphasis>
from Tura trail (
<geoCoordinate id="B39450749662334547AC535D5D7EF8C6" box="[151,284,1849,1872]" degrees="25" direction="north" minutes="30" orientation="latitude" pageId="4" pageNumber="667" precision="15" seconds="28" value="25.507778">25°30′28′′N</geoCoordinate>
, 
<geoCoordinate id="B3945074966233454613535D5DC8F8C6" box="[296,426,1849,1872]" degrees="90" direction="east" minutes="13" orientation="longitude" pageId="4" pageNumber="667" precision="15" seconds="54" value="90.23167">90°13′54′′E</geoCoordinate>
), Tura Peak, West Garo Hills District, 
<taxonomicName id="11A04D309662334547C3533E5D1BF8F9" box="[248,377,1881,1903]" class="Arachnida" family="Stylocellidae" genus="Meghalaya" kingdom="Animalia" order="Opiliones" pageId="4" pageNumber="669" phylum="Arthropoda" rank="genus">Meghālaya</taxonomicName>
, 
<collectingCountry id="AEB776239662334546B3533E5DA5F8F9" box="[392,455,1881,1903]" name="India" pageId="4" pageNumber="667">India</collectingCountry>
