<collectionCode id="B0B1AE76966233454637537B5D01F8A7" box="[268,355,1820,1841]" country="Switzerland" httpUri="http://grbio.org/cool/2jyg-xmd2" name="Museum d'Histoire Naturelle" pageId="4" pageNumber="667" type="Museum">MHNG</collectionCode>
