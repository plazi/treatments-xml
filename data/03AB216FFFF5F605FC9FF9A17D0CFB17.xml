<quantity id="4CFA3D9CFFF4F602FEC1F90E7DD0F94E" box="[289,390,1758,1780]" metricMagnitude="-3" metricUnit="m" metricValue="8.43" pageId="27" pageNumber="704" unit="mm" value="8.43">8.43 mm</quantity>
, branch tip diameter 5.59–5.75– 
<quantity id="4CFA3D9CFFF4F602FF44F92D7D5CF8A8" box="[164,266,1789,1811]" metricMagnitude="-3" metricUnit="m" metricValue="5.91" pageId="27" pageNumber="704" unit="mm" value="5.91">5.91 mm</quantity>
