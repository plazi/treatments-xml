<quantity id="4CFA3D9CFFF4F602FE43F9107DB4F96F" box="[419,482,1728,1749]" metricMagnitude="1" metricUnit="m" metricValue="1.7" pageId="27" pageNumber="704" unit="m" value="17.0">17 m</quantity>
