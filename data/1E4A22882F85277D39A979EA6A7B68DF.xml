<taxonomicName id="697C70896FD4DB95587D7AF9D2AA3153" authorityName="Crowson" authorityYear="1964" lsidName="" pageId="137" pageNumber="138" rank="tribe" tribe="Lophocaterini">Lophocaterini</taxonomicName>
