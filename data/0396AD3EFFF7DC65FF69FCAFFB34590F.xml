<collectionCode id="ED2E84EDFFF7DC65FECEFB63FEF958B4" box="[260,343,1255,1278]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="16" pageNumber="659">MNHN</collectionCode>
