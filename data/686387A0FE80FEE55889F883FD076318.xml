<taxonomicName id="27CA4D35FE83FEE55A72FE72FCBC6360" authorityName="Rondani" authorityYear="1847" box="[620,783,511,537]" class="Insecta" family="Tachinidae" genus="Mintho" kingdom="Animalia" order="Diptera" pageId="334" pageNumber="335" phylum="Arthropoda" rank="species" species="lacera">
