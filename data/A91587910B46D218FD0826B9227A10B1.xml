<taxonomicName id="E6BC4D040B47D21FFEAC224A21EA131E" box="[278,470,1369,1389]" class="Insecta" family="Carabidae" genus="Styphlomerus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="467" phylum="Arthropoda" rank="species" species="undetermined">
