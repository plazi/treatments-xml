<taxonomicName id="4C6D13AD34653F5DFF7AF8F8FEF9608A" authorityName=", Lucena" authorityYear="1987" box="[129,306,1681,1705]" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="31" pageNumber="224" phylum="Chordata" rank="species" species="gibbosus">
