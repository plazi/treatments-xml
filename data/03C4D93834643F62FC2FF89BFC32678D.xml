<taxonomicName id="4C6D13AD34653F5DFD11F878FF24606B" authorityName="Eigenmann" authorityYear="1912" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="31" pageNumber="224" phylum="Chordata" rank="species" species="rupununi">
