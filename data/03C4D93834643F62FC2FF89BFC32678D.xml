<taxonomicName id="4C6D13AD34653F5DFE9AF8F8FE15608B" authorityName="Eigenmann" authorityYear="1912" box="[353,478,1681,1704]" class="Actinopterygii" family="Characidae" genus="Charax" kingdom="Animalia" order="Characiformes" pageId="31" pageNumber="224" phylum="Chordata" rank="species" species="rupununi">
