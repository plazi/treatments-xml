<taxonomicName id="4C7C4D6AD21F9D323862DE8AED1FF836" box="[574,879,1970,1996]" class="Dinophyceae" family="Peridiniaceae" genus="Glenodinium" higherTaxonomySource="GBIF" kingdom="Chromista" order="Peridiniales" pageId="10" pageNumber="145" phylum="Myzozoa" rank="species" species="gymnodinium">
<emphasis id="B908EAFBD21F9D323862DE8AED1FF836" box="[574,879,1970,1996]" italics="true" pageId="10" pageNumber="145">Glenodinium gymnodinium</emphasis>
