</geoCoordinate>
, 
<quantity id="4CDDDFF1FFF97D11FBA33F6C6F24FEB6" box="[1144,1309,284,310]" metricMagnitude="3" metricUnit="m" metricValue="5.154999999999999" metricValueMax="5.585" metricValueMin="4.725" pageId="3" pageNumber="4" unit="m" value="5155.0" valueMax="5585.0" valueMin="4725.0">4725–5585 m</quantity>
