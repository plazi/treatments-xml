</geoCoordinate>
, 
<quantity id="4CDDDFF1FFF87D11FB3A39386B58FEB6" lastPageId="3" lastPageNumber="4" metricMagnitude="3" metricUnit="m" metricValue="3.47" metricValueMax="3.67" metricValueMin="3.27" pageId="2" pageNumber="3" unit="m" value="3470.0" valueMax="3670.0" valueMin="3270.0">3270– 3670 m</quantity>
