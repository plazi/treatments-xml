<taxonomicName id="4C6B4D61FF95093FFACC6B25FC62FD01" class="Lycopodiopsida" family="Lycopodiaceae" genus="Lycopodiella" kingdom="Plantae" order="Lycopodiales" pageId="2" pageNumber="247" phylum="Tracheophyta" rank="species" species="pendulina">
<emphasis id="B91FEAF0FF95093FFACC6B25FC62FD01" italics="true" pageId="2" pageNumber="247">Lycopodiella pendulina</emphasis>
</taxonomicName>
