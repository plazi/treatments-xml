<emphasis id="B933EA8D3B6FA1406AECF8E5AF5DF8C3" bold="true" box="[151,350,1880,1907]" pageId="48" pageNumber="49">
<taxonomicName id="4C474D1C3B6FA1406AECF8E5AEEAF8C3" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[151,233,1880,1907]" class="Arachnida" family="Gnaphosidae" genus="Kituba" kingdom="Animalia" order="Araneae" pageId="48" pageNumber="49" phylum="Arthropoda" rank="genus" status="gen. nov.">
