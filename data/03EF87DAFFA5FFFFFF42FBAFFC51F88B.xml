<specimenCount id="9D40FD45FFA5FFFFFC3FF9B4FBC1F95F" box="[1002,1124,1657,1683]" pageId="12" pageNumber="578" type="female">21 females</specimenCount>
&amp; 
<specimenCount id="9D40FD45FFA5FFFFFB53F9B4FB7EF95F" box="[1158,1243,1657,1683]" pageId="12" pageNumber="578" type="male">7 males</specimenCount>
