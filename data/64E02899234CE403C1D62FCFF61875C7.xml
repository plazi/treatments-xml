<paragraph id="896DBBB8D0629A42591406DBD7E354E0">
<taxonomicName id="0B6A6DD2D8ED8C55F83776392A7CDA75" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:180342" class="Insecta" family="Formicidae" genus="Cataulacus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cataulacus intrudens tristiculus Santschi" lsidName-HNS="Cataulacus intrudens tristiculus Santschi" order="Hymenoptera" phylum="Arthropoda" rank="subSpecies" species="intrudens" subSpecies="tristiculus">Cataulacus intrudens Sm. st. tristiculus</taxonomicName>
<taxonomicNameLabel id="A888DCF1F70697426D5875D391365BC6" rank="subSpecies">n. st.</taxonomicNameLabel>
