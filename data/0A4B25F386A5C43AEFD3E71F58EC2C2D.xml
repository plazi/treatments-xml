<taxonomicName id="B04C042F48A95542312B3371C82D42C3" LSID="http://zoobank.org/1F0CC178-2E35-4E59-8BAC-8E73F1983095" class="Insecta" family="Carabidae" genus="Parorthomus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Parorthomus" order="Coleoptera" pageId="19" pageNumber="40" phylum="Arthropoda" rank="genus">Parorthomus</taxonomicName>
