<collectionCode id="99CDAE2C090EFFA4FD12576FB361FE31" box="[733,804,378,404]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="16" pageNumber="29" type="Museum">MCZ</collectionCode>
