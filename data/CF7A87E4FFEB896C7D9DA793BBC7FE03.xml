<taxonomicName id="80D34D71FFE8896C7E89A3E8BE01FDD0" authorityName="Oman" authorityYear="1949" box="[899,1058,461,487]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="tribe" tribe="Xerophloeini">Xerophloeini</taxonomicName>
