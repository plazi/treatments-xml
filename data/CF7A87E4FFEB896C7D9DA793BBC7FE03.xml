</taxonomicName>
(= 
<taxonomicName id="80D34D71FFE8896C7FEDA3E8B977FDD1" box="[743,852,461,486]" class="Insecta" family="Hemiptera (awaiting allocation)" genus="Epiclines" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="genus">
