<taxonomicName id="80D34D71FFE8896C7F52A3E8B8E6FDD1" authorityName="Spinola" authorityYear="1850" box="[600,709,461,486]" class="Insecta" family="Cicadellidae" genus="Proranus" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="genus">
