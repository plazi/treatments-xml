<taxonomicName id="80D34D71FFE8896C794CA383BEBCFDF7" authorityName="Evans" authorityYear="1947" box="[1094,1183,422,448]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="tribe" tribe="Ledrini">Ledrini</taxonomicName>
