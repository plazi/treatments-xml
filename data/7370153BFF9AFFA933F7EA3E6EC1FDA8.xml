, width 
<quantity id="3C2109C8FF98FFA93119EE6F68ECFDE0" box="[633,721,512,536]" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="34" pageNumber="35" unit="mm" value="1.4">1.4 mm</quantity>
, height 
