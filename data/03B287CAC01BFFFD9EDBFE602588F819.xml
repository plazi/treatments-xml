<collectingDate id="EFE1E9F4C01BFFFD9A93FA2824B8F9E1" box="[1247,1431,1510,1536]" pageId="9" pageNumber="300" value="1971-08-11" valueMax="1971-08-14" valueMin="1971-08-11">11-14.VIII.1971</collectingDate>
</date>
, 
<collectorName id="26EE530AC01BFFFD9EDBF9C42011F9C5" box="[151,318,1546,1573]" pageId="9" pageNumber="300">C. &amp; M. Vardy</collectorName>
col., at light, 
<specimenCode id="DBBD9EA7C01BFFFD9F95F9C52355F9C5" box="[473,634,1545,1573]" collectionCode="BM" country="United Kingdom" name="Bristol Museum" pageId="9" pageNumber="300">BM 1971-533</specimenCode>
; in the 
<collectorName id="26EE530AC01BFFFD9C87F9C422AEF9C4" box="[715,897,1546,1573]" pageId="9" pageNumber="300">British Museum</collectorName>
(Natural History), 
<collectorName id="26EE530AC01BFFFD9A1BF9C42599F9C5" box="[1111,1206,1546,1572]" pageId="9" pageNumber="300">London.</collectorName>
