<paragraph id="8BAF242D5C755909FF48FC9A20BCFCF2" blockId="82.[151,497,799,859]" box="[151,497,834,859]" pageId="82" pageNumber="83">
<materialsCitation id="3B782E705C755909FF48FC9A20BCFCF2" ID-GBIF-Occurrence="1657189032" box="[151,497,834,859]" location="Costa" pageId="82" pageNumber="83" specimenCount="1" stateProvince="Costa">
= 
<taxonomicName id="4C105FAE5C755909FF79FC9D2040FCF3" box="[166,269,836,859]" class="Insecta" family="Tenthredinidae" genus="Pontania" kingdom="Animalia" order="Hymenoptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="genus">
