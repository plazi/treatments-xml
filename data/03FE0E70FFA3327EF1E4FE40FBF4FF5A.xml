<number id="3D9FAD41FFA33274F524F99EFBD0F9C6" box="[1111,1124,1578,1604]" pageId="3" pageNumber="24" value="6.0">6</number>
-segmented with 
<number id="3D9FAD41FFA33274F45CF99EFAE9F9C6" box="[1327,1373,1578,1604]" pageId="3" pageNumber="24" value="2.4">2, 4</number>
, 
<number id="3D9FAD41FFA33274F41AF99EFA23F9C6" box="[1385,1431,1578,1604]" pageId="3" pageNumber="24" value="2.2">2, 2</number>
, 
<number id="3D9FAD41FFA33274F1E4F9E5FF1CF9E9" box="[151,168,1617,1643]" pageId="3" pageNumber="24" value="2.0">2</number>
