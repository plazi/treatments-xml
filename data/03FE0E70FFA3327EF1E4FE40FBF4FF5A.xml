<number id="3D9FAD41FFA33274F3ACFB7AFD5AFB6A" box="[735,750,1230,1256]" pageId="3" pageNumber="24" value="2.0">2</number>
with 
<number id="3D9FAD41FFA33274F243FB7AFC8BFB6A" box="[816,831,1230,1256]" pageId="3" pageNumber="24" value="9.0">9</number>
