<number id="3D9FAD41FFA33274F2E3FAA8FC2BFAB4" box="[912,927,1308,1334]" pageId="3" pageNumber="24" value="5.0">5</number>
setae; exopod with 
<number id="3D9FAD41FFA33274F5FBFAA8FB22FAB4" box="[1160,1174,1308,1334]" pageId="3" pageNumber="24" value="7.0">7</number>
