<number id="3D9FAD41FFA33274F5A6FB80FAB6FBCC" box="[1237,1282,1076,1102]" pageId="3" pageNumber="24" value="0.1">0, 1</number>
, 
<number id="3D9FAD41FFA33274F463FB80FA88FBCC" box="[1296,1340,1076,1102]" pageId="3" pageNumber="24" value="1.1">1, 1</number>
