<number id="3D9FAD41FFA33274F438FB80FAC3FBCC" box="[1355,1399,1076,1102]" pageId="3" pageNumber="24" value="1.1">1, 1</number>
, 
<number id="3D9FAD41FFA33274F4F6FB80FA20FBCC" box="[1413,1428,1076,1102]" pageId="3" pageNumber="24" value="1.0">1</number>
