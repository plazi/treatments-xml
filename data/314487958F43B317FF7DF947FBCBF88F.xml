<quantity id="7E159B668F43B317FD81F8F7FD02F8A3" box="[619,710,1838,1862]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="42" pageNumber="43" unit="mm" value="2.4">2.4 mm</quantity>
