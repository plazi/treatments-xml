<quantity id="7E159B668F43B317FE5EF8F4FDCAF8A0" box="[436,526,1837,1862]" metricMagnitude="-3" metricUnit="m" metricValue="6.4" pageId="42" pageNumber="43" unit="mm" value="6.4">6.4 mm</quantity>
