<taxonomicName id="22A5BDF1FFA6FF95025BFDE4FDCEFDFA" authority="(Caporiacco)" baseAuthorityName="Caporiacco" baseAuthorityYear="1954" class="Arachnida" family="Theraphosidae" genus="Neostenotarsus" kingdom="Animalia" order="Araneae" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="guianensis">
