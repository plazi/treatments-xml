<quantity id="4CD20FD77C5FFFDBFEE2FE213C90E0C4" box="[337,400,450,470]" metricMagnitude="3" metricUnit="m" metricValue="1.95" pageId="4" pageNumber="1435" unit="m" value="1950.0">1950m</quantity>
