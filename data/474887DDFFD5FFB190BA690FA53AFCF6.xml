<taxonomicName id="08E14D48FFD5FFB192F76B46A707FCD5" box="[730,823,773,796]" class="Insecta" family="Curculionidae" genus="Xyleborus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="protii">
