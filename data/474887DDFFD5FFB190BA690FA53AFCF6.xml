</taxonomicName>
, and 
<taxonomicName id="08E14D48FFD5FFB1935B6B46A7DDFCD5" box="[886,1005,773,796]" class="Insecta" family="Curculionidae" genus="Xyleborus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="viaticus">
