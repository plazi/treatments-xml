<typeStatus id="105A8869FFD5FFB191A66B47A5CEFCD5" box="[395,510,772,796]" pageId="26" pageNumber="27">Holotypes</typeStatus>
of 
<taxonomicName id="08E14D48FFD5FFB192096B46A6FBFCD5" box="[548,715,773,796]" class="Insecta" family="Curculionidae" genus="Xyleborus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="canarivorus">
