<location id="453D6007CC2CFF8FEBBB05130BDBF03E" LSID="urn:lsid:plazi:treatment:C84B87CACC2CFF8FE8DD03930BD7F03E:453D6007CC2CFF8FEBBB05130BDBF03E" box="[1009,1220,1364,1389]" municipality="East Antarctic" name="Possession Islands" pageId="58" pageNumber="59" stateProvince="Cape">Possession Islands</location>
