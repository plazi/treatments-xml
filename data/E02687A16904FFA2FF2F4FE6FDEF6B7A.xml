<taxonomicName id="AF8F4D346905FFA2FE0449B5FD256C4E" box="[444,681,249,273]" class="Malacostraca" family="Atyidae" genus="Troglocaris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="anophthalmus" subGenus="Troglocaris">
