.– 
<date id="FF9E37DD821CFF999FB1CB87CA43FC7E" box="[151,270,946,972]" pageId="9" pageNumber="66" value="1987-05-20">
<number id="3DE8033A821CFF999FB1CB87CB83FC7E" box="[151,206,946,972]" pageId="9" pageNumber="66" value="20.5">20.5</number>
. 
<number id="3DE8033A821CFF999FF7CB87CA43FC7E" box="[209,270,946,972]" pageId="9" pageNumber="66" value="1987.0">1987</number>
</date>
