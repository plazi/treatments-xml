<elevation id="005BD1F7FF94AB0D9C64179D44E83FF1" box="[571,693,1976,2002]" metricMagnitude="2" metricUnit="m" metricValue="3.44" metricValueMax="6.029999999999999" metricValueMin="0.85" pageId="120" pageNumber="121" unit="m" value="344.0" valueMax="603.0" valueMin="85.0">85–603 m</elevation>
