<collectingDate id="3C74486AFFD75814FC672A91FB884446" box="[976,1107,764,788]" pageId="87" pageNumber="88" value="1960-11-18">18.XI.1960</collectingDate>
</date>
; 
<specimenCount id="4E885CCBFFD75814FBD62A91FB564441" box="[1121,1165,764,788]" pageId="87" pageNumber="88" type="male">1 ♂</specimenCount>
(
<specimenCode id="08283F39FFD75814FB282A91FB224446" box="[1183,1273,764,788]" collectionCode="SB" pageId="87" pageNumber="88">SB 329</specimenCode>
, was found in separate vial, not in the same vial as 
<specimenCount id="4E885CCBFFD75814FD852B4DFD9C4465" box="[562,583,800,823]" pageId="87" pageNumber="88" type="female">♀</specimenCount>
<specimenCode id="08283F39FFD75814FDF92B72FD7F4465" box="[590,676,799,823]" collectionCode="SB" pageId="87" pageNumber="88">SB 331</specimenCode>
), 
<collectingRegion id="9A4A59A0FFD75814FD0E2B4DFCC94465" box="[697,786,800,823]" country="Ireland" name="Monaghan" pageId="87" pageNumber="88">MNHN</collectingRegion>
