<collectingDate id="AF54E9FEA858FF87FDC5F97EFEB20FC4" pageId="64" pageNumber="64" value="1927-02-16">16 February 1927</collectingDate>
</date>
. 
<location id="CE71600DA858FF87FE8BF91CFE170FC5" LSID="urn:lsid:plazi:treatment:430787C0A858FF87FF02FE2CFB8E0BA5:CE71600DA858FF87FE8BF91CFE170FC5" box="[286,426,1737,1759]" country="Guatemala" county="He" municipality="Finca Sepacuite" name="Underdown" pageId="64" pageNumber="64">Underdown</location>
