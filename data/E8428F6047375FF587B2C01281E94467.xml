<taxonomicName id="22EFF0A841F6791C0467AF5559F1B2E7" baseAuthorityName="Morley" baseAuthorityYear="1913" class="Insecta" family="Ichneumonidae" genus="Enicospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enicospilus orientalis" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="orientalis">Enicospilus orientalis</taxonomicName>
