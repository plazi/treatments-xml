<taxonomicName id="FB327813EF5B8D17193E6A96B142C640" baseAuthorityName="Morley" baseAuthorityYear="1913" class="Insecta" family="Ichneumonidae" genus="Enicospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Enicospilus orientalis" order="Hymenoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="orientalis">Enicospilus orientalis</taxonomicName>
