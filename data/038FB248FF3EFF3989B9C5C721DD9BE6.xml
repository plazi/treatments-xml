<taxonomicName id="4C2678DDFF3EFF398BFCC32623FC9B3E" box="[722,950,1333,1358]" class="Actinopterygii" family="Hexagrammidae" genus="Ophiodon" kingdom="Animalia" order="Scorpaeniformes" pageId="143" pageNumber="144" phylum="Chordata" rank="species" species="elongatus">
