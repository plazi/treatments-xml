<taxonomicName id="43CB4D18FFEC215EFC4AFD6CFB94FD78" box="[959,1053,653,677]" class="Insecta" family="Aphodiidae" genus="Aphodius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="45" phylum="Arthropoda" rank="genus">
