, 
<taxonomicName id="43CB4D18FFEC215EFBDCFD6CFB1DFD78" authorityName="Hope" authorityYear="1837" box="[1065,1172,653,677]" class="Insecta" family="Scarabaeidae" genus="Catharsius" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="45" phylum="Arthropoda" rank="genus">
