<taxonomicName id="CDA04D5FAF5AFFE98187FE5EFEBAFE5A" authority="Cope ED, 1876" authorityName="Cope ED" authorityYear="1876" baseAuthorityName="Cope" box="[149,364,437,458]" class="Amphibia" family="Amblystomidae" genus="Scapherpeton" higherTaxonomySource="GBIF" kingdom="Animalia" order="Caudata" pageId="16" pageNumber="356" phylum="Chordata" rank="species" species="laticolle" status="sp. nov.">
<emphasis id="38D4EACEAF5AFFE98187FE5EFEBAFE5A" bold="true" box="[149,364,437,458]" pageId="16" pageNumber="356">Seapharpeton laticolle</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="23E757B5AF5AFFE98069FE51FE16FE55" box="[379,448,442,457]" pageId="16" pageNumber="356" rank="species">
