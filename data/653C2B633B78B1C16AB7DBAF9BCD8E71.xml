(circles) and 
<taxonomicName id="3352ADD179B9A8A2C1B7E7FBD086C146" class="Insecta" family="Nemouridae" genus="Prostoia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Prostoia completa" order="Plecoptera" pageId="7" pageNumber="18" phylum="Arthropoda" rank="species" species="completa">Prostoia completa</taxonomicName>
