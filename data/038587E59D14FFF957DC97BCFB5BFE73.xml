<subSubSection id="C33665789D0BFFF9578C945CFD5BFE0F" box="[199,653,369,395]" pageId="16" pageNumber="17" type="etymology">
<paragraph id="8B9336F39D0BFFF9578C945CFD5BFE0F" blockId="16.[151,1436,153,503]" box="[199,653,369,395]" pageId="16" pageNumber="17">
<emphasis id="B958EAE19D0BFFF9578C945CFE98FE0F" bold="true" box="[199,334,369,395]" pageId="16" pageNumber="17">Etymology.</emphasis>
