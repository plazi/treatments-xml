<paragraph id="F6681C052A45FF8BFF6CFEEAFA58FE7B" blockId="13.[151,1437,222,928]" pageId="13" pageNumber="14">
<keyLead id="4D26BF302A45FF8BFF6CFEEAFA58FE7B" pageId="13" pageNumber="14">
<number id="401F0E222A45FF8BFF6CFEEAFF66FE55" box="[151,165,371,392]" pageId="13" pageNumber="14" value="3.0">3</number>
