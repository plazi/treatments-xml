<collectingCountry id="F324765FFFA6FFD2FD7EFEB818E01193" box="[659,722,358,384]" name="Italy" pageId="5" pageNumber="6">Italy</collectingCountry>
:
</emphasis>
<number id="3DFB24E8FFA6FFD2FD0CFEB918C21192" box="[737,752,359,385]" pageId="5" pageNumber="6" value="1.0">1</number>
ɗ, 
<number id="3DFB24E8FFA6FFD2FCF9FEB919111192" box="[788,803,359,385]" pageId="5" pageNumber="6" value="1.0">1</number>
Ψ, 
<number id="3DFB24E8FFA6FFD2FCAAFEB919671192" box="[839,853,359,385]" pageId="5" pageNumber="6" value="5.0">5</number>
ex., Basilicata, Oasi WWF, Pantano di Pignola (PZ), 
<quantity id="4CCB9B2AFFA6FFD2FE40FE511BC111BA" box="[429,499,399,425]" metricMagnitude="2" metricUnit="m" metricValue="7.7" pageId="5" pageNumber="6" unit="m" value="770.0">770m</quantity>
, under 
<taxonomicName id="4C334D4CFFA6FFD2FDA4FE5118B111BB" box="[585,643,399,424]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="5" pageNumber="6" phylum="Chordata" rank="genus">
<emphasis id="B947EADDFFA6FFD2FDA4FE5118B111BB" box="[585,643,399,424]" italics="true" pageId="5" pageNumber="6">Salix</emphasis>
</taxonomicName>
