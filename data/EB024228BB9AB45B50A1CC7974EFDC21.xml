<taxonomicName id="71E2837FEE3AE1854E4724D12F398B94" class="Insecta" family="Choreutidae" genus="Anthophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthophila magnus" order="Lepidoptera" pageId="0" pageNumber="8050" phylum="Arthropoda" rank="species" species="magnus">magnus</taxonomicName>
relative to 
<taxonomicName id="FD02824D271E610CF94A12611E4C54A4" class="Insecta" family="Choreutidae" genus="Anthophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthophila lucorum" order="Lepidoptera" pageId="0" pageNumber="8050" phylum="Arthropoda" rank="species" species="lucorum">lucorum</taxonomicName>
