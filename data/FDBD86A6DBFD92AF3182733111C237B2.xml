<taxonomicName id="38D8D0A4A725E1A55E00E7FDEEE018C7" class="Insecta" family="Drosophilidae" genus="Scaptodrosophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scaptodrosophila coracina" order="Diptera" pageId="13" pageNumber="132" phylum="Arthropoda" rank="species" species="coracina">Scaptodrosophila coracina</taxonomicName>
