, 
<taxonomicName id="C3AA310A5B523FEC23E917998767BEAD" class="Insecta" family="Crambidae" genus="Phostria" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phostria mapetalis" order="Lepidoptera" pageId="223" pageNumber="224" phylum="Arthropoda" rank="species" species="mapetalis">Phostria mapetalis</taxonomicName>
