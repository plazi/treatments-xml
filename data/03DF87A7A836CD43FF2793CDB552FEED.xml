<taxonomicName id="4C764D32A835CD43FDC994A9B4EBFF7D" authority="Redtenbacher, 1891" authorityName="Redtenbacher" authorityYear="1891" box="[633,997,189,215]" class="Insecta" family="Tettigoniidae" genus="Caulopsis" kingdom="Animalia" order="Orthoptera" pageId="10" pageNumber="51" phylum="Arthropoda" rank="genus">
