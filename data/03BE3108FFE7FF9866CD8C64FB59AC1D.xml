<taxonomicName id="4C17FB9DFFE4FF98640A8BB5FD2CADD5" box="[592,754,188,213]" class="Insecta" family="Phlaeothripidae" genus="Mystrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="6" pageNumber="353" phylum="Arthropoda" rank="species" species="dammermanni">
