<taxonomicName id="EAAB4D66FFD7FFD9FF56FF3FFD40FF74" authority="Hopffer, 1855" authorityName="Hopffer" authorityYear="1855" box="[207,677,185,210]" class="Insecta" family="Hesperiidae" genus="Borbo" kingdom="Animalia" order="Lepidoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="subSpecies" species="fatuellus" subSpecies="fatuellus">
