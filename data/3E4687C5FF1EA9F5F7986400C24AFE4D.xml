for 510, 
<quantity id="71179B36FF13A9F4F6376137C2EFF980" box="[312,397,1638,1660]" metricMagnitude="-4" metricUnit="m" metricValue="1.9" pageId="186" pageNumber="643" unit="mm" value="0.19">0.19 mm</quantity>
for 513, 
