<paragraph id="343472341778A957989DFA5C7B72FA4D" blockId="73.[151,817,1431,1498]" box="[151,817,1431,1457]" pageId="73" pageNumber="74">
<heading id="6F7CC5581778A957989DFA5C7B72FA4D" bold="true" box="[151,817,1431,1457]" fontSize="11" level="1" pageId="73" pageNumber="74" reason="1">
<emphasis id="06FFAE261778A957989DFA5C7B72FA4D" bold="true" box="[151,817,1431,1457]" pageId="73" pageNumber="74">
<taxonomicName id="F38B09B71778A957989DFA5C7AD7FA4D" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[151,660,1431,1457]" class="Insecta" family="Hydropsychidae" genus="Hydropsyche" kingdom="Animalia" order="Trichoptera" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="narayana" status="new species">
<emphasis id="06FFAE261778A957989DFA5C79F3FA4D" bold="true" box="[151,432,1431,1457]" italics="true" pageId="73" pageNumber="74">Hydropsyche narayana</emphasis>
Oláh &amp; Johanson
</taxonomicName>
, 
<taxonomicNameLabel id="1DCC135D1778A9579AABFA5C7B72FA4D" box="[673,817,1431,1457]" pageId="73" pageNumber="74" rank="species">new species</taxonomicNameLabel>
