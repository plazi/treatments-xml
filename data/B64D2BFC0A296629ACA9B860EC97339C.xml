25. 
<taxonomicName id="7048C5869A8C2A8B09F694219D1C6311" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26303" authority="Forel, 1891" authorityName="Forel" authorityYear="1891" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus cambouei Forel" order="Hymenoptera" pageNumber="68" phylum="Arthropoda" rank="species" species="cambouei" status="n. sp.">CAMPONOTUS CAMBOUEI</taxonomicName>
