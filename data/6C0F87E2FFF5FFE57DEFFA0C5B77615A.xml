species:
</emphasis>
<taxonomicName id="23A64D77FFF5FFE67C3BFA435B3F67F6" box="[323,519,1501,1526]" class="Actinopterygii" family="Cyprinidae" genus="Hongshuia" kingdom="Animalia" order="Cypriniformes" pageId="1" pageNumber="34" phylum="Chordata" rank="species" species="paoli" status="new species">
