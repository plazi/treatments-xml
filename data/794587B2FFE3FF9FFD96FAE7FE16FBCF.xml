<taxonomicName id="36EC4D27FFE3FF9EFAE2F959FACEF8C2" box="[1327,1399,1785,1811]" class="Gastropoda" family="Vermetidae" genus="Aletes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="34" pageNumber="35" phylum="Mollusca" rank="genus">
<emphasis id="C398EAB6FFE3FF9EFAE2F959FACEF8C2" box="[1327,1399,1785,1811]" italics="true" pageId="34" pageNumber="35">Aletes</emphasis>
