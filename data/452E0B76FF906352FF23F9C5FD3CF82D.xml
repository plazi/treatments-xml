<quantity id="0A7F1785FF906352FEE6F8C3FE77F89D" box="[338,428,1871,1897]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="18" pageNumber="19" unit="mm" value="3.6">3.6 mm</quantity>
