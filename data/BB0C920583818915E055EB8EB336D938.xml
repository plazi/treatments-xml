<taxonomicName id="ADA823E3A0EAD1A1266066071209D3B5" class="Malacostraca" family="Akanthophoreidae" genus="Tumidochelia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tumidochelia uncinata" order="Tanaidacea" pageId="7" pageNumber="26" phylum="Arthropoda" rank="species" species="uncinata">Tumidochelia uncinata</taxonomicName>
