<taxonomicName id="ECCA4D4FFF96FFB59CB18901498B7E1E" box="[151,252,1538,1561]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
