<treatmentCitationGroup id="FDDB11A3FFCFFFCDEAA4F984B235F9AA" box="[151,595,1540,1563]" pageId="29" pageNumber="30">
<taxonomicName id="1ACB4D0EFFCFFFCDEAA4F984B246F9AA" authority="Richards, 1941: 324" authorityName="Richards" authorityPageNumber="324" authorityYear="1941" box="[151,544,1540,1563]" class="Insecta" family="Sphaeroceridae" genus="Penola" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="eudyptidis">
<emphasis id="EFBFEA9FFFCFFFCDEAA4F984B12BF9AB" box="[151,333,1541,1563]" italics="true" pageId="29" pageNumber="30">Penola eudyptidis</emphasis>
<treatmentCitation id="5C6A109CFFCFFFCDEB60F985B246F9AA" author="Richards" box="[339,544,1540,1562]" page="324" pageId="29" pageNumber="30" year="1941">
