<taxonomicName id="4C294D5EFFD70736FE06962DFC03A308" authority="(Ashmead, 1901)" authorityName="Zimmerman" authorityYear="1978" baseAuthorityName="Ashmead" baseAuthorityYear="1901" box="[484,906,548,574]" class="Insecta" family="Eulophidae" genus="Euderus" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="metallicus">
