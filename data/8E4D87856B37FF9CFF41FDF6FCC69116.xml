<taxonomicName id="C1E44D106B36FF9CFDA4FB8CFD6A9115" box="[626,724,1032,1057]" class="Insecta" family="Coenagrionidae" genus="Telebasis" kingdom="Animalia" order="Odonata" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="lenkoi">
<emphasis id="3490EA816B36FF9CFDA4FB8CFD6A9115" box="[626,724,1032,1057]" italics="true" pageId="5" pageNumber="58">T. lenkoi</emphasis>
</taxonomicName>
or 
<taxonomicName id="C1E44D106B36FF9CFD2BFB8CFCCF9115" box="[765,881,1032,1057]" class="Insecta" family="Coenagrionidae" genus="Telebasis" kingdom="Animalia" order="Odonata" pageId="5" pageNumber="58" phylum="Arthropoda" rank="species" species="parecei">
<emphasis id="3490EA816B36FF9CFD2BFB8CFCCF9115" box="[765,881,1032,1057]" italics="true" pageId="5" pageNumber="58">T. parecei</emphasis>
</taxonomicName>
.
