<taxonomicName id="16AECEBCC377BA37FF18FC0580E5FC1F" authority="(Linnaeus)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[151,505,951,977]" class="Insecta" family="Diaspididae" genus="Lepidosaphes" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="ulmi">
<emphasis id="E3DA692DC377BA37FF18FC05806FFC1F" box="[151,371,952,977]" italics="true" pageId="11" pageNumber="12">Lepidosaphes ulmi</emphasis>
(Linnaeus)
</taxonomicName>
