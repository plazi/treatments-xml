<taxonomicName id="4C1B4D4B7C37FFA7FBD2F829FB3D4223" authorityName="Black Tern MGT T" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[1142,1233,2047,2070]" class="Aves" family="Laridae" genus="Chlidonias" kingdom="Animalia" order="Charadriiformes" pageId="12" pageNumber="413" phylum="Chordata" rank="species" species="niger">
