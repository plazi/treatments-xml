<quantity id="E22F8FAAFFA9FF9EFBDBA745FB79F9B6" box="[1051,1193,1629,1651]" metricMagnitude="-3" metricUnit="m" metricValue="1.645" metricValueMax="1.76" metricValueMin="1.53" pageId="21" pageNumber="1037" unit="mm" value="1.645" valueMax="1.76" valueMin="1.53">1.53–1.76 mm</quantity>
