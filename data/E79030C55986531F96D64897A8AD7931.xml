<emphasis id="13C4DC952B89C079E5A3D408D0A246BC" bold="true" pageId="0" pageNumber="49">Figure 7.</emphasis>
<taxonomicName id="C19140D1067B3049AF085A8EF503085B" baseAuthorityName="Malloch" baseAuthorityYear="1939" class="Insecta" family="Tephritidae" genus="Bactrocera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bactrocera simulata" order="Diptera" pageId="0" pageNumber="49" phylum="Arthropoda" rank="species" species="simulata">
