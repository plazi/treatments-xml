<taxonomicName id="4C3D4D18FF2EE3376F649FA0FCF6F898" authority="Tan &amp; Zhou, 1995" authorityName="Tan &amp; Zhou" authorityYear="1995" box="[323,812,1784,1806]" class="Insecta" family="Chrysomelidae" genus="Chlamisus" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="flavomaculatus">
<taxonomicName id="4C3D4D18FF2EE3376F649FA0FD3DF898" authority="Tan &amp; Zhou" authorityName="Tan &amp; Zhou" box="[323,743,1784,1806]" class="Insecta" family="Chrysomelidae" genus="Chlamisus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="flavomaculatus">
