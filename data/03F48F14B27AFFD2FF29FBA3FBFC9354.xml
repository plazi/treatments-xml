<taxonomicName id="4C5D4581B279FFD2FB54FC68FAC89370" authorityName="Sasa" authorityYear="1961" box="[1258,1388,1015,1040]" class="Arachnida" family="Pygmephoridae" genus="Bakerdania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="278" phylum="Arthropoda" rank="genus">
