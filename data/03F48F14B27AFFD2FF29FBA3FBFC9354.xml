<taxonomicName id="4C5D4581B279FFD2FC6EFB83FBF69355" authorityName="Sasa" authorityYear="1961" box="[976,1106,1052,1077]" class="Arachnida" family="Pygmephoridae" genus="Bakerdania" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="278" phylum="Arthropoda" rank="genus">
<emphasis id="B929E210B279FFD2FC6EFB83FBF69355" box="[976,1106,1052,1077]" italics="true" pageId="2" pageNumber="278">Bakerdania</emphasis>
