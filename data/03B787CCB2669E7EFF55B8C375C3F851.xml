<taxonomicName id="4C1E4D59B2669E7EFDD2BAF5710CF90D" authority="Rhyne, Calado &amp; Dos Santos, 2012" authorityName="Rhyne, Calado &amp; Dos Santos" authorityYear="2012" box="[528,1069,1756,1781]" class="Malacostraca" family="Hippolytidae" genus="Lysmata" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="jundalini">
