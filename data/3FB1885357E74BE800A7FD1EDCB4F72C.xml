<taxonomicName id="9D1764A57D62177A2543D03C1FFCFE6E" authorityName="Reeve" authorityYear="1845" class="Hypotrichea" family="Mitridae" genus="Mitra" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mitra typha" order="Neogastropoda" pageId="23" pageNumber="68" phylum="Ciliophora" rank="species" species="typha">
