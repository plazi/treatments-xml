<taxonomicName id="4C324D04727BFFBBFBAEF9979AF1F9F5" authorityName="Sturm" authorityYear="1818" class="Insecta" family="Carabidae" genus="Harpalus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="57" phylum="Arthropoda" rank="species" species="froelichi">
