<paragraph blockId="194.[268,1302,1738,1819]" pageId="194" pageNumber="195">
<taxonomicName authority="Baba, 2018" authorityName="Baba" authorityYear="2018" box="[268,434,1773,1793]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="194" pageNumber="195" phylum="Arthropoda" rank="species" species="echinatus" status="n. sp.">
