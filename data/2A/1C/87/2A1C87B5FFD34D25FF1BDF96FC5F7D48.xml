and 
<taxonomicName authority="Baba, 2018" authorityName="Baba" authorityYear="2018" box="[1302,1407,1452,1475]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="parisus" status="n. sp.">
