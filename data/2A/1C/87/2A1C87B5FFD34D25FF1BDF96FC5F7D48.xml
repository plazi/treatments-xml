<taxonomicName authorityName="Benedict" authorityYear="1902" box="[327,449,1521,1545]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="scandens">
