<paragraph blockId="476.[174,1407,238,748]" pageId="476" pageNumber="477">
<emphasis box="[204,252,238,262]" italics="true" pageId="476" pageNumber="477">
<collectionCode box="[204,221,238,262]" pageId="476" pageNumber="477">P</collectionCode>
2-4
