) or 1.3 × (
<typeStatus box="[979,1083,412,436]" pageId="476" pageNumber="477">paratypes</typeStatus>
) length of 
<collectionCode box="[1199,1213,412,436]" pageId="476" pageNumber="477">P</collectionCode>
