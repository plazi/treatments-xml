<subSubSection pageId="55" pageNumber="56" type="etymology">
<paragraph blockId="55.[204,1436,237,331]" pageId="55" pageNumber="56">
<emphasis bold="true" box="[204,342,237,260]" pageId="55" pageNumber="56">ETYMOLOGY„</emphasis>
