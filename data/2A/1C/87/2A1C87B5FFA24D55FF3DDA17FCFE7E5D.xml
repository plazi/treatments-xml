<taxonomicName authority="Baba, 2005" authorityName="Baba" authorityYear="2005" box="[204,427,307,331]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="longior">
