<collectionCode box="[993,1007,1382,1406]" pageId="105" pageNumber="106">P</collectionCode>
2 and 
<collectionCode box="[1067,1083,1382,1406]" pageId="105" pageNumber="106">P</collectionCode>
3, 
<collectionCode box="[1104,1118,1382,1406]" pageId="105" pageNumber="106">P</collectionCode>
4 merus 0.9 × as long as and 0.9 × as broad as 
<collectionCode box="[346,361,1417,1441]" pageId="105" pageNumber="106">P</collectionCode>
3 merus; length-breadth ratio, 2.8-3.2 on 
<collectionCode box="[786,801,1417,1441]" pageId="105" pageNumber="106">P</collectionCode>
2, 2.8-2.9 on 
<collectionCode box="[933,949,1417,1441]" pageId="105" pageNumber="106">P</collectionCode>
3, 2.8 on 
<collectionCode box="[1041,1056,1417,1441]" pageId="105" pageNumber="106">P</collectionCode>
4; 
<collectionCode box="[1079,1093,1417,1441]" pageId="105" pageNumber="106">P</collectionCode>
2 merus 0.7 × as long as carapace and subequal to length of 
<collectionCode box="[477,491,1452,1476]" pageId="105" pageNumber="106">P</collectionCode>
2 propodus; 
<collectionCode box="[623,638,1452,1476]" pageId="105" pageNumber="106">P</collectionCode>
3 merus 0.8 × length of 
<collectionCode box="[887,902,1452,1476]" pageId="105" pageNumber="106">P</collectionCode>
3 propodus; 
