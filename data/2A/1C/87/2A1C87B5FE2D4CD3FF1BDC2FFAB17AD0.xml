<quantity box="[357,490,1625,1649]" metricMagnitude="-3" metricUnit="m" metricValue="7.15" metricValueMax="10.1" metricValueMin="4.2" pageId="443" pageNumber="444" unit="mm" value="7.15" valueMax="10.1" valueMin="4.2">4.2-10.1 mm</quantity>
; females, 4.0-10.0 mm; ovigerous females from 
<quantity box="[980,1062,1625,1649]" metricMagnitude="-3" metricUnit="m" metricValue="5.8" pageId="443" pageNumber="444" unit="mm" value="5.8">5.8 mm</quantity>
