<subSubSection pageId="44" pageNumber="45" type="etymology">
<paragraph blockId="44.[174,1406,1727,1788]" pageId="44" pageNumber="45">
<emphasis bold="true" box="[174,312,1727,1750]" pageId="44" pageNumber="45">ETYMOLOGY„</emphasis>
