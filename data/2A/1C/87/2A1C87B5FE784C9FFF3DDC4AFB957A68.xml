2 and 
<collectionCode box="[961,976,1452,1476]" pageId="497" pageNumber="498">P</collectionCode>
4, longest on 
<collectionCode box="[1115,1131,1452,1476]" pageId="497" pageNumber="498">P</collectionCode>
3, or subequal on 
<collectionCode box="[1317,1333,1452,1476]" pageId="497" pageNumber="498">P</collectionCode>
