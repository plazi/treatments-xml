<paragraph blockId="587.[379,1297,1711,1819]" pageId="587" pageNumber="588">
<taxonomicName authority="Baba, 2018" authorityName="Baba" authorityYear="2018" box="[379,537,1746,1766]" class="Malacostraca" family="Chirostylidae" genus="Heteroptychus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="587" pageNumber="588" phylum="Arthropoda" rank="species" species="colini" status="n. sp.">
