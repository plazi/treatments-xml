<paragraph blockId="472.[174,1406,238,956]" pageId="472" pageNumber="473">
<emphasis box="[204,231,238,262]" italics="true" pageId="472" pageNumber="473">
<collectionCode box="[204,221,238,262]" pageId="472" pageNumber="473">P</collectionCode>
1
</emphasis>
