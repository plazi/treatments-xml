<collectionCode box="[200,215,550,574]" pageId="472" pageNumber="473">P</collectionCode>
3 merus), subequally broad on 
<collectionCode box="[534,548,550,574]" pageId="472" pageNumber="473">P</collectionCode>
2 and 
<collectionCode box="[612,628,550,574]" pageId="472" pageNumber="473">P</collectionCode>
3, somewhat narrower on 
<collectionCode box="[898,912,550,574]" pageId="472" pageNumber="473">P</collectionCode>
4 (
