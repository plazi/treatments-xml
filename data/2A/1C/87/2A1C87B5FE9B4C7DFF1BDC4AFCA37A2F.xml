<collectionCode box="[971,986,1174,1198]" pageId="271" pageNumber="272">P</collectionCode>
3 and 
<collectionCode box="[1049,1064,1174,1198]" pageId="271" pageNumber="272">P</collectionCode>
4. Propodi subequal in length on 
<collectionCode box="[1410,1424,1174,1198]" pageId="271" pageNumber="272">P</collectionCode>
2 and 
