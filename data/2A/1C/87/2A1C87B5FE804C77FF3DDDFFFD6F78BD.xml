<collectingCountry box="[1227,1402,446,470]" name="Solomon Islands" pageId="280" pageNumber="281">Solomon Islands</collectingCountry>
, Wallis-Futuna Islands, 
<collectingCountry box="[411,500,481,504]" name="Vanuatu" pageId="280" pageNumber="281">Vanuatu</collectingCountry>
, Chesterfield Islands, 
<collectingCountry box="[726,889,481,505]" name="New Caledonia" pageId="280" pageNumber="281">New Caledonia</collectingCountry>
, Norfolk Ridge, and 
<collectingCountry box="[1100,1167,481,505]" name="Tonga" pageId="280" pageNumber="281">Tonga</collectingCountry>
; 
<quantity box="[1177,1302,481,505]" metricMagnitude="2" metricUnit="m" metricValue="8.305" metricValueMax="14.34" metricValueMin="2.27" pageId="280" pageNumber="281" unit="m" value="830.5" valueMax="1434.0" valueMin="227.0">227-1434 m</quantity>
