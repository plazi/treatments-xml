<collectionCode box="[670,684,1660,1684]" pageId="89" pageNumber="90">P</collectionCode>
2 propodus; 
<collectionCode box="[817,832,1660,1684]" pageId="89" pageNumber="90">P</collectionCode>
3 merus as long as 
<collectionCode box="[1031,1046,1660,1684]" pageId="89" pageNumber="90">P</collectionCode>
