<quantity box="[357,395,1729,1753]" metricMagnitude="-1" metricUnit="m" metricValue="1.016" pageId="89" pageNumber="90" unit="in" value="4.0">4 in</quantity>
<typeStatus box="[400,496,1729,1753]" pageId="89" pageNumber="90">holotype</typeStatus>
), dactylus-propodus length ratio, 0.4-0.5 on 
<collectionCode box="[955,970,1729,1753]" pageId="89" pageNumber="90">P</collectionCode>
