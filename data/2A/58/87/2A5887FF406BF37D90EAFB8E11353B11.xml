</taxonomicName>
and 
<taxonomicName authority="Sasakawa" authorityName="Sasakawa" authorityYear="1986" box="[905,1157,1818,1845]" class="Insecta" family="Muscidae" genus="Cordilura" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="472" phylum="Arthropoda" rank="species" species="nubecula">
<emphasis box="[905,1039,1818,1845]" italics="true" pageId="1" pageNumber="472">C. nubecula</emphasis>
