<taxonomicName class="Insecta" family="Carabidae" genus="Selenophorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Selenophorus" order="Coleoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">Selenophorus</taxonomicName>
