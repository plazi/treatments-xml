<subSubSection pageId="0" pageNumber="1" type="type species">
<paragraph pageId="0" pageNumber="1">Type species.</paragraph>
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Songsangchote, Sippawat, Khaikaew &amp; Chomphuphuang" authorityYear="2022" family="Theraphosidae" genus="Taksinus" kingdom="Animalia" lsidName="Taksinus bambus" order="Araneae" pageId="0" pageNumber="1" rank="species" species="bambus">
