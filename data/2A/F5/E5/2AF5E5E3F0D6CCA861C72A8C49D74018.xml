34. 
<taxonomicName LSID="urn:lsid:zoobank.org:act:08FF1F00-3CCB-4E95-997B-4B170BFF6069" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus granum" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="granum">Trigonopterus granum Riedel</taxonomicName>
