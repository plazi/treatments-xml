<subSubSection box="[166,690,674,698]" pageId="2" pageNumber="181" type="type_taxon">
<paragraph blockId="2.[166,690,674,698]" box="[166,690,674,698]" pageId="2" pageNumber="181">
<emphasis bold="true" box="[166,309,674,698]" pageId="2" pageNumber="181">
<typeStatus box="[166,223,674,698]" pageId="2" pageNumber="181">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName authority="Johnson, 2004" authorityName="Johnson" authorityYear="2004" box="[324,632,675,698]" class="Actinopterygii" family="Aploactinidae" genus="Pseudopataecus" kingdom="Animalia" order="Scorpaeniformes" pageId="2" pageNumber="181" phylum="Chordata" rank="species" species="taenianotus" status="n.sp.">
<emphasis box="[324,632,675,698]" italics="true" pageId="2" pageNumber="181">Pseudopataecus taenianotus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[639,690,675,698]" pageId="2" pageNumber="181" rank="species">n.sp.</taxonomicNameLabel>
</paragraph>
