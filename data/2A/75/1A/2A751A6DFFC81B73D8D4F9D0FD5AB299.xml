<taxonomicName box="[1045,1183,1804,1829]" class="Arachnida" family="Zalmoxidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="1" pageNumber="254" phylum="Arthropoda" rank="family">Zalmoxidae</taxonomicName>
