<emphasis bold="true" box="[466,485,1493,1514]" pageId="17" pageNumber="18">A</emphasis>
, occlusal; 
<emphasis bold="true" box="[602,619,1493,1514]" pageId="17" pageNumber="18">B</emphasis>
