<paragraph blockId="2.[151,1436,1531,2032]" pageId="2" pageNumber="183">
<emphasis box="[199,400,1531,1556]" italics="true" pageId="2" pageNumber="183">Anogenital region</emphasis>
. Five pairs of genital (
<emphasis box="[654,668,1532,1555]" italics="true" pageId="2" pageNumber="183">g</emphasis>
<number box="[668,676,1546,1560]" pageId="2" pageNumber="183" value="1.0">1</number>
– 
<emphasis box="[690,704,1532,1555]" italics="true" pageId="2" pageNumber="183">g</emphasis>
<number box="[704,798,1531,1560]" pageId="2" pageNumber="183" value="10.57" valueMax="16.0" valueMin="5.14">5, 14–16</number>
), 
<number box="[819,833,1532,1556]" pageId="2" pageNumber="183" value="3.0">3</number>
pairs of aggenital (
<emphasis box="[1049,1077,1532,1555]" italics="true" pageId="2" pageNumber="183">ag</emphasis>
, 
<number box="[1091,1122,1532,1556]" pageId="2" pageNumber="183" value="20.0">20</number>
), 
<number box="[1142,1156,1532,1556]" pageId="2" pageNumber="183" value="3.0">3</number>
pairs of adanal (
<emphasis box="[1343,1371,1531,1556]" italics="true" pageId="2" pageNumber="183">ad</emphasis>
<number box="[1372,1380,1546,1560]" pageId="2" pageNumber="183" value="1.0">1</number>
– 
<emphasis box="[1393,1421,1531,1556]" italics="true" pageId="2" pageNumber="183">ad</emphasis>
<number pageId="2" pageNumber="183" value="15.620000000000001" valueMax="28.0" valueMin="3.24">3, 24–28</number>
) and 
<number box="[291,305,1570,1594]" pageId="2" pageNumber="183" value="2.0">2</number>
pairs of anal (
<emphasis box="[478,506,1571,1594]" italics="true" pageId="2" pageNumber="183">an</emphasis>
