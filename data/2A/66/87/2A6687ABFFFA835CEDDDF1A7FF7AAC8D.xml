<taxonomicName authority="(Schluger, 1960)" baseAuthorityName="Schluger" baseAuthorityYear="1960" box="[283,753,830,854]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="10" pageNumber="339" phylum="Arthropoda" rank="species" species="gracipalpe">
