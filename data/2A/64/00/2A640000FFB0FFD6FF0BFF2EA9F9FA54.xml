<taxonomicName box="[1094,1241,1470,1495]" class="Insecta" family="Ricaniidae" genus="Parapiromis" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="39" phylum="Arthropoda" rank="genus">
