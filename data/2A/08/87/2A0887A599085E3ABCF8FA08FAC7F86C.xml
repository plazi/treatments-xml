<subSubSection box="[151,1401,1647,1672]" pageId="23" pageNumber="80" type="etymology">
<paragraph blockId="23.[151,1437,1647,2032]" box="[151,1401,1647,1672]" pageId="23" pageNumber="80">
<emphasis bold="true" box="[151,286,1647,1672]" pageId="23" pageNumber="80">Etymology.</emphasis>
