<taxonomicNameLabel box="[752,838,1196,1221]" pageId="2" pageNumber="363" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName box="[852,1039,1197,1220]" class="Insecta" family="Tiphiidae" genus="Rhagigaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="363" phylum="Arthropoda" rank="species" species="neocorrugatus" status="sp. nov.">
<emphasis box="[852,1039,1197,1220]" italics="true" pageId="2" pageNumber="363">R. neocorrugatus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1046,1132,1196,1221]" pageId="2" pageNumber="363">
<taxonomicNameLabel box="[1046,1132,1196,1221]" pageId="2" pageNumber="363" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
and 
<taxonomicName box="[1184,1304,1196,1221]" class="Insecta" family="Tiphiidae" genus="Rhagigaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="363" phylum="Arthropoda" rank="species" species="schichai" status="sp. nov.">
