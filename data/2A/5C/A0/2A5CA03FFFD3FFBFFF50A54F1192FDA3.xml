<number box="[1423,1435,1499,1520]" pageId="22" pageNumber="383" value="2.0">2</number>
</keyLead>
</paragraph>
<paragraph blockId="22.[151,1436,1470,2019]" pageId="22" pageNumber="383">
<keyLead pageId="22" pageNumber="383">
