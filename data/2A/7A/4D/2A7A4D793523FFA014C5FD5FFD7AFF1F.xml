<taxonomicName authority="(Moench) Fernald, L." authorityName="Fernald, L." baseAuthorityName="Moench" box="[527,860,1053,1080]" class="Magnoliopsida" family="Asteraceae" genus="Lactuca" kingdom="Plantae" order="Asterales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="species" species="biennis">
