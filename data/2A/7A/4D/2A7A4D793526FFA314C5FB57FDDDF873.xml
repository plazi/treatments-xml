: 
<taxonomicName authority="Benth." authorityName="Benth." box="[422,658,1665,1691]" class="Magnoliopsida" family="Garryaceae" genus="Garrya" kingdom="Plantae" order="Garryales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="ovata">
