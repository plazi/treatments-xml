, 
<collectionCode box="[860,903,1341,1367]" country="South Africa" lsid="urn:lsid:biocol.org:col:14911" name="South African National Biodiversity Institute" pageId="9" pageNumber="10" type="Herbarium">NH</collectionCode>
, 
<collectionCode box="[913,953,1341,1367]" country="Czech Republic" lsid="urn:lsid:biocol.org:col:12554" name="Agricultural Museum of Praha" pageId="9" pageNumber="10" type="Herbarium">OH</collectionCode>
(
<bibRefCitation author="Foote, B. A." box="[967,1098,1341,1367]" pageId="9" pageNumber="10" pagination="166 - 175" refId="ref18934" refString="Foote, B. A. (2004) Acalyptrate Diptera associated with stands of Carex lacustris and C. stricta (Cyperaceae) in northeastern Ohio. Proceedings of the Entomological Society of Washington, 106 (1), 166 - 175." type="journal article" year="2004">Foote 2004</bibRefCitation>
