<collectionCode box="[430,469,1197,1223]" country="Sweden" lsid="urn:lsid:biocol.org:col:13421" name="Katedralskolan" pageId="7" pageNumber="8" type="Herbarium">SK</collectionCode>
; Europe. We have also seen leaf mines (including photographs posted to www.Bug- Guide.net) in 
<collectionCode box="[306,343,1233,1259]" country="South Africa" lsid="urn:lsid:biocol.org:col:14492" name="University of Cape Town" pageId="7" pageNumber="8" type="Herbarium">CT</collectionCode>
, 
<collectionCode box="[354,402,1233,1259]" country="Germany" name="Museum Donaueschingen" pageId="7" pageNumber="8">MD</collectionCode>
, 
<collectionCode box="[413,450,1233,1259]" country="Italy" lsid="urn:lsid:biocol.org:col:14381" name="Università degli Studi di Milano" pageId="7" pageNumber="8" type="Herbarium">MI</collectionCode>
, 
<collectionCode box="[461,509,1233,1259]" country="Brazil" name="Museu Nacional, Universidade Federal do Rio de Janeiro" pageId="7" pageNumber="8">MN</collectionCode>
, 
<collectionCode box="[520,554,1233,1259]" country="Sierra Leone" lsid="urn:lsid:biocol.org:col:13045" name="Njala University College" pageId="7" pageNumber="8" type="Herbarium">NJ</collectionCode>
, 
<collectionCode box="[565,602,1233,1259]" country="USA" lsid="urn:lsid:biocol.org:col:15556" name="William and Lynda Steere Herbarium of the New York Botanical Garden" pageId="7" pageNumber="8" type="Herbarium">NY</collectionCode>
, and 
<collectionCode box="[663,707,1233,1259]" country="Czech Republic" lsid="urn:lsid:biocol.org:col:12554" name="Agricultural Museum of Praha" pageId="7" pageNumber="8" type="Herbarium">OH</collectionCode>
