<paragraph id="922136E09D569211FF2EFBD5CD212CE7" blockId="74.[189,1399,1131,1157]" box="[189,1399,1131,1157]" pageId="74" pageNumber="75">
<taxonomicName id="559E4D639D569211FF2EFBD5C9C32CE7" authority="Telnov, 2020" authorityName="Telnov" authorityYear="2020" box="[189,405,1131,1157]" class="Insecta" family="Scarabaeidae" genus="Maechidius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="maleo" status="sp. nov.">
<emphasis id="A0EAEAF29D569211FF2EFBD5C9C32CE7" box="[189,405,1131,1157]" italics="true" pageId="74" pageNumber="75">Maechidius maleo</emphasis>
</taxonomicName>
<taxonomicNameLabel id="BBD957899D569211FE08FBD5C9A32CE7" box="[411,501,1131,1157]" pageId="74" pageNumber="75" rank="species">sp. nov.</taxonomicNameLabel>
