<emphasis id="B94DEAC7FFCCFFAF1388B73EA1CB05FB" bold="true" box="[151,491,1904,1930]" pageId="17" pageNumber="154">
Tribe 
<taxonomicName id="4C394D56FFCCFFAF13FDB73EA1CB05FB" authority="Wagner" authorityName="Wagner" authorityYear="1963" box="[226,491,1904,1930]" class="Insecta" family="Delphacidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="154" phylum="Arthropoda" rank="tribe" tribe="Stenocranini">Stenocranini Wagner</taxonomicName>
