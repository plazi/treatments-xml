<taxonomicName id="4C744D7C0678FFE8874FFBF9FA1E03C9" box="[1303,1451,1239,1263]" class="Liliopsida" family="Smilacaceae" genus="Smilax" kingdom="Plantae" order="Liliales" pageId="5" pageNumber="112" phylum="Tracheophyta" rank="species" species="megacarpa">
