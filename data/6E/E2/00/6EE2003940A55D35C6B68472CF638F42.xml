<subSubSection pageId="null" pageNumber="215" type="vernacular_name">
<paragraph pageId="null" pageNumber="215">
Schwimmende 
<taxonomicName class="Liliopsida" family="Alismataceae" genus="Elisma" higherTaxonomySource="GBIF" kingdom="Plantae" order="Alismatales" pageId="null" pageNumber="215" phylum="Tracheophyta" rank="genus">Elisma</taxonomicName>
