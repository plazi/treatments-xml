<paragraph blockId="8.[151,1436,151,429]" pageId="8" pageNumber="301">
<emphasis bold="true" box="[199,273,260,285]" pageId="8" pageNumber="301">Notes.</emphasis>
<taxonomicName authorityName="Zhou" authorityYear="2017" box="[280,364,260,283]" class="Insecta" family="Staphylinidae" genus="Ocypus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="301" phylum="Arthropoda" rank="genus">
