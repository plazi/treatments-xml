<collectingCountry box="[1102,1166,464,490]" name="Spain" pageId="79" pageNumber="80">Spain</collectingCountry>
and 
<collectingCountry box="[1222,1317,464,490]" name="Portugal" pageId="79" pageNumber="80">Portugal</collectingCountry>
