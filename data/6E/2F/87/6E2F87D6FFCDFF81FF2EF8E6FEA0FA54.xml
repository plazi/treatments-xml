<taxonomicName authority="(Asso)" baseAuthorityName="Asso" baseAuthorityYear="1801" box="[874,1179,1943,1969]" class="Actinopterygii" family="Sciaenidae" genus="Argyrosomus" kingdom="Animalia" order="Perciformes" pageId="79" pageNumber="80" phylum="Chordata" rank="species" species="regius">
