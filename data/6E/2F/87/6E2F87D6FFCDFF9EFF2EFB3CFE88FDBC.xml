<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" class="Actinopterygii" family="Scaridae" genus="Sparisoma" kingdom="Animalia" order="Perciformes" pageId="79" pageNumber="80" phylum="Chordata" rank="species" species="cretense">
