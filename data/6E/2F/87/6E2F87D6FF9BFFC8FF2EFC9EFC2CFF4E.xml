<collectingCountry box="[583,728,1015,1041]" name="South Africa" pageId="25" pageNumber="26">South Africa</collectingCountry>
, 
<collectingCountry box="[740,875,1015,1041]" name="Madagascar" pageId="25" pageNumber="26">Madagascar</collectingCountry>
and 
<collectingCountry box="[929,1038,1015,1041]" name="Mauritius" pageId="25" pageNumber="26">Mauritius</collectingCountry>
