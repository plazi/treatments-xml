<collectingRegion box="[856,1110,1571,1598]" country="South Africa" name="Eastern Cape" pageId="115" pageNumber="116">Eastern Cape Province</collectingRegion>
(
<collectingCountry box="[1123,1271,1571,1598]" name="South Africa" pageId="115" pageNumber="116">South Africa</collectingCountry>
), Persian Gulf, 
<collectingCountry box="[151,274,1607,1633]" name="Seychelles" pageId="115" pageNumber="116">Seychelles</collectingCountry>
, 
<collectingCountry box="[285,420,1607,1633]" name="Madagascar" pageId="115" pageNumber="116">Madagascar</collectingCountry>
