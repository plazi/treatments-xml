<collectingCountry box="[831,917,153,179]" name="Yemen" pageId="125" pageNumber="126">Socotra</collectingCountry>
and Persian Gulf east to 
<collectingCountry box="[1207,1333,153,179]" name="Philippines" pageId="125" pageNumber="126">Philippines</collectingCountry>
and 
<collectingCountry box="[1390,1432,153,179]" name="Fiji" pageId="125" pageNumber="126">Fiji</collectingCountry>
, north to southern 
<collectingCountry box="[352,419,189,215]" name="Japan" pageId="125" pageNumber="126">Japan</collectingCountry>
, south to Ningaloo Reef (
<collectingRegion box="[710,914,189,216]" country="Australia" name="Western Australia" pageId="125" pageNumber="126">Western Australia</collectingRegion>
) and 
<collectingRegion box="[977,1179,189,215]" country="Australia" name="New South Wales" pageId="125" pageNumber="126">New South Wales</collectingRegion>
(
<collectingCountry box="[1194,1300,189,216]" name="Australia" pageId="125" pageNumber="126">Australia</collectingCountry>
) at 
<geoCoordinate box="[1344,1436,189,215]" degrees="29" direction="south" minutes="23" orientation="latitude" pageId="125" pageNumber="126" precision="925" value="-29.383333">29°23’S</geoCoordinate>
