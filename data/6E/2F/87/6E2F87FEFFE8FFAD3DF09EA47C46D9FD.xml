, 
<taxonomicName authority="Boisduval" authorityName="Boisduval" class="Insecta" family="Noctuidae" genus="Spodoptera" kingdom="Animalia" order="Lepidoptera" pageId="24" pageNumber="393" phylum="Arthropoda" rank="species" species="littoralis">
