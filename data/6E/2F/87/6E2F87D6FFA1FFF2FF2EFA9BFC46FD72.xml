<collectingCountry box="[487,585,1431,1457]" name="Lebanon" pageId="35" pageNumber="36">Lebanon</collectingCountry>
as 
<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[622,831,1431,1457]" class="Actinopterygii" family="Ophichthidae" genus="Ophisurus" kingdom="Animalia" order="Anguilliformes" pageId="35" pageNumber="36" phylum="Chordata" rank="species" species="serpens">
