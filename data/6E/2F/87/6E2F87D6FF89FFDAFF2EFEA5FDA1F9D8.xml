<taxonomicName authority=", Cuv." authorityName="Cuv." baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[636,909,441,467]" class="Elasmobranchii" family="Scyliorhinidae" genus="Scyllium" kingdom="Animalia" order="Carcharhiniformes" pageId="11" pageNumber="12" phylum="Chordata" rank="species" species="caninula">
