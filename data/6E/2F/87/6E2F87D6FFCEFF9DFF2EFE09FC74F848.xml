<taxonomicName baseAuthorityName="Bloch" baseAuthorityYear="1790" box="[853,1099,621,647]" class="Actinopterygii" family="Lobotidae" genus="Lobotes" kingdom="Animalia" order="Perciformes" pageId="76" pageNumber="77" phylum="Chordata" rank="species" species="surinamensis">
