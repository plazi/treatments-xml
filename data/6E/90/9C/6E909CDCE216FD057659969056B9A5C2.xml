<taxonomicName family="Hesperiidae" lsidName="" pageId="292" pageNumber="293" rank="family">Hesperiidae</taxonomicName>
, 
<taxonomicName class="Insecta" family="Hesperiidae" genus="Urbanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Urbanus dorantes" order="Lepidoptera" pageId="292" pageNumber="293" phylum="Arthropoda" rank="species" species="dorantes">Urbanus dorantes</taxonomicName>
, 
<taxonomicName class="Insecta" family="Hesperiidae" genus="Urbanus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Urbanus teleus" order="Lepidoptera" pageId="292" pageNumber="293" phylum="Arthropoda" rank="species" species="teleus">Urbanus teleus</taxonomicName>
.
</paragraph>
</subSubSection>
