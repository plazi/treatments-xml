<subSubSection pageId="4" pageNumber="41" type="materials_examined">
<paragraph blockId="4.[151,1436,152,498]" pageId="4" pageNumber="41">
<materialsCitation country="Brazil" county="Atlantic Forest" location="Rufous Gnateater" municipality="Usually" pageId="4" pageNumber="41" specimenCount="1" typeStatus="holotype">
<emphasis bold="true" box="[197,331,431,457]" pageId="4" pageNumber="41">
