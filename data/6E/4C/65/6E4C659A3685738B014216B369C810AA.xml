<taxonomicName authority="L." class="Magnoliopsida" family="Asteraceae" genus="Buphthalmum" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="521" phylum="Tracheophyta" rank="species" species="salicifolium">
<pageBreakToken pageId="null" pageNumber="521" start="start">Buphthalmum</pageBreakToken>
