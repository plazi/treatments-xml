<taxonomicName authority="Kirkpatrick, 1890" authorityName="Kirkpatrick" authorityYear="1890" box="[283,705,1810,1832]" class="Gymnolaemata" family="Candidae" genus="Scrupocellaria" kingdom="Animalia" order="Cheilostomatida" pageId="9" pageNumber="38" phylum="Bryozoa" rank="species" species="frondis">
