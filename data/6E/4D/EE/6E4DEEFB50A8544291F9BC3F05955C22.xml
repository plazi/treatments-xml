<geoCoordinate degrees="21" direction="north" minutes="36.827" orientation="latitude" precision="1" value="21.613783">21°36.827'N</geoCoordinate>
, 
<geoCoordinate degrees="101" direction="east" minutes="34.847" orientation="longitude" precision="1" value="101.58078">101°34.847'E</geoCoordinate>
