Figure 
<number box="[1014,1028,1181,1202]" pageId="2" pageNumber="298" value="5.0">5</number>
. 
<taxonomicName box="[1039,1161,1181,1202]" class="Insecta" family="Curculionidae" genus="Diapus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="298" phylum="Arthropoda" rank="species" species="orientalis">
