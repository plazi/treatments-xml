<emphasis bold="true" box="[151,254,1181,1202]" pageId="2" pageNumber="298">
<collectionCode box="[151,229,1181,1202]" pageId="2" pageNumber="298">PLATE</collectionCode>
<number box="[236,249,1181,1202]" pageId="2" pageNumber="298" value="2.0">2</number>
.
</emphasis>
Head of 
<taxonomicName box="[346,514,1181,1202]" class="Insecta" family="Curculionidae" genus="Diapus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="298" phylum="Arthropoda" rank="species" species="orientalis" status="n. sp.">
<emphasis box="[346,514,1181,1202]" italics="true" pageId="2" pageNumber="298">Diapus orientalis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[521,577,1181,1202]" pageId="2" pageNumber="298" rank="species">
<emphasis bold="true" box="[521,577,1181,1202]" pageId="2" pageNumber="298">n. sp.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName authority="Chapuis." authorityName="Chapuis." box="[625,938,1181,1202]" class="Insecta" family="Curculionidae" genus="Diapus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="298" phylum="Arthropoda" rank="species" species="quadrispinatus">
