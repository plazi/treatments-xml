<paragraph pageId="5" pageNumber="6">
<taxonomicName class="Arachnida" family="Anapidae" genus="ANAPISONA" higherTaxonomySource="CoL" kingdom="Animalia" order="Araneae" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">ANAPISONA</taxonomicName>
