<subSubSection lastPageId="23" lastPageNumber="224" pageId="22" pageNumber="223" type="discussion">
<paragraph blockId="22.[151,1437,258,2020]" lastBlockId="23.[151,1436,151,429]" lastPageId="23" lastPageNumber="224" pageId="22" pageNumber="223">
<emphasis bold="true" box="[199,325,1959,1984]" pageId="22" pageNumber="223">Discussion</emphasis>
