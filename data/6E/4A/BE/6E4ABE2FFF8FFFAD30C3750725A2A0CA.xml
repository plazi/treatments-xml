<subSubSection pageId="33" pageNumber="234" type="materials_examined">
<paragraph blockId="33.[151,1437,786,1675]" pageId="33" pageNumber="234">
<materialsCitation ID-GBIF-Occurrence="1324639439" collectingDate="2012-05-18" collectorName="Basse Terre, NE &amp; R. H. Turnbow" country="Guadeloupe" latitude="16.4404" location="Material" longLatPrecision="5" longitude="-61.74977" pageId="33" pageNumber="234" specimenCount="1" specimenCount-male="1">
<emphasis bold="true" box="[199,430,1614,1639]" pageId="33" pageNumber="234">
