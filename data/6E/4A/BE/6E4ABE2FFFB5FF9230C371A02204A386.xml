<taxonomicName box="[151,373,1741,1767]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="228" phylum="Arthropoda" rank="species" species="waughi" status="new species">
<emphasis bold="true" box="[151,373,1741,1767]" italics="true" pageId="27" pageNumber="228">Dasyhelea waughi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[380,524,1741,1767]" pageId="27" pageNumber="228" rank="species">new species</taxonomicNameLabel>
</emphasis>
