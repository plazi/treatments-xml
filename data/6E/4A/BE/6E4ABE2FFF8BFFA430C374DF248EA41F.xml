<caption httpUri="https://zenodo.org/record/164740/files/figure.png" pageId="39" pageNumber="240" targetBox="[218,1375,467,1939]" targetPageId="39">
<paragraph blockId="39.[151,1436,1975,2027]" pageId="39" pageNumber="240">
<emphasis bold="true" box="[151,287,1975,1997]" pageId="39" pageNumber="240">FIGURE 22.</emphasis>
<taxonomicName box="[293,516,1975,1997]" class="Insecta" family="Ceratopogonidae" genus="Dasyhelea" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="240" phylum="Arthropoda" rank="species" species="megatheca" status="n. sp.">
<emphasis box="[293,516,1975,1997]" italics="true" pageId="39" pageNumber="240">Dasyhelea megatheca</emphasis>
</taxonomicName>
<emphasis bold="true" box="[522,579,1975,1996]" pageId="39" pageNumber="240">
<taxonomicNameLabel box="[522,579,1975,1996]" pageId="39" pageNumber="240" rank="species">n. sp.</taxonomicNameLabel>
