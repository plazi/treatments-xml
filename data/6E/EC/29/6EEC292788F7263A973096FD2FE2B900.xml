<subSubSection pageId="7" pageNumber="30" type="nomenclature">
<paragraph pageId="7" pageNumber="30">
<taxonomicName authority="X. Y. Ma, K. D. Hyde &amp; Jayawardena" class="Sordariomycetes" family="Glomerellaceae" genus="Colletotrichum" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Colletotrichum chiangraiense" order="Glomerellales" pageId="7" pageNumber="30" phylum="Ascomycota" rank="species" species="chiangraiense">Colletotrichum chiangraiense X.Y. Ma, K.D. Hyde &amp; Jayawardena</taxonomicName>
