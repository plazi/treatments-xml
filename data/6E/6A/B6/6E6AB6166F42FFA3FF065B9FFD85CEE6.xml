. 
<emphasis box="[961,1059,369,392]" italics="true" pageId="53" pageNumber="54">japonica</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Wheeler" baseAuthorityYear="1933" box="[1119,1230,368,392]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="incerta">
