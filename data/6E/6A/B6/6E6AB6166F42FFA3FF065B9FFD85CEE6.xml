<quantity box="[281,451,512,536]" metricMagnitude="-4" metricUnit="m" metricValue="3.25" metricValueMax="3.3" metricValueMin="3.2" pageId="53" pageNumber="54" unit="mm" value="0.325" valueMax="0.33" valueMin="0.32">0.32–0.33 mm</quantity>
