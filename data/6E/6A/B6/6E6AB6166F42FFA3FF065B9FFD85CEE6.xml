<taxonomicName authorityName="Xu" authorityYear="2001" box="[481,563,655,680]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="baka">
<emphasis box="[481,495,657,680]" italics="true" pageId="53" pageNumber="54">P</emphasis>
. 
<emphasis box="[509,563,655,680]" italics="true" pageId="53" pageNumber="54">baka</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Terayama" authorityYear="2009" box="[578,714,655,680]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="shennong">
<emphasis box="[578,592,657,680]" italics="true" pageId="53" pageNumber="54">P</emphasis>
. 
<emphasis box="[606,714,655,680]" italics="true" pageId="53" pageNumber="54">shennong</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName=": Wilson" authorityYear="1957" box="[728,841,656,680]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="swezeyi">
