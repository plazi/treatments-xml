. 
<emphasis box="[711,765,583,608]" italics="true" pageId="53" pageNumber="54">baka</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Terayama" authorityYear="2009" box="[780,915,583,608]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="shennong">
