<quantity box="[982,1153,511,536]" metricMagnitude="-4" metricUnit="m" metricValue="3.8" metricValueMax="4.0" metricValueMin="3.5999999999999996" pageId="53" pageNumber="54" unit="mm" value="0.38" valueMax="0.4" valueMin="0.36">0.36–0.40 mm</quantity>
