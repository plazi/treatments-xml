<emphasis box="[1327,1341,693,716]" italics="true" pageId="53" pageNumber="54">P</emphasis>
. 
<emphasis box="[1356,1435,693,716]" italics="true" pageId="53" pageNumber="54">incerta</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Wheeler" authorityYear="1906" box="[254,381,728,752]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="japonica">
