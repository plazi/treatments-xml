<taxonomicName authorityName="Wheeler" authorityYear="1906" box="[395,521,621,644]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="japonica">
<emphasis box="[395,409,621,644]" italics="true" pageId="53" pageNumber="54">P</emphasis>
. 
<emphasis box="[423,521,621,644]" italics="true" pageId="53" pageNumber="54">japonica</emphasis>
</taxonomicName>
;
</paragraph>
<paragraph blockId="53.[151,1436,476,824]" pageId="53" pageNumber="54">
