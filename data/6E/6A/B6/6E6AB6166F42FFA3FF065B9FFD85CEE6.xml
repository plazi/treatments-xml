<quantity box="[1159,1260,476,500]" metricMagnitude="-4" metricUnit="m" metricValue="2.8" pageId="53" pageNumber="54" unit="mm" value="0.28">0.28 mm</quantity>
) and 
<taxonomicName authorityName=": Wilson" authorityYear="1957" box="[1322,1435,477,500]" class="Insecta" family="Formicidae" genus="Ponera" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="swezeyi">
<emphasis box="[1322,1336,477,500]" italics="true" pageId="53" pageNumber="54">P</emphasis>
. 
<emphasis box="[1350,1435,477,500]" italics="true" pageId="53" pageNumber="54">swezeyi</emphasis>
</taxonomicName>
