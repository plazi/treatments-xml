<paragraph id="0CD046FF0EF9E5ACAFC6056B8787DCC9" pageId="18" pageNumber="43">
<taxonomicName id="5B898AFA7C8863B6AF4C9A10812B98B6" authorityName="Chatrou" authorityYear="2019" class="Magnoliopsida" family="Annonaceae" genus="Klarobelia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Klarobelia rocioae" order="Magnoliales" pageId="18" pageNumber="43" phylum="Tracheophyta" rank="species" species="rocioae">
