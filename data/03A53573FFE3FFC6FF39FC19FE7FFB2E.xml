<emphasis id="B9785877FFE3FFC6FF39FC19FDB1FC0E" bold="true" box="[238,627,925,949]" pageId="3" pageNumber="774">
Genus 
<taxonomicName id="4C0CFFE6FFE3FFC6FEE8FC19FDB1FC0E" authority="Richard, 1895" authorityName="Richard" authorityYear="1895" box="[319,627,925,949]" class="Branchiopoda" family="Bosminidae" genus="Bosminopsis" kingdom="Animalia" order="Diplostraca" pageId="3" pageNumber="774" phylum="Arthropoda" rank="genus">
<emphasis id="B9785877FFE3FFC6FEE8FC19FE08FC0E" bold="true" box="[319,458,925,949]" italics="true" pageId="3" pageNumber="774">Bosminopsis</emphasis>
<bibRefCitation id="EF9DF994FFE3FFC6FE05FC19FDB1FC0E" author="Richard J" box="[466,627,925,949]" pageId="3" pageNumber="774" pagination="279 - 397" refId="ref15011" refString="Richard J (1895) Revision des Cladoceres I et II. Annales des sciences naturelles. Zoologie et biologie animale, 18: 279 - 397." type="journal article" year="1895">Richard, 1895</bibRefCitation>
