<taxonomicName id="4C615EB7FFDBDD2236A3F9F3FCD2F9B9" authorityName="Say" authorityYear="1818" box="[755,864,1552,1577]" class="Chlorophyceae" family="Characiaceae" genus="Lanceola" kingdom="Plantae" order="Sphaeropleales" pageId="10" pageNumber="11" phylum="Chlorophyta" rank="genus">
<emphasis id="B915F926FFDBDD2236A3F9F3FCD2F9B9" box="[755,864,1552,1577]" italics="true" pageId="10" pageNumber="11">Lanceola</emphasis>
</taxonomicName>
