<collectionCode id="ED08AE365B30FFC6FB38F3BFFAA2FE3C" box="[1238,1300,402,426]" country="Germany" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="85" pageNumber="86" type="Museum">ZSM</collectionCode>
