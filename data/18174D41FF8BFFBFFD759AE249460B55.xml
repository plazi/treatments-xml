<taxonomicName id="57BE87D4FF95FFBFFEA19CCB4F4D0B2E" authorityName="de Blainville" authorityYear="1816" box="[359,518,1023,1049]" class="Elasmobranchii" family="Heterodontidae" genus="Heterodontus" kingdom="Animalia" order="Heterodontiformes" pageId="16" pageNumber="17" phylum="Chordata" rank="genus">
