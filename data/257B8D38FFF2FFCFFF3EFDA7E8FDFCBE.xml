<taxonomicName id="6AD247ADFFF2FFCFFEBAFD1BEB5DFD12" authorityName="Silva &amp; Fernandes" authorityYear="2013" box="[275,582,727,753]" class="Insecta" family="Pentatomidae" genus="Paraedessa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="subretangulata">
