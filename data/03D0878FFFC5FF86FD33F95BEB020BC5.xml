<paragraph id="8BC63699FFC5FF9AFD65FEEBE9B20E01" blockId="13.[745,1180,1491,1543]" pageId="13" pageNumber="14">
<taxonomicName id="4C794D1AFFC5FF9AFD65FEEBE9B20E01" authority="Cockerell" class="Insecta" family="Apidae" genus="Oreopasites" kingdom="Animalia" order="Hymenoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="vanduzeei" subGenus="Oreopasites">
<emphasis id="B90DEA8BFFC5FF9AFD65FEEBE93C0DEC" box="[745,885,1491,1514]" italics="true" pageId="13" pageNumber="14">Oreopasites</emphasis>
