<taxonomicName id="4C794D1AFFD8FF87FD70FF7BE9AC0C5E" box="[764,997,1091,1112]" class="Insecta" family="Apidae" genus="Oreopasites" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="barbarae">
