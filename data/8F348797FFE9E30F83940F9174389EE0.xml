<subSubSection id="4F87650AFFEAE30F83C40BD974389EE0" box="[199,1343,1233,1259]" pageId="6" pageNumber="349" type="etymology">
<paragraph id="07223681FFEAE30F83C40BD974389EE0" blockId="6.[151,1437,153,1259]" box="[199,1343,1233,1259]" pageId="6" pageNumber="349">
<emphasis id="35E9EA93FFEAE30F83C40BD970499EE0" bold="true" box="[199,334,1233,1259]" pageId="6" pageNumber="349">Etymology.</emphasis>
