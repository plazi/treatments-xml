<taxonomicName id="4C704D5FFFDDA1129D53FA42FE0EE8B3" box="[312,484,1424,1447]" class="Insecta" family="Orphnidae" genus="Triodontus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="1074" phylum="Arthropoda" rank="species" species="itremoi">
