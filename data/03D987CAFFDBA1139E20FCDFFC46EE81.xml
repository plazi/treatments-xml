<taxonomicName id="4C704D5FFFDDA1129E64FA42FD9DE8B3" baseAuthorityName="Guérin-Méneville" baseAuthorityYear="1829" box="[527,631,1424,1447]" class="Insecta" family="Orphnidae" genus="Triodontus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="1074" phylum="Arthropoda" rank="species" species="nitidulus">
