]&quot; &quot;Holotype 
<taxonomicName id="CC2751CC3492DD94740F4DD26281D187" authorityName="Hendrich &amp; Watts &amp; Balke" authorityYear="2020" class="Insecta" family="Dytiscidae" genus="Gibbidessus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Gibbidessus kangarooensis" order="Coleoptera" pageId="0" pageNumber="11" phylum="Arthropoda" rank="species" species="kangarooensis">
