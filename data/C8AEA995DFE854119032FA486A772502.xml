<paragraph id="0E6071CABC3B5C40B362D87238CCECC7" pageId="0" pageNumber="11">Type material.</paragraph>
<paragraph id="3F7F473AFF5D0C5A03F4D219900BB1D4" pageId="0" pageNumber="11">
<emphasis id="0876390F6C52EB5B2E2050583CD4E27C" bold="true" pageId="0" pageNumber="11">
<emphasis id="F89C8DD20AA8F4F57AD680E6FE369F9F" italics="true" pageId="0" pageNumber="11">Holotype</emphasis>
