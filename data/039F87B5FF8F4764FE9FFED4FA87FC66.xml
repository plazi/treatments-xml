<taxonomicName id="4C364D20FF884764FB66FD7EFA87FD16" authorityName="Kuschel" authorityYear="2008" baseAuthorityName="Kuschel" baseAuthorityYear="2008" box="[1257,1357,678,701]" class="Insecta" family="Curculionidae" genus="Idastes" kingdom="Animalia" order="Coleoptera" pageId="76" pageNumber="175" phylum="Arthropoda" rank="species" species="montanus">
