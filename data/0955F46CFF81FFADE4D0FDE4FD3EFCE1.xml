<taxonomicName id="46FC3EF9FF81FFADE558FC8BFD3DFCE1" authority="Schonmann, 1994" authorityName="Schonmann" authorityYear="1994" box="[287,664,773,797]" class="Insecta" family="Hydrophilidae" genus="Pelthydrus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="105" phylum="Arthropoda" rank="subGenus" subGenus="Globipelthydrus">
