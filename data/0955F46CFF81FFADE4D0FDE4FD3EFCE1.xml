(
<emphasis id="B3889968FF81FFADE5A0FD6BFD31FD01" box="[487,660,742,765]" italics="true" pageId="22" pageNumber="105">Globipelthydrus</emphasis>
) 
<emphasis id="B3889968FF81FFADE6E3FD6BFD54FD01" box="[676,753,742,765]" italics="true" pageId="22" pageNumber="105">jendeki</emphasis>
</taxonomicName>
