<number id="3DE724ED3675FF8BBAD65A04A861FAC0" box="[485,513,1380,1404]" pageId="1" pageNumber="46" value="13.0">13</number>
: 
<number id="3DE724ED3675FF8BB92A5A04A858FAC0" box="[537,568,1380,1404]" pageId="1" pageNumber="46" value="35.0">35</number>
; length of femur, tibia and basitarsus of hind leg 
<number id="3DE724ED3675FF8BBF595A04AEEFFAC0" box="[1130,1167,1380,1404]" pageId="1" pageNumber="46" value="3.5">3.5</number>
, 
<number id="3DE724ED3675FF8BBFAF5A04AEDFFAC0" box="[1180,1215,1380,1404]" pageId="1" pageNumber="46" value="8.5">8.5</number>
