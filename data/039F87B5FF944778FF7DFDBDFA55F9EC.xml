<taxonomicName id="4C364D20FF944778FF7DFDBDFE10FDD7" authority="Kuschel, 2008" authorityName="Kuschel" authorityYear="2008" box="[242,474,613,636]" class="Insecta" family="Curculionidae" genus="Ottistira" kingdom="Animalia" order="Coleoptera" pageId="80" pageNumber="179" phylum="Arthropoda" rank="species" species="koghiensis" status="n. sp.">
