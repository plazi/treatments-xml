<normalizedToken id="C62586EFE1011853C337892C9AC52713" originalValue="Stål">Stal</normalizedToken>
(= 
<taxonomicName id="47DBBE377BC905D72E58BA7303273FF0" class="Insecta" family="Chrysomelidae" genus="Pachybrachis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pachybrachis crassicollis" order="Coleoptera" pageId="37" pageNumber="48" phylum="Arthropoda" rank="species" species="crassicollis">Pachybrachis crassicollis</taxonomicName>
