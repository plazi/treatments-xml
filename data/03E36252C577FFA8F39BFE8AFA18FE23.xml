</taxonomicName>
from 
<collectingCountry id="F35D93D4C577FFA9F16CFE8AFD2BFEDB" box="[608,686,295,321]" name="Oman" pageId="72" pageNumber="73">Oman</collectingCountry>
</emphasis>
