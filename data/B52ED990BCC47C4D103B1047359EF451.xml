<taxonomicName id="AF179FD3FE06E1420DD107169FEAB58D" class="Magnoliopsida" family="Tetramelaceae" genus="Octomeles" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Octomeles" order="Cucurbitales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="genus">
<emphasis id="83B5A2C3C493BD23F4D66A6C289E641C" italics="true" pageId="0" pageNumber="1">Octomeles</emphasis>
</taxonomicName>
