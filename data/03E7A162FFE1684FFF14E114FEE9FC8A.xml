<location id="8E9146AFFFE1684FFB61E2C8FA63FD7E" LSID="urn:lsid:plazi:treatment:03E7A162FFE1684FFF14E114FEE9FC8A:8E9146AFFFE1684FFB61E2C8FA63FD7E" box="[1250,1432,691,717]" county="Bregetova" municipality="Zoological Institute" name="Sergey Mironov" pageId="18" pageNumber="350" stateProvince="Leningrad">Sergey Mironov</location>
