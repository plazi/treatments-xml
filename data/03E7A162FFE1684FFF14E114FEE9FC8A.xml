<location id="8E9146AFFFE1684FFE2DE2ACFDDFFD42" LSID="urn:lsid:plazi:treatment:03E7A162FFE1684FFF14E114FEE9FC8A:8E9146AFFFE1684FFE2DE2ACFDDFFD42" box="[430,548,727,753]" county="Bregetova" municipality="Zoological Institute" name="University" pageId="18" pageNumber="350" stateProvince="Leningrad">University</location>
