<paragraph id="8B4936A5161BFFA5FF2BF942FD9BFD79" blockId="15.[151,584,1767,1793]" box="[151,584,1767,1793]" pageId="15" pageNumber="58">
<heading id="D00181C9161BFFA5FF2BF942FD9BFD79" bold="true" box="[151,584,1767,1793]" fontSize="11" level="1" pageId="15" pageNumber="58" reason="1">
<taxonomicName id="4CF64D26161BFFA5FF2BF942FD9BFD79" authority="(Signoret, 1853)" baseAuthorityName="Signoret" baseAuthorityYear="1853" box="[151,584,1767,1793]" class="Insecta" family="Cicadellidae" genus="Exogonia" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="58" phylum="Arthropoda" rank="species" species="assimilis">
