<taxonomicName id="CFA5546AA06ADE46170B2EE3BC3EF40F" authority="Wesmael, 1837" authorityName="Wesmael" authorityYear="1837" class="Insecta" family="Braconidae" genus="Therophilus" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Therophilus" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Therophilus</taxonomicName>
