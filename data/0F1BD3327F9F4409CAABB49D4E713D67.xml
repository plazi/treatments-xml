<taxonomicName id="ED7190A737AADF1DDB06D1D40BB0331D" authority="Wesmael, 1837" authorityName="Wesmael" authorityYear="1837" class="Insecta" family="Braconidae" genus="Therophilus" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Therophilus" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Therophilus</taxonomicName>
