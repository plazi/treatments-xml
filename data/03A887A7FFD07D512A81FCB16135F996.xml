(
<bibRefCitation id="EF904B40FFD07D512F11FA1365EEF996" author="Deichmann, E." pageId="30" pageNumber="31" refId="ref25431" refString="Deichmann, E. (1936) The Alcyonaria of the western part of the Atlantic Ocean. Memoirs of the Museum of Comparative Zoology, 53, 317 pp." type="book" year="1936">Deichmann, 1936</bibRefCitation>
; 
