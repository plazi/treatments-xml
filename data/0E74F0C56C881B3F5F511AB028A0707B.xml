<taxonomicName id="D9AB0D0FF33FCA6F949661F554457D94" authorityName="Kimsey" authorityYear="2011" class="Hexapoda" family="Tiphiidae" genus="Methocha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Hymenoptera" pageId="9" pageNumber="54" phylum="Arthropoda" rank="species" species="flavipalpus">
