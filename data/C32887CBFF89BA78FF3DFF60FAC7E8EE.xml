<quantity id="8C799B38FF89BA7AFEE4F947FE78ECEB" box="[334,405,1712,1737]" metricMagnitude="4" metricUnit="m" metricValue="2.0" pageId="25" pageNumber="26" unit="km" value="20.0">20 km</quantity>
