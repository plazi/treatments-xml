<quantity id="05629B2FFFB1FFF4FDC6FCF69DB3977B" box="[572,674,852,879]" metricMagnitude="-2" metricUnit="m" metricValue="1.02" pageId="15" pageNumber="416" unit="mm" value="10.2">10.2 mm</quantity>
, D= 
<quantity id="05629B2FFFB1FFF4FD28FCF69C24977B" box="[722,821,852,879]" metricMagnitude="-2" metricUnit="m" metricValue="1.41" pageId="15" pageNumber="416" unit="mm" value="14.1">14.1 mm</quantity>
