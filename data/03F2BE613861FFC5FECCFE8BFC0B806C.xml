) 
<number id="3D931D503861FFC2FE00FC70FE30816E" box="[452,481,999,1025]" pageId="3" pageNumber="44" value="21.0">21</number>
(
<number id="3D931D503861FFC2FE37FC70FDEE816E" box="[499,575,999,1025]" pageId="3" pageNumber="44" value="23.0" valueMax="25.0" valueMin="21.0">21–25</number>
