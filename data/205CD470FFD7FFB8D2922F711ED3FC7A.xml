<taxonomicName id="6FF51EE5FFD7FFB8D0652FCE1E8CFC35" box="[581,641,917,938]" class="Liliopsida" family="Asparagaceae" genus="Hosta" kingdom="Plantae" order="Asparagales" pageId="39" pageNumber="1716" phylum="Tracheophyta" rank="genus">
