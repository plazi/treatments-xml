<taxonomicName id="8A48F224C09DEC6388C866300774A82A" class="Magnoliopsida" family="Caryophyllaceae" kingdom="Plantae" order="Caryophyllales" phylum="Tracheophyta" rank="family">Caryophyllaceae</taxonomicName>
