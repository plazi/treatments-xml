<taxonomicName id="47B04D34A211BB5CFC79DC23FB798812" box="[947,1155,1584,1607]" class="Bivalvia" family="Poromyidae" genus="Dilemma" kingdom="Animalia" order="Anomalodesmata" pageId="13" pageNumber="14" phylum="Mollusca" rank="species" species="spectralis">
