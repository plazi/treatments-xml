<collectingCountry id="3181E7C7FF89FFE6FDBEFB78FD286F0F" box="[617,697,1266,1291]" name="China" pageId="4" pageNumber="192">China</collectingCountry>
(
<collectingRegion id="8B5269B5FF89FFE6FD1BFB78FCA96F0F" box="[716,824,1266,1291]" country="China" name="Yunnan" pageId="4" pageNumber="192">Yunnan</collectingRegion>
