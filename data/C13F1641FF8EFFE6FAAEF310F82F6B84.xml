<caption id="1DE9F7DFFF89FFE6FF63FCB2FBD76C42" ID-DOI="http://doi.org/10.5281/zenodo.5734715" ID-Zenodo-Dep="5734715" httpUri="https://zenodo.org/record/5734715/files/figure.png" inLine="true" pageId="4" pageNumber="192" targetBox="[177,769,285,699]" targetPageId="4">
