<emphasis id="5DDAEAA0FFB1E15AFF5DE0F6CE0F89E8" bold="true" box="[151,368,439,465]" italics="true" pageId="60" pageNumber="61">
<taxonomicName id="A8AE4D31FFB1E15AFF5DE0F6CE1589E8" authority="Nielson, 2011" authorityName="Nielson" authorityYear="2011" box="[151,362,439,465]" class="Insecta" family="Cicadellidae" genus="Docalidia" kingdom="Animalia" order="Hemiptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="tantula" status="sp. nov.">Docalidia tantula</taxonomicName>
