<taxonomicName id="04F14D34F301FFBE16B3F894FBD9FE42" box="[904,1032,1864,1888]" class="Insecta" family="Aradidae" genus="Aradus" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="83" phylum="Arthropoda" rank="species" species="persicus">
