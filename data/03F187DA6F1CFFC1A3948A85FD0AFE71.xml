<taxonomicName id="4C584D4F6F1BFFC0A1038EEEFCABF84B" authority="(Deshayes, 1855)" baseAuthorityName="Deshayes" baseAuthorityYear="1855" box="[512,895,1976,1998]" class="Bivalvia" family="Tellinidae" genus="Pristipagia" kingdom="Animalia" order="Cardiida" pageId="76" pageNumber="77" phylum="Mollusca" rank="species" species="radians">
