<superScript id="38802804FF8F7D2BFF9193C62A1A97CD" attach="right" box="[109,117,671,683]" fontSize="5" pageId="7" pageNumber="8">a</superScript>
<taxonomicName id="08F5FECFFF8F7D2BFF8993FA2B9797D0" authority="Cockerell" authorityName="Cockerell" authorityYear="1933" box="[117,504,675,694]" class="Insecta" family="Apidae" genus="Tetralonia" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="trichardti" subGenus="Thygatina">
<emphasis id="FD81595EFF8F7D2BFF8993FA2ABE97D0" box="[117,209,675,694]" italics="true" pageId="7" pageNumber="8">Tetralonia</emphasis>
(
