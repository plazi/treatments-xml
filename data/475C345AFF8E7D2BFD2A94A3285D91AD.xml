<taxonomicName id="08F5FECFFF8F7D2BFEBF93E7286F97B6" authority="Cockerell" authorityName="Cockerell" authorityYear="1936" box="[323,512,701,721]" class="Insecta" family="Apidae" genus="Tetralonia" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="cinctula">
