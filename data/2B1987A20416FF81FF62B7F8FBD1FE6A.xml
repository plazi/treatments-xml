<taxonomicName id="64B04D370415FF87FDEEB477FCBEF9B0" box="[539,866,1552,1576]" pageId="122" pageNumber="123" rank="species" species="brachyurus" subGenus="Cosmocomoidea">
(
<emphasis id="91C4EAA60415FF87FDD1B476FD0AF9B0" box="[548,726,1553,1576]" italics="true" pageId="122" pageNumber="123">Cosmocomoidea</emphasis>
) 
<taxonomicName id="64B04D370415FF87FD1CB476FCBEF9B0" box="[745,866,1553,1576]" pageId="122" pageNumber="123" rank="species" species="brachyurus">
<emphasis id="91C4EAA60415FF87FD1CB476FCBEF9B0" box="[745,866,1553,1576]" italics="true" pageId="122" pageNumber="123">brachyurus</emphasis>
