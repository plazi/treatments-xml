<taxonomicName id="64B04D370415FF87FE73B476FCBEF9B0" baseAuthorityName="Ogloblin" baseAuthorityYear="1938" box="[390,866,1552,1576]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="122" pageNumber="123" phylum="Arthropoda" rank="species" species="brachyurus" subGenus="Cosmocomoidea">
