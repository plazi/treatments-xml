<taxonomicName id="3CECCAED608BD4C9FFFA0178F5C0CDCA" authority="L." class="Magnoliopsida" family="Lamiaceae" genus="Teucrium" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="97" phylum="Tracheophyta" rank="species" species="scorodonia">
