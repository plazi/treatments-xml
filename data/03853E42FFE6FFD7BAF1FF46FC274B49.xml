<taxonomicName id="4C2CF4D7FFE4FFD8BB82F906FD154D1D" authority="(Schacht, 1897)" baseAuthorityName="Schacht" baseAuthorityYear="1897" box="[254,685,1374,1397]" class="Hexanauplia" family="Diaptomidae" genus="Aglaodiaptomus" kingdom="Animalia" order="Calanoida" pageId="7" pageNumber="484" phylum="Arthropoda" rank="species" species="clavipes">
<emphasis id="B9585346FFE4FFD8BB82F906FE414D1C" box="[254,505,1375,1397]" italics="true" pageId="7" pageNumber="484">Aglaodiaptomus clavipes</emphasis>
