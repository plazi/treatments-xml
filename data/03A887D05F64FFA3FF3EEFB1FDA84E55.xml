<subSubSection id="C31B654D5F68FFA3FF6EEBD4FBBD4E70" pageId="42" pageNumber="43" type="etymology">
<paragraph id="8BBE36C65F68FFA3FF6EEBD4FBCA4FE5" blockId="42.[151,1436,150,357]" pageId="42" pageNumber="43">
<emphasis id="B975EAD45F68FFA3FF6EEBD4FEFD4F80" bold="true" box="[199,338,150,176]" pageId="42" pageNumber="43">Etymology:</emphasis>
