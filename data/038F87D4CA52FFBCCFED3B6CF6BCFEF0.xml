<collectingCountry id="F3317652CA52FFBDC2C736D6F684F56B" box="[2210,2306,2782,2807]" name="Kuwait" pageId="2" pageNumber="621">Kuwait</collectingCountry>
, 
<collectingCountry id="F3317652CA52FFBDC37636D6F614F56B" box="[2323,2450,2782,2807]" name="Lebanon" pageId="2" pageNumber="621">Lebanon</collectingCountry>
