<quantity id="4CDC9B72FF93901BF8D2FED1BF3AFE2E" metricMagnitude="-3" metricUnit="m" metricValue="1.4" pageId="5" pageNumber="312" unit="mm" value="1.4">1.4 mm</quantity>
