<quantity id="4CDC9B72FF93901BFE74FE11BFF5FE0E" box="[813,918,418,441]" metricMagnitude="-3" metricUnit="m" metricValue="1.15" pageId="5" pageNumber="312" unit="mm" value="1.15">1.15 mm</quantity>
