<quantity id="4CDC9B72FF93901BFEF6FE31B872FE2E" box="[943,1041,386,409]" metricMagnitude="-3" metricUnit="m" metricValue="1.03" pageId="5" pageNumber="312" unit="mm" value="1.03">1.03 mm</quantity>
