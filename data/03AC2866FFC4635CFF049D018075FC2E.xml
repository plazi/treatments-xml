</figureCitation>
B) Setae 
<emphasis id="B9714562FFC7635CFDD79898837AFF4F" box="[580,606,152,176]" italics="true" pageId="15" pageNumber="548">
<number id="3DCD8B57FFC7635CFDD798988374FF4F" box="[580,592,152,176]" pageId="15" pageNumber="548" value="1.0">1</number>
c
</emphasis>
, 
<emphasis id="B9714562FFC7635CFDFC989883ADFF4F" box="[623,649,152,176]" italics="true" pageId="15" pageNumber="548">
<number id="3DCD8B57FFC7635CFDFC98988359FF4F" box="[623,637,152,176]" pageId="15" pageNumber="548" value="3.0">3</number>
c
</emphasis>
barbed; setae 
<emphasis id="B9714562FFC7635CFCA09898826AFF4F" box="[819,846,152,176]" italics="true" pageId="15" pageNumber="548">
<number id="3DCD8B57FFC7635CFCA09898821BFF4F" box="[819,831,152,176]" pageId="15" pageNumber="548" value="1.0">1</number>
a
</emphasis>
, 
<emphasis id="B9714562FFC7635CFCCC9898825FFF4F" box="[863,891,152,176]" italics="true" pageId="15" pageNumber="548">
<number id="3DCD8B57FFC7635CFCCC98988249FF4F" box="[863,877,152,176]" pageId="15" pageNumber="548" value="2.0">2</number>
a
</emphasis>
, 
<emphasis id="B9714562FFC7635CFC1F9898828CFF4F" box="[908,936,152,176]" italics="true" pageId="15" pageNumber="548">
<number id="3DCD8B57FFC7635CFC1F989882BEFF4F" box="[908,922,152,176]" pageId="15" pageNumber="548" value="3.0">3</number>
a
</emphasis>
