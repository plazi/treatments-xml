<emphasis id="B9714562FFC4635FFD2C9F9083FFF856" box="[703,731,1936,1961]" italics="true" pageId="12" pageNumber="545">lm</emphasis>
and 
<emphasis id="B9714562FFC4635FFC819F90820CF856" box="[786,808,1936,1961]" italics="true" pageId="12" pageNumber="545">lp</emphasis>
, 
<emphasis id="B9714562FFC4635FFCA69F908267F856" box="[821,835,1936,1961]" italics="true" pageId="12" pageNumber="545">h</emphasis>
<number id="3DCD8B57FFC4635FFCD79F9F8268F852" box="[836,844,1951,1965]" pageId="12" pageNumber="545" value="3.0">3</number>
