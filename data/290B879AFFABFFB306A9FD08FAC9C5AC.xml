<number id="176A24ABFFABFFB306D7FCFBFE84C12C" box="[233,261,855,879]" pageId="2" pageNumber="249" value="33.0">33</number>
(
<number id="176A24ABFFABFFB30726FCFAFEE2C12C" box="[280,355,854,879]" pageId="2" pageNumber="249" value="34.0" valueMax="35.0" valueMin="33.0">33–35</number>
