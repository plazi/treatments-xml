<number id="176A24ABFFABFFB30709FC9EFED2C108" box="[311,339,818,843]" pageId="2" pageNumber="249" value="35.0">35</number>
(
<number id="176A24ABFFABFFB3075AFC9EFE2FC108" box="[356,430,818,843]" pageId="2" pageNumber="249" value="36.0" valueMax="37.0" valueMin="35.0">35–37</number>
