<taxonomicName id="4C4453C13D14BD2CFEEE8F9F9E66FA43" authority="Durga-Prasad, Radhakrishna, Khalaf &amp; Al-Jaafery, 1981" authorityName="Durga-Prasad, Radhakrishna, Khalaf &amp; Al-Jaafery" authorityYear="1981" box="[270,1087,1482,1509]" class="Branchiopoda" family="Cyzicidae" genus="Eocyzicus" kingdom="Animalia" order="Diplostraca" pageId="11" pageNumber="265" phylum="Arthropoda" rank="species" species="spinifer">
