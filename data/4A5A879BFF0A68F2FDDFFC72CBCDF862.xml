<taxonomicName id="05F34D0EFF0B68F2FBC9F839CBC5F862" authorityName="Bellan-Santini" authorityYear="1972" box="[1086,1234,2001,2027]" class="Malacostraca" family="Epimeriidae" genus="Subepimeria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="141" pageNumber="142" phylum="Arthropoda" rank="genus">
