<taxonomicName id="4C2B4D40FFF5FFB81312C5A32CC53EE9" box="[889,1019,1451,1476]" class="Pycnogonida" family="Ammotheidae" genus="Tanystylum" higherTaxonomySource="CoL" kingdom="Animalia" order="Pantopoda" pageId="4" pageNumber="61" phylum="Arthropoda" rank="genus">
