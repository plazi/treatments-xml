var. 
<emphasis id="4899EAC16E01FFC018A7FADFFD9BCB12" bold="true" box="[470,556,1352,1373]" italics="true" pageId="1" pageNumber="1468">syriacus</emphasis>
SCHMIEDEKNECHT
</emphasis>
, 
<emphasis id="4899EAC16E01FFC01A74FADFFC8FCB12" bold="true" box="[773,824,1352,1373]" pageId="1" pageNumber="1468">1910</emphasis>
