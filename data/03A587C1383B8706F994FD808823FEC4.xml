<taxonomicName id="4C0C4D54383B8703FBA2FC8C8A60FCA7" authorityName="" authorityYear="1829" box="[673,777,870,894]" class="Magnoliopsida" family="Fagaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="1" pageNumber="396" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
