<taxonomicName id="4C0C4D54383B8703F8B4FC8F8B67FCA7" authorityName="" authorityYear="1758" box="[439,526,869,894]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="396" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
