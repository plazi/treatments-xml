<number id="6C002498FFB4FFF08A3FF886800BF8A5" box="[862,870,1878,1892]" pageId="7" pageNumber="380" value="2.0">2</number>
, S 
<number id="6C002498FFB4FFF08AE4F88680E0F8A5" box="[901,909,1878,1892]" pageId="7" pageNumber="380" value="3.0">3</number>
, S 
<number id="6C002498FFB4FFF08ACCF88680D8F8A5" box="[941,949,1878,1892]" pageId="7" pageNumber="380" value="4.0">4</number>
, S 
<date id="AE76107FFFB4FFF08AB5F886808DF8A5" box="[980,992,1878,1892]" pageId="7" pageNumber="380">
<number id="6C002498FFB4FFF08AB5F88680B1F8A5" box="[980,988,1878,1892]" pageId="7" pageNumber="380" value="4.0">4</number>
i
</date>
, S 
<number id="6C002498FFB4FFF08D61F8868765F8A5" box="[1024,1032,1878,1892]" pageId="7" pageNumber="380" value="5.0">5</number>
, S 
<date id="AE76107FFFB4FFF08D42F8868742F8A5" box="[1059,1071,1878,1892]" pageId="7" pageNumber="380">
<number id="6C002498FFB4FFF08D42F8868746F8A5" box="[1059,1067,1878,1892]" pageId="7" pageNumber="380" value="5.0">5</number>
