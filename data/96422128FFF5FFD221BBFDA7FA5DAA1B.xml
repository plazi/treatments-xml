<taxonomicName id="D9EBEBBDFFF5FFD2203EFDA7FD5AA05E" authorityName="Lojacono-Pojero" authorityYear="1891" box="[269,646,590,617]" class="Magnoliopsida" family="Fabaceae" genus="Anthyllis" kingdom="Plantae" order="Fabales" pageId="2" pageNumber="213" phylum="Tracheophyta" rank="variety" species="heterophylla" variety="busambarensis">
<emphasis id="2C9F4C2CFFF5FFD2203EFDA7FE42A05E" bold="true" box="[269,414,590,617]" italics="true" pageId="2" pageNumber="213">heterophylla</emphasis>
var. 
<emphasis id="2C9F4C2CFFF5FFD220EAFDA7FD5AA05E" bold="true" box="[473,646,590,617]" italics="true" pageId="2" pageNumber="213">busambarensis</emphasis>
