<taxonomicName id="DBC639AB297E0702FBBDAE9571CEC4DD" baseAuthorityName="Horvath" baseAuthorityYear="1893" box="[1094,1429,1737,1761]" class="Insecta" family="Pentatomidae" genus="Diploxys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="288" phylum="Arthropoda" rank="species" species="holubi" subGenus="Paracoponia">
