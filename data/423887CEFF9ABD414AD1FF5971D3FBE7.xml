<collectingCounty id="234F4E54FF9ABD414809FC727042FC0B" box="[592,1044,950,976]" pageId="5" pageNumber="234">Parque Estadual da Serra do Brigadeiro</collectingCounty>
, 
<collectingRegion id="0855F83AFF9ABD414E79FC7270EDFC0B" box="[1056,1211,950,976]" country="Brazil" name="Minas Gerais" pageId="5" pageNumber="234">Minas Gerais</collectingRegion>
