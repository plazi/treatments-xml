<taxonomicName id="17FBE0237FF80D0E5E85AAAD70DF2E89" authority="Bourguignat, 1884" authorityName="Bourguignat" authorityYear="1884" class="Gastropoda" family="Melanopsidae" genus="Fagotia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Fagotia sobrievskii" order="Sorbeoconcha" pageId="254" pageNumber="255" phylum="Mollusca" rank="species" species="sobrievskii">
