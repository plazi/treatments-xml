<subSubSection id="C32615A4FFADBF2DFF5218DBFC81B8A0" pageId="8" pageNumber="209" type="etymology">
<paragraph id="8B83462FFFADBF2DFF5218DBFA91B8C5" blockId="8.[151,1437,151,1221]" box="[199,1360,1159,1185]" pageId="8" pageNumber="209">
<emphasis id="B9489A3DFFADBF2DFF5218DBFE8FB8C5" bold="true" box="[199,334,1159,1184]" pageId="8" pageNumber="209">Etymology.</emphasis>
