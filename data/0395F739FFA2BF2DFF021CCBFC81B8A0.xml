(
<quantity id="4CC4EBCAFFADBF2DFF441838FEB9B819" box="[209,376,1124,1149]" metricMagnitude="-4" metricUnit="m" metricValue="4.699999999999999" metricValueMax="5.1" metricValueMin="4.3" pageId="8" pageNumber="209" unit="mm" value="0.47" valueMax="0.51" valueMin="0.43">0.43–0.51 mm</quantity>
