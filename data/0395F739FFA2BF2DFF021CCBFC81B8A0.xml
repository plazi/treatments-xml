<quantity id="4CC4EBCAFFADBF2DFD171840FD27B851" box="[642,742,1052,1076]" metricMagnitude="-3" metricUnit="m" metricValue="1.91" pageId="8" pageNumber="209" unit="mm" value="1.91">1.91 mm</quantity>
(
<number id="3DF45408FFADBF2DFD601840FCE7B850" box="[757,806,1052,1077]" pageId="8" pageNumber="209" value="2.11">2.11</number>
– 
<quantity id="4CC4EBCAFFADBF2DFCA31840FC5AB851" box="[822,923,1052,1077]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" pageId="8" pageNumber="209" unit="mm" value="1.75">1.75 mm</quantity>
, N = 
<number id="3DF45408FFADBF2DFC4E1840FC3BB851" box="[987,1018,1052,1076]" pageId="8" pageNumber="209" value="10.0">10</number>
), pronotal length = average 
<quantity id="4CC4EBCAFFADBF2DFAAD1840FA5DB851" box="[1336,1436,1052,1077]" metricMagnitude="-4" metricUnit="m" metricValue="6.6" pageId="8" pageNumber="209" unit="mm" value="0.66">0.66 mm</quantity>
