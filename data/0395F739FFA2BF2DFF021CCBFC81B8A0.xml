, N = 
<number id="3DF45408FFADBF2DFE17181CFE60B83D" box="[386,417,1088,1112]" pageId="8" pageNumber="209" value="10.0">10</number>
), pronotal width = average 
<quantity id="4CC4EBCAFFADBF2DFD411863FCF6B832" box="[724,823,1087,1112]" metricMagnitude="-4" metricUnit="m" metricValue="9.6" pageId="8" pageNumber="209" unit="mm" value="0.96">0.96 mm</quantity>
(
<number id="3DF45408FFADBF2DFCD3181CFCB8B83D" box="[838,889,1088,1112]" pageId="8" pageNumber="209" value="0.92">0.92</number>
–1.00 mm, N = 
<number id="3DF45408FFADBF2DFBBC181CFB86B83D" box="[1065,1095,1088,1112]" pageId="8" pageNumber="209" value="10.0">10</number>
), elytral length = average 
