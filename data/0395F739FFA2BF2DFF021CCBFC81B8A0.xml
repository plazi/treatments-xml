(
<quantity id="4CC4EBCAFFADBF2DFCF01838FBCDB819" box="[869,1036,1124,1149]" metricMagnitude="-4" metricUnit="m" metricValue="6.35" metricValueMax="6.6" metricValueMin="6.1" pageId="8" pageNumber="209" unit="mm" value="0.635" valueMax="0.66" valueMin="0.61">0.61–0.66 mm</quantity>
, N = 
<number id="3DF45408FFADBF2DFBDC1838FBA9B819" box="[1097,1128,1124,1148]" pageId="8" pageNumber="209" value="10.0">10</number>
).
</paragraph>
