(
<bibRefCitation id="E843E6FDC34B9640502CB8D9C2B6FC67" author="Lang K" box="[1281,1413,993,1015]" pageId="34" pageNumber="443" pagination="1 - 560" refId="ref25283" refString="Lang K. 1965. Copepoda Harpacticoida from the Californian Pacific coast. Kungliga Svenska Vetenskapsakademiens Handlingar (4) 10: 1 - 560." type="journal article" year="1965">Lang, 1965</bibRefCitation>
