<number id="3DF824F07332FFFF69ED6044AEBDCD74" box="[222,253,1832,1856]" pageId="22" pageNumber="323" value="97.0">97</number>
</figureCitation>
; 
<number id="3DF824F07332FFFF683A604BAF2ECD74" box="[265,366,1831,1856]" pageId="22" pageNumber="323" value="104.5" valueMax="105.0" valueMin="104.0">104–105</number>
