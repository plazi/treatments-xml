<taxonomicName id="4C35ECC8036D044BFD15FF326329FF39" box="[688,853,152,176]" class="Adenophorea" family="Plectidae" genus="Plectus" kingdom="Animalia" order="Araeolaimida" pageId="11" pageNumber="12" phylum="Nematoda" rank="species" species="parvus">
