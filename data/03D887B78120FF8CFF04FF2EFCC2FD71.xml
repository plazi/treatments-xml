<taxonomicName id="4C714D228120FF8CFA1FFE65FF46FDC1" class="Liliopsida" family="Orchidaceae" genus="Acampe" kingdom="Plantae" order="Asparagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="longifolia">
