<taxonomicName id="4C714D228120FF8CFF3CFE65FECCFE3D" box="[176,383,469,495]" class="Liliopsida" family="Orchidaceae" genus="Acampe" kingdom="Plantae" order="Asparagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="longifolia">
