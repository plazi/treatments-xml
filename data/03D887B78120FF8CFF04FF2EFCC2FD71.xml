<taxonomicName id="4C714D228120FF8CFB7BFE65FAE8FE3D" box="[1271,1371,469,495]" class="Liliopsida" family="Orchidaceae" genus="Aerides" kingdom="Plantae" order="Asparagales" pageId="15" pageNumber="16" phylum="Tracheophyta" rank="species" species="rigida">
