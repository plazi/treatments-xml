<taxonomicName id="A1DF4D2FFF61FF60BEDABC0710218E46" baseAuthorityName="Goetghebuer" baseAuthorityYear="1921" box="[999,1113,357,378]" class="Insecta" family="Chironomidae" genus="Pseudosmittia" kingdom="Animalia" order="Diptera" pageId="150" pageNumber="151" phylum="Arthropoda" rank="species" species="forcipata">
