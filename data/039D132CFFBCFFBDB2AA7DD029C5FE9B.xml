, NE 
<collectingCountry id="F323E2AAFFB3FFBDB3CF79A22F00FF7F" box="[498,556,223,248]" name="India" pageId="8" pageNumber="449">India</collectingCountry>
(north of 
<collectingRegion id="49F06CD8FFB3FFBDB0A0799D2E01FF7F" box="[669,813,223,248]" country="India" name="West Bengal" pageId="8" pageNumber="449">West Bengal</collectingRegion>
, 
<collectingRegion id="49F06CD8FFB3FFBDB10579A22EA7FF7F" box="[824,907,223,248]" country="India" name="Sikkim" pageId="8" pageNumber="449">Sikkim</collectingRegion>
) (
<bibRefCitation id="EFA5DFCBFFB3FFBDB19C79A22905FF7F" author="Elwes" box="[929,1065,223,248]" pageId="8" pageNumber="449" refString="Elwes, H. J. (1890) On some new moths from India. Proceedings of the Zoological Society of London, 1890, 378 - 401." type="proceedings paper" year="1890">Elwes 1890</bibRefCitation>
; 
<bibRefCitation id="EFA5DFCBFFB3FFBDB608799D29CFFF7F" author="Hampson" box="[1077,1251,224,248]" pageId="8" pageNumber="449" refString="Hampson, G. F. (1900) Catalogue of the Arctiadae (Nolinae, Lithosianae) in the British Museum. Catalogue of the Lepidoptera Phalaenae in the British Museum, 2, 1 - 590." type="journal article" year="1900">Hampson 1900</bibRefCitation>
