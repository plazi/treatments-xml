<geoCoordinate id="739C17DEAA3FDF702A94BE566D74AD6D" direction="north" orientation="latitude" precision="5" value="40.2418">40.2418°N</geoCoordinate>
, 
<geoCoordinate id="D2A4DF235495D8CE691EF4AB1E94B213" direction="east" orientation="longitude" precision="5" value="20.6414">20.6414°E</geoCoordinate>
