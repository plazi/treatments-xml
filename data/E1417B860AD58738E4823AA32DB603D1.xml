, 
<taxonomicName id="E0C68A406C5AD5B725AA9A903B1B7E1D" baseAuthorityName="Reeve" baseAuthorityYear="1849" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="84" pageNumber="85" phylum="Mollusca" rank="species" species="laetus" subGenus="Mesembrinus">
