<collectingMunicipality id="CD70AC9FFFDAFFD4FF0EF888FE9DF880" box="[151,376,1805,1830]" pageId="55" pageNumber="56">
western 
<collectingCountry id="55BC7675FFDAFFD4FF6CF888FEA7F880" box="[245,322,1806,1830]" name="Kenya" pageId="55" pageNumber="56">Kenya</collectingCountry>
. The
</collectingMunicipality>
