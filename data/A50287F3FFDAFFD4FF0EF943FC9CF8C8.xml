<location id="2874603EFFDAFFD4FEC4F96FFE3CF8A4" LSID="urn:lsid:plazi:treatment:A50287F3FFDAFFD4FF0EF943FC9CF8C8:2874603EFFDAFFD4FEC4F96FFE3CF8A4" box="[349,473,1769,1794]" country="Guinea-Bissau" municipality="western Kenya. The" name="This forest" pageId="55" pageNumber="56">This forest</location>
species is distributed from 
<collectingCountry id="55BC7675FFDAFFD4FC87F96FFC26F8A4" box="[798,963,1769,1794]" name="Guinea-Bissau" pageId="55" pageNumber="56">Guinea-Bissau</collectingCountry>
to 
<collectingCountry id="55BC7675FFDAFFD4FC77F96FFB13F8A4" box="[1006,1270,1769,1794]" isEnumeration="true" pageId="55" pageNumber="56">
<collectingCountry id="55BC7675FFDAFFD4FC77F96FFBA6F8A4" box="[1006,1091,1769,1794]" name="Angola" pageId="55" pageNumber="56">Angola</collectingCountry>
