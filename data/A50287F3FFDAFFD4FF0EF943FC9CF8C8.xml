WL: 
<quantity id="EA539B00FFDAFFD4FE9DF96CFEB5F8A4" box="[260,336,1770,1794]" metricMagnitude="-2" metricUnit="m" metricValue="1.7" pageId="55" pageNumber="56" unit="mm" value="17.0">17mm</quantity>
