<taxonomicName id="4C3395F3DD2A044BD21DFDDEFFD3A41B" box="[1293,1435,599,624]" class="Insecta" family="Apidae" genus="Bremus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="religiosus">
<emphasis id="B9473262DD2A044BD21DFDDEFFD3A41B" box="[1293,1435,599,624]" italics="true" pageId="30" pageNumber="31">B. religiosus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C3395F3DD2A044BD7D6FDFAFB09A4FE" box="[198,321,636,661]" class="Insecta" family="Apidae" genus="Bombus" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="sushkini">
<emphasis id="B9473262DD2A044BD7D6FDFAFB09A4FE" box="[198,321,636,661]" italics="true" pageId="30" pageNumber="31">B. sushkini</emphasis>
</taxonomicName>
(see the key; 
<figureCitation id="1308F2F5DD2A044BD6CDFDFBF87AA4FF" box="[477,562,636,661]" captionStart="FIGURES 30 – 31. 30" captionStartId="29.[151,264,1763,1785]" captionTargetBox="[162,1435,1308,1738]" captionTargetId="figure@29.[151,1436,1308,1738]" captionTargetPageId="29" captionText="FIGURES 30 – 31. 30. Map showing the distribution of B. religiosus in North China with records as black spots, the province boundaries in dark grey, and all sites with records of all bumblebee species in light grey. 31. B. religiosus worker from Gansu." httpUri="https://zenodo.org/record/286577/files/figure.png" pageId="30" pageNumber="31">
Figs 
<number id="3DFBFC57DD2A044BD504FDFBF87AA4FF" box="[532,562,636,660]" pageId="30" pageNumber="31" value="31.33">31</number>
</figureCitation>
, 
<figureCitation id="1308F2F5DD2A044BD52EFDFBF814A4FF" box="[574,604,636,660]" captionStart="FIGURES 32 – 33. 32" captionStartId="30.[151,264,1272,1294]" captionTargetBox="[155,1435,823,1247]" captionTargetId="figure@30.[151,1436,823,1247]" captionTargetPageId="30" captionText="FIGURES 32 – 33. 32. Map showing the distribution of B. czerskii in North China with records as black spots, the province boundaries in dark grey, and all sites with records of all bumblebee species in light grey. 33. B. czerskii worker from Neimenggu." httpUri="https://zenodo.org/record/286578/files/figure.png" pageId="30" pageNumber="31">33</figureCitation>
