<taxonomicName id="E8BF4D3CE451FFAC85C41DAB2FEDFB66" authorityName="Tahami &amp; Muilwijk &amp; Lohaj &amp; Sadeghi" authorityYear="2017" box="[467,702,1169,1191]" class="Insecta" family="Carabidae" genus="Laemostenus" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="131" phylum="Arthropoda" rank="species" species="yazdensis">
