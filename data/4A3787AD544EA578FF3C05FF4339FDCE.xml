<taxonomicName id="059E4D38544DA579FE8E063F451AF993" box="[293,516,1610,1632]" class="Clitellata" family="Megascolecidae" genus="Metaphire" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opisthopora" pageId="22" pageNumber="517" phylum="Annelida" rank="species" species="hilgendorfi">
