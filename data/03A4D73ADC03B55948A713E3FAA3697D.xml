<taxonomicName id="4C0D1DAFDC03B55948841023FE11697D" authorityName="R.Chamberlin" authorityYear="1943" box="[227,452,1602,1626]" class="Chilopoda" family="Himantariidae" genus="Straberax" kingdom="Animalia" order="Geophilomorpha" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="morelus">
<emphasis id="B979BA3EDC03B55948841023FE11697D" box="[227,452,1602,1626]" italics="true" pageId="6" pageNumber="5">Straberax morelus</emphasis>
</taxonomicName>
para el 
<collectingRegion id="49C9A8CEDC03B5594A431023FCD6697D" box="[548,771,1602,1626]" country="Mexico" name="Morelos" pageId="6" pageNumber="5">estado de Morelos</collectingRegion>
. Primer registro del género para la 
<collectionCode id="ED1CFEE9DC03B5594CD61023FAD5697D" box="[1201,1280,1602,1626]" pageId="6" pageNumber="5">EBCH</collectionCode>
