<taxonomicName id="4C12B7A7FFEF350ABEE8FA12002C00E3" box="[1023,1104,1420,1446]" class="Magnoliopsida" family="Phyllanthaceae" genus="Bridelia" kingdom="Plantae" order="Malpighiales" pageId="1" pageNumber="234" phylum="Tracheophyta" rank="genus">
