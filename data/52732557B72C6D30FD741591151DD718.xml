<taxonomicName id="D32FBC93E2783089631671E7CB9EE1A6" class="Magnoliopsida" family="Juglandaceae" genus="Pterocarya" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Pterocarya rhoifolia" order="Fagales" pageId="0" pageNumber="12982" phylum="Tracheophyta" rank="species" species="rhoifolia">Pterocarya rhoifolia</taxonomicName>
