1946 
<taxonomicName id="4C764D6FFFFA4C6CFF5AE92E6E84FCD0" authority="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord, 2022" authorityName="Bate &amp; Horne &amp; Horne &amp; Douglas &amp; Miller &amp; Lord" authorityYear="2022" box="[208,436,868,892]" class="Ostracoda" family="Cyprideidae" genus="Paracypridea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="obovata" status="n. sp.">
