<emphasis id="B967C4746A45F561FF06F8348B49F865" bold="true" box="[151,297,1975,1999]" pageId="113" pageNumber="114">FIGURE 66.</emphasis>
<taxonomicName id="4C1363E56A45F561FEA2F83B887DF865" authority="Błażewicz-Paszkowycz, 2007" authorityName="Błażewicz-Paszkowycz" authorityYear="2007" box="[307,541,1976,1999]" class="Malacostraca" family="Typhlotanaidae" genus="Typhlotanais" kingdom="Animalia" order="Tanaidacea" pageId="113" pageNumber="114" phylum="Arthropoda" rank="species" species="mimosis" status="n. sp.">
<emphasis id="B967C4746A45F561FEA2F83B887DF865" box="[307,541,1976,1999]" italics="true" pageId="113" pageNumber="114">Typhlotanais mimosis</emphasis>
</taxonomicName>
<emphasis id="B967C4746A45F561FDB6F8348807F865" bold="true" box="[551,615,1975,1999]" pageId="113" pageNumber="114">
