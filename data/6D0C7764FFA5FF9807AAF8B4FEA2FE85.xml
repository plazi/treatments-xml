<taxonomicName id="22A5BDF1FFA4FF970345FC13FAB0FBD5" authorityName="Pocock" authorityYear="1901" box="[1144,1309,1015,1041]" class="Arachnida" family="Theraphosidae" genus="Pamphobeteus" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
