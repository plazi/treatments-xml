<taxonomicName id="22A5BDF1FFA4FF97039BF90FFAB2F8C1" authorityName="Reichling &amp; West" authorityYear="1996" box="[1190,1311,1771,1797]" class="Arachnida" family="Theraphosidae" genus="Crassicrus" kingdom="Animalia" order="Araneae" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
