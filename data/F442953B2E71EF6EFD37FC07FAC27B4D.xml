<taxonomicName id="BBEB5FAE2E71EF6EFD4AFC07FC177A20" authorityName="Bleeker" authorityYear="1859" box="[748,930,1007,1033]" class="Elasmobranchii" family="Centrophoridae" kingdom="Animalia" order="Squaliformes" pageId="12" pageNumber="13" phylum="Chordata" rank="family">Centrophoridae</taxonomicName>
