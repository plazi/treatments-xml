<taxonomicName id="C2F64D2D6116897B2F52FBFC5CD34168" box="[828,953,1093,1118]" class="Insecta" family="Tortricidae" genus="Sorolopha" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
