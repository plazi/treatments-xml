<paragraph id="8BDB6503FFE688229A97FF1BFE9AFF43" blockId="2.[151,459,151,211]" box="[151,273,186,211]" pageId="2" pageNumber="368">
(
<figureCitation id="135F7986FFE688229A9FFF1BFF63FF43" box="[159,232,186,211]" captionStart="FIGURES 1, 2" captionStartId="3.[151,264,1975,1998]" captionTargetBox="[187,1371,210,1903]" captionTargetId="figure@3.[181,1405,193,1932]" captionTargetPageId="3" captionText="FIGURES 1, 2. Psallops schmitzi sp. nov., holotype—dorsal view." httpUri="https://zenodo.org/record/231308/files/figure.png" pageId="2" pageNumber="368">
Figs. 
<number id="3DAC7724FFE688229ADEFF1AFF63FF43" box="[222,232,187,211]" pageId="2" pageNumber="368" value="4.5" valueMax="8.0" valueMin="1.0">1</number>
</figureCitation>
