<number id="3DAC7724FFE688229E22FB53FBD8FA9B" box="[1058,1107,1266,1291]" pageId="2" pageNumber="368" value="0.23">0.23</number>
; width— 
<number id="3DAC7724FFE688229EC1FB53FB78FA9B" box="[1217,1267,1266,1291]" pageId="2" pageNumber="368" value="0.5">0.50</number>
; height— 
<number id="3DAC7724FFE688229F66FB53FA13FA9B" box="[1382,1432,1266,1291]" pageId="2" pageNumber="368" value="0.4">0.40</number>
; dorsal width of eye— 
<number id="3DAC7724FFE688229BA4FAB7FE5DFABF" box="[420,470,1302,1327]" pageId="2" pageNumber="368" value="0.16">0.16</number>
; width of vertex— 
<number id="3DAC7724FFE6882298C4FAB6FD7EFABF" box="[708,757,1303,1327]" pageId="2" pageNumber="368" value="0.18">0.18</number>
; antennal segments—missing; rostral segments: I— 
<number id="3DAC7724FFE688229F66FAB6FA13FABF" box="[1382,1432,1303,1327]" pageId="2" pageNumber="368" value="0.31">0.31</number>
; II— 
<number id="3DAC7724FFE688229AC7FA9BFF72FAC3" box="[199,249,1338,1363]" pageId="2" pageNumber="368" value="0.36">0.36</number>
; III—0,31, IV—0,28; length of pronotum— 
<number id="3DAC7724FFE6882298FCFA9AFCA6FAC3" box="[764,813,1339,1363]" pageId="2" pageNumber="368" value="0.29">0.29</number>
; anterior width— 
<number id="3DAC7724FFE6882299F4FA9BFBADFAC3" box="[1012,1062,1338,1363]" pageId="2" pageNumber="368" value="0.42">0.42</number>
; posterior width— 
<number id="3DAC7724FFE688229EFBFA9BFAA6FAC3" box="[1275,1325,1338,1363]" pageId="2" pageNumber="368" value="0.79">0.79</number>
