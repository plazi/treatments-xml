<emphasis id="F9BF6543FFD1FF87FF64F8EFFE8D5A01" bold="true" box="[151,299,1825,1850]" pageId="0" pageNumber="261">
<typeStatus id="147007F3FFD1FF87FF64F8EFFF745A01" box="[151,210,1825,1850]" pageId="0" pageNumber="261">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName id="0CCBC2D2FFD1FF87FECAF8ECFCA85A01" authority="Stebbing, 1905" authorityName="Stebbing" authorityYear="1905" box="[313,782,1825,1850]" class="Malacostraca" family="Dynomenidae" genus="Dynomene" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="261" phylum="Arthropoda" rank="species" species="platyarthrodes">
<emphasis id="F9BF6543FFD1FF87FECAF8ECFDFD5A02" box="[313,603,1825,1849]" italics="true" pageId="0" pageNumber="261">Dynomene platyarthrodes</emphasis>
<bibRefCitation id="AF5AC4A0FFD1FF87FD91F8EFFCA85A01" author="Stebbing" box="[610,782,1825,1850]" pageId="0" pageNumber="261" refString="Stebbing, T. R. R. (1905) South African Crustacea. Part III. Marine Investigations in South Africa, Cape of Good Hope, Department of Agriculture, Cape Town 4, 21 - 123, pls. 17 - 21, 21 bis, 22 - 26." type="journal article" year="1905">
Stebbing, 
<number id="7D03AB76FFD1FF87FD27F8EFFCA85A01" box="[724,782,1825,1850]" pageId="0" pageNumber="261" value="1905.0">1905</number>
</bibRefCitation>
</taxonomicName>
