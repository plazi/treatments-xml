<collectingCountry id="FB9D4CE5FFD0FFF4FF17F822FF46B5A8" box="[151,226,1985,2010]" name="Russia" pageId="3" pageNumber="154">Russia</collectingCountry>
and 
<collectingCountry id="FB9D4CE5FFD0FFF4FE98F821FECFB5A8" box="[280,363,1986,2010]" name="Greece" pageId="3" pageNumber="154">Greece</collectingCountry>
