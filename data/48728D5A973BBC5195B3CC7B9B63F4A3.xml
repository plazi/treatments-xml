<taxonomicName id="420D459D1E3611D304F304F6D402CA06" authority="O.G.Costa, 1844" authorityName="O.G.Costa" authorityYear="1844" class="Malacostraca" family="Palaemonidae" genus="Periclimenes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Periclimenes ornatus" order="Decapoda" pageId="3" pageNumber="28" phylum="Arthropoda" rank="species" species="ornatus">Periclimenes ornatus</taxonomicName>
