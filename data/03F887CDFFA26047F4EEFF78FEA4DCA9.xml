<taxonomicName id="4C514D58FFA26047F056FDD7FAA0DC94" authority="Matsumura, 1933" authorityName="Matsumura" authorityYear="1933" box="[1071,1406,630,655]" genus="Palimpsestis" kingdom="Animalia" pageId="11" pageNumber="12" rank="species" species="akanensis">
