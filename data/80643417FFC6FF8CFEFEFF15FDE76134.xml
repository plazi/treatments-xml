<materialsCitation id="B8A58F5CFFC6FF8DFEC3FE61FC8F61DF" ID-GBIF-Occurrence="3111648309" box="[359,798,402,428]" collectingDate="1995-04-15" collectionCode="PMSL" collectorName="Y. Isobe" country="Japan" location="Aritoshi" municipality="Takami River" pageId="2" pageNumber="96" specimenCount="4" specimenCount-female="3" specimenCount-male="1" stateProvince="Nara" typeStatus="paratype">
