<collectorName id="741C5302EA15FFDEFB6082E1FAF6FA0F" box="[1182,1353,1479,1521]" pageId="5" pageNumber="70">Fullaway.</collectorName>
<materialsCitation id="69813C89EA15FFDEFA9882E1FBC0F9A6" ID-GBIF-Occurrence="3331287311" collectedfrom="Hernandia" collectingDate="1936-05-31" collectorName="R. L. Usinger" country="Guam" location="Agat" pageId="5" pageNumber="70" specimenCount="1">
