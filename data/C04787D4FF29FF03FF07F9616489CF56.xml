150. 
<taxonomicName id="8FEE4D41FF29FF02FF67F9616762CDA6" authority="Neumann, 1906" authorityName="in Matthysse and Colbo" authorityYear="1987" box="[247,545,1734,1761]" class="Arachnida" family="Ixodidae" genus="Haemaphysalis" kingdom="Animalia" order="Ixodida" pageId="192" pageNumber="193" phylum="Arthropoda" rank="species" species="spinulosa">
<emphasis id="7A9AEAD0FF29FF02FF67F9616762CDA6" bold="true" box="[247,545,1734,1761]" italics="true" pageId="192" pageNumber="193">Haemaphysalis spinulosa</emphasis>
</taxonomicName>
</emphasis>
<bibRefCitation id="2C7F4B33FF29FF02FDBCF96067A6CDA7" author="Neumann, L. G." box="[556,741,1734,1761]" pageId="192" pageNumber="193" pagination="195 - 219" refId="ref233460" refString="Neumann, L. G. (1906) Notes sur les Ixodides. IV. Archives de Parasitologie, 10, 195 - 219." type="journal article" year="1906">Neumann, 1906</bibRefCitation>
