<geoCoordinate id="D2FE5078FF7FE2DB84F8FD94DF1D6749" box="[439,531,583,608]" direction="south" orientation="latitude" pageId="146" pageNumber="147" precision="555" value="-26.75">26.75°S</geoCoordinate>
