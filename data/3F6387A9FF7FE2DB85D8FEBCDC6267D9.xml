, at 
<geoCoordinate id="D2FE5078FF7FE2DB800CFDF7D8926714" box="[1347,1436,548,573]" direction="south" orientation="latitude" pageId="146" pageNumber="147" precision="555" value="-11.25">11.25°S</geoCoordinate>
<geoCoordinate id="D2FE5078FF7FE2DB85D8FD9BDDF26749" box="[151,252,584,608]" direction="west" orientation="longitude" pageId="146" pageNumber="147" precision="555" value="-37.1">37.10°W</geoCoordinate>
