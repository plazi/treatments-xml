<taxonomicName id="03BB4D4B81718D3A5D803C57FED7F8A5" authorityName="Crispolon &amp; Yap" authorityYear="2021" box="[189,360,1836,1862]" class="Insecta" family="Cercopidae" genus="Mioscarta" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="113" phylum="Arthropoda" rank="species" species="translucida">
