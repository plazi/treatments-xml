<taxonomicName id="F510F3A325750440EB1F1D8E2606A72D" class="Diplopoda" family="Fuhrmannodesmidae" genus="Hemisphaeroparia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Hemisphaeroparia falcata" order="Polydesmida" pageId="0" pageNumber="31" phylum="Arthropoda" rank="species" species="falcata">
