<specimenCode id="38426D09FF9D9940FC38F9108A0BF958" box="[944,1087,1688,1709]" collectionCode="IVPP" country="China" httpUri="http://grbio.org/cool/wd8c-kdma" name="Institute of Vertebrate Paleontology and Paleoanthropology" pageId="1" pageNumber="574">IVPP 84019</specimenCode>
