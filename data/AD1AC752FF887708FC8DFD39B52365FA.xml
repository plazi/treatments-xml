<taxonomicName id="E2B30DC7FF88770EFC26FD26B2026710" authority="PATTERSON, 1975" authorityName="Patterson" authorityYear="1975" box="[962,1394,751,773]" class="Mammalia" family="Orycteropodidae" genus="Leptorycteropus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Tubulidentata" pageId="27" pageNumber="676" phylum="Chordata" rank="genus">
<emphasis id="17C7AA56FF88770EFC26FD26B3EC6716" box="[962,1180,752,773]" italics="true" pageId="27" pageNumber="676">LEPTORYCTEROPUS</emphasis>
<bibRefCitation id="41220BB5FF88770EFB42FD26B2026710" author="Patterson B" box="[1190,1394,751,773]" pageId="27" pageNumber="676" pagination="185 - 237" refId="ref44710" refString="Patterson B. 1975. The fossil Aardvarks (Mammalia: Tubulidentata). Bulletin of the Museum of Comparative Zoology, Cambridge 147: 185 - 237." type="journal article" year="1975">PATTERSON, 1975</bibRefCitation>
