locality in 
<collectingRegion id="E20058913D74FF93FD63FE12FCB1FE63" box="[745,858,475,501]" country="Ecuador" name="Pichincha" pageId="7" pageNumber="8">Pichincha</collectingRegion>
, 
<collectingCountry id="58D3D6E33D74FF93FCE0FE12FC2CFE63" box="[874,967,475,501]" name="Ecuador" pageId="7" pageNumber="8">Ecuador</collectingCountry>
and 
<collectingRegion id="E20058913D74FF93FB8FFE12FB34FE63" box="[1029,1247,475,501]" country="Colombia" name="Narino" pageId="7" pageNumber="8">Nariño department</collectingRegion>
, 
<collectingCountry id="58D3D6E33D74FF93FB67FE12FAB0FE63" box="[1261,1371,475,501]" name="Colombia" pageId="7" pageNumber="8">Colombia</collectingCountry>
