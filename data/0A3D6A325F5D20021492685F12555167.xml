<taxonomicName id="949305F68F75171A6BF6047DEB1D9801" class="Insecta" family="Chrysopidae" genus="Kymachrysa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Kymachrysa intacta" order="Neuroptera" pageId="15" pageNumber="102" phylum="Arthropoda" rank="species" species="intacta">Kymachrysa intacta</taxonomicName>
under the name placita/placitus [as 
<taxonomicName id="A8FF8E6FFA10D77F858F18097D38D90D" class="Insecta" family="Chrysopidae" genus="Chrysopa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysopa" order="Neuroptera" pageId="15" pageNumber="102" phylum="Arthropoda" rank="genus">Chrysopa</taxonomicName>
, 
<taxonomicName id="E6EA085290F6A244C501C1E49A9A77D7" class="Insecta" family="Chrysopidae" genus="Ceraeochrysa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ceraeochrysa" order="Neuroptera" pageId="15" pageNumber="102" phylum="Arthropoda" rank="genus">Ceraeochrysa</taxonomicName>
, 
<taxonomicName id="596BCEA6CE5C3D03E66E80C498A9D360" class="Insecta" family="Chrysopidae" genus="Chrysopodes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysopodes (Neosuarius)" order="Neuroptera" pageId="15" pageNumber="102" phylum="Arthropoda" rank="subGenus" subGenus="Neosuarius">Chrysopodes (Neosuarius)</taxonomicName>
, 
<taxonomicName id="E5868C96E9035E661450383737D2BB06" genus="Oviedus" lsidName="Oviedus" pageId="15" pageNumber="102" rank="genus">Oviedus</taxonomicName>
