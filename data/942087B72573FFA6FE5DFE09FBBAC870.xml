<taxonomicName id="DB894D222573FFA6FCA4FB18FBBAC990" authority="Fabricius, 1775" authorityName="Fabricius" authorityYear="1775" box="[877,1126,1192,1217]" class="Insecta" family="Tenebrionidae" genus="Helops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="301" phylum="Arthropoda" rank="genus">
