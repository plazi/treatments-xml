<taxonomicName id="DB894D222573FFA6FC9CFBB8FBBAC970" authority="Seidlitz, 1896" authorityName="Seidlitz" authorityYear="1896" box="[853,1126,1032,1057]" class="Insecta" family="Tenebrionidae" genus="Probaticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="301" phylum="Arthropoda" rank="genus">
