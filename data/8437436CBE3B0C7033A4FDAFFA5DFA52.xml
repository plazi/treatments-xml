<emphasis id="3EEA2E68BE3B0C4F32D0F953FBB9F933" box="[144,222,1667,1691]" italics="true" pageId="29" pageNumber="2338">Length</emphasis>
<quantity id="CB665F9FBE3B0C4F32A6F953FA3CF933" box="[230,347,1667,1691]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="2.0" metricValueMin="1.5" pageId="29" pageNumber="2338" unit="mm" value="1.75" valueMax="2.0" valueMin="1.5">1.5–2 mm</quantity>
. 
