<taxonomicName id="8B0C56E1BBDBA93CC6EA6A4314DC1A57" authority="Latzel, 1880" authorityName="Latzel" authorityYear="1880" class="Chilopoda" family="Lithobiidae" genus="Polybothrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lithobiomorpha" pageId="0" pageNumber="144" phylum="Arthropoda" rank="subSpecies" species="leostygis" subSpecies="patens">
<pageBreakToken id="83B0F410256A6DBF19C13D497B0E9813" pageId="0" pageNumber="144" start="start">Polybothrus</pageBreakToken>
leostygis patens
</taxonomicName>
<taxonomicNameLabel id="EA681CE2B7CEA5AF1F53A3616E4E59C8" pageId="0" pageNumber="144" rank="subSpecies">nov. subsp.</taxonomicNameLabel>
