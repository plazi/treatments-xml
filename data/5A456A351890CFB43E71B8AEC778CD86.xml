<taxonomicName id="DBE1A6BE12057F484F3D3258C60F80D2" class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus" order="Malpighiales" pageId="6" pageNumber="13" phylum="Tracheophyta" rank="genus">Populus</taxonomicName>
