<number id="163D24E2FFB1FFFEEAC0F8ECFE543B52" box="[403,416,1797,1821]" pageId="4" pageNumber="35" value="5.0">5</number>
IS= 
<number id="163D24E2FFB1FFFEEA84F8ECFE123B52" box="[471,486,1797,1821]" pageId="4" pageNumber="35" value="1.0">1</number>
; T 
<number id="163D24E2FFB1FFFEE956F8ECFDE63B52" box="[517,530,1797,1821]" pageId="4" pageNumber="35" value="6.0">6</number>
