<taxonomicName id="A9CF4D443064FFC134582D2A9870FCED" box="[346,605,778,803]" class="Arachnida" family="Eriophyidae" genus="Rhombacus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="74" pageNumber="75" phylum="Arthropoda" rank="species" species="eucalypti">
