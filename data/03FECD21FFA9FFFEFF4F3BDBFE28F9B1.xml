<taxonomicName id="4C5707B4FFABFFF1FEC13AE9FD8AF866" box="[281,548,1997,2018]" class="Diplopoda" family="Paradoxosomatidae" genus="Enghoffosoma" kingdom="Animalia" order="Polydesmida" pageId="7" pageNumber="158" phylum="Arthropoda" rank="species" species="triangulare" status="sp. nov.">
<emphasis id="B923A025FFABFFF1FEC13AE9FD8AF866" box="[281,548,1997,2018]" italics="true" pageId="7" pageNumber="158">
Enghoffosoma 
<taxonomicName id="4C5707B4FFABFFF1FE693AE9FD8AF866" box="[433,548,1997,2018]" pageId="7" pageNumber="158" rank="species" species="triangulare" status="sp. nov.">triangulare</taxonomicName>
