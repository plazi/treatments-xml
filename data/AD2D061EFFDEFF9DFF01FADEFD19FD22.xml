<taxonomicName id="E284CC8BFFDEFF9EFF01FA39FDA0FA72" authority="ASHMEAD, 1906" authorityName="ASHMEAD" authorityYear="1906" box="[178,551,1430,1452]" class="Insecta" family="Ichneumonidae" genus="Hemiteles" kingdom="Animalia" order="Hymenoptera" pageId="37" pageNumber="1714" phylum="Arthropoda" rank="species" species="sapporoensis">
<emphasis id="17F06B1AFFDEFF9EFF01FA39FE0DFA72" box="[178,394,1430,1452]" italics="true" pageId="37" pageNumber="1714">Hemiteles sapporoensis</emphasis>
<bibRefCitation id="4115CAF9FFDEFF9EFE23FA38FDA0FA72" author="ASHMEAD W" box="[400,551,1430,1452]" pageId="37" pageNumber="1714" pagination="169 - 201" refId="ref40839" refString="ASHMEAD W. H. (1906): Descriptions of new Hymenoptera from Japan. - Proc. U. S. Nat. Mus. 30: 169 - 201." type="journal article" year="1906">ASHMEAD, 1906</bibRefCitation>
</taxonomicName>
– 
<typeStatus id="FA3F09AAFFDEFF9EFD8DFA39FD1BFA72" box="[574,668,1430,1452]" pageId="37" pageNumber="1714">Holotypus</typeStatus>
(
<specimenCount id="33827C81FFDEFF9EFD19FA33FD31FA6F" box="[682,694,1436,1457]" pageId="37" pageNumber="1714" type="female">♀</specimenCount>
) untersucht (
<collectingRegion id="E74079EAFFDEFF9EFC87FA39FC20FA72" box="[820,935,1430,1452]" country="United States of America" name="Washington" pageId="37" pageNumber="1714">Washington</collectingRegion>
