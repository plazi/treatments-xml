<taxonomicName id="6AD55A3BC82A7BD5CC44F4C0C70129C0" class="Insecta" family="Blaberidae" genus="Pseudophoraspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pseudophoraspis tramlapensis" order="Blattodea" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="tramlapensis">Pseudophoraspis tramlapensis</taxonomicName>
