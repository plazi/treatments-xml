<paragraph id="F7BC98424C042353C5B25981C5DDA931" pageId="0" pageNumber="63864">
<taxonomicName id="86679A5A1F7381AC612AD380729C2110" authority="Dissanayake &amp; Wijayawardene &amp; Dayarathne &amp; Samarakoon &amp; Dai &amp; Hyde &amp; Kang, 2021" authorityName="Dissanayake &amp; Wijayawardene &amp; Dayarathne &amp; Samarakoon &amp; Dai &amp; Hyde &amp; Kang" authorityYear="2021" genus="Paraeutypella" lsidName="Paraeutypella" pageId="0" pageNumber="63864" rank="genus" status="gen. nov.">Paraeutypella</taxonomicName>
<taxonomicName id="D217C0029711C78BC88EDE3D1237AAD8" authority="Dissanayake &amp; Wijayawardene &amp; Dayarathne &amp; Samarakoon &amp; Dai &amp; Hyde &amp; Kang, 2021" authorityName="Dissanayake &amp; Wijayawardene &amp; Dayarathne &amp; Samarakoon &amp; Dai &amp; Hyde &amp; Kang" authorityYear="2021" genus="Paraeutypella" lsidName="Paraeutypella guizhouensis" pageId="0" pageNumber="63864" rank="species" species="guizhouensis" status="gen. nov.">Paraeutypella guizhouensis</taxonomicName>
