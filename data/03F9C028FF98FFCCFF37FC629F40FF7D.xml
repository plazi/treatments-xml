<number id="3D986319FF98FFCEFE70FB879CABFBCB" box="[464,493,1106,1132]" pageId="12" pageNumber="51" value="70.0">70</number>
(
<number id="3D986319FF98FFCEFE5FFB879F0BFBCB" box="[511,589,1106,1132]" pageId="12" pageNumber="51" value="72.5" valueMax="75.0" valueMin="70.0">70–75</number>
