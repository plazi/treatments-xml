<taxonomicName id="4BBF789840B2E60B54DA430FDD0CC2B3" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28905" class="Insecta" family="Formicidae" genus="Echinopla" kingdom="Animalia" lsidName-HNS="Echinopla pallipes Smith" order="Hymenoptera" pageNumber="80" phylum="Arthropoda" rank="species" species="pallipes">Echinopla pallipes</taxonomicName>
