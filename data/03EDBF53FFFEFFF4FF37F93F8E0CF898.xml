<caption id="DF3B5ECDFFFFFFFBFF37F81E8FCCF83F" ID-DOI="http://doi.org/10.5281/zenodo.5134047" ID-Zenodo-Dep="5134047" httpUri="https://zenodo.org/record/5134047/files/figure.png" pageId="7" pageNumber="8" startId="7.[151,255,1923,1947]" targetBox="[173,1436,223,1897]" targetPageId="7">
<paragraph id="8BFB0E45FFFFFFFBFF37F81E8FCCF83F" blockId="7.[151,1436,1923,1981]" pageId="7" pageNumber="8">
<emphasis id="B930D257FFFFFFFBFF37F81E8D4AF819" bold="true" box="[151,280,1923,1947]" pageId="7" pageNumber="8">FIGURE 1.</emphasis>
<taxonomicName id="4C4475C6FFFFFFFBFEBFF8198EA4F819" authority="Schultz" authorityName="Schultz" box="[287,758,1924,1947]" class="Arachnida" family="Phytoseiidae" genus="Typhlodromus" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="acaciae" subGenus="Anthoseius">
