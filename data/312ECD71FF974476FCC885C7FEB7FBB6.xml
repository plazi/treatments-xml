<taxonomicName id="7E8707E4FF954471FD858385FE91FB7B" authorityName="Heiss" authorityYear="2010" box="[199,319,1260,1278]" class="Insecta" family="Aradidae" genus="Tiomanaptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="36" phylum="Arthropoda" rank="genus">
<emphasis id="8BF3A075FF954471FD858385FE91FB7B" box="[199,319,1260,1278]" italics="true" pageId="3" pageNumber="36">Tiomanaptera</emphasis>
</taxonomicName>
species. 5, 6 – 
<taxonomicName id="7E8707E4FF954471FC818385FDFAFB7B" authority="Heiss &amp; Baňař, 2013" authorityName="Heiss &amp; Baňař" authorityYear="2013" box="[451,596,1260,1278]" class="Insecta" family="Aradidae" genus="Tiomanaptera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="schwendingeri" status="sp. nov.">
<emphasis id="8BF3A075FF954471FC818385FDFAFB7B" box="[451,596,1260,1278]" italics="true" pageId="3" pageNumber="36">T. schwendingeri</emphasis>
</taxonomicName>
