</taxonomicName>
(
<taxonomicName id="4C044D27FFB95D0236F1FD68FC06FD34" box="[943,1011,649,673]" class="Malacostraca" family="Hippolytidae" genus="Lysmata" kingdom="Animalia" order="Decapoda" pageId="12" pageNumber="28" phylum="Arthropoda" rank="species" species="bahia">Bahia</taxonomicName>
