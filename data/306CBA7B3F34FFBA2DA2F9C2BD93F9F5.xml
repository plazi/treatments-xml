<emphasis id="8AB1D77F3F34FFBD2DFCF88FBDC7F899" bold="true" box="[201,428,1792,1817]" pageId="3" pageNumber="56">Material examined</emphasis>
. 
<typeStatus id="677EB5CF3F34FFBD2C8EF88FBE43F898" box="[443,552,1792,1816]" pageId="3" pageNumber="56">Holotype</typeStatus>
: ɗ, 
<emphasis id="8AB1D77F3F34FFBD2F60F88FBE87F899" bold="true" box="[597,748,1792,1817]" pageId="3" pageNumber="56">
<collectingCountry id="C0D24BFD3F34FFBD2F60F88FBE87F899" box="[597,748,1792,1817]" name="Thailand" pageId="3" pageNumber="56">THAILAND</collectingCountry>
</emphasis>
: Chiang Mai, 
<geoCoordinate id="DDF16DAA3F34FFBD2EA4F88FBF85F898" box="[913,1006,1792,1816]" direction="north" orientation="latitude" pageId="3" pageNumber="56" precision="555" value="18.7">
<number id="0E0D194A3F34FFBD2EA4F88FBFA4F898" box="[913,975,1792,1816]" pageId="3" pageNumber="56" value="18.7">18.70</number>
ºN
</geoCoordinate>
<geoCoordinate id="DDF16DAA3F34FFBD2EC3F88FB83AF898" box="[1014,1105,1792,1816]" direction="east" orientation="longitude" pageId="3" pageNumber="56" precision="555" value="98.8">
<number id="0E0D194A3F34FFBD2EC3F88FB85CF898" box="[1014,1079,1792,1816]" pageId="3" pageNumber="56" value="98.8">98.80</number>
ºE
</geoCoordinate>
, 
<date id="CC7B2DAD3F34FFBD2968F88FB897F898" box="[1117,1276,1792,1816]" pageId="3" pageNumber="56" value="1997-12-01" valueMax="1997-12-31" valueMin="1997-12-01">
