<taxonomicName id="4C714D14FF91FFDBFF714DC7FDDFFC75" authority="Zhu &amp; Gong 1991: 23" authorityName="Zhu &amp; Gong" authorityPageNumber="23" authorityYear="1991" box="[243,593,921,944]" class="Arachnida" family="Pholcidae" genus="Pholcus" kingdom="Animalia" order="Araneae" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="wuyiensis">
