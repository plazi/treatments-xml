<paragraph id="2F862AF53D2774D266F302DD25651C33" pageId="2" pageNumber="177">
<taxonomicName id="465E348E78BDA3512EEE8F2D59A97E4C" class="Insecta" family="Dictyopharidae" genus="Dictyophara" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Dictyophara reticulata" order="Hemiptera" pageId="2" pageNumber="177" phylum="Arthropoda" rank="species" species="reticulata">Dictyophara reticulata</taxonomicName>
