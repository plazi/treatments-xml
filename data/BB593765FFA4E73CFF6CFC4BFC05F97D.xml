<taxonomicName id="F4F0FDF0FFA4E73CFF6CFC4BFDD4FC0A" authority="Savigny, 1816" authorityName="Savigny" authorityYear="1816" box="[151,544,943,970]" class="Ascidiacea" family="Ascidiidae" genus="Phallusia" kingdom="Animalia" order="Enterogona" pageId="13" pageNumber="14" phylum="Chordata" rank="species" species="arabica">
