<taxonomicName id="D8C90BADFFA8F2414B79FD1092FAFD69" box="[1371,1474,695,719]" class="Magnoliopsida" family="Poaceae" genus="Candollea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Poales" pageId="8" pageNumber="83" phylum="Tracheophyta" rank="genus">
