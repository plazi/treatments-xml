<taxonomicName id="4C724D611F6B2C6212A8F809F535F864" baseAuthorityName="Claus" baseAuthorityYear="1890" box="[722,844,2014,2037]" class="Ostracoda" family="Halocyprididae" genus="Porroecia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Halocyprida" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="porrecta">
<emphasis id="B906EAF01F6B2C6212A8F809F535F864" box="[722,844,2014,2037]" italics="true" pageId="79" pageNumber="80">P. porrecta</emphasis>
</taxonomicName>
.
</paragraph>
