<specimenCode id="DBA62B4EFF8DFFC9FAD29F60FA5BFC9C" box="[1395,1429,806,833]" collectionCode="C" country="Denmark" name="University of Copenhagen" pageId="3" pageNumber="247" type="Herbarium">C2</specimenCode>
