<taxonomicName id="4C7A4D1FB602FEB7660D7CCEDF8CFD8E" box="[857,1020,507,533]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Monoicomyces" kingdom="Fungi" order="Laboulbeniales" pageId="339" pageNumber="340" phylum="Ascomycota" rank="species" species="homalotae">
