<quantity id="0E19CEE104E3FF60FF33FD0576E112D4" box="[151,243,712,739]" metricMagnitude="0" metricUnit="m" metricValue="1.25" pageId="179" pageNumber="180" unit="m" value="1.25">1,250 m</quantity>
