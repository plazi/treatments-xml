<taxonomicName id="0EE1188704E3FF60FE50FD0574FC12D5" baseAuthorityName="Lesueur" baseAuthorityYear="1824" box="[500,750,712,738]" class="Holothuroidea" family="Synaptidae" genus="Synaptula" kingdom="Animalia" order="Apodida" pageId="179" pageNumber="180" phylum="Echinodermata" rank="species" species="hydriformis">
