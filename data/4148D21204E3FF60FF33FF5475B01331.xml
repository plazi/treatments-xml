<geoCoordinate id="ACD505C304E3FF60FC77FD69724D1288" box="[979,1119,676,703]" degrees="38" direction="north" minutes="35" orientation="latitude" pageId="179" pageNumber="180" precision="15" seconds="00" value="38.583332">38°35’00”N</geoCoordinate>
, 
<geoCoordinate id="ACD505C304E3FF60FBCFFD6972EF1288" box="[1131,1277,676,703]" degrees="28" direction="west" minutes="05" orientation="longitude" pageId="179" pageNumber="180" precision="15" seconds="45" value="-28.095835">28°05’45”W</geoCoordinate>
