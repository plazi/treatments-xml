<paragraph id="FF0E3698FF9B3B231FFBFA8AFB94FAA6" blockId="24.[151,1437,1346,2012]" box="[151,1083,1346,1372]" pageId="24" pageNumber="25">
<typeStatus id="200A883AFF9B3B231FFBFA8AFF7EFAA6" box="[151,209,1346,1372]" pageId="24" pageNumber="25">Type</typeStatus>
species: 
<taxonomicName id="38B14D1BFF9B3B231E50FA8BFD0FFAA6" box="[316,672,1347,1372]" class="Insecta" family="Leptoceridae" genus="Fernandoschmidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="aramaniya" status="new species">
<emphasis id="CDC5EA8AFF9B3B231E50FA8BFD0FFAA6" box="[316,672,1347,1372]" italics="true" pageId="24" pageNumber="25">Fernandoschmidia aramaniya</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="D6F657F1FF9B3B231DC3FA8AFC91FAA6" box="[687,830,1346,1372]" pageId="24" pageNumber="25" rank="species">new species</taxonomicNameLabel>
