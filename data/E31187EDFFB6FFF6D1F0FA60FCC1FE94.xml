<materialsCitation id="DBD03CA6FFB6FFF3D316F941FA18F89A" ID-GBIF-Occurrence="2521262715" box="[625,1432,1782,1809]" collectingDate="2015-08-24" collectorName="K. Castillo" pageId="1" pageNumber="447" specimenCount="1">
ibid., 
<collectorName id="C64D532DFFB6FFF3D3D6F940FCADF89B" box="[689,813,1782,1809]" pageId="1" pageNumber="447">K. Castillo</collectorName>
, MUPADI (nymphs: m # 1, f # 1, adults: m # 2, f # 16)
