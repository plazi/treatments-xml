<quantity id="4C824FE87967FFFAD424FC2E3EABFC66" box="[311,384,985,1008]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="8" pageNumber="9" unit="mm" value="4.0">4 mm</quantity>
