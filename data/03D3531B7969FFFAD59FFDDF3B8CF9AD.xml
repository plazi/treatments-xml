<materialsCitation id="3B12E8507967FFFAD58AFCDE3D18FB4D" collectionCode="TYPE, MATERIAL, AMNH" collectorName="White Oaks &amp; Bob Mesak &amp; The &amp; Portions &amp; Since &amp; Still" county="Middlesex Co." location="Sayreville" pageId="8" pageNumber="9" specimenCode="NJ- 1023" specimenCount="1" stateProvince="New Jersey" typeStatus="holotype">
<collectionCode id="ED6B7AC87967FFFAD58AFCDE3E72FCA9" box="[153,345,809,832]" pageId="8" pageNumber="9">TYPE MATERIAL</collectionCode>
: 
<emphasis id="B90E3E1F7967FFFAD462FCDE3EC2FCD7" bold="true" box="[369,489,809,833]" pageId="8" pageNumber="9">
<typeStatus id="54C15CAF7967FFFAD462FCDE3ECFFCD7" box="[369,484,809,833]" pageId="8" pageNumber="9">Holotype</typeStatus>
.
</emphasis>
<collectionCode id="ED6B7AC87967FFFAD4E9FCDE3D78FCD6" box="[506,595,809,832]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="8" pageNumber="9">AMNH</collectionCode>
<specimenCode id="DBDC4A767967FFFAD770FCDE3F92FCCB" collectionCode="NJ-" pageId="8" pageNumber="9">NJ­ 1023</specimenCode>
