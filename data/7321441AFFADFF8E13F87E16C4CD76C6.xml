<taxonomicName id="3C888E8FFFACFF8D107B7D93C68175BF" authorityName="Bolton &amp; Fisher" authorityYear="2012" box="[788,983,1496,1518]" class="Insecta" family="Formicidae" genus="Tanipone" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="species" species="maculata">
