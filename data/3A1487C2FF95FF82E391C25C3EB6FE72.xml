<emphasis id="80C9EAC6FF95FF82E08AC2CD3E46FE55" box="[908,1124,441,466]" italics="true" pageId="3">
<taxonomicName id="75BD4D57FF95FF82E08AC2CD3E42FE55" box="[908,1120,441,466]" class="Insecta" family="Curculionidae" genus="Pteleobius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" phylum="Arthropoda" rank="species" species="vittatus">Pteleobius vittatus</taxonomicName>
