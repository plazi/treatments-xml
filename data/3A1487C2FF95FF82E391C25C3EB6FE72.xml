<emphasis id="80C9EAC6FF95FF82E27FC2A938BCFE72" box="[377,670,477,502]" italics="true" pageId="3">
<taxonomicName id="75BD4D57FF95FF82E27FC2A93812FE72" authorityName="Moench" authorityYear="1785" box="[377,560,477,502]" class="Magnoliopsida" family="Ulmaceae" genus="Ulmus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="3" phylum="Tracheophyta" rank="species" species="suberosa">Ulmus suberosa</taxonomicName>
