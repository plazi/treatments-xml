<taxonomicName id="75BD4D57FF95FF82E13DC2AA38BCFE72" authorityName="Pall" authorityYear="1784" box="[571,670,477,502]" class="Magnoliopsida" family="Ulmaceae" genus="Ulmus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="3" phylum="Tracheophyta" rank="species" species="laevis">U. laevis</taxonomicName>
</emphasis>
