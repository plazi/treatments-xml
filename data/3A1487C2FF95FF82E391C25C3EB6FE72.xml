<taxonomicName id="75BD4D57FF95FF82E1D3C2AA3973FE72" authorityName="Gilib" authorityYear="1792" box="[725,849,477,502]" class="Magnoliopsida" family="Ulmaceae" genus="Ulmus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="3" phylum="Tracheophyta" rank="species" species="foliacea">
<emphasis id="80C9EAC6FF95FF82E1D3C2AA3973FE72" box="[725,849,477,502]" italics="true" pageId="3">U. foliacea</emphasis>
