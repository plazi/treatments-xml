<paragraph id="B20236D4FF95FF82E3C1C2CE3EB6FE72" blockId="3.[151,1436,366,502]" pageId="3">
<taxonomicName id="75BD4D57FF95FF82E3C1C2CE3B0DFE55" box="[199,303,442,465]" class="Magnoliopsida" family="Betulaceae" genus="Carpinus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="3" phylum="Tracheophyta" rank="genus">
<emphasis id="80C9EAC6FF95FF82E3C1C2CE3B0DFE55" box="[199,303,442,465]" italics="true" pageId="3">Carpinus</emphasis>
