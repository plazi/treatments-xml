<taxonomicName id="209C4D79321F1B79A8005E29FC18E367" box="[776,927,1875,1898]" class="Reptilia" family="Teiidae" genus="Dracaena" kingdom="Animalia" order="Squamata" pageId="49" pageNumber="50" phylum="Chordata" rank="species" species="undetermined">
<emphasis id="D5E8EAE8321F1B79A8005E29FCFFE367" box="[776,888,1875,1898]" italics="true" pageId="49" pageNumber="50">Dracaena</emphasis>
sp.
</taxonomicName>
(
<taxonomicName id="209C4D79321F1B79A8A65E28FBCAE366" authorityName="de Jussieu" authorityYear="1789" box="[942,1101,1874,1899]" class="Liliopsida" family="Asparagaceae" kingdom="Plantae" order="Asparagales" pageId="49" pageNumber="50" phylum="Tracheophyta" rank="family">Asparagaceae</taxonomicName>
) (SCC in 
