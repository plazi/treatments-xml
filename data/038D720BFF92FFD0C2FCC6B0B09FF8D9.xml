</emphasis>
<taxonomicName id="4C24B89EFF92FFD0C341C136B1B1FBE5" authority="Grazia, Fernandes &amp; Schwertner, 1998" authorityName="Grazia, Fernandes &amp; Schwertner" authorityYear="1998" box="[298,968,1036,1061]" class="Insecta" family="Pentatomidae" genus="Luridocimex" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="wygodzinskyi">
