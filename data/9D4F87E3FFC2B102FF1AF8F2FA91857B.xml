<taxonomicName id="D2E64D76FFC3B102FED9FB43FE4C87B2" authority="Latreille, 1806" authorityName="Latreille" authorityYear="1806" box="[340,456,1171,1195]" class="Insecta" family="Cantharidae" genus="Malthinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="54" phylum="Arthropoda" rank="genus">Malthinus</taxonomicName>
