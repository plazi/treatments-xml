<subSubSection id="C3776556ED753811FF78FA74FD06FA34" pageId="548" pageNumber="549" type="etymology">
<paragraph id="8BD236DDED753811FF78FA74FD06FA34" blockId="548.[136,779,1295,2071]" pageId="548" pageNumber="549">
<emphasis id="B919EACFED753811FF78FA74FEFBFA15" bold="true" box="[189,318,1466,1492]" pageId="548" pageNumber="549">Etymology</emphasis>
