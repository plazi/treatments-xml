<taxonomicName id="F2F44D37567DFFC8D097FA87FDCB6002" authorityName="Osgood" authorityYear="1916" box="[428,636,1381,1405]" class="Mammalia" family="Phyllostomidae" genus="Artibeus" kingdom="Animalia" order="Chiroptera" pageId="97" pageNumber="97" phylum="Chordata" rank="species" species="anderseni">
