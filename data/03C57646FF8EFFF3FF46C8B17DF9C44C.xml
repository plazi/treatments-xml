<taxonomicName id="4C6CBCD3FF8DFFF3FB39CC51787CC76C" box="[1256,1365,260,285]" class="Aves" family="Parulidae" kingdom="Animalia" order="Passeriformes" pageId="6" pageNumber="407" phylum="Chordata" rank="family">Parulidae</taxonomicName>
