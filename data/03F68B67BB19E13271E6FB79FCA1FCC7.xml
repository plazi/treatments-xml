<emphasis id="B92BE663BB19E13171E6FAECFE77FABD" box="[151,421,1369,1391]" italics="true" pageId="13" pageNumber="458">
<taxonomicName id="4C5F41F2BB19E13171E6FAECFE73FABD" box="[151,417,1369,1391]" class="Insecta" family="Agromyzidae" genus="Japanagromyza" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="458" phylum="Arthropoda" rank="species" species="salicifolii">Japanagromyza salicifolii</taxonomicName>
