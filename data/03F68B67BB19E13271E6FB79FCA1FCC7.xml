<taxonomicName id="4C5F41F2BB1AE13270EEFEB2FE29FECF" box="[415,507,260,285]" class="Magnoliopsida" family="Salicaceae" genus="Populus" kingdom="Plantae" order="Malpighiales" pageId="14" pageNumber="459" phylum="Tracheophyta" rank="genus">
<emphasis id="B92BE663BB1AE13270EEFEB2FE29FECF" box="[415,507,260,285]" italics="true" pageId="14" pageNumber="459">Populus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C5F41F2BB1AE1327378FEB2FD2EFECE" authority="Spencer, 1974" authorityName="Spencer" authorityYear="1974" box="[521,764,260,285]" class="Ascidiacea" family="Polycitoridae" genus="Salix" kingdom="Animalia" order="Enterogona" pageId="14" pageNumber="459" phylum="Chordata" rank="genus">
<emphasis id="B92BE663BB1AE1327378FEB2FD93FECF" box="[521,577,260,285]" italics="true" pageId="14" pageNumber="459">Salix</emphasis>
(Spencer, 
<number id="3D972856BB1AE13273C9FEB2FD26FECE" box="[696,756,260,284]" pageId="14" pageNumber="459" value="1974.0">1974</number>
)
</taxonomicName>
