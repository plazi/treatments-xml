<taxonomicName id="FCA14D30CB4DAA6EFF11FE6A91B3A0CE" box="[151,234,404,428]" class="Arachnida" family="Sparassidae" genus="Pseudopoda" kingdom="Animalia" order="Araneae" pageId="9" pageNumber="282" phylum="Arthropoda" rank="species" species="lutea">
