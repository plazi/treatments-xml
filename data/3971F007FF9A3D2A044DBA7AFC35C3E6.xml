<taxonomicName id="76D83A92FF9A3D250719B93DFD61C63A" authorityName="Marseul" authorityYear="1855" box="[604,724,1633,1658]" class="Insecta" family="Histeridae" genus="Saprinus" kingdom="Animalia" order="Coleoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="buqueti">
