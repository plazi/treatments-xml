<taxonomicName id="4C52A683744DFFFF5F34FABAE2B9FADC" authorityName="Ozdikmen" authorityYear="2008" box="[1336,1436,1366,1388]" class="Arachnida" family="Oribatellidae" genus="Fberninia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="12" pageNumber="270" phylum="Arthropoda" rank="genus">
<emphasis id="B9260112744DFFFF5F34FABAE2B9FADC" bold="true" box="[1336,1436,1366,1388]" italics="true" pageId="12" pageNumber="270">Fberninia</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
