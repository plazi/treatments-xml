<taxonomicName id="4C52A683744DFFFF5F0AFB2FE2BEFB69" authorityName="Grandjean" authorityYear="1953" box="[1286,1435,1219,1241]" class="Arachnida" family="Oribatellidae" genus="Ophidiotrichus" kingdom="Animalia" order="Sarcoptiformes" pageId="12" pageNumber="270" phylum="Arthropoda" rank="genus">
