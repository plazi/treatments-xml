<taxonomicName id="4C52A683744DFFFF5F56FAF7E2B3FA81" authorityName="Oudemans" authorityYear="1906" box="[1370,1430,1307,1329]" class="Arachnida" family="Oribatellidae" genus="Joelia" kingdom="Animalia" order="Sarcoptiformes" pageId="12" pageNumber="270" phylum="Arthropoda" rank="genus">
<emphasis id="B9260112744DFFFF5F56FAF7E2B3FA81" bold="true" box="[1370,1430,1307,1329]" italics="true" pageId="12" pageNumber="270">Joelia</emphasis>
</taxonomicName>
</keyLead>
