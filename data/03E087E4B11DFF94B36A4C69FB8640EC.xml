<paragraph id="8BF636F2B11DFF91B36A4F12FB6E4243" blockId="1.[264,1325,1583,1889]" pageId="1" pageNumber="2">
<emphasis id="B93DEAE0B11DFF91B36A4F12FE25431A" box="[264,458,1583,1608]" italics="true" pageId="1" pageNumber="2">
<typeStatus id="54F28850B11DFF91B36A4F12FED0431A" box="[264,319,1583,1608]" pageId="1" pageNumber="2">Type</typeStatus>
