<taxonomicName id="1CD883F5FFF3FFABFE02B2C9FCAD83D0" box="[492,798,1489,1513]" class="Insecta" family="Tortricidae" genus="Poedisca" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="solandriana">
<emphasis id="E9AC2464FFF3FFABFE02B2C9FDE083D0" box="[492,595,1489,1513]" italics="true" pageId="21" pageNumber="22">Poedisca</emphasis>
[sic] 
<emphasis id="E9AC2464FFF3FFABFD79B2C9FCAD83D0" box="[663,798,1489,1513]" italics="true" pageId="21" pageNumber="22">solandriana</emphasis>
</taxonomicName>
<taxonomicName id="1CD883F5FFF3FFABFCC6B2CAFC7983D0" box="[808,970,1490,1513]" pageId="21" pageNumber="22" rank="variety" variety="trapezana">
var. 
<emphasis id="E9AC2464FFF3FFABFCB4B2CAFC7983D0" box="[858,970,1490,1513]" italics="true" pageId="21" pageNumber="22">trapezana</emphasis>
