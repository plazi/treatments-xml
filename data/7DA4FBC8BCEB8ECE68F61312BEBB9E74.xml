<taxonomicName id="C7DD9D183FEF731B6EC4D639DA7FC721" class="Magnoliopsida" family="Primulaceae" genus="Myrsine" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Myrsine lessertiana" order="Ericales" pageId="0" pageNumber="109" phylum="Tracheophyta" rank="species" species="lessertiana">Myrsine lessertiana</taxonomicName>
