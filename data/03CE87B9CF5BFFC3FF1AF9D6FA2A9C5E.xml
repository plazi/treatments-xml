<taxonomicName id="4C674D2CCF58FFC3FBE4FC1AFA2A9BB6" authority="Thomson, 1868" authorityName="Thomson" authorityYear="1868" box="[1129,1436,966,987]" class="Insecta" family="Cerambycidae" genus="Hesycha" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="442" phylum="Arthropoda" rank="species" species="paupercula">
