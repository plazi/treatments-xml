<taxonomicName id="4C584D70FF81FFBDFE9C72B0FDA3C4B9" authority="Kreiter &amp; Tixier, 2010" authorityName="Kreiter &amp; Tixier" authorityYear="2010" box="[375,632,670,695]" class="Arachnida" family="Phytoseiidae" genus="Ragusaseius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="2" pageNumber="198" phylum="Arthropoda" rank="species" species="ferraguti" status="n. sp.">
<emphasis id="B92CEAE1FF81FFBDFE9C72B0FDA3C4B9" bold="true" box="[375,632,670,695]" italics="true" pageId="2" pageNumber="198">Ragusaseius ferraguti</emphasis>
