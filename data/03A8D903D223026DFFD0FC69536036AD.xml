<taxonomicName id="4C011396D223026DFFD0FC1950B1371B" box="[121,435,1005,1029]" class="Bacilli" family="Lactobacillaceae" genus="Lactiplantibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="44" pageNumber="2826" phylum="Firmicutes" rank="species" species="dongliensis">
