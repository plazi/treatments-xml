<taxonomicName id="4C104D30FFC1D0082D65CA72FE73F9CC" box="[291,461,1575,1599]" class="Magnoliopsida" family="Rhamnaceae" genus="Crumenaria" kingdom="Plantae" order="Rosales" pageId="2" pageNumber="75" phylum="Tracheophyta" rank="species" species="choretroides">
