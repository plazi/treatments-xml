<quantity id="4CDF9B48FF99FF83D1418C45C6FCB922" box="[375,495,352,378]" metricMagnitude="-2" metricUnit="m" metricValue="6.2" metricValueMax="6.9" metricValueMin="5.5" pageId="23" pageNumber="24" unit="mm" value="62.0" valueMax="69.0" valueMin="55.0">55–69 µm</quantity>
