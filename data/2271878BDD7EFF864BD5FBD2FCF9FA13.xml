<collectionCode id="CCC9AE58DD7EFF864FB5FA9FFAADFAA3" box="[1271,1336,1367,1393]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="16" pageNumber="41">ZMB</collectionCode>
