(
<collectionCode id="CCC9AE58DD7EFF864AADFACFFDD3FAF3" box="[495,582,1287,1313]" httpUri="http://grbio.org/cool/1trn-7b59" name="Museo Civico di Storia Naturale di Genova 'Giacomo Doria'" pageId="16" pageNumber="41">MSNG</collectionCode>
