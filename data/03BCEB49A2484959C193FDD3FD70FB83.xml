<materialsCitation id="3B7D5002A2484959C3CFFC3BFAA5FC4B" ID-GBIF-Occurrence="1328378007" box="[715,1293,942,967]" location="Azores" pageId="6" pageNumber="487" specimenCount="1" stateProvince="Azores">
