<taxonomicName id="19ED65AA75B15A4D820931A2F04E89AD" family="Combretaceae" genus="C." kingdom="Animalia" lsidName="C. paurus" order="Coleoptera" pageId="0" pageNumber="609" rank="species" species="paurus">
