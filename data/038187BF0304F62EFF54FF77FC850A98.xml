<taxonomicName id="4C284D2A0304F62EFA41FCD7FE5A0F61" authority="Bartsch 2003" authorityName="Bartsch" authorityYear="2003" class="Arachnida" family="Halacaridae" genus="Caspihalacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="417" phylum="Arthropoda" rank="species" species="meridianus">
