<taxonomicName id="4C284D2A0304F62EFC40FB06FC340915" box="[899,980,1257,1280]" class="Anthozoa" family="Fungiidae" genus="Fungia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Scleractinia" pageId="2" pageNumber="417" phylum="Cnidaria" rank="genus">
