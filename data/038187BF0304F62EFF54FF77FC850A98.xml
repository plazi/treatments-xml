<collectingRegion id="49ECF84B0304F62EFF54FB08FF1D0915" box="[151,253,1255,1280]" country="Tanzania" name="Kaskazini Unguja" pageId="2" pageNumber="417">
<collectingCountry id="F33F76390304F62EFF54FB08FF1D0915" box="[151,253,1255,1280]" name="Tanzania" pageId="2" pageNumber="417">Zanzibar</collectingCountry>
</collectingRegion>
