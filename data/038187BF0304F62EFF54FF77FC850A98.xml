<taxonomicName id="4C284D2A0304F62EFB7FFAE3FABE0930" authorityName="Link" authorityYear="1820" box="[1212,1374,1292,1317]" class="Ulvophyceae" family="Ulvaceae" genus="Enteromorpha" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ulvales" pageId="2" pageNumber="417" phylum="Chlorophyta" rank="genus">
