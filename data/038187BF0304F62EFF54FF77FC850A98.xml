(2008a)
</bibRefCitation>
identified 
<taxonomicName id="4C284D2A0304F62EFDF6FC4AFD500FA8" baseAuthorityName="Schulz" baseAuthorityYear="1933" box="[565,688,932,957]" class="Phyllopharyngea" family="Thecacinetidae" genus="Praethecacineta" higherTaxonomySource="GBIF" kingdom="Chromista" order="Exogenida" pageId="2" pageNumber="417" phylum="Ciliophora" rank="species" species="halacari">
