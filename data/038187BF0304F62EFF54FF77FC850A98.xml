<taxonomicName id="4C284D2A0304F62EFD22FB93FC6C0880" box="[737,908,1148,1173]" class="Arachnida" family="Halacaridae" genus="Copidognathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="417" phylum="Arthropoda" rank="genus">
