<taxonomicName id="4C284D2A0304F62EFF54FB2BFC7C08C8" authority="Chatterjee, De Troch &amp; Chang, 2006" authorityName="Chatterjee, De Troch &amp; Chang" authorityYear="2006" box="[151,924,1220,1245]" class="Arachnida" family="Halacaridae" genus="Copidognathus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="2" pageNumber="417" phylum="Arthropoda" rank="species" species="ungujaensis">
