<taxonomicName id="4C5B4D086C63FFFAB4CFF9BEE8C4F999" authority="(Morawitz, 1875)" baseAuthorityName="Morawitz" baseAuthorityYear="1875" box="[151,966,1554,1581]" class="Insecta" family="Megachilidae" genus="Pseudoanthidium" kingdom="Animalia" order="Hymenoptera" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="cribratum" subGenus="Pseudoanthidium">
