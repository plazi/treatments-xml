<paragraph id="4FEDB00E5ABEACB82373A4FC1D84290D" pageId="0" pageNumber="19">
<taxonomicName id="A4A923D96F6C2BA2F126641FEE455361" LSID="http://zoobank.org1EAF9667-E896-445B-ADED-0DA2B055DF2B" authority="Schmidt &amp; Will" authorityName="Schmidt &amp; Will" authorityYear="2022" family="Carabidae" genus="Quasicalathus" higherTaxonomySource="treatment-meta" kingdom="Animalia" lsidName="Quasicalathus conservans" order="Coleoptera" pageId="0" pageNumber="19" rank="species" species="conservans" status="sp. nov.">Quasicalathus conservans Schmidt &amp; Will</taxonomicName>
<taxonomicNameLabel id="5A6E0E0A4F921EB6ECF35FBD659CA53E" pageId="0" pageNumber="19">sp. nov.</taxonomicNameLabel>
