<taxonomicName id="4C11CBC5FFD23419FE90FB85FE3F542A" box="[292,396,1122,1147]" class="Insecta" family="Cercopidae" genus="Pseudomachaerota" kingdom="Animalia" order="Hemiptera" pageId="13" pageNumber="450" phylum="Arthropoda" rank="species" species="cucullata">
