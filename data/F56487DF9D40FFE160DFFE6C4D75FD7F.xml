. 
<taxonomicName id="BACD4D4A9D40FFE66107F89C4F5BF8B3" authority="Hirschmann, 1992" authorityName="Hirschmann" authorityYear="1992" box="[335,885,1887,1910]" class="Arachnida" family="Uropodidae" genus="Rotundabaloghia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="3" pageNumber="21" phylum="Arthropoda" rank="species" species="ukoguruensis">
