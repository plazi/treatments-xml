<taxonomicName id="2BF04D73AF60FF9B5FBCFF26DFB33E95" box="[481,673,151,176]" class="Magnoliopsida" family="Chenopodiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="family">Chenopodiaceae</taxonomicName>
), 
<taxonomicName id="2BF04D73AF60FF9B5CE9FF26DE753E95" box="[692,871,151,176]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="hyrcanus">
<emphasis id="DE84EAE2AF60FF9B5CE9FF26DE753E95" box="[692,871,151,176]" italics="true" pageId="65" pageNumber="66">Rubus hyrcanus</emphasis>
</taxonomicName>
, 
<taxonomicName id="2BF04D73AF60FF9B5D2AFF26D9793E95" box="[887,1131,151,176]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="conglomeratus">
<emphasis id="DE84EAE2AF60FF9B5D2AFF26D9793E95" box="[887,1131,151,176]" italics="true" pageId="65" pageNumber="66">Rubus conglomeratus</emphasis>
