<taxonomicName id="2BF04D73AF61FF9A5FD4F80FDCE339F2" box="[393,497,1982,2007]" class="Magnoliopsida" family="Apiaceae" kingdom="Plantae" order="Apiales" pageId="64" pageNumber="65" phylum="Tracheophyta" rank="family">Apiaceae</taxonomicName>
(
<bibRefCitation id="88614B01AF61FF9A5C5DF80FDFC639F2" author="Farahbakhsh" box="[512,724,1982,2007]" pageId="64" pageNumber="65" refString="Farahbakhsh, Gh. (1961) Family Pentatomidae (Heteroptera), In: Farahbakhsh, Gh. (ed.). A checklist of economically important insects and other enemies of plants and agricultural products in Iran. Department of Plant Protection, Ministry of Agriculture, Tehran, Publication No. 1, pp. 25 - 28." type="book chapter" year="1961">
Farahbakhsh 
<number id="5A3824D7AF61FF9A5CC4F80FDFC639F2" box="[665,724,1982,2007]" pageId="64" pageNumber="65" value="1961.0">1961</number>
</bibRefCitation>
; 
<bibRefCitation id="88614B01AF61FF9A5CBCF80FDECF39F2" author="Modarres" box="[737,989,1982,2007]" pageId="64" pageNumber="65" refString="Modarres Awal, M. (1997 b) Family Pentatomidae (Heteroptera), pp. 76 - 81. In: Modarres Awal, M. (ed.), List of agricultural pests and their natural enemies in Iran. Ferdowsi University Press, 429 pp." type="book chapter" year="1997" yearSuffix="b">
Modarres Awal 
<number id="5A3824D7AF61FF9A5DCAF80FDEC239F2" box="[919,976,1982,2007]" pageId="64" pageNumber="65" value="1997.0">1997</number>
b
</bibRefCitation>
) like 
<taxonomicName id="2BF04D73AF61FF9A5A7DF80ED81939F2" box="[1056,1291,1982,2007]" class="Insecta" family="Pentatomidae" genus="Graphosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="64" pageNumber="65" phylum="Arthropoda" rank="subSpecies" species="lineatum" subSpecies="lineatum">
<emphasis id="DE84EAE2AF61FF9A5A7DF80ED81939F2" box="[1056,1291,1982,2007]" italics="true" pageId="64" pageNumber="65">G. lineatum lineatum</emphasis>
</taxonomicName>
, and also on 
<taxonomicName id="2BF04D73AF60FF9B5ECAFF26DCC03E95" box="[151,466,151,176]" class="Magnoliopsida" family="Amaranthaceae" genus="Chenopodium" kingdom="Plantae" order="Caryophyllales" pageId="65" pageNumber="66" phylum="Tracheophyta" rank="species" species="ambrosioides">
<emphasis id="DE84EAE2AF60FF9B5ECAFF26DCC03E95" box="[151,466,151,176]" italics="true" pageId="65" pageNumber="66">Chenopodium ambrosioides</emphasis>
</taxonomicName>
