<taxonomicName id="4C684D4B4370FFB188BEFB9D6CACFB9E" authorityName="Stein &amp; Tompkins" authorityYear="1989" box="[450,694,1040,1062]" class="Actinopterygii" family="Liparidae" genus="Paraliparis" kingdom="Animalia" order="Scorpaeniformes" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="fuscolingua">Paraliparis fuscolingua</taxonomicName>
