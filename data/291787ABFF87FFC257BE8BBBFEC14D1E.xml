<taxonomicName id="66BE4D3EFF8BFFC455EC8D37FB824D2E" box="[896,1103,1083,1105]" class="Insecta" family="Eumenidae" genus="Cephalastor" kingdom="Animalia" order="Hymenoptera" pageId="11" pageNumber="1618" phylum="Arthropoda" rank="species" species="rominae">
