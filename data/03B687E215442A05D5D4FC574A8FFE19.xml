<typeStatus id="54A4885615442A06D4C8FC2648CAFC60" box="[430,511,990,1010]" pageId="33" pageNumber="760">Holotype</typeStatus>
, 
<collectingRegion id="49DBF81615442A06D74CFC264B57FC60" box="[554,610,990,1010]" country="Malaysia" name="Sabah" pageId="33" pageNumber="760">Sabah</collectingRegion>
