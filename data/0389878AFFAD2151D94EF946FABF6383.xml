<taxonomicName id="4C204D1FFFAD2151DBABF8B6FD57655F" authorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" authorityYear="2020" baseAuthorityName="Zheng &amp; Wittouck &amp; Salvetti &amp; Franz &amp; Harris &amp; Mattarelli &amp; O’Toole &amp; Pot &amp; Vandamme &amp; Walter &amp; Watanabe &amp; Wuyts &amp; Felis &amp; Gänzle &amp; Lebeer" baseAuthorityYear="2020" box="[668,760,1824,1848]" class="Bacilli" family="Lactobacillaceae" genus="Lacticaseibacillus" higherTaxonomySource="GBIF" kingdom="Bacteria" order="Lactobacillales" pageId="32" pageNumber="2814" phylum="Firmicutes" rank="species" species="sharpeae">
