<taxonomicName id="4C314D5BFF9A3A6FBA19F8B46262B883" box="[348,570,1903,1925]" class="Insecta" family="Empididae" genus="Hemerodromia" kingdom="Animalia" order="Diptera" pageId="20" pageNumber="229" phylum="Arthropoda" rank="species" species="smithi" status="sp. nov.">
<emphasis id="B945EACAFF9A3A6FBA19F8B46262B883" box="[348,570,1903,1925]" italics="true" pageId="20" pageNumber="229">Hemerodromia smithi</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A27657B1FF9A3A6FB904F8B462CAB882" box="[577,658,1903,1924]" pageId="20" pageNumber="229" rank="species">
