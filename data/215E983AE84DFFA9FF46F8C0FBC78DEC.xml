<taxonomicName id="6EF752AFE84DFFAAFDA5F808FD598B9F" authorityName="Pearse" authorityYear="1952" box="[628,706,1962,1988]" class="Hexanauplia" family="Telsidae" genus="Telson" kingdom="Animalia" order="Cyclopoida" pageId="13" pageNumber="51" phylum="Arthropoda" rank="genus">Telson</taxonomicName>
