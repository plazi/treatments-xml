<taxonomicName id="017A37D00237B3081E4A81A999A7A047" class="Magnoliopsida" family="Amaranthaceae" genus="S." kingdom="Plantae" lsidName="S. zhakovi" order="Caryophyllales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="zhakovi">
<emphasis id="E1C1D7F307543657CCE995FD010BA13F" italics="true" pageId="0" pageNumber="19">S. zhakovi</emphasis>
