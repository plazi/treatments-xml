<bibRefCitation id="69461DD516FB300482067402A14C95F1" author="Boeggemann, M." journalOrPublisher="Abhandlungen der Senckenbergischen Naturforschenden Gesellschaft" pageId="0" pageNumber="20997" pagination="1 - 249" title="Revision of the Glyceridae Grube 1850 (Annelida: Polychaeta)" volume="555" year="2002">
<normalizedToken id="75EEA7632487A3E80C56E19E8FC94985" originalValue="BÃ¶ggemann">Boeggemann</normalizedToken>
(2002)
</bibRefCitation>
considers 
<taxonomicName id="950415D1620196693A201580ED3EB7BB" class="Polychaeta" family="Glyceridae" genus="Glycera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Glycera convoluta" order="Phyllodocida" pageId="0" pageNumber="20997" phylum="Annelida" rank="species" species="convoluta">Glycera convoluta</taxonomicName>
