<taxonomicName id="91B078C984017635FF2BF9DD9EBD22A2" authority="Berta" authorityName="Berta" box="[151,450,1653,1680]" class="Insecta" family="Braconidae" genus="Cremnops" kingdom="Animalia" order="Hymenoptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="willinki">
