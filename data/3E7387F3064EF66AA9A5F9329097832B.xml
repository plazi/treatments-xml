<emphasis id="84AEEAF7064EF66BABCEF8FA975E8090" box="[764,1139,1904,1929]" italics="true" pageId="30" pageNumber="251">
<taxonomicName id="71DA4D66064EF66BABCEF8FA97428090" box="[764,1135,1904,1929]" class="Demospongiae" family="Petrosiidae" genus="Xestospongia" kingdom="Animalia" order="Haplosclerida" pageId="30" pageNumber="251" phylum="Porifera" rank="species" species="subtriangularis">Xestospongia subtriangularis</taxonomicName>
