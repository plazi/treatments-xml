<geoCoordinate id="3AEB5056FF9F0D10193EFD32103FFD29" box="[882,1056,686,712]" degrees="28" direction="south" minutes="32" orientation="latitude" pageId="34" pageNumber="485" precision="1" seconds="37.14" value="-28.54365">28°32’37.14’’S</geoCoordinate>
