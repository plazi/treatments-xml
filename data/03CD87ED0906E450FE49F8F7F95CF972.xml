<taxonomicName id="4C644D780906E457FF53F7D9FA29F799" authority="Chilton, 1925" authorityName="Chilton" authorityYear="1925" class="Malacostraca" family="Neoniphargidae" genus="Neoniphargus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="11" pageNumber="172" phylum="Arthropoda" rank="species" species="westralis">
