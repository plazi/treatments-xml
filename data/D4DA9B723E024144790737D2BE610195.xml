<taxonomicName id="E8927D16A501115CC794579EA3EC68C6" class="Insecta" family="Ichneumonidae" genus="Sinarachna" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sinarachna" order="Hymenoptera" pageId="0" pageNumber="992" phylum="Arthropoda" rank="genus">Sinarachna</taxonomicName>
