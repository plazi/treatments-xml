, 
<taxonomicName id="4C4A4D43FFBBFFC7DD81F79DE5A90B80" authority="(S. Moricand, 1836)" baseAuthorityName="S. Moricand" baseAuthorityYear="1836" class="Gastropoda" family="Simpulopsidae" genus="Simpulopsis" kingdom="Animalia" order="Stylommatophora" pageId="23" pageNumber="80" phylum="Mollusca" rank="species" species="pseudosuccinea" subGenus="Eudioptus">
