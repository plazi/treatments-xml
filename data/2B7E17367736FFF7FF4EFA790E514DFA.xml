<specimenCount id="B5D16DA97736FFF7FD9DF946096A4D66" box="[580,603,1791,1817]" pageId="13" pageNumber="394" type="male">♂</specimenCount>
, 
<quantity id="642F0BC57736FFF7FDBEF94609C04D66" box="[615,753,1791,1817]" metricMagnitude="-3" metricUnit="m" metricValue="3.55" metricValueMax="3.7" metricValueMin="3.4" pageId="13" pageNumber="394" unit="mm" value="3.55" valueMax="3.7" valueMin="3.4">3.4–3.7 mm</quantity>
; 
<specimenCount id="B5D16DA97736FFF7FD24F94608234D66" box="[765,786,1791,1817]" pageId="13" pageNumber="394" type="female">♀</specimenCount>
, 
<quantity id="642F0BC57736FFF7FCF9F947089B4D66" box="[800,938,1790,1817]" metricMagnitude="-3" metricUnit="m" metricValue="4.35" metricValueMax="4.5" metricValueMin="4.2" pageId="13" pageNumber="394" unit="mm" value="4.35" valueMax="4.5" valueMin="4.2">4.2–4.5 mm</quantity>
