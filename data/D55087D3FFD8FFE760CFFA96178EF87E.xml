<taxonomicName id="9AF94D46FFD8FFE760A0FA961710FABF" box="[248,349,1334,1359]" class="Aves" family="Strigidae" genus="Otus" kingdom="Animalia" order="Strigiformes" pageId="8" pageNumber="37" phylum="Chordata" rank="species" species="mauli">
