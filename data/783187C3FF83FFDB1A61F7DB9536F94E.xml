<taxonomicName id="37984D56FF83FFDB1AE4F6CD953FFA14" authority="Lowry &amp; Stoddart, 2002" authorityName="Lowry &amp; Stoddart" authorityYear="2002" box="[481,538,1492,1515]" class="Malacostraca" family="Amaryllididae" genus="Devo" kingdom="Animalia" order="Amphipoda" pageId="70" pageNumber="199" phylum="Arthropoda" rank="genus" status="n.gen.">
<emphasis id="C2ECEAC7FF83FFDB1AE4F6CD953FFA14" box="[481,538,1492,1515]" italics="true" pageId="70" pageNumber="199">Devo</emphasis>
</taxonomicName>
<taxonomicNameLabel id="D9DF57BCFF83FFDB1924F6CD9543FA14" box="[545,614,1492,1515]" pageId="70" pageNumber="199" rank="genus">n.gen.</taxonomicNameLabel>
; 
