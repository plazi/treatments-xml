<taxonomicName id="37984D56FF83FFDB1A68F6E8956EF9F7" authority="Walker, 1904" authorityName="Walker" authorityYear="1904" box="[365,587,1521,1544]" class="Malacostraca" family="Amaryllididae" genus="Vijaya" kingdom="Animalia" order="Amphipoda" pageId="70" pageNumber="199" phylum="Arthropoda" rank="genus">
