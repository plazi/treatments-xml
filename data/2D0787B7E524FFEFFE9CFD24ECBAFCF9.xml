<taxonomicName id="62AE4D22E527FFEEFED0FB25EC31FB5E" authority="(Brown, 1952)" authorityName="Gordon &amp; Sutherland &amp; Perez &amp; Waeschenbach &amp; Taylor &amp; Di Martino" authorityYear="2020" baseAuthorityName="Brown" baseAuthorityYear="1952" box="[257,595,1192,1215]" class="Gymnolaemata" family="Electridae" genus="Conopeum" kingdom="Animalia" order="Cheilostomatida" pageId="18" pageNumber="964" phylum="Bryozoa" rank="species" species="ongleyi">
