<taxonomicName id="4C1D4D1DFFF33F79FEF494FBFD9AF83E" box="[322,586,1976,2000]" class="Insecta" family="Staphylinidae" genus="Ischnosoma" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="231" phylum="Arthropoda" rank="species" species="fusciventre">
