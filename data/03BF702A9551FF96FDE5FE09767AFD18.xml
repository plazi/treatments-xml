, 
<taxonomicName id="4C16BABF9551FF96F9CCFD627627FC8C" authorityName="Gillette" authorityYear="1893" box="[1214,1354,742,768]" class="Insecta" family="Curculionidae" genus="Synergus" kingdom="Animalia" order="Coleoptera" pageId="84" pageNumber="85" phylum="Arthropoda" rank="species" species="garryana">
