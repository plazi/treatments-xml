<number id="3DEA2498FFB8FF9CFC2617EAFC74F827" box="[896,949,1924,1949]" pageId="7" pageNumber="44" value="5.52">5.52</number>
; tibia 
<number id="3DEA2498FFB8FF9CFBA517EBFBD2F827" box="[1027,1043,1925,1949]" pageId="7" pageNumber="44" value="1.0">1</number>
