, 
<collectingCountry id="56E09096FFEAFFEBFE5EFEDAC46AFE99" box="[458,573,266,290]" name="Venezuela" pageId="14" pageNumber="1389">Venezuela</collectingCountry>
, northern 
<collectingCountry id="56E09096FFEAFFEBFD29FEDAC6ADFEFF" name="French Guiana" pageId="14" pageNumber="1389">French Guiana</collectingCountry>
, 
<collectingCountry id="56E09096FFEAFFEBFE90FEFCC710FEFF" box="[260,327,300,324]" name="Brazil" pageId="14" pageNumber="1389">Brazil</collectingCountry>
and 
