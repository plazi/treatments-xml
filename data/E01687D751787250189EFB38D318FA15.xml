<taxonomicName id="AFBF4D42517E72501B43FA84D3CEFA5B" authorityName="Basden" authorityYear="1961" box="[842,962,1395,1417]" class="Insecta" family="Drosophilidae" genus="Lordiphosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="66" phylum="Arthropoda" rank="genus">
