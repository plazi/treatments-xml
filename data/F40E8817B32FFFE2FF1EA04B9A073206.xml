<taxonomicName id="BBA74282B32FFFE2FAA4A0FF99E7325E" authority="Miers 1886" authorityName="Miers" authorityYear="1886" class="Malacostraca" family="Leucosiidae" genus="Randallia" kingdom="Animalia" order="Decapoda" pageId="5" pageNumber="50" phylum="Arthropoda" rank="species" species="granulata">
<emphasis id="4ED3E513B32FFFE2FAA4A0FF996C325E" italics="true" pageId="5" pageNumber="50">Randallia granulata</emphasis>
