<taxonomicName id="A9FD4D1F674E0452F2EEB455FE9FF86D" authorityName="Kugelann" box="[151,272,1902,1928]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="72" pageNumber="73" phylum="Arthropoda" rank="genus">
<emphasis id="5C89EA8E674E0452F2EEB455FE9FF86D" bold="true" box="[151,272,1902,1928]" italics="true" pageId="72" pageNumber="73">Hydraena</emphasis>
</taxonomicName>
