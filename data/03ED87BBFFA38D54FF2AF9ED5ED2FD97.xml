<emphasis id="B930EABFFFA38D55FF2AF9ED5D51F909" bold="true" box="[151,538,1663,1687]" pageId="0" pageNumber="594">
<emphasis id="B930EABFFFA38D55FF2AF9ED5EE9F909" bold="true" box="[151,418,1663,1687]" italics="true" pageId="0" pageNumber="594">
<taxonomicName id="4C444D2EFFA38D55FF2AF9ED5ED0F909" box="[151,411,1663,1687]" class="Diplopoda" family="Pyrgodesmidae" genus="Myrmecodesmus" kingdom="Animalia" order="Polydesmida" pageId="0" pageNumber="594" phylum="Arthropoda" rank="species" species="sheari">Myrmecodesmus sheari</taxonomicName>
,
</emphasis>
