<quantity id="49319B4F324EFFF9FAFFFB463DBEDD39" box="[1346,1420,1274,1298]" metricMagnitude="2" metricUnit="m" metricValue="8.2" pageId="12" pageNumber="545" unit="m" value="820.0">820 m</quantity>
