<taxonomicName id="4C6D4D59FC2A443BFEE10A3CFDBF2A36" box="[320,628,1590,1611]" class="Insecta" family="Ditomyiidae" genus="Calliceratomyia" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="435" phylum="Arthropoda" rank="species" species="pantelhomyia" status="sp. n.">
