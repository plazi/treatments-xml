<taxonomicName id="4C6D4D59FC294438FE710AA8FDBF2ACA" box="[464,628,1698,1719]" class="Insecta" family="Ditomyiidae" genus="Calliceratomyia" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="436" phylum="Arthropoda" rank="genus">
