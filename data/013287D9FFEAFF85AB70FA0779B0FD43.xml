<location id="8C446014FFEAFF86AB26F9B17806F99C" LSID="urn:lsid:plazi:treatment:013287D9FFEAFF85AB70FA0779B0FD43:8C446014FFEAFF86AB26F9B17806F99C" box="[311,514,1632,1656]" name="Serra do Caparao" pageId="37" pageNumber="2796" stateProvince="Minas Gerais, Rio de Janeiro and Sao Paulo">Serra do Caparaó</location>
