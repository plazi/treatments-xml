</subSubSection>
<subSubSection pageId="2" pageNumber="3" type="reference_group">
<paragraph pageId="2" pageNumber="3">
<taxonomicName class="Insecta" family="Perlidae" genus="Neoperla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neoperla duratubulata" order="Plecoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="duratubulata">Neoperla duratubulata</taxonomicName>
