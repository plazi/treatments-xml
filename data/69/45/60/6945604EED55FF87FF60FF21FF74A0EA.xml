<subSubSection pageId="1" pageNumber="44" type="discussion">
<paragraph blockId="1.[151,1436,366,970]" pageId="1" pageNumber="44">
<emphasis box="[199,318,875,898]" italics="true" pageId="1" pageNumber="44">Comments</emphasis>
