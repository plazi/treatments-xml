<taxonomicName authority="Hille Ris Lambers 1960" authorityName="Hille Ris Lambers" authorityYear="1960" class="Insecta" family="Aphididae" genus="Aphis" kingdom="Animalia" order="Aphidomorpha" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="longituba">
