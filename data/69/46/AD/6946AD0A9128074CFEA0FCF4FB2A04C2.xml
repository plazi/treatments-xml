<number box="[1023,1081,894,920]" pageId="11" pageNumber="12" value="1983.0">1983</number>
</bibRefCitation>
</taxonomicName>
= 
<taxonomicName authority="Basu 1964" authorityName="Basu" authorityYear="1964" class="Insecta" family="Aphididae" genus="Indomasonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="anaphalidis">
<emphasis bold="true" italics="true" pageId="11" pageNumber="12">Indomasonaphis anaphalidis</emphasis>
(Basu 
<number box="[492,552,936,962]" pageId="11" pageNumber="12" value="1964.0">1964</number>
)
</taxonomicName>
</emphasis>
