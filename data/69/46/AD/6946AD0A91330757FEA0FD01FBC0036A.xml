<taxonomicName authority="Verma 1971" authorityName="Verma" authorityYear="1971" box="[864,1223,678,704]" class="Insecta" family="Aphididae" genus="Indomasonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
