(
<taxonomicName box="[1137,1255,1223,1249]" class="Insecta" family="Aphididae" genus="Protaphis" kingdom="Animalia" order="Aphidomorpha" pageId="26" pageNumber="27" phylum="Arthropoda" rank="genus">Protaphis</taxonomicName>
