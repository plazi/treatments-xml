<emphasis bold="true" box="[264,1183,592,618]" pageId="17" pageNumber="18">
<taxonomicName authority="Zhang, Chen, Zhong &amp; Li 1999" authorityName="Zhang, Chen, Zhong &amp; Li" authorityYear="1999" box="[264,780,592,618]" class="Insecta" family="Aphididae" genus="Piceaphis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[264,381,592,618]" italics="true" pageId="17" pageNumber="18">Piceaphis</emphasis>
Zhang, Chen, Zhong &amp; Li 
<number box="[722,780,592,618]" pageId="17" pageNumber="18" value="1999.0">1999</number>
</taxonomicName>
= 
<taxonomicName authority="Del Guercio" authorityName="Del Guercio" box="[812,1183,592,618]" class="Insecta" family="Aphididae" genus="Macrosiphoniella" kingdom="Animalia" order="Aphidomorpha" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
