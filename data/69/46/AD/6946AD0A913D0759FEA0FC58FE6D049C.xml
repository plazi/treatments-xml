<taxonomicName authority="Bozhko" authorityName="Bozhko" class="Insecta" family="Aphididae" genus="Obtusicauda" kingdom="Animalia" order="Aphidomorpha" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="moldavica">
