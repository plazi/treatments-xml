<collectingCountry box="[1164,1234,420,446]" name="Japan" pageId="12" pageNumber="13">Japan</collectingCountry>
, 
<collectingCountry box="[1245,1318,420,446]" name="China" pageId="12" pageNumber="13">China</collectingCountry>
, 
<collectingCountry box="[264,338,463,489]" name="South Korea" pageId="12" pageNumber="13">Korea</collectingCountry>
