<taxonomicName authority="Gillette &amp; Palmer 1933" authorityName="Gillette &amp; Palmer" authorityYear="1933" class="Insecta" family="Aphididae" genus="Pleotrichophorus" kingdom="Animalia" order="Aphidomorpha" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="pullus">
