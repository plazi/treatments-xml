<date box="[867,965,1497,1523]" pageId="8" pageNumber="9" value="1980-09-30">
<number box="[867,899,1497,1523]" pageId="8" pageNumber="9" value="30.0">30</number>
.ix. 
<number box="[933,965,1497,1523]" pageId="8" pageNumber="9" value="80.0">80</number>
</date>
