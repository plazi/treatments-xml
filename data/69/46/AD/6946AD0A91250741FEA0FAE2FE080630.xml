<emphasis bold="true" box="[264,1318,1349,1375]" pageId="6" pageNumber="7">
<taxonomicName box="[264,543,1349,1375]" class="Insecta" family="Aphididae" genus="Aphis" kingdom="Animalia" order="Aphidomorpha" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="senecionisdoriae">
<emphasis bold="true" box="[264,543,1349,1375]" italics="true" pageId="6" pageNumber="7">Aphis senecionisdoriae</emphasis>
</taxonomicName>
ssp. 
<emphasis bold="true" box="[604,741,1349,1375]" italics="true" pageId="6" pageNumber="7">fluviatilidis</emphasis>
<bibRefCitation author="Bozhko" box="[748,908,1349,1375]" pageId="6" pageNumber="7" refString="Bozhko, M. P. (1976 a) [Aphids of cultivated plants.] Izdatelskoe Obedinente &quot; Vishtsha Shkola &quot; Izdatelstvo Kharkovskom Gosudarstvennom Universitete, Kharkov, 135 pp." type="book" year="1976">
Bozhko 
<number box="[850,908,1349,1375]" pageId="6" pageNumber="7" value="1976.0">1976</number>
</bibRefCitation>
= 
<taxonomicName authority="Schrank 1801" authorityName="Schrank" authorityYear="1801" box="[940,1318,1349,1375]" class="Insecta" family="Aphididae" genus="Aphis" kingdom="Animalia" order="Aphidomorpha" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="jacobaeae">
<emphasis bold="true" box="[940,1138,1349,1375]" italics="true" pageId="6" pageNumber="7">Aphis jacobaeae</emphasis>
Schrank 
<number box="[1259,1318,1349,1375]" pageId="6" pageNumber="7" value="1801.0">1801</number>
</taxonomicName>
</emphasis>
</heading>
</paragraph>
