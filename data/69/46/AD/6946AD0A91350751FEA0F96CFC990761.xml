<taxonomicName box="[956,1158,1825,1850]" class="Insecta" family="Aphididae" genus="Trichosiphonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="polygonifoliae">
<emphasis box="[956,1158,1825,1850]" italics="true" pageId="22" pageNumber="23">T. polygonifoliae</emphasis>
</taxonomicName>
. 
<taxonomicName box="[1178,1292,1825,1850]" class="Magnoliopsida" family="Oleaceae" genus="Forsythia" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
