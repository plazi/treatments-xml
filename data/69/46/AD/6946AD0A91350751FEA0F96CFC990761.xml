<taxonomicName box="[637,855,1869,1894]" class="Insecta" family="Aphididae" genus="Trichosiphonaphis" kingdom="Animalia" order="Aphidomorpha" pageId="22" pageNumber="23" phylum="Arthropoda" rank="genus">
