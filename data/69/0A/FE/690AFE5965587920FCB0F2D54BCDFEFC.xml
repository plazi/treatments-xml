, 
<emphasis bold="true" box="[970,1415,1661,1680]" pageId="38" pageNumber="389">
<taxonomicName authority="Aguiar &amp; Melo, 2011" authorityName="Aguiar &amp; Melo" authorityYear="2011" box="[970,1316,1661,1680]" class="Insecta" family="Apidae" genus="Paratetrapedia" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="389" phylum="Arthropoda" rank="species" species="tocantinensis" status="sp. nov.">
<emphasis bold="true" box="[970,1316,1661,1680]" italics="true" pageId="38" pageNumber="389">Paratetrapedia tocantinensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1326,1415,1661,1680]" pageId="38" pageNumber="389" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName baseAuthorityName="FRIESE" baseAuthorityYear="1923" box="[144,386,1690,1709]" class="Insecta" family="Apidae" genus="Paratetrapedia" kingdom="Animalia" order="Hymenoptera" pageId="38" pageNumber="389" phylum="Arthropoda" rank="species" species="romani">
<emphasis box="[144,386,1690,1709]" italics="true" pageId="38" pageNumber="389">Paratetrapedia romani</emphasis>
</taxonomicName>
