<collectingRegion box="[445,529,654,676]" country="Brazil" name="Amapa" pageId="35" pageNumber="386">Amapá</collectingRegion>
, Amazonas, Pará; 
<collectingCountry box="[164,256,685,706]" name="Guyana" pageId="35" pageNumber="386">Guyana</collectingCountry>
; 
<collectingCountry box="[270,445,685,707]" name="French Guiana" pageId="35" pageNumber="386">French Guiana</collectingCountry>
(
<figureCitation box="[463,544,685,707]" captionStart="Figure 68" captionStartId="29.[164,243,898,917]" captionText="Figure 68. Geographical distribution of the species of the bicolor group." pageId="35" pageNumber="386">Fig. 68</figureCitation>
