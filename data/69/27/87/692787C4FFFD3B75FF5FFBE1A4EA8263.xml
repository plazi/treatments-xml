,female. 
<emphasis bold="true" box="[635,677,1772,1792]" pageId="27" pageNumber="434">40-41</emphasis>
, 
<taxonomicName authority="Braet, 2016" authorityName="Braet" authorityYear="2016" box="[685,861,1772,1792]" class="Insecta" family="Braconidae" genus="Austroascogaster" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="434" phylum="Arthropoda" rank="species" species="elsae" status="n.sp.">
