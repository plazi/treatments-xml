<emphasis bold="true" box="[1051,1069,1772,1792]" pageId="27" pageNumber="434">37</emphasis>
, 
<emphasis bold="true" box="[1077,1095,1772,1792]" pageId="27" pageNumber="434">40</emphasis>
, head, face; 
<emphasis bold="true" box="[1192,1210,1772,1792]" pageId="27" pageNumber="434">38</emphasis>
, head,posterior; 
<emphasis bold="true" box="[204,246,1799,1819]" pageId="27" pageNumber="434">39-41</emphasis>
