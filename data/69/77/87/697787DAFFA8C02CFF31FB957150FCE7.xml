<typeStatus box="[394,498,546,570]" pageId="14" pageNumber="453">Holotype</typeStatus>
Ƥ, 
<collectingCountry box="[558,715,546,570]" name="Indonesia" pageId="14" pageNumber="453">INDONESIA</collectingCountry>
