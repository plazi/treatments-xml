<subSubSection pageId="25" pageNumber="26" type="description">
<paragraph blockId="25.[189,357,480,506]" box="[189,357,480,506]" pageId="25" pageNumber="26">
<heading bold="true" box="[189,357,480,506]" fontSize="11" level="3" pageId="25" pageNumber="26" reason="3">
