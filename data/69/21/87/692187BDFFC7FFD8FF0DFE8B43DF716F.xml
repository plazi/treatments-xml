<number box="[975,1029,1250,1276]" pageId="7" pageNumber="41" value="0.16">0.16</number>
, ALE 
<number box="[1110,1163,1250,1276]" pageId="7" pageNumber="41" value="0.11">0.11</number>
, PME 
<number box="[1247,1301,1250,1276]" pageId="7" pageNumber="41" value="0.13">0.13</number>
