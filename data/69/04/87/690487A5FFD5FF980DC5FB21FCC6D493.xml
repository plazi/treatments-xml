<collectingCountry box="[159,275,1453,1479]" name="Myanmar" pageId="22" pageNumber="214">Myanmar</collectingCountry>
, 
<collectingCountry box="[295,399,1453,1479]" name="Thailand" pageId="22" pageNumber="214">Thailand</collectingCountry>
