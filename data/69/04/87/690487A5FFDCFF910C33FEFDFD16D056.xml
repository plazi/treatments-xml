<subSubSection pageId="31" pageNumber="223" type="distribution">
<paragraph blockId="31.[159,772,272,805]" box="[159,302,403,429]" pageId="31" pageNumber="223">Distribution</paragraph>
<paragraph blockId="31.[159,772,272,805]" pageId="31" pageNumber="223">
E 
<collectingCountry box="[225,371,437,463]" name="Afghanistan" pageId="31" pageNumber="223">Afghanistan</collectingCountry>
, N 
<collectingCountry box="[420,522,437,463]" name="Pakistan" pageId="31" pageNumber="223">Pakistan</collectingCountry>
, N and C 
<collectingCountry box="[657,720,437,463]" name="India" pageId="31" pageNumber="223">India</collectingCountry>
, SE 
<collectingRegion box="[159,223,472,498]" country="China" name="Xizang" pageId="31" pageNumber="223">Tibet</collectingRegion>
(
<collectingCountry box="[255,334,472,498]" name="China" pageId="31" pageNumber="223">China</collectingCountry>
), 
<collectingCountry box="[372,493,472,498]" name="Myanmar" pageId="31" pageNumber="223">Myanmar</collectingCountry>
, 
<collectingCountry box="[520,632,472,498]" name="Thailand" pageId="31" pageNumber="223">Thailand</collectingCountry>
, 
<collectingCountry box="[660,767,472,498]" name="Vietnam" pageId="31" pageNumber="223">Vietnam</collectingCountry>
, through SE Asia to Lesser Sunda Islands and the 
<collectingCountry box="[159,295,540,566]" name="Philippines" pageId="31" pageNumber="223">Philippines</collectingCountry>
.
</paragraph>
