<number box="[1354,1368,1845,1871]" pageId="16" pageNumber="17" value="4.0">4</number>
<number box="[1376,1431,1845,1871]" pageId="16" pageNumber="17" value="0.68">0.68</number>
; P- 
<number box="[177,191,1884,1910]" pageId="16" pageNumber="17" value="2.0">2</number>
