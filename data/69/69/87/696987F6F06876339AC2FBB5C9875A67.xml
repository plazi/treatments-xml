(
<number box="[1002,1058,1845,1871]" pageId="16" pageNumber="17" value="34.3">34.3</number>
), P- 
<number box="[1107,1120,1845,1871]" pageId="16" pageNumber="17" value="5.0">5</number>
<number box="[1129,1158,1845,1871]" pageId="16" pageNumber="17" value="38.0">38</number>
(
<number box="[1175,1231,1845,1871]" pageId="16" pageNumber="17" value="15.9">15.9</number>
