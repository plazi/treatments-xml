<figureCitation box="[415,464,1352,1373]" captionStart="Figure 33" captionStartId="60.[144,223,1062,1081]" captionTargetBox="[303,1263,195,1033]" captionTargetId="figure-304@60.[303,1263,195,1033]" captionTargetPageId="60" captionText="Figure 33. Legs: A, Heraeus concolor Slater &amp; Baranowski, 1994; B, Heraeus ecuatorianus sp. nov.; C, Heraeus loja sp. nov.; D, Heraeus mesoamericanus sp. nov.; E, Heraeus mexicanus sp. nov.; F, Heraeus morganae sp. nov." pageId="62" pageNumber="91">33C</figureCitation>
, 
<figureCitation box="[477,560,1352,1373]" captionStart="Figure 34" captionStartId="63.[164,241,1714,1733]" captionTargetBox="[256,1349,194,1683]" captionTargetId="figure-0@63.[253,1354,192,1684]" captionTargetPageId="63" captionText="Figure 34. Male genitalia. Heraeus loja sp. nov.: A, pygophore, dorsal view; B, pygophore, lateral view; C, right paramere, inner view; D, right paramere, external view. Heraeus mesoamericanus sp. nov.: E, pygophore, dorsal view; F, pygophore, lateral view; G, right paramere, inner view; H, right paramere, external view. Heraeus mexicanus sp. nov.: I, pygophore, dorsal view; J, pygophore, lateral view; K, right paramere, inner view; L, right paramere, external view. Heraeus morganae sp. nov.: M, pygophore, dorsal view; N, pygophore, lateral view; O, right paramere, inner view; P, right paramere, external view." pageId="62" pageNumber="91">34A–D</figureCitation>
, 
<figureCitation box="[573,601,1352,1374]" captionStart="Figure 35" captionStartId="64.[144,223,1163,1182]" captionTargetBox="[148,1420,198,1129]" captionTargetId="figure-301@64.[144,1424,194,1133]" captionTargetPageId="64" captionText="Figure 35. Distributional map of Heraeus loja sp. nov. and Heraeus pallidinervis sp. nov." pageId="62" pageNumber="91">35</figureCitation>
