<taxonomicName authority="Dellapé &amp; Melo &amp; Henry, 2016" authorityName="Dellapé &amp; Melo &amp; Henry" authorityYear="2016" box="[352,609,1059,1079]" class="Insecta" family="Rhyparochromidae" genus="Heraeus" kingdom="Animalia" order="Hemiptera" pageId="70" pageNumber="99" phylum="Arthropoda" rank="species" species="nicaraguensis" status="sp. nov.">
<emphasis bold="true" box="[352,609,1059,1079]" italics="true" pageId="70" pageNumber="99">Heraeus nicaraguensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[614,694,1060,1079]" pageId="70" pageNumber="99" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; B, 
<taxonomicName authority="Barber, 1925" authorityName="Barber" authorityYear="1925" box="[736,1047,1059,1079]" class="Insecta" family="Rhyparochromidae" genus="Heraeus" kingdom="Animalia" order="Hemiptera" pageId="70" pageNumber="99" phylum="Arthropoda" rank="species" species="pacificus">
<emphasis box="[736,911,1060,1079]" italics="true" pageId="70" pageNumber="99">Heraeus pacificus</emphasis>
<bibRefCitation author="Barber HG" box="[917,1047,1059,1078]" pageId="70" pageNumber="99" pagination="241 - 254" refId="ref75170" refString="Barber HG. 1925. Hemiptera-Heteroptera from the Williams Galapagos expeditions. Zoologica 5: 241 - 254." type="journal article" year="1925">Barber, 1925</bibRefCitation>
</taxonomicName>
; C, 
<emphasis bold="true" box="[1086,1415,1059,1079]" pageId="70" pageNumber="99">
<taxonomicName authority="Dellapé &amp; Melo &amp; Henry, 2016" authorityName="Dellapé &amp; Melo &amp; Henry" authorityYear="2016" box="[1086,1329,1059,1079]" class="Insecta" family="Rhyparochromidae" genus="Heraeus" kingdom="Animalia" order="Hemiptera" pageId="70" pageNumber="99" phylum="Arthropoda" rank="species" species="pallidinervis" status="sp. nov.">
