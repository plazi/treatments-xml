<collectingCountry box="[144,268,1880,1902]" name="Costa Rica" pageId="70" pageNumber="99">Costa Rica</collectingCountry>
and 
<collectingCountry box="[331,450,1880,1902]" name="Nicaragua" pageId="70" pageNumber="99">Nicaragua</collectingCountry>
(
<figureCitation box="[468,549,1880,1902]" captionStart="Figure 38" captionStartId="68.[144,222,1166,1185]" captionTargetBox="[144,1424,195,1136]" captionTargetId="figure-238@68.[144,1424,195,1136]" captionTargetPageId="68" captionText="Figure 38. Distributional map of Heraeus mexicanus sp. nov., Heraeus nicaraguensis sp. nov., and Heraeus pacificus Barber, 1925." pageId="70" pageNumber="99">Fig. 38</figureCitation>
). 
<emphasis box="[806,925,1166,1187]" italics="true" pageId="70" pageNumber="99">Etymology</emphasis>
</paragraph>
