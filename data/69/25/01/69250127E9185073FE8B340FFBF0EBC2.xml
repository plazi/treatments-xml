and 
<collectingCountry box="[1004,1084,749,771]" name="Bolivia" pageId="59" pageNumber="88">Bolivia</collectingCountry>
(
<figureCitation box="[1102,1183,749,771]" captionStart="Figure 28" captionStartId="52.[144,223,1163,1182]" captionTargetBox="[148,1420,199,1129]" captionTargetId="figure-284@52.[144,1424,195,1133]" captionTargetPageId="52" captionText="Figure 28. Distributional map of Heraeus alvarengai sp. nov. and Heraeus brevirostris sp. nov." pageId="59" pageNumber="88">Fig. 28</figureCitation>
