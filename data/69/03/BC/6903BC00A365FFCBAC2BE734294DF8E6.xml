<subSubSection box="[555,1033,1635,1662]" pageId="43" pageNumber="44" type="nomenclature">
<paragraph blockId="43.[555,1033,1635,1697]" box="[555,1033,1635,1662]" pageId="43" pageNumber="44">
<taxonomicName authorityName="Burmeister" authorityYear="1838" box="[555,676,1635,1661]" class="Insecta" family="Cicadellidae" genus="Athysanus" kingdom="Animalia" order="Hemiptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="genus">
<emphasis box="[555,676,1635,1661]" italics="true" pageId="43" pageNumber="44">Athysanus</emphasis>
</taxonomicName>
-group (informal genus group)
</paragraph>
</subSubSection>
<subSubSection box="[753,834,1670,1697]" pageId="43" pageNumber="44" type="description">
