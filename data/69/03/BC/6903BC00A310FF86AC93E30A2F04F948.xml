<paragraph blockId="96.[189,1232,915,1681]" box="[189,1061,985,1012]" pageId="96" pageNumber="97">
<taxonomicName authority="Oman, 1949" authorityName="Oman" authorityYear="1949" box="[189,483,986,1012]" class="Insecta" family="Cicadellidae" genus="Attenuipyga" kingdom="Animalia" order="Hemiptera" pageId="96" pageNumber="97" phylum="Arthropoda" rank="genus">
