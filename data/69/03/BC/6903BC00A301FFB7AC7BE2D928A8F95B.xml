<paragraph blockId="81.[189,1014,1463,1666]" box="[189,543,1604,1630]" pageId="81" pageNumber="82">
<taxonomicName authority="Kirkaldy, 1901" authorityName="Kirkaldy" authorityYear="1901" box="[189,543,1604,1630]" class="Insecta" family="Cicadellidae" genus="Paradorydium" kingdom="Animalia" order="Hemiptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="genus">
