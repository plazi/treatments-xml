<subSubSection pageId="13" pageNumber="566" type="etymology">
<paragraph blockId="13.[151,1437,1058,1516]" pageId="13" pageNumber="566">
<emphasis bold="true" box="[199,334,1202,1228]" pageId="13" pageNumber="566">Etymology.</emphasis>
