<paragraph id="3235D716615A31A8A7D2018A8E8C9DD5" pageId="5" pageNumber="142">
<taxonomicName id="CC80CB27AFFBC3F797DE79CC2FEB4AC5" authority="" authorityName="" class="Insecta" family="Ichneumonidae" genus="Thalessa" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Thalessa" order="Hymenoptera" pageId="5" pageNumber="142" phylum="Arthropoda" rank="genus">Thalessa</taxonomicName>
