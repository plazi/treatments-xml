<taxonomicName id="A2BDD91C5E3348625DFE017B9D772FA6" class="Insecta" family="Ichneumonidae" genus="Megarhyssa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Megarhyssa" order="Hymenoptera" pageId="5" pageNumber="142" phylum="Arthropoda" rank="genus">Megarhyssa</taxonomicName>
