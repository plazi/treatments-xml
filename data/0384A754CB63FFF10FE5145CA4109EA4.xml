<number id="3DE50465CB63FFF00BD014C1A2D69A7C" box="[1186,1230,1000,1026]" pageId="38" pageNumber="39" value="105.0">105</number>
(
<number id="3DE50465CB63FFF00B9214C1A3509A7C" box="[1248,1352,1000,1026]" pageId="38" pageNumber="39" value="106.5" valueMax="110.0" valueMin="103.0">103–110</number>
