<number id="3DE50465CB63FFF00EA21339A7F59A54" box="[464,493,1040,1066]" pageId="38" pageNumber="39" value="90.0">90</number>
(
<number id="3DE50465CB63FFF00E8C1339A4559A54" box="[510,589,1040,1066]" pageId="38" pageNumber="39" value="88.5" valueMax="90.0" valueMin="87.0">87–90</number>
), projecting downwards, pedipalp coxal setae (
<emphasis id="B959CA50CB63FFF00B091339A28F9A57" box="[1147,1175,1040,1065]" italics="true" pageId="38" pageNumber="39">ep</emphasis>
) 
<number id="3DE50465CB63FFF00BDB1339A2A09A54" box="[1193,1208,1040,1066]" pageId="38" pageNumber="39" value="5.0">5</number>
(
<number id="3DE50465CB63FFF00BBB1339A2E09A54" box="[1225,1272,1040,1066]" pageId="38" pageNumber="39" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), dorsal pedipalp genual setae (
<emphasis id="B959CA50CB63FFF00E011311A79A9A2F" box="[371,386,1080,1105]" italics="true" pageId="38" pageNumber="39">d</emphasis>
) 
<number id="3DE50465CB63FFF00EE61311A7A99A2C" box="[404,433,1080,1106]" pageId="38" pageNumber="39" value="12.0">12</number>
(
<number id="3DE50465CB63FFF00EB01311A4169A2C" box="[450,526,1080,1106]" pageId="38" pageNumber="39" value="11.5" valueMax="12.0" valueMin="11.0">11–12</number>
), chelicerae 
<number id="3DE50465CB63FFF00DD01311A4A79A2C" box="[674,703,1080,1106]" pageId="38" pageNumber="39" value="82.0">82</number>
(
<number id="3DE50465CB63FFF00DA21311A5079A2C" box="[720,799,1080,1106]" pageId="38" pageNumber="39" value="81.5" valueMax="83.0" valueMin="80.0">80–83</number>
