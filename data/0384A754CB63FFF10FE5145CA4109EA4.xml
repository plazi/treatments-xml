(
<number id="3DE50465CB63FFF00C5114C1A5889A7C" box="[803,912,1000,1026]" pageId="38" pageNumber="39" value="343.5" valueMax="347.0" valueMin="340.0">340–347</number>
), 
<number id="3DE50465CB63FFF00CD414C1A5C89A7C" box="[934,976,1000,1026]" pageId="38" pageNumber="39" value="111.0">111</number>
(
<number id="3DE50465CB63FFF00C9314C1A25F9A7C" box="[993,1095,1000,1026]" pageId="38" pageNumber="39" value="108.0" valueMax="111.0" valueMin="105.0">105–111</number>
