, 
<collectingCountry id="DEB2763101526674FD75FBFFFC994209" box="[672,792,1114,1140]" name="Zimbabwe" pageId="12" pageNumber="41">Zimbabwe</collectingCountry>
and 
<collectingCountry id="DEB2763101526674FC9BFBFFFC674209" box="[846,998,1114,1140]" name="Mozambique" pageId="12" pageNumber="41">Mozambique</collectingCountry>
