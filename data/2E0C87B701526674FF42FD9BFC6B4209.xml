<taxonomicName id="61A54D2201526674FEB2FBFEFE644209" authorityName="Bolivar" authorityYear="1915" box="[359,485,1114,1141]" class="Insecta" family="Pamphagidae" genus="Cultrinotus" kingdom="Animalia" order="Orthoptera" pageId="12" pageNumber="41" phylum="Arthropoda" rank="species" species="poultoni">
<emphasis id="94D1EAB301526674FEB2FBFEFE644209" box="[359,485,1114,1141]" italics="true" pageId="12" pageNumber="41">C. poultoni</emphasis>
