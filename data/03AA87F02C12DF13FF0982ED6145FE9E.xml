<taxonomicName id="4C034D652C11DF15FE2281DD60D1F978" authority="Sánchez, 2020" authorityName="Sánchez" authorityYear="2020" box="[444,664,1763,1787]" class="Insecta" family="Asilidae" genus="Regasilus" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="223" phylum="Arthropoda" rank="species" species="huiracocha" status="sp. nov.">
