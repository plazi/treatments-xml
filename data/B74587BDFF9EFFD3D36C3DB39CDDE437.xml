<taxonomicName id="F8EC4D28FF9DFFD3D6C83D4A9FAFE437" authority="Putzeys" authorityName="Putzeys" class="Insecta" family="Carabidae" genus="Ardistomis" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="elongatulus">
