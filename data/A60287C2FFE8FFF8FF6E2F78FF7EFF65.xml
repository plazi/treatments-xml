<emphasis id="1CDFEAC6FFE8FFFDFF6E2F78FD58FA0A" bold="true" box="[151,766,1402,1429]" pageId="1" pageNumber="402">
<taxonomicName id="E9AB4D57FFE8FFFDFF6E2F78FE0BFA0B" authority="Malicky &amp; Ivanov &amp; Melnitsky, 2020" authorityName="Malicky &amp; Ivanov &amp; Melnitsky" authorityYear="2020" box="[151,429,1402,1429]" class="Insecta" family="Polycentropodidae" genus="Nyctiophylax" kingdom="Animalia" order="Trichoptera" pageId="1" pageNumber="402" phylum="Arthropoda" rank="species" species="dosyhpor" status="new species">
