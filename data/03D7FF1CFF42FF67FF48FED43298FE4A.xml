<paragraph id="8BC14E0AFF41FF67FF48FA0F3298FE4A" blockId="138.[192,1440,353,442]" pageId="138" pageNumber="137">
<emphasis id="B90A9218FF41FF67FF48FA0F3074FE89" bold="true" box="[192,264,353,377]" pageId="138" pageNumber="137">Note.</emphasis>
<taxonomicName id="4C7E3589FF41FF67FE87FA0F308DFE89" baseAuthorityName="Perty" baseAuthorityYear="1830" box="[271,497,353,377]" class="Insecta" family="Rutelidae" genus="Lagochile" kingdom="Animalia" order="Coleoptera" pageId="138" pageNumber="137" phylum="Arthropoda" rank="species" species="brunnea">
