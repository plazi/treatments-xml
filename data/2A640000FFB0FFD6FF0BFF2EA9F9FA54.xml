<subSubSection id="EAD7E29DFFB3FFD6FF5AFACAAD45FA24" box="[198,1348,1395,1421]" pageId="10" pageNumber="39" type="etymology">
<paragraph id="A272B116FFB3FFD6FF5AFACAAD45FA24" blockId="10.[151,1437,723,1533]" box="[198,1348,1395,1421]" pageId="10" pageNumber="39">
