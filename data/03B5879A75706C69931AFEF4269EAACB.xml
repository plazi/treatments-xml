<taxonomicName id="4C1C4D0F75706C669375FCCF2076AB67" authority="Eliot" authorityName="Eliot" authorityYear="1904" box="[823,1078,840,862]" class="Gastropoda" family="Discodorididae" genus="Thordisa" kingdom="Animalia" order="Nudibranchia" pageId="39" pageNumber="574" phylum="Mollusca" rank="species" species="stellata">
