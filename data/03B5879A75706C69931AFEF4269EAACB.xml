<collectingRegion id="49D8F86E75706C66951BFD6B21D7AB3B" box="[1369,1431,748,770]" country="Philippines" name="Cebu" pageId="39" pageNumber="574">Cebu</collectingRegion>
, 
<collectingCountry id="F30B761C75706C669371FC8D27F5AB19" box="[819,949,778,800]" name="Philippines" pageId="39" pageNumber="574">Philippines</collectingCountry>
<quantity id="4CE49B6975706C669386FC8D201FAB19" box="[964,1119,778,800]" metricMagnitude="0" metricUnit="m" metricValue="1.86415" pageId="39" pageNumber="574" unit="mm" value="1864.15">1864, 15 mm</quantity>
preserved length, leg. C. Semper (
<collectionCode id="ED0DAE4975706C6693DDFCAE27ADAB07" box="[927,1005,809,830]" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="39" pageNumber="574">ZMUC</collectionCode>
<collectionCode id="ED0DAE4975706C6693BCFCAE207EAB06" box="[1022,1086,809,831]" pageId="39" pageNumber="574">GAS-</collectionCode>
