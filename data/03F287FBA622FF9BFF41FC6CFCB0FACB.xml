<taxonomicName id="4C5B4D6EA622FF9BFF41FB9BFDEDFB83" authority="LeConte, 1884:22" authorityName="LeConte" authorityPageNumber="22" authorityYear="1884" box="[151,533,1074,1097]" class="Insecta" family="Cerambycidae" genus="Phymatodes" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="41" phylum="Arthropoda" rank="species" species="ater">
