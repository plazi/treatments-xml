<taxonomicName id="CCCF4D08E92F852BFE09E85E0C6EFEF4" box="[497,612,260,285]" class="Insecta" family="Ephydridae" kingdom="Animalia" order="Diptera" pageId="62" pageNumber="63" phylum="Arthropoda" rank="tribe" tribe="Ephydrini">Ephydrini</taxonomicName>
