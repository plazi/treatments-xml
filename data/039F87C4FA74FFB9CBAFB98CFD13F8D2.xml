<paragraph id="8B8936D2FA74FFB9CBAFB819FB40FEFC" blockId="17.[151,1436,258,403]" pageId="17" pageNumber="18">
<taxonomicName id="4C364D51FA74FFB9CBAFB819FEEEFEB2" box="[151,262,258,280]" class="Gastropoda" family="Drilliidae" genus="Agladrillia" kingdom="Animalia" order="Neogastropoda" pageId="17" pageNumber="18" phylum="Mollusca" rank="genus">
