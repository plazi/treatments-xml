<taxonomicName id="B71B701F25754DF588FF6DD503CA569E" class="Pinopsida" family="Taxodiaceae" genus="Cunninghamia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Cunninghamia" order="Pinales" pageId="0" pageNumber="115" phylum="Coniferophyta" rank="genus">
<emphasis id="BCA85554C558661407A60586B42D32E5" italics="true" pageId="0" pageNumber="115">Cunninghamia</emphasis>
