<taxonomicName id="E63CE859884FA6DD3F68D2D731D3B9C8" class="Dicotyledoneae" family="Fagaceae" genus="Quercus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Quercus semecarpifolia" order="Fagales" pageId="0" pageNumber="115" phylum="Angiospermae" rank="species" species="semecarpifolia">
<emphasis id="A469F62C6A7B7587201E6F8028F56551" italics="true" pageId="0" pageNumber="115">Quercus semecarpifolia</emphasis>
