<subSubSection id="C3526504FFF4E928EDC3FB3A997596EE" pageId="5" pageNumber="312" type="materials_examined">
<paragraph id="8BF7368FFFF4E928EDC3FB3A997596EE" blockId="5.[677,1209,1160,1210]" pageId="5" pageNumber="312">
