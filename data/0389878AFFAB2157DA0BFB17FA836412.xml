<paragraph id="8B9F369CFFAB2157DA0BFB68FA34676D" blockId="38.[826,1469,1153,1654]" box="[828,1435,1276,1302]" pageId="38" pageNumber="2820">
Basonym: 
<taxonomicName id="4C204D1FFFAB2157DA9CFB68FAE66771" authority="Puertas et al. 2014" authorityName="Puertas" authorityYear="2014" box="[939,1353,1278,1302]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="38" pageNumber="2820" phylum="Firmicutes" rank="species" species="sicerae">
