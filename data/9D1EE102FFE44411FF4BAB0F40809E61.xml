<emphasis id="27C38C06FFE44411FF1BABBE40CB9DA9" bold="true" box="[199,497,430,455]" pageId="9" pageNumber="70">Habitat and distribution.</emphasis>
<taxonomicName id="D2B72B97FFE44411FE2BABBF434E9DA8" box="[503,628,430,454]" class="Entognatha" family="Campodeidae" genus="Campodea" kingdom="Animalia" order="Diplura" pageId="9" pageNumber="70" phylum="Arthropoda" rank="species" species="pieltaini">
<emphasis id="27C38C06FFE44411FE2BABBF434E9DA8" box="[503,628,430,454]" italics="true" pageId="9" pageNumber="70">C. pieltaini</emphasis>
</taxonomicName>
