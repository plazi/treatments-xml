<taxonomicName id="4096F398F7E27130C41D9CF756322664" LSID="http://zoobank.org/546F9FE5-C3BA-469F-A96A-8FC2117C7634" authority="Z. Peng &amp; L. - Z. Li" class="Insecta" family="Staphylinidae" genus="Lobrathium" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lobrathium kedian" order="Coleoptera" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="kedian">Lobrathium kedian Z. Peng &amp; L.-Z. Li</taxonomicName>
<taxonomicNameLabel id="19313AD33A53B9E5CD42BE5D900AAD41" pageId="3" pageNumber="54">sp. n.</taxonomicNameLabel>
