<heading id="E26481ADFFA06B3AAE112144FC894BCD" box="[300,815,1141,1165]" centered="true" fontSize="10" level="2" pageId="20" pageNumber="501" reason="2">
<taxonomicName id="7E934D42FFA06B3AAE112144FD784BCD" authority="Lee and Patterson" authorityName="Lee and Patterson" authorityYear="2000" box="[300,734,1141,1165]" class="Euglenoidea" family="Peranemaceae" genus="Heteronema" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Peranemida" pageId="20" pageNumber="501" phylum="Euglenozoa" rank="species" species="larseni" status="n. sp.">
<emphasis id="8BE7EAD3FFA06B3AAE112144FDA34BCD" bold="true" box="[300,517,1141,1165]" italics="true" pageId="20" pageNumber="501">Heteronema larseni</emphasis>
Lee and Patterson
</taxonomicName>
, 
<taxonomicNameLabel id="90D457A8FFA06B3AADCD2147FC894BCD" box="[752,815,1142,1165]" pageId="20" pageNumber="501" rank="species">n. sp.</taxonomicNameLabel>
