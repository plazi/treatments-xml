<taxonomicName id="BEF6F76AFB95A3C65AFDB301CB65F37B" authorityName="Mateu" authorityYear="1974" class="Hexapoda" family="Carabidae" genus="Microlestes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="805" pageNumber="806" phylum="Arthropoda" rank="subSpecies" species="brevilobus" subSpecies="mexicanus">
