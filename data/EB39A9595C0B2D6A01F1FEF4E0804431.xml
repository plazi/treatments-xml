<subSubSection id="2B8A4BC45C052D6A01A1FC85E0804431" pageId="9" pageNumber="596" type="etymology">
<paragraph id="632F184F5C052D6A01A1FC85E0804431" blockId="9.[151,1437,151,861]" pageId="9" pageNumber="596">
<emphasis id="51E4C45D5C052D6A01A1FC85E0034455" bold="true" box="[199,334,799,825]" pageId="9" pageNumber="596">Etymology.</emphasis>
