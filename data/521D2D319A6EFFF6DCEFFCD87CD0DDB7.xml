<taxonomicName id="1DB4E7A49A6EFFF6DCE6FBB67936DD6E" authorityName="" authorityYear="1758" box="[158,317,1039,1063]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="537" phylum="Arthropoda" rank="order">Hymenoptera</taxonomicName>
