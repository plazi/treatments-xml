<taxonomicName id="4C704D766701FF8012CD9AEAB14CA8A1" authority="(Sousa-Lopes et al. 2016)" authorityName="Sousa-Lopes et al." authorityYear="2016" box="[671,1061,1713,1738]" class="Magnoliopsida" family="Fabaceae" genus="Mimosa" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fabales" pageId="7" pageNumber="158" phylum="Magnoliopsida" rank="genus">
