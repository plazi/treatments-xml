<taxonomicName id="16A64D12FF9ABF3DFEFEF8C5FE3BF843" authority="Qian &amp; Xiang &amp; Jiao &amp; Du, 2019" authorityName="Qian &amp; Xiang &amp; Jiao &amp; Du" authorityYear="2019" box="[321,492,1919,1943]" class="Insecta" family="Perlidae" genus="Agnetina" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="291" phylum="Arthropoda" rank="species" species="donans" status="sp. nov.">
<emphasis id="E3D2EA83FF9ABF3DFEFEF8C5FE3BF843" box="[321,492,1919,1943]" italics="true" pageId="2" pageNumber="291">
Agnetina 
<taxonomicName id="16A64D12FF9ABF3DFE1CF8C5FE3BF843" box="[419,492,1919,1943]" pageId="2" pageNumber="291" rank="species" species="donans" status="sp. nov.">donans</taxonomicName>
</emphasis>
</taxonomicName>
