<taxonomicName id="B9C4F19C14104060FE013242FD47F982" box="[479,734,1634,1657]" class="Insecta" family="Tenebrionidae" genus="Ascelosodis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="punctulata" status="sp. nov.">Ascelosodis punctulata</taxonomicName>
,
</emphasis>
<emphasis id="4CB0560D14104060FD323241FCDEF982" bold="true" box="[748,839,1633,1657]" pageId="7" pageNumber="8">
