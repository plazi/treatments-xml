<subSubSection id="E7A2B298FFEC78536FBE1ED7FB227F6E" box="[199,1259,1518,1543]" pageId="25" pageNumber="26" type="etymology">
<paragraph id="AF07E113FFEC78536FBE1ED7FB227F6E" blockId="25.[151,1436,798,1614]" box="[199,1259,1518,1543]" pageId="25" pageNumber="26">
<emphasis id="9DCC3D01FFEC78536FBE1ED7FE877F6F" bold="true" box="[199,334,1519,1543]" inLineHeading="true" pageId="25" pageNumber="26" reason="1">Etymology.</emphasis>
