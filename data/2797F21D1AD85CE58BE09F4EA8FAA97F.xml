<paragraph id="849C7AA4A05D4985CFA200CFD62436C0" pageId="0" pageNumber="67">
<taxonomicName id="E23C99C5B3F597A94A76A6B7F1F84877" authorityName="R. H. Shan" authorityYear="1941" baseAuthorityName="H. Wolff et Hand. - Mazz." class="Magnoliopsida" family="Apiaceae" genus="Physospermopsis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Physospermopsis alepidioides" order="Apiales" pageId="0" pageNumber="67" phylum="Tracheophyta" rank="species" species="alepidioides">
