<taxonomicName id="4C0F23F5F61FFFFAE3FBFDD9FEBAFDC3" box="[151,364,548,572]" class="Arachnida" family="Pachylaelapidae" genus="Pachylaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="248" phylum="Arthropoda" rank="subGenus" subGenus="Longipachylaelaps">
