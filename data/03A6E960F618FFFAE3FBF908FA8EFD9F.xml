<taxonomicName id="4C0F23F5F61FFFFAE75FFDFDFB11FDE8" authorityName="Masan" authorityYear="2007" box="[1075,1223,512,536]" class="Arachnida" family="Pachylaelapidae" genus="Pachylaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="5" pageNumber="248" phylum="Arthropoda" rank="species" species="vicarius" subGenus="Longipachylaelaps">
