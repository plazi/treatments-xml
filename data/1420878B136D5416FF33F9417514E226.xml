<treatmentCitationGroup id="BC9911B3136D5417FF33F88872B7E665" box="[151,1233,1801,1823]" pageId="28" pageNumber="29">
<taxonomicName id="5B894D1E136D5417FF33F8887783E665" authority="Li &amp; Wang 1995: 189" authorityName="Li &amp; Wang" authorityPageNumber="189" authorityYear="1995" box="[151,485,1801,1823]" class="Insecta" family="Cicadellidae" genus="Striatanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="genus">
<emphasis id="AEFDEA8F136D5417FF33F8887698E665" box="[151,254,1802,1823]" italics="true" pageId="28" pageNumber="29">Striatanus</emphasis>
<treatmentCitation id="1D28108C136D5417FEA0F88B7783E665" author="" box="[260,485,1801,1823]" page="189" pageId="28" pageNumber="29">
<bibRefCitation id="F8184B6C136D5417FEA0F88B77D4E665" box="[260,434,1801,1823]" pageId="28" pageNumber="29" refId="ref34831" refString="Li, Z. Z. &amp; Wang, L. M. (1995) Two new genera and three new species of Evacanthinae (Homoptera: Cicadellidae) from China. Entomotaxanomia, 17 (93), 189 - 196. [in Chinese with English Summary]" type="journal article">Li &amp; Wang 1995</bibRefCitation>
