<taxonomicName id="4C054D47B84EFFDA00A6F89F20CEDB33" authorityName="Krammer" authorityYear="2003" baseAuthorityName="Pantocsek" box="[204,461,1823,1846]" class="Bacillariophyceae" family="Cymbellaceae" genus="Cymbopleura" higherTaxonomySource="GBIF" kingdom="Chromista" order="Cymbellales" pageId="17" pageNumber="18" phylum="Bacillariophyta" rank="species" species="budayana">
