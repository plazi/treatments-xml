<taxonomicName id="4C054D47B84CFFD80095FED823E1DD70" authority="Kutzing (1844)" authorityName="Kutzing" authorityYear="1844" box="[255,738,344,373]" class="Bacillariophyceae" family="Gomphonemataceae" higherTaxonomySource="GBIF" kingdom="Chromista" order="Cymbellales" pageId="19" pageNumber="20" phylum="Bacillariophyta" rank="family">
<emphasis id="B971EAD6B84CFFD80095FED82315DD70" bold="true" box="[255,534,344,373]" pageId="19" pageNumber="20">Gomphonemataceae</emphasis>
