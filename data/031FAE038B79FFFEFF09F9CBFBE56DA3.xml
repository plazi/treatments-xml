<collectingCountry id="F3A15F858B79FFFEFB89F8E4FB2E6D37" box="[1047,1184,1893,1919]" name="Madagascar" pageId="13" pageNumber="14">Madagascar</collectingCountry>
, 
<collectingCountry id="F3A15F858B79FFFEFB33F8E4FACA6D37" box="[1197,1348,1893,1919]" name="Mozambique" pageId="13" pageNumber="14">Mozambique</collectingCountry>
, 
