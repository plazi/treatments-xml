<taxonomicName id="F5F24D1BFFBFA76D1775FC0D2FBC919F" authorityName="Grichanov" authorityYear="2003" box="[1064,1179,936,963]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="55" phylum="Arthropoda" rank="species" species="ambila">
<emphasis id="0086EA8AFFBFA76D1775FC0D2FBC919F" box="[1064,1179,936,963]" italics="true" pageId="8" pageNumber="55">A. ambila</emphasis>
</taxonomicName>
, 
<taxonomicName id="F5F24D1BFFBFA76D17F1FC0D2E48919F" authorityName="Grichanov" authorityYear="1999" box="[1196,1391,936,963]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="55" phylum="Arthropoda" rank="species" species="ankaratrensis">
