<taxonomicName id="F5F24D1BFFBFA76D13B3FC21291291C2" baseAuthorityName="Grimshaw" baseAuthorityYear="1901" box="[238,565,901,927]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="55" phylum="Arthropoda" rank="species" species="pallidicornis">
