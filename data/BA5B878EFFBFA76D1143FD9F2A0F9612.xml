<taxonomicName id="F5F24D1BFFBFA76D104FFC0D28EC919F" baseAuthorityName="Grimshaw" baseAuthorityYear="1901" box="[786,971,936,963]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="55" phylum="Arthropoda" rank="species" species="pallidicornis">
