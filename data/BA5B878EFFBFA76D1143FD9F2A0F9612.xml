<heading id="690581F4FFBFA76D1143FD9F2F32900B" box="[542,1045,571,598]" centered="true" fontSize="11" level="2" pageId="8" pageNumber="55" reason="2">
<taxonomicName id="F5F24D1BFFBFA76D1143FD9F2848900B" baseAuthorityName="Grimshaw" baseAuthorityYear="1901" box="[542,879,571,598]" class="Insecta" family="Dolichopodidae" genus="Amblypsilopus" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="55" phylum="Arthropoda" rank="species" species="pallidicornis">
<emphasis id="0086EA8AFFBFA76D1143FD9F2848900B" bold="true" box="[542,879,571,598]" italics="true" pageId="8" pageNumber="55">Amblypsilopus pallidicornis</emphasis>
