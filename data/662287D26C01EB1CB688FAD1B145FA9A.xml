<quantity id="29739B216C00EB1BB5ADFC9AB6BAFC90" box="[946,1004,849,872]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="3" pageNumber="66" unit="mm" value="1.0">1mm</quantity>
); 
<number id="584324E36C00EB1BB21FFC9AB146FC90" box="[1024,1040,849,872]" pageId="3" pageNumber="66" value="2.0">2</number>
