12. 
<taxonomicName id="95CDB20A59267F960AFE68328FEA5B6D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:28546" class="Insecta" family="Formicidae" genus="Cyphomyrmex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Cyphomyrmex salvini Forel" order="Hymenoptera" pageNumber="190" phylum="Arthropoda" rank="species" species="salvini">Cyphomyrmex salvini Forel</taxonomicName>
