<quantity id="4CE46A93FFB3FFB8FEE481A6FD953995" box="[380,519,1681,1705]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" metricValueMax="2.1" metricValueMin="1.9" pageId="15" pageNumber="183" unit="mm" value="2.0" valueMax="2.1" valueMin="1.9">1.9–2.1 mm</quantity>
