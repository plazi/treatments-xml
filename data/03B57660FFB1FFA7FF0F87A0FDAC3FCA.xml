<taxonomicName id="4C1CBCF5FFB2FFB9FD8F808EFDE238F3" authority="Hansson &amp; Kenyon &amp; Benrey, 2019" authorityName="Hansson &amp; Kenyon &amp; Benrey" authorityYear="2019" box="[535,624,1977,1998]" class="Insecta" family="Eulophidae" genus="Horismenus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="182" phylum="Arthropoda" rank="species" species="stator" status="sp. nov.">
<emphasis id="B9681B64FFB2FFB9FD8F808EFDE238F3" box="[535,624,1977,1998]" italics="true" pageId="14" pageNumber="182">H. stator</emphasis>
</taxonomicName>
<emphasis id="B9681B64FFB2FFB9FDEE808FFD5438F0" bold="true" box="[630,710,1976,1997]" pageId="14" pageNumber="182">
<taxonomicNameLabel id="A25BA61FFFB2FFB9FDEE808FFD5438F0" box="[630,710,1976,1997]" pageId="14" pageNumber="182" rank="species">sp. nov.</taxonomicNameLabel>
