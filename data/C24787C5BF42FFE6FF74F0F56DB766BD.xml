<taxonomicName id="8DEE4D50BF42FFE6FDA6F39A6A6B666A" box="[581,789,1016,1041]" class="Magnoliopsida" family="Fabaceae" genus="Pongamia" kingdom="Plantae" order="Fabales" pageId="17" pageNumber="168" phylum="Tracheophyta" rank="species" species="pinnata">
