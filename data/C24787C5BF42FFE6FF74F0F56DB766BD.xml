<subSubSection id="02F46558BF42FFE6FF24F3956D646669" box="[199,1050,1015,1042]" pageId="17" pageNumber="168" type="biology_ecology">
<paragraph id="4A5136D3BF42FFE6FF24F3956D646669" blockId="17.[151,1437,259,1222]" box="[199,1050,1015,1042]" pageId="17" pageNumber="168">
<emphasis id="789AEAC1BF42FFE6FF24F395685F666A" bold="true" box="[199,289,1015,1041]" pageId="17" pageNumber="168">Biology</emphasis>
