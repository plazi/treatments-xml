<taxonomicName id="4C6BDEE9FFB7FF962DC2FC0FFBB9FB83" authorityName="Leonardi" authorityYear="1897" box="[1015,1144,995,1019]" class="Insecta" family="Diaspididae" genus="Odonaspis" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="genus">
<emphasis id="B91F7978FFB7FF962DC2FC0FFBB9FB83" box="[1015,1144,995,1019]" italics="true" pageId="5" pageNumber="4">Odonaspis</emphasis>
</taxonomicName>
