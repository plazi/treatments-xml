<taxonomicName id="4C6BDEE9FFB7FF962CCFFBE8FBE0FC63" authority="Balachowsky" authorityName="Balachowsky" authorityYear="1957" box="[762,1057,1027,1051]" class="Insecta" family="Diaspididae" genus="Odonaspis" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="4" phylum="Arthropoda" rank="species" species="benardi">
