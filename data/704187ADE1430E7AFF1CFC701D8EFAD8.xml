body length 6.5–8.0 mm, wing length 
<quantity id="3F109B5EE1430E7CFBE8FA8B18CFFACC" box="[1123,1273,1292,1316]" metricMagnitude="-2" metricUnit="m" metricValue="1.0399999999999998" metricValueMax="1.1099999999999999" metricValueMin="0.97" pageId="19" pageNumber="20" unit="mm" value="10.399999999999999" valueMax="11.1" valueMin="9.7">9.7–11.1 mm</quantity>
