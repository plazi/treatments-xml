<collectingRegion id="2A5EF82C9059FF91E22DA18E1755FA08" box="[539,604,1444,1469]" country="Maldives" name="Dhekunu" pageId="14" pageNumber="589">South</collectingRegion>
and East 
<collectingCountry id="908D765E9059FF91E2FBA18E1619FA08" box="[717,784,1444,1469]" name="China" pageId="14" pageNumber="589">China</collectingCountry>
seas, 
<collectingCountry id="908D765E9059FF91E360A18F1690FA09" box="[854,921,1445,1468]" name="Japan" pageId="14" pageNumber="589">Japan</collectingCountry>
, 
<collectingCountry id="908D765E9059FF91E392A18E112BFA08" box="[932,1058,1444,1469]" name="Philippines" pageId="14" pageNumber="589">Philippines</collectingCountry>
and 
<collectingCountry id="908D765E9059FF91E46DA18E11C5FA08" box="[1115,1228,1444,1469]" name="Indonesia" pageId="14" pageNumber="589">Indonesia</collectingCountry>
; at depths of 
<quantity id="2F629B2B9059FF91E552A18E15DEFA55" metricMagnitude="2" metricUnit="m" metricValue="2.92" metricValueMax="4.0" metricValueMin="1.8399999999999999" pageId="14" pageNumber="589" unit="m" value="292.0" valueMax="400.0" valueMin="184.0">184– 400m</quantity>
(
<bibRefCitation id="8C0B4B3F9059FF91E0D3A1ED145BFA55" author="Li" box="[229,338,1479,1504]" pageId="14" pageNumber="589" refString="Li, X. (2006 b) Report on some pandalid shrimps from the East China Sea (Decapoda, Caridea). Crustaceana, 79, 1281 - 1296. https: // doi. org / 10.1163 / 156854006779277303" type="journal article" year="2006" yearSuffix="b">Li 2006b</bibRefCitation>
).
</paragraph>
