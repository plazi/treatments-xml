<taxonomicName id="DFA34D69FFF7FFD4FE12F9403905AEB8" authority="Foldi. For" authorityName="Foldi. For" box="[434,808,1725,1749]" class="Insecta" family="Margarodidae" genus="Stigmacoccus" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="garmilleri">
