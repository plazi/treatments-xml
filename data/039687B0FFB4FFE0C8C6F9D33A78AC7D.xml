<taxonomicName id="4C3F4D25FFB5FFE0CC08FE1E3CA8AFB5" box="[1113,1249,440,464]" class="Insecta" family="Elateridae" genus="Dima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="isabellae">
<emphasis id="B94BEAB4FFB5FFE0CC08FE1E3C24AFB5" box="[1113,1133,441,464]" italics="true" pageId="19" pageNumber="20">D</emphasis>
