<taxonomicName id="4C3F4D25FFB5FFE0CAFBFE7A3B59AF91" box="[682,784,476,500]" class="Insecta" family="Elateridae" genus="Dima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="arndti">
