<taxonomicName id="4C1C4D18655C8766799F0D3D9F0DF85C" box="[977,1113,1919,1940]" class="Magnoliopsida" family="Myrtaceae" genus="Calyptranthes" kingdom="Plantae" order="Myrtales" pageId="2" pageNumber="137" phylum="Tracheophyta" rank="genus">
