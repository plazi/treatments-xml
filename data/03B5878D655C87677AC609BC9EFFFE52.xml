<taxonomicName id="4C1C4D18655C876679020D3D98C4F85C" box="[844,912,1919,1940]" class="Magnoliopsida" family="Myrtaceae" genus="Myrcia" kingdom="Plantae" order="Myrtales" pageId="2" pageNumber="137" phylum="Tracheophyta" rank="genus">
