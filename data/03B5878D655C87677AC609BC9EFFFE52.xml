<taxonomicName id="4C1C4D18655C87667F4C0D3D9EF6F85C" authorityName="M.F.Santos &amp; E.Lucas" authorityYear="2016" box="[1282,1442,1919,1940]" class="Magnoliopsida" family="Myrtaceae" genus="Sympodiomyrcia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Myrtales" pageId="2" pageNumber="137" phylum="Tracheophyta" rank="genus">
