of 
<taxonomicName id="AFD9A596124AFFC7FD29F96EFBC1F97D" authority="Graff, 1875" authorityName="Graff" authorityYear="1875" box="[746,1124,1695,1720]" class="Gastropoda" family="Eulimidae" genus="Stylina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Littorinimorpha" pageId="48" pageNumber="49" phylum="Mollusca" rank="species" species="comatulicola">
