<typeStatus id="B70D2DB7DA770746FE631BBB844AF93B" box="[498,547,1685,1706]" pageId="3" pageNumber="252">type</typeStatus>
genus by monotypy 
<taxonomicName id="AFB6E896DA770746FF331B9A846EF959" authority="Chamberlin &amp; Ivie. See" authorityName="Chamberlin &amp; Ivie. See" box="[162,519,1715,1737]" class="Arachnida" family="Pimoidae" genus="Pimoa" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="252" phylum="Arthropoda" rank="genus">
