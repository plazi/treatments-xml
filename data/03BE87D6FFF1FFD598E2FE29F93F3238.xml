<paragraph id="8BA836C0FFF1FFD59ACEFE29F93F3238" blockId="96.[151,1437,474,753]" pageId="96" pageNumber="97">
<emphasis id="B963EAD2FFF1FFD59ACEFE29F9FA31F1" bold="true" box="[691,955,474,501]" pageId="96" pageNumber="97">Western Mosquitofish</emphasis>
. To 
<quantity id="4CEF9B25FFF1FFD59B92FE29FE6631F0" box="[1007,1063,475,501]" metricMagnitude="-2" metricUnit="m" metricValue="6.0" pageId="96" pageNumber="97" unit="cm" value="6.0">6 cm</quantity>
(
<quantity id="4CEF9B25FFF1FFD59C4AFE29FE3B31F3" box="[1079,1146,475,502]" metricMagnitude="-2" metricUnit="m" metricValue="6.096" pageId="96" pageNumber="97" unit="in" value="2.4">2.4 in</quantity>
