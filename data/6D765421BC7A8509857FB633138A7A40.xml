<taxonomicName id="22DF9EB4BC7A850985F6B633107E7904" box="[385,590,283,309]" class="Arachnida" family="Rhagidiidae" kingdom="Animalia" order="Prostigmata" pageId="41" pageNumber="52" phylum="Arthropoda" rank="family">RHAGIDIIDAE</taxonomicName>
