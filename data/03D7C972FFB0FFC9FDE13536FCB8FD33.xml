<quantity id="4C86D581FFB1FFC9FE663165FD8CFDA7" box="[482,589,592,619]" metricMagnitude="-3" metricUnit="m" metricValue="2.15" pageId="49" pageNumber="50" unit="mm" value="2.15">2.15 mm</quantity>
