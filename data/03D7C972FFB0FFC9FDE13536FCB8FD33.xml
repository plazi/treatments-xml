<quantity id="4C86D581FFB1FFC9FEAD3165FE55FDA7" box="[297,404,592,619]" metricMagnitude="-3" metricUnit="m" metricValue="1.05" pageId="49" pageNumber="50" unit="mm" value="1.05">1.05 mm</quantity>
