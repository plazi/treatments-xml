<quantity id="4C86D581FFB1FFC9FE513119FD83FD8B" box="[469,578,556,583]" metricMagnitude="-3" metricUnit="m" metricValue="1.65" pageId="49" pageNumber="50" unit="mm" value="1.65">1.65 mm</quantity>
