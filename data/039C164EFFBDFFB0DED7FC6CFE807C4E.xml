<paragraph id="8B8AA758FFBDFFB1DED7FC6CFB097B8E" blockId="60.[741,1171,379,402]" box="[741,1171,379,402]" pageId="60" pageNumber="61">
<heading id="D0C21034FFBDFFB1DED7FC6CFB097B8E" box="[741,1171,379,402]" centered="true" fontSize="9" level="2" pageId="60" pageNumber="61" reason="2">
<taxonomicName id="4C35DCDBFFBDFFB1DED7FC6CFC617B8D" box="[741,1019,379,402]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="zairensis" status="new species">
<emphasis id="B9417B4AFFBDFFB1DED7FC6CFC617B8D" bold="true" box="[741,1019,379,402]" italics="true" pageId="60" pageNumber="61">Paramblynotus zairensis</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFBDFFB1D83EFC6AFB097B8E" box="[1036,1171,380,402]" pageId="60" pageNumber="61" rank="species">new species</taxonomicNameLabel>
