<taxonomicName id="4C714D228105FFA9FF04FC4EFD86FBCA" authority="Rchb." authorityName="Rchb." box="[136,565,1022,1049]" class="Liliopsida" family="Orchidaceae" genus="Bulbophyllum" kingdom="Plantae" order="Asparagales" pageId="42" pageNumber="43" phylum="Tracheophyta" rank="species" species="psittacoglossum">
