<taxonomicName id="4C6EDFA6FFF02F77E8ED90C4FCA6A38F" authorityName="Kloet &amp; Hincks" authorityYear="1944" box="[726,894,1978,2002]" class="Insecta" family="Hydroptilidae" genus="Tricholeiochiton" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="501" phylum="Arthropoda" rank="genus">
