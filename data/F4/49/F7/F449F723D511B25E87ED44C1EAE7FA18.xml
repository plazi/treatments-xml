<taxonomicName authority="(Gory and Percheron)" authorityName="" baseAuthorityName="Gory and Percheron" baseAuthorityYear="1833" box="[221,592,463,484]" class="Insecta" family="Cetoniidae" genus="Euphoria" kingdom="Animalia" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="westermanni">
