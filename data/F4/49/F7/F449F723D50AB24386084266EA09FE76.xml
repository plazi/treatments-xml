<taxonomicName authority="Fall" authorityName="Fall" authorityYear="1905" box="[958,1117,1518,1540]" class="Insecta" family="Cetoniidae" genus="Euphoria" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="limbalis">
