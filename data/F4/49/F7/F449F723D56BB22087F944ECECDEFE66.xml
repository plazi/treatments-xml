, 
<collectingCountry box="[173,245,1708,1729]" name="Mexico" pageId="74" pageNumber="75">Mexico</collectingCountry>
(
<figureCitation box="[260,338,1708,1729]" captionStart="Fig" captionStartId="158.[197,229,1686,1705]" captionTargetBox="[259,1148,186,1662]" captionTargetId="figure-0@158.[174,1192,183,1662]" captionTargetPageId="158" captionText="Fig. 39. Euphoria casselberryi. a) Habitus of male, b) Head, c) Parameres, d) Female, e) Temporal distribution," figureDoi="http://doi.org/10.5281/zenodo.4907330" httpUri="https://zenodo.org/record/4907330/files/figure.png" pageId="74" pageNumber="75">Fig. 39f</figureCitation>
