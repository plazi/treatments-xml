, 
<collectorName box="[453,526,991,1012]" pageId="29" pageNumber="30">La Paz</collectorName>
, and 
<collectingRegion country="Bolivia" name="Santa Cruz" pageId="29" pageNumber="30">Santa Cruz</collectingRegion>
in 
<collectingCountry box="[232,302,1018,1039]" name="Bolivia" pageId="29" pageNumber="30">Bolivia</collectingCountry>
