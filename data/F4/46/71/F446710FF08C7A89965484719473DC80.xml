<taxonomicName authority="Latreille, 1806" authorityName="Latreille" authorityYear="1806" class="Insecta" family="Chrysididae" genus="Hedychrum" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Hedychrum bidentulum" order="Hymenoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="bidentulum">
<pageBreakToken pageId="63" pageNumber="64" start="start">Hedychrum</pageBreakToken>
