<quantity box="[952,1089,1163,1187]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="8.0" metricValueMin="7.0" pageId="3" pageNumber="190" unit="mm" value="0.75" valueMax="0.8" valueMin="0.7">0.7–0.8 mm</quantity>
