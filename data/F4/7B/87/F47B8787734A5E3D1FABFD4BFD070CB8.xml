<number box="[435,508,1518,1543]" pageId="9" pageNumber="310" value="28.0" valueMax="30.0" valueMin="26.0">26–30</number>
. S 
<number box="[537,548,1519,1543]" pageId="9" pageNumber="310" value="1.0">1</number>
