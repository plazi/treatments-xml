<number box="[1058,1086,1482,1507]" pageId="9" pageNumber="310" value="66.0">66</number>
(
<number box="[1102,1134,1483,1507]" pageId="9" pageNumber="310" value="34.0">34</number>
), 
<number box="[1154,1182,1482,1507]" pageId="9" pageNumber="310" value="55.0">55</number>
(
<number box="[1199,1230,1483,1507]" pageId="9" pageNumber="310" value="28.0">28</number>
), 
<number box="[1251,1279,1482,1507]" pageId="9" pageNumber="310" value="50.0">50</number>
(
<number box="[1295,1326,1482,1507]" pageId="9" pageNumber="310" value="25.0">25</number>
), 
<number box="[1347,1375,1483,1507]" pageId="9" pageNumber="310" value="40.0">40</number>
