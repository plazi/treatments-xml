<taxonomicName authority="Miers 1885: 586" authorityName="Miers" authorityPageNumber="586" authorityYear="1885" box="[151,711,1480,1507]" class="Malacostraca" family="Goneplacidae" genus="Bathyplax" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="49" phylum="Arthropoda" rank="subSpecies" species="typhlus" subSpecies="oculiferus">
<emphasis bold="true" box="[151,499,1481,1507]" italics="true" pageId="4" pageNumber="49">Bathyplax typhlus oculiferus</emphasis>
Miers 
<number box="[587,650,1480,1506]" pageId="4" pageNumber="49" value="1885.0">1885</number>
: 
<number box="[663,711,1480,1506]" pageId="4" pageNumber="49" value="586.0">586</number>
</taxonomicName>
; 
<number box="[724,787,1480,1506]" pageId="4" pageNumber="49" value="1886.0">1886</number>
: 
<number box="[799,844,1480,1506]" pageId="4" pageNumber="49" value="230.0">230</number>
, pl. XX, fig. 
<number box="[1001,1017,1480,1506]" pageId="4" pageNumber="49" value="3.0">3</number>
