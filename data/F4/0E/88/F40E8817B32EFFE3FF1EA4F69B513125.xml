<taxonomicName authority="Miers 1885: 586" authorityName="Miers" authorityPageNumber="586" authorityYear="1885" box="[151,643,474,500]" class="Insecta" family="Brentidae" genus="Stenorhynchus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="49" phylum="Arthropoda" rank="species" species="spinifer">
<emphasis bold="true" box="[151,432,474,500]" italics="true" pageId="4" pageNumber="49">Stenorhynchus spinifer</emphasis>
Miers 
<number box="[520,583,474,500]" pageId="4" pageNumber="49" value="1885.0">1885</number>
: 
<number box="[595,643,474,500]" pageId="4" pageNumber="49" value="586.0">586</number>
</taxonomicName>
; 
<number box="[656,719,474,500]" pageId="4" pageNumber="49" value="1886.0">1886</number>
: 
<number box="[732,748,474,500]" pageId="4" pageNumber="49" value="7.0">7</number>
, pl. I, fig. 
<number box="[874,890,474,500]" pageId="4" pageNumber="49" value="2.0">2</number>
.
