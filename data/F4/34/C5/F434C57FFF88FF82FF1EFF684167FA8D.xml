: 
<taxonomicName authority="BERNHAUER" authorityName="BERNHAUER" box="[878,1084,998,1018]" class="Insecta" family="Staphylinidae" genus="Leptusa" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="432" phylum="Arthropoda" rank="species" species="merkli">
