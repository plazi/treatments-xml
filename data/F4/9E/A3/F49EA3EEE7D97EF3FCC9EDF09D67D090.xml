<taxonomicName authority="sp. 1" class="Polychaeta" family="Sphaerodoridae" genus="Sphaerephesia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphaerephesia" order="Phyllodocida" pageId="59" pageNumber="60" phylum="Annelida" rank="genus">
