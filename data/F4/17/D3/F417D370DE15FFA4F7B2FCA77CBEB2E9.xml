, anterior hamuli; 
<emphasis box="[973,1014,1916,1938]" italics="true" pageId="2" pageNumber="597">Alm</emphasis>
, anterior lamina; 
<emphasis box="[1192,1212,1916,1938]" italics="true" pageId="2" pageNumber="597">dt</emphasis>
, denticles; 
<emphasis box="[1327,1341,1916,1938]" italics="true" pageId="2" pageNumber="597">fl</emphasis>
