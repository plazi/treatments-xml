<taxonomicName authority="(Sasa)" authorityName="Saether" authorityYear="2006" baseAuthorityName="Saether" baseAuthorityYear="2006" box="[848,1071,1656,1679]" class="Insecta" family="Chironomidae" genus="Pseudosmittia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="34" phylum="Arthropoda" rank="species" species="jintuoctava">
