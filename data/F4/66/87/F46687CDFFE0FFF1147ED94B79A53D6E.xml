<quantity box="[383,460,324,350]" metricMagnitude="1" metricUnit="m" metricValue="6.0" pageId="14" pageNumber="35" unit="m" value="60.0">60 m</quantity>
and 
<quantity box="[517,593,324,350]" metricMagnitude="1" metricUnit="m" metricValue="2.3" pageId="14" pageNumber="35" unit="m" value="23.0">23 m</quantity>
