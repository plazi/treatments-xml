<collectingCountry box="[359,421,1163,1189]" name="India" pageId="11" pageNumber="202">India</collectingCountry>
: 
<collectingRegion box="[432,573,1163,1189]" country="India" name="Uttarakhand" pageId="11" pageNumber="202">Uttarakhand</collectingRegion>
, 
<collectingRegion box="[584,727,1163,1189]" country="India" name="Maharashtra" pageId="11" pageNumber="202">Maharashtra</collectingRegion>
, 
<collectingRegion box="[738,853,1163,1189]" country="India" name="Karnataka" pageId="11" pageNumber="202">Karnataka</collectingRegion>
(
<figureCitation box="[868,953,1163,1190]" captionStart="FIGURE 36" captionStartId="13.[151,250,1821,1845]" captionTargetBox="[151,1436,597,1798]" captionTargetId="figure-174@13.[151,1436,597,1798]" captionTargetPageId="13" captionText="FIGURE 36. Distribution map of the Indian subcontinent species of Odontomutilla Ashmead." figureDoi="http://doi.org/10.5281/zenodo.4401441" httpUri="https://zenodo.org/record/4401441/files/figure.png" pageId="11" pageNumber="202">Fig. 36</figureCitation>
