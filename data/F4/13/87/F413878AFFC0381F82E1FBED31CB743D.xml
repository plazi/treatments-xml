<quantity box="[981,1103,1771,1798]" metricMagnitude="-2" metricUnit="m" metricValue="2.35" metricValueMax="2.5" metricValueMin="2.2" pageId="12" pageNumber="13" unit="mm" value="23.5" valueMax="25.0" valueMin="22.0">22–25 mm</quantity>
, 
<specimenCount box="[1114,1135,1772,1798]" pageId="12" pageNumber="13" type="female">♀</specimenCount>
