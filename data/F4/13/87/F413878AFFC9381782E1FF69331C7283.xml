<taxonomicName authorityName="Hubner" authorityYear="1823" box="[1136,1257,1407,1433]" class="Insecta" family="Geometridae" genus="Eurranthis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
