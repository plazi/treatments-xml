<taxonomicName authorityName="Hubner" authorityYear="1823" box="[151,272,1479,1505]" class="Insecta" family="Geometridae" genus="Eurranthis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="genus">
