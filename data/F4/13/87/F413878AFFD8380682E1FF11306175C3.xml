<specimenCount box="[452,473,431,457]" pageId="20" pageNumber="21" type="male">♂</specimenCount>
<quantity box="[482,607,431,457]" metricMagnitude="-2" metricUnit="m" metricValue="3.5" metricValueMax="3.6" metricValueMin="3.4" pageId="20" pageNumber="21" unit="mm" value="35.0" valueMax="36.0" valueMin="34.0">34–36 mm</quantity>
