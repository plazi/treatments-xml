<paragraph blockId="11.[151,632,151,178]" box="[151,632,151,178]" pageId="11" pageNumber="406">
<heading bold="true" box="[151,632,151,178]" fontSize="11" level="1" pageId="11" pageNumber="406" reason="1">
<emphasis bold="true" box="[151,632,151,178]" pageId="11" pageNumber="406">
Genus 
<taxonomicName authorityName="Schweitzer &amp; Feldmann" authorityYear="2009" box="[238,401,151,178]" class="Malacostraca" family="Bucculentidae" genus="Bucculentum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="11" pageNumber="406" phylum="Arthropoda" rank="genus">
