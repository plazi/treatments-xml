<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:179000" authority="Forel" authorityName="Forel" class="Hexapoda" family="Formicidae" genus="Euponera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Euponera (Mesoponera) caffraria Eemry" order="Hymenoptera" pageNumber="190" phylum="Arthropoda" rank="species" species="caffraria" subGenus="Mesoponera">E. (Mesoponera) caffraria Sm.</taxonomicName>
