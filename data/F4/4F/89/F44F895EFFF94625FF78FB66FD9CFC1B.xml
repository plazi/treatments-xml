<quantity box="[425,562,929,953]" metricMagnitude="2" metricUnit="m" metricValue="9.49" metricValueMax="10.5" metricValueMin="8.48" pageId="31" pageNumber="64" unit="m" value="949.0" valueMax="1050.0" valueMin="848.0">848–1050 m</quantity>
.
