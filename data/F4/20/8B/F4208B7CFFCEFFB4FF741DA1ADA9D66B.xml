, 
<number box="[578,631,1189,1213]" pageId="1" pageNumber="45" value="0.45">0.45</number>
); II 
<number box="[681,730,1189,1213]" pageId="1" pageNumber="45" value="2.76">2.76</number>
(
<number box="[746,799,1189,1213]" pageId="1" pageNumber="45" value="0.92">0.92</number>
, 
<number box="[812,864,1189,1213]" pageId="1" pageNumber="45" value="0.15">0.15</number>
, 
<number box="[877,929,1189,1213]" pageId="1" pageNumber="45" value="0.72">0.72</number>
