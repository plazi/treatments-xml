<number box="[903,962,1004,1029]" pageId="10" pageNumber="67" value="2450.0">2450</number>
, 
<date box="[973,1085,1004,1029]" pageId="10" pageNumber="67" value="1973-06-13">
<number box="[973,1024,1004,1029]" pageId="10" pageNumber="67" value="13.6">13.6</number>
. 
<number box="[1027,1085,1004,1029]" pageId="10" pageNumber="67" value="1973.0">1973</number>
</date>
(
<collectionCode box="[1100,1174,1004,1028]" pageId="10" pageNumber="67">SBCP</collectionCode>
).
</paragraph>
<paragraph blockId="10.[151,1437,895,2037]" box="[199,1242,1039,1064]" pageId="10" pageNumber="67">
