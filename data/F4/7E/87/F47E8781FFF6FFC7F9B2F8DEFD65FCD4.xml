<collectionCode LSID="urn:lsid:biocol.org:col:34613" box="[580,660,836,860]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="4" pageNumber="62">ZFMK</collectionCode>
