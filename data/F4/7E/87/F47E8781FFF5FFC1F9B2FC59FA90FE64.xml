<number box="[1212,1285,1142,1166]" pageId="1" pageNumber="59" value="16.0" valueMax="18.0" valueMin="14.0">14–18</number>
; scales along lateral fold 
<number box="[283,356,1178,1202]" pageId="1" pageNumber="59" value="89.0" valueMax="97.0" valueMin="81.0">81–97</number>
