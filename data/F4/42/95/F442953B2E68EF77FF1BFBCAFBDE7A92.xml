<taxonomicName baseAuthorityName="Lutken" baseAuthorityYear="1898" box="[987,1119,1091,1115]" class="Actinopterygii" family="Alepocephalidae" genus="Bajacalifornia" kingdom="Animalia" order="Osmeriformes" pageId="21" pageNumber="22" phylum="Chordata" rank="species" species="megalops">
<emphasis box="[987,1119,1091,1115]" italics="true" pageId="21" pageNumber="22">B. megalops</emphasis>
