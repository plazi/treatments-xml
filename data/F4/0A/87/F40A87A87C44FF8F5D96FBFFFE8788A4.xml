<taxonomicName box="[281,379,1123,1147]" class="Magnoliopsida" family="Solanaceae" genus="Solanum" kingdom="Plantae" order="Solanales" pageId="14" pageNumber="40" phylum="Tracheophyta" rank="genus">
<emphasis box="[281,379,1123,1147]" italics="true" pageId="14" pageNumber="40">Solanum</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[432,564,1122,1146]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="14" pageNumber="40" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
