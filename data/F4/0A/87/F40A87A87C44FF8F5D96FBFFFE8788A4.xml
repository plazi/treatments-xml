<taxonomicName box="[687,895,1950,1974]" class="Insecta" family="Triozidae" genus="Bactericera" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="40" phylum="Arthropoda" rank="species" species="solani">
<emphasis box="[687,895,1950,1974]" italics="true" pageId="14" pageNumber="40">Bactericera solani</emphasis>
</taxonomicName>
to 
<taxonomicName box="[930,1046,1952,1975]" class="Insecta" family="Triozidae" genus="Leuronota" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="40" phylum="Arthropoda" rank="genus">
<emphasis box="[930,1046,1952,1975]" italics="true" pageId="14" pageNumber="40">Leuronota</emphasis>
</taxonomicName>
as 
<taxonomicName authority="Rubsaamen" authorityName="Rubsaamen" box="[1082,1432,1950,1975]" class="Insecta" family="Triozidae" genus="Leuronota" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="40" phylum="Arthropoda" rank="species" species="solani" status="comb. nov.">
<emphasis box="[1082,1272,1950,1975]" italics="true" pageId="14" pageNumber="40">Leuronota solani</emphasis>
(Rübsaamen)
</taxonomicName>
, 
<taxonomicNameLabel box="[199,328,1987,2011]" pageId="14" pageNumber="40">
<emphasis bold="true" box="[199,328,1987,2011]" pageId="14" pageNumber="40">comb. nov.</emphasis>
</taxonomicNameLabel>
