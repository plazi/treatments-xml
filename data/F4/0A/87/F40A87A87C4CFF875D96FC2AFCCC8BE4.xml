<number box="[1007,1065,1010,1034]" pageId="6" pageNumber="32" value="1942.0">1942</number>
</bibRefCitation>
, as 
<taxonomicName box="[1118,1338,1011,1035]" class="Insecta" family="Aphalaridae" genus="Rhinocola" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="32" phylum="Arthropoda" rank="species" species="succinla">
