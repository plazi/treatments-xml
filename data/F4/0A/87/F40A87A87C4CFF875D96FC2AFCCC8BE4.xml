<taxonomicName box="[199,436,1046,1070]" class="Insecta" family="Aphalaridae" genus="Agonoscena" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="32" phylum="Arthropoda" rank="species" species="succinta">
<emphasis box="[199,436,1046,1070]" italics="true" pageId="6" pageNumber="32">Agonoscena succinta</emphasis>
</taxonomicName>
[sic]), SP (
<bibRefCitation author="Burckhardt, D. &amp; Lauterer, P." box="[561,887,1046,1070]" firstAuthor="Burckhardt" journalOrPublisher="Journal of Natural History" pageId="6" pageNumber="32" pagination="643 - 712" part="23" refId="ref10062" refString="Burckhardt, D. &amp; Lauterer, P. (1989) Systematics and biology of the Rhinocolinae (Homoptera: Psylloidea). Journal of Natural History, 23, 643 - 712." title="Systematics and biology of the Rhinocolinae (Homoptera: Psylloidea)" type="journal article" year="1989">
Burckhardt &amp; Lauterer 
<number box="[827,887,1046,1070]" pageId="6" pageNumber="32" value="1989.0">1989</number>
</bibRefCitation>
), MG (
<collectionCode box="[966,1035,1046,1070]" pageId="6" pageNumber="32">ELEF</collectionCode>
, 
<collectionCode box="[1046,1135,1046,1070]" httpUri="http://grbio.org/cool/1d5a-4efs" name="Naturhistorisches Museum, Basel" pageId="6" pageNumber="32">NHMB</collectionCode>
