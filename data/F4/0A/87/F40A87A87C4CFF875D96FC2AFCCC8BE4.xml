, 
<taxonomicName box="[498,652,1083,1107]" class="Magnoliopsida" family="Rutaceae" genus="Ruta" kingdom="Plantae" order="Sapindales" pageId="6" pageNumber="32" phylum="Tracheophyta" rank="species" species="graveolens">
<emphasis box="[498,515,1084,1107]" italics="true" pageId="6" pageNumber="32">R</emphasis>
. 
<emphasis box="[529,652,1083,1107]" italics="true" pageId="6" pageNumber="32">graveolens</emphasis>
