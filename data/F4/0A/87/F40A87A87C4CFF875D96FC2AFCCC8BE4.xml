(
<taxonomicName box="[666,775,1082,1106]" class="Magnoliopsida" family="Rutaceae" kingdom="Plantae" order="Sapindales" pageId="6" pageNumber="32" phylum="Tracheophyta" rank="family">Rutaceae</taxonomicName>
