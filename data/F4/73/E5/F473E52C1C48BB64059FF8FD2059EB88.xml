<taxonomicName authority="Bresciani, 1964" authorityName="Bresciani" authorityYear="1964" box="[290,690,1975,1997]" class="Hexanauplia" family="Clausiidae" genus="Rhodinicola" kingdom="Animalia" order="Cyclopoida" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="gibbosus">
