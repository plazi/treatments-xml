<subSubSection box="[151,362,506,532]" pageId="37" pageNumber="38" type="nomenclature">
<paragraph blockId="37.[151,362,506,532]" box="[151,362,506,532]" pageId="37" pageNumber="38">
<heading bold="true" box="[151,362,506,532]" fontSize="11" level="2" pageId="37" pageNumber="38" reason="4">
<emphasis bold="true" box="[151,362,506,532]" pageId="37" pageNumber="38">
<taxonomicName authority="Kim &amp; Sikorski &amp; O’Reilly &amp; Boxshall, 2013" authorityName="Kim &amp; Sikorski &amp; O’Reilly &amp; Boxshall" authorityYear="2013" box="[151,273,506,532]" class="Hexanauplia" family="Clausiidae" genus="Sheaderia" kingdom="Animalia" order="Cyclopoida" pageId="37" pageNumber="38" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[151,273,506,532]" italics="true" pageId="37" pageNumber="38">Sheaderia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[280,362,506,532]" pageId="37" pageNumber="38" rank="genus">n. gen.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
