, 
<taxonomicName baseAuthorityName="Lamarck" baseAuthorityYear="1822" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="71" pageNumber="72" phylum="Mollusca" rank="species" species="mexicanus" subGenus="Drymaeus">
