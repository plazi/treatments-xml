<quantity id="991010C4BF57FFE9FD2FFCABFD35FCA6" box="[654,717,870,894]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="28" pageNumber="29" unit="m" value="15.0">15 m</quantity>
, 
<emphasis id="6C9C6133BF57FFE9FD7EFCABFCD9FCA6" box="[735,801,870,894]" italics="true" pageId="28" pageNumber="29">M076</emphasis>
: fertile (female) colony on 
<taxonomicName id="99E8C6A2BF57FFE9FBD4FCABFEB2FC7D" authority="Allman, 1877" authorityName="Allman" authorityYear="1877" class="Hydrozoa" family="Sertulariidae" genus="Thyroscyphus" kingdom="Animalia" order="Leptothecata" pageId="28" pageNumber="29" phylum="Cnidaria" rank="species" species="marginatus">
