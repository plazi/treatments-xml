<taxonomicName id="99E8C6A2BF57FFE9FBC7F86FFB57F86F" authority="Allman, 1877" authorityName="Allman" authorityYear="1877" box="[1126,1199,1954,1975]" pageId="28" pageNumber="29" rank="species" species="gracilis">gracilis</taxonomicName>
</emphasis>
