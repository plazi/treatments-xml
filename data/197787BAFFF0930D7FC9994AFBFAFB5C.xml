<taxonomicName id="56DE4D2FFFF0930D7FC999C4FD0BFD68" authority="Bolli et al., 1985, p. 199" authorityName="Bolli" authorityPageNumber="199" authorityYear="1985" box="[151,719,712,736]" class="Globothalamea" family="Globigerinidae" genus="Praeorbulina" kingdom="Chromista" order="Rotaliida" pageId="35" pageNumber="36" phylum="Foraminifera" rank="subSpecies" species="glomerosa" subSpecies="curva">
