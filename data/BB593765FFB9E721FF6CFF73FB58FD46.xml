, 
<collectingCountry id="4BE7C6E3FFB9E721FE2CFDC7FDB5FDFE" box="[471,577,547,573]" name="Australia" pageId="16" pageNumber="17">Australia</collectingCountry>
, 
<collectingCountry id="4BE7C6E3FFB9E721FDB7FDC7FD5CFDFE" box="[588,680,547,573]" name="Bahrain" pageId="16" pageNumber="17">Bahrain</collectingCountry>
, 
<collectingCountry id="4BE7C6E3FFB9E721FD49FDC7FCECFDFE" box="[690,792,547,573]" name="Thailand" pageId="16" pageNumber="17">Thailand</collectingCountry>
