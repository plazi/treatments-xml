<taxonomicName id="7EBD4D7C7109FFD017B6FE3DFBDDFE60" authorityName="JEANNEL" authorityYear="1948" box="[920,1059,441,463]" class="Insecta" family="Carabidae" genus="Acupalpus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="528" phylum="Arthropoda" rank="subGenus" subGenus="Stenolophidius">Stenolophidius</taxonomicName>
) / quadrisetosus 
<taxonomicNameLabel id="90FA57967109FFD0151AFE54FEA1FE4A" box="[308,351,464,485]" pageId="7" pageNumber="520" rank="species">sp.n.</taxonomicNameLabel>
