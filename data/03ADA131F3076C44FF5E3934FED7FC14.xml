<subSubSection id="C31E43ACF3056C44FF0E3C9FFBB8FF2C" box="[199,1127,187,213]" pageId="14" pageNumber="40" type="etymology">
<paragraph id="8BBB1027F3056C44FF0E3C9FFBB8FF2C" blockId="14.[151,1437,151,1004]" box="[199,1127,187,213]" pageId="14" pageNumber="40">
<emphasis id="B970CC35F3056C44FF0E3C9FFEBDFF2C" bold="true" box="[199,354,187,212]" pageId="14" pageNumber="40">Etymology. “</emphasis>
