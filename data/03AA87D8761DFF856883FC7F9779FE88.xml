and 
<taxonomicName id="4C034D4D761DFF846CBEFAC492A0FA80" authority="Willey, 1905" authorityName="Willey" authorityYear="1905" class="Polychaeta" family="Polynoidae" genus="Hololepidella" kingdom="Animalia" order="Phyllodocida" pageId="2" pageNumber="27" phylum="Annelida" rank="genus">
