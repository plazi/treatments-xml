<taxonomicName id="51F182F0FFE2EC7FFEA779F4C02EFF36" authority="Zacharda, 2000" authorityName="Zacharda" authorityYear="2000" box="[328,561,174,198]" class="Arachnida" family="Rhagidiidae" genus="Foveacheles" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="13" pageNumber="258" phylum="Arthropoda" rank="species" species="gigantea" status="n. sp.">
