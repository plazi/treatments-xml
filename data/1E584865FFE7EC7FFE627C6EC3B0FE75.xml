<collectingRegion id="54353791FFE2EC7FFCBB7877C18DFEB5" box="[852,914,301,325]" country="Austria" name="Tirol" pageId="13" pageNumber="258">Tyrol</collectingRegion>
Alps, E of 
<location id="932EAFA8FFE2EC7FFFB97817C2D7FE95" LSID="urn:lsid:plazi:treatment:1E584865FFE7EC7FFE627C6EC3B0FE75:932EAFA8FFE2EC7FFFB97817C2D7FE95" box="[86,200,333,357]" name="Innsbruck" pageId="13" pageNumber="258" stateProvince="Tirol">Innsbruck</location>
, 
