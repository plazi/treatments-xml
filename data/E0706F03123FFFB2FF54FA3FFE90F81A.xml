<taxonomicName id="AFD9A596123FFFB2FC5DF8C9FAB8F88A" authority="(Philippi, 1836)" authorityName="Philippi" authorityYear="1836" baseAuthorityName="Philippi" baseAuthorityYear="1836" box="[926,1309,1846,1871]" class="Gastropoda" family="Pyramidellidae" genus="Pyrgiscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Heterostropha" pageId="69" pageNumber="70" phylum="Mollusca" rank="species" species="rufus">
