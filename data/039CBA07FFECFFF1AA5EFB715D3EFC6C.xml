<taxonomicName id="4C357092FFE3FFF1AF7DFC7E5D6FFC0B" authorityName="Krommelbein" authorityYear="1967" box="[1117,1334,919,945]" class="Ostracoda" family="Krithidae" genus="Aracajuia" kingdom="Animalia" order="Podocopida" pageId="24" pageNumber="25" phylum="Arthropoda" rank="species" species="benderi">
