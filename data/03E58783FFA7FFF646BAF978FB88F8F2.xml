<taxonomicName id="4C4C4D16FFA7FFF5478AF8E8FD28F892" authority="Sanborn, 2021" authorityName="Sanborn" authorityYear="2021" box="[423,738,1811,1837]" class="Insecta" family="Cicadidae" genus="Deremeces" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="57" pageNumber="58" phylum="Arthropoda" rank="species" species="andringitraensis" status="n. gen., n. sp.">
