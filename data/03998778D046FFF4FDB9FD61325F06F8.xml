<taxonomicName id="4C304DEDD046FFF2FCA7FBEA33E40749" box="[871,922,1040,1063]" class="Magnoliopsida" family="Amaranthaceae" genus="Beta" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="355" phylum="Tracheophyta" rank="genus">
