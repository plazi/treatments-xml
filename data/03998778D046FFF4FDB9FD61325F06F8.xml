<collectionCode id="ED21AEABD046FFF2FDDEFBEA324F0749" box="[542,561,1040,1063]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="19" pageNumber="355" type="Museum">V</collectionCode>
