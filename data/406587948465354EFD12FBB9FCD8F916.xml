<taxonomicName id="0FCC4D018465354DFD12FBB9FB16FB40" authority="Zunino and Halffter" authorityName="Zunino and Halffter. A" authorityYear="1988" box="[729,1170,1151,1172]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="539" phylum="Arthropoda" rank="species" species="howdeni">
