<taxonomicName id="0FCC4D018466354EFECAFC2BFE3DFBD4" authorityName="Harold" authorityYear="1869" box="[257,441,1005,1024]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="540" phylum="Arthropoda" rank="species" species="retusus">
