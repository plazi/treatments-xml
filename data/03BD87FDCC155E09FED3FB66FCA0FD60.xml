<collectingCountry id="F303767BCC165E08FC70FEFAFC59FE03" box="[958,1010,362,386]" name="Italy" pageId="6" pageNumber="269">Italy</collectingCountry>
] 
<taxonomicName id="4C144D68CC165E08FF93FE1AFDA3FE23" authority="(Gordon, 1958)" baseAuthorityName="Gordon" baseAuthorityYear="1958" box="[93,520,394,418]" class="Malacostraca" family="Stygiomysidae" genus="Stygiomysis" kingdom="Animalia" order="Mysida" pageId="6" pageNumber="269" phylum="Arthropoda" rank="species" species="holthuisi">
<emphasis id="B960EAF9CC165E08FF93FE1AFEE1FE23" box="[93,330,394,418]" italics="true" pageId="6" pageNumber="269">Stygiomysis holthuisi</emphasis>
(
<bibRefCitation id="EF854B1ACC165E08FE95FE1AFE56FE23" author="GORDON, I." box="[347,509,394,418]" pageId="6" pageNumber="269" pagination="1553" refId="ref7145" refString="GORDON, I., 1958, A new subterranean crustacean from the West Indies, Nature, 181, 1552 ± 1553." type="journal article" year="1958">Gordon, 1958</bibRefCitation>
