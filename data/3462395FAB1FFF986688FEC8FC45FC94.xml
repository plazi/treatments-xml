<taxonomicName id="7BCBF3CAAB18FF9A673DF87FFA62F87D" authorityName="Lowry &amp; Kilgallen" authorityYear="2014" box="[290,560,1951,1972]" class="Malacostraca" family="Tryphosidae" genus="Paralysianopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="dandenong" status="sp. nov.">
<emphasis id="8EBF545BAB18FF9A673DF87FFA62F87D" box="[290,560,1951,1972]" italics="true" pageId="28" pageNumber="29">Paralysianopsis dandenong</emphasis>
</taxonomicName>
<emphasis id="8EBF545BAB18FF9A6427F87FFAD4F87C" bold="true" box="[568,646,1951,1973]" pageId="28" pageNumber="29">
<taxonomicNameLabel id="958CE920AB18FF9A6427F87FFAD4F87C" box="[568,646,1951,1973]" pageId="28" pageNumber="29" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
