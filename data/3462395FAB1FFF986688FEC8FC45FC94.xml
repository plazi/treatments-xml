<caption id="E8B4D8C1AB19FF9B6688F87FFC43F81B" ID-DOI="http://doi.org/10.5281/zenodo.5116688" ID-Zenodo-Dep="5116688" httpUri="https://zenodo.org/record/5116688/files/figure.png" pageId="29" pageNumber="30" startId="29.[151,244,1951,1973]" targetBox="[179,1407,213,1835]" targetPageId="29">
