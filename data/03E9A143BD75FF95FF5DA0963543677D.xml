<emphasis id="B934CC47BD74FF95FEE5A6823543677D" box="[304,610,230,256]" italics="true" pageId="15" pageNumber="158">
<taxonomicName id="4C406BD6BD74FF95FEE5A682357F677D" box="[304,606,230,256]" class="Sordariomycetes" family="Diatrypaceae" genus="Phaeoisaria" kingdom="Fungi" order="Xylariales" pageId="15" pageNumber="158" phylum="Ascomycota" rank="species" species="annesophieae">Phaeoisaria annesophieae</taxonomicName>
