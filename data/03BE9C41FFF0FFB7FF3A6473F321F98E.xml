<taxonomicName id="4C1756D4FFF3FFB7FC146211F521FBEE" box="[953,1072,1094,1120]" class="Insecta" family="Ichneumonidae" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="567" phylum="Arthropoda" rank="subFamily" subFamily="Pimplinae">Pimplinae</taxonomicName>
