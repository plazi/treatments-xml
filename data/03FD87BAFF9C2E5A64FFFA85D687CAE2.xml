<taxonomicNameLabel id="A21357C5FF9C2E5B66EEF91BD08BCC8C" box="[646,708,1698,1723]" pageId="112" pageNumber="113" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C544D2FFF9C2E5B66BBF91BD656CC8C" authority="Gerken, 2018" authorityName="Gerken" authorityYear="2018" box="[723,1049,1698,1723]" class="Malacostraca" family="Lampropidae" genus="Phallolamprops" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Cumacea" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="pribilofensis" status="n. sp.">
<emphasis id="B920EABEFF9C2E5B66BBF91BD656CC8C" box="[723,1049,1698,1723]" italics="true" pageId="112" pageNumber="113">Phallolamprops pribilofensis</emphasis>
</taxonomicName>
<emphasis id="B920EABEFF9C2E5B6048F91BD610CC8C" bold="true" box="[1056,1119,1698,1723]" pageId="112" pageNumber="113">
