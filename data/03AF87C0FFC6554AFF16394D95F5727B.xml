<quantity id="4CFE9B33FFC7554AFF163B4C96597008" box="[151,308,1357,1381]" metricMagnitude="3" metricUnit="m" metricValue="1.6" metricValueMax="2.0" metricValueMin="1.2" pageId="3" pageNumber="4" unit="m" value="1600.0" valueMax="2000.0" valueMin="1200.0">1200–2000 m</quantity>
