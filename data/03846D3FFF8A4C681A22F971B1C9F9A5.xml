<taxonomicName id="4C2DA7AAFF894C6818DEF9A5B169F980" authorityName="Ratcliffe and Cave" authorityYear="2011" box="[478,563,1602,1623]" class="Insecta" family="Dynastidae" genus="Hispanioryctes" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="sculptilis">
