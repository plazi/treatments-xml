species from China. 1 
<taxonomicName id="75FCA299C3359F18809053D28010E6E4" class="Arachnida" family="Agelenidae" genus="Flexicoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flexicoelotes huyunensis" order="Araneae" pageId="7" pageNumber="48" phylum="Arthropoda" rank="species" species="huyunensis">Flexicoelotes huyunensis</taxonomicName>
sp. n. 2 
<taxonomicName id="84B53E64C53E564C5C655091D095C935" class="Arachnida" family="Agelenidae" genus="Flexicoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flexicoelotes jiaohanyanensis" order="Araneae" pageId="7" pageNumber="48" phylum="Arthropoda" rank="species" species="jiaohanyanensis">Flexicoelotes jiaohanyanensis</taxonomicName>
sp. n. 3 
<taxonomicName id="8E5A6DB4931DE55E97BB23F0325DD70B" class="Arachnida" family="Agelenidae" genus="Flexicoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flexicoelotes jinlongyanensis" order="Araneae" pageId="7" pageNumber="48" phylum="Arthropoda" rank="species" species="jinlongyanensis">Flexicoelotes jinlongyanensis</taxonomicName>
sp. n. 4 
<taxonomicName id="8EC46D1EDB15D3A46EABE2A1DC7E1F87" class="Arachnida" family="Agelenidae" genus="Flexicoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flexicoelotes pingzhaiensis" order="Araneae" pageId="7" pageNumber="48" phylum="Arthropoda" rank="species" species="pingzhaiensis">Flexicoelotes pingzhaiensis</taxonomicName>
sp. n. 5 
<taxonomicName id="EA67E18AC9A8C99B0A5275ACC0FFD352" class="Arachnida" family="Agelenidae" genus="Flexicoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flexicoelotes xingwangensis" order="Araneae" pageId="7" pageNumber="48" phylum="Arthropoda" rank="species" species="xingwangensis">Flexicoelotes xingwangensis</taxonomicName>
sp. n.
</paragraph>
</caption>
