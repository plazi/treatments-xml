<taxonomicNameLabel id="DC07DEF762D1CEF97C7A37519938DF9E" pageId="5" pageNumber="237" rank="species">sp. n.</taxonomicNameLabel>
, group 
<taxonomicName id="27431B49E95CE8F96A5BB526B16CAB31" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:34876" class="Insecta" family="Formicidae" genus="Polyrhachis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Polyrhachis guerini Roger" order="Hymenoptera" pageId="5" pageNumber="237" phylum="Arthropoda" rank="species" species="guerini">guerini</taxonomicName>
