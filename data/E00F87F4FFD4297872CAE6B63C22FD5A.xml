<collectingCountry id="10B17672FFD429787119E6953BA3FD9A" box="[836,912,493,517]" name="Sri Lanka" pageId="3" pageNumber="264">Ceylan</collectingCountry>
<superScript id="9FD39BAAFFD4297871CAE6963B8CFE61" attach="left" box="[919,959,491,517]" fontSize="8" pageId="3" pageNumber="264">Vge</superScript>
Humbert&quot; and &quot; 
