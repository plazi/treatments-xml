<taxonomicName id="AFA64D61FFD429787329E5D73B69FD5A" authority="(Thunberg, 1815)" baseAuthorityName="Thunberg" baseAuthorityYear="1815" box="[372,858,685,710]" class="Entomophthoromycetes" family="Entomophthoraceae" genus="Empusa" kingdom="Fungi" order="Entomophthorales" pageId="3" pageNumber="264" phylum="Entomophthoromycota" rank="species" species="pennata" subGenus="Empusa">
<emphasis id="5AD2EAF0FFD429787329E5D73AA5FD59" box="[372,662,686,710]" italics="true" pageId="3" pageNumber="264">Empusa (Empusa) pennata</emphasis>
