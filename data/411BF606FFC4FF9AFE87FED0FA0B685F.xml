<paragraph id="C90D4710FFC4FF9BFC82FECFFC166BE6" blockId="38.[809,1425,322,438]" pageId="38" pageNumber="39">
<taxonomicName id="0EB23C93FFC4FF9BFC82FECFFBA56B27" authorityName="Epstein &amp; Hallas &amp; Johnson &amp; Lopez &amp; Gosliner" authorityYear="2018" box="[809,1075,354,375]" class="Gastropoda" family="Chromodorididae" genus="Hypselodoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Nudibranchia" pageId="38" pageNumber="39" phylum="Mollusca" rank="species" species="paradisa">
<emphasis id="FBC69B02FFC4FF9BFC82FECFFBA56B27" box="[809,1075,354,375]" italics="true" pageId="38" pageNumber="39">Hypselodoris paradisa</emphasis>
</taxonomicName>
