<number id="4B3C2493A162FFA2FC019A0597F7FB2F" box="[944,1006,324,350]" pageId="64" pageNumber="65" value="3328.0">3328</number>
; 
<number id="4B3C2493A162FFA2FBBC9A059056FB2F" box="[1037,1103,324,350]" pageId="64" pageNumber="65" value="0.005">0.005</number>
SD, n= 
<number id="4B3C2493A162FFA2FB009A0590D6FB2F" box="[1201,1231,324,350]" pageId="64" pageNumber="65" value="10.0">10</number>
, 
<collectionCode id="9BE5AE71A162FFA2FB6F9A059132FB2F" box="[1246,1323,324,350]" httpUri="http://grbio.org/cool/nyq3-3g6s" name="National Institute of Water and Atmospheric Research" pageId="64" pageNumber="65">NIWA</collectionCode>
