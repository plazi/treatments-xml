(
<number id="4B3C2493A162FFA2FD389A0596D2FB2F" box="[649,715,324,350]" pageId="64" pageNumber="65" value="0.004">0.004</number>
SD, n= 
<number id="4B3C2493A162FFA2FC9F9A059755FB2F" box="[814,844,324,350]" pageId="64" pageNumber="65" value="10.0">10</number>
