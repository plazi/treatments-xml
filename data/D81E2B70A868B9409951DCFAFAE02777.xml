<collectingCountry id="28A0DAF6A868B9409928DCBDFED926F7" box="[266,369,1740,1763]" name="Malaysia" pageId="12" pageNumber="223">Malaysia</collectingCountry>
: Pasoh, 1998; 
<collectingCountry id="28A0DAF6A868B9409A34DCBDFD2F26F7" box="[534,647,1740,1763]" name="Singapore" pageId="12" pageNumber="223">Singapore</collectingCountry>
