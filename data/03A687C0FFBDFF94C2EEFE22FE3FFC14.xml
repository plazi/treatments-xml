<quantity id="4CF79B33FFBDFF94C1EEFDF4FC5BFDAD" box="[919,973,557,577]" metricMagnitude="2" metricUnit="m" metricValue="4.25" pageId="82" pageNumber="237" unit="m" value="425.0">425m</quantity>
