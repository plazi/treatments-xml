<quantity id="4CF79B33FFBDFF94C3DBFDA4FE72FD7D" box="[418,484,637,657]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="82" pageNumber="237" unit="m" value="1100.0">1100m</quantity>
