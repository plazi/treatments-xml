<taxonomicName id="4C4D4D31FF90C908E7F54706FD1CFAF9" authority="Carpintero &amp; Dellape" authorityName="Carpintero &amp; Dellape" box="[151,733,1352,1378]" class="Insecta" family="Thaumastocoridae" genus="Thaumastocoris" kingdom="Animalia" order="Hemiptera" pageId="54" pageNumber="55" phylum="Arthropoda" rank="species" species="peregrinus">
