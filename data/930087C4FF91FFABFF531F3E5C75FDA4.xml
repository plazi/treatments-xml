<number id="AD6124F5FF92FFABFE1019915F6DFE11" box="[468,530,413,439]" pageId="10" pageNumber="31" value="1742.0">1742</number>
/ 
<number id="AD6124F5FF92FFABFDD819915F3CFE11" box="[540,579,413,439]" pageId="10" pageNumber="31" value="5.0" valueMax="6.0" valueMin="4.0">4-6</number>
