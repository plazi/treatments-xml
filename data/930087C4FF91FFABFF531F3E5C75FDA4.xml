<typeStatus id="C4128870FF92FFABFBF319CE59A8FE7A" box="[1079,1239,450,476]" pageId="10" pageNumber="31">paralectotype</typeStatus>
<number id="AD6124F5FF92FFABFB1B19CE5864FE7A" box="[1247,1307,450,476]" pageId="10" pageNumber="31" value="1742.0">1742</number>
