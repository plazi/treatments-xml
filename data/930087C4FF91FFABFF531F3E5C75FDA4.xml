<typeStatus id="C4128870FF92FFABFB2C199158E6FE11" box="[1256,1433,413,439]" pageId="10" pageNumber="31">paralectotypes</typeStatus>
. The 
<typeStatus id="C4128870FF92FFABFF0919CE5C06FE7A" box="[205,377,450,476]" pageId="10" pageNumber="31">paralectotypes</typeStatus>
<number id="AD6124F5FF92FFABFE4619CE5CC0FE7A" box="[386,447,450,476]" pageId="10" pageNumber="31" value="1742.0">1742</number>
/ 
<number id="AD6124F5FF92FFABFE0D19CE5C92FE7A" box="[457,493,450,476]" pageId="10" pageNumber="31" value="3.5" valueMax="4.0" valueMin="3.0">3-4</number>
