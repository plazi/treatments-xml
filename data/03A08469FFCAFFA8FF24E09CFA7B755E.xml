<taxonomicName id="4C094EFCFFCBFFA8FB36E02EFA7B755E" authority="Yunakov, 2005" authorityName="Germann" authorityYear="2017" box="[1157,1436,527,549]" class="Insecta" family="Curculionidae" genus="Amicromias" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="314" phylum="Arthropoda" rank="species" species="cephalotes" status="sp. n.">
<emphasis id="B97DE96DFFCBFFA8FB36E02EFAE2755E" box="[1157,1285,527,549]" italics="true" pageId="3" pageNumber="314">A. cephalotes</emphasis>
