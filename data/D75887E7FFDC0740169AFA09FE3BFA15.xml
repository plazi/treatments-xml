<collectingCountry id="27E67661FFDF07401435FA2AFAE9FA77" box="[115,171,1455,1479]" name="Japan" pageId="2" pageNumber="335">Japan</collectingCountry>
, 
<collectingCountry id="27E67661FFDF074014F5FA2AFB29FA77" box="[179,363,1455,1479]" name="Papua New Guinea" pageId="2" pageNumber="335">Papua New Guinea</collectingCountry>
