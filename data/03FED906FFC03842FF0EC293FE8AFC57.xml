: 
<collectingRegion id="4993A6F2FFC03842FDA5C14CFD57FC9F" box="[572,659,840,864]" country="Canada" name="Alberta" pageId="34" pageNumber="35">Alberta</collectingRegion>
, 
<collectingRegion id="4993A6F2FFC03842FD3BC14CFD3EFC9F" box="[674,762,840,864]" country="Canada" name="Quebec" pageId="34" pageNumber="35">Québec</collectingRegion>
