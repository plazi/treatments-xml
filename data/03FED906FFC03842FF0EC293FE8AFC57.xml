<taxonomicName id="4C571393FFC03842FF0EC293FD67FF4E" authority="(Johannsen)" baseAuthorityName="Johannsen" baseAuthorityYear="1905" box="[151,675,151,177]" class="Insecta" family="Chironomidae" genus="Parametriocnemus" kingdom="Animalia" order="Diptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="lundbeckii">
