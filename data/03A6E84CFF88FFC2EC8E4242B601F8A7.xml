<taxonomicName id="4C0F22D9FF88FFC2E8BA430FB0B5F883" box="[1187,1270,1858,1883]" class="Insecta" family="Arctiidae" genus="Ichoria" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="523" phylum="Arthropoda" rank="genus">
