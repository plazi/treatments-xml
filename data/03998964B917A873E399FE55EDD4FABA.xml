<taxonomicName id="4C3043F1B917A873E049FE55EEC8FE7A" authority="Senna, 2010" authorityName="Senna" authorityYear="2010" box="[531,686,391,415]" class="Malacostraca" family="Phoxocephalidae" genus="Bathybirubius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="2078" phylum="Arthropoda" rank="genus" status="gen. nov.">
