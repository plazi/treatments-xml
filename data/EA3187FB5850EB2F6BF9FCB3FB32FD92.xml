<taxonomicName id="A5984D6E5857EB2F69EEFF54FD2FFF07" box="[640,751,152,178]" class="Magnoliopsida" family="Oleaceae" kingdom="Plantae" order="Lamiales" pageId="4" pageNumber="65" phylum="Tracheophyta" rank="family">Oleaceae</taxonomicName>
