<collectingCountry id="86D18DB0FF94FFCEFCD2704EDBBBFA02" box="[857,963,1385,1409]" name="Singapore" pageId="37" pageNumber="50">Singapore</collectingCountry>
and Borneo (
<collectingCountry id="86D18DB0FF94FFCEFBE9704EDCBDFA02" box="[1122,1221,1385,1409]" name="Malaysia" pageId="37" pageNumber="50">Malaysia</collectingCountry>
: 
<collectingRegion id="3C0203C2FF94FFCEFB5F704EDD6EFA02" box="[1236,1302,1385,1409]" country="Malaysia" name="Sabah" pageId="37" pageNumber="50">Sabah</collectingRegion>
; 
<collectingCountry id="86D18DB0FF94FFCEFAAF704EDD17FA02" box="[1316,1391,1385,1409]" name="Brunei" pageId="37" pageNumber="50">Brunei</collectingCountry>
