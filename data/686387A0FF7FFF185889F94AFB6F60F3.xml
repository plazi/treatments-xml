<taxonomicName id="27CA4D35FF7EFF185A67FE8EFCE26064" authorityName="Rondani" authorityYear="1876" box="[633,849,259,285]" class="Insecta" family="Psilidae" genus="Loxocera" kingdom="Animalia" order="Diptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="species" species="maculata">
