<taxonomicName id="F87C8CC6567C6B4C40A138DD35E2FC86" class="Insecta" family="Braconidae" genus="Exotela" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Exotela" order="Hymenoptera" pageId="0" pageNumber="8151" phylum="Arthropoda" rank="genus">Exotela</taxonomicName>
