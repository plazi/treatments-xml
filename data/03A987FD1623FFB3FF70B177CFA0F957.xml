<taxonomicName id="4C004D681624FFB3FD8FB188CDF1F972" authorityName="Lawrence" authorityYear="2019" box="[616,689,1665,1688]" class="Insecta" family="Ciidae" genus="Notapterocis" kingdom="Animalia" order="Coleoptera" pageId="28" pageNumber="479" phylum="Arthropoda" rank="species" species="acutus">
