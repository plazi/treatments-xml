<subSubSection id="C31A65601624FFB3FF20B176CFA0F957" pageId="28" pageNumber="479" type="etymology">
<paragraph id="8BBF36EB1624FFB3FF20B176CFA0F957" blockId="28.[151,1437,151,1725]" pageId="28" pageNumber="479">
<emphasis id="B974EAF91624FFB3FF20B176CE0EF972" bold="true" box="[199,334,1663,1688]" pageId="28" pageNumber="479">Etymology.</emphasis>
