<subSubSection type="nomenclature">
<paragraph pageNumber="470">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181910" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole nebulosa Wilson" lsidName-HNS="Pheidole nebulosa Wilson" order="Hymenoptera" pageNumber="470" phylum="Arthropoda" rank="species" species="nebulosa">Pheidole nebulosa</taxonomicName>
