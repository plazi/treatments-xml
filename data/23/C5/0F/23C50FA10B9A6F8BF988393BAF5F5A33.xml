, 
<taxonomicName authorityName="Dejean" authorityYear="1831" class="Insecta" family="Carabidae" genus="Stenomorphus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="684" pageNumber="685" phylum="Arthropoda" rank="genus">
