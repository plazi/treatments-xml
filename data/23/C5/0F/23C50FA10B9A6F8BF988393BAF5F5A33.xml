<taxonomicName authorityName="Morawitz" authorityYear="1863" class="Hexapoda" family="Carabidae" genus="Trichotichnus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="684" pageNumber="685" phylum="Arthropoda" rank="genus">
