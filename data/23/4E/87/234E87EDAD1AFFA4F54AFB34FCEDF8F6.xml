<subSubSection pageId="16" pageNumber="17" type="distribution">
<paragraph blockId="16.[264,1323,1516,1902]" pageId="16" pageNumber="17">
<emphasis box="[312,455,1716,1741]" italics="true" pageId="16" pageNumber="17">Distribution</emphasis>
. Also known from 
<collectingCountry box="[706,774,1716,1742]" name="Israel" pageId="16" pageNumber="17">Israel</collectingCountry>
, 
<collectingCountry box="[789,895,1716,1742]" name="Tanzania" pageId="16" pageNumber="17">Zanzibar</collectingCountry>
, 
<collectingCountry box="[910,1024,1716,1742]" name="Seychelles" pageId="16" pageNumber="17">Seychelle</collectingCountry>
