<collectingCountry box="[494,611,1756,1782]" name="Indonesia" pageId="16" pageNumber="17">Indonesia</collectingCountry>
, 
<collectingCountry box="[628,848,1756,1782]" name="Philippines" pageId="16" pageNumber="17">Philippine Islands</collectingCountry>
, Queensland, 
<collectingCountry box="[1023,1264,1756,1782]" name="Papua New Guinea" pageId="16" pageNumber="17">Papua New Guinea</collectingCountry>
, and 
<collectingCountry box="[264,461,1796,1822]" name="Marshall Islands" pageId="16" pageNumber="17">Marshall Islands</collectingCountry>
,
</paragraph>
