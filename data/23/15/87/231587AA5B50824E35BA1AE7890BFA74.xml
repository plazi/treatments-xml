<collectingCountry box="[1267,1326,1322,1348]" name="United States of America" pageId="29" pageNumber="30">USA</collectingCountry>
recorded from 
<collectingRegion box="[216,407,1362,1388]" country="United States of America" name="New Hampshire" pageId="29" pageNumber="30">New Hampshire</collectingRegion>
south to 
<collectingRegion box="[519,603,1362,1388]" country="United States of America" name="Florida" pageId="29" pageNumber="30">Florida</collectingRegion>
and west to 
<collectingRegion box="[755,867,1362,1388]" country="United States of America" name="Michigan" pageId="29" pageNumber="30">Michigan</collectingRegion>
(
<bibRefCitation author="White, R. E." box="[883,1026,1362,1388]" pageId="29" pageNumber="30" pagination="529 - 70" refId="ref23969" refString="White, R. E. (1982) A catalogue of the Coleoptera of America north of Mexico, Family Anobiidae. United States Department of Agriculture, Agricultural Research Service, Agricultural Handbook 529 - 70." type="book chapter" year="1982">White 1982</bibRefCitation>
