<collectingCountry box="[1305,1364,392,418]" name="United States of America" pageId="12" pageNumber="13">USA</collectingCountry>
(
<bibRefCitation author="Beal, R. S., Jr." pageId="12" pageNumber="13" pagination="391 - 404" refId="ref21495" refString="Beal, R. S., Jr. (2003) Annotated checklist of Nearctic Dermestidae with revised key to the genera. The Coleopterists Bulletin, 57 (4), 391 - 404." type="journal article" year="2003">Beal 2003</bibRefCitation>
); in 
<collectingCountry box="[264,352,432,458]" name="Canada" pageId="12" pageNumber="13">Canada</collectingCountry>
recorded from 
<collectingRegion box="[532,624,432,458]" country="Canada" name="Ontario" pageId="12" pageNumber="13">Ontario</collectingRegion>
, 
<collectingRegion box="[636,729,432,458]" country="Canada" name="Quebec" pageId="12" pageNumber="13">Québec</collectingRegion>
