<emphasis box="[301,511,1347,1372]" italics="true" pageId="4" pageNumber="59">C. aurantiacopes</emphasis>
Grismer &amp; Ngo
</taxonomicName>
, 
<taxonomicName authority="Strauch" authorityName="Strauch" box="[729,998,1346,1372]" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="59" phylum="Chordata" rank="species" species="boulengeri">
<emphasis box="[729,897,1347,1372]" italics="true" pageId="4" pageNumber="59">C. boulengeri</emphasis>
Strauch
</taxonomicName>
, 
<taxonomicName authority="Grismer &amp; Ngo" authorityName="Grismer &amp; Ngo" box="[1015,1395,1346,1372]" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="59" phylum="Chordata" rank="species" species="caudanivea">
<emphasis box="[1015,1191,1347,1372]" italics="true" pageId="4" pageNumber="59">C. caudanivea</emphasis>
Grismer &amp; Ngo
</taxonomicName>
, 
<taxonomicName authority="Bauer &amp; Das" authorityName="Bauer &amp; Das" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="59" phylum="Chordata" rank="species" species="chanthaburiensis">
<emphasis italics="true" pageId="4" pageNumber="59">C. chanthaburiensis</emphasis>
Bauer &amp; Das
</taxonomicName>
, 
<taxonomicName authority="Chan &amp; Grismer" authorityName="Chan &amp; Grismer" box="[532,899,1384,1410]" class="Reptilia" family="Gekkonidae" genus="Cnemaspis" kingdom="Animalia" order="Squamata" pageId="4" pageNumber="59" phylum="Chordata" rank="species" species="flavigaster">
<emphasis box="[532,692,1384,1409]" italics="true" pageId="4" pageNumber="59">C. flavigaster</emphasis>
Chan &amp; Grismer
