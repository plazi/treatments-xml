<emphasis box="[1373,1422,1645,1669]" italics="true" pageId="4" pageNumber="19">et al</emphasis>
., 
<number box="[151,212,1681,1705]" pageId="4" pageNumber="19" value="2005.0">2005</number>
</bibRefCitation>
). 
<taxonomicName box="[230,317,1682,1705]" class="Arachnida" family="Phytoseiidae" genus="Euseius" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="19" phylum="Arthropoda" rank="genus">
