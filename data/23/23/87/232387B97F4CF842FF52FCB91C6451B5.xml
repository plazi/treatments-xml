<paragraph blockId="2.[151,439,860,887]" box="[151,439,860,887]" pageId="2" pageNumber="117">
<heading bold="true" box="[151,439,860,887]" fontSize="11" level="1" pageId="2" pageNumber="117" reason="1">
<emphasis bold="true" box="[151,439,860,887]" pageId="2" pageNumber="117">
<taxonomicName authority="Chu &amp; Wang &amp; Sun, 2018" authorityName="Chu &amp; Wang &amp; Sun" authorityYear="2018" box="[151,322,860,886]" class="Malacostraca" family="Potamidae" genus="Semicirculara" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="117" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[151,322,860,886]" italics="true" pageId="2" pageNumber="117">Semicirculara</emphasis>
