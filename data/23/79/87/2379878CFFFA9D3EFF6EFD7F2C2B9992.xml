<taxonomicName authority="Hunt, Wilkerson &amp; Coetzee" authorityName="Hunt, Wilkerson &amp; Coetzee" box="[151,867,717,743]" class="Insecta" family="Culicidae" genus="Anopheles" kingdom="Animalia" order="Diptera" pageId="18" pageNumber="264" phylum="Arthropoda" rank="species" species="amharicus" status="sp.n." subGenus="Cellia">
<emphasis bold="true" box="[151,278,717,743]" italics="true" pageId="18" pageNumber="264">Anopheles</emphasis>
(
<emphasis bold="true" box="[295,366,717,743]" italics="true" pageId="18" pageNumber="264">Cellia</emphasis>
) 
<emphasis bold="true" box="[384,512,717,743]" italics="true" pageId="18" pageNumber="264">amharicus</emphasis>
Hunt, Wilkerson &amp; Coetzee
