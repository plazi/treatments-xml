<number box="[1345,1360,1595,1619]" pageId="10" value="3.0">3</number>
- 
<number box="[1369,1384,1595,1619]" pageId="10" value="3.0">3</number>
- 
<number box="[1393,1408,1595,1619]" pageId="10" value="3.0">3</number>
- 
<number box="[1417,1432,1595,1619]" pageId="10" value="3.0">3</number>
