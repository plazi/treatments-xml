<taxonomicName authority="Leach" authorityName="Leach" authorityYear="1817" box="[600,799,1034,1060]" class="Insecta" family="Dytiscidae" genus="Hydaticus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="152" phylum="Arthropoda" rank="genus">
