<geoCoordinate degrees="27" direction="north" minutes="51.6307" orientation="latitude" precision="1" value="27.860512">27°51.6307'N</geoCoordinate>
, 
<geoCoordinate degrees="87" direction="east" minutes="25.4768" orientation="longitude" precision="1" value="87.424614">87°25.4768'E</geoCoordinate>
