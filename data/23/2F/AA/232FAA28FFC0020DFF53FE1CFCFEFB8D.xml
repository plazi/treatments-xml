<paragraph blockId="6.[151,503,393,419]" box="[151,503,393,419]" pageId="6" pageNumber="7">
<heading bold="true" box="[151,503,393,419]" fontSize="11" level="1" pageId="6" pageNumber="7" reason="1">
<emphasis bold="true" box="[151,503,393,419]" pageId="6" pageNumber="7">
The 
<taxonomicName authority="KATAEV ET AL." box="[207,328,393,419]" class="Insecta" family="Carabidae" genus="Chydaeus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="kasaharai">
