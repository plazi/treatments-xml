• 
<specimenCount box="[414,458,469,495]" pageId="6" pageNumber="489" type="male">1 ♂</specimenCount>
, 
<collectingCountry box="[470,563,469,495]" name="Egypt" pageId="6" pageNumber="489">EGYPT</collectingCountry>
