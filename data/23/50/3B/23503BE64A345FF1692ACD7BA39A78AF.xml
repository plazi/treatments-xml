<subSubSection pageId="0" pageNumber="1" type="reference_group">
<paragraph pageId="0" pageNumber="1">
<taxonomicName authorityName="Pirie" authorityYear="2004" class="Magnoliopsida" family="Annonaceae" genus="Cremastosperma" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cremastosperma yamayakatense" order="Magnoliales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="yamayakatense">Cremastosperma yamayakatense</taxonomicName>
