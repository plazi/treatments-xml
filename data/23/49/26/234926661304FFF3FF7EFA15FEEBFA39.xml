<emphasis box="[880,1022,1416,1437]" italics="true" pageId="11" pageNumber="166">L. bisuditum</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Assing" authorityYear="2013" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="166" phylum="Arthropoda" rank="species" species="verminatum">
