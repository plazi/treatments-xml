<emphasis italics="true" pageId="0" pageNumber="49">N. annandalii</emphasis>
</taxonomicName>
(Boulenger, 1920), 
<taxonomicName genus="N." lsidName="N. gammii" pageId="0" pageNumber="49" rank="species" species="gammii">
<emphasis italics="true" pageId="0" pageNumber="49">N. gammii</emphasis>
</taxonomicName>
(Anderson, 1871b), 
<taxonomicName genus="N." lsidName="N. liebigii" pageId="0" pageNumber="49" rank="species" species="liebigii">
<emphasis italics="true" pageId="0" pageNumber="49">N. liebigii</emphasis>
</taxonomicName>
(
<normalizedToken originalValue="Günther">Guenther</normalizedToken>
, 1860), 
<taxonomicName genus="N." lsidName="N. polunini" pageId="0" pageNumber="49" rank="species" species="polunini">
<emphasis italics="true" pageId="0" pageNumber="49">N. polunini</emphasis>
</taxonomicName>
(Smith, 1951), 
