Genus 
<taxonomicName id="4C1A34E7C97FFFA664AF89AEDF3EFF31" authority="Thorell" authorityName="Thorell" box="[509,622,203,231]" class="Arachnida" family="Liocranidae" genus="Jacaena" kingdom="Animalia" order="Araneae" pageId="65" pageNumber="465" phylum="Arthropoda" rank="genus">
