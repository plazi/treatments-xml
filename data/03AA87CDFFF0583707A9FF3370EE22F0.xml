<taxonomicName id="4C034D58FFF05837078BFA2073742288" box="[181,319,1412,1436]" class="Monogenea" family="Dactylogyridae" genus="Anacanthorus" kingdom="Animalia" order="Dactylogyridea" pageId="3" pageNumber="60" phylum="Platyhelminthes" rank="species" species="contortus" status="sp. n.">
