<td id="36EBD82D4956F876B67EEDDC63B5C6DC" align="left" pageId="null" pageNumber="542" valign="top">
1*. 
<normalizedToken id="EF8FE0A8850E58D8576BDBD2B147CBFE" originalValue="Blütenstiele" pageId="null" pageNumber="542">Bluetenstiele</normalizedToken>
<normalizedToken id="B9F5128626A472308BE69B757E289A01" originalValue="ungefähr" pageId="null" pageNumber="542">ungefaehr</normalizedToken>
