<quantity id="4CAF3DEEFFE0EF63FDE1E54EFD5D3D02" box="[586,679,1590,1615]" metricMagnitude="-2" metricUnit="m" metricValue="1.0699999999999998" pageId="11" pageNumber="818" unit="mm" value="10.7">10.7 mm</quantity>
