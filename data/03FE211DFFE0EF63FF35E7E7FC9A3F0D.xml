<quantity id="4CAF3DEEFFE0EF63FBAFE236FBB93A2A" box="[1028,1091,334,359]" metricMagnitude="0" metricUnit="m" metricValue="2.5" metricValueMax="4.0" metricValueMin="1.0" pageId="11" pageNumber="818" unit="m" value="2.5" valueMax="4.0" valueMin="1.0">
1- 
<specimenCount id="9D515B82FFE0EF63FBBDE236FBB93A2A" box="[1046,1091,334,359]" pageId="11" pageNumber="818" type="generic">4 m</specimenCount>
