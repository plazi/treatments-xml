<typeStatus id="F13488451623905238C9FE29FF7680A4" box="[151,201,417,438]" pageId="1" pageNumber="384">Type</typeStatus>
species: 
<taxonomicName id="E98F4D64162390523978FE28FD3D80A4" authority="Uhler, 1893" authorityName="Uhler" authorityYear="1893" box="[294,642,416,438]" class="Insecta" family="Miridae" genus="Agalliastes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="384" phylum="Arthropoda" rank="species" species="stigmosus">
<emphasis id="1CFBEAF5162390523978FE28FDBE80A4" box="[294,513,416,438]" italics="true" pageId="1" pageNumber="384">Agalliastes stigmosus</emphasis>
Uhler, 
<number id="984724C0162390523A12FE28FD3D80A4" box="[588,642,416,438]" pageId="1" pageNumber="384" value="1893.0">1893</number>
</taxonomicName>
