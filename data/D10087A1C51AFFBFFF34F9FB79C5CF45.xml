<taxonomicName id="9EA94D34C51AFFBFFE2DF90D7C9BCEAB" box="[398,465,1739,1764]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="16" pageNumber="43" phylum="Tracheophyta" rank="genus">
