<collectingRegion id="49C1D2A13D1EFFFAFD31901ADD08FBBB" box="[721,820,1088,1109]" country="Venezuela" name="Portuguesa" pageId="29" pageNumber="1700">Portuguesa</collectingRegion>
, Guanare, 
<date id="FFBB3A833D1EFFFAFC74901ADDCCFBB8" box="[916,1008,1088,1110]" pageId="29" pageNumber="1700" value="1969-02-26">26.II.1969</date>
