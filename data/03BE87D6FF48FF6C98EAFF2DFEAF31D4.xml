<taxonomicName id="4C174D43FF48FF6C9AFBFE61FE0B31A8" authority="Schmidt, 1904" authorityName="Schmidt" authorityYear="1904" box="[646,1098,403,429]" class="Actinopterygii" family="Pleuronectidae" genus="Acanthopsetta" kingdom="Animalia" order="Pleuronectiformes" pageId="217" pageNumber="218" phylum="Chordata" rank="species" species="nadeshnyi">
