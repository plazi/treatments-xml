<taxonomicName id="4C174D43FF48FF6C9A91FE45F93F31D4" authorityName="Schmidt" authorityYear="1904" box="[748,894,439,465]" class="Actinopterygii" family="Pleuronectidae" genus="Acanthopsetta" kingdom="Animalia" order="Pleuronectiformes" pageId="217" pageNumber="218" phylum="Chordata" rank="species" species="nadeshnyi">
