<taxonomicName id="B7FA68C65472E241A775F75BC37167B8" class="Magnoliopsida" family="Fagaceae" genus="Quercus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Quercus" order="Fagales" pageId="4" pageNumber="39" phylum="Tracheophyta" rank="genus">Quercus</taxonomicName>
