<taxonomicName id="4E10EE7E79927AD621B39DA27C223259" authority="A.Berlese, 1910" authorityName="A.Berlese" authorityYear="1910" class="Arachnida" family="Pachylaelapidae" genus="Pachyseius" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pachyseius humeralis" order="Mesostigmata" pageId="4" pageNumber="39" phylum="Arthropoda" rank="species" species="humeralis">Pachyseius humeralis</taxonomicName>
