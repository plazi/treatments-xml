<taxonomicName id="FDE0848EFF8CB27EF71DFADCFB7EA022" authority="Vacelet, 1977" authorityName="Vacelet" authorityYear="1977" box="[852,1178,1296,1320]" class="Calcarea" family="Minchinellidae" genus="Tulearinia" kingdom="Animalia" order="Lithonida" pageId="55" pageNumber="258" phylum="Porifera" rank="species" species="stylifera">
