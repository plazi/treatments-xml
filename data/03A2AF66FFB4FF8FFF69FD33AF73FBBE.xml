<taxonomicName id="4C0B65F3FFB4FF8FFF27FCFAAA8FFCFF" box="[217,446,796,818]" class="Diplopoda" family="Anthroleucosomatidae" kingdom="Animalia" order="Chordeumatida" pageId="2" pageNumber="153" phylum="Arthropoda" rank="family">Anthroleucosomatidae</taxonomicName>
