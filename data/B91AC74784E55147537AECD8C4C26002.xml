<subSubSection id="171A92A1A2EE59489E4414DA18EDFB84" pageId="null" pageNumber="233" type="vernacular_name">
<paragraph id="5ACB3CCA0C927CBD5818BBF70DAEB0E0" pageId="null" pageNumber="233">
Rauhhaarige 
<normalizedToken id="05B720EF9F75E777AE1230EAD69AC79A" originalValue="Gänsekresse" pageId="null" pageNumber="233">Gaensekresse</normalizedToken>
</paragraph>
</subSubSection>
