<pageBreakToken id="41747F87C5DA7FEAFDE93D0288768A66" pageId="null" pageNumber="233" start="start">Artengruppe</pageBreakToken>
der 
<taxonomicName id="47680E620A62DE9B3B7DC1F4653FF16F" authority="(L.) Scop." class="Magnoliopsida" family="Brassicaceae" genus="Arabis" kingdom="Plantae" order="Brassicales" pageId="null" pageNumber="233" phylum="Tracheophyta" rank="species" species="hirsuta">
