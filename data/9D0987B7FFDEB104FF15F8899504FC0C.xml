<taxonomicName id="D2A04D22FFDEB105FF15F889966DF897" authority="Fallen, 1807" authorityName="Fallen" authorityYear="1807" box="[151,631,1839,1865]" class="Insecta" family="Miridae" genus="Megalocoleus" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="36" phylum="Arthropoda" rank="species" species="molliculus">
