<taxonomicName id="4C6484A20900885FFD08DF6B6F01F9BE" box="[725,820,1585,1609]" class="Insecta" family="Phaneropteridae" genus="Elimaea" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus" status="sp.n.">Elimaea</taxonomicName>
