<geoCoordinate id="EE425043FFC5FFFA5CFFFD99FD90FD8A" box="[484,533,512,539]" degrees="2" direction="east" minutes="5" orientation="longitude" pageId="6" pageNumber="175" precision="925" value="2.0833333">
<emphasis id="B902EA96FFC5FFFA5CFFFD99FE7BFD8A" box="[484,510,513,539]" italics="true" pageId="6" pageNumber="175">e2</emphasis>
5
</geoCoordinate>
, 
<emphasis id="B902EA96FFC5FFFA5F3AFD99FDC9FD8A" box="[545,588,513,539]" italics="true" pageId="6" pageNumber="175">ag1</emphasis>
35, 
