<taxonomicName id="4C7E3589FFB9FF9FFF48F8A0337EFC16" authority="Laporte, 1832" authorityName="Laporte" authorityYear="1832" box="[192,514,974,998]" class="Insecta" family="Melolonthidae" genus="Clavipalpus" kingdom="Animalia" order="Coleoptera" pageId="114" pageNumber="113" phylum="Arthropoda" rank="genus">
