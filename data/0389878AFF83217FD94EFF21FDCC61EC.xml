<paragraph id="8B9F369CFF83217FD94EFF21FE2162B6" blockId="14.[120,762,183,908]" box="[121,398,183,209]" pageId="14" pageNumber="2796">
<heading id="D0D781F0FF83217FD94EFF21FE2162B6" bold="true" box="[121,398,183,209]" fontSize="9" level="2" pageId="14" pageNumber="2796" reason="2">
<taxonomicName id="4C204D1FFF83217FD94EFF21FE2162B6" authorityName="Moore &amp; Holdeman" authorityYear="1970" baseAuthorityName="Brygoo &amp; Aladame" baseAuthorityYear="1953" box="[121,398,183,209]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="14" pageNumber="2796" phylum="Firmicutes" rank="species" species="crispatus">
