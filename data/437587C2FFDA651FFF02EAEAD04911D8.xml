<taxonomicName id="0CDC4D57FFDA651FFDA4ED03D78F1110" authority="(Gray, 1850)" authorityName="Gray" authorityYear="1850" baseAuthorityName="Gray" baseAuthorityYear="1850" box="[561,933,1371,1398]" class="Gastropoda" family="Charopidae" genus="Therasiella" kingdom="Animalia" order="Stylommatophora" pageId="30" pageNumber="31" phylum="Mollusca" rank="species" species="celinde">
<emphasis id="F9A8EAC6FFDA651FFDA4ED03D7211110" box="[561,779,1372,1398]" italics="true" pageId="30" pageNumber="31">Therasiella celinde</emphasis>
