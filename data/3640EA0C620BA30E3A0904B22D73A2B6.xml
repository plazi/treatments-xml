<paragraph id="FFC13C5FF5462FD9B2F6A45906347129" pageId="2" pageNumber="49">Type species.</paragraph>
<paragraph id="125861C1F8F3D29165D7732F4ABDCCAB" pageId="2" pageNumber="49">
<taxonomicName id="6C35C471ACE415C17B606645C45AB13F" class="Insecta" family="Termitidae" genus="Nasutitermes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Nasutitermes brooksi" order="Blattodea" pageId="2" pageNumber="49" phylum="Arthropoda" rank="species" species="brooksi">Nasutitermes brooksi</taxonomicName>
