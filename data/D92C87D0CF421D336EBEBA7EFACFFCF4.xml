<taxonomicName id="96854D45CF421D336B3FBCFAFC53FD98" authorityName="Bravo-Hollis" authorityYear="1970" class="Monogenea" family="Loimoidae" genus="Loimosina" kingdom="Animalia" order="Monocotylidea" pageId="37" pageNumber="538" phylum="Platyhelminthes" rank="species" species="parawilsoni">
