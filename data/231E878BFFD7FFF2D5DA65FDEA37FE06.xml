<materialsCitation id="1BDF3CC0FFD7FFF2D43A62F0ED2BFF26" ID-GBIF-Occurrence="3312842309" collectingDate="2014-10-19" collectionCode="CNUIC" collectorName="KJ Ahn &amp; IS Yoo &amp; JS Lee" country="South Korea" county="Deposited" elevation="2" lastPageId="3" lastPageNumber="264" latitude="33.231792" location="Haye-dong" longitude="126.37217" municipality="Seogwipo-si" pageId="2" pageNumber="263" specimenCount="1" specimenCount-male="1" stateProvince="Jeju" typeStatus="holotype">
<typeStatus id="740C883FFFD7FFF3D43A62F0EEB3F846" box="[375,482,1947,1973]" pageId="2" pageNumber="263">Holotype</typeStatus>
, 
