<emphasis id="B971BF728FA3FF1EFF54FA4041463961" box="[250,336,1528,1551]" italics="true" pageId="247" pageNumber="248">Ixchela</emphasis>
</taxonomicName>
(incertae sedis are: 
<emphasis id="B971BF728FA3FF1EFDFAFA4040A83943" italics="true" pageId="247" pageNumber="248">‘‘C.’’ clara</emphasis>
, 
<emphasis id="B971BF728FA3FF1EFF60F9AE41033943" box="[206,277,1558,1581]" italics="true" pageId="247" pageNumber="248">faceta</emphasis>
, 
<emphasis id="B971BF728FA3FF1EFE8AF9AE41403943" box="[292,342,1558,1581]" italics="true" pageId="247" pageNumber="248">iviei</emphasis>
, 
<emphasis id="B971BF728FA3FF1EFEC8F9AE41893943" box="[358,415,1558,1581]" italics="true" pageId="247" pageNumber="248">tigra</emphasis>
, 
<taxonomicName id="4C0518E38FA3FF1EFE03F9AE42263943" authorityName="Kraus" authorityYear="1955" box="[429,560,1558,1581]" class="Arachnida" family="Pholcidae" genus="Coryssocnemis" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="species" species="viridescens">
<emphasis id="B971BF728FA3FF1EFE03F9AE42263943" box="[429,560,1558,1581]" italics="true" pageId="247" pageNumber="248">viridescens</emphasis>
</taxonomicName>
; see appendix 3), 
<collectingRegion id="49C1AD828FA3FF1EFEACF98B416B3924" box="[258,381,1587,1610]" country="Ecuador" name="Galapagos" pageId="247" pageNumber="248">Galápagos</collectingRegion>
