<taxonomicName id="4C0518E38FA3FF1EFE67F9D6422939EB" box="[457,575,1646,1669]" class="Arachnida" family="Pholcidae" genus="Coryssocnemis" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="species" species="uncata">
