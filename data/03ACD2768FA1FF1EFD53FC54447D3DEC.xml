<taxonomicName id="4C0518E38FA3FF1EFF2FF9D640E439EB" authorityName="Mello-Leitao" authorityYear="1947" box="[129,242,1646,1669]" class="Arachnida" family="Pholcidae" genus="Coryssocnemis" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="species" species="paraensis">
<emphasis id="B971BF728FA3FF1EFF2FF9D640E439EB" box="[129,242,1646,1669]" italics="true" pageId="247" pageNumber="248">paraensis</emphasis>
