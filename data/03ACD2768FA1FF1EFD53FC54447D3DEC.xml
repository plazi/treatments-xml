<taxonomicName id="4C0518E38FA3FF1EFEB3F9E8416A3909" authorityName="HUBER" authorityYear="2000" box="[285,380,1616,1639]" class="Arachnida" family="Pholcidae" genus="Tupigea" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="genus">
<emphasis id="B971BF728FA3FF1EFEB3F9E8416A3909" box="[285,380,1616,1639]" italics="true" pageId="247" pageNumber="248">Tupigea</emphasis>
</taxonomicName>
; 
<taxonomicName id="4C0518E38FA3FF1EFE21F9E8424A3909" box="[399,604,1616,1639]" class="Arachnida" family="Pholcidae" genus="Coryssocnemis" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="variety" species="togata" variety="banksi">
<emphasis id="B971BF728FA3FF1EFE21F9E841E83909" box="[399,510,1616,1639]" italics="true" pageId="247" pageNumber="248">C. togata</emphasis>
, 
<emphasis id="B971BF728FA3FF1EFDBEF9E8424A3909" box="[528,604,1616,1639]" italics="true" pageId="247" pageNumber="248">banksi</emphasis>
</taxonomicName>
