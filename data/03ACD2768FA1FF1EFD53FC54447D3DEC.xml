<taxonomicName id="4C0518E38FA3FF1EFDACF98B42703924" authorityName="HUBER" authorityYear="2000" box="[514,614,1587,1610]" class="Arachnida" family="Pholcidae" genus="Aymaria" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="genus">
<emphasis id="B971BF728FA3FF1EFDACF98B42703924" box="[514,614,1587,1610]" italics="true" pageId="247" pageNumber="248">Aymaria</emphasis>
</taxonomicName>
; 
<taxonomicName id="4C0518E38FA3FF1EFDD8F98B40E63909" class="Arachnida" family="Pholcidae" genus="Coryssocnemis" kingdom="Animalia" order="Araneae" pageId="247" pageNumber="248" phylum="Arthropoda" rank="species" species="altiventer">
<emphasis id="B971BF728FA3FF1EFDD8F98B40E63909" italics="true" pageId="247" pageNumber="248">C. altiventer</emphasis>
</taxonomicName>
