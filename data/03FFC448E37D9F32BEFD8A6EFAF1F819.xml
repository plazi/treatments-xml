<subSubSection id="C34C26D5E37C9F32BEFD8F3DFAF6F8C3" box="[192,1402,1858,1885]" pageId="5" pageNumber="4" type="biology_ecology">
<paragraph id="8BE9755EE37C9F32BEFD8F3DFAF6F8C3" blockId="5.[192,1402,1858,1885]" box="[192,1402,1858,1885]" pageId="5" pageNumber="4">
