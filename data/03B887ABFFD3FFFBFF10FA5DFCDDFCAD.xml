<collectingRegion id="49D5F85FFFD3FFFBFE33F9FEFDE5F9CA" box="[411,559,1564,1588]" country="United Kingdom" name="Leicestershire" pageId="7" pageNumber="146">Leicestershire</collectingRegion>
, 
<collectingCountry id="F306762DFFD3FFFBFD93F9FEFF1FF9B3" name="United Kingdom" pageId="7" pageNumber="146">United Kingdom</collectingCountry>
. 
<typeStatus id="54AA881FFFD3FFFBFF49F9D7FE8BF9B3" box="[225,321,1589,1613]" pageId="7" pageNumber="146">Holotype</typeStatus>
of 
<taxonomicName id="4C114D3EFFD3FFFBFECFF9D7FD4CF9B5" authority=": NHML" authorityName="NHML" box="[359,646,1588,1613]" class="Actinopterygii" family="Pycnodontiformes incertae sedis" genus="Eomesodon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="7" pageNumber="146" phylum="Chordata" rank="species" species="liassicus">
