<materialsCitation id="3B4D3CBBFFF3863BFDA3D795FC99FDBB" collectorName="Hym. Typ. No." location="Department of Zoology" pageId="5" pageNumber="606" specimenCount="1" specimenCount-female="1" stateProvince="Budapest" typeStatus="paratype">
