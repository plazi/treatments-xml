, 
<number id="2E4224ACFFCCCB79A0C343F7FC2F61C8" box="[943,995,658,682]" pageId="19" pageNumber="420" value="0.28">0.28</number>
; interdistances: 
<number id="2E4224ACFFCCCB79A7F243F7FB1E61C8" box="[1182,1234,658,682]" pageId="19" pageNumber="420" value="0.22">0.22</number>
, 
