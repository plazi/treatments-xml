<taxonomicName id="5ADEC9CB8042FFE1E970C9501DCA09EE" authority="Minding, 1832" authorityName="Minding" authorityYear="1832" box="[1051,1175,1913,1938]" class="Actinopterygii" family="Lophiidae" genus="Lophioides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lophiiformes" pageId="70" pageNumber="71" phylum="Chordata" rank="genus">
<emphasis id="AFAA6E5A8042FFE1E970C9501DCA09EE" box="[1051,1175,1913,1938]" italics="true" pageId="70" pageNumber="71">Lophioides</emphasis>
</taxonomicName>
, collected off northern 
<collectingRegion id="5F1A7CAA8042FFE1EDFCC9B7187809CA" box="[151,293,1949,1974]" country="Papua New Guinea" name="New Ireland" pageId="70" pageNumber="71">New Ireland</collectingRegion>
