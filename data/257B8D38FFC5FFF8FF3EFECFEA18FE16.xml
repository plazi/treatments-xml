<taxonomicName id="6AD247ADFFC5FFF8FEBDFEA3EB02FE6B" authorityName="Ruckes" authorityYear="1966" box="[276,537,367,393]" class="Insecta" family="Pentatomidae" genus="Dryptocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="crenata">
