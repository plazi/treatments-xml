ORDER 
<taxonomicName id="4C0974AE6404FFB1FED0F97AFD25506C" authority="Gregory, 1910" authorityName="Gregory" authorityYear="1910" box="[264,740,1772,1798]" class="Mammalia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Erinaceomorpha" pageId="14" pageNumber="15" phylum="Chordata" rank="order">ERINACEOMORPHA Gregory, 1910</taxonomicName>
