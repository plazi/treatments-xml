<taxonomicName id="86E77BBAFFA23330FF2398E3A489AEB4" authority="Komai &amp; Rahayu, 2013" authorityName="Komai &amp; Rahayu" authorityYear="2013" box="[204,491,2003,2023]" class="Malacostraca" family="Paguridae" genus="Catapaguroides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="4" pageNumber="147" phylum="Arthropoda" rank="species" species="brevidactylus" status="new species">
<emphasis id="7393DC2BFFA23330FF2398E3A489AEB4" box="[204,491,2003,2023]" italics="true" pageId="4" pageNumber="147">Catapaguroides brevidactylus</emphasis>
