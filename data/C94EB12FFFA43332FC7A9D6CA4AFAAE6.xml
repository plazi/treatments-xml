<caption id="159850B1FFA23330FF6698E2A7E0A172" ID-DOI="http://doi.org/10.5281/zenodo.5351722" ID-Zenodo-Dep="5351722" httpUri="https://zenodo.org/record/5351722/files/figure.png" pageId="4" pageNumber="147" startId="4.[137,172,2002,2024]" targetBox="[157,1447,178,1969]" targetPageId="4">
<paragraph id="41580039FFA23330FF6698E2A7E0A172" blockId="4.[137,1474,2002,2082]" pageId="4" pageNumber="147">
