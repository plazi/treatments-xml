(
<bibRefCitation id="EF7C369EFFECFF9121992725C567538A" author="Brady" box="[921,1074,364,390]" pageId="5" pageNumber="50" refString="Brady, G. S. (1868) Contributions to the study of the Entomostraca. No. 2. Marine Ostracoda of the Mauritius. The Annals and Magazine of natural History, ser. 4, 2, 178 - 183." type="journal article" year="1868">
Brady, 
<number id="3D255948FFECFF9121F32725C567538A" box="[1011,1074,364,390]" pageId="5" pageNumber="34" value="1868.0">1868</number>
</bibRefCitation>
)
</taxonomicName>
