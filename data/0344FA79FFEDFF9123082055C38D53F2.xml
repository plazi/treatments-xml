<taxonomicName id="4CED30ECFFECFF9120572725C569538A" authority="Brady, 1868" authorityName="Brady" authorityYear="1868" box="[599,1084,364,390]" class="Ostracoda" family="Pontocyprididae" genus="Propontocypris" kingdom="Animalia" order="Podocopida" pageId="5" pageNumber="34" phylum="Arthropoda" rank="species" species="attenuata">
