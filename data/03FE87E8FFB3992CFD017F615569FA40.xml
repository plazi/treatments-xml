<taxonomicName id="4C574D7DFFB0992CFC137C3B527EFD88" box="[928,1084,566,587]" class="Magnoliopsida" family="Caprifoliaceae" genus="Symphoricarpos" kingdom="Plantae" order="Dipsacales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="genus">
<emphasis id="B923EAECFFB0992CFC137C3B527EFD88" box="[928,1084,566,587]" italics="true" pageId="54" pageNumber="55">Symphoricarpos</emphasis>
</taxonomicName>
, probably 
<emphasis id="B923EAECFFB0992CFD717C5D551BFDA6" box="[706,857,592,613]" italics="true" pageId="54" pageNumber="55">
<taxonomicName id="4C574D7DFFB0992CFD717C5D5517FDA6" box="[706,853,592,613]" class="Magnoliopsida" family="Caprifoliaceae" genus="Symphoricarpos" kingdom="Plantae" order="Dipsacales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="species" species="vaccinioides">S. vaccinioides</taxonomicName>
