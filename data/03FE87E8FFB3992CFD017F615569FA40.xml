<taxonomicName id="4C574D7DFFB0992CFC597C16525BFDF3" box="[1002,1049,539,560]" class="Magnoliopsida" family="Rosaceae" genus="Rosa" kingdom="Plantae" order="Rosales" pageId="54" pageNumber="55" phylum="Tracheophyta" rank="genus">
