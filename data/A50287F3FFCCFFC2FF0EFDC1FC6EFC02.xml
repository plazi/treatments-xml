<collectionCode id="4BBAAE20FFCCFFC2FF7DFD16FEC5FD0E" box="[228,288,656,680]" pageId="33" pageNumber="34">
<collectingCountry id="55BC7675FFCCFFC2FF7DFD16FEC5FD0E" box="[228,288,656,680]" name="Democratic Republic of the Congo" pageId="33" pageNumber="34">DRC</collectingCountry>
