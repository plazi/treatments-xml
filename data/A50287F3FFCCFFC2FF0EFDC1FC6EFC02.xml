<quantity id="EA539B00FFCCFFC2FE98FDEAFEAEFD22" box="[257,331,620,644]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="33" pageNumber="34" unit="mm" value="20.0">20mm</quantity>
