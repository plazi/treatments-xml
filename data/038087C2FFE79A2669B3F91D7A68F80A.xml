<taxonomicName id="4C294D57FFE79A266B67F95C7B14F8FD" authority="Fabricius 1801" authorityName="Fabricius" authorityYear="1801" box="[579,881,1772,1794]" class="Insecta" family="Curculionidae" genus="Lixus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="303" phylum="Arthropoda" rank="species" species="antilope">
