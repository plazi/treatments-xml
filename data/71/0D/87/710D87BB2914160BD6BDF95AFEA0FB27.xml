<collectionCode box="[1159,1253,1175,1199]" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="3" pageNumber="304">NHMW</collectionCode>
<number box="[1263,1321,1175,1199]" pageId="3" pageNumber="304" value="4017.0">4017</number>
