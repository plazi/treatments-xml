<taxonomicName class="Arachnida" family="Brachychthoniidae" genus="Brachychthonius" genus.bestMatchDistance="141" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" order="Sarcoptiformes" pageId="33" pageNumber="262" phylum="Arthropoda" rank="species" species="berlesei">Br. berlesei</taxonomicName>
