<emphasis box="[1226,1435,818,841]" italics="true" pageId="15" pageNumber="16">
........ 
<taxonomicName box="[1284,1435,818,841]" class="Insecta" family="Cimicidae" genus="Hesperocimex" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">Hesperocimex</taxonomicName>
