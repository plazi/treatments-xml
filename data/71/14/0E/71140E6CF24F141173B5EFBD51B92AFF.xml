<emphasis box="[334,468,1564,1590]" italics="true" pageId="38" pageNumber="39">Calvaropsis</emphasis>
<taxonomicName authority="(Pic)" authorityName="Ruta" authorityYear="2019" baseAuthorityName="Pic" baseAuthorityYear="1931" box="[475,680,1564,1591]" class="Insecta" family="Scirtidae" genus="Calvariopsis" kingdom="Animalia" order="Coleoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="sculptipenne">
