<subSubSection box="[199,1261,296,323]" pageId="26" pageNumber="27" type="etymology">
<paragraph blockId="26.[151,1436,152,323]" box="[199,1261,296,323]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[199,334,297,323]" pageId="26" pageNumber="27">Etymology.</emphasis>
