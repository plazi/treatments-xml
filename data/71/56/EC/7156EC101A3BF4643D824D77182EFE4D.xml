<collectingRegion box="[1158,1357,189,215]" isEnumeration="true" pageId="44" pageNumber="45">Maine and Ohio</collectingRegion>
in the 
<collectionCode box="[151,236,226,252]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="44" pageNumber="45" type="Museum">USNM</collectionCode>
collection. 
<collectingCounty box="[373,505,226,252]" pageId="44" pageNumber="45">Distributed</collectingCounty>
