<location LSID="urn:lsid:plazi:treatment:7156EC101A3BF4643D824D77182EFE4D:FC200BDD1A3CF4643C964E021E50FED8" box="[387,534,264,290]" county="Distributed" municipality="Dallas-Fort Worth area" name="Brownsville" pageId="44" pageNumber="45" stateProvince="Maine and Ohio">Brownsville</location>
