<taxonomicName box="[424,594,858,884]" class="Magnoliopsida" family="Fabaceae" genus="Trifolium" kingdom="Plantae" order="Fabales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[424,544,858,883]" italics="true" pageId="45" pageNumber="46">Trifolium</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName box="[611,736,858,884]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
