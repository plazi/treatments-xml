<taxonomicName authority="F. H. Wigg." authorityName="F. H. Wigg." box="[161,548,970,996]" class="Magnoliopsida" family="Asteraceae" genus="Taraxacum" kingdom="Plantae" order="Asterales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="officinale">
