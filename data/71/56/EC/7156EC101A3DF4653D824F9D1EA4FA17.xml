<taxonomicName box="[903,1003,858,884]" class="Liliopsida" family="Poaceae" genus="Poa" kingdom="Plantae" order="Poales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[903,953,858,883]" italics="true" pageId="45" pageNumber="46">Poa</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName authorityName="Barnhart" authorityYear="1895" baseAuthorityName="R.Br." box="[1020,1130,858,884]" class="Liliopsida" family="Poaceae" kingdom="Plantae" order="Poales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="family">Poaceae</taxonomicName>
