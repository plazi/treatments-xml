<quantity box="[646,805,404,430]" metricMagnitude="-3" metricUnit="m" metricValue="2.825" metricValueMax="3.05" metricValueMin="2.6" pageId="15" pageNumber="16" unit="mm" value="2.825" valueMax="3.05" valueMin="2.6">2.6–3.05 mm</quantity>
