<quantity box="[468,624,444,470]" metricMagnitude="-3" metricUnit="m" metricValue="3.425" metricValueMax="3.8" metricValueMin="3.05" pageId="15" pageNumber="16" unit="mm" value="3.425" valueMax="3.8" valueMin="3.05">3.05–3.8 mm</quantity>
long (mean = 
<quantity box="[795,900,444,470]" metricMagnitude="-3" metricUnit="m" metricValue="3.34" pageId="15" pageNumber="16" unit="mm" value="3.34">3.34 mm</quantity>
