. 
<taxonomicName authority="Hernandes, 2020" authorityName="Hernandes" authorityYear="2020" box="[281,504,1336,1360]" class="Arachnida" family="Gabuciniidae" genus="Gabucinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="neotropica" status="sp. nov.">
