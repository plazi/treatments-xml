<paragraph blockId="12.[151,1436,1205,2037]" pageId="12" pageNumber="513">
<emphasis box="[199,326,1645,1668]" italics="true" pageId="12" pageNumber="513">
<taxonomicName box="[199,322,1645,1668]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="12" pageNumber="513" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
