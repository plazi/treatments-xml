<paragraph blockId="17.[151,1437,251,1304]" pageId="17" pageNumber="518">
<emphasis box="[199,326,655,678]" italics="true" pageId="17" pageNumber="518">
<taxonomicName box="[199,322,655,678]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="17" pageNumber="518" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
