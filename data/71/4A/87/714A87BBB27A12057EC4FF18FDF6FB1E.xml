<paragraph blockId="21.[151,1437,258,1238]" pageId="21" pageNumber="522">
<emphasis box="[199,326,625,648]" italics="true" pageId="21" pageNumber="522">
<taxonomicName box="[199,322,625,648]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="21" pageNumber="522" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
