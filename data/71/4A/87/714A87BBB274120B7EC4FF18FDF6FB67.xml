<paragraph blockId="27.[151,1437,258,1200]" pageId="27" pageNumber="528">
<emphasis box="[199,326,553,576]" italics="true" pageId="27" pageNumber="528">
<taxonomicName box="[199,322,553,576]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="27" pageNumber="528" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
