</taxonomicName>
, 
<taxonomicName authority="Strand" authorityName="Strand" box="[151,530,1679,1704]" class="Insecta" family="Crambidae" genus="Chilo" kingdom="Animalia" order="Lepidoptera" pageId="16" pageNumber="517" phylum="Arthropoda" rank="species" species="orichalcociliellus">
