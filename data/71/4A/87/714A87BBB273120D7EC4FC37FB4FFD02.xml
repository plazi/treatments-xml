<paragraph blockId="28.[151,1437,1053,2033]" pageId="28" pageNumber="529">
<emphasis bold="true" box="[199,266,1757,1782]" pageId="28" pageNumber="529">Color</emphasis>
. Head and 
<taxonomicName box="[411,530,1759,1782]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="28" pageNumber="543" phylum="Arthropoda" rank="genus">mesosoma</taxonomicName>
