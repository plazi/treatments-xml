<paragraph blockId="32.[151,1437,1234,2034]" pageId="32" pageNumber="533">
<emphasis box="[199,326,1673,1696]" italics="true" pageId="32" pageNumber="533">
<taxonomicName box="[199,322,1673,1696]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="32" pageNumber="533" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
