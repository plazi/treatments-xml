<paragraph blockId="23.[151,1437,151,910]" pageId="23" pageNumber="524">
<emphasis box="[199,326,153,176]" italics="true" pageId="23" pageNumber="524">
<taxonomicName box="[199,322,153,176]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="23" pageNumber="524" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
