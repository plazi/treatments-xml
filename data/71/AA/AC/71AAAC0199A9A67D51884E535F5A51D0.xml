<paragraph pageNumber="263">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33573" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole ajax Forel" lsidName-HNS="Pheidole ajax Forel" order="Hymenoptera" pageNumber="263" phylum="Arthropoda" rank="species" species="ajax">Pheidole ajax</taxonomicName>
