, lateral view; 39, 
<taxonomicName authority="(Lundbeck)" baseAuthorityName="Lundbeck" baseAuthorityYear="1923" class="Insecta" family="Sciomyzidae" genus="Colobaea" kingdom="Animalia" order="Diptera" pageId="42" pageNumber="43" phylum="Arthropoda" rank="species" species="punctata">
