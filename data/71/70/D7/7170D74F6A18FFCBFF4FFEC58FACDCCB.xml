: at 
<collectingMunicipality box="[180,301,955,981]" pageId="26" pageNumber="27">Snøgedam</collectingMunicipality>
, 
<collectingCounty box="[314,407,955,981]" pageId="26" pageNumber="27">Hillerød</collectingCounty>
(
<specimenCode box="[423,565,954,981]" collectionCode="FT" country="Italy" lsid="urn:lsid:biocol.org:col:15179" name="Centro Studi Erbario Tropicale, Università degli Studi di Firenze" pageId="26" pageNumber="27" type="Herbarium">FT 6012–15</specimenCode>
) (
<date box="[590,881,954,982]" pageId="26" pageNumber="27" value="1960-07-05" valueMax="1960-09-01" valueMin="1960-07-05">
<collectingDate box="[590,881,954,982]" pageId="26" pageNumber="27" value="1960-07-05" valueMax="1960-09-01" valueMin="1960-07-05">5 July–1 September 1960</collectingDate>
</date>
) and at Vollerup Mose, near Sorø (
