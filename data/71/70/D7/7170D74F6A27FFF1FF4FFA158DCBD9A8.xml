, 
<collectingCountry box="[862,923,1541,1567]" name="United States of America" pageId="37" pageNumber="38">USA</collectingCountry>
). 
<location LSID="urn:lsid:plazi:treatment:7170D74F6A27FFF1FF4FFA158DCBD9A8:FC0630826A27FFF2FC76F9AD8DD8DA07" box="[942,1021,1541,1567]" country="United States of America" county="Chorion" municipality="Ventral" name="Length" pageId="37" pageNumber="38" stateProvince="New York">Length</location>
<quantity box="[1029,1195,1541,1568]" metricMagnitude="-4" metricUnit="m" metricValue="3.8" metricValueMax="4.0" metricValueMin="3.5999999999999996" pageId="37" pageNumber="38" unit="mm" value="0.38" valueMax="0.4" valueMin="0.36">0.36–0.40 mm</quantity>
