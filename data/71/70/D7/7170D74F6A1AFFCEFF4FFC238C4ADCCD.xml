</taxonomicName>
and 
<taxonomicName box="[665,821,151,177]" class="Insecta" family="Sciomyzidae" genus="Colobaea" kingdom="Animalia" order="Diptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="flavipleura">
