<emphasis box="[568,703,512,536]" italics="true" pageId="3" pageNumber="58">C. biserrata</emphasis>
</taxonomicName>
, 
<taxonomicName box="[717,830,512,536]" class="Malacostraca" family="Nannastacidae" genus="Cumella" kingdom="Animalia" order="Cumacea" pageId="3" pageNumber="58" phylum="Arthropoda" rank="species" species="serrata">
<emphasis box="[717,830,512,536]" italics="true" pageId="3" pageNumber="58">C. serrata</emphasis>
