<taxonomicName box="[844,1018,512,536]" class="Malacostraca" family="Nannastacidae" genus="Cumella" kingdom="Animalia" order="Cumacea" pageId="3" pageNumber="58" phylum="Arthropoda" rank="species" species="meridionalis">
<emphasis box="[844,1018,512,536]" italics="true" pageId="3" pageNumber="58">C. meridionalis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1031,1163,512,536]" class="Malacostraca" family="Nannastacidae" genus="Cumella" kingdom="Animalia" order="Cumacea" pageId="3" pageNumber="58" phylum="Arthropoda" rank="species" species="spinifera">
<emphasis box="[1031,1163,512,536]" italics="true" pageId="3" pageNumber="58">C. spinifera</emphasis>
