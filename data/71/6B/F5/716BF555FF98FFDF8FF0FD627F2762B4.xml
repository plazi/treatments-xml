<paragraph blockId="19.[151,788,749,775]" box="[151,788,749,775]" pageId="19" pageNumber="20">
<heading bold="true" box="[151,788,749,775]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis bold="true" box="[151,788,749,775]" pageId="19" pageNumber="20">
<taxonomicName authority="Nguyen &amp; Bae, 2004" authorityName="Nguyen &amp; Bae" authorityYear="2004" box="[151,604,749,775]" class="Insecta" family="Heptageniidae" genus="Iron" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ephemeroptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="longitibius">
<emphasis bold="true" box="[151,341,749,775]" italics="true" pageId="19" pageNumber="20">Iron longitibius</emphasis>
Nguyen &amp; Bae, 
<number box="[545,604,749,775]" pageId="19" pageNumber="20" value="2004.0">2004</number>
</taxonomicName>
(questionable)
</emphasis>
</heading>
</paragraph>
