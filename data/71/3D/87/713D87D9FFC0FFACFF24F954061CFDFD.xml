<subSubSection box="[199,1287,1861,1887]" pageId="2" pageNumber="139" type="etymology">
<paragraph blockId="2.[151,1436,1861,2037]" box="[199,1287,1861,1887]" pageId="2" pageNumber="139">
<emphasis bold="true" box="[199,328,1861,1887]" pageId="2" pageNumber="139">Etymology</emphasis>
