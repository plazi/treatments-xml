<taxonomicName id="B78B4D14FFC9177DFF78FE69FDD2FE57" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[151,595,422,448]" class="Clitellata" family="Piscicolidae" genus="Piscicola" kingdom="Animalia" order="Rhynchobdellida" pageId="10" pageNumber="42" phylum="Annelida" rank="species" species="geometra">
