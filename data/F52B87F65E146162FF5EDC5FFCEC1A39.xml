<location id="785D603B5E146163FDFADCD0FDCC1F99" LSID="urn:lsid:plazi:treatment:F52B87F65E146162FF5EDC5FFCEC1A39:785D603B5E146163FDFADCD0FDCC1F99" box="[563,605,1681,1703]" county="This" latitude="46.537" longLatPrecision="67" longitude="6.192" municipality="Swiss" name="The" pageId="34" pageNumber="167" stateProvince="Jura">The</location>
