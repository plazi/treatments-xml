E, 
<quantity id="BA7A9B055E146163FE07DCD1FD8D1F98" box="[462,540,1680,1702]" metricMagnitude="3" metricUnit="m" metricValue="1.45" pageId="34" pageNumber="167" unit="m" value="1450.0">
<elevation id="F6AFD1D35E146163FE07DCD1FD8D1F98" box="[462,540,1680,1702]" metricMagnitude="3" metricUnit="m" metricValue="1.45" pageId="34" pageNumber="167" unit="m" value="1450.0">1450 m</elevation>
