<quantity id="4CB947DAFFE39A278D7925BBCCE77969" box="[587,663,1022,1044]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="5" pageNumber="330" unit="mm" value="2.4">2,4mm</quantity>
