<taxonomicName id="6CB0802749E5AFE1023F46FE6BEE6DEA" class="Insecta" family="Corydiidae" genus="Eucorydia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucorydia tristis" order="Blattodea" pageId="26" pageNumber="43" phylum="Arthropoda" rank="species" species="tristis">Eucorydia tristis</taxonomicName>
