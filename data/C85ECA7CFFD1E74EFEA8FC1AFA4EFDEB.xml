<specimenCount id="56F1B0E3FFD1E74EFBD5FEBCFB29FE88" box="[1151,1266,311,335]" pageId="3" pageNumber="135" type="female">2 females</specimenCount>
, on 
<taxonomicName id="87F700E9FFD1E74EFA81FEB3FCBCFEB5" class="Magnoliopsida" family="Malvaceae" genus="Urena" kingdom="Plantae" order="Malvales" pageId="3" pageNumber="135" phylum="Tracheophyta" rank="species" species="lobata">
<emphasis id="7283A778FFD1E74EFA81FEB3FCBCFEB5" italics="true" pageId="3" pageNumber="135">Urena lobata</emphasis>
