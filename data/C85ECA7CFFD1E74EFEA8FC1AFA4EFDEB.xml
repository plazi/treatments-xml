<date id="34495DAAFFD1E74EFA97FED1FCA9FE52" pageId="3" pageNumber="135" value="2006-08">August 2006</date>
; 
<specimenCount id="56F1B0E3FFD1E74EFC2AFEF6FC2AFE52" box="[896,1009,381,405]" pageId="3" pageNumber="135" type="female">2 females</specimenCount>
on 
<taxonomicName id="87F700E9FFD1E74EFB88FEF6FAF4FE52" box="[1058,1327,381,405]" class="Magnoliopsida" family="Anacardiaceae" genus="Anacardium" kingdom="Plantae" order="Sapindales" pageId="3" pageNumber="135" phylum="Tracheophyta" rank="species" species="occidentalis">
<emphasis id="7283A778FFD1E74EFB88FEF6FAF4FE52" box="[1058,1327,381,405]" italics="true" pageId="3" pageNumber="135">Anacardium occidentalis</emphasis>
</taxonomicName>
L., 
<specimenCount id="56F1B0E3FFD1E74EFACAFEF6FCA5FE7F" pageId="3" pageNumber="135" type="female">5 females</specimenCount>
and 
<specimenCount id="56F1B0E3FFD1E74EFC11FE2BFBD2FE7F" box="[955,1033,416,440]" pageId="3" pageNumber="135" type="male">1 male</specimenCount>
on 
<taxonomicName id="87F700E9FFD1E74EFB9DFE2AFB30FE7F" box="[1079,1259,417,440]" class="Magnoliopsida" family="Moraceae" genus="Ficus" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="135" phylum="Tracheophyta" rank="species" species="sycomorus">
