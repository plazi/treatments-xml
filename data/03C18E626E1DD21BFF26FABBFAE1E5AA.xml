<taxonomicName id="4C6844F76E1DD21BFC7CF9D4FB9AE588" box="[936,1125,1653,1677]" class="Gastropoda" family="Muricidae" genus="Pterynotus" kingdom="Animalia" order="Neogastropoda" pageId="10" pageNumber="447" phylum="Mollusca" rank="species" species="undetermined">
