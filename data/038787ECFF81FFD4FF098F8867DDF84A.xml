<taxonomicName id="4C2E4D79FF81FFD4FD938E446494F8B6" authorityName="Viereck" authorityYear="1911" box="[525,704,1874,1900]" class="Insecta" family="Braconidae" genus="Dolichogenidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="409" phylum="Arthropoda" rank="genus">
