<taxonomicName id="4C0F4D55FFB5FF9CC2EEFCB2FE1CFC6D" box="[151,394,875,897]" class="Magnoliopsida" family="Sarraceniaceae" genus="Sarracenia" kingdom="Plantae" order="Ericales" pageId="90" pageNumber="245" phylum="Tracheophyta" rank="species" species="leucophylla">
<emphasis id="B97BEAC4FFB5FF9CC2EEFCB2FE1CFC6D" bold="true" box="[151,394,875,897]" italics="true" pageId="90" pageNumber="245">Sarracenia leucophylla</emphasis>
</taxonomicName>
