<taxonomicName box="[379,467,1473,1491]" class="Liliopsida" family="Arecaceae" genus="Nenga" kingdom="Plantae" order="Arecales" pageId="22" pageNumber="155" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[379,433,1473,1491]" italics="true" pageId="22" pageNumber="155">Nenga</emphasis>
sp.
</taxonomicName>
aff. 
<taxonomicName authorityName="Bianucci, Lambert &amp; Post" authorityYear="2007" box="[504,666,1473,1491]" class="Liliopsida" family="Arecaceae" genus="Nenga" kingdom="Plantae" order="Arecales" pageId="22" pageNumber="155" phylum="Tracheophyta" rank="species" species="meganasalis">
