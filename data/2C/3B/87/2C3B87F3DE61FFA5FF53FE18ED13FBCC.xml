<taxonomicName baseAuthorityName="Belokobylskij" baseAuthorityYear="2002" box="[254,433,1047,1072]" class="Insecta" family="Braconidae" genus="Termitospathius" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="382" phylum="Arthropoda" rank="genus">
