, 
<number box="[533,585,1941,1965]" pageId="14" pageNumber="531" value="0.21">0.21</number>
, 
<number box="[596,649,1941,1965]" pageId="14" pageNumber="531" value="0.48">0.48</number>
; leg II 
<number box="[729,781,1941,1965]" pageId="14" pageNumber="531" value="0.19">0.19</number>
, 
<number box="[792,844,1940,1965]" pageId="14" pageNumber="531" value="0.56">0.56</number>
, 
<number box="[856,908,1941,1965]" pageId="14" pageNumber="531" value="0.24">0.24</number>
, 
<number box="[920,972,1940,1965]" pageId="14" pageNumber="531" value="0.46">0.46</number>
, 
<number box="[984,1036,1941,1965]" pageId="14" pageNumber="531" value="0.2">0.20</number>
, 
<number box="[1047,1099,1941,1965]" pageId="14" pageNumber="531" value="0.47">0.47</number>
; leg III 
<number box="[1189,1241,1941,1965]" pageId="14" pageNumber="531" value="0.18">0.18</number>
, 
<number box="[1253,1305,1941,1965]" pageId="14" pageNumber="531" value="0.44">0.44</number>
, 
<number box="[1316,1368,1941,1965]" pageId="14" pageNumber="531" value="0.19">0.19</number>
, 
<number box="[1380,1432,1941,1965]" pageId="14" pageNumber="531" value="0.31">0.31</number>
, 
