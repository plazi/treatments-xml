<collectingCountry box="[359,426,1876,1901]" name="China" pageId="2" pageNumber="497">China</collectingCountry>
(
<collectingRegion box="[441,525,1876,1901]" country="China" name="Jiangxi" pageId="2" pageNumber="497">Jiangxi</collectingRegion>
) (
<figureCitation box="[547,606,1876,1901]" captionStart="FIGURE 1" captionStartId="2.[151,250,1275,1297]" captionTargetBox="[188,1398,195,1251]" captionTargetId="figure@2.[188,1398,195,1251]" captionTargetPageId="2" captionText="FIGURE 1. Map of China showing the recorded localities of the genus Lucasioides (L.)." httpUri="https://zenodo.org/record/242380/files/figure.png" pageId="2" pageNumber="497">Fig 1</figureCitation>
