<number box="[523,531,1869,1883]" pageId="5" pageNumber="44" value="1.0">
<emphasis box="[523,531,1869,1883]" italics="true" pageId="5" pageNumber="44">1</emphasis>
</number>
and 
<emphasis box="[585,598,1853,1878]" italics="true" pageId="5" pageNumber="44">c</emphasis>
<number box="[598,606,1869,1883]" pageId="5" pageNumber="44" value="2.0">
<emphasis box="[598,606,1869,1883]" italics="true" pageId="5" pageNumber="44">2</emphasis>
</number>
