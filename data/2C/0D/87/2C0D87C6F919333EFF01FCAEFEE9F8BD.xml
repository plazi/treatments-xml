<emphasis box="[1119,1132,1492,1517]" italics="true" pageId="10" pageNumber="49">c</emphasis>
<number box="[1132,1140,1508,1522]" pageId="10" pageNumber="49" value="1.0">
<emphasis box="[1132,1140,1508,1522]" italics="true" pageId="10" pageNumber="49">1</emphasis>
</number>
