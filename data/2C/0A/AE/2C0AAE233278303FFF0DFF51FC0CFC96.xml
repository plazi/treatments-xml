. 
<number box="[207,271,514,540]" pageId="5" pageNumber="48" value="2002.0">2002</number>
</date>
, S 
<geoCoordinate box="[298,407,514,540]" direction="east" orientation="longitude" pageId="5" pageNumber="48" precision="925" value="34.333332">
<number box="[298,331,514,540]" pageId="5" pageNumber="48" value="34.0">34</number>
° 
