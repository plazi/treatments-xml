<taxonomicName authority="GRUVEL, 1905" authorityName="GRUVEL" authorityYear="1905" box="[303,694,1431,1456]" class="Maxillopoda" family="Dendrogastridae" kingdom="Animalia" order="Dendrogastrida" pageId="34" pageNumber="823" phylum="Arthropoda" rank="family">DENDROGASTRIDAE GRUVEL, 1905</taxonomicName>
</heading>
