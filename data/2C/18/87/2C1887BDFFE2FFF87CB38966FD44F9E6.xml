<subSubSection pageId="40" pageNumber="829" type="diagnosis">
<paragraph blockId="40.[145,761,1214,1298]" pageId="40" pageNumber="829">
<emphasis box="[145,264,1215,1236]" italics="true" pageId="40" pageNumber="829">Diagnosis:</emphasis>
