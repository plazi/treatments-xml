<emphasis box="[597,703,1591,1615]" italics="true" pageId="48" pageNumber="49">A. boltoni</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[709,793,1592,1615]" pageId="48" pageNumber="49" rank="species">sp. nov.</taxonomicNameLabel>
and 
<taxonomicName authorityName="Emery" authorityYear="1895" box="[844,953,1591,1615]" class="Insecta" family="Formicidae" genus="Aenictus" kingdom="Animalia" order="Hymenoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="mariae">A. mariae</taxonomicName>
