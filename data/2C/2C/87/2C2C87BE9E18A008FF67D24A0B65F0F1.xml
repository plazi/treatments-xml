<collectionCode box="[247,336,232,258]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="104" pageNumber="105" type="Museum">USNM</collectionCode>
