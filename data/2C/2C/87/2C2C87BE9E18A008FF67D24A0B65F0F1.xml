from 
<collectingCountry box="[1202,1352,192,218]" name="South Africa" pageId="104" pageNumber="105">South Africa</collectingCountry>
(
<collectionCode box="[1367,1432,192,218]" country="Canada" httpUri="http://grbio.org/cool/y2kv-9w3k" name="Canadian National Collection of Insects, Arachnids, and Nematodes" pageId="104" pageNumber="105">CNC</collectionCode>
, 
<collectionCode box="[151,234,232,258]" country="USA" httpUri="http://grbio.org/cool/m2jc-wmeq" name="University of California, Riverside" pageId="104" pageNumber="105" type="University or college">UCRC</collectionCode>
