to 
<geoCoordinate box="[930,1078,764,788]" direction="north" orientation="latitude" pageId="43" pageNumber="494" precision="1" value="12.201633">12°12.098´N</geoCoordinate>
, intertidal– 
<specimenCount box="[1208,1255,764,789]" pageId="43" pageNumber="494" type="generic">
<quantity box="[1208,1251,764,789]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="43" pageNumber="494" unit="m" value="3.0">3 m</quantity>
