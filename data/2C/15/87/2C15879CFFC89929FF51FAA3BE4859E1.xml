<taxonomicName baseAuthorityName="Martins" baseAuthorityYear="1842" box="[628,810,1447,1472]" class="Mammalia" family="Muridae" genus="Microtus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="5" pageNumber="216" phylum="Chordata" rank="species" species="nivalis">
