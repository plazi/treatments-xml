(
<bibRefCitation author="Friese" box="[1294,1428,248,271]" pageId="3" pageNumber="52" refString="Friese, H. (1903). Neue Meliponiden II. (Hym.). Zeitschrift fur systematische Hymenopterologie und Dipterologie, 3, 359 - 361." type="journal article" year="1903">
Friese, 
<number box="[1374,1428,248,271]" pageId="3" pageNumber="40" value="1903.0">1903</number>
</bibRefCitation>
)
</keyLead>
</paragraph>
<paragraph blockId="3.[151,1436,152,335]" pageId="3" pageNumber="40">
<keyLead pageId="3" pageNumber="40">
