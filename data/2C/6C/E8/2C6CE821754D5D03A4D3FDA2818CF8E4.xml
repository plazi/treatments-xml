<taxonomicName authorityName="J. V. Lamouroux" authorityYear="1813" box="[235,354,1749,1774]" class="Florideophyceae" family="Rhodomelaceae" genus="Laurencia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ceramiales" pageId="7" pageNumber="48" phylum="Rhodophyta" rank="genus">
