</taxonomicName>
; 
<taxonomicName authority="Norman and Stebbing, 1886" authorityName="Norman and Stebbing" authorityYear="1886" box="[733,1167,886,911]" class="Malacostraca" family="Tanaellidae" genus="Tanaella" kingdom="Animalia" order="Tanaidacea" pageId="3" pageNumber="551" phylum="Arthropoda" rank="genus">
