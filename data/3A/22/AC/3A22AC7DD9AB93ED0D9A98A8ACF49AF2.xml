<taxonomicName authorityName="Hopkins" authorityYear="1915" class="Insecta" family="Curculionidae" genus="Coptodryas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coptodryas confusa" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="confusa">
