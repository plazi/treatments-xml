<emphasis box="[294,473,1708,1730]" italics="true" pageId="22" pageNumber="423">Niphargus sharifi</emphasis>
</taxonomicName>
, 
<emphasis bold="true" box="[486,567,1708,1729]" pageId="22" pageNumber="423">
<taxonomicNameLabel box="[486,567,1708,1729]" pageId="22" pageNumber="423" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, Sarab-e-Robat, male 
<quantity box="[795,866,1708,1730]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="22" pageNumber="423" unit="mm" value="19.0">19 mm</quantity>
(
<typeStatus box="[880,974,1708,1730]" pageId="22" pageNumber="423">holotype</typeStatus>
). A, PL 
<number box="[1058,1071,1708,1730]" pageId="22" pageNumber="423" value="1.0">1</number>
; B, PL 
<number box="[1151,1166,1708,1730]" pageId="22" pageNumber="423" value="2.0">2</number>
; C, PL 
<number box="[1243,1257,1708,1730]" pageId="22" pageNumber="423" value="3.0">3</number>
; D, 
<collectionCode box="[1301,1359,1708,1730]" httpUri="http://grbio.org/cool/11pi-mg53" name="Universiti Sains Malaysia" pageId="22" pageNumber="423">USM</collectionCode>
; E, U 
<number box="[1418,1431,1708,1730]" pageId="22" pageNumber="423" value="2.0">2</number>
; F, U 
<number box="[200,215,1741,1763]" pageId="22" pageNumber="423" value="3.0">3</number>
; G, EP 
<number box="[290,327,1741,1763]" pageId="22" pageNumber="423" value="2.0" valueMax="3.0" valueMin="1.0">1–3</number>
; H, T. Scale bars: 
<number box="[513,523,1741,1763]" pageId="22" pageNumber="423" value="1.0">1</number>
= 
<quantity box="[546,623,1741,1763]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="22" pageNumber="423" unit="mm" value="0.5">0.5 mm</quantity>
(G–H); 
<number box="[708,721,1741,1763]" pageId="22" pageNumber="423" value="2.0">2</number>
= 
<quantity box="[741,793,1741,1763]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="22" pageNumber="423" unit="mm" value="1.0">1mm</quantity>
(A–E); 
<number box="[874,886,1741,1763]" pageId="22" pageNumber="423" value="3.0">3</number>
= 
<quantity box="[907,959,1741,1763]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="22" pageNumber="423" unit="mm" value="2.0">2mm</quantity>
