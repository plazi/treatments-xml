<taxonomicName authorityName="Casey" authorityYear="1884" class="Hexapoda" family="Carabidae" genus="Harpalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="693" pageNumber="694" phylum="Arthropoda" rank="species" species="lustrans">
