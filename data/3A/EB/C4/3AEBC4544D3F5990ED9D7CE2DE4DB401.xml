<paragraph pageId="693" pageNumber="694">
<taxonomicName authorityName="Csiki" authorityYear="1932" class="Hexapoda" family="Carabidae" genus="Harpalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="693" pageNumber="694" phylum="Arthropoda" rank="species" species="maxwellensis">Harpalus maxwellensis</taxonomicName>
