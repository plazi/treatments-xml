<taxonomicName authorityName="Casey" authorityYear="1914" class="Hexapoda" family="Carabidae" genus="Harpalus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="692" pageNumber="693" phylum="Arthropoda" rank="species" species="illectus">Harpalus illectus</taxonomicName>
