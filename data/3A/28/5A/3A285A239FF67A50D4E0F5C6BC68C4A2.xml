<paragraph pageId="16" pageNumber="395">
<taxonomicName LSID="urn:lsid:zoobank.org:act:8311E1B3-19D9-4D87-ABAF-15E4971D5721" authority="Sharkey" authorityName="Foerster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Lytopylus rebeccashapleyae" order="Hymenoptera" pageId="16" pageNumber="395" phylum="Arthropoda" rank="species" species="rebeccashapleyae">Lytopylus rebeccashapleyae Sharkey</taxonomicName>
