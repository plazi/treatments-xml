<subSubSection lastPageId="3" lastPageNumber="194" pageId="2" pageNumber="193" type="etymology">
<paragraph blockId="2.[151,1127,1418,1601]" pageId="2" pageNumber="193">
<emphasis bold="true" box="[151,280,1546,1569]" pageId="2" pageNumber="193">Etymology.</emphasis>
