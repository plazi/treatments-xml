<paragraph pageId="null" pageNumber="669">
<taxonomicName authority="L." class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="669" phylum="Tracheophyta" rank="species" species="falcata">
