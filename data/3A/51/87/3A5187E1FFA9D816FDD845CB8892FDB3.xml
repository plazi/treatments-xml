<specimenCount box="[824,935,1519,1543]" pageId="20" pageNumber="1150" type="male">one male</specimenCount>
) deposited in 
<collectionCode box="[144,232,1551,1575]" country="Denmark" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="20" pageNumber="1150">ZMUC</collectionCode>
; 
<specimenCount box="[243,473,1551,1576]" pageId="20" pageNumber="1150" type="female" typeStatus="paratype">one paratype female</specimenCount>
deposited in 
<collectionCode box="[623,698,1551,1575]" pageId="20" pageNumber="1150">OPPC</collectionCode>
, 
<specimenCount box="[708,950,1551,1576]" pageId="20" pageNumber="1150" type="female" typeStatus="paratype">two paratype females</specimenCount>
