<taxonomicName baseAuthorityName="Uhler" baseAuthorityYear="1877" box="[1295,1436,153,176]" class="Insecta" family="Cydnidae" genus="Tominotus" kingdom="Animalia" order="Hemiptera" pageId="30" pageNumber="431" phylum="Arthropoda" rank="species" species="communis">
