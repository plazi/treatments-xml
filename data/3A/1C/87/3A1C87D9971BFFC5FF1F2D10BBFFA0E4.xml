<taxonomicName baseAuthorityName="Golovatch" baseAuthorityYear="1992" box="[734,866,1586,1612]" class="Diplopoda" family="Paradoxosomatidae" genus="Delarthrum" kingdom="Animalia" order="Polydesmida" pageId="19" pageNumber="504" phylum="Arthropoda" rank="genus">Delarthrum</taxonomicName>
