<taxonomicName baseAuthorityName="Golovatch" baseAuthorityYear="1992" box="[661,793,1514,1540]" class="Diplopoda" family="Paradoxosomatidae" genus="Delarthrum" kingdom="Animalia" order="Polydesmida" pageId="19" pageNumber="504" phylum="Arthropoda" rank="genus">Delarthrum</taxonomicName>
