<taxonomicName authority="(Pocock, 1895)" baseAuthorityName="Pocock" baseAuthorityYear="1895" box="[348,704,1486,1510]" class="Diplopoda" family="Paradoxosomatidae" genus="Pocockina" kingdom="Animalia" order="Polydesmida" pageId="21" pageNumber="506" phylum="Arthropoda" rank="species" species="pilifera">
