<collectingDate box="[428,545,617,641]" pageId="9" pageNumber="10" value="1959-12-28">28.12.1959</collectingDate>
</date>
, leg. 
<collectorName box="[596,756,617,641]" pageId="9" pageNumber="10">R.B. Cumming</collectorName>
(
<collectionCode box="[159,243,649,673]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" lsid="urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="9" pageNumber="10" type="Museum">FMNH</collectionCode>
)
