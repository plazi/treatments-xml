<subSubSection type="nomenclature">
<paragraph pageNumber="398">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181858" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole charazana Wilson" lsidName-HNS="Pheidole charazana Wilson" order="Hymenoptera" pageNumber="398" phylum="Arthropoda" rank="species" species="charazana">Pheidole charazana</taxonomicName>
