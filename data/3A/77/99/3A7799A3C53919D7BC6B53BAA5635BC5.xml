<paragraph pageId="67" pageNumber="68">
<taxonomicName authority="Abeille de Perrin, 1879" authorityName="Abeille de Perrin" authorityYear="1879" class="Insecta" family="Chrysididae" genus="Philoctetes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Philoctetes micans" order="Hymenoptera" pageId="67" pageNumber="68" phylum="Arthropoda" rank="species" species="micans">Philoctetes micans</taxonomicName>
