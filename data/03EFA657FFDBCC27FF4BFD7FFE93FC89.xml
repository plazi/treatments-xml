<quantity id="4CBEBAA4FFDBCC26FEF2F96AFE26F8B7" box="[302,421,1784,1810]" metricMagnitude="-2" metricUnit="m" metricValue="1.25" metricValueMax="1.3" metricValueMin="1.2" pageId="24" pageNumber="25" unit="mm" value="12.5" valueMax="13.0" valueMin="12.0">12–13mm</quantity>
