<taxonomicName id="7FC26785432063AEF741CE522A0C5D61" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:188600" authority="Agassiz, 1846" authorityName="Agassiz" authorityYear="1846" class="Insecta" family="Formicidae" genus="Cremastogaster" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Cremastogaster inermis Sewellii var. dentatus Forel" order="Hymenoptera" pageNumber="196" phylum="Arthropoda" rank="variety" species="sewellii" status="nov. var." variety="dentatus">Cremastogaster Sewellii, var. dentatus</taxonomicName>
, 
<taxonomicNameLabel id="E47EC54714E43C3A3A6B17317CFEB781" rank="variety">nov. var.</taxonomicNameLabel>
</paragraph>
