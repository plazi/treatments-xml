<taxonomicName id="E3E18191FF5EFF571DAFFDA1FCC6FBB3" authorityName="Timmermann &amp; Kuhlmann" authorityYear="2009" box="[728,847,1021,1046]" class="Insecta" family="Halictidae" genus="Patellapis" kingdom="Animalia" order="Hymenoptera" pageId="180" pageNumber="181" phylum="Arthropoda" rank="genus">
