<heading id="D0C481B3FF9FFF80FCF7FB81FB9EFB8B" box="[839,1087,1080,1104]" centered="true" fontSize="36" level="2" pageId="31" pageNumber="32" reason="8">
<emphasis id="B947EACDFF9FFF80FCF7FB81FC16FB94" bold="true" box="[839,951,1080,1104]" pageId="31" pageNumber="32">
<taxonomicName id="4C334D5CFF9FFF80FCF7FB81FC11FB94" authority="BLAGODEROV &amp; GRIMALDI, 2004" authorityName="BLAGODEROV &amp; GRIMALDI" authorityYear="2004" box="[839,944,1080,1104]" class="Insecta" family="Mycetophilidae" genus="Gregikia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="genus" status="new genus">
