<collectingRegion id="49828DA6F3177F7017C7A38DFBBCFDA8" box="[1007,1103,510,535]" country="Angola" name="Huambo" pageId="19" pageNumber="220">Huambo</collectingRegion>
and 
<collectingRegion id="49828DA6F3177F7010AEA38DFB5EFDA8" box="[1158,1197,510,535]" country="Angola" name="Bie" pageId="19" pageNumber="220">Bié</collectingRegion>
