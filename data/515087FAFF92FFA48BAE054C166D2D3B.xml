GROUVELLE, 1909 (
<collectingCountry id="A1EE767CFF91FFA488B505DD16DA2FA3" box="[908,960,387,409]" name="Serbia" pageId="2" pageNumber="1423">Mali</collectingCountry>
, 
<collectingCountry id="A1EE767CFF91FFA488E905DD11052FA3" box="[976,1055,387,409]" name="Nigeria" pageId="2" pageNumber="1423">Nigeria</collectingCountry>
, 
<collectingCountry id="A1EE767CFF91FFA48BAE05FF15C32F8D" box="[151,217,417,439]" name="Sudan" pageId="2" pageNumber="1423">Sudan</collectingCountry>
, 
<collectingCountry id="A1EE767CFF91FFA48BD205FF14202F8D" box="[235,314,417,439]" name="Zambia" pageId="2" pageNumber="1423">Zambia</collectingCountry>
