<taxonomicName id="D2E64D76FFC2B103FEC7FEA8FE23828B" box="[330,423,376,402]" class="Insecta" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="53" phylum="Arthropoda" rank="order">Diptera</taxonomicName>
