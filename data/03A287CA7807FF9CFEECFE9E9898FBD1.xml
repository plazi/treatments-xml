<quantity id="4CF39B397806FF9CFC5DFB96983FFBD1" box="[953,1035,1044,1070]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="19" pageNumber="20" unit="m" value="2000.0">2000m</quantity>
(
<collectingCountry id="F31C764C7806FF9CFBFEFB969894FBD1" box="[1050,1184,1044,1070]" name="Guatemala" pageId="19" pageNumber="20">Guatemala</collectingCountry>
