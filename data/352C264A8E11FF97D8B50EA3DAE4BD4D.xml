<collectionCode id="DB940F998E11FF97DCF70D96DDC4BD07" box="[1200,1267,751,775]" country="New Zealand" httpUri="http://grbio.org/cool/chf3-57df" name="New Zealand Oceanographic Institute" pageId="72" pageNumber="553">NZOI</collectionCode>
