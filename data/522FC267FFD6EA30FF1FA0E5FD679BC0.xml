<materialsCitation id="6AEE792CFFD6EA30FF1FA1B2FE3D9CD0" ID-GBIF-Occurrence="1288040422" collectionCode="ZMB" country="Malaysia" location="Malacca" pageId="2" pageNumber="577" specimenCount="1">
