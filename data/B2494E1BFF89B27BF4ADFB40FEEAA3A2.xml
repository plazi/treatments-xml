<taxonomicName id="FDE0848EFF89B27BF554FB0CFDC4A1D2" authority="Kirk, 1895" authorityName="Kirk" authorityYear="1895" box="[285,544,1216,1240]" class="Calcarea" family="Baeriidae" genus="Lamontia" kingdom="Animalia" order="Baerida" pageId="50" pageNumber="253" phylum="Porifera" rank="species" species="zona">
