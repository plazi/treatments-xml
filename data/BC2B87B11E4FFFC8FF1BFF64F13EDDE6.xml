<materialsCitation id="84EA3CFA1E4FFFC8FF1BFCBEF13ADD72" ID-GBIF-Occurrence="1451082360" collectingDate="1905-08" collectorName="Co. Ariz. White &amp; H. Skinner" county="Cochise Co." location="White" municipality="Huachuca Mts." pageId="3" pageNumber="472" specimenCount="1">
