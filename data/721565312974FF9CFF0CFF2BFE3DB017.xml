<elevation id="719133142974FF9CFFC4FD1DFF58B6D4" box="[113,185,650,674]" metricMagnitude="0" metricUnit="m" metricValue="2.0" metricValueMax="3.0" metricValueMin="1.0" pageId="29" pageNumber="52" unit="m" value="2.0" valueMax="3.0" valueMin="1.0">1–3 m</elevation>
