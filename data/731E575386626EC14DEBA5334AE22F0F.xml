<taxonomicName id="891912C8E9B7A3E301E8DB07F2716DBA" LSID="http://zoobank.org/960227BF-2234-483D-9363-3C7A40D0F32D" class="Insecta" family="Perlidae" genus="Neoperla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neoperla chebalinga" order="Plecoptera" pageId="1" pageNumber="104" phylum="Arthropoda" rank="species" species="chebalinga">Neoperla chebalinga</taxonomicName>
