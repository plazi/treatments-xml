<quantity id="5A261FAD80AFFF0CEE8ECEF61D630E8B" box="[997,1086,223,248]" metricMagnitude="1" metricUnit="m" metricValue="3.8" metricValueMax="7.5" metricValueMin="0.1" pageId="171" pageNumber="172" unit="m" value="38.0" valueMax="75.0" valueMin="1.0">1–75 m</quantity>
