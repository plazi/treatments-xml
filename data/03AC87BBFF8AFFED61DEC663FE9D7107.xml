<emphasis id="B971EABFFF8AFFEE61DEC663FE787705" bold="true" box="[151,427,1879,1905]" pageId="17" pageNumber="18">
<taxonomicName id="4C054D2EFF8AFFEE61DEC663FF2F7705" box="[151,252,1879,1905]" class="Insecta" family="Staphylinidae" genus="Cephennomicrus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="fujianus">
