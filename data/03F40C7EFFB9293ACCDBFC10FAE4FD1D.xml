<taxonomicName id="4C5DC6EBFFBB293AC8BEFD18FA28FD40" box="[1266,1432,644,669]" class="Demospongiae" family="Latrunculiidae" kingdom="Animalia" order="Poecilosclerida" pageId="30" pageNumber="31" phylum="Porifera" rank="family">Latrunculiidae</taxonomicName>
