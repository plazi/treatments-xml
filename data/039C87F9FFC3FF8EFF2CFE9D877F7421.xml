<taxonomicName id="4C354D6CFFC2FF8EFE6CFCF180507445" baseAuthorityName="Jaschhof &amp; Jaschhof" baseAuthorityYear="2009" box="[471,610,801,827]" class="Insecta" family="Cecidomyiidae" genus="Antennardia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="352" phylum="Arthropoda" rank="genus">
