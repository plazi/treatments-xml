<caption id="DF4A6667FFC3FF8FFF2CF989805071C4" ID-DOI="http://doi.org/10.5281/zenodo.3707460" ID-Zenodo-Dep="3707460" httpUri="https://zenodo.org/record/3707460/files/figure.png" pageId="2" pageNumber="351" startId="2.[151,264,1625,1650]" targetBox="[157,1429,425,1594]" targetPageId="2">
<paragraph id="8B8A36EFFFC3FF8FFF2CF989805071C4" blockId="2.[151,1437,1625,1722]" pageId="2" pageNumber="351">
