, 
<taxonomicName id="9AA04D3F0108DC39FA7AA11AFBB22F5A" authority="Mann" authorityName="Mann" box="[892,1060,1880,1904]" class="Insecta" family="Formicidae" genus="Crematogaster" kingdom="Animalia" order="Hymenoptera" pageId="12" pageNumber="59" phylum="Arthropoda" rank="species" species="elysii">
