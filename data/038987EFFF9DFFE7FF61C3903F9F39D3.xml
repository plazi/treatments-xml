<taxonomicName id="4C204D7AFF9DFFE8FC9DC13038103E2B" box="[875,1034,1573,1597]" class="Insecta" family="Anthelidae" genus="Gephyroneura" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
