<collectingCountry id="F3377669FF9DFFE8FE6AC1303E103E2B" box="[412,522,1573,1597]" name="Indonesia" pageId="7" pageNumber="8">Indonesia</collectingCountry>
. 
<bibRefCitation id="EFB14B08FF9DFFE8FDE0C1303EB53E2B" author="Kocak" box="[534,687,1573,1597]" pageId="7" pageNumber="8" refString="Kocak, A. O. (1981) On the nomenclature of some genera of Lepidoptera. Priamus, 1 (3), 112 - 122." type="journal article" year="1981">
Koçak (
<number id="3DE824DEFF9DFFE8FD99C1303EB23E2B" box="[623,680,1573,1597]" pageId="7" pageNumber="8" value="1981.0">1981</number>
)
</bibRefCitation>
