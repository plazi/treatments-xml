<taxonomicName id="4C204D7AFF9DFFE8FD08C2263F713D5D" box="[766,875,1331,1355]" class="Insecta" family="Tortricidae" genus="Anthozela" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="genus">
