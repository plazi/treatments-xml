<taxonomicName id="4C3E4D7B9A19F073FE42F7100B8FF8DF" authority="Trach, 2012" authorityName="Trach" authorityYear="2012" box="[432,761,1819,1843]" class="Arachnida" family="Laelapidae" genus="Gaeolaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="12" pageNumber="598" phylum="Arthropoda" rank="species" species="carabidophilus">
