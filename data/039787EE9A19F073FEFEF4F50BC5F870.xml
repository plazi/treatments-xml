on 
<taxonomicName id="4C3E4D7B9A19F073FE0BF6F40851F8DF" authority="Linnaeus, 1961" authorityName="Linnaeus" authorityYear="1961" class="Insecta" family="Carabidae" genus="Carabus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="598" phylum="Arthropoda" rank="species" species="intricatus">
<emphasis id="B94AEAEA9A19F073FE0BF6F40BB4F8FC" box="[505,706,1784,1808]" italics="true" pageId="12" pageNumber="598">Carabus intricatus</emphasis>
Linnaeus, 
<quantity id="4CC69B1D9A19F073FF00F7170851F8DF" box="[242,295,1819,1843]" metricMagnitude="1" metricUnit="m" metricValue="4.98094" pageId="12" pageNumber="598" unit="in" value="1961.0">1961</quantity>
</taxonomicName>
