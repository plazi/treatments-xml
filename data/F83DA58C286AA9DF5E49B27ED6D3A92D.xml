<taxonomicName id="FDF2A10A57177F044A532010DFAEEE80" LSID="urn:lsid:zoobank.org:act:3B994BCD-645A-4233-96F4-B7CA4CCA2BCD" authority="Heiss, 2013" authorityName="Heiss" authorityYear="2013" class="Insecta" family="Aradidae" genus="Osellaptera" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Osellaptera" order="Hemiptera" pageId="1" pageNumber="138" phylum="Arthropoda" rank="genus">Osellaptera</taxonomicName>
