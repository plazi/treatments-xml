<taxonomicName id="4C144D37C660A212F3FDF923F3A545A6" authorityName="Templeton" authorityYear="1848" box="[3077,3146,1721,1760]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="22" pageNumber="249" phylum="Chordata" rank="species" species="ater">ater</taxonomicName>
