<taxonomicName id="4C144D37C660A212F4DCF91EF47A45ED" authorityName="Temminck" authorityYear="1834" box="[2852,2965,1668,1707]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="22" pageNumber="249" phylum="Chordata" rank="species" species="bicolor">
