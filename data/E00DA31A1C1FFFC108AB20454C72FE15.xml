<typeStatus id="B71FACAE1C1FFFC009BD23A14CABF8B1" box="[385,489,1772,1798]" pageId="26" pageNumber="27">holotype</typeStatus>
+ 
<number id="DE6C002B1C1FFFC00A3623A14F5BF8B1" box="[522,537,1772,1798]" pageId="26" pageNumber="27" value="1.0">1</number>
ɗ; 
<typeStatus id="B71FACAE1C1FFFC00A0023A14FE1F8B1" box="[572,675,1772,1798]" pageId="26" pageNumber="27">holotype</typeStatus>
listed first): total length 
<number id="DE6C002B1C1FFFC00BF423A14EB2F8B1" box="[968,1008,1772,1798]" pageId="26" pageNumber="27" value="6.4">6.4</number>
, 
<number id="DE6C002B1C1FFFC00BC123A14967F8B1" box="[1021,1061,1772,1798]" pageId="26" pageNumber="27" value="5.8">5.8</number>
; carapace length 
<number id="DE6C002B1C1FFFC00CCD23A1485AF8B1" box="[1265,1304,1772,1798]" pageId="26" pageNumber="27" value="2.8">2.8</number>
, 
<number id="DE6C002B1C1FFFC00D1923A1480FF8B1" box="[1317,1357,1772,1798]" pageId="26" pageNumber="27" value="2.9">2.9</number>
, width 
<number id="DE6C002B1C1FFFC008AB22594DFDF899" box="[151,191,1812,1838]" pageId="26" pageNumber="27" value="2.2">2.2</number>
, 2.0, index 
<number id="DE6C002B1C1FFFC0097822594C38F899" box="[324,378,1812,1838]" pageId="26" pageNumber="27" value="1.27">1.27</number>
, 
<number id="DE6C002B1C1FFFC009B922594CFEF899" box="[389,444,1812,1838]" pageId="26" pageNumber="27" value="1.45">1.45</number>
; AM-AM distance/AM diameter 
<number id="DE6C002B1C1FFFC00B7D22594E35F899" box="[833,887,1812,1838]" pageId="26" pageNumber="27" value="1.27">1.27</number>
, 
<number id="DE6C002B1C1FFFC00BBE22594EFAF899" box="[898,952,1812,1838]" pageId="26" pageNumber="27" value="1.15">1.15</number>
; leg lengths: I 
<number id="DE6C002B1C1FFFC00C5E225949DAF899" box="[1122,1176,1812,1838]" pageId="26" pageNumber="27" value="12.5">12.5</number>
, 
<number id="DE6C002B1C1FFFC00C9F2259499BF899" box="[1187,1241,1812,1838]" pageId="26" pageNumber="27" value="12.7">12.7</number>
