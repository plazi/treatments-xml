<subSubSection id="20BE41871C1FFFC008FA209949DAFA59" box="[198,1176,1492,1518]" pageId="26" pageNumber="27" type="etymology">
<paragraph id="681B120C1C1FFFC008FA209949DAFA59" blockId="26.[151,1436,1408,1998]" box="[198,1176,1492,1518]" pageId="26" pageNumber="27">
<emphasis id="5AD0CE1E1C1FFFC008FA20994C11FA59" bold="true" box="[198,339,1492,1518]" pageId="26" pageNumber="27">Etymology.</emphasis>
