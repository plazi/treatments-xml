<number id="DE6C002B1C1FFFC0091822714C18F8E1" box="[292,346,1852,1878]" pageId="26" pageNumber="27" value="10.1">10.1</number>
, 
<number id="DE6C002B1C1FFFC0095A22714CDFF8E1" box="[358,413,1852,1878]" pageId="26" pageNumber="27" value="10.1">10.1</number>
