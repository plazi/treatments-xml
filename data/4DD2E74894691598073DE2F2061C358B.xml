<taxonomicName id="0AF7B1345EFB4DCDC262BE73FF87B384" LSID="http://zoobank.org/EE96A747-DA48-45D9-9EB9-3E55CAA73507" class="Arachnida" family="Linyphiidae" genus="Neriene" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neriene circifolia" order="Araneae" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="circifolia">Neriene circifolia</taxonomicName>
<taxonomicNameLabel id="8EAF71F18FCC66A776EDB8AA67F23B81" pageId="27" pageNumber="28">sp. n.</taxonomicNameLabel>
