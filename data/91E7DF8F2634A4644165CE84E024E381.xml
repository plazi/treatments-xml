<emphasis id="D1B7965854392435F1FC0B4DA742BA3B" italics="true" pageId="308" pageNumber="309">q.v</emphasis>
.] and 
<taxonomicName id="D7DA7C7B40B6FD71AD2FE1800DF75D7A" authorityName="Dejean" authorityYear="1821" class="Hexapoda" family="Carabidae" genus="Blemus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="308" pageNumber="309" phylum="Arthropoda" rank="genus">
