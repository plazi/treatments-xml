<collectingCountry id="B79576795C5B1644F803F8DDFE3DF81A" box="[363,492,1901,1926]" name="Costa Rica" pageId="8">Costa Rica</collectingCountry>
, 
<collectingCountry id="B79576795C5B1644F894F8DEFD95F81A" box="[508,580,1901,1926]" name="Brazil" pageId="8">Brazil</collectingCountry>
