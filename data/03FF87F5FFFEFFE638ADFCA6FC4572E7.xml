<number id="3D9E24C4FFFEFFE93BD4FB5DFB8574DF" box="[1006,1076,1215,1239]" pageId="23" pageNumber="24" value="72801.0">72801</number>
handwritten in black]; “ 
<typeStatus id="54ED8841FFFEFFE93D72FB5DFA2374DF" box="[1352,1426,1215,1239]" pageId="23" pageNumber="24">
<collectionCode id="ED47AE26FFFEFFE93D72FB5DFA2374DF" box="[1352,1426,1215,1239]" pageId="23" pageNumber="24">TYPE</collectionCode>
</typeStatus>
