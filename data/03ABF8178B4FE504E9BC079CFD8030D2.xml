<subSubSection id="C3181A8A8B4EE504E9E201B5FBA23495" box="[201,1131,151,176]" pageId="25" pageNumber="26" type="etymology">
<paragraph id="8BBD49018B4EE504E9E201B5FBA23495" blockId="25.[151,1437,151,1256]" box="[201,1131,151,176]" pageId="25" pageNumber="26">
<emphasis id="B97695138B4EE504E9E201B5FE86348A" bold="true" box="[201,335,152,176]" pageId="25" pageNumber="26">Etymology.</emphasis>
