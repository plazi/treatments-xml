<paragraph id="8BDAB700F55CFA11FF8AF4C16837FB10" blockId="2.[151,588,1224,1248]" box="[151,588,1224,1248]" pageId="2" pageNumber="33">
<taxonomicName id="4C65CC83F55CFA11FF8AF4C16837FB10" authority="Ashmead, 1895" authorityName="Ashmead" authorityYear="1895" box="[151,588,1224,1248]" class="Insecta" family="Braconidae" genus="Spathius" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="flavotestaceous">
<emphasis id="B9116B12F55CFA11FF8AF4C16BE6FB2F" box="[151,413,1225,1248]" italics="true" pageId="2" pageNumber="33">Spathius flavotestaceous</emphasis>
Ashmead, 
<number id="3DADA527F55CFA11FD0AF4C06837FB10" box="[535,588,1224,1247]" pageId="2" pageNumber="33" value="1895.0">1895</number>
</taxonomicName>
</paragraph>
