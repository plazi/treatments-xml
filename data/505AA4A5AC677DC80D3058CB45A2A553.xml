<taxonomicName id="9AF538F9293365DCF5D962B9686A7A53" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:27350" authority="Mayr" authorityName="Mayr" class="Insecta" family="Formicidae" genus="Cardiocondyla" kingdom="Animalia" lsidName-HNS="Cardiocondyla nuda (Mayr)" order="Hymenoptera" pageId="6" pageNumber="379" phylum="Arthropoda" rank="species" species="nuda">
Cardiocondyla nuda 
<authority id="0DF02E95905EF638CFCAC04AD4850FAB" pageId="6" pageNumber="379">Mayr</authority>
</taxonomicName>
(
<taxonomicName id="51C4EE08EF68BC2F1F3EE40F5D98B897" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:136358" class="Insecta" family="Formicidae" genus="Leptothorax" kingdom="Animalia" lsidName-HNS="Leptothorax nudus Mayr" order="Hymenoptera" pageId="6" pageNumber="379" phylum="Arthropoda" rank="species" species="nudus">Leptothorax nudus</taxonomicName>
