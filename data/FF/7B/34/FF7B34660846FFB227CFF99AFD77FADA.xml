<subSubSection box="[151,499,1586,1611]" pageId="6" pageNumber="7" type="nomenclature">
<paragraph blockId="6.[151,499,1586,1611]" box="[151,499,1586,1611]" pageId="6" pageNumber="7">
<heading bold="true" box="[151,499,1586,1611]" fontSize="10" level="2" pageId="6" pageNumber="7" reason="2">
<emphasis bold="true" box="[151,499,1586,1611]" pageId="6" pageNumber="7">
<taxonomicName box="[151,402,1586,1611]" class="Insecta" family="Ichneumonidae" genus="Acropimpla" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="mucronis" status="sp. nov.">
<emphasis bold="true" box="[151,402,1586,1611]" italics="true" pageId="6" pageNumber="7">Acropimpla mucronis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[409,499,1587,1611]" pageId="6" pageNumber="7" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
