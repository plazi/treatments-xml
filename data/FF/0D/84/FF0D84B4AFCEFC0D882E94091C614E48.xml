<taxonomicName LSID="http://species-id.net/wiki/Euagathis_chinensis" authority="(Holmgren, 1868)" baseAuthorityName="Holmgren" baseAuthorityYear="1868" class="Insecta" family="Braconidae" genus="Euagathis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Euagathis chinensis" order="Hymenoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="chinensis">
