<caption httpUri="https://zenodo.org/record/231716/files/figure.png" pageId="4" pageNumber="148" targetBox="[150,1436,193,1864]" targetPageId="4">
<paragraph blockId="4.[151,1436,1887,1939]" pageId="4" pageNumber="148">
<emphasis bold="true" box="[151,277,1887,1909]" pageId="4" pageNumber="148">FIGURE 6.</emphasis>
<taxonomicName box="[285,608,1888,1909]" class="Insecta" family="Tortricidae" genus="Compsocommosis" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="147" phylum="Arthropoda" rank="species" species="vietnamensis">
<emphasis box="[285,608,1888,1909]" italics="true" pageId="4" pageNumber="148">COmpSOcOmmOSiS vietnamenSiS</emphasis>
