<paragraph pageId="null" pageNumber="882">
<taxonomicName authority="L." class="Magnoliopsida" family="Apiaceae" genus="Pimpinella" kingdom="Plantae" order="Apiales" pageId="null" pageNumber="882" phylum="Tracheophyta" rank="species" species="saxifraga">
Pimpinella 
<normalizedToken originalValue="saxífraga" pageId="null" pageNumber="882">saxifraga</normalizedToken>
<authorityName pageId="null" pageNumber="882">L.</authorityName>
</taxonomicName>
