<geoCoordinate degrees="42" direction="north" minutes="04" orientation="latitude" precision="1" seconds="29.0" value="42.074722">42°04'29.0&quot;N</geoCoordinate>
, 
<geoCoordinate degrees="19" direction="east" minutes="06" orientation="longitude" precision="1" seconds="11.8" value="19.103277">19°06'11.8&quot;E</geoCoordinate>
. 
<taxonomicName authority="Méquignon, 1946" authorityName="Méquignon" authorityYear="1946" class="Insecta" family="Mordellidae" genus="Variimorda" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Variimorda caprai" order="Coleoptera" pageId="0" pageNumber="151" phylum="Arthropoda" rank="species" species="caprai">
