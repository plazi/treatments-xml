<taxonomicName authorityName="Scarbrough" authorityYear="2008" box="[817,973,1059,1083]" class="Insecta" family="Asilidae" genus="Ommatius" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="6" phylum="Arthropoda" rank="species" species="nebulosus">
