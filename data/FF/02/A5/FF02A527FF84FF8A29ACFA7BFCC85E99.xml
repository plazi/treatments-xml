<subSubSection box="[151,512,1472,1499]" pageId="1" pageNumber="464" type="nomenclature">
<paragraph blockId="1.[151,512,1472,1499]" box="[151,512,1472,1499]" pageId="1" pageNumber="464">
<heading bold="true" box="[151,512,1472,1499]" fontSize="11" level="1" pageId="1" pageNumber="464" reason="1">
<emphasis bold="true" box="[151,512,1472,1499]" pageId="1" pageNumber="464">
Genus 
<taxonomicName box="[238,395,1473,1499]" class="Malacostraca" family="Paragammaropsidae" genus="Stebbingiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="464" phylum="Arthropoda" rank="genus" status="gen. nov.">
