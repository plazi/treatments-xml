<collectingCountry id="21BE7627C507FFA2FD20FD5E7F88CAFD" box="[643,706,664,690]" name="India" pageId="13" pageNumber="40">India</collectingCountry>
, 
<collectingCountry id="21BE7627C507FFA2FD6EFD5E7E79CAFD" box="[717,819,664,690]" name="Pakistan" pageId="13" pageNumber="40">Pakistan</collectingCountry>
, 
<collectingCountry id="21BE7627C507FFA2FC9CFD5E7EDDCAFD" box="[831,919,664,690]" name="Bhutan" pageId="13" pageNumber="40">Bhutan</collectingCountry>
