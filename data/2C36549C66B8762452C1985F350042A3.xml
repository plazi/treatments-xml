<bibRefCitation id="3C8DE950C2BE0C23E1E34F68B3FA7848" author="Mocsary, A" journalOrPublisher="Termeszetrajzi Fuezetek" pageId="75" pageNumber="76" pagination="12 - 20" title="Studia Synonymica." volume="11" year="1887">
<normalizedToken id="DDAE1CF41C6663F009E2AED6833FEE1C" originalValue="MocsÃ¡ry">Mocsary</normalizedToken>
