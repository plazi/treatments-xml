<taxonomicName id="28166CA6A2E6309BDEBDBC9542E93CEB" class="Insecta" family="Masaridae" genus="Chrysis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Chrysis carinata" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="carinata">Chrysis carinata</taxonomicName>
