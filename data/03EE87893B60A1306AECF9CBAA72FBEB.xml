</emphasis>
and 
<taxonomicName id="4C474D1C3B1FA1306900FF5FADCDFF4B" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[891,974,225,251]" class="Arachnida" family="Gnaphosidae" genus="Kikongo" kingdom="Animalia" order="Araneae" pageId="64" pageNumber="65" phylum="Arthropoda" rank="species" species="buta" status="sp. nov.">
