<taxonomicName id="BFDF4D50FF8ED268FC4CFD1EFBDE8F58" authority="Rathbun" authorityName="Rathbun" authorityYear="1897" box="[907,1135,764,790]" class="Malacostraca" family="Inachidae" genus="Ericerodes" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="318" phylum="Arthropoda" rank="genus">
