<taxonomicName id="BFDF4D50FF8ED268FF50FCC2FEBE8F74" authorityName="Stimpson" authorityYear="1860" box="[151,271,800,826]" class="Malacostraca" family="Inachidae" genus="Podochela" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="318" phylum="Arthropoda" rank="genus">
