<taxonomicName id="BFDF4D50FF8ED268FDB6FD1EFCD58F58" authorityName="Coelho" authorityYear="1972" box="[625,868,764,790]" class="Malacostraca" family="Inachidae" genus="Podochela" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="minuscula">
