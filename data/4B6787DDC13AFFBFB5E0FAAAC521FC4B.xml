<number id="750624ECC13EFFBFB4C3FD96C797FDBE" box="[436,474,512,536]" pageId="12" pageNumber="143" value="0.3">0.3</number>
; III: 
<number id="750624ECC13EFFBFB766FD96C47EFDBE" box="[529,563,512,536]" pageId="12" pageNumber="143" value="1.3">1.3</number>
