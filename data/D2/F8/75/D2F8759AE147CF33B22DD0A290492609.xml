<taxonomicName genus="A." lsidName="A. kranskopensis" pageId="0" pageNumber="1" rank="species" species="kranskopensis">A. kranskopensis</taxonomicName>
Lotz, 1996, 
<taxonomicName genus="A." lsidName="A. lawrencei" pageId="0" pageNumber="1" rank="species" species="lawrencei">A. lawrencei</taxonomicName>
Lotz, 1996, 
<taxonomicName genus="A." lsidName="A. ngomensis" pageId="0" pageNumber="1" rank="species" species="ngomensis">A. ngomensis</taxonomicName>
Lotz, 1996, 
<taxonomicName genus="A." lsidName="A. royalensis" pageId="0" pageNumber="1" rank="species" species="royalensis">A. royalensis</taxonomicName>
Lotz, 2006, 
<taxonomicName genus="A." lsidName="A. woodae" pageId="0" pageNumber="1" rank="species" species="woodae">A. woodae</taxonomicName>
