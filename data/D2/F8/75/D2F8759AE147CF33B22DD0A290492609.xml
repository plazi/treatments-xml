<taxonomicName genus="A." lsidName="A. ansieae" pageId="0" pageNumber="1" rank="species" species="ansieae">A. ansieae</taxonomicName>
Lotz, 2015, 
<taxonomicName genus="A." lsidName="A. bergae" pageId="0" pageNumber="1" rank="species" species="bergae">A. bergae</taxonomicName>
Lotz, 1996, 
<taxonomicName genus="A." lsidName="A. entabeniensis" pageId="0" pageNumber="1" rank="species" species="entabeniensis">A. entabeniensis</taxonomicName>
Lotz, 2003, 
<taxonomicName genus="A." lsidName="A. fernkloofensis" pageId="0" pageNumber="1" rank="species" species="fernkloofensis">A. fernkloofensis</taxonomicName>
Lotz, 1996, 
<taxonomicName genus="A." lsidName="A. godfreyi" pageId="0" pageNumber="1" rank="species" species="godfreyi">A. godfreyi</taxonomicName>
