(Cuvier, 
<number box="[482,542,1545,1571]" pageId="12" pageNumber="13" value="1830.0">1830</number>
)
</taxonomicName>
(as 
<taxonomicName box="[599,834,1545,1570]" class="Actinopterygii" family="Sciaenidae" genus="Otolithes" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="13" phylum="Chordata" rank="species" species="maculatus">
<emphasis box="[599,834,1545,1570]" italics="true" pageId="12" pageNumber="13">Otolithes maculatus</emphasis>
</taxonomicName>
);
</paragraph>
<paragraph blockId="12.[151,1437,655,2035]" box="[199,1435,1584,1610]" pageId="12" pageNumber="13">
<taxonomicName box="[199,325,1584,1610]" class="Actinopterygii" family="Serranidae" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="13" phylum="Chordata" rank="family">Serranidae</taxonomicName>
<taxonomicName authority="Thunberg, 1793" authorityName="Thunberg" authorityYear="1793" box="[333,894,1584,1610]" class="Actinopterygii" family="Serranidae" genus="Epinephelus" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="13" phylum="Chordata" rank="species" species="septemfasciatus">
