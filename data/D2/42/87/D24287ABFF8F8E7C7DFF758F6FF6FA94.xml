<paragraph blockId="87.[151,1436,547,1349]" box="[151,1368,547,573]" pageId="87" pageNumber="88">
<emphasis bold="true" box="[151,303,547,573]" pageId="87" pageNumber="88">Dimensions.</emphasis>
d (min)=up to 
<quantity box="[481,566,547,573]" metricMagnitude="-2" metricUnit="m" metricValue="1.2" pageId="87" pageNumber="88" unit="mm" value="12.0">12 mm</quantity>
