<subSubSection pageId="152" pageNumber="153" type="description">
<paragraph blockId="152.[151,1436,1439,1803]" box="[151,713,1439,1465]" pageId="152" pageNumber="153">
<emphasis bold="true" box="[151,303,1439,1465]" pageId="152" pageNumber="153">Dimensions.</emphasis>
d= 
<quantity box="[343,455,1439,1465]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" metricValueMax="11.0" metricValueMin="8.0" pageId="152" pageNumber="153" unit="mm" value="9.5" valueMax="11.0" valueMin="8.0">8–11 mm</quantity>
