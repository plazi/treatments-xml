<emphasis bold="true" box="[151,303,794,820]" pageId="75" pageNumber="76">Dimensions.</emphasis>
d (min)= 
<quantity box="[414,529,794,820]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" metricValueMax="10.0" metricValueMin="6.0" pageId="75" pageNumber="76" unit="mm" value="8.0" valueMax="10.0" valueMin="6.0">6–10 mm</quantity>
; d (max)= 
<quantity box="[651,764,794,820]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" metricValueMax="1.4" metricValueMin="0.8" pageId="75" pageNumber="76" unit="mm" value="11.0" valueMax="14.0" valueMin="8.0">8–14 mm</quantity>
