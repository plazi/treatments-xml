<quantity box="[413,543,1752,1778]" metricMagnitude="-2" metricUnit="m" metricValue="2.25" metricValueMax="2.3" metricValueMin="2.2" pageId="67" pageNumber="68" unit="mm" value="22.5" valueMax="23.0" valueMin="22.0">22–23 mm</quantity>
