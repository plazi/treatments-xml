<materialsCitation county="Solitary" location="Pourtales" municipality="Septal" pageId="179" pageNumber="180" specimenCount="1" stateProvince="Ceuta">
<collectingCounty box="[290,384,311,337]" pageId="179" pageNumber="180">Solitary</collectingCounty>
