<collectingCountry box="[259,363,653,679]" name="Bulgaria" pageId="183" pageNumber="184">Bulgaria</collectingCountry>
, Lower Aptian of 
<collectingCountry box="[599,688,653,679]" name="Austria" pageId="183" pageNumber="184">Austria</collectingCountry>
, 
<collectingCountry box="[705,815,653,679]" name="Germany" pageId="183" pageNumber="184">Germany</collectingCountry>
