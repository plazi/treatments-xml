<subSubSection box="[151,749,158,184]" pageId="166" pageNumber="167" type="description">
<paragraph blockId="166.[151,1436,158,658]" box="[151,749,158,184]" pageId="166" pageNumber="167">
<emphasis bold="true" box="[151,303,158,184]" pageId="166" pageNumber="167">Dimensions.</emphasis>
d= 
<quantity box="[341,442,158,184]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" metricValueMax="4.0" metricValueMin="2.0" pageId="166" pageNumber="167" unit="mm" value="3.0" valueMax="4.0" valueMin="2.0">2–4 mm</quantity>
