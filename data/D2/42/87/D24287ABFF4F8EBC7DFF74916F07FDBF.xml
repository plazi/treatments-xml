<taxonomicName authorityName="Milne Edwards &amp; Haime" authorityYear="1849" box="[818,954,553,578]" class="Anthozoa" family="Micrabaciidae" genus="Micrabacia" kingdom="Animalia" order="Scleractinia" pageId="151" pageNumber="152" phylum="Cnidaria" rank="genus">
