<quantity box="[341,442,1029,1055]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" metricValueMax="5.0" metricValueMin="4.0" pageId="196" pageNumber="197" unit="mm" value="4.5" valueMax="5.0" valueMin="4.0">4–5 mm</quantity>
