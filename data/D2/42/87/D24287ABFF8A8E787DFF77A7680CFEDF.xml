<quantity box="[414,546,1174,1200]" metricMagnitude="-2" metricUnit="m" metricValue="3.75" metricValueMax="4.0" metricValueMin="3.5" pageId="82" pageNumber="83" unit="mm" value="37.5" valueMax="40.0" valueMin="35.0">35–40 mm</quantity>
