<quantity box="[345,444,1063,1089]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" metricValueMax="7.0" metricValueMin="5.0" pageId="117" pageNumber="118" unit="mm" value="6.0" valueMax="7.0" valueMin="5.0">5–7 mm</quantity>
