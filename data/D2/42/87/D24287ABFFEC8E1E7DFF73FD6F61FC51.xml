<collectingCountry box="[597,706,870,896]" name="Denmark" pageId="53" pageNumber="54">Denmark</collectingCountry>
(
<collectingCountry box="[721,850,870,896]" name="Greenland" pageId="53" pageNumber="54">Greenland</collectingCountry>
).
</paragraph>
