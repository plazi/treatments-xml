<collectingCountry box="[359,426,1268,1294]" name="China" pageId="19" pageNumber="516">China</collectingCountry>
(
<collectingRegion box="[441,528,1268,1295]" country="China" name="Hainan" pageId="19" pageNumber="516">Hainan</collectingRegion>
