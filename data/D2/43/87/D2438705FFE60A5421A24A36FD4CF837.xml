<collectionCode box="[159,206,1482,1508]" country="France" name="Musee de Lectoure" pageId="8" pageNumber="9">ML</collectionCode>
bootstrap = 100%; posterior probability = 1.0). 
<taxonomicName authorityName="Parnmen &amp; Lumbsch" authorityYear="2013" box="[819,1046,1483,1508]" class="Lecanoromycetes" family="Cladoniaceae" genus="Cladia" kingdom="Fungi" order="Lecanorales" pageId="8" pageNumber="9" phylum="Ascomycota" rank="species" species="blanchonii">
<emphasis box="[819,1046,1483,1508]" italics="true" pageId="8" pageNumber="9">Cladia blanchonii</emphasis>
