<number box="[1275,1303,1679,1703]" pageId="6" pageNumber="377" value="18.0">18</number>
x 
<quantity box="[1331,1358,1679,1703]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="6" pageNumber="377" unit="mm" value="11.0">11</quantity>
