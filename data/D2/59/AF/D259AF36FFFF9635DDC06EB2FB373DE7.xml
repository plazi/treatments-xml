<tr box="[156,1427,1101,1153]" gridrow="0" pageId="1" pageNumber="438">
<th box="[156,360,1101,1153]" gridcol="0" gridrow="0" pageId="1" pageNumber="438">Antennomere 3</th>
<th box="[409,633,1101,1153]" gridcol="1" gridrow="0" pageId="1" pageNumber="438">as long as antennomere 4</th>
