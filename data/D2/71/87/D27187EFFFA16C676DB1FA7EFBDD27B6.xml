<taxonomicName class="Arachnida" family="Pygmephoridae" genus="Mesopotamiophorus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="4" pageNumber="51" phylum="Arthropoda" rank="genus">
