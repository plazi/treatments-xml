<number box="[788,797,1909,1933]" pageId="7" pageNumber="54" value="1.0">1</number>
-fe 
<number box="[831,846,1909,1933]" pageId="7" pageNumber="54" value="4.0">4</number>
-ge 
<number box="[883,898,1909,1933]" pageId="7" pageNumber="54" value="4.0">4</number>
-ti 
<number box="[923,937,1909,1933]" pageId="7" pageNumber="54" value="6.0">6</number>
(
<number box="[946,961,1909,1933]" pageId="7" pageNumber="54" value="2.0">2</number>
)-ta 
<number box="[1000,1028,1909,1933]" pageId="7" pageNumber="54" value="12.0">12</number>
(
<number box="[1036,1051,1909,1933]" pageId="7" pageNumber="54" value="2.0">2</number>
