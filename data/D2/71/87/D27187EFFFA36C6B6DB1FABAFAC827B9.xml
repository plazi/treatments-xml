<number box="[1341,1357,1984,2008]" pageId="7" pageNumber="54" value="9.0">9</number>
). Setal formula: tr 
<number box="[275,285,151,175]" pageId="8" pageNumber="55" value="1.0">1</number>
-fe 
<number box="[320,333,151,175]" pageId="8" pageNumber="55" value="3.0">3</number>
-ge 
<number box="[372,385,151,175]" pageId="8" pageNumber="55" value="3.0">3</number>
-ti 
<number box="[412,428,151,175]" pageId="8" pageNumber="55" value="4.0">4</number>
(
<number box="[437,447,151,175]" pageId="8" pageNumber="55" value="1.0">1</number>
