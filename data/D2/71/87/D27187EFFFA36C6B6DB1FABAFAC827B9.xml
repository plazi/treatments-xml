<number box="[171,180,260,284]" pageId="8" pageNumber="55" value="1.0">1</number>
-fe 
<number box="[215,230,260,284]" pageId="8" pageNumber="55" value="2.0">2</number>
-ge 
<number box="[267,276,260,284]" pageId="8" pageNumber="55" value="1.0">1</number>
-ti 
<number box="[301,316,260,284]" pageId="8" pageNumber="55" value="4.0">4</number>
-ta 
<number box="[348,362,260,284]" pageId="8" pageNumber="55" value="7.0">7</number>
