<date box="[716,737,375,403]" pageId="4" pageNumber="51">
<emphasis box="[716,728,375,398]" italics="true" pageId="4" pageNumber="51">v</emphasis>
<number box="[729,737,389,403]" pageId="4" pageNumber="51" value="2.0">2</number>
