<taxonomicName authority="Sevastianov and Zahida Al Douri, 1989: 31" authorityName="Sevastianov and Zahida Al Douri" authorityPageNumber="31" authorityYear="1989" box="[151,778,758,781]" class="Arachnida" family="Pygmephoridae" genus="Bakerdania" kingdom="Animalia" order="Prostigmata" pageId="2" pageNumber="49" phylum="Arthropoda" rank="species" species="nouri">
