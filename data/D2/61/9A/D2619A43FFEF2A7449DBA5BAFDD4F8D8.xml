: 
<taxonomicName box="[420,526,943,968]" class="Magnoliopsida" family="Lamiaceae" genus="Leonurus" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="genus">
<emphasis box="[420,526,943,968]" italics="true" pageId="22" pageNumber="23">Leonurus</emphasis>
</taxonomicName>
L. (
<bibRefCitation author="Benavent-Corai, J. &amp; Martinez, M. &amp; Jimenez Peydro, R." box="[576,891,942,969]" pageId="22" pageNumber="23" pagination="1 - 97" refId="ref72091" refString="Benavent-Corai, J., Martinez, M. &amp; Jimenez Peydro, R. (2005) Catalogue of the host plants of the world Agromyzidae (Diptera). Bollettino di Zoologica Agraria e di Bachicoltura, Series II, 37, 1 - 97." type="journal article" year="2005">
Benavent-Corai 
<emphasis box="[764,815,943,969]" italics="true" pageId="22" pageNumber="23">et al</emphasis>
. 2005
</bibRefCitation>
), 
<taxonomicName box="[912,1083,943,969]" class="Magnoliopsida" family="Lamiaceae" genus="Lamium" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="album">
<emphasis box="[912,1083,943,969]" italics="true" pageId="22" pageNumber="23">Lamium album</emphasis>
</taxonomicName>
L. (
<bibRefCitation author="Papp, L. &amp; Cerny, M." box="[1133,1369,943,971]" pageId="22" pageNumber="23" refId="ref74172" refString="Papp, L. &amp; Cerny, M. (2016) Agromyzidae (Diptera) of Hungary. Vol. 2. Phytomyzinae I. Pars Ltd, Nagycovacsi, 385 pp." type="book" year="2016">Papp &amp; Černý 2016</bibRefCitation>
). 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Lamiaceae" genus="Betonica" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="officinalis">
<emphasis italics="true" pageId="22" pageNumber="23">Betonica officinalis</emphasis>
</taxonomicName>
L., 
<taxonomicName box="[377,614,979,1005]" class="Magnoliopsida" family="Lamiaceae" genus="Phlomoides" kingdom="Plantae" order="Lamiales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="tuberosa">
