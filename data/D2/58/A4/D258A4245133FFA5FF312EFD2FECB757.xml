<paragraph blockId="10.[151,1118,843,898]" box="[151,1118,876,898]" pageId="10" pageNumber="145">
<taxonomicName authority="Alder &amp; Hancock 1907" authorityName="Alder &amp; Hancock" authorityYear="1907" box="[151,663,876,898]" class="Ascidiacea" family="Corellidae" genus="Corella" kingdom="Animalia" order="Enterogona" pageId="10" pageNumber="145" phylum="Chordata" rank="species" species="parallelogramma">
<emphasis box="[151,411,876,898]" italics="true" pageId="10" pageNumber="145">Corella parallelogramma</emphasis>
: Alder &amp; Hancock 
<number box="[610,663,876,898]" pageId="10" pageNumber="145" value="1907.0">1907</number>
</taxonomicName>
, Berrill 
<number box="[746,797,876,898]" pageId="10" pageNumber="145" value="1950.0">1950</number>
