<taxonomicName authorityName="Caporiacco" authorityYear="1934" class="Arachnida" family="Oonopidae" genus="Camptoscaphiella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Camptoscaphiella fulva" order="Araneae" pageId="0" pageNumber="43" phylum="Arthropoda" rank="species" species="fulva">
