<taxonomicName authority="Alder, 1856" authorityName="Alder" authorityYear="1856" class="Gymnolaemata" family="Cheilostomatida (awaiting allocation)" genus="Buskia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="3" phylum="Bryozoa" rank="species" species="nitens">
<taxonomicName authority="Alder" authorityName="Alder" genus="B." pageId="2" pageNumber="3" rank="species" species="nitens">
<taxonomicName genus="B." pageId="2" pageNumber="3" rank="species" species="nitens">
<emphasis box="[1373,1398,1792,1817]" italics="true" pageId="2" pageNumber="3">
<collectionCode box="[1373,1394,1792,1817]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15534" name="Botanischer Garten und Botanisches Museum Berlin-Dahlem, Zentraleinrichtung der Freien Universitaet" pageId="2" pageNumber="3" type="Herbarium">B</collectionCode>
.
</emphasis>
