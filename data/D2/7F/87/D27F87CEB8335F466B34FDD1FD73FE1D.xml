</taxonomicName>
</taxonomicName>
Alder
</taxonomicName>
, 
<date box="[347,406,1826,1853]" pageId="2" pageNumber="3" value="1856">1856</date>
</taxonomicName>
and 
<taxonomicName authority="Hincks, 1887" authorityName="Hincks" authorityYear="1887" box="[462,749,1826,1852]" class="Gymnolaemata" family="Cheilostomatida (awaiting allocation)" genus="Buskia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomatida" pageId="2" pageNumber="3" phylum="Bryozoa" rank="species" species="socialis">
<taxonomicName authority="Hincks" authorityName="Hincks" box="[462,678,1826,1852]" genus="B." pageId="2" pageNumber="3" rank="species" species="socialis">
<taxonomicName box="[462,585,1826,1852]" genus="B." pageId="2" pageNumber="3" rank="species" species="socialis">
