<paragraph pageId="4" pageNumber="15">
<taxonomicName class="Insecta" family="Tettigoniidae" genus="Athaumaspis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Athaumaspis minutus" order="Orthoptera" pageId="4" pageNumber="15" phylum="Arthropoda" rank="species" species="minutus">Athaumaspis minutus</taxonomicName>
sp. n.
</paragraph>
