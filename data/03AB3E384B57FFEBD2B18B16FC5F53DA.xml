<number id="3DCA9D094B50FFEBD3328D5DFEB35333" box="[276,328,260,284]" pageId="5" pageNumber="166" value="0.77">0.77</number>
, ti 
<number id="3DCA9D094B50FFEBD34C8D5DFE655333" box="[362,414,260,284]" pageId="5" pageNumber="166" value="2.4">2.40</number>
, mt 
<number id="3DCA9D094B50FFEBD3E88D5DFDFB5332" box="[462,512,260,285]" pageId="5" pageNumber="166" value="1.75">1.75</number>
, total fe–mt 
<number id="3DCA9D094B50FFEBD0A98D5DFD385333" box="[655,707,260,284]" pageId="5" pageNumber="166" value="7.49">7.49</number>
; leg II fe 
