<number id="3DCA9D094B50FFEBD1A98D5DFC395333" box="[911,962,260,284]" pageId="5" pageNumber="166" value="0.74">0.74</number>
, ti 
<number id="3DCA9D094B50FFEBD1C38D5DFBE35333" box="[997,1048,260,284]" pageId="5" pageNumber="166" value="1.72">1.72</number>
, mt 
<number id="3DCA9D094B50FFEBD66F8D5DFB805333" box="[1097,1147,260,284]" pageId="5" pageNumber="166" value="1.4">1.40</number>
, ta 
<number id="3DCA9D094B50FFEBD6858D5DFB2D5332" box="[1187,1238,260,285]" pageId="5" pageNumber="166" value="0.68">0.68</number>
