<taxonomicName id="4C184D3DCD1DFE074F6CF8945D1DF8B4" box="[868,1062,1885,1909]" class="Actinopterygii" family="Gobiidae" genus="Callogobius" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="237" phylum="Chordata" rank="species" species="dori">
