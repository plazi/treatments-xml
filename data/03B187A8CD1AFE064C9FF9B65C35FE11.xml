<taxonomicName id="4C184D3DCD1DFE074CD3F8F05810F890" box="[219,299,1849,1873]" class="Actinopterygii" family="Gobiidae" genus="Callogobius" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="237" phylum="Chordata" rank="species" species="dori">
<emphasis id="B96CEAACCD1DFE074CD3F8F05810F890" box="[219,299,1849,1873]" italics="true" pageId="12" pageNumber="237">C. dori</emphasis>
</taxonomicName>
examined by 
<collectorName id="26ED5368CD1DFE074DC3F8F15B78F890" box="[459,579,1848,1873]" pageId="12" pageNumber="237">McKinney</collectorName>
(
<specimenCode id="DBBE9EC5CD1DFE074E5AF8F05A38F890" box="[594,771,1849,1873]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="237">
<collectionCode id="ED09AE7BCD1DFE074E5AF8F05B9EF890" box="[594,677,1849,1873]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="237">USNM</collectionCode>
220030
</specimenCode>
, 
<collectingRegion id="49DCF85CCD1DFE074F07F8F15A54F890" box="[783,879,1848,1873]" country="Sudan" name="Al Bahr al Ahmar" pageId="12" pageNumber="237">Red Sea</collectingRegion>
, 
<location id="8EC76065CD1DFE074F73F8F15D1AF890" LSID="urn:lsid:plazi:treatment:03B187A8CD1AFE064C9FF9B65C35FE11:8EC76065CD1DFE074F73F8F15D1AF890" box="[891,1057,1848,1873]" country="Seychelles" county="Ressource Island" municipality="St. Joseph Atoll" name="Gulf of Aqaba" pageId="12" pageNumber="237" stateProvince="Red Sea">Gulf of Aqaba</location>
, 
