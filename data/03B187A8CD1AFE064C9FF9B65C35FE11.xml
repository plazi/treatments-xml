<materialsCitation id="3B703CE3CD1DFE074CCFF8DC5AFDF820" ID-GBIF-Occurrence="1324332048" collectingDate="1964-03-07" collectionCode="USNM" collectorName="McKinney &amp; Master's &amp; B. One &amp; Bay &amp; El Himeira" country="Seychelles" county="Ressource Island" elevation="2" location="Gulf of Aqaba" municipality="St. Joseph Atoll" pageId="12" pageNumber="237" specimenCode="USNM 220030, USNM 385746" specimenCount="4" stateProvince="Red Sea">
