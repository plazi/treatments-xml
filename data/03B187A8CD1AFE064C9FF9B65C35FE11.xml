at 
<collectorName id="26ED5368CD1DFE074875F8F15DC6F890" box="[1149,1277,1848,1873]" pageId="12" pageNumber="237">El Himeira</collectorName>
, 
<specimenCount id="9D1EFD37CD1DFE074900F8F05CAEF890" box="[1288,1429,1848,1873]" pageId="12" pageNumber="237" type="generic">4 specimens</specimenCount>
) was labeled as “ 
<taxonomicName id="4C184D3DCD1DFE074D5FF89458F6F8B4" box="[343,461,1885,1909]" class="Actinopterygii" family="Gobiidae" genus="Callogobius" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="237" phylum="Chordata" rank="species" species="aquilus">
