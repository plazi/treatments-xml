<taxonomicName id="DFAAE0B5FFDDB56462EE6003AD4B2273" box="[1161,1317,1589,1613]" class="Diplopoda" family="Paradoxosomatidae" genus="Desmoxytes" kingdom="Animalia" order="Polydesmida" pageId="7" pageNumber="490" phylum="Arthropoda" rank="species" species="rhinoceros" status="sp. n.">
