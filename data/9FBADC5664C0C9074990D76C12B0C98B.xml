<geoCoordinate id="B5CA6DEAEAC8F2968BEE6870F7530B1F" direction="north" orientation="latitude" precision="5" value="45.9866">45.9866°N</geoCoordinate>
, 
<geoCoordinate id="86CC97C64B9F4230F354E92224E76D81" direction="west" orientation="longitude" precision="5" value="-66.3841">66.3841°W</geoCoordinate>
, 21-29.VII.2009, 4-11.VIII.2009, 11-18.VIII.2009, R. Webster &amp; M.-A. 
<normalizedToken id="2F29705357EC38C9560AF2B1F1DB2D0E" originalValue="Giguère">Giguere</normalizedToken>
