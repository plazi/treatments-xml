<taxonomicName id="0C9F4D15FA441849BB79FE72FE5BFE62" authorityName="Schell" authorityYear="1943" box="[299,453,394,418]" class="Insecta" family="Ochteridae" genus="Ochterus" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="39" phylum="Arthropoda" rank="species" species="hungerfordi">
