<taxonomicName id="0CB546D39721FFF4FD61F91C82D1F950" authority="Matsumura" authorityName="Matsumura" authorityYear="1829" box="[740,1011,1731,1754]" class="Insecta" family="Tortricidae" genus="Semasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="61" phylum="Arthropoda" rank="species" species="phaseoli">
