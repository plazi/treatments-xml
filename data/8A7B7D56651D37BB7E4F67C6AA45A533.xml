<taxonomicName id="63E24E1CE46BB85501186CCBEB5B8CFF" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145632" authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" class="Insecta" family="Cryptoceridae" genus="Cryptocerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptocerus gibbosus Smith" lsidName-HNS="Cryptocerus gibbosus Smith" order="Hymenoptera" pageNumber="605" phylum="Arthropoda" rank="species" species="gibbosus">Cryptocerus gibbosus</taxonomicName>
