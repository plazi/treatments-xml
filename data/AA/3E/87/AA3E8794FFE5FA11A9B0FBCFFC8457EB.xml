<collectingCountry box="[311,460,1258,1284]" name="United States of America" pageId="9" pageNumber="40">United States</collectingCountry>
(
<collectingRegion box="[473,648,1258,1285]" country="United States of America" name="North Carolina" pageId="9" pageNumber="40">North Carolina</collectingRegion>
), 
<collectingCountry box="[666,754,1258,1284]" name="Mexico" pageId="9" pageNumber="40">Mexico</collectingCountry>
