<collectingCountry box="[764,826,1258,1284]" name="Cuba" pageId="9" pageNumber="40">Cuba</collectingCountry>
, Antilles, 
<collectingCountry box="[937,1055,1258,1284]" name="Venezuela" pageId="9" pageNumber="40">Venezuela</collectingCountry>
, 
<collectingCountry box="[1065,1133,1258,1284]" name="Brazil" pageId="9" pageNumber="40">Brazil</collectingCountry>
