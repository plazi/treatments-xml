<taxonomicName authority="Meigen" authorityName="Meigen" box="[1251,1436,1128,1150]" class="Insecta" family="Ephydridae" genus="Discomyza" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="501" phylum="Arthropoda" rank="genus">
