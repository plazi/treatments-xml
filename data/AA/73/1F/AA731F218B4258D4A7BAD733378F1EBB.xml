<paragraph pageId="0" pageNumber="57572">Distribution</paragraph>
<paragraph pageId="0" pageNumber="57572">
<taxonomicName authorityName="Chamberlin" authorityYear="1919" class="Polychaeta" family="Sabellariidae" genus="Idanthyrsus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Idanthyrsus cretus" order="Sabellida" pageId="0" pageNumber="57572" phylum="Annelida" rank="species" species="cretus">
