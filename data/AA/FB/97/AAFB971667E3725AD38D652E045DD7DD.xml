<subSubSection pageId="null" pageNumber="336" type="vernacular_name">
<paragraph pageId="null" pageNumber="336">
<normalizedToken originalValue="Nüßlisalat" pageId="null" pageNumber="336">Nuesslisalat</normalizedToken>
, Feldsalat
</paragraph>
