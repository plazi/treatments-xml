<specimenCount box="[856,910,1562,1585]" pageId="21" pageNumber="352" type="generic">
<quantity box="[856,906,1562,1585]" metricMagnitude="0" metricUnit="m" metricValue="1.0" pageId="21" pageNumber="352" unit="m" value="1.0">1 m</quantity>
.
</specimenCount>
