<taxonomicName baseAuthorityName="K. H. Barnard" baseAuthorityYear="1955" box="[895,1081,1533,1556]" class="Malacostraca" family="Aoridae" genus="Bemlos" kingdom="Animalia" order="Amphipoda" pageId="21" pageNumber="352" phylum="Arthropoda" rank="species" species="teleporus">
