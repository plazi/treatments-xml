<subSubSection pageId="2" pageNumber="15" type="type species">
<paragraph pageId="2" pageNumber="15">Type species.</paragraph>
<paragraph pageId="2" pageNumber="15">
<taxonomicName authority="Choi, Chandler &amp; Park, 2019" authorityName="Choi, Chandler &amp; Park" authorityYear="2019" class="Insecta" family="Staphylinidae" genus="Porongurup" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Porongurup angulatus" order="Coleoptera" pageId="2" pageNumber="15" phylum="Arthropoda" rank="species" species="angulatus">
