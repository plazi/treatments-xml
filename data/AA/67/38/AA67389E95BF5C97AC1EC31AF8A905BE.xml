<subSubSection pageId="2" pageNumber="15" type="etymology">
<paragraph pageId="2" pageNumber="15">Etymology.</paragraph>
<paragraph pageId="2" pageNumber="15">
<taxonomicName authority="Choi, Chandler &amp; Park, 2019" authorityName="Choi, Chandler &amp; Park" authorityYear="2019" class="Insecta" family="Staphylinidae" genus="Porongurup" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Porongurup" order="Coleoptera" pageId="2" pageNumber="15" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="2" pageNumber="15">Porongurup</emphasis>
