<subSubSection box="[198,1101,1605,1631]" pageId="9" pageNumber="10" type="etymology">
<paragraph blockId="9.[151,1437,933,1706]" box="[198,1101,1605,1631]" pageId="9" pageNumber="10">
<emphasis bold="true" box="[198,339,1605,1631]" pageId="9" pageNumber="10">Etymology.</emphasis>
<taxonomicName box="[346,431,1605,1630]" class="Insecta" family="Ecnomidae" genus="Wellsomina" kingdom="Animalia" order="Trichoptera" pageId="9" pageNumber="21" phylum="Arthropoda" rank="species" species="bucera">
