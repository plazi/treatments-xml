<subSubSection box="[151,1270,1746,1770]" pageId="5" pageNumber="6" type="materials_examined">
<paragraph blockId="5.[151,1270,1746,1770]" box="[151,1270,1746,1770]" pageId="5" pageNumber="6">
<typeStatus box="[151,204,1746,1769]" pageId="5" pageNumber="6">Type</typeStatus>
species. 
<taxonomicName box="[300,500,1746,1769]" class="Insecta" family="Ecnomidae" genus="Wellsomina" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="stuarti" status="sp. nov.">
<emphasis box="[300,500,1746,1769]" italics="true" pageId="5" pageNumber="6">Wellsomina stuarti</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[507,593,1746,1770]" pageId="5" pageNumber="6" rank="species">
<emphasis bold="true" box="[507,593,1746,1770]" pageId="5" pageNumber="6">sp. nov.</emphasis>
</taxonomicNameLabel>
by original designation, from the Northern Territory, 
<collectingCountry box="[1165,1266,1746,1769]" name="Australia" pageId="5" pageNumber="6">Australia</collectingCountry>
