<subSubSection pageId="17" pageNumber="18" type="nomenclature">
<paragraph pageId="17" pageNumber="18">
<taxonomicName LSID="http://zoobank.org/FD1907D2-8250-4909-A886-027FEFA5F496" class="Insecta" family="Hydroptilidae" genus="Acritoptila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acritoptila macrospina" order="Trichoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="macrospina">Acritoptila macrospina</taxonomicName>
