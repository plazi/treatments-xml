<taxonomicName class="Insecta" family="Metretopodidae" genus="Siphloplecton" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Siphloplecton landolti" order="Ephemeroptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="landolti">
