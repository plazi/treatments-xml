, 
<collectorName box="[264,442,1177,1203]" pageId="14" pageNumber="15">O.M.P.Oliveira</collectorName>
coll., two adults (
<quantity box="[652,780,1177,1203]" metricMagnitude="-2" metricUnit="m" metricValue="4.2" metricValueMax="4.8" metricValueMin="3.6" pageId="14" pageNumber="15" unit="mm" value="42.0" valueMax="48.0" valueMin="36.0">36–48 mm</quantity>
