male, and 
<emphasis bold="true" box="[885,911,1654,1676]" pageId="19" pageNumber="52">24</emphasis>
<emphasis box="[916,1039,1654,1676]" italics="true" pageId="19" pageNumber="52">
<taxonomicName authorityName="Thomson" authorityYear="1862" box="[916,1034,1654,1676]" class="Insecta" family="Lucanidae" genus="Chiasognathus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="52" phylum="Arthropoda" rank="species" species="mniszechii">C. mniszechii</taxonomicName>
,
</emphasis>
