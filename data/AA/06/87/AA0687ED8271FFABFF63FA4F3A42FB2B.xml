, 
<taxonomicName authorityName="Reiche" authorityYear="1850" box="[591,706,1622,1644]" class="Insecta" family="Lucanidae" genus="Chiasognathus" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="59" phylum="Arthropoda" rank="species" species="jousselinii">C. jousselinii</taxonomicName>
