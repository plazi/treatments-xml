<quantity box="[140,315,202,229]" metricMagnitude="-2" metricUnit="m" metricValue="1.375" metricValueMax="1.5699999999999998" metricValueMin="1.18" pageId="27" pageNumber="60" unit="mm" value="13.75" valueMax="15.7" valueMin="11.8">11.8–15.7 mm</quantity>
