<taxonomicName authority="Scopoli, 1777" authorityName="Scopoli" authorityYear="1777" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bulimus thompsonii" order="Stylommatophora" pageId="44" pageNumber="45" phylum="Mollusca" rank="species" species="thompsonii">Bulimus thompsonii</taxonomicName>
