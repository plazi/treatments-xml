<paragraph blockId="4.[138,1185,622,809]" pageId="4" pageNumber="3061">
<materialsCitation collectionCode="SMNH" pageId="4" pageNumber="3061" specimenCode="SMNH 5297" specimenCount="1" specimenCount-exuvia="1" typeStatus="syntype">
<typeStatus box="[165,273,720,744]" pageId="4" pageNumber="3061">Syntypes</typeStatus>
