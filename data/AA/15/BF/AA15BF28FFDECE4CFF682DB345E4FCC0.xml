<specimenCount box="[1239,1259,615,640]" pageId="4" pageNumber="132" type="male">♂</specimenCount>
and 
<specimenCount box="[1314,1334,615,640]" pageId="4" pageNumber="132" type="female">♀</specimenCount>
9 
<superScript attach="left" box="[1352,1367,615,629]" fontSize="6" pageId="4" pageNumber="132">th</superScript>
sterna (
