<geoCoordinate id="EE7B5061FFE3C83077177B12EFC97112" box="[1082,1207,1319,1343]" degrees="47" direction="east" minutes="57" orientation="longitude" pageId="15" pageNumber="166" precision="15" seconds="07" value="47.951946">47°57’07”E</geoCoordinate>
