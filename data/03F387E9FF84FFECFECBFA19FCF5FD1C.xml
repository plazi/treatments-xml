<quantity id="4CA29B1AFF84FFEBFC8FF9A9FCABF9C5" box="[780,860,1611,1637]" metricMagnitude="4" metricUnit="m" metricValue="1.9" pageId="11" pageNumber="12" unit="km" value="19.0">19 km</quantity>
