<number id="3DD25A48FF8C4259FD63FBC407D1EF42" box="[665,708,1052,1076]" pageId="127" pageNumber="128" value="3.0" valueMax="4.0" valueMin="2.0">2–4</number>
, 
<number id="3DD25A48FF8C4259FD2AFBC407E6EF43" box="[720,755,1052,1077]" pageId="127" pageNumber="128" value="6.1">6.1</number>
