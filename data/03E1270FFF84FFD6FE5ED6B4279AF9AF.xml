<taxonomicName id="4C48ED9AFF85FFD6FD22D7F527BAFA68" authorityName="Kejval &amp; Cz" authorityYear="2013" box="[679,752,1501,1524]" class="Insecta" family="Anthicidae" genus="Mecynotarsus" kingdom="Animalia" order="Coleoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="obesus">
