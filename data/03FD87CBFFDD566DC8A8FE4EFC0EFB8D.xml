(= 
<taxonomicName id="4C544D5EFFDD566DCBEAFE23FEC4FDE4" authority="sensu Dubatolov et al. (2012)" authorityName="Dubatolov et al." authorityYear="2012" class="Insecta" family="Erebidae" genus="Stigmatophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="129" phylum="Arthropoda" rank="species" species="roseivena">
