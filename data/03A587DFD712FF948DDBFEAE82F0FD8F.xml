); pronotal length (
<number id="3DC424EED712FF97895FFCBE8304FCC8" box="[1043,1132,823,848]" pageId="13" pageNumber="458" value="3.35" valueMax="3.5" valueMin="3.2">3.2–3.5</number>
); pronotal width (
