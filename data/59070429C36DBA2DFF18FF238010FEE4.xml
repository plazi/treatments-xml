<taxonomicName id="16AECEBCC36DBA2DFB45FF798480FF13" baseAuthorityName="Bouche" baseAuthorityYear="1844" box="[1226,1436,196,221]" class="Insecta" family="Coccidae" genus="Eulecanium" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="corni">
