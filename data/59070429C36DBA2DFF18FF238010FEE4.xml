<taxonomicName id="16AECEBCC36DBA2DFC1BFF7982F6FF13" authorityName="Linnaeus" authorityYear="1758" box="[916,1002,196,221]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
