<taxonomicName id="16AECEBCC36DBA2DFF43FF5780F0FECD" box="[204,492,234,259]" class="Insecta" family="Coccidae" genus="Parthenolecanium" kingdom="Animalia" order="Hemiptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="corni">
<emphasis id="E3DA692DC36DBA2DFF43FF5780F0FECD" box="[204,492,234,259]" italics="true" pageId="17" pageNumber="18">Parthenolecanium corni</emphasis>
</taxonomicName>
