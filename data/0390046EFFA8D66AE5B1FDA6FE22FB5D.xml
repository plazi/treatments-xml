<taxonomicName id="4C39CEFBFFA8D66AE4DCFC6CFD2CFC7D" box="[506,750,934,960]" class="Magnoliopsida" family="Asteraceae" genus="Ageratum" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="155" phylum="Tracheophyta" rank="species" species="conyzoides">
<emphasis id="B94D696AFFA8D66AE4DCFC6CFD2CFC7D" box="[506,750,934,960]" italics="true" pageId="4" pageNumber="155">Ageratum conyzoides</emphasis>
</taxonomicName>
L., 
<taxonomicName id="4C39CEFBFFA8D66AE607FC6CFC6DFC7D" box="[801,943,934,960]" class="Insecta" family="Cerambycidae" genus="Eclipta" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="155" phylum="Arthropoda" rank="species" species="alba">
<emphasis id="B94D696AFFA8D66AE607FC6CFC6DFC7D" box="[801,943,934,960]" italics="true" pageId="4" pageNumber="155">Eclipta alba</emphasis>
</taxonomicName>
(L.) Hassk, 
<taxonomicName id="4C39CEFBFFA8D66AE162FC6CFACCFC7D" box="[1092,1294,934,960]" class="Magnoliopsida" family="Asteraceae" genus="Melanthera" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="155" phylum="Tracheophyta" rank="species" species="nivea">
