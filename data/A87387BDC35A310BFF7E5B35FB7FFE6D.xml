<taxonomicName id="E7DA4D28C35A3108FEE35B89FE39FCD2" authorityName="Franz" authorityYear="1967" box="[266,409,763,787]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="82" phylum="Arthropoda" rank="genus">Protoconnus</taxonomicName>
/ 
<taxonomicName id="E7DA4D28C35A3108FE5C5B88FDBFFCD2" box="[437,543,762,787]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="62" phylum="Arthropoda" rank="species" species="peruensis">peruensis</taxonomicName>
/ m. / PARATYPUS&quot; [yellow, handwritten and printed], &quot; 
<taxonomicName id="E7DA4D28C35A3108FB2B5B89FA2AFCD3" authorityName="Franz" authorityYear="1967" box="[1218,1418,763,786]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis id="12AEEAB9C35A3108FB2B5B89FA2AFCD3" box="[1218,1418,763,786]" italics="true" pageId="21" pageNumber="62">PROTOCONNUS</emphasis>
</taxonomicName>
/ 
<taxonomicName id="E7DA4D28C35A3108FF7E5A6CFEECFCF6" authorityName="Jałoszyński" authorityYear="2018" box="[151,332,798,823]" class="Insecta" family="Staphylinidae" genus="Protoconnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="62" phylum="Arthropoda" rank="species" species="quillabambanus">
<emphasis id="12AEEAB9C35A3108FF7E5A6CFEECFCF6" box="[151,332,798,823]" italics="true" pageId="21" pageNumber="62">quillabambanus</emphasis>
