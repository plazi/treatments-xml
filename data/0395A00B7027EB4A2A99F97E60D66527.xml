<paragraph id="8B83111D7027EB492AC9F84267186087" blockId="5.[151,1437,1770,2012]" box="[199,585,1950,1977]" pageId="5" pageNumber="6">
<emphasis id="B948CD0F7027EB492AC9F84264576086" bold="true" box="[199,262,1950,1976]" pageId="5" pageNumber="6">Host.</emphasis>
<taxonomicName id="4C3C6A9E7027EB492B03F84264D86086" box="[269,393,1950,1976]" class="Magnoliopsida" family="Urticaceae" kingdom="Plantae" order="Rosales" pageId="5" pageNumber="6" phylum="Tracheophyta" rank="family">Urticaceae</taxonomicName>
