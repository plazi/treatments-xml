<subSubSection id="D3A195D6FFD6FFE4FF04FB6CFABBFB00" box="[151,1308,1163,1190]" pageId="15" pageNumber="16" type="etymology">
<paragraph id="9B04C65DFFD6FFE4FF04FB6CFABBFB00" blockId="15.[151,1438,1163,2002]" box="[151,1308,1163,1190]" pageId="15" pageNumber="16">
<emphasis id="A9CF1A4FFFD6FFE4FF04FB6CFE80FB03" bold="true" box="[151,295,1163,1189]" pageId="15" pageNumber="16">Etymology:</emphasis>
