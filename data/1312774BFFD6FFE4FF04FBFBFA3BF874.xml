<taxonomicName id="5CBBBDDEFFD6FFE4FDADFB6BFD26FB00" box="[574,641,1164,1190]" class="Insecta" family="Coenagrionidae" genus="Tuberculobasis" kingdom="Animalia" order="Odonata" pageId="15" pageNumber="32" phylum="Arthropoda" rank="species" species="arara">Arara</taxonomicName>
