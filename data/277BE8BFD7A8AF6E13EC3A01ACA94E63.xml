<taxonomicName id="E559645683F80E308B29B480C3791E50" authorityName="LeConte" authorityYear="1847" class="Insecta" family="Carabidae" genus="Piosoma" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="684" pageNumber="685" phylum="Arthropoda" rank="species" species="setosum">
<emphasis id="EE15B74FFF44512C2E76347068298E8C" italics="true" pageId="684" pageNumber="685">Piosoma setosum</emphasis>
</taxonomicName>
