<taxonomicName id="8DD84D78FFCFBC55FD372CCEFCB3F849" authorityName="Lesson" authorityYear="1828" box="[735,858,1928,1950]" class="Aves" family="Buphagidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Passeriformes" pageId="21" pageNumber="422" phylum="Chordata" rank="family">Buphagidae</taxonomicName>
