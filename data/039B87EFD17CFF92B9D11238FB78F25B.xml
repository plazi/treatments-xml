<taxonomicName id="4C324D7AD17CFF92B80E1334FD4BF25B" authority="subtessellatus" box="[328,706,1300,1325]" class="Insecta" family="Crabronidae" genus="Liris" kingdom="Animalia" order="Hymenoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="subSpecies" species="subtessellatus" subSpecies="subtessellatus">
