<taxonomicName id="8DEE4D50BF5DFFF9FD78F0986D6E6369" authority="Poppius, 1913" authorityName="Poppius" authorityYear="1913" box="[667,1040,250,274]" class="Insecta" family="Miridae" genus="Isometopidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="165" phylum="Arthropoda" rank="species" species="lieweni">
