<taxonomicName id="4C2C4BA3FFE3FF993F84FD81FA874D7D" box="[1215,1361,512,536]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="465" phylum="Arthropoda" rank="species" species="jalapensis">
<emphasis id="B958EC32FFE3FF993F84FD81FA874D7D" box="[1215,1361,512,536]" italics="true" pageId="10" pageNumber="465">C. jalapensis</emphasis>
</taxonomicName>
