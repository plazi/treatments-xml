<taxonomicName id="4C2C4BA3FFE3FF993969FDA4FD234D59" box="[594,757,548,572]" class="Insecta" family="Curculionidae" genus="Cnemidophorus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="465" phylum="Arthropoda" rank="species" species="ocellifer">
