<taxonomicName id="4C674D281456FF81FEB3FADCD5C0FAD4" box="[319,496,1323,1342]" class="Insecta" family="Chrysomelidae" genus="Aulonodera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="219" phylum="Arthropoda" rank="species" species="darwini">
