, 
<collectingRegion id="B455B4EFFFD3FFD0F9A0FE15FCF1E52D" box="[756,821,511,536]" country="Maldives" name="Dhekunu" pageId="5" pageNumber="562">South</collectingRegion>
<collectingCountry id="0E863A9DFFD3FFD0F86AFE15FC45E52D" box="[830,897,511,536]" name="China" pageId="5" pageNumber="562">China</collectingCountry>
Sea, 
<collectingCountry id="0E863A9DFFD3FFD0F894FDEAFBC7E522" box="[960,1027,512,535]" name="Japan" pageId="5" pageNumber="562">Japan</collectingCountry>
