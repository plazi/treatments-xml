<taxonomicName id="4C1B4D374231FFF8FE57FA84FC81FA01" authority="(Haliday, 1833)" baseAuthorityName="Haliday" baseAuthorityYear="1833" box="[454,807,1397,1419]" class="Insecta" family="Phoridae" genus="Metopina" kingdom="Animalia" order="Diptera" pageId="101" pageNumber="605" phylum="Arthropoda" rank="species" species="galeata">
