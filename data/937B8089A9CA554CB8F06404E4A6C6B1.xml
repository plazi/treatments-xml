<quantity id="3DF1D7D0AC8EB2909D29E7FE72E6FFFF" metricMagnitude="-2" metricUnit="m" metricValue="1.25" metricValueMax="1.5" metricValueMin="1.0" unit="mm" value="12.5" valueMax="15.0" valueMin="10.0">10-15 mm</quantity>
