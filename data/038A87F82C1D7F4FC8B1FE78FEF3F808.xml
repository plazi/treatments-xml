<quantity id="4CDB9B0B2C1D7F4FCC9DFDB1FA81FD42" box="[1211,1305,630,656]" metricMagnitude="3" metricUnit="m" metricValue="1.58" pageId="21" pageNumber="22" unit="m" value="1580.0">1580 m</quantity>
