<taxonomicName id="4C474D1C3B6EA1416995F8CEAA8CF83C" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[1006,1167,1906,1932]" class="Arachnida" family="Gnaphosidae" genus="Kituba" kingdom="Animalia" order="Araneae" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="langalanga" status="sp. nov.">
<emphasis id="B933EA8D3B6EA1416995F8CEAA8CF83C" box="[1006,1167,1906,1932]" italics="true" pageId="49" pageNumber="50">K. langalanga</emphasis>
</taxonomicName>
<emphasis id="B933EA8D3B6EA1416EEDF8CFAAF3F83C" bold="true" box="[1174,1264,1906,1932]" pageId="49" pageNumber="50">
<taxonomicNameLabel id="A20057F63B6EA1416EEDF8CFAAF3F83C" box="[1174,1264,1906,1932]" pageId="49" pageNumber="50" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
