<taxonomicName id="5A452D517B7BCABD189BECC1C836AE46" class="Insecta" genus="P." higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="P. hernanlequericai" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="hernanlequericai">
