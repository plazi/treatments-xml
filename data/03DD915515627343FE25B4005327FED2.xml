<quantity id="4C8C8DA615627349FE18B4C15525FA98" box="[473,564,1277,1301]" metricMagnitude="-3" metricUnit="m" metricValue="3.3" pageId="3" pageNumber="1957" unit="mm" value="3.3">3.3 mm</quantity>
