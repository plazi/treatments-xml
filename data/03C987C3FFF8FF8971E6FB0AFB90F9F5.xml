<taxonomicName id="4C604D56FFE6FF8970D9F99BFB91F9D1" authority="Michener (Michener, 2002)" authorityName="Michener (Michener" authorityYear="2002" box="[424,1076,1591,1616]" class="Insecta" family="Colletidae" genus="Chilicola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="espeleticola" subGenus="Oroediscelis">
