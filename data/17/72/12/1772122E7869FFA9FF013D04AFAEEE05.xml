<paragraph blockId="111.[140,1108,1047,1108]" pageId="111" pageNumber="112">
<emphasis bold="true" box="[185,323,1047,1073]" pageId="111" pageNumber="112">Etymology:</emphasis>
<taxonomicName authorityName="Wesener &amp; Enghoff &amp; Sierwald" authorityYear="2009" box="[330,436,1047,1072]" class="Diplopoda" family="Spirobolellidae" genus="Hylekobolus" kingdom="Animalia" order="Spirobolida" pageId="111" pageNumber="112" phylum="Arthropoda" rank="species" species="montanus">
<emphasis box="[330,436,1047,1072]" italics="true" pageId="111" pageNumber="112">montanus</emphasis>
