<number box="[324,376,2002,2028]" pageId="41" pageNumber="42" value="0.16">0.16</number>
long, 
<number box="[453,505,2002,2028]" pageId="41" pageNumber="42" value="0.24">0.24</number>
wide. Sternum 
<number box="[695,747,2002,2028]" pageId="41" pageNumber="42" value="1.12">1.12</number>
long, 
<number box="[824,876,2002,2028]" pageId="41" pageNumber="42" value="0.9">0.90</number>
wide. Leg measurements: I: femur 
<number box="[1304,1344,2002,2028]" pageId="41" pageNumber="42" value="2.9">2.9</number>
, patella 
<number box="[151,191,152,178]" pageId="42" pageNumber="43" value="0.5">0.5</number>
, tibia 
<number box="[261,300,152,178]" pageId="42" pageNumber="43" value="3.1">3.1</number>
