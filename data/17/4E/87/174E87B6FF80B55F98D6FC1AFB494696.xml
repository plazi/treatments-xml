, 
<number box="[248,287,190,216]" pageId="42" pageNumber="43" value="0.5">0.5</number>
, 
<number box="[299,338,190,216]" pageId="42" pageNumber="43" value="2.1">2.1</number>
, 
<number box="[352,391,190,216]" pageId="42" pageNumber="43" value="2.2">2.2</number>
, 
<number box="[403,442,190,216]" pageId="42" pageNumber="43" value="0.6">0.6</number>
, 
<number box="[455,495,190,216]" pageId="42" pageNumber="43" value="7.7">7.7</number>
