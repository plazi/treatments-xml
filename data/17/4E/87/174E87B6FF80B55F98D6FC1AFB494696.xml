<number box="[682,736,152,178]" pageId="42" pageNumber="43" value="10.6">10.6</number>
; II: 
<number box="[783,822,152,178]" pageId="42" pageNumber="43" value="2.1">2.1</number>
, 
<number box="[835,874,152,178]" pageId="42" pageNumber="43" value="0.5">0.5</number>
, 
<number box="[885,925,152,178]" pageId="42" pageNumber="43" value="2.2">2.2</number>
, 
<number box="[937,976,152,178]" pageId="42" pageNumber="43" value="2.6">2.6</number>
, 
<number box="[988,1027,152,178]" pageId="42" pageNumber="43" value="0.6">0.6</number>
, 8.0; III: 
<number box="[1135,1174,152,178]" pageId="42" pageNumber="43" value="1.6">1.6</number>
, 
<number box="[1187,1226,152,178]" pageId="42" pageNumber="43" value="0.5">0.5</number>
, 
<number box="[1238,1277,152,178]" pageId="42" pageNumber="43" value="1.2">1.2</number>
