<subSubSection box="[198,1377,1307,1334]" pageId="25" pageNumber="26" type="etymology">
<paragraph blockId="25.[151,1437,1230,2030]" box="[198,1377,1307,1334]" pageId="25" pageNumber="26">
<emphasis bold="true" box="[198,331,1307,1333]" pageId="25" pageNumber="26">Etymology</emphasis>
