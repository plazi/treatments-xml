<number box="[1073,1113,1196,1222]" pageId="28" pageNumber="29" value="4.7">4.7</number>
. Carapace domed, 
<number box="[1333,1370,1196,1222]" pageId="28" pageNumber="29" value="2.1">2.1</number>
long, 
<number box="[151,188,1234,1260]" pageId="28" pageNumber="29" value="1.9">1.9</number>
