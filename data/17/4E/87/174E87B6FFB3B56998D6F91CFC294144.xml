long, 
<number box="[373,424,1273,1299]" pageId="28" pageNumber="29" value="0.28">0.28</number>
wide. Sternum 
<number box="[608,659,1273,1299]" pageId="28" pageNumber="29" value="1.04">1.04</number>
long, 
<number box="[732,783,1273,1299]" pageId="28" pageNumber="29" value="0.86">0.86</number>
wide. Leg measurements: I: femur 
<number box="[1198,1237,1273,1299]" pageId="28" pageNumber="29" value="2.7">2.7</number>
, patella 
<number box="[1333,1372,1273,1299]" pageId="28" pageNumber="29" value="0.5">0.5</number>
, tibia 
<number box="[151,191,1312,1338]" pageId="28" pageNumber="29" value="2.8">2.8</number>
, metatarsus 
<number box="[338,377,1312,1338]" pageId="28" pageNumber="29" value="3.1">3.1</number>
, tarsus 
<number box="[467,507,1312,1338]" pageId="28" pageNumber="29" value="0.6">0.6</number>
, total 
