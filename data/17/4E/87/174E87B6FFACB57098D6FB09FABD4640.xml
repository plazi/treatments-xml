<subSubSection box="[197,1189,1462,1488]" pageId="4" pageNumber="5" type="etymology">
<paragraph blockId="4.[151,1437,1384,2030]" box="[197,1189,1462,1488]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[197,330,1462,1488]" pageId="4" pageNumber="5">Etymology</emphasis>
