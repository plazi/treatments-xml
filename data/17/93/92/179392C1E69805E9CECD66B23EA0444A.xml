<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:159446" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName="Monomorium salomonis st. subopacum var. senegalensis Santschi" lsidName-HNS="Monomorium salomonis st. subopacum var. senegalensis Santschi" order="Hymenoptera" pageNumber="5" phylum="Arthropoda" rank="variety" species="salomonis" subSpecies="subopacum" variety="senegalensis">Monomorium salomonis L. st. subopacum, Sm. v. senegalensis</taxonomicName>
<taxonomicNameLabel rank="variety">n. var.</taxonomicNameLabel>
