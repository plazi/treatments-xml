<taxonomicName authority="Malicky &amp; Chantaramongkol 1993" authorityName="Malicky &amp; Chantaramongkol" authorityYear="1993" box="[527,1035,403,429]" class="Insecta" family="Psychomyiidae" genus="Psychomyia" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="135" phylum="Arthropoda" rank="species" species="monto">
