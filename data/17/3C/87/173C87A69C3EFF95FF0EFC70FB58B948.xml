<taxonomicName box="[151,257,672,697]" class="Magnoliopsida" family="Asteraceae" genus="Cousinia" kingdom="Plantae" order="Asterales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="genus">
<emphasis box="[151,257,672,697]" italics="true" pageId="62" pageNumber="63">Cousinia</emphasis>
</taxonomicName>
or 
<taxonomicName box="[296,476,672,698]" class="Magnoliopsida" family="Asteraceae" genus="Scorzonera" kingdom="Plantae" order="Asterales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="species" species="undetermined">
<emphasis box="[296,429,672,697]" italics="true" pageId="62" pageNumber="63">Scorzonera</emphasis>
spp
