<paragraph blockId="21.[151,1437,401,454]" pageId="21" pageNumber="173">
<taxonomicName authority="Haller, 1880: 375" authorityName="Haller" authorityPageNumber="375" authorityYear="1880" box="[151,574,401,423]" class="Malacostraca" family="Cymothoidae" genus="Cymothoa" kingdom="Animalia" order="Isopoda" pageId="21" pageNumber="173" phylum="Arthropoda" rank="species" species="rotundifrons">
<emphasis box="[151,387,401,423]" italics="true" pageId="21" pageNumber="173">Cymothoa rotundifrons</emphasis>
Haller, 
<number box="[470,524,402,423]" pageId="21" pageNumber="173" value="1880.0">1880</number>
: 
<number box="[536,574,401,423]" pageId="21" pageNumber="173" value="376.0" valueMax="377.0" valueMin="375.0">375</number>
</taxonomicName>
–377, pl. XVIII, figs. 
<number box="[799,840,402,423]" pageId="21" pageNumber="173" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
; Nierstrasz, 
<number box="[969,1023,402,423]" pageId="21" pageNumber="173" value="1931.0">1931</number>
: 
<number box="[1034,1075,401,423]" pageId="21" pageNumber="173" value="136.0">136</number>
; Trilles, 
<number box="[1165,1219,402,423]" pageId="21" pageNumber="173" value="1994.0">1994</number>
: 
<number box="[1230,1271,402,423]" pageId="21" pageNumber="173" value="137.0">137</number>
; Kensley, 
<number box="[1378,1433,402,423]" pageId="21" pageNumber="173" value="2001.0">2001</number>
: 
<number box="[199,237,432,453]" pageId="21" pageNumber="173" value="233.0">233</number>
