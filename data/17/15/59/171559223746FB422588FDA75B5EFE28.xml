<taxonomicName authority="(Geoffroy, 1785)" baseAuthorityName="Geoffroy" baseAuthorityYear="1785" box="[425,879,531,555]" class="Insecta" family="Scutelleridae" genus="Eurygaster" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="testudinaria">
<emphasis bold="true" box="[425,879,531,555]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[425,685,531,555]" italics="true" pageId="10" pageNumber="11">Eurygaster testudinaria</emphasis>
