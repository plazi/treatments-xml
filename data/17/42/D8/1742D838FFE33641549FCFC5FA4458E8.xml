<taxonomicName authorityName="Priesner" authorityYear="1933" box="[247,449,657,684]" class="Insecta" family="Phlaeothripidae" genus="Mesandrothrips" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Thysanoptera" pageId="5" pageNumber="332" phylum="Arthropoda" rank="genus">
