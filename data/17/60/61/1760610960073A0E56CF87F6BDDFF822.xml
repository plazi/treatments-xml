<paragraph blockId="1.[151,600,510,571]" box="[151,600,546,571]" pageId="1" pageNumber="93">
Figures 
<number box="[242,326,546,571]" pageId="1" pageNumber="93" value="4.075" valueMax="7.15" valueMin="1.0">1–7, 15</number>
– 
<number box="[342,412,546,571]" pageId="1" pageNumber="93" value="17.21">17, 21</number>
, 
<number box="[424,497,546,571]" pageId="1" pageNumber="93" value="25.29">25, 29</number>
, 
<number box="[508,536,547,571]" pageId="1" pageNumber="93" value="32.0">32</number>
&amp; 
