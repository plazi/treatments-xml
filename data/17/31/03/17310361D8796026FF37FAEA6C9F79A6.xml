<taxonomicName box="[457,758,1868,1890]" class="Insecta" family="Coccidae" genus="Austrolecanium" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="127" phylum="Arthropoda" rank="species" species="cryptocaryae">
<taxonomicName box="[457,616,1868,1890]" class="Insecta" family="Coccidae" genus="Austrolecanium" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="8" pageNumber="127" phylum="Arthropoda" rank="genus">
