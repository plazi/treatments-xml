<paragraph pageId="18" pageNumber="125">
<taxonomicName LSID="http://zoobank.org/89E3D8D8-60B7-419A-8129-9D49A0D21EAD" authority="Sharkey" authorityName="Sharkey" authorityYear="2017" class="Insecta" family="Braconidae" genus="Zosteragathis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zosteragathis" order="Hymenoptera" pageId="18" pageNumber="125" phylum="Arthropoda" rank="genus">Zosteragathis Sharkey</taxonomicName>
