<taxonomicName id="69CC4D2BD522FFFE09FA7266FEA7FB72" baseAuthorityName="Archibald and Averianov" baseAuthorityYear="2001" box="[214,375,1135,1158]" class="Mammalia" family="Nyctitheriidae" genus="Paranyctoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Soricomorpha" pageId="72" pageNumber="73" phylum="Chordata" rank="genus">
<emphasis id="9CB8EABAD522FFFE09FA7266FEA7FB72" box="[214,375,1135,1158]" italics="true" pageId="72" pageNumber="73">Paranyctoides</emphasis>
</taxonomicName>
and 
<taxonomicName id="69CC4D2BD522FFFE08997266FDE8FB72" baseAuthorityName="Averianov and Archibald" baseAuthorityYear="2005" box="[437,568,1135,1158]" class="Mammalia" family="Zhelestidae" genus="Eozhelestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Proteutheria" pageId="72" pageNumber="73" phylum="Chordata" rank="genus">
<emphasis id="9CB8EABAD522FFFE08997266FDE8FB72" box="[437,568,1135,1158]" italics="true" pageId="72" pageNumber="73">Eozhelestes</emphasis>
</taxonomicName>
</heading>
</paragraph>
