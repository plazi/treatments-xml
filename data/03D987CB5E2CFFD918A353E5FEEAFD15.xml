<emphasis id="B904EACF5E2CFFD818B85107FCF4F994" box="[703,852,1564,1587]" italics="true" pageId="20" pageNumber="175">nom. superfl. pro</emphasis>
<taxonomicName id="4C704D5E5E2CFFD8195E5107FB15F994" authority="Thouars (1822)" authorityName="Thouars" authorityYear="1822" box="[857,1205,1564,1587]" class="Liliopsida" family="Orchidaceae" genus="Dryopeia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asparagales" pageId="20" pageNumber="175" phylum="Tracheophyta" rank="species" species="tripetaloides">
