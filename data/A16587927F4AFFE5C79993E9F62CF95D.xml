</taxonomicName>
<taxonomicName id="EECC4D077F4BFFE5C6789186F681FB5A" authority="Zhang, Yin, Bao &amp; Kim, 1997" authorityName="Zhang, Yin, Bao &amp; Kim" authorityYear="1997" box="[374,733,1242,1264]" pageId="1" pageNumber="184" rank="species" species="gongi">
