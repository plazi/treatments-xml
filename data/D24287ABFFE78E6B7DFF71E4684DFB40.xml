v*1943 
<taxonomicName id="9DEB4D3EFFE78E147C4F70636E63FA43" authority="Durham" authorityName="Durham" authorityYear="2008" box="[295,604,1403,1426]" class="Anthozoa" family="Deltocyathidae" genus="Deltocyathus" kingdom="Animalia" order="Scleractinia" pageId="63" pageNumber="64" phylum="Cnidaria" rank="species" species="whitei" status="n. sp.">
