<subSubSection id="C3798EA7F038DA1814AB3D45A0D5661B" box="[199,968,812,833]" pageId="5" pageNumber="67" type="etymology">
<paragraph id="8BDCDD2CF038DA1814AB3D45A0D5661B" blockId="5.[175,1005,732,913]" box="[199,968,812,833]" pageId="5" pageNumber="67">
<emphasis id="B917013EF038DA1814AB3D45A226661B" bold="true" box="[199,315,812,833]" pageId="5" pageNumber="67">Etymology.</emphasis>
