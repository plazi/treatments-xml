<taxonomicName id="4C06A6F6FFCBFFD66E5D8C8CA330FF14" authority="Shen &amp; Sun in Sun, Wu &amp; Shen, 1978" authorityName="Shen &amp; Sun in Sun, Wu &amp; Shen" authorityYear="1978" box="[320,874,224,251]" class="Polychaeta" family="Nereididae" genus="Nereis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="16" pageNumber="17" phylum="Annelida" rank="species" species="dayana">
