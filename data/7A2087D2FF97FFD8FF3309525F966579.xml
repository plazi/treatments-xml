<taxonomicName id="35894D47FF94FFD8FD700CF25D7E62F1" box="[724,868,650,675]" class="Insecta" family="Mantidae" genus="Mantis" kingdom="Animalia" order="Mantodea" pageId="10" pageNumber="140" phylum="Arthropoda" rank="species" species="phyllopus">
