<specimenCount id="30CEA38CFF98FFFFFA8D4A6BFAFCFE8B" box="[1312,1393,313,340]" pageId="57" pageNumber="58" type="generic">
<quantity id="E130C5E0FF98FFFFFA8D4A6BFAE0FE8B" box="[1312,1389,313,340]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="57" pageNumber="58" unit="m" value="500.0">500 m</quantity>
