<geoCoordinate id="639E25A58022ACF9210EE343D82E41E0" direction="north" orientation="latitude" precision="55" value="9.064">09.064°N</geoCoordinate>
, 
<geoCoordinate id="38DA7965A8691430892644646DE78324" direction="east" orientation="longitude" precision="55" value="125.642">125.642°E</geoCoordinate>
