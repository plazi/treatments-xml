van Doesburg (
<number id="12538A204C5C7751A895E2A1FE67BFC7" box="[444,508,1922,1948]" pageId="5" pageNumber="26" value="1957.0">1957</number>
: 
<number id="12538A204C5C7751AB2EE2A1FDADBFC7" box="[519,566,1922,1948]" pageId="5" pageNumber="26" value="109.0">109</number>
), 
<taxonomicName id="639BE3844C5C7751AB63E2A0FBA4BFC7" authority="Keiser (1971: 256)" authorityName="Keiser" authorityPageNumber="256" authorityYear="1971" box="[586,1087,1922,1948]" class="Insecta" family="Syrphidae" genus="Ceratophya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="26" phylum="Arthropoda" rank="species" species="madecassa">
<emphasis id="96EF44154C5C7751AB63E2A0FCC6BFC7" box="[586,861,1923,1948]" italics="true" pageId="5" pageNumber="26">Ceratophya madecassa</emphasis>
<bibRefCitation id="C00AE5F64C5C7751AA4AE2A1FBA4BFC7" author="Keiser" box="[867,1087,1922,1948]" pageId="5" pageNumber="47" refString="Keiser, F. (1971) Syrphidae von Madagaskar (Dipt.). Verhandlungen der Naturforschenden Gesellschaft in Basal, 81, 223 - 318. [1971.11. ??]" type="journal article" year="1971">
Keiser (
<number id="12538A204C5C7751AA96E2A1FC65BFC7" box="[959,1022,1922,1948]" pageId="5" pageNumber="26" value="1971.0">1971</number>
