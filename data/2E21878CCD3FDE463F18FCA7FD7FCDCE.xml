<subSubSection id="EE926511CD3FDE593F20FB49FB17CB86" pageId="15" pageNumber="416" type="etymology">
<paragraph id="A637369ACD3FDE593F20FB49FB17CB86" blockId="15.[840,1477,1221,1269]" pageId="15" pageNumber="416">
<emphasis id="94FCEA88CD3FDE593F20FB49FC7CCBA9" box="[840,926,1221,1242]" italics="true" pageId="15" pageNumber="416">Etymology</emphasis>
