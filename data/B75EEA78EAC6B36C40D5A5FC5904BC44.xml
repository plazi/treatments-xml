<taxonomicName id="29D834FE5287C199ED92CAED39EC8E84" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:298362" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera perparva Bolton &amp; Fisher" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="perparva">perparva</taxonomicName>
