<taxonomicName id="A3BF865AC68FCEC0B4169BF37C943169" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:298359" class="Insecta" family="Formicidae" genus="Hypoponera" genus.completionEvidence="missing, matching species" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Hypoponera molesta Bolton &amp; Fisher" order="Hymenoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="molesta">molesta</taxonomicName>
