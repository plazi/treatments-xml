<taxonomicName id="4C5831AFE20CFFA50C18BA54D6CFFEF3" box="[849,973,345,370]" class="Magnoliopsida" family="Myrtaceae" genus="Melaleuca" higherTaxonomySource="CoL" kingdom="Fungi" order="Myrtales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="genus">
