<collectionCode id="BC8BAE259F3CCF53FEA8FB81FE81FBD0" box="[314,379,1034,1057]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34978" name="Museum Victoria" pageId="9" pageNumber="160">NMV</collectionCode>
