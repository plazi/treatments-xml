<taxonomicName id="2CAD93E3EF67FF9B364FFF208595FF7C" authority="Emeljanov, 2019" authorityName="Emeljanov" authorityYear="2019" box="[321,438,152,176]" pageId="1" pageNumber="397" rank="subGenus" status="subgen.nov." subGenus="Atropidius">
