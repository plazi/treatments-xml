<taxonomicName LSID="urn:lsid:zoobank.org:act:6280FCDF-06BA-46F8-A795-3AFF52A5A001" authority="Smith, 2011" authorityName="Smith" authorityYear="2011" class="Aves" family="Alcidae" genus="Miomancalla" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Miomancalla" order="Charadriiformes" pageId="13" pageNumber="14" phylum="Chordata" rank="genus">Miomancalla</taxonomicName>
