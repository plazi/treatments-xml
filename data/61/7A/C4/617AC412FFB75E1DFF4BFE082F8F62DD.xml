<taxonomicName authority="(Gemminger, 1869)" baseAuthorityName="Gemminger" baseAuthorityYear="1869" box="[282,689,1952,1977]" class="Insecta" family="Scirtidae" genus="Contacyphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="571" phylum="Arthropoda" rank="species" species="tristis">
