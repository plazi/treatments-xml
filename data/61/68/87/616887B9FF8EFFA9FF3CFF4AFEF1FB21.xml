<paragraph blockId="19.[151,1436,151,1221]" box="[151,683,151,176]" pageId="19" pageNumber="60">
<heading bold="true" box="[151,683,151,176]" fontSize="10" level="2" pageId="19" pageNumber="60" reason="2">
