<taxonomicName authority="Gouan." authorityName="Gouan." box="[580,891,367,392]" class="Magnoliopsida" family="Apiaceae" genus="Eryngium" kingdom="Plantae" order="Apiales" pageId="23" pageNumber="64" phylum="Tracheophyta" rank="species" species="bourgatii">
