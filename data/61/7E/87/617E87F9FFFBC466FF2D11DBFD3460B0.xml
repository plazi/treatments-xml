<subSubSection box="[199,801,1845,1870]" pageId="7" pageNumber="556" type="etymology">
<paragraph blockId="7.[151,1436,870,2014]" box="[199,801,1845,1870]" pageId="7" pageNumber="556">
<emphasis bold="true" box="[199,334,1845,1870]" pageId="7" pageNumber="556">Etymology.</emphasis>
