. 
<location LSID="urn:lsid:plazi:treatment:6140B34B4B1604421C97F9CAFE57FF1C:EC3654864B19044118A4FAF9FAE6FA9C" box="[1188,1318,1340,1365]" country="Kenya" county="Forest" municipality="Nairobi" name="At Mufindi" pageId="8" pageNumber="495" stateProvince="Kakamega">At Mufindi</location>
</materialsCitation>
, 
<materialsCitation ID-GBIF-Occurrence="1426520780" collectionCode="TCEC" country="Tanzania" location="Tanzania" pageId="8" pageNumber="495" specimenCount="1">
<collectingCountry box="[1328,1431,1340,1365]" name="Tanzania" pageId="8" pageNumber="495">Tanzania</collectingCountry>
, 
<collectionCode box="[151,223,1377,1401]" pageId="8" pageNumber="495">TCEC</collectionCode>
