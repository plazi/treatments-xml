<emphasis italics="true" pageId="0" pageNumber="85">Salacia</emphasis>
</taxonomicName>
sp. (
<taxonomicName class="Dicotyledoneae" family="Celastraceae" genus="Salacia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Celastrales" pageId="0" pageNumber="85" phylum="Angiospermae" rank="family">Celastraceae</taxonomicName>
