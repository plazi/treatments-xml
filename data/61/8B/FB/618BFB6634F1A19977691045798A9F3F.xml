<subSubSection type="nomenclature">
<paragraph pageNumber="280">
<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181725" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole cuevasi Wilson" lsidName-HNS="Pheidole cuevasi Wilson" order="Hymenoptera" pageNumber="280" phylum="Arthropoda" rank="species" species="cuevasi">Pheidole cuevasi</taxonomicName>
