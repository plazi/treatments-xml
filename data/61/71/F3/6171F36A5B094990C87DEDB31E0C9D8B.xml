<taxonomicName LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138697" class="Insecta" family="Formicidae" genus="Ectatomma" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ectatomma (Holcoponera) obscurum Emery" lsidName-HNS="Ectatomma (Holcoponera) obscurum Emery" order="Hymenoptera" pageNumber="16" phylum="Arthropoda" rank="species" species="obscurum" subGenus="Holcoponera">E. (H.) obscurum</taxonomicName>
<taxonomicNameLabel rank="species">n. sp.</taxonomicNameLabel>
