<number box="[399,434,439,464]" pageId="5" pageNumber="456" value="6.1">6.1</number>
(
<number box="[452,541,439,464]" pageId="5" pageNumber="456" value="6.1" valueMax="6.3" valueMin="5.9">5.9–6.3</number>
), female 
<number box="[651,689,439,464]" pageId="5" pageNumber="456" value="5.9">5.9</number>
; pronotum width (including pronotal collar): male 
<number box="[1281,1316,439,464]" pageId="5" pageNumber="456" value="6.8">6.8</number>
(
<number box="[1335,1372,439,464]" pageId="5" pageNumber="456" value="6.6">6.6</number>
–7.0), female 
<number box="[234,272,476,501]" pageId="5" pageNumber="456" value="6.5">6.5</number>
; mesonotum width: male 
<number box="[561,596,476,501]" pageId="5" pageNumber="456" value="6.1">6.1</number>
(
<number box="[612,701,476,501]" pageId="5" pageNumber="456" value="6.15" valueMax="6.4" valueMin="5.9">5.9–6.4</number>
