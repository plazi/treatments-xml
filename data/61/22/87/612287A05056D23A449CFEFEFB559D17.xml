<taxonomicName baseAuthorityName="Champion" baseAuthorityYear="1898" class="Insecta" family="Gerridae" genus="Tachygerris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="508" phylum="Arthropoda" rank="species" species="quadrilineatus">
