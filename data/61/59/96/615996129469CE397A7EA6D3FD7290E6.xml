<taxonomicName authority="Bonaparte, 1835" authorityName="Bonaparte" authorityYear="1835" box="[651,1027,644,671]" class="Actinopterygii" family="Paralepididae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aulopiformes" pageId="15" pageNumber="16" phylum="Chordata" rank="family">
