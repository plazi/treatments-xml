<heading box="[699,888,1210,1236]" centered="true" fontSize="11" level="2" pageId="12" pageNumber="13" reason="2">
<taxonomicName authorityName="Gilbert" authorityYear="1891" box="[699,847,1210,1236]" class="Actinopterygii" family="Congridae" genus="Xenomystax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="12" pageNumber="13" phylum="Chordata" rank="genus">
<emphasis bold="true" box="[699,847,1210,1236]" italics="true" pageId="12" pageNumber="13">Xenomystax</emphasis>
