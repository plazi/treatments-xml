<taxonomicName authority="Cuvier, 1829" authorityName="Cuvier" authorityYear="1829" box="[675,995,365,392]" class="Actinopterygii" family="Trachichthyidae" genus="Hoplostethus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Beryciformes" pageId="25" pageNumber="26" phylum="Chordata" rank="genus">
