<subSubSection box="[192,612,291,315]" pageId="3" pageNumber="2" type="nomenclature">
<paragraph blockId="3.[192,612,291,315]" box="[192,612,291,315]" pageId="3" pageNumber="2">
<heading bold="true" box="[192,612,291,315]" fontSize="10" level="2" pageId="3" pageNumber="2" reason="4">
<emphasis bold="true" box="[192,612,291,315]" pageId="3" pageNumber="2">
<taxonomicName authority="Swift" authorityName="Swift" authorityYear="2008" box="[192,458,291,315]" class="Insecta" family="Cerambycidae" genus="Nubosoplatus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[192,379,291,315]" italics="true" pageId="3" pageNumber="2">Nubosoplatus</emphasis>
Swift
</taxonomicName>
, 
<taxonomicNameLabel box="[469,612,291,315]" pageId="3" pageNumber="2" rank="genus">new genus</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
