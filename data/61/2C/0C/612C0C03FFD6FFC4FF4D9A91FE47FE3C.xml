<subSubSection pageId="5" pageNumber="577" type="etymology">
<paragraph blockId="5.[151,1437,1579,2037]" pageId="5" pageNumber="577">
<emphasis bold="true" box="[199,338,1867,1893]" pageId="5" pageNumber="577">Etymology:</emphasis>
