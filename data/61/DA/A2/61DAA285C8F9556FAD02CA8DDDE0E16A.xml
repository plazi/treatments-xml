<subSubSection pageId="0" pageNumber="145" type="materials_examined">
<paragraph pageId="0" pageNumber="145">Type.</paragraph>
<paragraph pageId="0" pageNumber="145">
CHINA. Zhejiang Province: Shengzhou city, Chongren Town, Liwang village, 
<geoCoordinate degrees="29" direction="north" minutes="39" orientation="latitude" precision="925" value="29.65">29°39'N</geoCoordinate>
, 
<geoCoordinate degrees="120" direction="east" minutes="39" orientation="longitude" precision="925" value="120.65">120°39'E</geoCoordinate>
