<taxonomicName authority="Ehrenberg, 1834" authorityName="Ehrenberg" authorityYear="1834" box="[692,1193,1368,1394]" class="Anthozoa" family="Actinodendridae" genus="Megalactis" kingdom="Animalia" order="Actiniaria" pageId="11" pageNumber="96" phylum="Cnidaria" rank="species" species="hemprichii">
