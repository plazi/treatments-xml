<taxonomicName id="D19CECADFFA0FFEEFEB2FCCF262CFCC0" authority="(LINNAEUS, 1758)" baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[368,840,788,814]" class="Gastropoda" family="Bithyniidae" genus="Bithynia" kingdom="Animalia" order="Littorinimorpha" pageId="2" pageNumber="195" phylum="Mollusca" rank="species" species="tentaculata">
