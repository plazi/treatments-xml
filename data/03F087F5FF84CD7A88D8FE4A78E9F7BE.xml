<taxonomicName id="4C594D60FF84CD7A890AFE4A7B04F09F" authority="Simon, 1892" authorityName="Simon" authorityYear="1892" box="[285,536,467,491]" class="Anthozoa" family="Hormathiidae" genus="Phelliactis" kingdom="Animalia" order="Actiniaria" pageId="46" pageNumber="46" phylum="Cnidaria" rank="genus">
