<taxonomicName id="4C644D56FF901762FC4AFA4FEAF095E0" authorityName="Kamalanathan &amp; Mohanraj" authorityYear="2017" box="[910,1006,1465,1489]" class="Insecta" family="Scelionidae" genus="Cremastobaeus" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="2047" phylum="Arthropoda" rank="species" species="fuscus">
<emphasis id="B910EAC7FF901762FC4AFA4FEAF095E0" box="[910,1006,1465,1489]" italics="true" pageId="59" pageNumber="2047">C. fuscus</emphasis>
