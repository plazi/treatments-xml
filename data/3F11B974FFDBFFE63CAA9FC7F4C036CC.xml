<quantity id="7040A587FFDBFFE63DF49C95F56B31D7" box="[502,585,1696,1720]" metricMagnitude="-2" metricUnit="m" metricValue="4.0" pageId="18" pageNumber="619" unit="mm" value="40.0">40 mm</quantity>
