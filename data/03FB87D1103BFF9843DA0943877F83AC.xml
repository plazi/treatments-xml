<paragraph id="8BED36C7103BFF9E43D909D0810085A5" blockId="43.[236,1351,1615,1763]" pageId="43" pageNumber="1520">
syn. 
<taxonomicName id="4C524D44103BFF9E421709D086B385C5" authority="HEINRICH 1980" authorityName="HEINRICH" authorityYear="1980" box="[291,756,1677,1702]" class="Insecta" family="Ichneumonidae" genus="Coelichneumon" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="1520" phylum="Arthropoda" rank="species" species="warnckei">
