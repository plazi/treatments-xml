<taxonomicName id="4C5DB1EF9B1B81305224E57100AFE5D9" authority="Carvalho, 1981" authorityName="Carvalho" authorityYear="1981" box="[294,702,1914,1936]" class="Insecta" family="Miridae" genus="Jessopocoris" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="0" pageNumber="47" phylum="Arthropoda" rank="species" species="scutellatus">
