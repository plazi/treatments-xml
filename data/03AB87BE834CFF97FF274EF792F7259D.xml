<collectionCode id="ED13AE6D834CFF94FE174D1A97F92731" box="[423,479,1596,1620]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="1" pageNumber="198">SMF</collectionCode>
