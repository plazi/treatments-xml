<taxonomicName id="7BE1E53378B6B6D4209B97B004D1B734" class="Arachnida" family="Leguminosae" genus="Dalea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="" order="Fabales" pageId="0" pageNumber="89" phylum="Arthropoda" rank="family">Leguminosae</taxonomicName>
