<taxonomicName id="7D27964B72F8978D059F79176FEFA37B" class="Arachnida" family="Leguminosae" genus="Dalea" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Dalea" order="Fabales" pageId="0" pageNumber="89" phylum="Arthropoda" rank="genus">
<emphasis id="99533D522E8E9277459E9EAB4A9B92E1" italics="true" pageId="0" pageNumber="89">Dalea</emphasis>
</taxonomicName>
