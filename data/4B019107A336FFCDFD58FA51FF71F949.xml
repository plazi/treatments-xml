<taxonomicName id="04A85B92A330FFCDFEE6F9A9FD9EF9AE" box="[372,522,1609,1636]" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="20" pageNumber="75" phylum="Tracheophyta" rank="species" species="trilineata">
