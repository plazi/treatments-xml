<paragraph id="C3172011A330FFCDFF23FA51FF71F949" blockId="20.[151,684,1456,1667]" pageId="20" pageNumber="75">
<collectionCode id="A5B9B8D4A330FFCDFF23FA51FF56FA01" box="[177,194,1457,1483]" country="USA" lsid="urn:lsid:biocol.org:col:15406" name="Harvard University - Arnold Arboretum" pageId="20" pageNumber="75" type="Herbarium">A</collectionCode>
