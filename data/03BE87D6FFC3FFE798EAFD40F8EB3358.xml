<quantity id="4CEF9B25FFC3FFE79A2DFD25F8EA32F4" box="[592,683,727,753]" metricMagnitude="-1" metricUnit="m" metricValue="4.43" pageId="82" pageNumber="83" unit="cm" value="44.3">44.3 cm</quantity>
