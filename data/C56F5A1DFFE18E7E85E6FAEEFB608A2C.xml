<taxonomicName id="8AC69088FFEE8E7C84D7F83DFD558BE9" authority="Chen &amp; Xu, 2013" authorityName="Chen &amp; Xu" authorityYear="2013" box="[422,736,1973,1997]" class="Insecta" family="Colletidae" genus="Hylaeus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="339" phylum="Arthropoda" rank="species" species="neatifrons" status="sp. nov." subGenus="Hylaeus">
