'W
</geoCoordinate>
, 
<quantity id="4CE09B6EFFB9DE44FD26FBEC7C44FBE3" box="[694,803,1084,1110]" metricMagnitude="3" metricUnit="m" metricValue="2.73" pageId="14" pageNumber="15" unit="m" value="2730.0">2730 m</quantity>
, 
<date id="FFA6104BFFB9DE44FCD0FBEC7CD9FBE3" box="[832,958,1084,1110]" pageId="14" pageNumber="15" value="1995-04-21">
