<quantity id="BA159B41FFF306336367F8C7FCD62B5B" box="[777,860,1823,1848]" metricMagnitude="3" metricUnit="m" metricValue="1.46304" pageId="62" pageNumber="63" unit="ft" value="4800.0">4800 ft</quantity>
