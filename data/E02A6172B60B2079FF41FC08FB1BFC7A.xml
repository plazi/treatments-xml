(male, 6.5 mm); 16— 
<taxonomicName id="AF83ABE7B60D2078FC1AF836FBABF816" authority="Bezděk, 2020" authorityName="Bezděk" authorityYear="2020" box="[972,1138,1935,1959]" class="Insecta" family="Chrysomelidae" genus="Agelopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="235" phylum="Arthropoda" rank="species" species="konstantinovi" status="sp. nov.">
<emphasis id="5AF70C76B60D2078FC1AF836FBABF816" box="[972,1138,1935,1959]" italics="true" pageId="12" pageNumber="235">A. konstantinovi</emphasis>
</taxonomicName>
<emphasis id="5AF70C76B60D2078FBAEF836FB10F816" bold="true" box="[1144,1225,1935,1959]" pageId="12" pageNumber="235">
<taxonomicNameLabel id="41C4B10DB60D2078FBAEF836FB10F816" box="[1144,1225,1935,1959]" pageId="12" pageNumber="235" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(holotype, male, 4.4 mm); 17— 
<taxonomicName id="AF83ABE7B60D2078FED1F80AFEA0F87A" authority="Bezděk, 2020" authorityName="Bezděk" authorityYear="2020" box="[263,377,1971,1995]" class="Insecta" family="Chrysomelidae" genus="Agelopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="235" phylum="Arthropoda" rank="species" species="malaisei" status="sp. nov.">
<emphasis id="5AF70C76B60D2078FED1F80AFEA0F87A" box="[263,377,1971,1995]" italics="true" pageId="12" pageNumber="235">A. malaisei</emphasis>
</taxonomicName>
<emphasis id="5AF70C76B60D2078FEA9F80AFE09F87A" bold="true" box="[383,464,1971,1995]" pageId="12" pageNumber="235">
