<taxonomicName id="AF83ABE7B60D2078FDB9F836FD31F816" authorityName="Bezděk" authorityYear="2020" baseAuthorityName="Chen" baseAuthorityYear="1976" box="[623,744,1935,1959]" class="Insecta" family="Chrysomelidae" genus="Agelopsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="235" phylum="Arthropoda" rank="species" species="tibetanus">
<emphasis id="5AF70C76B60D2078FDB9F836FD31F816" box="[623,744,1935,1959]" italics="true" pageId="12" pageNumber="235">A. tibetanus</emphasis>
