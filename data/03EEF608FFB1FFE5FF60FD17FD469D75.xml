<specimenCount id="9D418C97FFB1FFE5FF4CF993FE959C05" box="[187,339,1601,1625]" pageId="12" pageNumber="13" type="generic">
<number id="3D8F5539FFB1FFE5FF4CF993FF119C05" box="[187,215,1601,1625]" pageId="12" pageNumber="13" value="16.0">16</number>
specimens
</specimenCount>
