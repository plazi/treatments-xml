, Kermadec Islands, 
<quantity id="642F9B30FFA8E6504DE8FA46734CBF0C" box="[739,869,1528,1555]" metricMagnitude="2" metricUnit="m" metricValue="2.41" metricValueMax="2.87" metricValueMin="1.95" pageId="66" pageNumber="67" unit="m" value="241.0" valueMax="287.0" valueMin="195.0">
