<taxonomicName id="4C374AE5FF90FFFBFF3CCC1DFE5FA42B" authority="Araújo &amp; Bravo, 2018" authorityName="Araújo &amp; Bravo" authorityYear="2018" box="[220,400,2106,2131]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="muiraquita" status="sp.nov.">
<emphasis id="B943ED74FF90FFFBFF3CCC1DFE5FA42B" bold="true" box="[220,400,2106,2131]" italics="true" pageId="1" pageNumber="2">Trichomyia muiraquita</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A270500FFF90FFFBFE74CC1DFE1DA42B" box="[404,466,2106,2131]" pageId="1" pageNumber="2" rank="species">sp.nov.</taxonomicNameLabel>
