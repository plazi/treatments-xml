<heading id="D0FDEF72FFF4FFBD2A43FC4DFDEF9EEA" box="[295,556,985,1008]" centered="true" fontSize="8" level="2" pageId="1" pageNumber="118" reason="2">
<emphasis id="B97E840CFFF4FFBD2A43FC4DFDEF9EEA" bold="true" box="[295,556,985,1008]" pageId="1" pageNumber="118">
<taxonomicName id="4C0A239DFFF4FFBD2A43FC4DFE269EEA" authorityName="ROMER" authorityYear="1947" box="[295,485,985,1008]" class="Amphibia" family="Discosauriscidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Seymouriamorpha" pageId="1" pageNumber="118" phylum="Chordata" rank="family">Discosauriscidae</taxonomicName>
