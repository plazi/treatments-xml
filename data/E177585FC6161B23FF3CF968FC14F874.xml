<geoCoordinate id="0CEA8F8EC6161B23FEA0F84BFE1AF858" box="[267,415,1901,1925]" degrees="49" direction="east" minutes="54.108" orientation="longitude" pageId="16" pageNumber="17" precision="1" value="49.9018">49º 54.108'E</geoCoordinate>
