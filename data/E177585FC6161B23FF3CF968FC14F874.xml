<geoCoordinate id="0CEA8F8EC6161B23FADDF86FFE85F858" degrees="29" direction="north" minutes="29.897" orientation="latitude" pageId="16" pageNumber="17" precision="1" value="29.498283">29º 29.897'N</geoCoordinate>
