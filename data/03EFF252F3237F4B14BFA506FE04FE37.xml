<taxonomicName id="4C4638C7F32C7F4B11ACA33FFED9FE37" authorityName="Straneo" authorityYear="1952" class="Insecta" family="Carabidae" genus="Abacetus" kingdom="Animalia" order="Coleoptera" pageId="40" pageNumber="241" phylum="Arthropoda" rank="species" species="monardianus">
