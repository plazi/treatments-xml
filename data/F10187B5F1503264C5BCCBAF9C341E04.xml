<taxonomicName id="BEA84D20F1523264C70ECDF39B821E04" box="[570,727,232,258]" class="Liliopsida" family="Orchidaceae" genus="Otostylis" kingdom="Plantae" order="Asparagales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="species" species="alba">
<emphasis id="4BDCEAB1F1523264C70ECDF39B821E04" box="[570,727,232,258]" italics="true" pageId="23" pageNumber="24">Otostylis alba</emphasis>
