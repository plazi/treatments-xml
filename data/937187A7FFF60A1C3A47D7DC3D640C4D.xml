<taxonomicName id="DCD84D32FFF60A1D3B5ED1533EBA0B27" authorityName="Burton" authorityYear="1956" box="[93,246,1351,1375]" class="Trematoda" family="Heterophyidae" genus="Ascocotyle" kingdom="Animalia" order="Plagiorchiida" pageId="6" pageNumber="1737" phylum="Platyhelminthes" rank="species" species="leighi" subGenus="Ascocotyle">
<emphasis id="29ACEAA3FFF60A1D3B5ED1533E340B27" box="[93,120,1351,1375]" italics="true" pageId="6" pageNumber="1737">A.</emphasis>
