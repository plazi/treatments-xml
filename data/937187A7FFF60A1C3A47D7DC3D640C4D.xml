<taxonomicName id="DCD84D32FFF70A1C3A8AD49E3C540EDA" authorityName="Burton" authorityYear="1956" box="[393,536,138,162]" class="Trematoda" family="Heterophyidae" genus="Ascocotyle" kingdom="Animalia" order="Plagiorchiida" pageId="7" pageNumber="1738" phylum="Platyhelminthes" rank="species" species="leighi" subGenus="Ascocotyle">
