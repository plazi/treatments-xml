<quantity id="4CA59B109D7BFFCCFC84F94F3248F968" box="[814,883,1697,1721]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="7" pageNumber="658" unit="m" value="700.0">700 m</quantity>
