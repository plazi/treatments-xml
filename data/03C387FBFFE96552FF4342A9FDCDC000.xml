<collectingCountry id="F37D767DFFE96551FB3446E8FA8BC6CE" box="[1248,1324,1932,1956]" name="Ghana" pageId="45" pageNumber="46">Ghana</collectingCountry>
, 
<collectingCountry id="F37D767DFFE96551FA9546E8FA3FC6CE" box="[1345,1432,1932,1956]" name="Nigeria" pageId="45" pageNumber="46">Nigeria</collectingCountry>
