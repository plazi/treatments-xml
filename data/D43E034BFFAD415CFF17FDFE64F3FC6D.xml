: 
<taxonomicName id="9B97C9DEFFAD415CFEFBFCA06505FC6D" authority="Casey" authorityName="Casey" authorityYear="1906" box="[352,541,870,897]" class="Insecta" family="Staphylinidae" genus="Gnypeta" kingdom="Animalia" order="Coleoptera" pageId="48" pageNumber="59" phylum="Arthropoda" rank="species" species="helenae">
