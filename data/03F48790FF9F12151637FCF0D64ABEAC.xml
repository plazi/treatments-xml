) sa, 
<number id="3D9524A1FF9F12171435F990D434BCF1" box="[778,793,1657,1683]" pageId="12" pageNumber="13" value="1.0">1</number>
pa, 0 dc, 
<number id="3D9524A1FF9F121714AAF990D489BCF1" box="[917,932,1657,1683]" pageId="12" pageNumber="13" value="2.0">2</number>
