<quantity id="4CA59B63FF9F121716C7FA30D54CBF91" box="[504,609,1497,1523]" metricMagnitude="-4" metricUnit="m" metricValue="3.0" pageId="12" pageNumber="13" unit="mm" value="0.3">0.30 mm</quantity>
