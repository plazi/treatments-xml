<quantity id="4CDF9B3B9D3FFFB8FC80CFF26ED6FF7A" box="[893,938,190,216]" metricMagnitude="0" metricUnit="m" metricValue="7.0" pageId="44" pageNumber="197" unit="m" value="7.0">7 m</quantity>
depth
