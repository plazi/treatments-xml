<number id="3DF6E206FFB31455FD6C754709FF1A2C" box="[684,739,301,327]" pageId="18" pageNumber="19" value="0.75">0.75</number>
, 
<number id="3DF6E206FFB31455FD31754708391A2C" box="[753,805,301,327]" pageId="18" pageNumber="19" value="0.53">0.53</number>
