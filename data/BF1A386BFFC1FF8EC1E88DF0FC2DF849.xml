<paragraph id="370C897DFFC1FF8EC1B98BDEFE2FFF20" blockId="1.[151,1436,1955,2020]" lastBlockId="2.[151,1436,152,425]" lastPageId="2" lastPageNumber="67" pageId="1" pageNumber="66">
<emphasis id="05C7556FFFC1FF8DC1B98BDEFEFDF862" bold="true" box="[198,272,1955,1979]" pageId="1" pageNumber="66">Venter</emphasis>
(fig. 
<number id="817B9B5AFFC1FF8DC0328BDEFEB1F863" box="[333,348,1955,1978]" pageId="1" pageNumber="66" value="3.0">3</number>
