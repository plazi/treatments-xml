<emphasis id="05C7556FFFC2FF8EC3508CC0FDD1FF0D" box="[559,572,189,212]" italics="true" pageId="2" pageNumber="67">g</emphasis>
<number id="817B9B5AFFC2FF8EC3438CB1FDAEFF00" box="[572,579,204,217]" pageId="2" pageNumber="67" value="1.0">
<emphasis id="05C7556FFFC2FF8EC3438CB1FDAEFF00" box="[572,579,204,217]" italics="true" pageId="2" pageNumber="67">1</emphasis>
