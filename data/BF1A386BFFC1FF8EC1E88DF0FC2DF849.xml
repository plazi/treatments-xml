<number id="817B9B5AFFC2FF8EC51F8CB1FB8AFF00" box="[1120,1127,204,217]" pageId="2" pageNumber="67" value="2.0">2</number>
</emphasis>
and 
<emphasis id="05C7556FFFC2FF8EC5E78CC0FB5DFF0D" box="[1176,1200,189,212]" italics="true" pageId="2" pageNumber="67">ps</emphasis>
<emphasis id="05C7556FFFC2FF8EC5D08CB1FB5BFF00" box="[1199,1206,204,217]" italics="true" pageId="2" pageNumber="67">
