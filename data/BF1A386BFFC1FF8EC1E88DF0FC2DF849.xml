<number id="817B9B5AFFC1FF8DC5C58BCFFB2CF866" box="[1210,1217,1970,1983]" pageId="1" pageNumber="66" value="2.0">2</number>
about three quarters length of IC 
<number id="817B9B5AFFC1FF8DC0668BAAFECDF83D" box="[281,288,2007,2020]" pageId="1" pageNumber="66" value="1.0">1</number>
; IC 
<number id="817B9B5AFFC1FF8DC0358BAAFEBCF83D" box="[330,337,2007,2020]" pageId="1" pageNumber="66" value="3.0">3</number>
and IC 
<number id="817B9B5AFFC1FF8DC0DE8BAAFE45F83D" box="[417,424,2007,2020]" pageId="1" pageNumber="66" value="4.0">4</number>
