<taxonomicName id="1D8896F2FFEEFF9DFF58FBBB8265FBDF" authority=", Platonova &amp; Potin, 1972" authorityName=", Platonova &amp; Potin" authorityYear="1972" box="[238,772,1117,1143]" class="Chromadorea" family="Chromadoridae" genus="Harpagonchoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Chromadorida" pageId="31" pageNumber="32" phylum="Nematoda" rank="genus">
