, 
<taxonomicName id="25D64D4AFF8DFF87E75DF9BF55F3F949" authority="(Munster, 1844)" baseAuthorityName="Muenster" baseAuthorityYear="1844" box="[623,926,1657,1675]" class="Gastropoda" family="Gastropoda (awaiting allocation)" genus="Simploptyxis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Gastropoda (awaiting allocation)" pageId="10" pageNumber="359" phylum="Mollusca" rank="species" species="nobilis">
