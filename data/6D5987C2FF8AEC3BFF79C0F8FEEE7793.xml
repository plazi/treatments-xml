<quantity id="22089B31FF89EC3AFB81C530FB5D711D" box="[1135,1239,295,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.62" pageId="2" pageNumber="569" unit="mm" value="1.62">1.62 mm</quantity>
