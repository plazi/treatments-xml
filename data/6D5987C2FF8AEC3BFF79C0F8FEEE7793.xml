</taxonomicName>
on 
<taxonomicName id="22F04D57FF8AEC39FAEFC18AFA167597" box="[1281,1436,1437,1461]" class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="1" pageNumber="568" phylum="Tracheophyta" rank="species" species="idaeus">
