<taxonomicName id="018F00FCD02519CAE840C8828C136CE8" authority="Jurine, 1807" authorityName="Jurine" authorityYear="1807" class="Insecta" family="Dryinidae" genus="Anteon" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Anteon jurineanum" order="Hymenoptera" pageId="12" pageNumber="139" phylum="Arthropoda" rank="species" species="jurineanum">Anteon jurineanum</taxonomicName>
