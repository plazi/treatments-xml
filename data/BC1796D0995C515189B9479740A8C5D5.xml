<paragraph id="E52CF95BA23D9A938B5B9432A2CD2EFB" pageId="0" pageNumber="77">
<taxonomicName id="08B2543F294AF4010731E2B743AFC456" authorityName="Sharkey" authorityYear="2021" class="Insecta" family="Braconidae" genus="Triraphis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Triraphis doncombi" order="Hymenoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="doncombi">
