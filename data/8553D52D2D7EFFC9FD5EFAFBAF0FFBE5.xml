<quantity id="CA02C9DE2D7CFFC9FE0DFDE5AC58FDDF" box="[444,535,558,585]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="39" pageNumber="86" unit="mm" value="1.1">1.1 mm</quantity>
