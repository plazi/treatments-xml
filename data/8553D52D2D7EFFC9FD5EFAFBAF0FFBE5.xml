<quantity id="CA02C9DE2D7CFFC9FEC2FD86AF81FDFE" box="[371,462,589,616]" metricMagnitude="-3" metricUnit="m" metricValue="9.8" pageId="39" pageNumber="86" unit="mm" value="9.8">9.8 mm</quantity>
