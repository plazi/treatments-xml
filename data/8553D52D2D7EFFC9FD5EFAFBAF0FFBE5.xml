; width, 7.0 mm; thickness, 
<quantity id="CA02C9DE2D7EFFCBFB9BF942AACBF932" box="[1066,1156,1673,1700]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="37" pageNumber="84" unit="mm" value="4.4">4.4 mm</quantity>
