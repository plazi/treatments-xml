<specimenCode id="DBFA9EFCFF94FFDD7B804376FBC6F9BE" box="[983,1134,1545,1569]" collectionCode="PCZCU" pageId="4" pageNumber="96">PCZCU 2269</specimenCode>
a), two dorsal valves (
<specimenCode id="DBFA9EFCFF94FFDD7D0C4376FCC4F9DE" collectionCode="PCZCU" pageId="4" pageNumber="96">PCZCU 2267</specimenCode>
, 
<specimenCode id="DBFA9EFCFF94FFDD7B214356FBA2F9DE" box="[886,1034,1577,1601]" collectionCode="PCZCU" pageId="4" pageNumber="96">PCZCU 2294</specimenCode>
) and 
<specimenCount id="9D5AFD0EFF94FFDD7C134356FB6FF9DE" box="[1092,1223,1577,1601]" pageId="4" pageNumber="96" type="juvenile">one juvenile</specimenCount>
