<taxonomicName id="8CD98C73DE1AD3A47AE1072F7CFEC2E0" LSID="http://zoobank.org/7961DD3D-2423-42E1-A7E5-35C53C0A4612" authority="Huo &amp; Wang" authorityName="Kapur" authorityYear="1948" class="Insecta" family="Coccinellidae" genus="Pseudaspidimerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Pseudaspidimerus palatus" order="Coleoptera" pageId="1" pageNumber="110" phylum="Arthropoda" rank="species" species="palatus">Pseudaspidimerus palatus Huo &amp; Wang</taxonomicName>
<taxonomicNameLabel id="FFF00DDD1F0A619118A3C71D463310E0" pageId="1" pageNumber="110">sp. n.</taxonomicNameLabel>
