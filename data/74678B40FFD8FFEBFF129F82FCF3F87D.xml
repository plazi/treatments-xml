</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="3BCE41D5FFD8FFEBFD4B9A12FBC9F8AD" authority="Gorochov, 1988" authorityName="Gorochov" authorityYear="1988" box="[718,1044,1834,1860]" class="Insecta" family="Prophalangopsidae" genus="Tettaboilus" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="56" phylum="Arthropoda" rank="genus">
<emphasis id="CEBAE644FFD8FFEBFD4B9A12FC8DF8AA" box="[718,848,1834,1859]" italics="true" pageId="1" pageNumber="56">Tettaboilus</emphasis>
