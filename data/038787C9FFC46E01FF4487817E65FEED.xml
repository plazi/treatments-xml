<subSubSection id="C3346554FFC66E01FF1486EB79EBFEC3" pageId="3" pageNumber="244" type="materials_examined">
<paragraph id="8B9136DFFFC66E01FF1486EB79EBFEC3" blockId="3.[151,1436,152,313]" pageId="3" pageNumber="244">
