/ 
<number id="E25524F1FFCFFF950DA4F8146DFBA894" box="[919,949,1970,1996]" pageId="13" pageNumber="14" value="36.0">36</number>
/ 
<number id="E25524F1FFCFFF950D8CF8146D94A894" box="[959,986,1970,1996]" pageId="13" pageNumber="14" value="15.0">15</number>
