<typeStatus id="DE6D10F0FF92FF824506FAF0A0B7FAAF" box="[601,760,1359,1385]" pageId="8" pageNumber="9">HOLOTYPE</typeStatus>
. Ψ, 
<emphasis id="33A27240FF92FF82446BFAF1A1ADFAAE" bold="true" box="[820,994,1358,1384]" pageId="8" pageNumber="9">
<collectingCountry id="79C1EEC2FF92FF82446BFAF1A193FAAE" box="[820,988,1358,1384]" name="Colombia" pageId="8" pageNumber="9">COLOMBIA</collectingCountry>
