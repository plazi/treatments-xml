and 
<taxonomicName id="4C7C4D72E62B707DFB40FF576065FF30" authority="Bhise &amp; Patil &amp; Salunkhe, 2014" authorityName="Bhise &amp; Patil &amp; Salunkhe" authorityYear="2014" box="[1158,1330,159,182]" class="Dothideomycetes" family="Asterinaceae" genus="Asterina" kingdom="Fungi" order="Asterinales" pageId="3" pageNumber="286" phylum="Ascomycota" rank="species" species="myrtacearum" status="sp.nov.">A. myrtacearum</taxonomicName>
