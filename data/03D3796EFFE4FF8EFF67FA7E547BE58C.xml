= 
<taxonomicName id="4C7AB3FBFFE4FF8FFF5BF9FA56F4E72B" authority="Sakai, 2011" authorityName="Sakai" authorityYear="2011" box="[171,499,1613,1635]" class="Malacostraca" family="Callianassidae" genus="Thailandcallichirus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="14" pageNumber="467" phylum="Arthropoda" rank="genus">
