</paragraph>
<paragraph id="8B89369EFFF0FFD8A9A9FA3110E74585" blockId="16.[189,515,1430,1501]" box="[189,466,1474,1501]" pageId="16" pageNumber="17">
<taxonomicName id="4C364D1DFFF0FFD8A9A9FA31101F4584" box="[189,298,1474,1500]" class="Pinopsida" family="Pinaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="family">Pinaceae</taxonomicName>
