<taxonomicName id="4C364D1DFFF0FFD8A821FA3010F54584" box="[309,448,1474,1500]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="GBIF" kingdom="Plantae" order="Pinales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="abies">
<taxonomicName id="4C364D1DFFF0FFD8A821FA30104D4584" box="[309,376,1475,1500]" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="CoL" kingdom="Plantae" order="Pinales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="genus">Picea</taxonomicName>
