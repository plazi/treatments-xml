<taxonomicName id="4C2E4D2AFFE0B07FFE8EF8401B1FE0A4" authority="Park &amp; Kim" authorityName="Park &amp; Kim" authorityYear="2020" box="[281,630,1972,1997]" class="Insecta" family="Lecithoceridae" genus="Torodora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="326" phylum="Arthropoda" rank="species" species="occidentis" status="sp. nov.">
