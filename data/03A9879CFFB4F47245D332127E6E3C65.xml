<taxonomicName id="4C004D09FFB2F470447633297C92395A" box="[306,395,1895,1917]" class="Insecta" family="Bibionidae" genus="Bibiodes" kingdom="Animalia" order="Diptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="genus">
