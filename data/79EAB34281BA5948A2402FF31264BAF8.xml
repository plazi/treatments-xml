, 
<taxonomicName id="5D0CB21663DEC21D37D774F3F32C6C23" baseAuthorityName="L. Pfeiffer" baseAuthorityYear="1847" class="Gastropoda" family="Bulimulidae" genus="Drymaeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Pulmonata" pageId="109" pageNumber="110" phylum="Mollusca" rank="species" species="recluzianus" subGenus="Mesembrinus">
