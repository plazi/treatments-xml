<taxonomicName id="E1C81386FFF1FF96FD3B4925FD7BFD4E" box="[662,758,631,657]" class="Magnoliopsida" family="Cactaceae" genus="Opuntia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="80" pageNumber="81" phylum="Magnoliopsida" rank="genus">
<emphasis id="14BCB417FFF1FF96FD3B4925FD7BFD4E" box="[662,758,631,657]" italics="true" pageId="80" pageNumber="81">Opuntia</emphasis>
</taxonomicName>
, 
<taxonomicName id="E1C81386FFF1FF96FCA94924FC4FFD4F" box="[772,962,630,656]" class="Magnoliopsida" family="Asteraceae" genus="Argyranthemum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Asterales" pageId="80" pageNumber="81" phylum="Tracheophyta" rank="genus">
<emphasis id="14BCB417FFF1FF96FCA94924FC4FFD4F" box="[772,962,630,656]" italics="true" pageId="80" pageNumber="81">Argyranthemum</emphasis>
