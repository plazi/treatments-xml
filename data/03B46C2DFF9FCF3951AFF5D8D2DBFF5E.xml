<taxonomicName id="4C1DA6B8FF9FCF3E51D6F5D8D159FD7A" authority="Gervais, 1847" authorityName="Gervais" authorityYear="1847" box="[238,574,729,756]" class="Diplopoda" family="Cyrtodesmidae" genus="Cyrtodesmus" kingdom="Animalia" order="Polydesmida" pageId="19" pageNumber="170" phylum="Arthropoda" rank="genus">
