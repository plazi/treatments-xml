, 
<collectingCountry id="F36D764BFF86EC0A4F25210AFD58FBEA" box="[595,724,1081,1106]" name="Philippines" pageId="7" pageNumber="296">Philippines</collectingCountry>
, “IIigan, Mindanao, 
<collectingCountry id="F36D764BFF86EC0A4ECA210AFB8DFBEA" box="[956,1025,1081,1106]" name="United States Pacific Island Wildlife Refuges" pageId="7" pageNumber="296">Baker</collectingCountry>
” (
<collectionCode id="ED6BAE1EFF86EC0A496C2109FBFAFBEA" box="[1050,1142,1082,1106]" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="7" pageNumber="296">NMNH</collectionCode>
