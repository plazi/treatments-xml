<emphasis id="B9484069CA7FFF99FF41CA614EB2FEAE" bold="true" box="[151,413,350,375]" pageId="12" pageNumber="186">Specimens examined.</emphasis>
<specimenCount id="9D3A57F2CA7FFF99FE7DCA604D65FEAE" box="[427,586,350,375]" pageId="12" pageNumber="186" type="generic">
<number id="3DF48E5CCA7FFF99FE7DCA604EE8FEAE" box="[427,455,351,375]" pageId="12" pageNumber="186" value="20.0">20</number>
