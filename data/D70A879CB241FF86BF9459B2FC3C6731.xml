<emphasis id="6DD7EA98B241FF80BF94580FFE266375" bold="true" box="[151,386,1847,1872]" pageId="3" pageNumber="30">Material examined.</emphasis>
<collectingCountry id="27B4761AB241FF80BE8F5800FE60636A" box="[396,452,1848,1871]" name="Peru" pageId="3" pageNumber="30">Peru</collectingCountry>
: 
<number id="E96B24ADB241FF80BED3580FFE7A636A" box="[464,478,1847,1871]" pageId="3" pageNumber="30" value="3.0">3</number>
<typeStatus id="80188828B241FF80BEE45800FDF66375" box="[487,594,1848,1872]" pageId="3" pageNumber="30">paratypes</typeStatus>
(
<collectionCode id="39B2AE4FB241FF80BD60580FFD07636A" box="[611,675,1847,1871]" httpUri="http://grbio.org/cool/kdnf-0379" name="Zoologisches Museum Hamburg" pageId="3" pageNumber="30">ZMH</collectionCode>
<collectionCode id="39B2AE4FB241FF80BDAF580FFD62636A" box="[684,710,1847,1871]" pageId="3" pageNumber="30">P-</collectionCode>
<number id="E96B24ADB241FF80BDC5580FFCB4636A" box="[710,784,1847,1871]" pageId="3" pageNumber="30" value="19652.0">19652</number>
). El Camotal Island, La Punta, Callao; 
<quantity id="985B9B6FB241FF80BBD7580FFAA5636A" box="[1236,1281,1847,1871]" metricMagnitude="0" metricUnit="m" metricValue="3.0" pageId="3" pageNumber="30" unit="m" value="3.0">3 m</quantity>
