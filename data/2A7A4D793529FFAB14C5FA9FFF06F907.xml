<taxonomicName id="65D387EC3529FFAB155EFA6FFE40FA03" authorityName="A.M.C.Dumeril" authorityYear="1805" box="[268,395,1485,1511]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="4" pageNumber="5" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
