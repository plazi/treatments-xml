<taxonomicName id="4B3129FD2DE26036112E2310B6D76363" class="Gastropoda" family="Urocyclidae" genus="Phaneroporus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phaneroporus" order="Stylommatophora" pageId="5" pageNumber="16" phylum="Mollusca" rank="genus">Phaneroporus</taxonomicName>
