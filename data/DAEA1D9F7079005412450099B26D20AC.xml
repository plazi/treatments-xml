<taxonomicName id="2B34BFE0A87B8ED7923B7A9D182C21F1" class="Gastropoda" family="Urocyclidae" genus="Tanzalimax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tanzalimax" order="Stylommatophora" pageId="5" pageNumber="16" phylum="Mollusca" rank="genus">Tanzalimax</taxonomicName>
