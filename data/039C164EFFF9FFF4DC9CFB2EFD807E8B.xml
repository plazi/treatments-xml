<paragraph id="8B8AA758FFF9FFF5DC9CFB2EFDD47C4C" blockId="120.[174,590,1593,1616]" box="[174,590,1593,1616]" pageId="120" pageNumber="121">
<heading id="D0C21034FFF9FFF5DC9CFB2EFDD47C4C" box="[174,590,1593,1616]" centered="true" fontSize="9" level="2" pageId="120" pageNumber="121" reason="2">
<taxonomicName id="4C35DCDBFFF9FFF5DC9CFB2EFE2C7C53" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[174,438,1593,1616]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="120" pageNumber="121" phylum="Arthropoda" rank="species" species="aptatus" status="new species">
<emphasis id="B9417B4AFFF9FFF5DC9CFB2EFE2C7C53" bold="true" box="[174,438,1593,1616]" italics="true" pageId="120" pageNumber="121">Paramblynotus aptatus</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFF9FFF5DDF5FB2DFDD47C4C" box="[455,590,1594,1616]" pageId="120" pageNumber="121" rank="species">new species</taxonomicNameLabel>
