<taxonomicName id="4C35DCDBFFF8FFF4DE3AF973FDC17E65" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[520,603,1124,1145]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="121" pageNumber="122" phylum="Arthropoda" rank="species" species="aptatus">
