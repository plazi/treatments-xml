<collectingCountry id="71847636FFF6693CFCBA6D83D6E7ABD5" box="[862,986,461,487]" name="Costa Rica" pageId="24" pageNumber="465">Costa Rica</collectingCountry>
(
<collectingRegion id="CB57F844FFF6693CFC0F6D83D14CABD5" box="[1003,1137,461,487]" country="Costa Rica" name="Guanacaste" pageId="24" pageNumber="465">Guanacaste</collectingRegion>
, 
<collectingRegion id="CB57F844FFF6693CFB9A6D80D1C0ABDA" box="[1150,1277,462,488]" country="Costa Rica" name="Puntarenas" pageId="24" pageNumber="465">Puntarenas</collectingRegion>
), and remains known only from this country. It is now recorded from 
<collectingCountry id="71847636FFF6693CFCE56DBCD660A83E" box="[769,861,498,524]" name="Panama" pageId="24" pageNumber="465">Panama</collectingCountry>
