<emphasis id="B97A79722562FF8B2A9DE9EAFE3EF9F3" bold="true" box="[151,504,1518,1544]" pageId="1" pageNumber="62">
<taxonomicName id="4C0EDEE32562FF8B2A9DE9EAFE4FF9F3" box="[151,393,1518,1544]" class="Actinopterygii" family="Amphiliidae" genus="Amphilius" kingdom="Animalia" order="Siluriformes" pageId="1" pageNumber="62" phylum="Chordata" rank="species" species="jacksonii">
