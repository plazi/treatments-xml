<subSubSection id="C30C92B79513FFD4FDB5FE0C728CFFB4" pageId="22" pageNumber="23" type="biology_ecology">
<paragraph id="8BA9C13C9513FFD4FDB5FE0C728CFFB4" blockId="22.[151,1437,320,527]" pageId="22" pageNumber="23">
<emphasis id="B9621D2E9513FFD4FDB5FE0C724AFFD1" bold="true" box="[199,295,392,418]" pageId="22" pageNumber="23">Biology.</emphasis>
