<geoCoordinate id="4A8AE681FC46FF99FF5903B581779FF0" box="[258,286,1421,1446]" degrees="18, 21" direction="west" orientation="longitude" pageId="22" pageNumber="473" precision="555" value="-18.21">18</geoCoordinate>
