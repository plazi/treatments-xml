<quantity id="C50F8B85FFBDFF8AFABAFCCDFA05FCCB" box="[1333,1432,870,894]" metricMagnitude="3" metricUnit="m" metricValue="3.3" pageId="7" pageNumber="242" unit="m" value="3300.0">3.300 m</quantity>
