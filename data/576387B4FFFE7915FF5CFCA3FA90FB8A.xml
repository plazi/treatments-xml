<taxonomicName id="18CA4D21FFFE7915FF5CFCA3FE69FCA7" authority="Hood, 1936" authorityName="Hood" authorityYear="1936" box="[151,452,832,859]" class="Insecta" family="Thripidae" genus="Frankliniella" kingdom="Animalia" order="Thysanoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="gossypiana">
