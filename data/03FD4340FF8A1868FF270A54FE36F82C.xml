<taxonomicName id="4C5489D5FF8A1868FE1F0879FDC3F8BC" baseAuthorityName="Wiedemann" baseAuthorityYear="1830" box="[431,627,1871,1897]" class="Insecta" family="Syrphidae" genus="Palpada" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="nigripes">
