<taxonomicName id="4CFE4D1FFFB1FE01ACEF86B0FDF5FEC6" authority=", Labiatae und Compositae" authorityName="Labiatae und Compositae" box="[178,553,248,270]" class="Magnoliopsida" family="Rutaceae" kingdom="Plantae" order="Sapindales" pageId="5" pageNumber="1034" phylum="Tracheophyta" rank="family">Rutaceae, Labiatae und Compositae</taxonomicName>
