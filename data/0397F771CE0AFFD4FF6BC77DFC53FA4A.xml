species: 
<taxonomicName id="4C3E3DE4CE0AFFD5FEC0C71AFC97F9A7" authority="Maskell, 1896: 435" authorityName="Maskell" authorityPageNumber="435" authorityYear="1896" box="[316,775,1613,1639]" class="Insecta" family="Aleyrodidae" genus="Aleurodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="holmesii">
