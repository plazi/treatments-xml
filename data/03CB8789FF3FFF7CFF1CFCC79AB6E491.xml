<taxonomicName id="4C624D1CFF3FFF7CFED8FA439BC1E491" authority="Bezzi &amp; Stein, 1907: 362" authorityName="Bezzi &amp; Stein" authorityPageNumber="362" authorityYear="1907" box="[339,712,1458,1484]" class="Insecta" family="Tachinidae" genus="Redia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="142" pageNumber="143" phylum="Arthropoda" rank="genus">
