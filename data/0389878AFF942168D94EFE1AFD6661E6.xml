<taxonomicName id="4C204D1FFF942168D89CFDE3FD8060EB" authorityName="Wei and Gu" authorityYear="2019" box="[427,559,628,652]" class="Bacilli" family="Lactobacillaceae" genus="Lactobacillus" kingdom="Bacteria" order="Lactobacillales" pageId="25" pageNumber="2807" phylum="Firmicutes" rank="species" species="hulinensis">
