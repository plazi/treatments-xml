<paragraph id="A3856C0EAB5992DF3C800C899FD2FEB4" pageId="8" pageNumber="9">
<taxonomicName id="FED6B3EBE6FAD7A4AA8750156BD0BD86" class="Collembola" family="Onychiuridae" genus="Sensillonychiurus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sensillonychiurus minusculus" order="Poduromorpha" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="minusculus">Sensillonychiurus minusculus</taxonomicName>
