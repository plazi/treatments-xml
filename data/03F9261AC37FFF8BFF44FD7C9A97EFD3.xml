<taxonomicName id="4C50EC8FC37FFF8BFF1AFD7C9C34ED88" authority="Guerin 1838" authorityName="Guerin-Meneville" authorityYear="1838" box="[227,540,651,675]" class="Insecta" family="Perlidae" genus="Perla" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="175" phylum="Arthropoda" rank="species" species="caucasica">
