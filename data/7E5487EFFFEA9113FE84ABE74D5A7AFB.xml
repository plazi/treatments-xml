<quantity id="31059B1CFFEA9115FD0DAF674C937B66" box="[641,735,1793,1819]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="3" pageNumber="4" unit="mm" value="3.7">3.7 mm</quantity>
; 
<number id="403524DEFFEA9115FD60AF674CB97B66" box="[748,757,1793,1819]" pageId="3" pageNumber="4" value="1.0">1</number>
