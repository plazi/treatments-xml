<taxonomicName id="4C724D4A702DFFE1FA46FABB5B40DA9C" authority="(Guerin-Meneville, 1835)" baseAuthorityName="Guerin-Meneville" baseAuthorityYear="1835" class="Insecta" family="Gelastocoridae" genus="Gelastocoris" kingdom="Animalia" order="Hemiptera" pageId="14" pageNumber="59" phylum="Arthropoda" rank="species" species="flavus">
