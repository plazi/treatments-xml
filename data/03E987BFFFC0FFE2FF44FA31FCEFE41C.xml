<taxonomicName id="4C404D2AFFC6FFFEFE72F939FDFBE0DD" authorityName="Hughes" authorityYear="1948" box="[417,523,1766,1788]" class="Arachnida" family="Phytoseiidae" genus="Neoseiulus" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="464" phylum="Arthropoda" rank="species" species="barkeri">
