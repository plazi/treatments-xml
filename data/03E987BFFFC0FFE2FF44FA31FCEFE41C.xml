, 
<emphasis id="B934EABBFFC6FFFEFD85F840FD51E195" box="[598,673,1950,1972]" italics="true" pageId="13" pageNumber="464">idm1–6</emphasis>
, and 
<emphasis id="B934EABBFFC6FFFEFD0AF840FD08E195" box="[729,760,1950,1972]" italics="true" pageId="13" pageNumber="464">idx</emphasis>
