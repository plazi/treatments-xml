<taxonomicName id="02D756816313FFABFE11F97DFB0AF94F" box="[461,551,1679,1701]" class="Insecta" family="Staphylinidae" genus="Syndicus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="571" phylum="Arthropoda" rank="genus">
