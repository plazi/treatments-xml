<taxonomicName id="4C319EBF1913FFFF690996001B74F9A3" box="[885,1109,1550,1573]" class="Aves" family="Oriolidae" genus="Oriolus" kingdom="Animalia" order="Passeriformes" pageId="44" pageNumber="44" phylum="Chordata" rank="species" species="flavocinctus">
