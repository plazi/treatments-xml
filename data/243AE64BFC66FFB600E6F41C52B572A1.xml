<emphasis id="9EE78B4FFC67FFA90329F2F0539A7381" bold="true" box="[564,673,205,229]" pageId="15" pageNumber="2046">
<collectingRegion id="6E5799BFFC67FFA90329F2F0539A7381" box="[564,673,205,229]" country="Australia" name="Tasmania" pageId="15" pageNumber="2046">Tasmania</collectingRegion>
