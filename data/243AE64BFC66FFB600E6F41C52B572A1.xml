<quantity id="6B6BFAB8FC67FFA90004F31350647222" box="[281,351,302,326]" metricMagnitude="2" metricUnit="m" metricValue="6.0" pageId="15" pageNumber="2046" unit="m" value="600.0">600 m</quantity>
, 
<date id="D82D719DFC67FFA90070F313530A7222" box="[365,561,302,326]" pageId="15" pageNumber="2046" value="1999-01-21">21 January 1999</date>
