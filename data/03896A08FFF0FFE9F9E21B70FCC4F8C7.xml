<taxonomicName id="4C20A09DFFF0FFE9FD5B1B82FB52F97C" box="[1070,1198,1766,1789]" class="Insecta" family="Formicidae" genus="Bannapone" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="83" phylum="Arthropoda" rank="species" species="cryptica">
