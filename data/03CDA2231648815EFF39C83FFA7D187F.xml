<taxonomicName id="4C6468B61649815DFE88CCE3FCD618BE" authority="(Grouvelle)" authorityName="Halstead" authorityYear="2020" baseAuthorityName="Grouvelle" baseAuthorityYear="2020" box="[369,779,1436,1463]" class="Insecta" family="Silvanidae" genus="Austronausibius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="12" phylum="Arthropoda" rank="species" species="wagneri">
