</taxonomicName>
, 
<taxonomicName id="989FFED8A32FFFAF85AF5872B1A3685E" authority="(Berk. &amp; M. A. Curtis) Zmitr." authorityName="Zmitr." baseAuthorityName="Berk. &amp; M. A. Curtis" class="Insecta" family="Braconidae" genus="Pappia" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="320" phylum="Arthropoda" rank="species" species="fissilis">
<emphasis id="6DEB5949A32FFFAF85AF5872B5DA683A" box="[1017,1177,1544,1570]" italics="true" pageId="3" pageNumber="320">Pappia fissilis</emphasis>
(Berk. &amp; M.A. Curtis) Zmitr.
</taxonomicName>
, 
<taxonomicName id="989FFED8A32FFFAF86BA5856B08F685E" box="[236,460,1580,1606]" class="Agaricomycetes" family="Fomitopsidaceae" genus="Fomitopsis" kingdom="Fungi" order="Polyporales" pageId="3" pageNumber="320" phylum="Basidiomycota" rank="species" species="betulina">
<emphasis id="6DEB5949A32FFFAF86BA5856B08F685E" box="[236,460,1580,1606]" italics="true" pageId="3" pageNumber="320">Fomitopsis betulina</emphasis>
</taxonomicName>
