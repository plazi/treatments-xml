<taxonomicName id="989FFED8A32FFFAF87EA582BB3836872" box="[444,704,1616,1642]" class="Agaricomycetes" family="Polyporaceae" genus="Cerioporus" higherTaxonomySource="CoL" kingdom="Fungi" order="Polyporales" pageId="3" pageNumber="320" phylum="Basidiomycota" rank="species" species="squamosus">
<emphasis id="6DEB5949A32FFFAF87EA582BB3836872" box="[444,704,1616,1642]" italics="true" pageId="3" pageNumber="320">Cerioporus squamosus</emphasis>
