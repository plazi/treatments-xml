<taxonomicName id="989FFED8A32FFFAF84AA582AB2BA6872" box="[764,1017,1616,1642]" class="Agaricomycetes" family="Fomitopsidaceae" genus="Laetiporus" kingdom="Fungi" order="Polyporales" pageId="3" pageNumber="320" phylum="Basidiomycota" rank="species" species="sulphureus">
