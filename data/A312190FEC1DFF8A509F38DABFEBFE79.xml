<quantity id="EC4305FCEC1CFF8A542D3C36BFD1FEEB" box="[1061,1165,295,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.6" pageId="9" pageNumber="514" unit="mm" value="1.6">1.60 mm</quantity>
, Edist 
<quantity id="EC4305FCEC1CFF8A54D53C39BE19FEEB" box="[1245,1349,296,320]" metricMagnitude="-3" metricUnit="m" metricValue="1.08" pageId="9" pageNumber="514" unit="mm" value="1.08">1.08 mm</quantity>
, Ediam 
<quantity id="EC4305FCEC1CFF8A509F3C5DBBA1FEB0" box="[151,253,332,357]" metricMagnitude="-4" metricUnit="m" metricValue="5.8999999999999995" pageId="9" pageNumber="514" unit="mm" value="0.59">0.59 mm</quantity>
, LE 
<quantity id="EC4305FCEC1CFF8A513A3C5DBA88FEB0" box="[306,468,332,356]" metricMagnitude="-2" metricUnit="m" metricValue="1.1350000000000002" metricValueMax="1.1400000000000001" metricValueMin="1.1300000000000001" pageId="9" pageNumber="514" unit="mm" value="11.35" valueMax="11.4" valueMin="11.3">11.3–11.4 mm</quantity>
, antennomere 3: stem 
