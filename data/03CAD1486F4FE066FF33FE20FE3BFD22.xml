(
<bibRefCitation id="EFF21DAF6F4FE066FED1FD10FDBEFD07" author="Stekolnikov" box="[373,578,706,731]" pageId="8" pageNumber="503" refString="Stekolnikov, A. A (2013) Leptotrombidium (Acari: Trombiculidae) of the world. Zootaxa, 3728 (1), 1 - 173. http: // dx. doi. org / 10.11646 / zootaxa. 3728.1.1" type="journal article" year="2013">
