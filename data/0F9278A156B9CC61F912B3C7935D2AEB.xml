<taxonomicName id="BE26D94333AC00D7BE8EB2FB1047A40B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24818" class="Insecta" family="Formicidae" genus="Myrmecina" kingdom="Animalia" lsidName-HNS="Myrmecina Curtis" order="Hymenoptera" pageNumber="38" phylum="Arthropoda" rank="genus">Myrmecina</taxonomicName>
