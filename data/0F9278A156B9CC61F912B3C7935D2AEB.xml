<taxonomicName id="25BF95AE9357A1F5A7391E39D07F74A8" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:24925" class="Insecta" family="Formicidae" genus="Strongylognathus" kingdom="Animalia" lsidName-HNS="Strongylognathus Mayr" order="Hymenoptera" pageNumber="38" phylum="Arthropoda" rank="genus">Strongylognathus</taxonomicName>
