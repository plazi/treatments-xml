<paragraph id="A6F96AF8E5033BD60BF2BE99C3B28E94" pageNumber="36">
SUBFAMILY 
<taxonomicName id="5ED7557C902A88F260527EB423B6D69E" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:2258" lsidName-HNS="Myrmicinae Lepeletier" pageNumber="36" rank="subFamily" subFamily="Myrmicinae">MYRMICINAE</taxonomicName>
