<taxonomicName id="5EEC4D5CFFB3FFCFFE31A240FDDBFBBB" box="[474,565,1052,1075]" class="Insecta" family="Bethylidae" genus="Dissomphalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="secretus">
