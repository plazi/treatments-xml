<taxonomicName id="3CAE4D28FFDDFFCAFB2860CF388674A6" authority="Stephens" authorityName="Stephens" box="[1173,1435,1740,1765]" class="Insecta" family="Chrysomelidae" genus="Chaetocnema" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="114" phylum="Arthropoda" rank="genus">
<emphasis id="C9DAEAB9FFDDFFCAFB2860CF383274A7" box="[1173,1327,1740,1764]" italics="true" pageId="3" pageNumber="114">Chaetocnema</emphasis>
Stephens
</taxonomicName>
and 
<taxonomicName id="3CAE4D28FFDDFFCAFF7B60F23CA8754A" authority="Latreille" authorityName="Latreille" box="[198,437,1776,1801]" class="Insecta" family="Chrysomelidae" genus="Longitarsus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="114" phylum="Arthropoda" rank="genus">
<emphasis id="C9DAEAB9FFDDFFCAFF7B60F23C51754B" box="[198,332,1777,1800]" italics="true" pageId="3" pageNumber="114">Longitarsus</emphasis>
