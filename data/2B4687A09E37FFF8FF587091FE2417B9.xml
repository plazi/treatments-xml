</paragraph>
<paragraph id="A35036B69E37FFFFFF3370F5FF31131C" blockId="51.[128,654,1208,1361]" pageId="51" pageNumber="51">
<treatmentCitationGroup id="83FF11989E37FFFFFF3370F5FF31131C" pageId="51" pageNumber="51">
Now 
<taxonomicName id="64EF4D359E37FFFFFF7B70F5FDA4134C" authority="(Swainson, 1827)" baseAuthorityName="Swainson" baseAuthorityYear="1827" box="[200,594,1262,1282]" class="Aves" family="Icteridae" genus="Icterus" kingdom="Animalia" order="Passeriformes" pageId="51" pageNumber="51" phylum="Chordata" rank="species" species="bullockii">
