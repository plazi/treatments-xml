<taxonomicName id="E7EB4D25FFCBFFFCA47F82FDD839919F" box="[1042,1373,993,1017]" class="Insecta" family="Sarcophagidae" genus="Heteronychia" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="volcanoaetnica">
