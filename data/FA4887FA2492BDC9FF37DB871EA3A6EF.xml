<taxonomicName id="B5E14D6F2492BDCBFD58DB871FEFA6DE" authority="Poiteau (1822: 391)" authorityName="Poiteau" authorityPageNumber="391" authorityYear="1822" box="[760,1232,1477,1503]" class="Magnoliopsida" family="Arecaceae" genus="Gynestum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecales" pageId="140" pageNumber="141" phylum="Tracheophyta" rank="species" species="strictum">
