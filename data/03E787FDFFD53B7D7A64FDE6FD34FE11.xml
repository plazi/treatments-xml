<subSubSection id="C3546560FFD23B7D7A54F99FFD34FE11" lastPageId="5" lastPageNumber="6" pageId="4" pageNumber="5" type="materials_examined">
<paragraph id="8BF136EBFFD23B7C7A54F99FFD97F960" blockId="4.[264,1325,1653,1879]" pageId="4" pageNumber="5">
<emphasis id="B93AEAF9FFD23B7C7A54F99FFE6BF930" bold="true" box="[312,498,1653,1679]" pageId="4" pageNumber="5">
<typeStatus id="54F58849FFD23B7C7A54F99FFE33F930" box="[312,426,1653,1679]" pageId="4" pageNumber="5">Holotype</typeStatus>
male
