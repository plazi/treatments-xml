<collectionCode id="ED2FDEA2CE3FFFE0FF6BC432FF61FABF" box="[151,241,1381,1407]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="25" pageNumber="26" type="Museum">USNM</collectionCode>
