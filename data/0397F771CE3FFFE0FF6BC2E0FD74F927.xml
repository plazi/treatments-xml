</taxonomicName>
and 
<taxonomicName id="4C3E3DE4CE3FFFE0FC13C442FB4CFAEE" authorityName="Morgan" authorityYear="1892" box="[1007,1244,1301,1326]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="anonae">
