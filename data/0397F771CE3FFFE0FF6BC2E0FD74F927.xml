/ 
<taxonomicName id="4C3E3DE4CE3FFFE0FAC3C72AFA0CF956" box="[1343,1436,1661,1686]" class="Insecta" family="Aleyrodidae" genus="Aleyrodes" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="juleikae">
