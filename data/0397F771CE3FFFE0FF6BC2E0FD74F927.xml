/ 
<taxonomicName id="4C3E3DE4CE3FFFE0FB51C72AFAB7F956" baseAuthorityName="Maskell" baseAuthorityYear="1896" box="[1197,1319,1661,1686]" class="Insecta" family="Aleyrodidae" genus="Aleurodicus" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="pulvinatus">
