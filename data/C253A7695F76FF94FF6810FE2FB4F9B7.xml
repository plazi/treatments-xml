<taxonomicName id="8DFA6DFC5F76FF94FBC01257295CF993" authority="(Distant)" baseAuthorityName="Distant" baseAuthorityYear="1918" box="[1087,1344,1589,1616]" class="Insecta" family="Cicadellidae" genus="Stirellus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="402" phylum="Arthropoda" rank="species" species="lahorensis">
<emphasis id="788ECA6D5F76FF94FBC012572851F993" box="[1087,1101,1590,1616]" italics="true" pageId="4" pageNumber="402">S</emphasis>
