<taxonomicName id="8DFA6DFC5F76FF94FDA812572F20F993" authority="(Pruthi)" baseAuthorityName="Pruthi" baseAuthorityYear="1930" box="[599,828,1589,1616]" class="Insecta" family="Cicadellidae" genus="Stirellus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="402" phylum="Arthropoda" rank="species" species="viridulus">
