<taxonomicName id="8DFA6DFC5F76FF94FD3C123B2FB8F9B7" authority="(Pruthi)" baseAuthorityName="Pruthi" baseAuthorityYear="1930" box="[707,932,1625,1652]" class="Insecta" family="Cicadellidae" genus="Stirellus" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="402" phylum="Arthropoda" rank="species" species="viridulus">
<emphasis id="788ECA6D5F76FF94FD3C123B2ECDF9B7" box="[707,721,1626,1652]" italics="true" pageId="4" pageNumber="402">S</emphasis>
