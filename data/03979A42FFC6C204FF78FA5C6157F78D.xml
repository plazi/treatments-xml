sect. 
<taxonomicName id="4C3E50D7FFC6C204FF11F9956140F9F0" authorityName="Y. M. Shui &amp; W. H. Chen" baseAuthorityName="Klotzsch" baseAuthorityYear="1854" box="[225,370,1597,1622]" class="Magnoliopsida" family="Begoniaceae" genus="Begonia" kingdom="Plantae" order="Cucurbitales" pageId="1" pageNumber="75" phylum="Tracheophyta" rank="section" section="Petermannia">
