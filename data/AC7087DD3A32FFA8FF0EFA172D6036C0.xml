from the 
<collectionCode id="42C8AE0E3A32FFA9FCE8F9402DC333DD" LSID="urn:lsid:biocol.org:col:34988" box="[881,974,1669,1695]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="14" pageNumber="15">MNHN</collectionCode>
