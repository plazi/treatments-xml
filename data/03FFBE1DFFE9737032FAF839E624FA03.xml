</emphasis>
, 
<typeStatus id="54EDB1A9FFED734E31EEF830E0E3F878" box="[899,996,1930,1953]" pageId="30" pageNumber="31">holotype</typeStatus>
; 
<emphasis id="B922D319FFED734E3182F830E706F87B" bold="true" box="[1007,1025,1930,1954]" pageId="30" pageNumber="31">B</emphasis>
: 
<taxonomicName id="4C567488FFED734E367DF830E7A4F878" authority="Wesener, 2009" authorityName="Wesener" authorityYear="2009" box="[1040,1187,1930,1953]" class="Diplopoda" family="Arthrosphaeridae" genus="Zoosphaerium" kingdom="Animalia" order="Sphaerotheriida" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="pulchellum" status="n. sp.">
<emphasis id="B922D319FFED734E367DF830E7A4F878" box="[1040,1187,1930,1953]" italics="true" pageId="30" pageNumber="31">Z. pulchellum</emphasis>
</taxonomicName>
<emphasis id="B922D319FFED734E36C1F830E7EEF87B" bold="true" box="[1196,1257,1930,1954]" pageId="30" pageNumber="31">
<taxonomicNameLabel id="A2116E62FFED734E36C1F830E7EEF87B" box="[1196,1257,1930,1954]" pageId="30" pageNumber="31" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<typeStatus id="54EDB1A9FFED734E369AF830E65FF878" box="[1271,1368,1930,1953]" pageId="30" pageNumber="31">holotype</typeStatus>
; 
<emphasis id="B922D319FFED734E3709F830E670F87B" bold="true" box="[1380,1399,1930,1954]" pageId="30" pageNumber="31">C</emphasis>
