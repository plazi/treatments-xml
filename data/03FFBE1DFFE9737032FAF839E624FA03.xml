<quantity id="4CAEA2EEFFE8734B317CF975E055F93F" box="[785,850,1743,1766]" metricMagnitude="-3" metricUnit="m" metricValue="1.0" pageId="27" pageNumber="28" unit="mm" value="1.0">1 mm</quantity>
