<quantity id="0A119B28FFD9FF8DFF1CFEDAFE91FEFC" box="[151,281,332,357]" metricMagnitude="2" metricUnit="m" metricValue="5.64" metricValueMax="6.16" metricValueMin="5.12" pageId="2" pageNumber="195" unit="m" value="564.0" valueMax="616.0" valueMin="512.0">
