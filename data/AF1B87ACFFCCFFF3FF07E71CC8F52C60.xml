<taxonomicName id="E0B24D39FFCAFFF3FC24E074CE5C2C4A" box="[948,1125,301,326]" class="Insecta" family="Haglotettigoniidae" genus="Pteronemobius" kingdom="Animalia" order="Orthoptera" pageId="5" pageNumber="64" phylum="Arthropoda" rank="genus">
