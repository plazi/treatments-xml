<taxonomicName id="4C664D76FFFE6B6A887BACD38552FB4A" authority="Huang &amp; Zhang, 2006" authorityName="Huang &amp; Zhang" authorityYear="2006" box="[294,630,1226,1248]" class="Adenophorea" family="Xyalidae" genus="Paramphimonhystrella" kingdom="Animalia" order="Monhysterida" pageId="1" pageNumber="479" phylum="Nematoda" rank="species" species="elegans">
