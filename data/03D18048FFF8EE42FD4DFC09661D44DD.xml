<paragraph id="8BC7315EFFF8EE41FD06FC7264D245F2" blockId="5.[685,1172,948,1023]" pageId="5" pageNumber="226">
= 
<taxonomicName id="4C784ADDFFF8EE41FD14FC72644545E9" authorityName="Dejean" authorityYear="1829" box="[703,832,975,996]" class="Insecta" family="Carabidae" genus="Selenophorus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="226" phylum="Arthropoda" rank="genus">
