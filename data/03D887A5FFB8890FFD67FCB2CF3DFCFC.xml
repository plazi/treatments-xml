<taxonomicName id="4C714D30FFB7890FFD8EFEA8CEAAFE0E" authority="(Caramaschi, Silva, and Feio, 2004)" baseAuthorityName="Caramaschi, Silva, and Feio" baseAuthorityYear="2004" class="Amphibia" family="Hylidae" genus="Hypsiboas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="88" pageNumber="89" phylum="Chordata" rank="species" species="pombali">
