<paragraph id="8BCE368D6C13FF80FFA8FE75FCB5FE30" blockId="35.[106,1096,439,494]" pageId="35" pageNumber="36">
<taxonomicName id="4C714D0E6C13FF80FFA8FE75FF0FFE11" authorityName="LINDQVIST" authorityYear="1968" box="[106,253,439,463]" class="Insecta" family="Tenthredinidae" genus="Empria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="basalis">
