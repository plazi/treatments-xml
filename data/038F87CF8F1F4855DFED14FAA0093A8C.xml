<collectingCountry id="F33176498F1E4855DA471234A4ED3A34" box="[1386,1430,575,601]" name="Switzerland" pageId="9" pageNumber="8">CH</collectingCountry>
). 
<taxonomicName id="4C264D5A8F1E4855DFED1269A03A3A16" authorityName="Darlington" authorityYear="1952" box="[192,321,610,635]" class="Insecta" family="Carabidae" genus="Notagonum" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="8" phylum="Arthropoda" rank="genus">
<emphasis id="B952EACB8F1E4855DFED1269A03A3A16" box="[192,321,610,635]" italics="true" pageId="9" pageNumber="8">Notagonum</emphasis>
