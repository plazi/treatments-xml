<collectingRegion id="49E2F83B8F1E4855DE4D1216A0AB3A55" box="[352,464,541,568]" country="Australia" name="Tasmania" pageId="9" pageNumber="8">Tasmania</collectingRegion>
), 
<collectingCountry id="F33176498F1E4855DEC91216A3F13A5A" box="[484,650,541,567]" name="Norfolk Island" pageId="9" pageNumber="8">Norfolk Island</collectingCountry>
