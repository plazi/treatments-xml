<taxonomicName id="B4F7C2D2D5C7139792935FB408025015" authorityName="Wannenmacher &amp; Dietze &amp; Franz &amp; Schweigert" authorityYear="2021" class="Ostracoda" family="Bythocytheridae" genus="Eucytherura" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Eucytherura" order="Paracyprididae" pageId="6" pageNumber="7" phylum="Arthropoda" rank="genus">
