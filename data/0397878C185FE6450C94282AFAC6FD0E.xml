<taxonomicName id="4C3E4D19185EE6460D26296CFE74F986" box="[293,424,1582,1604]" class="Actinopterygii" family="Blenniidae" genus="Meiacanthus" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="48" phylum="Chordata" rank="genus">Meiacanthus</taxonomicName>
