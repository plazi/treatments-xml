<taxonomicName id="4C3E4D19185EE6460D26296CFDD3F986" box="[293,527,1582,1604]" class="Actinopterygii" family="Blenniidae" genus="Meiacanthus" kingdom="Animalia" order="Perciformes" pageId="9" pageNumber="48" phylum="Chordata" rank="species" species="erdmanni">
