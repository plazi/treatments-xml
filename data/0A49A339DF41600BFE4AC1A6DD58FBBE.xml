<taxonomicName id="45E069ACDF41600CFE4AC1A6DB92FC4C" authority="Kott, 2010" authorityName="Kott" authorityYear="2010" box="[463,749,990,1014]" class="Ascidiacea" family="Didemnidae" genus="Polysyncraton" kingdom="Animalia" order="Aplousobranchia" pageId="27" pageNumber="2481" phylum="Chordata" rank="species" species="montanum" status="sp. nov.">
