<taxonomicName id="4C694D72455CFF90FE24FE3EFD9149A3" box="[399,524,409,434]" class="Insecta" family="Formicidae" genus="Metapone" kingdom="Animalia" order="Hymenoptera" pageId="43" pageNumber="544" phylum="Arthropoda" rank="species" species="tricolor">
