<paragraph id="78D1AC5CFF48C2AA3172441490E6AA42" pageId="null" pageNumber="191">
<taxonomicName id="F7A0A924A564590D34C3E267DAFB7821" authority="L." class="Magnoliopsida" family="Plantaginaceae" genus="Veronica" kingdom="Plantae" order="Lamiales" pageId="null" pageNumber="191" phylum="Tracheophyta" rank="species" species="chamaedrys">
