in 
<date id="FF8B05893451E110FD353893FC9C7ECC" box="[643,804,1157,1180]" pageId="3" pageNumber="449" value="2012" valueMax="2015">
<number id="3DFD316E3451E110FD353893FD007ECC" box="[643,696,1157,1180]" pageId="3" pageNumber="449" value="2012.0">2012</number>
and 
<number id="3DFD316E3451E110FD5D3893FC9C7ECC" box="[747,804,1157,1180]" pageId="3" pageNumber="449" value="2015.0">2015</number>
