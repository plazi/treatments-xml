<paragraph id="ECDE7D0115F294E9BA1E39775343D5EA" pageId="null" pageNumber="136">
<taxonomicName id="EBE2D8BABF9365EE3124A609253CA265" authority="L." class="Polypodiopsida" family="Aspleniaceae" genus="Asplenium" kingdom="Plantae" order="Polypodiales" pageId="null" pageNumber="136" phylum="Tracheophyta" rank="species" species="ruta-muraria">
<pageBreakToken id="28DBA09E95EFBE2CD88E5140EE0DAC9E" pageId="null" pageNumber="136">Asplenium</pageBreakToken>
<normalizedToken id="0A67471657C2AF57A9E646BA6C5C2BAE" originalValue="Rúta-murária" pageId="null" pageNumber="136">Ruta-muraria</normalizedToken>
<authorityName id="FB0F9B5299DDF1812B628482745513CC" pageId="null" pageNumber="136">L.</authorityName>
</taxonomicName>
</paragraph>
