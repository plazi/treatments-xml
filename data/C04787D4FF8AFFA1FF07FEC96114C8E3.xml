in 
<bibRefCitation id="2C7F4B33FF8AFFA1FC11FCC161A9C8C7" author="Acevedo-Gutierrez, L. Y. &amp; Paternina, L. E. &amp; Perez-Perez, J. C. &amp; Londono, A. F. &amp; Lopez, G. &amp; Rodas, J. D." box="[897,1258,870,896]" pageId="99" pageNumber="100" pagination="126 - 139" refId="ref177868" refString="Acevedo-Gutierrez, L. Y., Paternina, L. E., Perez-Perez, J. C., Londono, A. F., Lopez, G. &amp; Rodas, J. D. (2020) Garrapatas duras (Acari: Ixodidae) de Colombia, una revision a su conocimiento en el pais. Acta Biologica. Colombiana, 25, 126 - 139. https: // doi. org / 10.15446 / abc. v 25 n 1.75252" type="journal article" year="2020">
Acevedo-Gutiérrez 
<emphasis id="7A9AEAD0FF8AFFA1FBCFFCC061DBC8C7" box="[1119,1176,870,896]" italics="true" pageId="99" pageNumber="100">et al.</emphasis>
(2020)
</bibRefCitation>
</taxonomicName>
need confirmation. 
<collectingRegion id="8A2AF820FF8AFFA1FF41FC2D65BAC8E3" box="[209,249,906,932]" country="South Sudan" name="Lakes" pageId="99" pageNumber="100">See</collectingRegion>
also 
<taxonomicName id="8FEE4D41FF8AFFA1FEA4FC2D6724C8E3" authorityName="Tonelli Rondelli" authorityYear="1939" box="[308,615,906,932]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="99" pageNumber="100" phylum="Arthropoda" rank="species" species="latepunctatum">
<emphasis id="7A9AEAD0FF8AFFA1FEA4FC2D6724C8E3" box="[308,615,906,932]" italics="true" pageId="99" pageNumber="100">Amblyomma latepunctatum</emphasis>
</taxonomicName>
