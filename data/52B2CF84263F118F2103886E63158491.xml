<paragraph id="9DB376E7C16149562E76F291054848ED" pageId="14" pageNumber="15">
<taxonomicName id="131A01BDC8F9991213299F354F068BBC" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus balimensis" order="Coleoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="balimensis">Trigonopterus balimensis</taxonomicName>
