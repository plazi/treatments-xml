<taxonomicName id="4C184D26315C6800FD4EF31DFCE2FBE0" box="[709,862,1052,1076]" class="Insecta" family="Cerambycidae" genus="Paragolsinda" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="40" phylum="Arthropoda" rank="genus">
