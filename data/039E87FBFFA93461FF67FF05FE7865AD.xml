<number id="3DFF24CAFFA93460FA91FBABFACB600C" box="[1377,1393,1081,1107]" pageId="10" pageNumber="11" value="5.0">5</number>
; R 
<number id="3DFF24CAFFA93460FA64FBDBFA266008" box="[1428,1436,1097,1111]" pageId="10" pageNumber="11" value="4.0">4</number>
