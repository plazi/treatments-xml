syn. 
<taxonomicName id="4C524D11CA47FF974640FD9085F0FDFE" authority="Bezzi, 1908: 167" authorityName="Bezzi" authorityPageNumber="167" authorityYear="1908" box="[315,689,558,582]" class="Insecta" family="Diopsidae" genus="Diopsis" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="breviseta">
