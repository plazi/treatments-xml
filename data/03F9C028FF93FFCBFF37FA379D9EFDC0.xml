<taxonomicName id="4C500ABDFF9DFFCBFDE5FE039E8DFE48" authority="Huang, 2001" authorityName="Huang" authorityYear="2001" box="[581,971,469,495]" class="Arachnida" family="Diptilomiopidae" genus="Diptacus" kingdom="Animalia" order="Prostigmata" pageId="9" pageNumber="48" phylum="Arthropoda" rank="species" species="tristylaus">
