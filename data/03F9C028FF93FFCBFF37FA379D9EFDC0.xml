<subSubSection id="C34A22B5FF9DFFCBFF66FE539D9EFDC0" pageId="9" pageNumber="48" type="etymology">
<paragraph id="8BEF713EFF9DFFCBFF66FE539CAAFE6F" blockId="9.[151,1436,152,615]" pageId="9" pageNumber="48">
<emphasis id="B924AD2CFF9DFFCBFF66FE539C15FE07" bold="true" box="[198,339,390,416]" pageId="9" pageNumber="48">Etymology.</emphasis>
