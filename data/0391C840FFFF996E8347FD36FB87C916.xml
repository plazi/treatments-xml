<paragraph id="8B877956FFFF996F8377F964FBA1CBB6" blockId="22.[264,1324,836,1902]" pageId="22" pageNumber="23">
<emphasis id="B94CA544FFFF996F8377F964FDE8CB8E" bold="true" box="[312,522,1716,1742]" italics="true" pageId="22" pageNumber="23">
