<specimenCode id="E45B9EDBFFFD7B026FE0E237D48F8CC1" box="[707,734,1655,1679]" collectionCode="T" name="Tavera, Department of Geology and Geophysics" pageId="9" pageNumber="61">T1</specimenCode>
– 
<specimenCode id="E45B9EDBFFFD7B026FC8E237D5598CC1" box="[747,776,1655,1679]" collectionCode="T" name="Tavera, Department of Geology and Geophysics" pageId="9" pageNumber="61">T7</specimenCode>
; disc of 
<specimenCode id="E45B9EDBFFFD7B026E47E237D52E8CC1" box="[868,895,1655,1679]" collectionCode="T" name="Tavera, Department of Geology and Geophysics" pageId="9" pageNumber="61">T3</specimenCode>
