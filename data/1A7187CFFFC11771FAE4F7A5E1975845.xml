</paragraph>
<paragraph id="926736D9FFC11776FB47F760E7F35352" blockId="43.[1277,2128,2226,2802]" pageId="43" pageNumber="360">
<taxonomicName id="55D84D5AFFC11776FB47F760E7A45284" authorityName="Matthew" authorityYear="1920" box="[1277,1500,2270,2302]" class="Reptilia" family="Hadrosauridae" genus="Procheneosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="43" pageNumber="360" phylum="Chordata" rank="genus">
