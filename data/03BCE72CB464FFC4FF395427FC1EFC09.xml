and 
<taxonomicName id="4C152DB9B466FFC4FAFF5336FEF3FE92" authorityName="Agardh" authorityYear="1832" class="Magnoliopsida" family="Rhizophoraceae" genus="Diatoma" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="217" phylum="Tracheophyta" rank="species" species="interstitiale">Diatoma interstitiale</taxonomicName>
.
