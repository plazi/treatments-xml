<heading id="1B2381B3712E555590C0144CFDBCD229" bold="true" box="[151,575,583,609]" fontSize="11" level="1" pageId="7" pageNumber="50" reason="1">
<emphasis id="72A0EACD712E555590C0144CFDBCD229" bold="true" box="[151,575,583,609]" pageId="7" pageNumber="50">
<taxonomicName id="87D44D5C712E555590C0144CFE59D229" box="[151,474,583,609]" class="Insecta" family="Leptohyphidae" genus="Tricorythopsis" kingdom="Animalia" order="Ephemeroptera" pageId="7" pageNumber="50" phylum="Arthropoda" rank="species" species="intercalatus" status="sp. nov.">
<emphasis id="72A0EACD712E555590C0144CFE59D229" bold="true" box="[151,474,583,609]" italics="true" pageId="7" pageNumber="50">Tricorythopsis intercalatus</emphasis>
