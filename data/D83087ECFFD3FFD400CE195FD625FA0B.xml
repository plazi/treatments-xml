<typeStatus id="8F228858FFD3FFD401061E44D2BDFB62" box="[351,458,1148,1173]" pageId="4" pageNumber="483">Holotype</typeStatus>
, female, length of body 
<quantity id="97619B1FFFD3FFD402831E44D037FB63" box="[730,832,1148,1173]" metricMagnitude="-3" metricUnit="m" metricValue="1.95" pageId="4" pageNumber="483" unit="mm" value="1.95">1.95 mm</quantity>
, fore wing 
<quantity id="97619B1FFFD3FFD403E71E44D76FFB63" box="[958,1048,1148,1172]" metricMagnitude="-3" metricUnit="m" metricValue="1.2" pageId="4" pageNumber="483" unit="mm" value="1.2">1.2 mm</quantity>
