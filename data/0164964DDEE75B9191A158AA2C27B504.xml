<taxonomicName id="6ED79293BCCE35CF62C096E5BD032487" class="Magnoliopsida" family="Amaranthaceae" genus="S." kingdom="Plantae" lsidName="S. zhakovi" order="Caryophyllales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="zhakovi">
