<taxonomicName id="A2C47FBDEAAA9A27D1DBC5A1F337AAFB" authorityName="Nupponen 1 &amp; Sihvonen" authorityYear="2022" class="Insecta" family="Scythrididae" genus="Scythris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scythris" order="Lepidoptera" pageId="0" pageNumber="19" phylum="Arthropoda" rank="genus">
<emphasis id="9D88B489DC6C68A8FE217818C6FD0723" italics="true" pageId="0" pageNumber="19">Scythris</emphasis>
