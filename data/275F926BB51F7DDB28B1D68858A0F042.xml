<subSubSection id="5F3457FD5096AC80372F429D268302BC" pageId="6" pageNumber="13" type="included species">
<paragraph id="AC47A35E8B5542542B6765BF002C640A" pageId="6" pageNumber="13">Included species.</paragraph>
<paragraph id="C88D0969E07D5E932F0385202F591311" pageId="6" pageNumber="13">
<taxonomicName id="91A266304D4659D7480B3C5CE8340528" class="Insecta" family="Acroceridae" genus="Leucopsina" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leucopsina burnsi" order="Diptera" pageId="6" pageNumber="13" phylum="Arthropoda" rank="species" species="burnsi">Leucopsina burnsi</taxonomicName>
(Paramonov, 1957); 
