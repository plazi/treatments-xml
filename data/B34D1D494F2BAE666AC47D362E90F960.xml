<taxonomicName id="FCE4D7DC4F2BAE656B0B7EFF297DF960" authorityName="Chandler and Geromini" authorityYear="1962" box="[780,889,1708,1729]" class="Insecta" family="Staphylinidae" genus="Custotychus" kingdom="Animalia" order="Coleoptera" pageId="5" phylum="Arthropoda" rank="species" species="natchez">
