<taxonomicName id="4C60D8B6963CFFC1FF0F2E4DB82BFAC6" authority="Kieffer 1900" authorityName="Kieffer" authorityYear="1900" box="[238,552,1372,1399]" class="Insecta" family="Aulacidae" genus="Pristaulacus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="188" phylum="Arthropoda" rank="genus">
