<quantity id="4C99CDB5FFAC9D2AFCD0F990FC16F90F" box="[862,965,1657,1682]" metricMagnitude="-3" metricUnit="m" metricValue="1.53" pageId="31" pageNumber="182" unit="mm" value="1.53">1.53 mm</quantity>
