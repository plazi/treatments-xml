<quantity id="4C99CDB5FFAC9D2AFDBCF993FD58F90F" box="[562,651,1658,1682]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="31" pageNumber="182" unit="mm" value="1.9">1.9 mm</quantity>
