<subSubSection id="7BCF6569FFAEE65D11B9FE5AFD8AFC82" pageId="17" pageNumber="208" type="materials_examined">
<paragraph id="336A36E2FFAEE65D11B9FE5AFAEFFDA3" blockId="17.[151,1437,498,1963]" pageId="17" pageNumber="208">
<emphasis id="01A1EAF0FFAEE65D11B9FE5AFE96FDFB" bold="true" box="[151,328,498,523]" pageId="17" pageNumber="208">Type material.</emphasis>
