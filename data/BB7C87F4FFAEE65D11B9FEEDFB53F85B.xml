<quantity id="F42D9B07FFAEE65D150CFD92FB57FDA2" box="[1058,1161,570,595]" metricMagnitude="-2" metricUnit="m" metricValue="3.57" pageId="17" pageNumber="208" unit="mm" value="35.7">35.7 mm</quantity>
