<quantity id="F42D9B07FFAEE65D15E6FE5BFAECFDFA" box="[1224,1330,499,523]" metricMagnitude="-2" metricUnit="m" metricValue="3.7399999999999998" pageId="17" pageNumber="208" unit="mm" value="37.4">37.4 mm</quantity>
