<quantity id="F42D9B07FFAEE65D10D9FD93FD80FDA2" box="[503,606,571,595]" metricMagnitude="-2" metricUnit="m" metricValue="3.8299999999999996" pageId="17" pageNumber="208" unit="mm" value="38.3">38.3 mm</quantity>
