<taxonomicName id="A9E2D6D9CA0BA70024AECDBFC446E781" class="Dicotyledoneae" family="Adoxaceae" genus="Viburnum" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Viburnum odoratissimum" order="Dipsacales" pageId="0" pageNumber="1" phylum="Angiospermae" rank="species" species="odoratissimum">
