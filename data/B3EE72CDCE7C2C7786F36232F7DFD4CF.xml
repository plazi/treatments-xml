<taxonomicName id="6E2C0686BF21E87802DD5678125D0E60" class="Insecta" family="Boreidae" genus="Caurinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Caurinus dectes" order="Mecoptera" pageId="7" pageNumber="42" phylum="Arthropoda" rank="species" species="dectes">Caurinus dectes</taxonomicName>
