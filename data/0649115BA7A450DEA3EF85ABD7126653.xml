<taxonomicName id="461327ADC4F47B33B856DD3E86A74DB2" authorityName="Csiki" authorityYear="1932" class="Hexapoda" family="Carabidae" genus="Bradycellus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="668" pageNumber="669" phylum="Arthropoda" rank="species" species="lecontei">
