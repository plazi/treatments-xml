<quantity id="4CBA9B2AFFB5EE44FEC1FDF907EFF519" box="[366,486,594,630]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="6" pageNumber="55" unit="mm" value="1.7">1.7 mm</quantity>
