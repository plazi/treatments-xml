<quantity id="D1329B33014F613BFDCEFB4FFD5703EA" box="[625,695,1178,1202]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="4" pageNumber="233" unit="mm" value="4.0">4 mm</quantity>
