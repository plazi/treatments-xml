<collectingCounty id="D24329D6C648FFEEFBA7FA45FB76A51D" box="[1093,1245,1861,1885]" pageId="0" pageNumber="593">Lewis County</collectingCounty>
, 
<collectingRegion id="F9599FB8C648FFEEFB05FA45FAC2A51D" box="[1255,1385,1861,1885]" country="United States of America" name="Washington" pageId="0" pageNumber="593">Washington</collectingRegion>
. 
<location id="3E420781C648FFEEFA91FA45FA37A51D" LSID="urn:lsid:plazi:treatment:B334E04CC648FFECFF75FB4CFE7FA07E:3E420781C648FFEEFA91FA45FA37A51D" box="[1395,1436,1861,1885]" county="Lewis County" latitude="46.4188" longLatPrecision="6" longitude="-122.8788" municipality="Gries Ranch Formation" name="The" pageId="0" pageNumber="593" stateProvince="Washington">The</location>
