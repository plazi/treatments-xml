<geoCoordinate id="5EA9379DC648FFEEFEC2FA8CFE37A5E4" box="[288,412,1932,1956]" degrees="46.4188" direction="north" orientation="latitude" pageId="0" pageNumber="593" precision="5" value="46.4188">46.4188º N</geoCoordinate>
