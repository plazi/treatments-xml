<number id="3DDE24EAFF99FF9FAEADFD63FBE0FD6D" box="[991,1101,741,764]" pageId="1" pageNumber="67" value="0.46499999999999997" valueMax="0.48" valueMin="0.45">0.45–0.48</number>
