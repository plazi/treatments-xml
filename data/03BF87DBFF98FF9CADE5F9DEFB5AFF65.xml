<paragraph id="8BA936CDFF99FF9FADB4FC81FAA6FCD0" blockId="1.[151,1436,151,903]" pageId="1" pageNumber="67">
<emphasis id="B962EADFFF99FF9FADB4FC81FE65FC8E" bold="true" box="[198,456,775,799]" pageId="1" pageNumber="67">
<typeStatus id="54AD886FFF99FF9FADB4FC81FF53FC8E" box="[198,254,775,799]" pageId="1" pageNumber="67">Type</typeStatus>
