<taxonomicName id="C5ADFBD9FF83FFB8FF61FEA4FC83FE23" authority="(Swartz 1788: 69) Candolle (1828: 188)" authorityName="Candolle" authorityPageNumber="188" authorityYear="1828" baseAuthorityName="Swartz" baseAuthorityPageNumber="69" baseAuthorityYear="1788" box="[185,835,374,401]" class="Magnoliopsida" family="Melastomataceae" genus="Miconia" kingdom="Plantae" order="Myrtales" pageId="18" pageNumber="79" phylum="Tracheophyta" rank="species" species="prasina">
<emphasis id="30D95C48FF83FFB8FF61FEA4FEB8FE23" bold="true" box="[185,376,374,400]" italics="true" pageId="18" pageNumber="79">Miconia prasina</emphasis>
(
<bibRefCitation id="663CFDABFF83FFB8FE5EFEA4FD81FE22" author="Swartz, O. P." box="[390,577,374,401]" pageId="18" pageNumber="79" pagination="68 - 73" refId="ref27785" refString="Swartz, O. P. (1788) Melastoma. Nova Genera et Species Plantarum seu Prodromus. Holmiae, Upsaliae &amp; Aboae, pp. 68 - 73." type="book chapter" year="1788">Swartz 1788: 69</bibRefCitation>
