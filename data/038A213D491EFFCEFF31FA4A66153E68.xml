<typeStatus id="54982E89491EFFC0FF31F9C7658739E0" box="[151,212,1648,1674]" pageId="6" pageNumber="35">Type</typeStatus>
species:
</emphasis>
<taxonomicName id="4C23EBA8491EFFC0FEE5F9C6648339E0" authority="Dietrich, 2008" authorityName="Dietrich" authorityYear="2008" box="[323,464,1649,1674]" class="Insecta" family="Membracidae" genus="Braxtonota" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="35" phylum="Arthropoda" rank="species" species="enigmata" status="n. sp.">
<emphasis id="B9574C39491EFFC0FEE5F9C6648339E0" box="[323,464,1649,1674]" italics="true" pageId="6" pageNumber="35">B. enigmata</emphasis>
