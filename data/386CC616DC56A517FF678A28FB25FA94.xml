<taxonomicName id="77C50C83DC57A514FED68F86FCE5F83B" authority="Klautau &amp; Valentine, 2003" authorityName="Klautau &amp; Valentine" authorityYear="2003" box="[294,811,1932,1954]" class="Calcarea" family="Clathrinidae" genus="Clathrina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Clathrinida" pageId="37" pageNumber="38" phylum="Porifera" rank="species" species="sinusarabica">
