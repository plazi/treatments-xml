<keyLead id="30E4959CFFBEFFB9AC984788FA4467D1" box="[151,1435,1759,1783]" pageId="2" pageNumber="594">
<number id="3DDD248EFFBEFFB9AC984788FF7B67D1" box="[151,164,1760,1783]" pageId="2" pageNumber="594" value="3.0">3</number>
