of the 
<location id="E5260B87471AFFF2FBFE7D0BC92C24B5" LSID="urn:lsid:plazi:treatment:6850EC4A471AFFF0FF707EC3C9E9203B:E5260B87471AFFF2FBFE7D0BC92C24B5" box="[1030,1131,630,652]" country="Iran" county="Azarbayejan province" latitude="38.98917" longLatPrecision="1" longitude="44.42358" municipality="Herbarium" name="University" pageId="1" pageNumber="39" stateProvince="Esfahan">University</location>
