<number box="[1412,1419,1470,1484]" pageId="3" pageNumber="566" value="3.0">3</number>
+ 
<number box="[1428,1436,1470,1484]" pageId="3" pageNumber="566" value="4.0">4</number>
with 
<number box="[208,277,1494,1518]" pageId="3" pageNumber="566" value="26.0" valueMax="30.0" valueMin="22.0">22–30</number>
