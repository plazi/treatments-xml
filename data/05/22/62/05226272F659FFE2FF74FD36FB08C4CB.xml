<number box="[778,786,1470,1484]" pageId="3" pageNumber="566" value="1.0">1</number>
with 
<number box="[851,865,1455,1479]" pageId="3" pageNumber="566" value="9.0">9</number>
setae. Cells r 
<number box="[1019,1027,1470,1484]" pageId="3" pageNumber="566" value="4.0">4</number>
+ 
<number box="[1036,1043,1470,1484]" pageId="3" pageNumber="566" value="5.0">5</number>
with 
<number box="[1109,1181,1455,1480]" pageId="3" pageNumber="566" value="76.5" valueMax="77.0" valueMin="76.0">76–77</number>
, m 
<number box="[1215,1220,1470,1484]" pageId="3" pageNumber="566" value="1.0">1</number>
+ 
