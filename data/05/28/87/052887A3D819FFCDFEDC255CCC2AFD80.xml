: 
<typeStatus box="[583,697,404,430]" pageId="4" pageNumber="47">Holotype</typeStatus>
: 
<collectingCountry box="[709,802,404,430]" name="Mexico" pageId="4" pageNumber="47">Mexico</collectingCountry>
, Sinaloa, Guerrero: ~ 
<quantity box="[1059,1113,404,430]" metricMagnitude="3" metricUnit="m" metricValue="1.609344" pageId="4" pageNumber="47" unit="mi" value="1.0">1 mi</quantity>
. (
<quantity box="[1133,1225,404,430]" metricMagnitude="3" metricUnit="m" metricValue="1.93" pageId="4" pageNumber="47" unit="m" value="1930.0">1,930m</quantity>
) W. Las Palmita[s], (Hwy. # 
<number box="[522,554,444,470]" pageId="4" pageNumber="47" value="40.0">40</number>
), [Lat. 
<number box="[665,807,444,470]" pageId="4" pageNumber="47" value="25.6166667">25.6166667</number>
, Long. ­ 
<number box="[931,1090,444,470]" pageId="4" pageNumber="47" value="108.4333333">108.4333333</number>
, Elevation 
<specimenCount box="[1248,1312,444,470]" pageId="4" pageNumber="47" type="generic">
<quantity box="[1248,1308,444,470]" metricMagnitude="1" metricUnit="m" metricValue="6.8" pageId="4" pageNumber="47" unit="m" value="68.0">68m</quantity>
.
</specimenCount>
], 
<date box="[264,392,484,510]" pageId="4" pageNumber="47" value="1977-09-13">
<number box="[264,293,484,510]" pageId="4" pageNumber="47" value="13.0">13</number>
.ix. 
<number box="[327,392,484,510]" pageId="4" pageNumber="47" value="1977.0">1977</number>
