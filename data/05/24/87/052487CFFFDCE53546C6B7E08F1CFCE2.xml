<taxonomicName authority="Bouzan &amp; Iniesta &amp; Brescovit, 2021" authorityName="Bouzan &amp; Iniesta &amp; Brescovit" authorityYear="2021" box="[1006,1100,619,645]" class="Diplopoda" family="Chelodesmidae" genus="Gangugia" kingdom="Animalia" order="Polydesmida" pageId="23" pageNumber="224" phylum="Arthropoda" rank="species" species="mula" status="n. sp.">
<emphasis box="[1006,1100,619,645]" italics="true" pageId="23" pageNumber="224">G. mula</emphasis>
</taxonomicName>
<emphasis bold="true" box="[1111,1177,619,645]" pageId="23" pageNumber="224">
<taxonomicNameLabel box="[1111,1177,619,645]" pageId="23" pageNumber="224" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
and rectangular in 
<taxonomicName authorityName="Schubart" authorityYear="1947" class="Diplopoda" family="Chelodesmidae" genus="Gangugia" kingdom="Animalia" order="Polydesmida" pageId="23" pageNumber="224" phylum="Arthropoda" rank="species" species="tapirapensis">
