<paragraph blockId="1.[151,1436,1834,2003]" pageId="1" pageNumber="446">
<emphasis bold="true" box="[199,321,1906,1931]" pageId="1" pageNumber="446">Diagnosis.</emphasis>
<taxonomicName box="[330,405,1907,1930]" class="Demospongiae" family="Irciniidae" genus="Ircinia" kingdom="Animalia" order="Dictyoceratida" pageId="1" pageNumber="446" phylum="Porifera" rank="genus">
