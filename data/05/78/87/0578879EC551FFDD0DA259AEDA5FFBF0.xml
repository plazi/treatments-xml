<collectingCountry box="[972,1040,958,981]" name="South Korea" pageId="4" pageNumber="448">Korea</collectingCountry>
and 
<collectingCountry box="[1107,1174,957,982]" name="China" pageId="4" pageNumber="448">China</collectingCountry>
(
<collectingRegion box="[1194,1345,957,982]" country="China" name="Heilongjiang" pageId="4" pageNumber="448">Heilongjiang</collectingRegion>
, 
<collectingRegion box="[1362,1432,957,982]" country="China" name="Hebei" pageId="4" pageNumber="448">Hebei</collectingRegion>
, 
<collectingRegion box="[151,244,993,1018]" country="China" name="Qinghai" pageId="4" pageNumber="448">Qinghai</collectingRegion>
, 
<collectingRegion box="[256,330,994,1018]" country="China" name="Gansu" pageId="4" pageNumber="448">Gansu</collectingRegion>
, 
<collectingRegion box="[342,435,993,1018]" country="China" name="Sichuan" pageId="4" pageNumber="448">Sichuan</collectingRegion>
, 
<collectingRegion box="[447,523,994,1017]" country="China" name="Henan" pageId="4" pageNumber="448">Henan</collectingRegion>
, 
<collectingRegion box="[535,639,993,1018]" country="China" name="Zhejiang" pageId="4" pageNumber="448">Zhejiang</collectingRegion>
,) (
<bibRefCitation author="Fang" box="[666,790,994,1018]" pageId="4" pageNumber="448" refString="Fang, C. (2000) Lepidoptera. Arctiidae. In: Fauna Sinica (Insecta). Uol. 19. Science Press, Beijing, pp. 1 - 590." type="book chapter" year="2000">Fang 2000</bibRefCitation>
