<subSubSection box="[199,1142,835,861]" pageId="11" pageNumber="534" type="etymology">
<paragraph blockId="11.[151,1437,726,1257]" box="[199,1142,835,861]" pageId="11" pageNumber="534">
<emphasis bold="true" box="[199,334,835,861]" pageId="11" pageNumber="534">Etymology.</emphasis>
