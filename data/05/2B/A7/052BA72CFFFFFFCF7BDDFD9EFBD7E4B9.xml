<collectingRegion box="[760,853,619,645]" country="China" name="Sichuan" pageId="13" pageNumber="536">Sichuan</collectingRegion>
, 
<collectingCountry box="[864,931,619,645]" name="China" pageId="13" pageNumber="536">China</collectingCountry>
(
<figureCitation box="[946,1029,619,645]" captionStart="FIGURE 19" captionStartId="18.[151,250,942,966]" captionTargetBox="[155,1431,187,914]" captionTargetId="figure-26@18.[151,1436,181,918]" captionTargetPageId="18" captionText="FIGURE 19. Distribution records of Ectatosticta spp. in this study." figureDoi="http://doi.org/10.5281/zenodo.5222632" httpUri="https://zenodo.org/record/5222632/files/figure.png" pageId="13" pageNumber="536">Fig. 19</figureCitation>
).
</paragraph>
