<caption httpUri="https://zenodo.org/record/170386/files/figure.png" pageId="5" pageNumber="52" targetBox="[333,1267,345,1684]" targetPageId="5">
<paragraph blockId="5.[264,1323,1722,1783]" pageId="5" pageNumber="52">
<emphasis bold="true" box="[264,460,1722,1746]" pageId="5" pageNumber="52">FIGURES 11–15.</emphasis>
<taxonomicName authority="Baly, 1858" authorityName="Baly" authorityYear="1858" box="[475,842,1723,1746]" class="Insecta" family="Chrysomelidae" genus="Sceloenopla" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="52" phylum="Arthropoda" rank="species" species="pretiosa">
<taxonomicName box="[475,699,1723,1746]" class="Insecta" family="Chrysomelidae" genus="Sceloenopla" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="52" phylum="Arthropoda" rank="species" species="pretiosa">
