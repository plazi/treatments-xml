), 
<emphasis bold="true" box="[983,997,366,392]" pageId="42" pageNumber="343">“</emphasis>
<location LSID="urn:lsid:plazi:treatment:056D7E4EFFFD270C659BFEB4834F9315:881B9983FFFD270C66E9FEFD853A91A5" box="[997,1058,367,393]" country="Cameroon" county="Bakossi Mountains" latitude="4.95" longLatPrecision="1306" longitude="9.65" municipality="Edib Hills" name="From" pageId="42" pageNumber="343" stateProvince="South-West">From</location>
