<taxonomicName authorityName="Kai, Ikeguchi &amp; Nakabo" authorityYear="2017" box="[867,1060,564,585]" class="Actinopterygii" family="Cyclopteridae" genus="Eumicrotremus" kingdom="Animalia" order="Scorpaeniformes" pageId="8" pageNumber="345" phylum="Chordata" rank="species" species="uenoi">Eumicrotremus uenoi</taxonomicName>
<taxonomicName authorityName="Lee and Kim" authorityYear="2017" box="[1547,1781,564,585]" class="Actinopterygii" family="Cyclopteridae" genus="Eumicrotremus" kingdom="Animalia" order="Scorpaeniformes" pageId="8" pageNumber="345" phylum="Chordata" rank="species" species="jindoensis">Eumicrotremus jindoensis</taxonomicName>
