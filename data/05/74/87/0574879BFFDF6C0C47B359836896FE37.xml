<emphasis box="[1316,1437,538,563]" italics="true" pageId="3" pageNumber="62">Nymphesgroup</emphasis>
(
<taxonomicName box="[240,351,577,602]" class="Insecta" family="Nymphidae" genus="Nymphes" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[240,351,577,602]" italics="true" pageId="3" pageNumber="62">Nymphes</emphasis>
</taxonomicName>
, 
<taxonomicName authority="Gerstaecker, 1885" authorityName="Gerstaecker" authorityYear="1885" box="[368,724,577,603]" class="Insecta" family="Nymphidae" genus="Nesydrion" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[368,493,577,602]" italics="true" pageId="3" pageNumber="62">Nesydrion</emphasis>
<bibRefCitation author="Gerstaecker" box="[501,724,577,603]" pageId="3" pageNumber="67" refString="Gerstaecker, C. E. A. (1885 a) Vier decaden von Neuropteren aus der Familie Megaloptera Burm. Mittheilungen aus dem Naturwissenschaftlichen Verein fur Neu-Vorpommern und Rugen, 16 (for 1884), 1 - 49." type="journal article" year="1885" yearSuffix="a">
Gerstaecker, 
<number box="[661,724,577,603]" pageId="3" pageNumber="62" value="1885.0">1885</number>
</bibRefCitation>
</taxonomicName>
a, 
<taxonomicName box="[752,938,577,602]" class="Insecta" family="Nymphidae" genus="Austronymphes" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[752,938,577,602]" italics="true" pageId="3" pageNumber="62">Austronymphes</emphasis>
</taxonomicName>
) and 
<emphasis box="[1009,1183,577,602]" italics="true" pageId="3" pageNumber="62">Myiodactylus-</emphasis>
group (
<taxonomicName authority="Brauer, 1866" authorityName="Brauer" authorityYear="1866" class="Insecta" family="Nymphidae" genus="Myiodactylus" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[1273,1437,577,602]" italics="true" pageId="3" pageNumber="62">Myiodactylus</emphasis>
<bibRefCitation author="Brauer" box="[151,316,616,642]" pageId="3" pageNumber="67" refString="Brauer, F. (1866) Zusatze und Berichtigungen zu Hagen's Hemerobidarum Synopsis Synonymica und Beschreibung einer neuen Nymphiden-gattung: Myiodactylus osmyloides aus Australien. Verhandlungen der Kaiserlich- Koniglichen Zoologisch-Botanischen Gesellschaft in Wien, 16, 983 - 992." type="journal article" year="1866">
Brauer, 
<number box="[251,316,616,642]" pageId="3" pageNumber="62" value="1866.0">1866</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Banks, 1913" authorityName="Banks" authorityYear="1913" box="[329,618,616,642]" class="Insecta" family="Nymphidae" genus="Osmylops" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[329,450,616,641]" italics="true" pageId="3" pageNumber="62">Osmylops</emphasis>
<bibRefCitation author="Banks" box="[459,618,616,642]" pageId="3" pageNumber="67" refString="Banks, N. (1913) Synopses and descriptions of exotic Neuroptera. Transactions of the American Entomological Society, 39, 201 - 242." type="journal article" year="1913">
Banks, 
<number box="[553,618,616,642]" pageId="3" pageNumber="62" value="1913.0">1913</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Banks, 1913" authorityName="Banks" authorityYear="1913" box="[631,965,616,642]" class="Insecta" family="Nymphidae" genus="Nymphydrion" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[631,798,616,641]" italics="true" pageId="3" pageNumber="62">Nymphydrion</emphasis>
<bibRefCitation author="Banks" box="[807,965,616,642]" pageId="3" pageNumber="67" refString="Banks, N. (1913) Synopses and descriptions of exotic Neuroptera. Transactions of the American Entomological Society, 39, 201 - 242." type="journal article" year="1913">
Banks, 
<number box="[901,965,616,642]" pageId="3" pageNumber="62" value="1913.0">1913</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Navas, 1922" authorityName="Navas" authorityYear="1922" box="[979,1262,616,642]" class="Insecta" family="Nymphidae" genus="Norfolius" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="62" phylum="Arthropoda" rank="genus">
<emphasis box="[979,1096,616,641]" italics="true" pageId="3" pageNumber="62">Norfolius</emphasis>
<bibRefCitation author="Navas" box="[1105,1262,616,642]" pageId="3" pageNumber="68" refString="Navas, L. (1922) Insectos nuevos o poco conocidos. Memorias de la Real Academia de Ciencias y Artes de Barcelona, (3), 17, 383 - 400." type="journal article" year="1922">
Navás, 
<number box="[1199,1262,616,642]" pageId="3" pageNumber="62" value="1922.0">1922</number>
</bibRefCitation>
</taxonomicName>
) (
<bibRefCitation author="New" box="[1288,1423,616,642]" pageId="3" pageNumber="68" refString="New, T. R. (1984) Intergeneric relationships in recent Nymphidae. In: Gepp, J., Aspock, H. &amp; Holzel, H. (Eds.), Progress in World's Neuropterology. Proceedings of the 1 st International Symposium on Neuropterology. Privately printed, Graz (Austria), 125 - 131." type="proceedings paper" year="1984">
