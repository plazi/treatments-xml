<typeStatus box="[151,210,639,664]" pageId="1" pageNumber="570">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Chen, 2018" authorityName="Chen" authorityYear="2018" box="[313,507,639,664]" class="Insecta" family="Taeniopterygidae" genus="Balticopteryx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="570" phylum="Arthropoda" rank="species" species="dui" status="gen. et sp. nov.">
