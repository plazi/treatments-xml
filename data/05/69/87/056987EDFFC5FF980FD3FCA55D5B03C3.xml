and 
<taxonomicName authority="Potiez &amp; Michaud, 1844" authorityName="Potiez &amp; Michaud" authorityYear="1844" box="[436,869,1381,1407]" class="Magnoliopsida" family="Malvaceae" genus="Modiola" kingdom="Animalia" order="Malvales" pageId="3" pageNumber="4" phylum="Tracheophyta" rank="species" species="favannii">
<emphasis box="[436,572,1381,1406]" italics="true" pageId="3" pageNumber="4">M. favannii</emphasis>
