<taxonomicName class="Insecta" family="Curculionidae" genus="Cryphiphorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cryphiphorus" order="Coleoptera" pageId="266" pageNumber="267" phylum="Arthropoda" rank="genus">Cryphiphorus</taxonomicName>
