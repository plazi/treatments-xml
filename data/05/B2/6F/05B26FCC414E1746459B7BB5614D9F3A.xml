<taxonomicName class="Insecta" family="Curculionidae" genus="Otiorhynchus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Otiorhynchus" order="Coleoptera" pageId="266" pageNumber="267" phylum="Arthropoda" rank="genus">Otiorhynchus</taxonomicName>
