<taxonomicName lsidName="" pageId="266" pageNumber="267" rank="subFamily" subFamily="Otiorhynchinae">Otiorhynchinae</taxonomicName>
<normalizedToken originalValue="Schönherr">Schoenherr</normalizedToken>
