<taxonomicName authority="Shasky, 1961" authorityName="Shasky" authorityYear="1961" box="[928,1302,1191,1216]" class="Gastropoda" family="Mitromorphidae" genus="Arielia" kingdom="Animalia" order="Neogastropoda" pageId="2" pageNumber="153" phylum="Mollusca" rank="species" species="mitriformis">
<emphasis box="[928,1141,1191,1216]" italics="true" pageId="2" pageNumber="153">Arielia mitriformis</emphasis>
<bibRefCitation author="Shasky" box="[1151,1302,1191,1216]" pageId="2" pageNumber="153" refString="Shasky, D. R. (1961) New deep water mollusks from the Gulf of California. The Veliger, 4, 18 - 21, pl. 4." type="journal article" year="1961">
Shasky, 
<number box="[1246,1302,1191,1216]" pageId="2" pageNumber="153" value="1961.0">1961</number>
</bibRefCitation>
</taxonomicName>
by original designation) and 
<taxonomicName authority="Shuto, 1983" authorityName="Shuto" authorityYear="1983" box="[354,628,1228,1253]" class="Gastropoda" family="Mitromorphidae" genus="Vexiariella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neogastropoda" pageId="2" pageNumber="153" phylum="Mollusca" rank="genus">
