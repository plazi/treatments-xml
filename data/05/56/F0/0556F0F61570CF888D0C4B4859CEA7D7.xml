(= 
<taxonomicName authority="Karsch, 1900" authorityName="Karsch" authorityYear="1900" class="Insecta" family="Acrididae" genus="Paracomacris" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Paracomacris" order="Orthoptera" pageId="27" pageNumber="64" phylum="Arthropoda" rank="genus">Paracomacris</taxonomicName>
) 
<taxonomicName genus="Acridina" lsidName="Acridina stenoptera" pageId="27" pageNumber="64" rank="species" species="stenoptera">stenoptera</taxonomicName>
