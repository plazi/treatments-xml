<paragraph blockId="4.[151,1437,1719,2032]" lastBlockId="5.[151,1437,151,716]" lastPageId="5" lastPageNumber="568" pageId="4" pageNumber="567">
FEMALE [
<number box="[325,341,1719,1743]" pageId="4" pageNumber="567" value="2.0">2</number>
<typeStatus box="[347,461,1720,1744]" pageId="4" pageNumber="567">paratypes</typeStatus>
