<taxonomicName class="Magnoliopsida" family="Salicaceae" genus="Populus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Populus tremuloides" order="Malpighiales" pageId="6" pageNumber="355" phylum="Tracheophyta" rank="species" species="tremuloides">Populus tremuloides</taxonomicName>
