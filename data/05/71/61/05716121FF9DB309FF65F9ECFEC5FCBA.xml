<collectingCountry box="[129,268,689,711]" name="Mozambique" pageId="16" pageNumber="73">Mozambique</collectingCountry>
and 
<collectingCountry box="[321,419,689,711]" name="Tanzania" pageId="16" pageNumber="73">Tanzania</collectingCountry>
.
