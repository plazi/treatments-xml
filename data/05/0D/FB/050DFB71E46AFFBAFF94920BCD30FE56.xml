<quantity box="[752,866,367,392]" metricMagnitude="0" metricUnit="m" metricValue="1.75" metricValueMax="3.0" metricValueMin="0.5" pageId="8" pageNumber="459" unit="m" value="1.75" valueMax="3.0" valueMin="0.5">0.5 to 3 m</quantity>
