<subSubSection id="C34219A2FFF0FFF4FF3219ABEC2CFE00" box="[197,1378,476,500]" pageId="12" pageNumber="13" type="etymology">
<paragraph id="8BE74A29FFF0FFF4FF3219ABEC2CFE00" blockId="12.[151,1437,151,536]" box="[197,1378,476,500]" pageId="12" pageNumber="13">
<emphasis id="B92C963BFFF0FFF4FF3219ABE802FE00" bold="true" box="[197,332,476,500]" pageId="12" pageNumber="13">Etymology.</emphasis>
