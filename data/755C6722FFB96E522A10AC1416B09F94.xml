<taxonomicName id="3AF5ADB7FFB96E552A10AC1413CE9CBB" box="[264,445,617,643]" class="Insecta" family="Leptoceridae" genus="Amazonatolica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="3" phylum="Arthropoda" rank="genus" status="new genus">
