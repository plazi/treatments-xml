<subSubSection id="72FAAC86FF9AB268F438FAE6FF18A056" pageId="33" pageNumber="236" type="materials_examined">
<paragraph id="3A5FFF0DFF9AB268F438FAE6FF18A056" blockId="33.[113,646,1322,1489]" pageId="33" pageNumber="236">
