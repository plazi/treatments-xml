<collectingCountry id="F338B38B8D1F7B5C0B0BFBBEB2B81155" box="[359,427,1031,1056]" name="Brazil" pageId="2" pageNumber="488">Brazil</collectingCountry>
(
<collectingRegion id="49EB3DF98D1F7B5C0BD6FBB1B1241155" box="[442,567,1032,1056]" country="Brazil" name="Amazonas" pageId="2" pageNumber="488">Amazonas</collectingRegion>
