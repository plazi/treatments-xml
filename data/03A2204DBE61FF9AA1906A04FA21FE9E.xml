<collectingCountry id="F31CD1CBBE61FF9BA31D6C7BFB5AFAEB" box="[538,610,1349,1375]" name="Brazil" pageId="4" pageNumber="41">Brazil</collectingCountry>
(Rondônia, Mato Grosso, Goiás, and Espírito Santo to Rio Grande do Sul), 
<collectingCountry id="F31CD1CBBE61FF9BA1D16C53F817FA33" box="[214,303,1389,1415]" name="Bolivia" pageId="4" pageNumber="41">Bolivia</collectingCountry>
, 
<collectingCountry id="F31CD1CBBE61FF9BA03C6C53F890FA33" box="[315,424,1389,1415]" name="Paraguay" pageId="4" pageNumber="41">Paraguay</collectingCountry>
