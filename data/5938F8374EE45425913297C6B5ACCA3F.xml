<taxonomicName id="5BD8B50942B3D9BB73B3D831BF40D32F" authority="J.Lindley, 1821" authorityName="J.Lindley" authorityYear="1821" class="Magnoliopsida" family="Rosaceae" genus="Eriobotrya" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Eriobotrya japonica" order="Rosales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="japonica">
<emphasis id="A7F776EBA13ACA0F463E67F605F7D584" italics="true" pageId="0" pageNumber="19">Eriobotrya japonica</emphasis>
</taxonomicName>
) and 
<taxonomicName id="A0A354B54CFD2E4505D2DDC032C01E3D" genus="R." lsidName="R. prinoides" pageId="0" pageNumber="19" rank="species" species="prinoides">
<emphasis id="653ACF06AFAE0A273B2DEB0B2CB77794" italics="true" pageId="0" pageNumber="19">R. prinoides</emphasis>
