<taxonomicName id="0010DCF636CF17D81763DDF348E9D8D4" class="Magnoliopsida" family="Rosaceae" genus="Rhaphiolepis" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Rhaphiolepis bibas" order="Rosales" pageId="0" pageNumber="19" phylum="Tracheophyta" rank="species" species="bibas">
