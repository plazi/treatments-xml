<taxonomicName authority="(Britton &amp; Rose) Seigler &amp; Ebinger" authorityName="Seigler &amp; Ebinger" authorityYear="2005" baseAuthorityName="Britton &amp; Rose" box="[804,1437,979,1005]" class="Magnoliopsida" family="Fabaceae" genus="Vachellia" kingdom="Plantae" order="Fabales" pageId="35" pageNumber="36" phylum="Tracheophyta" rank="species" species="vernicosa">
