<materialsCitation ID-GBIF-Occurrence="2988974430" collectingDate="1999-02-08" collectorName="T. L. Erwin " country="Ecuador" elevation="235" latitude="-3.1319444" location="Yasuni National Park" longLatPrecision="21" longitude="-76.144165" municipality="Tiputini Biodiversity Station" pageId="47" pageNumber="48" specimenCount="1" specimenCount-female="1" stateProvince="Orellana" typeStatus="holotype">
