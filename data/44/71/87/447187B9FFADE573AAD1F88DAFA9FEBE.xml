<figureCitation box="[672,772,187,213]" captionStart="FIGURE 27" captionStartId="53.[151,250,1960,1984]" captionTargetBox="[156,1431,494,1932]" captionTargetId="figure-161@53.[151,1436,489,1937]" captionTargetPageId="53" captionText="FIGURE 27. Orasema acuminata. Holotype female (UCRCENT00415025): A, habitus, lateral; B, head, anterior; C, mesosoma, lateral; D, antenna; F, mesosoma, dorsal; G, propodeum, posterior. Paratype male (UCRCENT00247959): E, antenna." figureDoi="http://doi.org/10.5281/zenodo.4297763" httpUri="https://zenodo.org/record/4297763/files/figure.png" pageId="50" pageNumber="51">Figs 27F</figureCitation>
, 
<figureCitation box="[783,835,187,213]" captionStart="FIGURE 28" captionStartId="54.[151,250,1630,1654]" captionTargetBox="[156,1431,171,1609]" captionTargetId="figure-22@54.[151,1436,166,1614]" captionTargetPageId="54" captionText="FIGURE 28. Orasema cerulea. Holotype female (UCRCENT00242604): A, habitus, lateral; B, head, anterior; C, mesosoma, lateral; D, antenna; E, acrosternite, anterior; F, ovipositor, lateral; G, mesosoma, dorsal; H, propodeum, posterior." figureDoi="http://doi.org/10.5281/zenodo.4297765" httpUri="https://zenodo.org/record/4297765/files/figure.png" pageId="50" pageNumber="51">28G</figureCitation>
), eyes bare (
<figureCitation box="[973,1078,187,213]" captionStart="FIGURE 27" captionStartId="53.[151,250,1960,1984]" captionTargetBox="[156,1431,494,1932]" captionTargetId="figure-161@53.[151,1436,489,1937]" captionTargetPageId="53" captionText="FIGURE 27. Orasema acuminata. Holotype female (UCRCENT00415025): A, habitus, lateral; B, head, anterior; C, mesosoma, lateral; D, antenna; F, mesosoma, dorsal; G, propodeum, posterior. Paratype male (UCRCENT00247959): E, antenna." figureDoi="http://doi.org/10.5281/zenodo.4297763" httpUri="https://zenodo.org/record/4297763/files/figure.png" pageId="50" pageNumber="51">Figs 27B</figureCitation>
, 
<figureCitation box="[1089,1139,187,213]" captionStart="FIGURE 28" captionStartId="54.[151,250,1630,1654]" captionTargetBox="[156,1431,171,1609]" captionTargetId="figure-22@54.[151,1436,166,1614]" captionTargetPageId="54" captionText="FIGURE 28. Orasema cerulea. Holotype female (UCRCENT00242604): A, habitus, lateral; B, head, anterior; C, mesosoma, lateral; D, antenna; E, acrosternite, anterior; F, ovipositor, lateral; G, mesosoma, dorsal; H, propodeum, posterior." figureDoi="http://doi.org/10.5281/zenodo.4297765" httpUri="https://zenodo.org/record/4297765/files/figure.png" pageId="50" pageNumber="51">28B</figureCitation>
), antecostal sulcus smooth (
<figureCitation box="[160,261,223,249]" captionStart="FIGURE 28" captionStartId="54.[151,250,1630,1654]" captionTargetBox="[156,1431,171,1609]" captionTargetId="figure-22@54.[151,1436,166,1614]" captionTargetPageId="54" captionText="FIGURE 28. Orasema cerulea. Holotype female (UCRCENT00242604): A, habitus, lateral; B, head, anterior; C, mesosoma, lateral; D, antenna; E, acrosternite, anterior; F, ovipositor, lateral; G, mesosoma, dorsal; H, propodeum, posterior." figureDoi="http://doi.org/10.5281/zenodo.4297765" httpUri="https://zenodo.org/record/4297765/files/figure.png" pageId="50" pageNumber="51">Fig. 28E</figureCitation>
