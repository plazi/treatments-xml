<taxonomicName authorityName="Espinoza et al." authorityYear="2013" baseAuthorityName="LeConte" baseAuthorityYear="1855" box="[615,769,1739,1764]" class="Insecta" family="Reduviidae" genus="Triatoma" kingdom="Animalia" order="Hemiptera" pageId="11" pageNumber="346" phylum="Arthropoda" rank="species" species="sanguisuga">
