<taxonomicName authority="Lederer" authorityName="Lederer" box="[1150,1436,1517,1542]" class="Insecta" family="Noctuidae" genus="Sesamia" kingdom="Animalia" order="Lepidoptera" pageId="8" phylum="Arthropoda" rank="species" species="cretica">
<emphasis box="[1150,1335,1518,1541]" italics="true" pageId="8">Sesamia cretica</emphasis>
Lederer
</taxonomicName>
(
<taxonomicName box="[158,300,1553,1578]" class="Insecta" kingdom="Animalia" order="Lepidoptera" pageId="8" phylum="Arthropoda" rank="order">Lepidoptera</taxonomicName>
: 
<taxonomicName box="[314,434,1553,1578]" class="Insecta" family="Noctuidae" kingdom="Animalia" order="Lepidoptera" pageId="8" phylum="Arthropoda" rank="family">Noctuidae</taxonomicName>
