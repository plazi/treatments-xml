locality: Ria 
<collectingCountry box="[930,1031,1225,1248]" name="Taiwan" pageId="9" pageNumber="586">Formosa</collectingCountry>
, 
<number box="[1046,1074,1224,1249]" pageId="9" pageNumber="586" value="36.0">36</number>
° 
<geoCoordinate box="[1083,1192,1224,1249]" direction="north" orientation="latitude" pageId="9" pageNumber="586" precision="55" value="59.961">
<number box="[1083,1162,1224,1249]" pageId="9" pageNumber="586" value="59.961">59.961</number>
N
</geoCoordinate>
