° 
<geoCoordinate box="[648,746,1296,1320]" direction="north" orientation="latitude" pageId="9" pageNumber="586" precision="55" value="0.317">
<number box="[648,714,1296,1320]" pageId="9" pageNumber="586" value="0.317">0.317</number>
N
</geoCoordinate>
, 
<number box="[759,772,1295,1320]" pageId="9" pageNumber="586" value="7.0">7</number>
° 
<geoCoordinate box="[781,898,1295,1320]" direction="west" orientation="longitude" pageId="9" pageNumber="586" precision="55" value="-58.08">
<number box="[781,862,1295,1320]" pageId="9" pageNumber="586" value="58.08">58.080</number>
W
