<number box="[1208,1221,1224,1249]" pageId="9" pageNumber="586" value="7.0">7</number>
° 
<geoCoordinate box="[1229,1348,1224,1249]" direction="west" orientation="longitude" pageId="9" pageNumber="586" precision="55" value="-54.964">
