, 
<number box="[487,500,2013,2038]" pageId="4" pageNumber="581" value="7.0">7</number>
° 
<geoCoordinate box="[509,626,2013,2038]" direction="west" orientation="longitude" pageId="4" pageNumber="581" precision="55" value="-48.674">
<number box="[509,590,2013,2038]" pageId="4" pageNumber="581" value="48.674">48.674</number>
