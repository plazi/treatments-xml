<taxonomicName box="[794,914,1016,1040]" class="Magnoliopsida" family="Asteraceae" kingdom="Plantae" order="Asterales" pageId="9" pageNumber="86" phylum="Tracheophyta" rank="family">Asteraceae</taxonomicName>
, 
<taxonomicName box="[923,1041,1015,1039]" class="Magnoliopsida" family="Lamiaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="9" pageNumber="86" phylum="Magnoliopsida" rank="family">Lamiaceae</taxonomicName>
