antenna; 
<emphasis bold="true" box="[151,188,1889,1914]" pageId="2" pageNumber="227">DA</emphasis>
body derm appearance; 
<emphasis bold="true" box="[436,496,1889,1913]" pageId="2" pageNumber="227">DMD</emphasis>
dorsal microduct; 
<emphasis bold="true" box="[687,719,1889,1914]" pageId="2" pageNumber="227">DS</emphasis>
dorsal seta; 
<emphasis bold="true" box="[846,899,1889,1913]" pageId="2" pageNumber="227">DTD</emphasis>
dorsal tubular duct; 
