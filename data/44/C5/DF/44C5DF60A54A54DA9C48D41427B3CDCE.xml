<subSubSection pageId="0" pageNumber="1" type="new records">
<paragraph pageId="0" pageNumber="1">New records.</paragraph>
<paragraph pageId="0" pageNumber="1">
Israel • 6 spcms; Ashqelon; 
<geoCoordinate degrees="31.7002" direction="north" orientation="latitude" precision="5" value="31.7002">31.7002°N</geoCoordinate>
, 
<geoCoordinate degrees="34.5498" direction="east" orientation="longitude" precision="5" value="34.5498">34.5498°E</geoCoordinate>
