<subSubSection pageId="18" pageNumber="101" type="nomenclature">
<paragraph pageId="18" pageNumber="101">
<taxonomicName LSID="http://zoobank.org/A19A757A-ED28-4613-B2C0-500F3A6068BF" class="Insecta" family="Melolonthidae" genus="Tetraserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetraserica xichouensis" order="Coleoptera" pageId="18" pageNumber="101" phylum="Arthropoda" rank="species" species="xichouensis">Tetraserica xichouensis</taxonomicName>
