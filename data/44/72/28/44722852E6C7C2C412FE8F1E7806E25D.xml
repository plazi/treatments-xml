<taxonomicName class="Insecta" family="Cerambycidae" genus="Aegoprepes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Aegoprepes" order="Coleoptera" pageId="212" pageNumber="213" phylum="Arthropoda" rank="genus">Aegoprepes</taxonomicName>
