(=. 
<taxonomicName authority="Ehara &amp; Moraes, 1998: 59" authorityName="Ehara &amp; Moraes" authorityPageNumber="59" authorityYear="1998" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="inouei" subGenus="Euseius">
