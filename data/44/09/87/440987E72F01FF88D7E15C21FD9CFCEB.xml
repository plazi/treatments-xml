<emphasis box="[160,247,655,678]" italics="true" pageId="21" pageNumber="322">Euseius</emphasis>
) 
<emphasis box="[264,334,655,678]" italics="true" pageId="21" pageNumber="322">inouei</emphasis>
Ehara &amp; Moraes, 
<number box="[542,602,655,679]" pageId="21" pageNumber="322" value="1998.0">1998</number>
: 
<number box="[613,644,654,679]" pageId="21" pageNumber="322" value="59.0">59</number>
</taxonomicName>
), which has been collected from 
<collectingCountry box="[1017,1085,654,679]" name="Brazil" pageId="21" pageNumber="322">Brazil</collectingCountry>
(Moraes 
<emphasis box="[1194,1251,654,679]" italics="true" pageId="21" pageNumber="322">et al.</emphasis>
, 
<number box="[1265,1325,655,679]" pageId="21" pageNumber="322" value="2004.0">2004</number>
a; Furtado 
<emphasis box="[151,207,690,715]" italics="true" pageId="21" pageNumber="322">et al.</emphasis>
, 
<number box="[221,280,690,715]" pageId="21" pageNumber="322" value="2006.0">2006</number>
; Castro &amp; Moraes, 
<number box="[500,559,691,715]" pageId="21" pageNumber="322" value="2010.0">2010</number>
; Cruz 
<emphasis box="[633,690,690,715]" italics="true" pageId="21" pageNumber="322">et al.</emphasis>
, 
<number box="[704,764,691,715]" pageId="21" pageNumber="322" value="2012.0">2012</number>
), 
<collectingCountry box="[784,882,691,715]" name="Uruguay" pageId="21" pageNumber="322">Uruguay</collectingCountry>
(Moraes 
<emphasis box="[989,1046,690,715]" italics="true" pageId="21" pageNumber="322">et al.</emphasis>
, 
<number box="[1060,1118,691,715]" pageId="21" pageNumber="322" value="2004.0">2004</number>
a) and 
<collectingCountry box="[1193,1305,690,715]" name="Argentina" pageId="21" pageNumber="322">Argentina</collectingCountry>
(Furtado 
<emphasis italics="true" pageId="21" pageNumber="322">et al.</emphasis>
, 
<number box="[197,258,727,751]" pageId="21" pageNumber="322" value="2007.0">2007</number>
