<taxonomicName authority="Ehara &amp; Moraes" authorityName="Ehara &amp; Moraes" box="[937,1258,618,643]" class="Arachnida" family="Phytoseiidae" genus="Euseius" kingdom="Animalia" order="Mesostigmata" pageId="21" pageNumber="322" phylum="Arthropoda" rank="species" species="inouei">
