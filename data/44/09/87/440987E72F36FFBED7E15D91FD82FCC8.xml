</taxonomicName>
<collectingCountry box="[1081,1185,655,680]" name="Denmark" pageId="35" pageNumber="336">Denmark</collectingCountry>
&amp; Andrews, 
<number box="[1336,1392,656,680]" pageId="35" pageNumber="336" value="1981.0">1981</number>
but the latter has shorter 
<emphasis box="[383,408,692,716]" italics="true" pageId="35" pageNumber="336">
z 
<number box="[395,408,692,716]" pageId="35" pageNumber="336" value="2.0">2</number>
</emphasis>
(
<number box="[423,452,692,716]" pageId="35" pageNumber="336" value="18.0">18</number>
) and longer 
<emphasis box="[592,614,693,716]" italics="true" pageId="35" pageNumber="336">
j 
<number box="[602,614,693,716]" pageId="35" pageNumber="336" value="5.0">5</number>
</emphasis>
, 
<emphasis box="[627,649,692,717]" italics="true" pageId="35" pageNumber="336">
j 
<number box="[637,649,692,717]" pageId="35" pageNumber="336" value="6.0">6</number>
</emphasis>
, 
<emphasis box="[662,688,692,716]" italics="true" pageId="35" pageNumber="336">
J 
<number box="[676,688,692,716]" pageId="35" pageNumber="336" value="2.0">2</number>
</emphasis>
and 
<emphasis box="[742,767,692,716]" italics="true" pageId="35" pageNumber="336">
s 
<number box="[753,767,692,716]" pageId="35" pageNumber="336" value="4.0">4</number>
</emphasis>
(
<number box="[781,854,692,717]" pageId="35" pageNumber="336" value="52.63">52, 63</number>
, 
<number box="[865,893,692,717]" pageId="35" pageNumber="336" value="60.0">60</number>
