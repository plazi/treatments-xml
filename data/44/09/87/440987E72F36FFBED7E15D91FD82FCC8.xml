<emphasis box="[160,203,295,319]" italics="true" pageId="34" pageNumber="335">
JV 
<number box="[193,203,295,319]" pageId="34" pageNumber="335" value="1.0">1</number>
</emphasis>
, 
<emphasis box="[219,263,295,318]" italics="true" pageId="34" pageNumber="335">
JV 
