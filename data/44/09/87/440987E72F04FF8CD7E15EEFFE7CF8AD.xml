<emphasis box="[942,967,1809,1833]" italics="true" pageId="17" pageNumber="318">
z 
<number box="[953,967,1809,1833]" pageId="17" pageNumber="318" value="4.0">4</number>
</emphasis>
, 
<emphasis box="[983,1013,1809,1833]" italics="true" pageId="17" pageNumber="318">
Z 
<number box="[1000,1013,1809,1833]" pageId="17" pageNumber="318" value="4.0">4</number>
</emphasis>
and 
<emphasis box="[1070,1100,1809,1832]" italics="true" pageId="17" pageNumber="318">
Z 
<number box="[1087,1100,1809,1832]" pageId="17" pageNumber="318" value="5.0">5</number>
</emphasis>
(
<number box="[1117,1190,1809,1833]" pageId="17" pageNumber="318" value="20.39">20, 39</number>
and 
<number box="[1248,1276,1808,1833]" pageId="17" pageNumber="318" value="59.0">59</number>
respectively). 
<taxonomicName box="[151,403,1844,1869]" class="Arachnida" family="Phytoseiidae" genus="Amblydromalus" kingdom="Animalia" order="Mesostigmata" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="laetus">
<emphasis box="[151,403,1844,1869]" italics="true" pageId="17" pageNumber="318">Amblydromalus laetus</emphasis>
</taxonomicName>
