<number box="[151,207,1772,1797]" pageId="17" pageNumber="318" value="1975.0">1975</number>
and 
<taxonomicName box="[267,367,1772,1797]" class="Arachnida" family="Phytoseiidae" genus="Amblydromalus" kingdom="Animalia" order="Mesostigmata" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="laetus">
