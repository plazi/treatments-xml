. 
<emphasis box="[300,367,1772,1797]" italics="true" pageId="17" pageNumber="318">laetus</emphasis>
</taxonomicName>
. 
<taxonomicName box="[384,684,1772,1797]" class="Arachnida" family="Phytoseiidae" genus="Amblydromalus" kingdom="Animalia" order="Mesostigmata" pageId="17" pageNumber="318" phylum="Arthropoda" rank="species" species="higuilloae">
<emphasis box="[384,684,1772,1797]" italics="true" pageId="17" pageNumber="318">Amblydromalus higuilloae</emphasis>
</taxonomicName>
