(
<number box="[410,483,548,572]" pageId="13" pageNumber="314" value="37.5" valueMax="41.0" valueMin="34.0">34–41</number>
) and 
<number box="[553,581,548,573]" pageId="13" pageNumber="314" value="16.0">16</number>
(
<number box="[600,672,548,572]" pageId="13" pageNumber="314" value="15.5" valueMax="19.0" valueMin="12.0">12–19</number>
) respectively] and 
<emphasis box="[899,928,548,572]" italics="true" pageId="13" pageNumber="314">
Z 
<number box="[915,928,548,572]" pageId="13" pageNumber="314" value="4.0">4</number>
</emphasis>
and 
<emphasis box="[990,1020,549,572]" italics="true" pageId="13" pageNumber="314">
Z 
<number box="[1007,1020,549,572]" pageId="13" pageNumber="314" value="5.0">5</number>
</emphasis>
shorter[
<number box="[1118,1146,548,573]" pageId="13" pageNumber="314" value="90.0">90</number>
(
<number box="[1165,1238,548,572]" pageId="13" pageNumber="314" value="88.0" valueMax="94.0" valueMin="82.0">82–94</number>
) and 
<number box="[1308,1336,548,572]" pageId="13" pageNumber="314" value="84.0">84</number>
(
<number box="[1355,1428,548,572]" pageId="13" pageNumber="314" value="85.0" valueMax="91.0" valueMin="79.0">79–91</number>
