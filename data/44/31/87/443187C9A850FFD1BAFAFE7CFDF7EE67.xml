<paragraph blockId="22.[151,1437,1038,1639]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[151,304,1038,1063]" pageId="22" pageNumber="23">Distribution.</emphasis>
<collectingCountry box="[349,456,1038,1063]" name="Denmark" pageId="22" pageNumber="23">Denmark</collectingCountry>
, 
<collectingCountry box="[506,593,1038,1063]" name="Estonia" pageId="22" pageNumber="23">Estonia</collectingCountry>
, 
<collectingCountry box="[643,731,1038,1063]" name="Finland" pageId="22" pageNumber="23">Finland</collectingCountry>
, 
