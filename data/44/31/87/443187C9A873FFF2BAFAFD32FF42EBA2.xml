<taxonomicName authority="Latreille, 1802" authorityName="Latreille" authorityYear="1802" box="[653,830,833,858]" class="Insecta" family="Chrysididae" genus="Cleptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="53" pageNumber="54" phylum="Arthropoda" rank="species" species="auratus">
