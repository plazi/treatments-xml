<number box="[751,868,332,357]" pageId="3" pageNumber="100" value="2.0140513E7">20140513</number>
- 
<number box="[876,903,332,357]" pageId="3" pageNumber="100" value="93.0">93</number>
. 
<typeStatus box="[914,1027,333,357]" pageId="3" pageNumber="100">Paratypes</typeStatus>
: four males on slides NJ- 
<number box="[1315,1429,332,357]" pageId="3" pageNumber="100" value="2.0140513E7">20140513</number>
- 
<number box="[151,181,368,392]" pageId="3" pageNumber="100" value="23.0">23</number>
, - 
<number box="[203,232,368,392]" pageId="3" pageNumber="100" value="31.0">31</number>
and - 
<number box="[297,328,368,392]" pageId="3" pageNumber="100" value="92.0">92</number>
, and two females on slides NJ- 
<number box="[686,802,367,392]" pageId="3" pageNumber="100" value="2.0140513E7">20140513</number>
- 
<number box="[810,835,367,392]" pageId="3" pageNumber="100" value="31.0">31</number>
and - 
<number box="[901,932,368,392]" pageId="3" pageNumber="100" value="73.0">73</number>
, and one juvenile on slide NJ- 
<number box="[1281,1398,367,392]" pageId="3" pageNumber="100" value="2.0140513E7">20140513</number>
- 
<number box="[1405,1433,367,392]" pageId="3" pageNumber="100" value="92.0">92</number>
. All 
<typeStatus box="[194,242,405,429]" pageId="3" pageNumber="100">type</typeStatus>
