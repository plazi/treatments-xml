<subSubSection pageId="86" pageNumber="87" type="nomenclature">
<paragraph pageId="86" pageNumber="87">
99. 
<taxonomicName LSID="urn:lsid:zoobank.org:act:8ACAA7C2-667F-47CE-BC45-1002891F195B" authority="Riedel" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus wamenaensis" order="Coleoptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="wamenaensis">Trigonopterus wamenaensis Riedel</taxonomicName>
