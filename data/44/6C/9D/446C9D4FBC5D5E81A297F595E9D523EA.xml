♂ China, Yunnan, Menglun, XTBG, 
<taxonomicName authorityName="Hu" authorityYear="1940" baseAuthorityName="Pierre" class="Magnoliopsida" family="Magnoliaceae" genus="Paramichelia" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Paramichelia baillonii" order="Magnoliales" pageId="0" pageNumber="43" phylum="Tracheophyta" rank="species" species="baillonii">
<emphasis italics="true" pageId="0" pageNumber="43">Paramichelia baillonii</emphasis>
</taxonomicName>
plantation; 
<geoCoordinate degrees="21" direction="north" minutes="53.823" orientation="latitude" precision="1" value="21.89705">21°53.823'N</geoCoordinate>
, 
<geoCoordinate degrees="101" direction="east" minutes="17.072" orientation="longitude" precision="1" value="101.28453">101°17.072'E</geoCoordinate>
