(
<taxonomicName authorityName="Leach" authorityYear="1815" box="[1063,1162,1568,1590]" class="Insecta" family="Tiphiidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="587" phylum="Arthropoda" rank="family">Tiphiidae</taxonomicName>
