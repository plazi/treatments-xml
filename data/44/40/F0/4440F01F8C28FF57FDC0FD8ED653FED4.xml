</paragraph>
<paragraph blockId="8.[189,1399,313,622]" box="[234,723,313,340]" pageId="8" pageNumber="9">
<location LSID="urn:lsid:plazi:treatment:4440F01F8C28FF57FDC0FD8ED653FED4:C93617D28C27FF56FF68FEE0D40DFED4" box="[234,298,314,340]" country="Chile" county="Elqui" municipality="Los Choros" name="Male" pageId="8" pageNumber="9">
