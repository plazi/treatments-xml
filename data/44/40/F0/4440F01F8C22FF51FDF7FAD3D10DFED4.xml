<subSubSection pageId="13" pageNumber="14" type="diagnosis">
<paragraph blockId="13.[189,310,1425,1451]" box="[189,310,1425,1451]" pageId="13" pageNumber="14">
<heading bold="true" box="[189,310,1425,1451]" fontSize="11" level="3" pageId="13" pageNumber="14" reason="3">
