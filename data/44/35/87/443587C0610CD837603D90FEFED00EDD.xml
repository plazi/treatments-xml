<subSubSection pageId="4" pageNumber="5" type="etymology">
<paragraph blockId="4.[264,1323,1526,1873]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[312,452,1646,1672]" pageId="4" pageNumber="5">Etymology.</emphasis>
