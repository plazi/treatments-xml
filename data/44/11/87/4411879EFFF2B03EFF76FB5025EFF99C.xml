<quantity box="[355,494,1513,1535]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" metricValueMax="2.6" metricValueMin="2.2" pageId="5" pageNumber="214" unit="mm" value="2.4" valueMax="2.6" valueMin="2.2">2.20–2.60 mm</quantity>
