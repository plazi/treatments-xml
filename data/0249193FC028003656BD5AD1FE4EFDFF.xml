<taxonomicName id="4DE0D3AAC028003556BD5AD1FDE5FA74" authority="(HORVATH, 1897)" authorityName="" baseAuthorityName="HORVATH" baseAuthorityYear="1897" box="[178,570,1484,1508]" class="Insecta" family="Cicadellidae" genus="Synophropsis" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="1238" phylum="Arthropoda" rank="species" species="lauri">
