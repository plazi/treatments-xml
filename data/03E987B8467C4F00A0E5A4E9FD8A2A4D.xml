(
<bibRefCitation id="EFD14B5F46414F00A372A6A8FC7A2A71" author="Li, J. &amp; Jin, D. - C. &amp; Yi, T. - C." box="[768,925,1269,1296]" pageId="32" pageNumber="247" pagination="1495 - 1520" refId="ref12142" refString="Li, J., Jin, D. - C. &amp; Yi, T. - C. (2017) Ontogenetic development and redescription of Oligonychus metasequoiae (Acari: Tetranychidae). Systematic and Applied Acarology, 22 (9), 1495 - 1520. https: // doi. org / 10.11158 / saa. 22.9.14" type="journal article" year="2017">
Li 
<emphasis id="B934EABC46414F00A350A6ABFCB32A71" box="[802,852,1269,1295]" italics="true" pageId="32" pageNumber="247">et al</emphasis>
. 2017
</bibRefCitation>
, 2018; 
<bibRefCitation id="EFD14B5F46414F00A382A6A8FB3D2A71" author="Khanjani, M. &amp; Khanjani, M. &amp; Seeman, O. D." box="[1008,1242,1269,1296]" pageId="32" pageNumber="247" pagination="223 - 288" refId="ref12081" refString="Khanjani, M., Khanjani, M. &amp; Seeman, O. D. (2018) The spider mites of the genus Oligonychus Berlese (Acari: Tetranychidae) from Iran. Systematic and Applied Acarology, 23 (2), 223 - 288. https: // doi. org / 10.11158 / saa. 23.2.4" type="journal article" year="2018">
Khanjani 
<emphasis id="B934EABC46414F00A42CA6ABFB772A71" box="[1118,1168,1269,1295]" italics="true" pageId="32" pageNumber="247">et al</emphasis>
. 2018
</bibRefCitation>
), and identical to some 
<taxonomicName id="4C404D2D46414F00A0ABA744FD8F2A4D" authority="(Khanjani et al. 2018)" baseAuthorityName="Khanjani" baseAuthorityYear="2018" box="[217,616,1305,1331]" class="Arachnida" family="Tetranychidae" genus="Oligonychus" kingdom="Animalia" order="Prostigmata" pageId="32" pageNumber="247" phylum="Arthropoda" rank="genus">
