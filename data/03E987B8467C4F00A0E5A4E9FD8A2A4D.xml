</bibRefCitation>
) and 
<taxonomicName id="4C404D2D46414F00A211A6A8FD162A71" box="[611,753,1269,1295]" class="Arachnida" family="Tetranychidae" genus="Oligonychus" kingdom="Animalia" order="Prostigmata" pageId="32" pageNumber="247" phylum="Arthropoda" rank="genus">
<emphasis id="B934EABC46414F00A211A6A8FD162A71" box="[611,753,1269,1295]" italics="true" pageId="32" pageNumber="247">Oligonychus</emphasis>
