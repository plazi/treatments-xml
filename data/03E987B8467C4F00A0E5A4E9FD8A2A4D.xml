<taxonomicName id="4C404D2D467F4F3EA154A111FDFF2C1A" authorityName=", Pritchard &amp; Baker" authorityYear="1952" baseAuthorityName=", Pritchard &amp; Baker" baseAuthorityYear="1952" box="[294,536,844,868]" class="Arachnida" family="Tetranychidae" genus="Tetranychus" kingdom="Animalia" order="Prostigmata" pageId="30" pageNumber="245" phylum="Arthropoda" rank="species" species="canadensis">
