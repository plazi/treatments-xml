; 4 – 
<taxonomicName id="4C3E3C907051FFD5FD0DABF9FC19CAE7" authority="Lindqvist, 1974" authorityName="Lindqvist" authorityYear="1974" box="[760,997,1510,1529]" class="Insecta" family="Tenthredinidae" genus="Pristiphora" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="270" phylum="Arthropoda" rank="species" species="angulata">
