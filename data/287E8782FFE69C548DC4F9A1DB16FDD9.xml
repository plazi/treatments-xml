<paragraph id="A0683694FFE69C568DF1F9E9D89DF9F8" blockId="0.[136,1452,1638,1905]" box="[189,666,1638,1662]" pageId="0" pageNumber="85">
<typeStatus id="7F6C8836FFE69C568DF1F9E9DAF5F9F8" box="[189,242,1638,1662]" pageId="0" pageNumber="85">Type</typeStatus>
species: 
<taxonomicName id="67D74D17FFE69C568C18F9E9D83AF9F8" authority="Liu, 2021" authorityName="Liu" authorityYear="2021" box="[340,573,1638,1662]" class="Insecta" family="Phoridae" genus="Sacculiphora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="85" phylum="Arthropoda" rank="species" species="cultrata" status="sp. nov.">
