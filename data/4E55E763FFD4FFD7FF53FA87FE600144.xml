<number id="70344452FFD2FFD6FED1F9C8FEBE0705" box="[277,331,1787,1811]" pageId="4" pageNumber="5" value="12.95" valueMax="16.9" valueMin="9.0">9–16</number>
</date>
</emphasis>
. 9– 
<number id="70344452FFD2FFD6FEB3F9CFFE670705" box="[375,402,1788,1811]" pageId="4" pageNumber="5" value="14.0">14</number>
, 
<taxonomicName id="01FC2DF6FFD2FFD6FE5AF9CFFD630705" box="[414,662,1788,1811]" class="Insecta" family="Vespidae" genus="Eustenogaster" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="fraterna">
<emphasis id="F4888A67FFD2FFD6FE5AF9CFFD630705" box="[414,662,1788,1811]" italics="true" pageId="4" pageNumber="5">Eustenogaster fraterna</emphasis>
</taxonomicName>
. 
<number id="70344452FFD2FFD6FD61F9CFFD1F0705" box="[677,746,1788,1811]" pageId="4" pageNumber="5" value="15.5" valueMax="16.0" valueMin="15.0">15–16</number>
, 
<taxonomicName id="01FC2DF6FFD2FFD6FD33F9CFFCA50705" box="[759,848,1788,1811]" class="Insecta" family="Vespidae" genus="Eustenogaster" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="nigra">
