<number id="70344452FFD2FFD6FB45F82DFB6E0723" box="[1153,1179,1822,1845]" pageId="4" pageNumber="5" value="11.0">11</number>
and 
<number id="70344452FFD2FFD6FB0BF82DFB190723" box="[1231,1260,1822,1845]" pageId="4" pageNumber="5" value="14.0">14</number>
