<taxonomicName id="39C6B6A3FF9BFFC1FB9E77AEDCEEFD22" authorityName="Assing" authorityYear="2016" box="[1045,1174,649,673]" class="Insecta" family="Staphylinidae" genus="Orphnebius" kingdom="Animalia" order="Coleoptera" pageId="42" pageNumber="55" phylum="Arthropoda" rank="species" species="carinatus">
<emphasis id="CCB21132FF9BFFC1FB9E77AEDCEEFD22" box="[1045,1174,649,673]" italics="true" pageId="42" pageNumber="55">O. carinatus</emphasis>
