<collectingRegion id="387BF84E20600C6A5703FD65FD17FD48" box="[547,696,718,740]" country="South Africa" name="Western Cape" pageId="1" pageNumber="838">Western Cape</collectingRegion>
, Vanrhynspass (östlich der Basis), 
<geoCoordinate id="9F8B506B20600C6A55B7FD41FF44FCAD" box="[151,235,747,769]" degrees="31" direction="south" minutes="19" orientation="latitude" pageId="1" pageNumber="838" precision="925" value="-31.316668">31°19’S</geoCoordinate>
- 
<geoCoordinate id="9F8B506B20600C6A5420FD41FEF5FCAD" box="[256,346,747,769]" degrees="19" direction="west" minutes="01" orientation="longitude" pageId="1" pageNumber="838" precision="925" value="-19.016666">19°01’O</geoCoordinate>
