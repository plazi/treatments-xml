<taxonomicName id="4C326DC91A7CFFE3D34F0DF8FDD4FB2C" authority="Schaum, 1953" authorityName="Schaum" authorityYear="1953" box="[278,583,1169,1194]" class="Insecta" family="Acrididae" genus="Catantops" kingdom="Animalia" order="Orthoptera" pageId="34" pageNumber="345" phylum="Arthropoda" rank="genus">
<emphasis id="B946CA581A7CFFE3D34F0DF8FE39FB2E" bold="true" box="[278,426,1169,1192]" italics="true" pageId="34" pageNumber="345">CATANTOPS</emphasis>
