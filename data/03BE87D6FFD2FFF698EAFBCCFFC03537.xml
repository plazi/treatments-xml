<quantity id="4CEF9B25FFD2FFF69CCBFB00FF60350B" box="[1206,1313,1266,1294]" metricMagnitude="3" metricUnit="m" metricValue="3.819144" pageId="67" pageNumber="68" unit="ft" value="12530.0">12,530 ft</quantity>
