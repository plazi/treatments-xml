<quantity id="4CEF9B25FFD2FFF69C3BFB01FEE43508" box="[1094,1189,1267,1293]" metricMagnitude="3" metricUnit="m" metricValue="3.82" pageId="67" pageNumber="68" unit="m" value="3820.0">3,820 m</quantity>
