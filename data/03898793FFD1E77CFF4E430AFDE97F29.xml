<typeStatus id="549B8827FFDFE77CFE1B4107FDD27D75" box="[450,554,1102,1126]" pageId="9" pageNumber="10">Holotype</typeStatus>
female, two 
<typeStatus id="549B8827FFDFE77CFD104106FCD17D74" box="[713,809,1103,1127]" pageId="9" pageNumber="10">paratype</typeStatus>
females, two 
<typeStatus id="549B8827FFDFE77CFC0A4106FBCC7D74" box="[979,1076,1103,1127]" pageId="9" pageNumber="10">paratype</typeStatus>
males, leaf litter, native shrub vegetation, McMinns Lagoon, near Darwin, Northern Territory, 
<geoCoordinate id="EE145042FFDFE77CFC5C413BFC277D99" box="[901,991,1138,1162]" direction="south" orientation="latitude" pageId="9" pageNumber="10" precision="925" value="-12.55">
<number id="3DE824A2FFDFE77CFC5C413BFC5B7D99" box="[901,931,1138,1162]" pageId="9" pageNumber="10" value="12.0">12</number>
° 
<number id="3DE824A2FFDFE77CFC74413BFC347D99" box="[941,972,1138,1162]" pageId="9" pageNumber="10" value="33.0">33</number>
'S
</geoCoordinate>
, 
<geoCoordinate id="EE145042FFDFE77CFC29413BFBA17D99" box="[1008,1113,1138,1162]" direction="east" orientation="longitude" pageId="9" pageNumber="10" precision="925" value="131.06667">
<number id="3DE824A2FFDFE77CFC29413BFBE27D99" box="[1008,1050,1138,1162]" pageId="9" pageNumber="10" value="131.0">131</number>
°04'E
</geoCoordinate>
, ca. 
<specimenCount id="9D26FD0CFFDFE77CFB4C413BFB237D99" box="[1173,1243,1138,1162]" pageId="9" pageNumber="10" type="generic">
<quantity id="4CD89B60FFDFE77CFB4C413BFB2F7D99" box="[1173,1239,1138,1162]" metricMagnitude="1" metricUnit="m" metricValue="3.5" pageId="9" pageNumber="10" unit="m" value="35.0">35 m</quantity>
.
</specimenCount>
, coll. I.A. Reid, 
<date id="FF9E1045FFDFE77CFF4E41DFFEFC7DBD" box="[151,260,1174,1198]" pageId="9" pageNumber="10" value="1995-01-21">
<number id="3DE824A2FFDFE77CFF4E41DFFF4D7DBD" box="[151,181,1174,1198]" pageId="9" pageNumber="10" value="21.0">21</number>
.i. 
<number id="3DE824A2FFDFE77CFF1C41DFFEFC7DBD" box="[197,260,1174,1198]" pageId="9" pageNumber="10" value="1995.0">1995</number>
