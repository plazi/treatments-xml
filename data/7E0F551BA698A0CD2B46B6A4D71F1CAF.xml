<subSubSection id="1B87FFBED73387846A3012C20DC08922" pageId="2" pageNumber="99" type="nomenclature">
<paragraph id="42EB86C21F9C325A89240E4891677585" pageId="2" pageNumber="99">
Genus 
<taxonomicName id="52DCDB0DC454638019525992BBED0CC4" LSID="http://zoobank.org/0E7CBC8A-C92E-426F-A331-93156EE2B0A8" class="Insecta" family="Carabidae" genus="Shuangheaphaenops" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Shuangheaphaenops" order="Coleoptera" pageId="2" pageNumber="99" phylum="Arthropoda" rank="genus">Shuangheaphaenops</taxonomicName>
