<taxonomicName id="C4844DD53D39AAA5B27C1123B156F964" authority="Mulsant, 1854" authorityName="Mulsant" authorityYear="1854" class="Insecta" family="Tenebrionidae" genus="Bioplanes" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bioplanes" order="Coleoptera" pageId="183" pageNumber="184" phylum="Arthropoda" rank="genus">Bioplanes</taxonomicName>
