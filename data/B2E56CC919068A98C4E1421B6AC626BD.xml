<taxonomicName id="623912D6084F4983B03137A19C00573E" authority="Illiger, 1802" authorityName="Illiger" authorityYear="1802" class="Insecta" family="Cerambycidae" genus="Isocerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Isocerus" order="Coleoptera" pageId="182" pageNumber="183" phylum="Arthropoda" rank="genus">Isocerus</taxonomicName>
Illiger, 1802 [
<taxonomicName id="5C87A435AE002D515C36A24EA560DB5B" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Magnoliopsida" family="Malvaceae" genus="Coleoptera" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Coleoptera" order="Malvales" pageId="182" pageNumber="183" phylum="Tracheophyta" rank="genus">Coleoptera</taxonomicName>
: 
<taxonomicName id="89C786CF5CEA4CBC4EA86BA57F5F466C" family="Cerambycidae" lsidName="" pageId="182" pageNumber="183" rank="family">Cerambycidae</taxonomicName>
