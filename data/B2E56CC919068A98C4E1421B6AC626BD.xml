<taxonomicName id="C610D89861284A17984B4512C2FD03DC" authority="Dejean, 1821" authorityName="Dejean" authorityYear="1821" class="Insecta" family="Tenebrionidae" genus="Dendarus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Dendarus" order="Coleoptera" pageId="183" pageNumber="184" phylum="Arthropoda" rank="genus">Dendarus</taxonomicName>
