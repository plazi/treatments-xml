<taxonomicName id="84DD54CFE8B7AC16794F312EF27F8908" authority="Dejean, 1834" authorityName="Dejean" authorityYear="1834" class="Insecta" family="Tenebrionidae" genus="Heliopates" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Heliopates" order="Coleoptera" pageId="183" pageNumber="184" phylum="Arthropoda" rank="genus">Heliopates</taxonomicName>
