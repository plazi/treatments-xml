<taxonomicName id="4C34B8AFFFD9D96ED351FEAC6E23FE2A" box="[366,633,376,401]" class="Polychaeta" family="Echiuridae" genus="Thalassema" higherTaxonomySource="CoL" kingdom="Animalia" order="Echiuroidea" pageId="6" pageNumber="66" phylum="Echiura" rank="species" species="steinbecki">
