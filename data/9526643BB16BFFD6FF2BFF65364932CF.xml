<subSubSection id="559586A6B16BFFD6FF2BFF48309336E1" pageId="2" pageNumber="239" type="materials_examined">
<paragraph id="1D30D52DB16BFFD6FF2BFF4836EC3657" blockId="2.[151,1436,151,1423]" pageId="2" pageNumber="239">
