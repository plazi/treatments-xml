<quantity id="DA7778C8B16BFFD6FD42FF2F377437B4" box="[766,836,221,244]" metricMagnitude="4" metricUnit="m" metricValue="5.0" pageId="2" pageNumber="239" unit="km" value="50.0">50 km</quantity>
