<subSubSection id="C367B4D5F140FFC9F5E7F99A60ACCD56" box="[199,1212,1554,1579]" pageId="4" pageNumber="119" type="etymology">
<paragraph id="8BC2E75EF140FFC9F5E7F99A60ACCD56" blockId="4.[151,1437,258,2011]" box="[199,1212,1554,1579]" pageId="4" pageNumber="119">
<emphasis id="B9093B4CF140FFC9F5E7F99A6541CD56" bold="true" box="[199,337,1554,1579]" pageId="4" pageNumber="119">Etymology:</emphasis>
