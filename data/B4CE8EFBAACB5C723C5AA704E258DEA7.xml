<taxonomicName id="89DAD581493C8B1D622F31B5C7F8D65E" authority="Ishihara, 1955" authorityName="Ishihara" authorityYear="1955" class="Insecta" family="Cicadellidae" genus="Onukigallia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Onukigallia" order="Hemiptera" pageId="1" pageNumber="86" phylum="Arthropoda" rank="genus">Onukigallia Ishihara, 1955</taxonomicName>
