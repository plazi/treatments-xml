<taxonomicName id="DB5816CB5DAEBFAD0D8E356E7BAEB1F6" class="Dicotyledoneae" family="Brassicaceae" genus="Arabis" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Arabis abietina" order="Papaverales" pageId="5" pageNumber="104" phylum="Angiospermae" rank="species" species="abietina">
