, 
<taxonomicName id="C636CA429F4535E7A907FE06164A2769" baseAuthorityName="L.Pfeiffer" baseAuthorityYear="1855" class="Gastropoda" family="Bulimulidae" genus="Neopetraeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="55" pageNumber="56" phylum="Mollusca" rank="species" species="excoriatus">
