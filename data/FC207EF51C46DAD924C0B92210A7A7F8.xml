<taxonomicName id="73CDDA8D5282D25F72075BFCEA2FD68C" class="Insecta" family="Dolichopodidae" genus="Xanthochlorus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Xanthochlorus henanensis" order="Diptera" pageId="7" pageNumber="148" phylum="Arthropoda" rank="species" species="henanensis">Xanthochlorus henanensis</taxonomicName>
