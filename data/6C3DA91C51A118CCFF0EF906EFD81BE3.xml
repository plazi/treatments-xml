<quantity id="236CB5EF51A118CCFD03F8DDEDD01B1A" box="[666,744,1858,1885]" metricMagnitude="-2" metricUnit="m" metricValue="1.1" pageId="154" pageNumber="155" unit="mm" value="11.0">11 mm</quantity>
