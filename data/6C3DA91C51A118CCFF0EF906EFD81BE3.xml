<taxonomicName id="2394638951A118CCFBF8F8DDEA751B1B" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[1121,1357,1858,1884]" class="Insecta" family="Lampyridae" genus="Abscondita" kingdom="Animalia" order="Coleoptera" pageId="154" pageNumber="155" phylum="Arthropoda" rank="species" species="chinensis">
<emphasis id="D6E0C41851A118CCFBF8F8DDEA751B1B" box="[1121,1357,1858,1884]" italics="true" pageId="154" pageNumber="155">Abscondita chinensis</emphasis>
</taxonomicName>
