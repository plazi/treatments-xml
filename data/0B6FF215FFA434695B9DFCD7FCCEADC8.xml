<taxonomicName id="44C63880FFA5346E5A08FB71FDAAAEE2" box="[285,551,1221,1242]" class="Insecta" family="Berothidae" genus="Halteriomantispa" kingdom="Animalia" order="Neuroptera" pageId="3" pageNumber="168" phylum="Arthropoda" rank="species" species="grimaldii">
