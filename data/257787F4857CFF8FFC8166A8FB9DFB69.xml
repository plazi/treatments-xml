<taxonomicName id="6ADE4D61857DFF89FE5F62A0FD54F834" authorityName="Patano Jr &amp; Amoroso &amp; Mohagan &amp; Guiang &amp; Yap" authorityYear="2021" box="[483,739,2004,2026]" class="Insecta" family="Curculionidae" genus="Metapocyrtus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="285" phylum="Arthropoda" rank="species" species="villalobosae">
<emphasis id="9FAAEAF0857DFF89FE5F62A0FD54F834" box="[483,739,2004,2026]" italics="true" pageId="3" pageNumber="285">Metapocyrtus villalobosae</emphasis>
