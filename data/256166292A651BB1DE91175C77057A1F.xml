<taxonomicName id="CD3ECA37FB3CC025B3DA52C96C95FF5C" class="Arachnida" family="Linyphiidae" genus="Oedothorax" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Oedothorax bipunctis" order="Araneae" pageId="12" pageNumber="27" phylum="Arthropoda" rank="species" species="bipunctis">Oedothorax bipunctis</taxonomicName>
