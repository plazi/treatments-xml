<taxonomicName id="4C7F4D54D00EFFAC90D12A7EA0ECF9DF" baseAuthorityName="Kohl" baseAuthorityYear="1886" box="[764,946,1558,1579]" class="Insecta" family="Pompilidae" genus="Ferreola" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="403" phylum="Arthropoda" rank="species" species="orchesica">
