<taxonomicName id="4C7F4D54D00EFFAC929F2A46A23EF9B7" baseAuthorityName="Kohl" baseAuthorityYear="1886" box="[178,352,1582,1603]" class="Insecta" family="Pompilidae" genus="Ferreola" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="403" phylum="Arthropoda" rank="species" species="orchesica">
