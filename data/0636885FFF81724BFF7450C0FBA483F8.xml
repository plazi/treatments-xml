), 
<collectingRegion id="4C5BF7ABFF81724BFC2C51BDFB8C80FC" box="[975,1025,1462,1485]" country="Indonesia" name="Jawa Timur" pageId="48" pageNumber="49">Java</collectingRegion>
(
<taxonomicName id="499F42CAFF81724BFBF151BDFB7380FF" authority="producta" authorityName="producta (Serville" authorityYear="1838" box="[1042,1278,1461,1486]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="subSpecies" species="producta" subSpecies="producta">
<emphasis id="BCEBE55BFF81724BFBF151BDFB7380FF" box="[1042,1278,1461,1486]" italics="true" pageId="48" pageNumber="49">S. producta producta</emphasis>
