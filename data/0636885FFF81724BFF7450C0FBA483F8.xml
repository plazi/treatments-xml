<taxonomicName id="499F42CAFF81724BFF4351D1FE3680C3" box="[160,443,1497,1522]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="subSpecies" species="producta" subSpecies="dammermani">
<emphasis id="BCEBE55BFF81724BFF4351D1FE3680C3" box="[160,443,1497,1522]" italics="true" pageId="48" pageNumber="49">S. producta dammermani</emphasis>
