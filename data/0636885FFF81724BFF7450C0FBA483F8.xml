<collectingRegion id="4C5BF7ABFF81724BFDE151D2FDEA80C3" box="[514,615,1497,1522]" country="Indonesia" name="Sulawesi Utara" pageId="48" pageNumber="49">Sulawesi</collectingRegion>
(
<taxonomicName id="499F42CAFF81724BFD9451D1FD7D80C3" baseAuthorityName="Bolivar" baseAuthorityYear="1887" box="[631,752,1497,1522]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="celebica">
