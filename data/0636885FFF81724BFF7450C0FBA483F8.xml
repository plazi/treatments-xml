<taxonomicName id="499F42CAFF81724BFDF351BDFD3D80FF" baseAuthorityName="Hancock" baseAuthorityYear="1907" box="[528,688,1461,1486]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="dentiumeris">
<emphasis id="BCEBE55BFF81724BFDF351BDFD3D80FF" box="[528,688,1461,1486]" italics="true" pageId="48" pageNumber="49">S. dentiumeris</emphasis>
</taxonomicName>
), 
<collectingRegion id="4C5BF7ABFF81724BFD2A51BDFCAB80FF" box="[713,806,1462,1486]" country="Indonesia" name="Sumatera Utara" pageId="48" pageNumber="49">Sumatra</collectingRegion>
(
<taxonomicName id="499F42CAFF81724BFCD451BDFC3580FF" baseAuthorityName="Serville" baseAuthorityYear="1838" box="[823,952,1461,1486]" class="Insecta" family="Tetrigidae" genus="Scelimena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="producta">
