<taxonomicName id="6AD247ADFFE5FFD8FEB8FAF7E8E8FAB6" authorityName="Thomas" authorityYear="1985" box="[273,499,1339,1365]" class="Insecta" family="Pentatomidae" genus="Chlorocoris" kingdom="Animalia" order="Hemiptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="isthmus">
