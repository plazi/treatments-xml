<taxonomicName id="AE967ECD3649FFDCFE2AF9F6FCE3CF2A" authority="Zhiltzova" authorityName="Zhiltzova" box="[478,772,1615,1639]" class="Insecta" family="Perlodidae" genus="Isoperla" kingdom="Animalia" order="Plecoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="kozlovi">
<emphasis id="5BE2D95C3649FFDCFE2AF9F6FD68CF2B" box="[478,655,1616,1639]" italics="true" pageId="13" pageNumber="14">Isoperla kozlovi</emphasis>
