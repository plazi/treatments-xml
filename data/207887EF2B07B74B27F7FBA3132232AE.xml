<taxonomicName id="6FD14D7A2B05B74B2632FF24127A32E2" authority="Hsieh &amp; Huang (Labiatae)" authorityName="Hsieh &amp; Huang (Labiatae" box="[338,1118,151,176]" class="Magnoliopsida" family="Lamiaceae" genus="Clinopodium" kingdom="Plantae" order="Lamiales" pageId="14" pageNumber="503" phylum="Tracheophyta" rank="variety" species="laxiflorum" variety="taiwanianum">
