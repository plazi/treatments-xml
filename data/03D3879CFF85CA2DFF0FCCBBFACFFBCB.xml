<paragraph id="8BC5368AFF85CA2EFF0FCD0EFC76F8AB" blockId="1.[151,968,1882,1904]" box="[151,968,1882,1904]" pageId="1" pageNumber="358">
http://lsid.speciesfile.org/urn:lsid: 
<taxonomicName id="4C7A4D09FF85CA2EFE7DCD0EFDE7F8AB" box="[485,601,1882,1904]" class="Insecta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="358" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
