<taxonomicName id="FC9180AEFFCB9D0E1CA3167DFA9AFAE9" authority="Masan &amp; Joharchi" authorityName="Masan &amp; Joharchi" authorityYear="2021" box="[902,1353,1297,1320]" class="Arachnida" family="Ascidae" genus="Mycomelichares" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="15" pageNumber="172" phylum="Arthropoda" rank="species" species="reductus" status="sp. nov.">
