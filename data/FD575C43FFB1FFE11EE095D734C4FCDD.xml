<taxonomicName id="B2FE96D6FFB1FFE11C99966D334DFCF2" box="[750,892,813,838]" class="Magnoliopsida" family="Loganiaceae" kingdom="Plantae" order="Gentianales" pageId="5" pageNumber="450" phylum="Tracheophyta" rank="family">Loganiaceae</taxonomicName>
