<number id="3DFF24E7CA392C5CFD53FC67FCB3FC4F" box="[706,820,943,967]" pageId="7" pageNumber="58" value="17.0" valueMax="17.5" valueMin="16.5">16.5–17.5</number>
, female 
<number id="3DFF24E7CA392C5CFC30FC67FB93FC4F" box="[929,1044,943,967]" pageId="7" pageNumber="58" value="20.3" valueMax="21.3" valueMin="19.3">19.3–21.3</number>
; pronotum length: male 3.0– 
<number id="3DFF24E7CA392C5CFAE5FC67FA1FFC4F" box="[1396,1432,943,967]" pageId="7" pageNumber="58" value="3.2">3.2</number>
, female 
<number id="3DFF24E7CA392C5CFF7BFC1CFEC5FC64" box="[234,322,980,1004]" pageId="7" pageNumber="58" value="3.9000000000000004" valueMax="4.2" valueMin="3.6">3.6–4.2</number>
