<collectingCountry id="F36D760CB792FF2766C77CB2DFA6FE01" box="[915,982,384,410]" name="Japan" pageId="195" pageNumber="196">Japan</collectingCountry>
on 
<taxonomicName id="4C7A4D1FB792FF2766A97CB2D967FE02" authority="Kult, 1961" authorityName="Kult" authorityYear="1961" box="[1021,1303,383,410]" class="Insecta" family="Carabidae" genus="Macrotachys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="195" pageNumber="196" phylum="Arthropoda" rank="genus">
<emphasis id="B90EEA8EB792FF2766A97CB2D8E5FE01" box="[1021,1173,384,410]" italics="true" pageId="195" pageNumber="196">Macrotachys</emphasis>
Kult, 1961
</taxonomicName>
(
<bibRefCitation id="EFEB4B6DB792FF2760737CB2DD75FE25" author="Terada K." pageId="195" pageNumber="196" pagination="77 - 84" refId="ref222500" refString="Terada K. 1998 a. New records of the carabidicolous Laboulbeniales (Ascomycetes) of Japan. Mycoscience 39 (1): 77 - 84. https: // doi. org / 10.1007 / BF 02461582" type="journal article" year="1998">Terada 1998a</bibRefCitation>
