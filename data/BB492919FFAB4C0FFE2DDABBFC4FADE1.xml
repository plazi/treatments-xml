<heading id="68172F63FFAB4C0FFE2DDABBFCE9ABF0" bold="true" box="[425,777,716,741]" centered="true" fontSize="8" level="2" pageId="1" pageNumber="224" reason="3">
<taxonomicName id="F4E0E38CFFAB4C0FFE2DDABBFCE9ABF0" authority="Ganglbauer, 1899" authorityName="Ganglbauer" authorityYear="1899" box="[425,777,716,741]" class="Insecta" family="Silvanidae" genus="Oryzaephilus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="224" phylum="Arthropoda" rank="genus">
