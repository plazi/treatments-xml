<collectingCountry id="F36176127D42D13DFE58521DFDE703B0" box="[494,549,375,394]" name="Peru" pageId="90" pageNumber="90">Peru</collectingCountry>
: Colonia Cal-leria, Rio Calleria, 
<quantity id="4C8E9B677D42D13DFE2D52FBFE16039E" box="[411,468,401,420]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="90" pageNumber="90" unit="km" value="15.0">15km</quantity>
E 
<collectingRegion id="49B2F8607D42D13DFE4152FBFD88039E" box="[503,586,401,420]" country="Peru" name="Ucayali" pageId="90" pageNumber="90">Ucayali</collectingRegion>
, 
<date id="FFC810427D42D13DFDE152FBFF270385" pageId="90" pageNumber="90" value="1961-10-01" valueMax="1961-10-15" valueMin="1961-10-01">1- 15-X-1961</date>
,/FMHD 
<emphasis id="B902EA907D42D13DFEFE52C6FE9E0384" box="[328,348,428,446]" italics="true" pageId="90" pageNumber="90">#</emphasis>
61 - 3002, sweeping in for. at night, B. Malkin/ 
<typeStatus id="54CD88207D42D13DFEFB52ACFE6C03E3" box="[333,430,454,473]" pageId="90" pageNumber="90">Holotype</typeStatus>
<taxonomicName id="4C764D017D42D13DFE0152AFFEC003CE" authority="Herman." authorityName="Herman." class="Insecta" family="Staphylinidae" genus="Oedichirus" kingdom="Animalia" order="Coleoptera" pageId="90" pageNumber="90" phylum="Arthropoda" rank="species" species="hamatus">
