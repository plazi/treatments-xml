<taxonomicName id="4E5EA1EF55C1E7FB149AF8EBC1C69113" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:230410" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus (Myrmoturba)" order="Hymenoptera" pageNumber="235" phylum="Arthropoda" rank="subGenus" subGenus="Myrmoturba">Myrmoturba Forel</taxonomicName>
