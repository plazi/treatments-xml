<taxonomicName id="9DEA4D90FFE40A5620BF4D7EFDA0FCF6" authorityName="Nylander" authorityYear="1870" baseAuthorityName="Sw." box="[394,547,769,794]" class="Lecanoromycetes" family="Cladoniaceae" genus="Cladia" kingdom="Fungi" order="Lecanorales" pageId="10" pageNumber="11" phylum="Ascomycota" rank="species" species="aggregata">
<emphasis id="689EEA01FFE40A5620BF4D7EFDA0FCF6" box="[394,547,769,794]" italics="true" pageId="10" pageNumber="11">C. aggregata</emphasis>
</taxonomicName>
.
</paragraph>
