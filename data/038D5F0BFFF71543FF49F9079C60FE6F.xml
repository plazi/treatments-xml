<collectingCountry id="F333AE8DFFF91543FE82FE4E9FEBFE6F" box="[348,415,439,463]" name="China" pageId="9" pageNumber="10">China</collectingCountry>
(
<collectingRegion id="49E020FFFFF91543FE70FE4E9C7CFE6F" box="[430,520,439,463]" country="China" name="Jiangsu" pageId="9" pageNumber="10">Jiangsu</collectingRegion>
).
</paragraph>
