<taxonomicName id="4C4727AD6A38FFB36ECB9FC0B57B45BE" authority="Gray, 1857" authorityName="Gray" authorityYear="1857" box="[151,520,1677,1703]" class="Anthozoa" family="Antipathidae" genus="Antipathes" kingdom="Animalia" order="Antipatharia" pageId="3" pageNumber="370" phylum="Cnidaria" rank="species" species="furcata">
