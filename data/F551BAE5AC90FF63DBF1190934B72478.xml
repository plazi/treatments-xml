<taxonomicName id="A40BAE0086629ED177242751E5FFAD3A" authority="Wagner, 1914" authorityName="Wagner" authorityYear="1914" class="Insecta" family="Brentidae" genus="Coelocephalapion" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Coelocephalapion emaciipes" order="Coleoptera" pageId="2" pageNumber="351" phylum="Arthropoda" rank="species" species="emaciipes">Coelocephalapion emaciipes</taxonomicName>
