<subSubSection pageId="null" pageNumber="289" type="nomenclature">
<paragraph pageId="null" pageNumber="289">
<taxonomicName authority="L." class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="289" phylum="Tracheophyta" rank="species" species="boreale">
Galium 
<normalizedToken originalValue="boreále" pageId="null" pageNumber="289">boreale</normalizedToken>
<authorityName pageId="null" pageNumber="289">L.</authorityName>
</taxonomicName>
</paragraph>
