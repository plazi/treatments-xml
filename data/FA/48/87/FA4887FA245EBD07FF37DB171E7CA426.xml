<subSubSection pageId="64" pageNumber="65" type="distribution">
<paragraph blockId="64.[151,1436,1651,1831]" pageId="64" pageNumber="65">
<emphasis bold="true" box="[198,541,1728,1754]" pageId="64" pageNumber="65">Distribution and habitat:—</emphasis>
From 8°27’– 
<geoCoordinate box="[691,778,1728,1754]" degrees="8" direction="north" minutes="58" orientation="latitude" pageId="64" pageNumber="65" precision="925" value="8.966666">8°58’N</geoCoordinate>
