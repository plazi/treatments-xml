<geoCoordinate box="[983,1092,583,609]" degrees="78" direction="west" minutes="56" orientation="longitude" pageId="59" pageNumber="60" precision="925" value="-78.933334">78°56’W</geoCoordinate>
in western 
<collectingCountry box="[1260,1376,583,609]" name="Colombia" pageId="59" pageNumber="60">Colombia</collectingCountry>
