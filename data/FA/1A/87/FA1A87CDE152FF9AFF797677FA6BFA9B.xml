<quantity box="[795,903,1199,1220]" metricMagnitude="-3" metricUnit="m" metricValue="6.4" metricValueMax="12.0" metricValueMin="0.8" pageId="5" pageNumber="58" unit="mm" value="6.4" valueMax="12.0" valueMin="0.8">0.8–12 mm</quantity>
