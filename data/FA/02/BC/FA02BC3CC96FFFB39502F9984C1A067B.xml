<taxonomicName baseAuthorityName="Bezzi" baseAuthorityYear="1928" box="[439,599,1754,1781]" class="Insecta" family="Syrphidae" genus="Allograpta" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="amphotera">
