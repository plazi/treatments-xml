<taxonomicName authority="Lundblad" authorityName="Lundblad" box="[264,518,1203,1230]" class="Arachnida" family="Hygrobatidae" genus="Szalayella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
