<collectionCode box="[1174,1223,1161,1187]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13893" lsid="urn:lsid:biocol.org:col:13893" name="Puslinch House" pageId="5" pageNumber="90" type="Herbarium">PUS</collectionCode>
