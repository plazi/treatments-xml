<emphasis box="[996,1010,1125,1151]" italics="true" pageId="5" pageNumber="90">b</emphasis>
= 5.7 (4.4–6.5); 
<emphasis box="[1200,1212,1126,1151]" italics="true" pageId="5" pageNumber="90">c</emphasis>
= 11.9 (10.6–13.2); 
<emphasis box="[151,170,1160,1187]" italics="true" pageId="5" pageNumber="90">c′</emphasis>
= 5.3 (4.8–5.7); 
<emphasis box="[359,376,1162,1187]" italics="true" pageId="5" pageNumber="90">V</emphasis>
= 80.1 (77.5–82.0); 
<emphasis box="[607,630,1160,1187]" italics="true" pageId="5" pageNumber="90">V′</emphasis>
= 87.5 (85.3–88.7); 
<collectionCode box="[861,912,1161,1187]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13893" lsid="urn:lsid:biocol.org:col:13893" name="Puslinch House" pageId="5" pageNumber="90" type="Herbarium">PUS</collectionCode>
