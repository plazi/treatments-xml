<emphasis box="[225,244,1916,1943]" italics="true" pageId="5" pageNumber="90">c′</emphasis>
= 4.3 (2.6–7.2); 
<emphasis box="[433,450,1918,1943]" italics="true" pageId="5" pageNumber="90">V</emphasis>
= 80.8 (77.1–84.9); 
<emphasis box="[683,706,1916,1943]" italics="true" pageId="5" pageNumber="90">V′</emphasis>
= 86.7 (78.4–89.8); 
<collectionCode box="[938,989,1917,1943]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13893" lsid="urn:lsid:biocol.org:col:13893" name="Puslinch House" pageId="5" pageNumber="90" type="Herbarium">PUS</collectionCode>
/ 
<collectionCode box="[997,1062,1917,1943]" pageId="5" pageNumber="90">VBW</collectionCode>
