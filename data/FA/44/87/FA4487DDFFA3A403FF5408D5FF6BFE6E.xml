<collectionCode box="[537,586,1366,1392]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13893" lsid="urn:lsid:biocol.org:col:13893" name="Puslinch House" pageId="10" pageNumber="95" type="Herbarium">PUS</collectionCode>
