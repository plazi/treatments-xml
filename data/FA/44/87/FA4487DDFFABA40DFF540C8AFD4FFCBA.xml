<collectingRegion country="Iran" name="Fars" pageId="5" pageNumber="90">Fars province</collectingRegion>
, 
<collectingCountry box="[263,312,189,215]" name="Iran" pageId="5" pageNumber="90">Iran</collectingCountry>
.
</paragraph>
