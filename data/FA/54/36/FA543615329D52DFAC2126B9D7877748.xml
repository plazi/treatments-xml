<geoCoordinate degrees="18.50" direction="north" orientation="latitude" precision="555" value="18.5">18.50°N</geoCoordinate>
, 
<geoCoordinate degrees="109.23" direction="east" orientation="longitude" precision="555" value="109.23">109.23°E</geoCoordinate>
