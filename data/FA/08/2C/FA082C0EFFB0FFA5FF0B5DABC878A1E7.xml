<caption ID-DOI="http://doi.org/10.5281/zenodo.5814238" ID-Zenodo-Dep="5814238" httpUri="https://zenodo.org/record/5814238/files/figure.png" pageId="1" pageNumber="172" startId="1.[136,229,957,979]" targetBox="[249,1338,316,932]" targetPageId="1">
<paragraph blockId="1.[136,1451,957,1052]" pageId="1" pageNumber="172">
