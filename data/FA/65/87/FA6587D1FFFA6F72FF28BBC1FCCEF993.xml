<subSubSection box="[151,911,572,594]" pageId="1" pageNumber="597" type="materials_examined">
<paragraph blockId="1.[151,911,572,594]" box="[151,911,572,594]" pageId="1" pageNumber="597">
<emphasis bold="true" box="[151,290,572,594]" pageId="1" pageNumber="597">Type species.</emphasis>
<taxonomicName authority="Chen, 2018" authorityName="Chen" authorityYear="2018" box="[297,595,572,593]" class="Insecta" family="Perlidae" genus="Pinguisoperla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="597" phylum="Arthropoda" rank="species" species="yangzhouensis" status="gen. et sp. nov.">
<emphasis box="[297,595,572,593]" italics="true" pageId="1" pageNumber="597">Pinguisoperla yangzhouensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[601,758,572,594]" pageId="1" pageNumber="597">
<taxonomicNameLabel box="[601,758,572,594]" pageId="1" pageNumber="597" rank="species">gen. et sp. nov.</taxonomicNameLabel>
