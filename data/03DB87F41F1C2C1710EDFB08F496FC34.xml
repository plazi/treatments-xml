<taxonomicName id="4C724D611F1C2C151151FA68F4FEFA44" authority="Muller, 1906" authorityName="Muller" authorityYear="1906" box="[299,647,1471,1493]" class="Ostracoda" family="Halocyprididae" genus="Conchoecia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Halocyprida" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="lophura">
