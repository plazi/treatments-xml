<taxonomicName id="AE757DA99912CD78F62461AEC2725FF5" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:138134" authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" class="Insecta" family="Cryptoceridae" genus="Cryptocerus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cryptocerus fenestralis Smith" lsidName-HNS="Cryptocerus fenestralis Smith" order="Hymenoptera" pageNumber="607" phylum="Arthropoda" rank="species" species="fenestralis">Cryptocerus fenestralis</taxonomicName>
