<taxonomicName id="E6B74D5BEB25FFEE6F1DF9A325D6F995" box="[594,700,1633,1656]" class="Insecta" family="Cicadellidae" genus="Abcoronalis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="391" phylum="Arthropoda" rank="species" species="protrusus">
