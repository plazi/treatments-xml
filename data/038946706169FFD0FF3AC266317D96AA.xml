<taxonomicName id="4C208CE56169FFD0FBA8C48730F396A9" box="[1029,1221,1190,1213]" class="Insecta" family="Thripidae" genus="Neohydatothrips" kingdom="Animalia" order="Thysanoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="novateutoniae">
