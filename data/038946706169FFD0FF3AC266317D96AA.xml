<taxonomicName id="4C208CE56169FFD0FCEBC48737E696AA" box="[838,976,1189,1214]" class="Insecta" family="Thripidae" genus="Neohydatothrips" kingdom="Animalia" order="Thysanoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="clavisetis">
