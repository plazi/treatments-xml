</paragraph>
<paragraph id="CE4936C8FFAC7441FF07FBF7FE87FB8E" blockId="18.[151,1125,1075,1138]" box="[151,309,1112,1138]" pageId="18" pageNumber="19">
<collectingRegion id="0C32F82AFFAC7441FF07FBF7FF66FB8E" box="[151,212,1112,1138]" country="Peru" name="Junin" pageId="18" pageNumber="19">Junín</collectingRegion>
