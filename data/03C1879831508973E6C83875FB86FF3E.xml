<number id="3DA024A931518970E75A3946FEDFFF04" box="[261,274,538,564]" pageId="9" pageNumber="52" value="1.0">1</number>
- 
<number id="3DA024A931518970E7423946FEE7FF04" box="[285,298,538,564]" pageId="9" pageNumber="52" value="1.0">1</number>
; genua I–IV: 
<number id="3DA024A931518970E7903946FE39FF04" box="[463,500,538,564]" pageId="9" pageNumber="52" value="3.0" valueMax="5.0" valueMin="1.0">1-5</number>
