<number id="3DA024A931518970E2C03946FB2DFF04" box="[1183,1248,538,564]" pageId="9" pageNumber="52" value="2.05" valueMax="2.1" valueMin="2.0">2-2, 1</number>
- 
<number id="3DA024A931518970E2B53946FB37FF04" box="[1258,1274,538,564]" pageId="9" pageNumber="52" value="3.0">3</number>
/ 
<number id="3DA024A931518970E3593946FAD5FF04" box="[1286,1304,538,564]" pageId="9" pageNumber="52" value="2.0">2</number>
