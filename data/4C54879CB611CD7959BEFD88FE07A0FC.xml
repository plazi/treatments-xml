, 
<collectingCountry id="BCEA761AB611CD795D07FC0FFB5FA048" box="[1070,1194,1007,1033]" name="Guatemala" pageId="76" pageNumber="77">Guatemala</collectingCountry>
, 
<collectingCountry id="BCEA761AB611CD795D91FC0FFADBA048" box="[1208,1326,1007,1033]" name="Nicaragua" pageId="76" pageNumber="77">Nicaragua</collectingCountry>
