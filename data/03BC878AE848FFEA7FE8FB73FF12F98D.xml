<paragraph id="8BAA369CE848FFEA7FE8FA0AFF12F98D" blockId="10.[151,1437,1232,1582]" pageId="10" pageNumber="411">
<emphasis id="B961EA8EE848FFEA7FE8FA0AFEB5FA60" bold="true" box="[151,330,1448,1475]" pageId="10" pageNumber="411">Type material:</emphasis>
<typeStatus id="54AE883EE848FFEA7E2BFA0BFE43FA61" box="[340,444,1448,1474]" pageId="10" pageNumber="411">Holotype</typeStatus>
