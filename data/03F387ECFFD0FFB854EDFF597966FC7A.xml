<paragraph id="8BE536FAFFD0FFB854EDFF747E3BFF58" blockId="41.[151,572,151,211]" box="[151,572,186,211]" pageId="41" pageNumber="42">
(
<figureCitation id="13612A7FFFD0FFB854E5FF747CE3FF58" box="[159,228,186,211]" captionStart="FIGURE 3" captionStartId="8.[151,249,1691,1713]" captionTargetBox="[216,1358,196,1661]" captionTargetId="figure@8.[216,1359,195,1661]" captionTargetPageId="8" captionText="FIGURE 3. Wing of male: A. Leucophenga acutifoliacea sp. nov.; B. L. albiterga sp. nov.; C. L. angustifoliacea sp. nov.; D. L. baculifoliacea sp. nov.; E. L. cornuta sp. nov.; F, G. L. hirticeps sp. nov.; H. L. latifascia sp. nov.; I. L. pinguifoliacea sp. nov.; J. L. retihirta sp. nov. Scale bars = 1 mm." httpUri="https://zenodo.org/record/287542/files/figure.png" pageId="41" pageNumber="42">
Figs 
<number id="3D9224DDFFD0FFB854ADFF757CE3FF58" box="[215,228,187,211]" pageId="41" pageNumber="42" value="3.0">3</number>
</figureCitation>
E, 
<number id="3D9224DDFFD0FFB8557EFF757D15FF58" box="[260,274,187,211]" pageId="41" pageNumber="42" value="8.0">8</number>
C, 
<number id="3D9224DDFFD0FFB85549FF757D47FF58" box="[307,320,187,211]" pageId="41" pageNumber="42" value="8.0">8</number>
D, 
<number id="3D9224DDFFD0FFB85519FF757D79FF58" box="[355,382,187,211]" pageId="41" pageNumber="42" value="11.0">11</number>
E, 
<number id="3D9224DDFFD0FFB855E7FF747DB0FF58" box="[413,439,186,211]" pageId="41" pageNumber="42" value="15.0">15</number>
G, 
<number id="3D9224DDFFD0FFB855A2FF747DF3FF58" box="[472,500,186,211]" pageId="41" pageNumber="42" value="15.0">15</number>
H, 
<number id="3D9224DDFFD0FFB8566CFF757E34FF58" box="[534,563,187,211]" pageId="41" pageNumber="42" value="31.0">31</number>
