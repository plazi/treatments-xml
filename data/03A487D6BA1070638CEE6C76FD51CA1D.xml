<subSubSection id="C317654BBA1070638CBE6F7CFEE0CE46" pageId="35" pageNumber="236" type="etymology">
<paragraph id="8BB236C0BA1070638CBE6F7CFEE0CE46" blockId="35.[151,1437,467,1574]" pageId="35" pageNumber="236">
<emphasis id="B979EAD2BA1070638CBE6F7CFED6CE61" bold="true" box="[199,334,576,602]" pageId="35" pageNumber="236">Etymology.</emphasis>
