<taxonomicName id="4C4E4D6D41119E7FEAE943BCFCC8FF66" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[467,792,222,247]" class="Insecta" family="Sphingidae" genus="Hemaris" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="414" phylum="Arthropoda" rank="species" species="fuciformis">
<emphasis id="B93AEAFC41119E7FEAE943BCFD9BFF66" bold="true" box="[467,587,222,247]" italics="true" pageId="21" pageNumber="414">fuciformis</emphasis>
