<taxonomicName id="41A44D550D21B223FE3F3874FD58E7A8" box="[468,703,1646,1669]" class="Insecta" family="Phoridae" genus="Megaselia" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="quintincisa">
