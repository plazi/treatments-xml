<taxonomicName id="B634FC8A6C9AC71DD50B5C4855AF8791" authorityName="Bonelli" authorityYear="1813" class="Insecta" family="Carabidae" genus="Dicaelus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="631" pageNumber="632" phylum="Arthropoda" rank="genus">
