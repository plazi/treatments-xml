<collectingCountry id="64CC7659FFB3FF9C7F5B13FBFE44FE60" box="[358,464,439,465]" name="Australia" pageId="18" pageNumber="498">Australia</collectingCountry>
, 
<collectingCountry id="64CC7659FFB3FF9C7FE613FBFDDFFE60" box="[475,587,439,465]" name="Indonesia" pageId="18" pageNumber="498">Indonesia</collectingCountry>
