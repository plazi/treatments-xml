<emphasis id="B95FF64A9C11FFCBFF122205FD81FCE9" bold="true" box="[151,635,834,861]" pageId="1" pageNumber="280">
Subgenus 
<taxonomicName id="4C2B51DB9C11FFCBFE922205FE23FCE9" authority="Nabozhenko &amp; Mckellar &amp; Bukejs, 2021" authorityName="Nabozhenko &amp; Mckellar &amp; Bukejs" authorityYear="2021" box="[279,473,834,861]" class="Insecta" family="Tenebrionidae" genus="Metaclisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="280" phylum="Arthropoda" rank="subGenus" status="subgen. nov." subGenus="Trichometaclisa">
<emphasis id="B95FF64A9C11FFCBFE922205FE23FCE9" bold="true" box="[279,473,834,861]" italics="true" pageId="1" pageNumber="280">Trichometaclisa</emphasis>
</taxonomicName>
