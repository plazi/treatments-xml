<heading id="D0A83C06FC51FFD8F346FCB4FDCCFCBD" box="[340,553,834,856]" fontSize="8" level="2" pageId="16" pageNumber="1025" reason="2">
<emphasis id="B92B5778FC51FFD8F346FCB4FDCCFCBD" bold="true" box="[340,553,834,856]" pageId="16" pageNumber="1025">
<taxonomicName id="4C5FF0E9FC51FFD8F346FCB4FE2CFCBD" authorityName="H Malicky &amp; P Chantaramongkol" authorityYear="2007" box="[340,457,834,856]" class="Insecta" family="Hydroptilidae" genus="Maeyaptila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="16" pageNumber="1025" phylum="Arthropoda" rank="genus" status="nov.gen.">
<emphasis id="B92B5778FC51FFD8F346FCB4FE2CFCBD" bold="true" box="[340,457,834,856]" italics="true" pageId="16" pageNumber="1025">Maeyaptila</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A218EA03FC51FFD8F3DDFCB4FDCCFCBD" box="[463,553,834,856]" pageId="16" pageNumber="1025" rank="genus">nov.gen.</taxonomicNameLabel>
