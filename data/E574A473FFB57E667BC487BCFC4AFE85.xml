times article 
<number id="DB150742FFB57E687E658216FA9CF9DC" box="[1334,1348,1580,1604]" pageId="38" pageNumber="39" value="2.0">2</number>
length; article 
<number id="DB150742FFB57E687BB7826AFF2AF9F0" box="[228,242,1616,1640]" pageId="38" pageNumber="39" value="2.0">2</number>
length 
