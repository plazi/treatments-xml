<taxonomicName id="AADD6EE6FFB57E687A07806DFE7FFBE8" box="[340,423,1111,1136]" class="Malacostraca" family="Ampithoidae" genus="Cymadusa" kingdom="Animalia" order="Amphipoda" pageId="38" pageNumber="41" phylum="Arthropoda" rank="species" species="hadros" status="sp. nov.">
