<taxonomicName id="CFF266F9FFCAA826FD38FA753E5DFA28" box="[737,850,1433,1457]" class="Insecta" family="Melyridae" genus="Malachius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="298" phylum="Arthropoda" rank="genus">
