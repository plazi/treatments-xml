<taxonomicName id="34B94D306419BA78FB14F94AFA8C67D2" authority="Forster" authorityName="Forster" box="[1188,1401,1700,1724]" class="Insecta" family="Ichneumonidae" genus="Sinophorus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="275" phylum="Arthropoda" rank="genus">
