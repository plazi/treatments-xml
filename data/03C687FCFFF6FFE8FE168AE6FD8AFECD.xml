can be seen inside 
<taxonomicName id="4C6F4D69FFF0FFF4FCB9882DFC70F9BE" authorityName="Esteban &amp; Finlay &amp; Olmo &amp; Tyler" authorityYear="2000" box="[821,952,1537,1558]" class="Oligohymenophorea" family="Lembadionidae" genus="Lembadion" higherTaxonomySource="GBIF" kingdom="Chromista" order="Hymenostomatida" pageId="13" pageNumber="170" phylum="Ciliophora" rank="species" species="curvatum">
