m, 29Ö 
<quantity id="4C979B0FFFF3FFF7FC4B8BA5FC3CFA09" box="[967,1012,1417,1441]" metricMagnitude="1" metricUnit="m" metricValue="3.2" pageId="14" pageNumber="171" unit="m" value="32.0">
