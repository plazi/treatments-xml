<taxonomicName id="6C39C8D06268D8392F2394C9EF86B36F" class="Actinopterygii" family="Mugilidae" genus="Mugil" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Mugil hospes" order="Mugiliformes" pageId="14" pageNumber="53" phylum="Chordata" rank="species" species="hospes">Mugil hospes</taxonomicName>
(see 
<bibRefCitation id="5D6F6614185134F0384E59654C329460" author="Richardson, H" journalOrPublisher="Proceedings of the United States National Museum" pageId="40" pageNumber="79" pagination="1 - 89" title="Contributions to the natural history of the Isopoda." url="10.5479/si.00963801.27-1369.657" volume="27" year="1904">Richardson 1904</bibRefCitation>
, 
<bibRefCitation id="F4BADBFA9D5AC2BD86EFFBFFD6A9BAAD" author="Brusca, RC" journalOrPublisher="Bulletin of the Southern California Academy of Sciences" pageId="33" pageNumber="72" pagination="128 - 131" title="Range extensions and new host records of cymothoid Isopods (Isopoda: Cymothoidae) in the Eastern Pacific Ocean." volume="76" year="1977">Brusca 1977</bibRefCitation>
, 
<bibRefCitation id="0D5E14B3EBFFB0D65AEE3BCA51E463ED" author="Brusca, RC" journalOrPublisher="Zoological Journal of the Linnean Society" pageId="33" pageNumber="72" pagination="117 - 199" title="A monograph on the IsopodaCymothoidae (Crustacea) of the eastern Pacific." url="10.1111/j.1096-3642.1981.tb01592.x" volume="73" year="1981">1981</bibRefCitation>
); and from a 
<normalizedToken id="AA708FD48D88506ECEFB04A2AC709C0C" originalValue="“flatfish”">&quot;flatfish&quot;</normalizedToken>
