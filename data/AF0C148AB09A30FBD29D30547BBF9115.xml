<taxonomicName id="E3217D8864A7A7D5F44E28616FE8D5B5" class="Insecta" family="Curculionidae" genus="Trigonopterus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trigonopterus sumbawensis" order="Coleoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="sumbawensis">Trigonopterus sumbawensis</taxonomicName>
