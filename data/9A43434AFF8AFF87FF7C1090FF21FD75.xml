<taxonomicName id="D5EA89DFFF8BFF87FD2117D6FB9BFE30" authority="Zibrowius, 1973" authorityName="Zibrowius" authorityYear="1973" box="[714,1108,453,479]" class="Polychaeta" family="Serpulidae" genus="Bathyvermilia" kingdom="Animalia" order="Sabellida" pageId="7" pageNumber="58" phylum="Annelida" rank="species" species="challengeri">
<emphasis id="209E2E4EFF8BFF87FD2117D6FCB4FE30" box="[714,891,454,479]" italics="true" pageId="7" pageNumber="58">B. challengeri</emphasis>
<bibRefCitation id="767B8FADFF8BFF87FC6F17D5FB9BFE30" author="Zibrowius" box="[900,1108,453,479]" pageId="7" pageNumber="58" refString="Zibrowius, H. W. (1973) Revision of some Serpulidae (Annelida, Polychaeta) from abyssal depths in the Atlantic and Pacific, collected by the &quot; Challenger &quot; and Prince of Monaco Expedition. Bulletin of British Museum of Natural History (Zoology), 24, 427 - 439." type="journal article" year="1973">
Zibrowius, 
<number id="A422E07BFF8BFF87FBF817D5FB9BFE30" box="[1043,1108,453,479]" pageId="7" pageNumber="58" value="1973.0">1973</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="D5EA89DFFF8BFF87FB8A17D6FAE2FE30" box="[1121,1325,454,479]" class="Polychaeta" family="Serpulidae" genus="Bathyvermilia" kingdom="Animalia" order="Sabellida" pageId="7" pageNumber="58" phylum="Annelida" rank="species" species="kupriyanovae">
<emphasis id="209E2E4EFF8BFF87FB8A17D6FAE2FE30" box="[1121,1325,454,479]" italics="true" pageId="7" pageNumber="58">B. kupriyanovae</emphasis>
</taxonomicName>
Bastida- 
<bibRefCitation id="767B8FADFF8BFF87FF7C17FAFEFCFDEB" author="Zavala" box="[151,307,490,516]" pageId="7" pageNumber="58" refString="Bastida-Zavala, J. R. (2008) Serpulids (Annelida: Polychaeta) from the Eastern Pacific, including a short mention of Hawaiian serpulids. Zootaxa, 1722, 1 - 61." type="journal article" year="2008">
Zavala, 
<number id="A422E07BFF8BFF87FF1D17FAFEFCFDEB" box="[246,307,490,516]" pageId="7" pageNumber="58" value="2008.0">2008</number>
</bibRefCitation>
, and 
<taxonomicName id="D5EA89DFFF8BFF87FE9917FBFD37FDEB" authority="Kupriyanova, 1993" authorityName="Kupriyanova" authorityYear="1993" box="[370,760,490,516]" class="Polychaeta" family="Serpulidae" genus="Bathyvermilia" kingdom="Animalia" order="Sabellida" pageId="7" pageNumber="58" phylum="Annelida" rank="species" species="zibrowiusi">
<emphasis id="209E2E4EFF8BFF87FE9917FBFDC3FDEB" box="[370,524,491,516]" italics="true" pageId="7" pageNumber="58">B. zibrowiusi</emphasis>
<bibRefCitation id="767B8FADFF8BFF87FDFF17FAFD37FDEB" author="Kupriyanova" box="[532,760,490,516]" pageId="7" pageNumber="58" refString="Kupriyanova, E. K. (1993) Deep-water Serpulidae (Annelida, Polychaeta) from the Kurile-Kamchatka trench: 2. Genera Bathyditrupa, Bathyvermilia and Protis. Zoologichesky Zhurnal, 72, 21 - 28. (In Russian)" type="journal article" year="1993">
Kupriyanova, 
<number id="A422E07BFF8BFF87FD5717FAFD37FDEB" box="[700,760,490,516]" pageId="7" pageNumber="58" value="1993.0">1993</number>
</bibRefCitation>
</taxonomicName>
) known from the North Pacific Ocean. 
<bibRefCitation id="767B8FADFF8BFF87FB2517FAFA53FDEB" author="Zibrowius" box="[1230,1436,490,516]" pageId="7" pageNumber="58" refString="Zibrowius, H. W. (1973) Revision of some Serpulidae (Annelida, Polychaeta) from abyssal depths in the Atlantic and Pacific, collected by the &quot; Challenger &quot; and Prince of Monaco Expedition. Bulletin of British Museum of Natural History (Zoology), 24, 427 - 439." type="journal article" year="1973">
Zibrowius (
<number id="A422E07BFF8BFF87FABD17FAFA5CFDEB" box="[1366,1427,490,516]" pageId="7" pageNumber="58" value="1973.0">1973</number>
)
</bibRefCitation>
summarized morphological differences for 
<taxonomicName id="D5EA89DFFF8BFF87FD771400FBDBFDC5" authority="Zibrowius, 1973" authorityName="Zibrowius" authorityYear="1973" box="[668,1044,528,554]" class="Polychaeta" family="Serpulidae" genus="Bathyvermilia" kingdom="Animalia" order="Sabellida" pageId="7" pageNumber="58" phylum="Annelida" rank="genus">
