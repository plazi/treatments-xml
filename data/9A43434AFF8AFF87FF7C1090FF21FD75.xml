<taxonomicName id="D5EA89DFFF8BFF87FCE31426FB5FFDA0" authority="Saint-Joseph, 1894" authorityName="Saint-Joseph" authorityYear="1894" box="[776,1168,565,591]" class="Polychaeta" family="Serpulidae" genus="Vermiliopsis" kingdom="Animalia" order="Sabellida" pageId="7" pageNumber="58" phylum="Annelida" rank="genus">
