<taxonomicName id="D5EA89DFFF8BFF87FF0A144BFEBDFD9B" box="[225,370,603,628]" class="Polychaeta" family="Serpulidae" genus="Vermiliopsis" kingdom="Animalia" order="Sabellida" pageId="7" pageNumber="58" phylum="Annelida" rank="genus">
