<taxonomicName id="4C2D19CC7723FFF8E734ACE72B86FA41" authority="Clay and Meinertzhagen 1938:74" authorityName="Clay and Meinertzhagen" authorityPageNumber="74" authorityYear="1938" box="[264,731,1478,1502]" class="Insecta" family="Philopteridae" genus="Picicola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="1" pageNumber="38" phylum="Arthropoda" rank="genus">
