<taxonomicName id="4C2C7491FFFD7B5EFDCBF85F369E0E96" baseAuthorityName="Walker" baseAuthorityYear="1865" box="[525,688,2000,2023]" class="Insecta" family="Noctuidae" genus="Anomis" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="1107" phylum="Arthropoda" rank="species" species="privata">
