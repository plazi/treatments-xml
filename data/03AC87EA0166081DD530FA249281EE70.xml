<taxonomicName id="4C054D7F0166081DD234F98A9201E0D0" authority="Cuvier 1819: 352" authorityName="Cuvier" authorityPageNumber="352" authorityYear="1819" class="Actinopterygii" family="Characidae" genus="Chalceus" kingdom="Animalia" order="Characiformes" pageId="4" pageNumber="633" phylum="Chordata" rank="species" species="fasciatus">
