<paragraph id="0E62F1A8DE6CF1DC0DE2D77D8364FF54" pageNumber="737">
Etymology L 
<taxonomicName id="EE1C5DB2CB0DF88438B5CF1A197DF8B7" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:182069" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole pullula Santschi" lsidName-HNS="Pheidole pullula Santschi" order="Hymenoptera" pageNumber="737" phylum="Arthropoda" rank="species" species="pullula">pullula</taxonomicName>
