<taxonomicName id="B9D782B7CA73F33711F3EF5AD42DFC8D" box="[449,515,826,849]" class="Magnoliopsida" family="Rosaceae" genus="Pyrus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="4" pageNumber="325" phylum="Tracheophyta" rank="genus">
