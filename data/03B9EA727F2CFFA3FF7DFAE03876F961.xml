<taxonomicName id="4C1020E77F2CFFA3FD87F95E3EC4F94D" authority="(Gruvel, 1900)" baseAuthorityName="Gruvel" baseAuthorityYear="1900" box="[621,957,1708,1733]" class="Maxillopoda" family="Scalpellidae" genus="Arcoscalpellum" kingdom="Animalia" order="Pedunculata" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="atlanticum">
