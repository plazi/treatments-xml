<taxonomicName id="B7E34D767A4BBD0FB68FFC58FCBFFC4B" authority="Fabricius, 1780" authorityName="Fabricius" authorityYear="1780" box="[411,829,950,974]" class="Polychaeta" family="Terebellidae" genus="Amphitrite" kingdom="Animalia" order="Terebellida" pageId="22" pageNumber="23" phylum="Annelida" rank="species" species="cincinnatus">
