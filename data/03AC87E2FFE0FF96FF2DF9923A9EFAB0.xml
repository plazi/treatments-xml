<quantity id="4CFD9B11FFE0FF96FD42FB003A46FB39" box="[701,801,1159,1181]" metricMagnitude="4" metricUnit="m" metricValue="5.632704" pageId="91" pageNumber="91" unit="mi" value="35.0">35 miles</quantity>
east of 
