<collectorName id="26F05322FFE0FF96FC6DFC733ABDFBAD" box="[914,986,1012,1033]" pageId="91" pageNumber="91">Howe</collectorName>
</materialsCitation>
; 
<materialsCitation id="3B6D3CA9FFE0FF96FC19FC743D4CFB1F" collectingDate="1908-09-25" collectionCode="AMNH" county="Carina" latitude="-35.18" location="Pinnaroo" longLatPrecision="717" longitude="140.54" municipality="Mathews. Kow Plains" pageId="91" pageNumber="91" specimenCode="AMNH 695262" specimenCount="1" stateProvince="Victoria" typeStatus="paratype">
and 
<emphasis id="B971EAE6FFE0FF96FBE7FC743DABFBAC" bold="true" box="[1048,1228,1011,1032]" pageId="91" pageNumber="91">
<specimenCode id="DBA39E8FFFE0FF96FBE7FC743DABFBAC" box="[1048,1228,1011,1032]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="91" pageNumber="91">AMNH 695262</specimenCode>
