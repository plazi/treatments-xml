<typeStatus id="9872885F476DFFA6FEC8FBF433DF7705" box="[345,452,1076,1100]" pageId="4" pageNumber="347">Holotype</typeStatus>
<taxonomicName id="80C94D7E476DFFA6FE5BFBF430607705" box="[458,635,1076,1100]" class="Insecta" family="Tingidae" genus="Atheas" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="347" phylum="Arthropoda" rank="species" species="birabeni">
