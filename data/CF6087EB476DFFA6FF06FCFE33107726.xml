<taxonomicName id="80C94D7E476DFFA6FED4FBD0300D7761" box="[325,534,1040,1064]" class="Insecta" family="Tingidae" genus="Birabena" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="347" phylum="Arthropoda" rank="species" species="birabeni">
