<emphasis id="1740F9614550EFCC288C1A39B157EA65" italics="true" pageNumber="361">Note:</emphasis>
Although 
<normalizedToken id="DAC476888C1A562D16013187120E00CF" originalValue="Kerguélen">Kerguelen</normalizedToken>
