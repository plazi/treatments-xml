<quantity id="AD0049EBFFAF78064987FA19FEA5FA5A" box="[227,314,1505,1529]" metricMagnitude="3" metricUnit="m" metricValue="1.5" pageId="3" pageNumber="43" unit="m" value="1500.0">1500 m</quantity>
