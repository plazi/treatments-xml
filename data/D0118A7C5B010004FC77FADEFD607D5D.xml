<caption id="0CC76BE25B02001AFF58F99CFA467E1D" ID-DOI="http://doi.org/10.5281/zenodo.5639956" ID-Zenodo-Dep="5639956" httpUri="https://zenodo.org/record/5639956/files/figure.png" pageId="14" pageNumber="250" startId="14.[151,186,1546,1568]" targetBox="[151,1436,257,1505]" targetPageId="14">
<paragraph id="58073B6A5B02001AFF58F99CFA467E1D" blockId="14.[151,1437,1546,1597]" pageId="14" pageNumber="250">
