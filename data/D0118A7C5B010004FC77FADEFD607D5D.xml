<taxonomicName id="9FB840E95B02001AFF2CF99CFE507E3F" baseAuthorityName="Uchida" baseAuthorityYear="1927" box="[227,395,1546,1567]" class="Hydrozoa" family="Pandeidae" genus="Pandeopsis" kingdom="Animalia" order="Anthoathecata" pageId="14" pageNumber="250" phylum="Cnidaria" rank="species" species="ikarii">
