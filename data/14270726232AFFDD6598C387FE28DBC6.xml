13, 
<collectingCountry id="E499F6A0232AFFDD65F0C272FE80DB74" box="[218,300,301,321]" name="Malaysia" pageId="4" pageNumber="85">Malaysia</collectingCountry>
, 
<collectingRegion id="5E4A78D2232AFFDD641EC272FEDADB74" box="[308,374,301,321]" country="Malaysia" name="Pahang" pageId="4" pageNumber="85">Pahang</collectingRegion>
, Cameron Highlands, Tanah Rata env., Gg. Beremban 
<quantity id="5B761BD5232AFFDD667CC272FC39DB74" box="[854,917,301,321]" metricMagnitude="3" metricUnit="m" metricValue="1.4" pageId="4" pageNumber="85" unit="m" value="1400.0">1400m</quantity>
