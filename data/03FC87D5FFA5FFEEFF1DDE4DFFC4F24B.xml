</taxonomicName>
</collectedFrom>
on 
<date id="FFEB1003FFA5FFEEFB23DE81FB66F213" box="[1193,1318,1267,1293]" pageId="17" pageNumber="218" value="1996-03-10">
<collectingDate id="EFAFE9EBFFA5FFEEFB23DE81FB66F213" box="[1193,1318,1267,1293]" pageId="17" pageNumber="218" value="1996-03-10">10.iii.1996</collectingDate>
</date>
