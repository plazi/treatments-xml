(Kusigemati)
</taxonomicName>
(1, 3, 5, 6, 8) and 
<taxonomicName id="4C414D17FFE170472D8A87E5FA660098" authority="(Kusigemati)" authorityName="Watanabe" authorityYear="2019" baseAuthorityName="Kusigemati" baseAuthorityYear="1982" box="[1160,1436,1679,1704]" class="Insecta" family="Ichneumonidae" genus="Xanthocampoplex" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="581" phylum="Arthropoda" rank="species" species="spulerinae">
