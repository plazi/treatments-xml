<emphasis id="B4E39B49FF868535FF26F930FEEBF974" bold="true" box="[136,292,1742,1768]" pageId="33" pageNumber="244">Distribution:</emphasis>
— 
<collectingCountry id="FE8007CBFF868535FEEFF930FE48F974" box="[321,391,1742,1768]" name="Brazil" pageId="33" pageNumber="244">Brazil</collectingCountry>
, 
<collectingCountry id="FE8007CBFF868535FE3CF930FDCCF974" box="[402,515,1742,1768]" name="Colombia" pageId="33" pageNumber="244">Colombia</collectingCountry>
, 
<collectingCountry id="FE8007CBFF868535FDA1F930FDA1F974" box="[527,622,1742,1768]" name="Ecuador" pageId="33" pageNumber="244">Ecuador</collectingCountry>
, 
<collectingCountry id="FE8007CBFF868535FDD7F930FD1CF974" box="[633,723,1742,1768]" name="Guyana" pageId="33" pageNumber="244">Guyana</collectingCountry>
, 
<collectingCountry id="FE8007CBFF868535FD70F931FCDBF975" box="[734,788,1743,1769]" name="Peru" pageId="33" pageNumber="244">Peru</collectingCountry>
, 
<collectingCountry id="FE8007CBFF868535FCB1F930FC45F974" box="[799,906,1742,1768]" name="Suriname" pageId="33" pageNumber="244">Suriname</collectingCountry>
