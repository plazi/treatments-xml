<collectionCode id="46FAAE63FFC8FFFFA3388557DECB9604" box="[853,943,1099,1122]" httpUri="http://grbio.org/cool/49rk-3jpq" name="Zoological Museum, University of Amoy" pageId="51" pageNumber="52">ZMUM</collectionCode>
