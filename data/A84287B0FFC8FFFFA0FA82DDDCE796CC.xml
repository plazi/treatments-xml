<taxonomicName id="E7EB4D25FFC8FFFFA13F8557DF269605" box="[338,578,1099,1123]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="smirnovi">
