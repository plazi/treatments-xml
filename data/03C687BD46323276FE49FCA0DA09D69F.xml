<paragraph id="8BD036AB46323276FF04FA8BDCE8D431" blockId="47.[189,988,1322,1349]" box="[189,988,1322,1349]" pageId="47" pageNumber="48">
<taxonomicName id="4C6F4D2846323276FF04FA8BDD98D430" authority="Guerin-Meneville, 1831" authorityName="Guerin-Meneville" authorityYear="1831" box="[189,684,1322,1349]" class="Insecta" family="Scarabaeidae" genus="Serica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="marmorea">
