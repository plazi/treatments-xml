, 
<taxonomicName id="D1DFB43F0F5A34B54BB97D3AF0B11691" baseAuthorityName="Pfeiffer" baseAuthorityYear="1846" class="Gastropoda" family="Bulimulidae" genus="Naesiotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="46" pageNumber="47" phylum="Mollusca" rank="species" species="darwini">
