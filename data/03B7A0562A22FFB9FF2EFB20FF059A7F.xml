<geoCoordinate id="EE2A77872A22FFBBFF2EFAE6FF569E09" box="[151,224,1315,1335]" degrees="38" direction="north" minutes="24" orientation="latitude" pageId="1" pageNumber="382" precision="925" value="38.4">38°24N</geoCoordinate>
