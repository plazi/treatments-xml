, 
<taxonomicName id="3AF8F5D47B30FF97FBB2EE629012F970" authority="(Smith)" baseAuthorityName="Smith" class="Insecta" family="Noctuidae" genus="Scotogramma" kingdom="Animalia" order="Lepidoptera" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="discolor">
