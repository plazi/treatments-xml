<collectionCode id="ED2079D4F241FFFE021AFC7BFDFA4DE9" box="[596,636,919,937]" country="Germany" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="28" pageNumber="255">SMF</collectionCode>
