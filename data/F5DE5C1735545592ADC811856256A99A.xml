, 
<taxonomicName id="2E5AF287CD16EFE6D139E5ECE79E7981" class="Gastropoda" family="Bulimulidae" genus="Kuschelenia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="127" pageNumber="128" phylum="Mollusca" rank="species" species="bicolor" subGenus="Kuschelenia">
<emphasis id="59383521265B59ACD9BD610889D9A92C" italics="true" pageId="127" pageNumber="128">Kuschelenia (Kuschelenia) bicolor</emphasis>
</taxonomicName>
