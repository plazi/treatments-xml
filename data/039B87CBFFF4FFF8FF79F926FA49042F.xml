<collectingCountry id="F325764DFFF5FFF8FE69FE0DFE170447" box="[391,450,503,525]" name="Brazil" pageId="13" pageNumber="400">Brazil</collectingCountry>
(
<collectingRegion id="49F6F83FFFF5FFF8FE20FE02FDB60447" box="[462,611,503,525]" country="Brazil" name="Sao Paulo" pageId="13" pageNumber="400">São Paulo state</collectingRegion>
)................................................. 
<taxonomicName id="4C324D5EFFF5FFF8FB22FE02FA9C0446" authority="Nascimento &amp; Bressan &amp; Bocakova, 2020" authorityName="Nascimento &amp; Bressan &amp; Bocakova" authorityYear="2020" box="[1228,1353,503,525]" class="Insecta" family="Lycidae" genus="Currhaeus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="400" phylum="Arthropoda" rank="species" species="polegattoi" status="sp. nov.">
<emphasis id="B946EACFFFF5FFF8FB22FE02FA9C0446" box="[1228,1353,503,525]" italics="true" pageId="13" pageNumber="400">C. polegattoi</emphasis>
</taxonomicName>
<emphasis id="B946EACFFFF5FFF8FABEFE02FA490447" bold="true" box="[1360,1436,504,525]" pageId="13" pageNumber="400">
