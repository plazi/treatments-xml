<number id="EE517224FFC0FF89361BF8C51E5FF8BC" box="[1093,1106,1881,1907]" pageId="3" pageNumber="23" value="5.0">5</number>
dL 
<number id="EE517224FFC0FF8936DFF8C51EA2F8BC" box="[1153,1199,1881,1907]" pageId="3" pageNumber="23" value="165.0">165</number>
, vL 
<number id="EE517224FFC0FF8936BAF8C51F09F8BC" box="[1252,1284,1881,1907]" pageId="3" pageNumber="23" value="80.0">80</number>
, dL/vL 
<number id="EE517224FFC0FF89373FF8C51F9AF8BC" box="[1377,1431,1881,1907]" pageId="3" pageNumber="23" value="2.06">2.06</number>
, HB 
<number id="EE517224FFC0FF893296F8E31AE5F856" box="[200,232,1919,1945]" pageId="3" pageNumber="23" value="55.0">55</number>
, dL/HB 3.0, S- 
<number id="EE517224FFC0FF8933FFF8E31BA1F856" box="[417,428,1919,1945]" pageId="3" pageNumber="23" value="1.0">1</number>
L 
<number id="EE517224FFC0FF893390F8E31BF0F856" box="[462,509,1919,1945]" pageId="3" pageNumber="23" value="138.0">138</number>
, L/W 
<number id="EE517224FFC0FF893016F8E31873F856" box="[584,638,1919,1945]" pageId="3" pageNumber="23" value="12.1">12.1</number>
, S- 
<number id="EE517224FFC0FF8930FBF8E318B9F856" box="[677,692,1919,1945]" pageId="3" pageNumber="23" value="2.0">2</number>
L 
<number id="EE517224FFC0FF893088F8E318FBF856" box="[726,758,1919,1945]" pageId="3" pageNumber="23" value="88.0">88</number>
, L/W 
<number id="EE517224FFC0FF89311FF8E31964F856" box="[833,873,1919,1945]" pageId="3" pageNumber="23" value="6.8">6.8</number>
, distance of sword setae of I-Leg- 
<number id="EE517224FFC0FF89375EF8E31F00F856" box="[1280,1293,1919,1945]" pageId="3" pageNumber="23" value="5.0">5</number>
<number id="EE517224FFC0FF89374BF8E31F38F856" box="[1301,1333,1919,1945]" pageId="3" pageNumber="23" value="46.0">46</number>
, L S- 
<number id="EE517224FFC0FF893726F8E31F8FF856" box="[1400,1410,1919,1945]" pageId="3" pageNumber="23" value="1.0">1</number>
/ 
<number id="EE517224FFC0FF8937D2F8E31F91F856" box="[1420,1436,1919,1945]" pageId="3" pageNumber="23" value="2.0">2</number>
<number id="EE517224FFC0FF8932C9F83A1AB2F80F" box="[151,191,1958,1984]" pageId="3" pageNumber="23" value="1.6">1.6</number>
; I-Leg- 
<number id="EE517224FFC0FF893349F83A1B29F80F" box="[279,292,1958,1984]" pageId="3" pageNumber="23" value="6.0">6</number>
L 
<number id="EE517224FFC0FF89331BF83A1B79F80F" box="[325,372,1958,1984]" pageId="3" pageNumber="23" value="156.0">156</number>
, HB 
<number id="EE517224FFC0FF8933EEF83A1BC2F80F" box="[432,463,1958,1984]" pageId="3" pageNumber="23" value="14.0">14</number>
, L/HB 
<number id="EE517224FFC0FF893078F83A1851F80F" box="[550,604,1958,1984]" pageId="3" pageNumber="23" value="11.5">11.5</number>
; L I-Leg- 
<number id="EE517224FFC0FF893093F83A18D7F80F" box="[717,730,1958,1984]" pageId="3" pageNumber="23" value="5.0">5</number>
/ 
<number id="EE517224FFC0FF8930BDF83A18FCF80F" box="[739,753,1958,1984]" pageId="3" pageNumber="23" value="6.0">6</number>
<number id="EE517224FFC0FF8930A6F83A1922F80F" box="[760,815,1958,1984]" pageId="3" pageNumber="23" value="1.06">1.06</number>
.
</paragraph>
<paragraph id="58266003FFC0FF89329BF8511B88F829" blockId="3.[151,1437,1181,2022]" box="[197,389,1996,2022]" pageId="3" pageNumber="23">
