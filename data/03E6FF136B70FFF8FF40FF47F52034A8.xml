<taxonomicName id="4C4F35866B70FFF8FF40FF47F6093798" authority="Kociolek &amp; Danz" authorityName="Kociolek &amp; A. Danz" authorityYear="2022" box="[136,556,157,184]" class="Bacillariophyceae" family="Stephanodiscaceae" genus="Grunowia" higherTaxonomySource="GBIF" kingdom="Chromista" order="Thalassiosirales" pageId="3" pageNumber="298" phylum="Ochrophyta" rank="species" species="mannii" status="sp. nov.">
<emphasis id="B93B92176B70FFF8FF40FF47F5733798" bold="true" box="[136,342,158,184]" italics="true" pageId="3" pageNumber="298">Grunowia mannii</emphasis>
Kociolek &amp; Danz
</taxonomicName>
, 
<taxonomicNameLabel id="A2082F6C6B70FFF8FDF1FF47F6B53798" box="[569,656,158,184]" pageId="3" pageNumber="298" rank="species">
<emphasis id="B93B92176B70FFF8FDF1FF47F6AC3798" bold="true" box="[569,649,158,184]" italics="true" pageId="3" pageNumber="298">sp. nov</emphasis>
