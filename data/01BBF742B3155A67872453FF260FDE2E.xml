<taxonomicName id="53C502294B796B1F2152AA2D6A06256F" LSID="urn:lsid:zoobank.org:act:CFED089B-4226-4FEA-84C2-71481A291F48" class="Demospongiae" family="Calthropellidae" genus="Calthropella" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Calthropella (Calthropella) xavierae" order="Tetractinellida" pageId="53" pageNumber="54" phylum="Porifera" rank="species" species="xavierae" subGenus="Calthropella">Calthropella Calthropella xavierae</taxonomicName>
