<taxonomicName id="07CAC9CA3A63DA44D111375CC4CDE04B" class="Dicotyledoneae" family="Betulaceae" genus="Corylus" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Corylus mandshurica" order="Fagales" pageId="0" pageNumber="19" phylum="Angiospermae" rank="species" species="mandshurica">
