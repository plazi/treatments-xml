<taxonomicName id="13F22FB9FF83BC692D78F9A092E10E50" box="[346,469,1609,1631]" class="Insecta" family="Scirtidae" genus="Eurycyphon" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Coleoptera" pageId="18" pageNumber="469" phylum="Arthropoda" rank="genus">
