<subSubSection id="C370655C5917666EFF5845F33C82FDB1" box="[199,943,511,537]" pageId="8" pageNumber="465" type="etymology">
<paragraph id="8BD536D75917666EFF5845F33C82FDB1" blockId="8.[151,1437,151,717]" box="[199,943,511,537]" pageId="8" pageNumber="465">
<emphasis id="B91EEAC55917666EFF5845F33E63FDB1" bold="true" box="[199,334,511,537]" pageId="8" pageNumber="465">Etymology.</emphasis>
