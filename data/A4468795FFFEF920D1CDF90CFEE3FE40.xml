(A, B) and 
<taxonomicName id="EBEF4D00FFFCF926D26DF83CFBAFF841" authority="Jacot, 1940" authorityName="Jacot" authorityYear="1940" box="[823,1146,1943,1965]" class="Arachnida" family="Galumnidae" genus="Galumna" kingdom="Animalia" order="Sarcoptiformes" pageId="3" pageNumber="584" phylum="Arthropoda" rank="species" species="natalensis">
