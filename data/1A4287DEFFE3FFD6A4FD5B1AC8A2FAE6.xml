<taxonomicName id="55EB4D4BFFFCFFD6A71C586FC878FAE6" authority="Xu, Olmi, Guglielmino &amp; Chen" authorityName="Xu, Olmi, Guglielmino &amp; Chen" authorityYear="1804" box="[886,1350,1373,1394]" class="Insecta" family="Dryinidae" genus="Dryinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="nanlingensis" status="sp. nov.">
<emphasis id="A09FEADAFFFCFFD6A71C586FC932FAE6" box="[886,1036,1373,1394]" italics="true" pageId="16" pageNumber="17">D. nanlingensis</emphasis>
Xu, Olmi, Guglielmino &amp; Chen
</taxonomicName>
, 
<emphasis id="A09FEADAFFFCFFD6A125586FC8A2FAE6" bold="true" box="[1359,1436,1373,1394]" pageId="16" pageNumber="17">
<taxonomicNameLabel id="BBAC57A1FFFCFFD6A125586FC8A2FAE6" box="[1359,1436,1373,1394]" pageId="16" pageNumber="17" rank="species">sp. nov.</taxonomicNameLabel>
