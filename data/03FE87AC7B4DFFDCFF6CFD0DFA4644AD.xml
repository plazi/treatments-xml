<number id="3D9F249D7B4AFFDCFD3DFD6FFD34428D" box="[710,739,674,700]" pageId="21" pageNumber="60" value="23.0">23</number>
(
<specimenCount id="9D51FD337B4AFFDCFD03FD6FFCB0428D" box="[760,871,674,700]" pageId="21" pageNumber="60" type="male">
