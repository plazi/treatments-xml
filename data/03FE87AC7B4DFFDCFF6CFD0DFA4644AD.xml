<collectionCode id="ED46AE7F7B4AFFDCFABCFD3FFA4B433D" LSID="urn:lsid:biocol.org:col:34795" box="[1351,1436,754,780]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="21" pageNumber="60">FMNH</collectionCode>
(
