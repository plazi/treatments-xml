<taxonomicName id="4C124D30FFA6FF8C6786FDD4FD976F10" authorityName="Archibald &amp; Gu &amp; Mathewes" authorityYear="2022" box="[407,520,547,572]" class="Insecta" family="Prophalangopsidae" genus="Ypopteron" kingdom="Animalia" order="Orthoptera" pageId="7" pageNumber="566" phylum="Arthropoda" rank="genus">
