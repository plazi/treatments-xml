<taxonomicName id="C5845F827C3E3827A863583AFC5D1A27" box="[811,995,786,811]" class="Insecta" family="Cicadellidae" genus="Graphocephala" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="genus">
