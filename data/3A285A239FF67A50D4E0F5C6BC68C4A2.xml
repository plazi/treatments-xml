<taxonomicName id="6D998C2494797F9558F7EEFD7A71944D" authority="Foerster, 1862" authorityName="Foerster" authorityYear="1862" class="Insecta" family="Braconidae" genus="Lytopylus" higherTaxonomySource="GBIF" kingdom="Metazoa" lsidName="Lytopylus rebeccashapleyae" order="Hymenoptera" pageId="17" pageNumber="396" phylum="Arthropoda" rank="species" species="rebeccashapleyae">Lytopylus rebeccashapleyae</taxonomicName>
