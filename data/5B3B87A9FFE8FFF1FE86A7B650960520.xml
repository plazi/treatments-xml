</taxonomicName>
e 
<taxonomicName id="14924D3CFFE9FFF1FDFCA5D750DC04C1" authorityName="Ruckes" authorityYear="1964" box="[610,756,1759,1783]" class="Insecta" family="Pentatomidae" genus="Antiteuchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="29" pageNumber="194" phylum="Arthropoda" rank="species" species="cuspidatus">
