<typeStatus id="114D886AFFAB7446FE0AFD8AFDB1FDC3" box="[410,515,549,575]" pageId="21" pageNumber="22">holotype</typeStatus>
.
</emphasis>
Bill length 
<quantity id="090E9B2DFFAB7446FD19FD8AFD5CFDBC" box="[649,750,549,576]" metricMagnitude="-2" metricUnit="m" metricValue="1.27" pageId="21" pageNumber="22" unit="mm" value="12.7">12.7 mm</quantity>
, wing length 
<quantity id="090E9B2DFFAB7446FC13FD8AFC5AFDBC" box="[899,1000,549,576]" metricMagnitude="-2" metricUnit="m" metricValue="8.37" pageId="21" pageNumber="22" unit="mm" value="83.7">83.7 mm</quantity>
