<taxonomicName id="827C5E4BF9801D8F69B7D278A20D9694" authorityName="Lin &amp; Li" authorityYear="2021" class="Arachnida" family="Macrothelidae" genus="Macrothele" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Macrothele emei" order="Araneae" pageId="0" pageNumber="1" phylum="Arthropoda" rank="species" species="emei">
