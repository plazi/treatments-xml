<taxonomicName id="4C164D2CFF9AFFA0FF59FA07BB2CFA77" authority="Gonzalez &amp; Griswold, 2011" authorityName="Gonzalez &amp; Griswold" authorityYear="2011" box="[151,812,1422,1446]" class="Insecta" family="Megachilidae" genus="Heriades" kingdom="Animalia" order="Hymenoptera" pageId="0" pageNumber="75" phylum="Arthropoda" rank="species" species="tayrona" subGenus="Neotrypetes">
