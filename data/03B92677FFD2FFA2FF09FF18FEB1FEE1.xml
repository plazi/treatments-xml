<taxonomicName id="4C10ECE2FFD2FFA2FC08FF5FFB69FF75" box="[918,1277,222,247]" class="Entognatha" family="Campodeidae" genus="Metriocampa" kingdom="Animalia" order="Diplura" pageId="8" pageNumber="305" phylum="Arthropoda" rank="species" species="rileyi" subGenus="Tricampa">
