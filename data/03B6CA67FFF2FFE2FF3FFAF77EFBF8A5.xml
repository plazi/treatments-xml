<specimenCode id="DBB9D30AFFF2FFE2FECFFA3F7EBFFA29" box="[359,504,1449,1475]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="5" pageNumber="116" type="Museum">PAL 641979</specimenCode>
