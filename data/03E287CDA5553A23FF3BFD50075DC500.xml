<taxonomicName id="4C4B4D58A5573A23FDEEFE6E07E8C6C8" box="[578,682,440,464]" class="Insecta" family="Ichneumonidae" genus="Ishigakia" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="177" phylum="Arthropoda" rank="genus">
