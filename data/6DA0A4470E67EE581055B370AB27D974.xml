<taxonomicName id="8A77C56B4ADF5EB21AB21A8B05D47BA0" class="Insecta" family="Apidae" genus="Anthophora" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthophora pulchra" order="Hymenoptera" pageId="44" pageNumber="123" phylum="Arthropoda" rank="species" species="pulchra">Anthophora pulchra</taxonomicName>
