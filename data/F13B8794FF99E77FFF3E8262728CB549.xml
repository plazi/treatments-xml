<taxonomicName id="BE924D01FF9BE77EFE1182BF7076B19B" box="[440,522,1464,1486]" pageId="6" pageNumber="65" rank="species" species="takensis" status="sp. nov.">takensis</taxonomicName>
</emphasis>
</taxonomicName>
<emphasis id="4BE6EA90FF9BE77EFDB982BE701DB19B" bold="true" box="[528,609,1465,1486]" pageId="6" pageNumber="65">
