<taxonomicName id="FD8BE7EE48DC13FD1DEEAC233A5FE152" class="Insecta" family="Coccidae" genus="Coccus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Coccus pseudomagnoliarum" order="Hemiptera" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="pseudomagnoliarum">Coccus pseudomagnoliarum</taxonomicName>
