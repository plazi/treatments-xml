<taxonomicName id="8084F70BBE813E64042DBA69706A628D" authority="Bonvouloir, 1871" authorityName="Bonvouloir" authorityYear="1871" class="Insecta" family="Eucnemidae" genus="Entomophthalmus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Entomophthalmus rufiolus" order="Coleoptera" pageId="6" pageNumber="83" phylum="Arthropoda" rank="species" species="rufiolus">Entomophthalmus rufiolus</taxonomicName>
