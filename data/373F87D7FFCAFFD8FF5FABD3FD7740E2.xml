<quantity id="786E9B24FFCAFFDBFC44AF1BFC7846EC" box="[908,1009,1901,1926]" metricMagnitude="-2" metricUnit="m" metricValue="2.85" pageId="85" pageNumber="86" unit="mm" value="28.5">28.5 mm</quantity>
