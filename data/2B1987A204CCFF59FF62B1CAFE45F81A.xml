<collectionCode id="C5A1AE7104CCFF5EFC2CB685FBECFB64" box="[985,1072,1250,1276]" country="USA" httpUri="http://grbio.org/cool/m2jc-wmeq" name="University of California, Riverside" pageId="163" pageNumber="164">UCRC</collectionCode>
