<location id="8E9F602FFF7C2A9BFDEAD44FFD81FD9C" LSID="urn:lsid:plazi:treatment:03E987E2FF7D2A9BFF3ED29FFC57FAB4:8E9F602FFF7C2A9BFDEAD44FFD81FD9C" box="[579,608,513,539]" country="Yemen" county="Tibial" municipality="Paramere" name="Rs" pageId="211" pageNumber="212">Rs</location>
