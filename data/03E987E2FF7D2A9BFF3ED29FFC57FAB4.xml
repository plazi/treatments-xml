<location id="8E9F602FFF7C2A9BFB92D793FB47FE70" LSID="urn:lsid:plazi:treatment:03E987E2FF7D2A9BFF3ED29FFC57FAB4:8E9F602FFF7C2A9BFB92D793FB47FE70" box="[1083,1190,477,503]" country="Yemen" county="Tibial" municipality="Paramere" name="Forewing" pageId="211" pageNumber="212">Forewing</location>
