<location id="8ED2EEE5FFECA768FD1FF913B26FF8AA" LSID="urn:lsid:plazi:treatment:03A40928FFECA768FF5FF95AB3ACF872:8ED2EEE5FFECA768FD1FF913B26FF8AA" box="[727,808,1771,1797]" country="India" municipality="The" name="Aizawl" pageId="1" pageNumber="384" stateProvince="Mizoram">Aizawl</location>
city (
<specimenCode id="DBAB1045FFECA768FCA1F913B541F8AA" box="[873,1030,1771,1797]" collectionCode="MZUHC" pageId="1" pageNumber="384">MZUHC 331</specimenCode>
). 
<collectingMunicipality id="6BD62244FFECA768FBD1F913B502F8AA" box="[1049,1093,1771,1797]" pageId="1" pageNumber="384">The</collectingMunicipality>
samples from 
<collectingRegion id="49C976DCFFECA768FB25F913B410F8AA" box="[1261,1367,1771,1797]" country="India" name="Mizoram" pageId="1" pageNumber="384">Mizoram</collectingRegion>
