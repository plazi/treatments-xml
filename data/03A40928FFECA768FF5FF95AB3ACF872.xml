<collectorName id="26F8DDE8FFECA768FB7BF93FB462F94E" box="[1203,1317,1735,1761]" pageId="1" pageNumber="384">Hmuifang</collectorName>
(
<specimenCode id="DBAB1045FFECA768FAFDF93FB183F8AA" collectionCode="MZUHC" pageId="1" pageNumber="384">MZUHC 324</specimenCode>
, 
<specimenCode id="DBAB1045FFECA768FF18F913B02FF8AA" box="[208,360,1770,1797]" collectionCode="MZUHC" pageId="1" pageNumber="384">MZUHC 325</specimenCode>
