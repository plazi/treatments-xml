<taxonomicName id="4C0DC3BDFFECA768FB61F867B42AF817" authorityName="Wu, Suwannapoom, Xu, Murphy, and Che" authorityYear="2019" box="[1193,1389,1951,1976]" class="Amphibia" family="Rhacophoridae" genus="Raorchestes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="1" pageNumber="384" phylum="Chordata" rank="species" species="cangyuanensis">
