<taxonomicName id="4C70B791FFAEB653FED9B02BD0BCF9D7" box="[269,431,1593,1617]" class="Aves" family="Cathartidae" genus="Sarcoramphus" kingdom="Animalia" order="Accipitriformes" pageId="3" pageNumber="582" phylum="Chordata" rank="genus">
