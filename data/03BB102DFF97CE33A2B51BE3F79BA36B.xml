(
<taxonomicName id="4C12DAB8FF94CE33A0931B48F44AA2B3" box="[689,803,1643,1667]" class="Magnoliopsida" family="Moraceae" kingdom="Plantae" order="Rosales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="family">Moraceae</taxonomicName>
