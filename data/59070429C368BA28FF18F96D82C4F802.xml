<taxonomicName id="16AECEBCC368BA28FF18F96D831FF924" authority="Gmelin, 1790: 2220" authorityName="Gmelin, 1790:" authorityPageNumber="2220" authorityYear="2220" box="[151,515,1744,1770]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="farinosus">
<emphasis id="E3DA692DC368BA28FF18F96D8015F924" bold="true" box="[151,265,1744,1770]" italics="true" pageId="20" pageNumber="21">farinosus</emphasis>
<bibRefCitation id="B53FC8CEC368BA28FE9FF96D831FF924" author="Gmelin, J. F." box="[272,515,1744,1770]" pageId="20" pageNumber="21" pagination="1517 - 2224" refId="ref30131" refString="Gmelin, J. F. (1790) Systema Naturae. Editio decima tertia. Acuta, reformata, cara. Lipsiae. Pages 1517 - 2224." type="book chapter" year="1790">Gmelin, 1790: 2220</bibRefCitation>
</taxonomicName>
</treatmentCitation>
, 
<taxonomicName id="16AECEBCC368BA28FD9FF96D8374F924" authorityName="Linnaeus" authorityYear="1758" box="[528,616,1744,1770]" class="Insecta" family="Coccidae" genus="Coccus" kingdom="Animalia" order="Hemiptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="genus">
<emphasis id="E3DA692DC368BA28FD9FF96D8374F924" bold="true" box="[528,616,1744,1770]" italics="true" pageId="20" pageNumber="21">Coccus</emphasis>
</taxonomicName>
</emphasis>
