Although 
<taxonomicName id="4C0974AE64AEFF1BFED0FA3BFE9D52AE" authorityName="Linnaeus" authorityYear="1758" box="[264,348,1453,1476]" class="Mammalia" family="Sciuridae" genus="Sciurus" kingdom="Animalia" order="Rodentia" pageId="164" pageNumber="165" phylum="Chordata" rank="genus">
