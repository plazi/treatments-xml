<paragraph id="E1501C1C912F074BFEA0FEDEFD7001EE" blockId="12.[264,1323,290,489]" pageId="12" pageNumber="13">
<typeStatus id="3E54A2BE912F074BFEA0FEDEFEBD0194" box="[264,378,377,403]" pageId="12" pageNumber="13">Paratypes</typeStatus>
of 
<taxonomicName id="26EF679F912F074BFE0BFEDEFDC10195" box="[419,518,377,402]" class="Insecta" family="Aphididae" genus="Macrosiphoniella" kingdom="Animalia" order="Aphidomorpha" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="aqua">
