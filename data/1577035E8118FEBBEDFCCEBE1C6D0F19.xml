<quantity id="5A261FAD8118FEBBEFD6CF651A590F18" box="[701,772,332,356]" metricMagnitude="0" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="284" pageNumber="285" unit="m" value="1.5" valueMax="3.0" valueMin="0.0">0–3 m</quantity>
