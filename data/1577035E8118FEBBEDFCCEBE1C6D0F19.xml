<emphasis id="AFAA6E5A8118FEBBEDACCF2D1B5D0F61" bold="true" box="[199,512,260,285]" pageId="284" pageNumber="285">Distribution and habitat.</emphasis>
<collectingRegion id="5F1A7CAA8118FEBBEF65CF2C1BF80F61" box="[526,677,260,285]" country="Papua New Guinea" name="New Ireland" pageId="284" pageNumber="285">New Ireland</collectingRegion>
