) and 
<taxonomicName id="02B53945FF569E8DA58A07B07C58BC82" class="Liliopsida" family="Heliconiaceae" genus="Heliconia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Heliconia stricta" order="Zingiberales" pageId="53" pageNumber="54" phylum="Tracheophyta" rank="species" species="stricta">Heliconia stricta</taxonomicName>
