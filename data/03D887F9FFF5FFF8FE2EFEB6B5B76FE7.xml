<taxonomicName id="4C714D6CFFF7FFF7FEF9F9BDB57D6F4A" authority="Cho &amp; Humphreys, 2010" authorityName="Cho &amp; Humphreys" authorityYear="2010" box="[316,596,1564,1586]" class="Malacostraca" family="Parabathynellidae" genus="Brevisomabathynella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Bathynellacea" pageId="6" pageNumber="997" phylum="Arthropoda" rank="species" species="magna" status="sp. nov.">
