<paragraph pageId="13" pageNumber="96">
<taxonomicName authority="Correa, 1950" authorityName="Correa" authorityYear="1950" class="Hoplonemertea" family="Ototyphlonemertidae" genus="Ototyphlonemertes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ototyphlonemertes erneba" order="Monostilifera" pageId="13" pageNumber="96" phylum="Nemertea" rank="species" species="erneba">
Ototyphlonemertes erneba (
