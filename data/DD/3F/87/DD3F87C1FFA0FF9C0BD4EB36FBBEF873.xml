<heading bold="true" box="[151,696,1661,1687]" fontSize="11" level="1" pageId="3" pageNumber="34" reason="1">
<taxonomicName authority="Rosenhauer, 1847" authorityName="Rosenhauer" authorityYear="1847" box="[151,696,1661,1687]" class="Insecta" family="Curculionidae" genus="Gymnetron" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="34" phylum="Arthropoda" rank="species" species="haemorrhoum">
<emphasis bold="true" box="[151,696,1661,1687]" pageId="3" pageNumber="34">
<emphasis bold="true" box="[151,469,1661,1687]" italics="true" pageId="3" pageNumber="34">Gymnetron haemorrhoum</emphasis>
<bibRefCitation author="Rosenhauer" box="[476,696,1661,1687]" pageId="3" pageNumber="34" refString="Rosenhauer, W. G. (1847) Beitrage zur Insekten-Fauna Europas, Vol. I, Erlangen, i - x + 1 - 160 pp." type="book chapter" year="1847">
Rosenhauer, 
<number box="[638,696,1661,1687]" pageId="3" pageNumber="34" value="1847.0">1847</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
</heading>
</paragraph>
