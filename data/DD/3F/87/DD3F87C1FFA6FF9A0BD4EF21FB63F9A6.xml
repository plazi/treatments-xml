<taxonomicName authority="Schrader" authorityName="Schrader" box="[868,1142,1458,1483]" class="Magnoliopsida" family="Scrophulariaceae" genus="Verbascum" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="36" phylum="Tracheophyta" rank="species" species="thapsiforme">
