</taxonomicName>
, and 
<quantity box="[1017,1107,176,200]" metricMagnitude="-1" metricUnit="m" metricValue="8.636" metricValueMax="8.89" metricValueMin="8.382" pageId="1" pageNumber="398" unit="in" value="34.0" valueMax="35.0" valueMin="33.0">33-35 in</quantity>
