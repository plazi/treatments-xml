<paragraph blockId="1.[136,747,465,553]" box="[136,604,531,553]" pageId="1" pageNumber="104">
<typeStatus box="[136,184,532,553]" pageId="1" pageNumber="104">Type</typeStatus>
species: 
<taxonomicName authority="Rondani." authorityName="Rondani." box="[272,604,531,553]" class="Insecta" family="Cecidomyiidae" genus="Brachineura" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="104" phylum="Arthropoda" rank="species" species="fuscogrisea">
