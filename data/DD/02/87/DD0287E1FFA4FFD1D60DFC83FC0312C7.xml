<quantity metricMagnitude="-4" metricUnit="m" metricValue="8.5" metricValueMax="13.0" metricValueMin="4.0" pageId="12" pageNumber="151" unit="mm" value="0.85" valueMax="1.3" valueMin="0.4">0,4- 1,3 mm</quantity>
