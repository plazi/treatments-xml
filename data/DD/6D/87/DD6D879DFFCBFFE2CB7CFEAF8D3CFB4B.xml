<subSubSection box="[160,845,944,1005]" pageId="13" pageNumber="1261" type="etymology">
<paragraph blockId="13.[160,845,944,1005]" box="[160,285,944,970]" pageId="13" pageNumber="1261">
<emphasis bold="true" box="[160,285,944,970]" italics="true" pageId="13" pageNumber="1261">Etymology</emphasis>
</paragraph>
<paragraph blockId="13.[160,845,944,1005]" box="[160,845,981,1005]" pageId="13" pageNumber="1261">
