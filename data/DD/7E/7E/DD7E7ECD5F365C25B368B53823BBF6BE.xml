<subSubSection pageId="0" pageNumber="17" type="nomenclature">
<paragraph pageId="0" pageNumber="17">
<taxonomicName LSID="DD7E7ECD-5F36-5C25-B368-B53823BBF6BE" authority="Maddison, 2019" authorityName="Maddison" authorityYear="2019" class="Arachnida" family="Salticidae" genus="Amilaps" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Amilaps" order="Araneae" pageId="0" pageNumber="17" phylum="Arthropoda" rank="genus">Amilaps</taxonomicName>
