<paragraph blockId="12.[236,1220,627,989]" box="[414,419,950,989]" pageId="12" pageNumber="157">.</paragraph>
<subSubSection pageId="12" pageNumber="157" type="materials_examined">
<paragraph blockId="12.[232,1214,1090,1822]" pageId="12" pageNumber="157">
