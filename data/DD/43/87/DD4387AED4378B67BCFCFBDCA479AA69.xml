<emphasis bold="true" box="[288,306,1988,2012]" pageId="6" pageNumber="71">D</emphasis>
, diagonal view; 
<emphasis bold="true" box="[475,492,1988,2012]" pageId="6" pageNumber="71">E</emphasis>
, parameres dorsal and lateral view; 
<emphasis bold="true" box="[858,873,1988,2012]" pageId="6" pageNumber="71">F</emphasis>
