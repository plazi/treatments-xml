<treatmentCitation id="A506A31AFFF18301FE385BE5FD3CFC66" author="Slipinski" box="[508,684,901,925]" journal="Mem. Mus. Hist. Nat." page="242" pageId="76" pageNumber="76" score="104.46155821149189" volume="7" year="1821">
