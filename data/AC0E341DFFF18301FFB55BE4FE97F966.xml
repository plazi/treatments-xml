<taxonomicName id="E3A7FE88FFF18301FF1D5E24FE52F9A6" box="[217,450,1604,1629]" class="Magnoliopsida" family="Brassicaceae" genus="Anchonium" kingdom="Plantae" order="Brassicales" pageId="76" pageNumber="76" phylum="Tracheophyta" rank="species" species="tournefortii">Anchonium tournefortii</taxonomicName>
