<tr id="ECC134D69B7AF8E4DCF1FB943DAEFB8B" box="[171,955,1110,1130]" gridrow="1" pageId="22" pageNumber="489" rowspan-0="1">
<td id="AF105DAA9B7AF8E4DD17FB943CFBFB8B" box="[333,750,1110,1130]" gridcol="1" gridrow="1" pageId="22" pageNumber="489">premier anneau radial:</td>
