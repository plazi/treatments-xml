<taxonomicName id="1BD44D49FF80FFBFFB3113A0F35CD57C" box="[1208,1300,1530,1554]" class="Liliopsida" family="Araceae" kingdom="Plantae" order="Alismatales" pageId="13" pageNumber="49" phylum="Tracheophyta" rank="family">Araceae</taxonomicName>
