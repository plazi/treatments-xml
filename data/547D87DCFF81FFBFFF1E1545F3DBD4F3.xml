<taxonomicName id="1BD44D49FF80FFBFFADF13A3F6BBD55B" class="Liliopsida" family="Araceae" genus="Philodendron" kingdom="Plantae" order="Alismatales" pageId="13" pageNumber="49" phylum="Tracheophyta" rank="genus">
