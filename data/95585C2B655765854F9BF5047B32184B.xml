<taxonomicName id="2F1B39A6278C5A4690B8A40C1602A3AE" LSID="http://zoobank.org/46E7CF19-5855-4F46-A307-1AEAE06FF76D" authority="Marsh" class="Insecta" family="Braconidae" genus="Heterospilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Heterospilus veintitres" order="Hymenoptera" pageId="422" pageNumber="423" phylum="Arthropoda" rank="species" species="veintitres">Heterospilus veintitres Marsh</taxonomicName>
