<taxonomicName id="A0C01EAFFF96FFDAFE01FB18FD83FB6E" authorityName="Ashmead" authorityYear="1904" box="[482,623,1171,1197]" class="Insecta" family="Eupelmidae" genus="Anastatus" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="398" phylum="Arthropoda" rank="species" species="japonicus">
