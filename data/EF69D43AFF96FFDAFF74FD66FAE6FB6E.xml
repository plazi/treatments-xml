<taxonomicName id="A0C01EAFFF96FFDAFD89FBE4FD0AFB4A" authorityName="Bolivar y Pieltain" authorityYear="1925" box="[618,742,1135,1161]" class="Insecta" family="Eupelmidae" genus="Anastatus" kingdom="Animalia" order="Hymenoptera" pageId="47" pageNumber="398" phylum="Arthropoda" rank="species" species="tenuipes">
