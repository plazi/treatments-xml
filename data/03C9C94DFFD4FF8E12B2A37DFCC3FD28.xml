<taxonomicNameLabel id="A2271932FFD7FF8F16E4A35FFA87FA73" box="[1217,1297,1406,1430]" pageId="14" pageNumber="345" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, holotype, ♂; 15, 
<taxonomicName id="4C6003D8FFD7FF8F1298A382FEF4FA5F" authority="Wang &amp; Zhu, 2020" authorityName="Wang &amp; Zhu" authorityYear="2020" box="[189,354,1442,1466]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="345" phylum="Arthropoda" rank="species" species="longaedeaga" status="sp. nov.">
<emphasis id="B914A449FFD7FF8F1298A382FF44FA5F" box="[189,210,1443,1466]" italics="true" pageId="14" pageNumber="345">M</emphasis>
