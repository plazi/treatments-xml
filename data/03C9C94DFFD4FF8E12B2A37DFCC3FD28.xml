<taxonomicName id="4C6003D8FFD7FF8F10B5A35EFCB4FA73" authority="Wang &amp; Zhu, 2020" authorityName="Wang &amp; Zhu" authorityYear="2020" box="[656,802,1406,1430]" class="Insecta" family="Cosmopterigidae" genus="Meleonoma" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="345" phylum="Arthropoda" rank="species" species="anisodonta" status="sp. nov.">
