<taxonomicName id="88C51DB7B905FFBEFEA7F9B7FE3A1345" box="[366,485,1605,1630]" class="Insecta" family="Lycidae" genus="Cautires" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="37" phylum="Arthropoda" rank="species" species="jendeki" status="sp. n.">
<emphasis id="7DB1BA26B905FFBEFEA7F9B7FE3A1345" box="[366,485,1605,1630]" italics="true" pageId="9" pageNumber="37">C. jendeki</emphasis>
</taxonomicName>
