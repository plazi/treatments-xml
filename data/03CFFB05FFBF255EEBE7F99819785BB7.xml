<taxonomicName id="4C663190FFBF255EEBE7F9981F555AF3" authority="Davis, 1919" authorityName="Davis" authorityYear="1919" box="[151,586,1663,1688]" class="Insecta" family="Mantidae" genus="Stagmomantis" kingdom="Animalia" order="Mantodea" pageId="14" pageNumber="515" phylum="Arthropoda" rank="species" species="floridensis">
<emphasis id="B9129601FFBF255EEBE7F9981F555AF3" bold="true" box="[151,586,1663,1688]" pageId="14" pageNumber="515">
<emphasis id="B9129601FFBF255EEBE7F9981CA45AF3" bold="true" box="[151,443,1663,1688]" italics="true" pageId="14" pageNumber="515">Stagmomantis floridensis</emphasis>
Davis, 
<number id="3DAE5834FFBF255EE962F9981F555AF3" box="[530,586,1663,1688]" pageId="14" pageNumber="515" value="1919.0">1919</number>
</emphasis>
</taxonomicName>
