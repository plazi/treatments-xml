</paragraph>
<paragraph id="8BDD36BA9B185F1D77CAFD585D265590" blockId="1.[151,1143,651,674]" box="[151,1143,651,674]" pageId="1" pageNumber="269">
<taxonomicName id="4C624D399B185F1D77CAFD585B1E5590" authority="Bigot, 1857: 285" authorityName="Bigot" authorityPageNumber="285" authorityYear="1857" box="[151,591,651,674]" class="Insecta" family="Xylophagidae" genus="Heterostomus" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="269" phylum="Arthropoda" rank="species" species="curvipalpis">
