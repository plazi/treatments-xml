<taxonomicName id="4C064D55FFFCD822FD911CADFBAB77A8" authority="(Bolivar, 1909)" authorityName="Bolivar" authorityYear="1909" baseAuthorityName="Bolivar" baseAuthorityYear="1909" box="[571,1029,151,176]" class="Insecta" family="Tetrigidae" genus="Bolivaritettix" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="21" pageNumber="324" phylum="Arthropoda" rank="species" species="sikkinensis">
