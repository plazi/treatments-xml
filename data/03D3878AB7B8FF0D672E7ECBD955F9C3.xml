species: 
<taxonomicName id="4C7A4D1FB7B8FF0D66297952D965FBE1" authority="(Thaxt.) I. I. Tav." box="[893,1301,1119,1146]" class="Laboulbeniomycetes" family="Laboulbeniaceae" genus="Diphymyces" kingdom="Fungi" order="Laboulbeniales" pageId="233" pageNumber="234" phylum="Ascomycota" rank="species" species="appendiculatus">
