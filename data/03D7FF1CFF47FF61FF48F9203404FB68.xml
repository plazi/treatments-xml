<taxonomicName id="4C7E3589FF47FF61FC80F9D2359EFD23" authority="Soula and Curoe, 2005" authorityName="Soula and Curoe" authorityYear="2005" box="[776,1250,699,723]" class="Insecta" family="Rutelidae" genus="Lagochile" kingdom="Animalia" order="Coleoptera" pageId="140" pageNumber="139" phylum="Arthropoda" rank="subSpecies" species="sparsa" subSpecies="curoei">
