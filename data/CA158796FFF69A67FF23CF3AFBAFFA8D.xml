(
<figureCitation id="DA872A05FFF69A66FF2BCF50FEB0F9B5" box="[200,279,1612,1639]" captionStart="Figures 49–54" captionStartId="45.[192,275,1465,1489]" captionTargetBox="[212,1428,225,1424]" captionTargetId="figure-11@45.[192,1440,210,1434]" captionTargetPageId="45" captionText="Figures 49–54. Adults. 49a) Homophoberia australis ♂, holotype, North Port, FL. 49b) Homophoberia australis ♀, paratype, North Port, FL. 49c) Homophoberia australis ♀, paratype, North Port, FL. 50) Leucania elephas ♂, paratype, Bahia Honda, FL. 51) Euscirrhopterus poeyi ♀, No Name Key, FL. 52) Euscirrhopterus argentata ♀, Sonora, MX. 53) Pyreferra slotteni ♂, holotype, Gainesville, FL. 54) Pyreferra pettiti ♂, Cayuga, ON." figureDoi="http://doi.org/10.5281/zenodo.4585798" httpUri="https://zenodo.org/record/4585798/files/figure.png" pageId="36" pageNumber="35">Fig. 53</figureCitation>
, 
<figureCitation id="DA872A05FFF69A66FEC1CF50FEEBF9B4" box="[290,332,1612,1638]" captionStart="Figures 100–104" captionStartId="53.[192,275,1838,1862]" captionTargetBox="[192,1432,264,1740]" captionTargetId="figure-10@53.[192,1440,213,1806]" captionTargetPageId="53" captionText="Figures 100–104. Male genitalia. 100) Leucania elephas, paratype, Bahia Honda, FL. 101) Leucania februalis, San Diego, CA. 102) Leucania humidicola, Calabozo, Venezuela. 103) Pyreferra slotteni, holotype, Gainesville, FL. 104) Pyreferra pettiti, Cayuga, ON." figureDoi="http://doi.org/10.5281/zenodo.4585818" httpUri="https://zenodo.org/record/4585818/files/figure.png" pageId="36" pageNumber="35">103</figureCitation>
, 
<figureCitation id="DA872A05FFF69A66FEB4CF50FE26F9B4" box="[343,385,1612,1638]" captionStart="Figures 132–138" captionStartId="57.[192,275,1596,1620]" captionTargetBox="[192,1318,234,1551]" captionTargetId="figure-73@57.[192,1440,213,1564]" captionTargetPageId="57" captionText="Figures 132–138. Female genitalia. 132) Litoprosopus linea, Islamorada, FL. 133) Tripudia calusa, paratype, Islamorada, FL. 134) Catabenoides insularis, paratype, Key Largo, FL. 135) Homophoberia australis, paratype, Highlands County, FL. 136) Diastema leo, paratype, Highlands County, FL. 137) Leucania elephas, paratype, Bahia Honda, FL. 138) Pyreferra slotteni, paratype, Grenada County, MS." figureDoi="http://doi.org/10.5281/zenodo.4585830" httpUri="https://zenodo.org/record/4585830/files/figure.png" pageId="36" pageNumber="35">138</figureCitation>
