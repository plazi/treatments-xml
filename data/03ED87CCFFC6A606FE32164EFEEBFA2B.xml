<paragraph id="8BFB36DAFFC6A606FE32164EFD79FE9B" blockId="63.[411,716,307,363]" box="[411,716,307,331]" pageId="63" pageNumber="118">
<heading id="D0B381B6FFC6A606FE32164EFD79FE9B" box="[411,716,307,331]" centered="true" fontSize="10" level="2" pageId="63" pageNumber="118" reason="3">
<taxonomicName id="4C444D59FFC6A606FE32164EFD33FE9A" authority="Burckhardt &amp; Basset, 2000" authorityName="Burckhardt &amp; Basset" authorityYear="2000" box="[411,646,307,331]" class="Insecta" family="Psyllidae" genus="Calophya" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="63" pageNumber="118" phylum="Arthropoda" rank="species" species="patagonica" status="sp. n.">
<emphasis id="B930EAC8FFC6A606FE32164EFD33FE9A" bold="true" box="[411,646,307,331]" italics="true" pageId="63" pageNumber="118">Calophya patagonica</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A20357B3FFC6A606FD26164EFD79FE9B" box="[655,716,307,330]" pageId="63" pageNumber="118" rank="species">sp. n.</taxonomicNameLabel>
