<taxonomicName id="F900284B13697A78DF3F3877C6485675" class="Insecta" family="Melolonthidae" genus="Serica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Serica gestroi" order="Coleoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="gestroi">Serica gestroi</taxonomicName>
