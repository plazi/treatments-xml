<paragraph id="8BE48845FF8EFF8B00EDE3C7FCE8FA2C" blockId="26.[177,1085,1257,1425]" pageId="26" pageNumber="1039">
<taxonomicName id="4C5BF3C6FF8EFF8B00EDE3C7FE95FB42" authorityName="Haeselbarth" authorityYear="2008" box="[177,341,1257,1279]" class="Insecta" family="Braconidae" genus="Perilitus" kingdom="Animalia" order="Hymenoptera" pageId="26" pageNumber="1039" phylum="Arthropoda" rank="species" species="alticae">
