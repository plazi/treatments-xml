(
<collectionCode id="ED6F89C3FFFEFFF7CC068A61A19FF940" LSID="urn:lsid:biocol.org:col:34871" box="[583,675,1652,1678]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="14" pageNumber="37">USNM</collectionCode>
