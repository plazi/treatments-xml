<taxonomicName id="957D4D0AC516B2DE25BEECE11DDB67A5" authorityName="Wood &amp; Bright" authorityYear="1992" class="Insecta" family="Cleridae" genus="Chaetophloeus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="43" pageNumber="44" phylum="Arthropoda" rank="species" species="parkinsoniae">
