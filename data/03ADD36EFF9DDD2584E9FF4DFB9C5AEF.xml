<taxonomicName id="4C0419FBFF9DDD2580A6FC22FAB75A3B" box="[1240,1372,1016,1041]" class="Magnoliopsida" family="Ranunculaceae" genus="Ranunculus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Ranunculales" pageId="5" pageNumber="312" phylum="Tracheophyta" rank="genus">
<emphasis id="B970BE6AFF9DDD2580A6FC22FAB75A3B" box="[1240,1372,1016,1041]" italics="true" pageId="5" pageNumber="312">Ranunculus</emphasis>
</taxonomicName>
, and 
<taxonomicName id="4C0419FBFF9DDD2584E9FBC6FEFC5A1F" box="[151,279,1052,1077]" class="Magnoliopsida" family="Lentibulariaceae" genus="Utricularia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Lamiales" pageId="5" pageNumber="312" phylum="Tracheophyta" rank="genus">
<emphasis id="B970BE6AFF9DDD2584E9FBC6FEFC5A1F" box="[151,279,1052,1077]" italics="true" pageId="5" pageNumber="312">Utricularia</emphasis>
