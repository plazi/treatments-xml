(
<bibRefCitation id="EF951F89FF9DDD258558FBC6FE5F5A1F" box="[294,436,1052,1077]" pageId="5" pageNumber="312" refId="ref6040" refString="Bailey, J. W. (1855) Notes on new species and localities of microscopical organisms. Smithsonian contributions to knowledge, 7 (Ar. III), 3, 1 - 16." type="journal article">Bailey 1855</bibRefCitation>
