<paragraph id="8BA8369902074429FEA2FA5AFD0EE05F" blockId="3.[313,1269,669,825]" pageId="3" pageNumber="145">
<taxonomicName id="4C174D1A02074429FEA2FA5AFD28E078" authority="Grichanov, 1995: 125" authorityName="Grichanov" authorityPageNumber="125" authorityYear="1995" box="[313,734,775,798]" class="Insecta" family="Dolichopodidae" genus="Olegonegrobovia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="145" phylum="Arthropoda" rank="genus">
<emphasis id="B963EA8B02074429FEA2FA5AFE16E07B" box="[313,480,776,797]" italics="true" pageId="3" pageNumber="145">Olegonegrobovia</emphasis>
Grichanov, 1995: 125
</taxonomicName>
<emphasis id="B963EA8B02074429FD76FA5AFCBBE07B" bold="true" box="[749,845,776,797]" pageId="3" pageNumber="145">syn. nov.</emphasis>
<typeStatus id="54AC883B02074429FCC6FA5AFC66E07B" box="[861,912,776,797]" pageId="3" pageNumber="145">Type</typeStatus>
