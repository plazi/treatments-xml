<caption id="0BCEA6A1EA2E0C3BFF1CFA3AF5F85532" ID-DOI="http://doi.org/10.5281/zenodo.5310987" ID-Zenodo-Dep="5310987" httpUri="https://zenodo.org/record/5310987/files/figure.png" pageId="49" pageNumber="50" startId="49.[151,272,1515,1539]" targetBox="[352,1232,812,1480]" targetPageId="49">
<paragraph id="5F0EF629EA2E0C3BFF1CFA3AF5F85532" blockId="49.[151,1436,1515,1573]" pageId="49" pageNumber="50">
