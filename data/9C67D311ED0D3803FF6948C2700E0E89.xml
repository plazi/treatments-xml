<quantity id="D336CFE2ED0D3800FE6B49E270EF0EB3" box="[405,472,439,463]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" pageId="17" pageNumber="118" unit="mm" value="3.0">3 mm</quantity>
