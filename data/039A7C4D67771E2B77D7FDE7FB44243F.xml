) (
<taxonomicName id="4C33B6D867771E2B733EFA47FB6227BF" box="[1065,1236,1507,1531]" class="Liliopsida" family="Bromeliaceae" kingdom="Plantae" order="Poales" pageId="11" pageNumber="10" phylum="Tracheophyta" rank="family">Bromeliaceae</taxonomicName>
).
</paragraph>
