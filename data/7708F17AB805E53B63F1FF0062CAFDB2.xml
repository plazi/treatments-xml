<taxonomicName id="38A13BEFB805E53B63D9FDB8622BFD66" authorityName="Williston" authorityYear="1885" box="[191,327,543,569]" class="Insecta" family="Asilidae" genus="Erax" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="dubius">
<emphasis id="CDD59C7EB805E53B63D9FDB8622BFD66" box="[191,327,543,569]" italics="true" pageId="39" pageNumber="40">Erax dubius</emphasis>
