<taxonomicName id="38A13BEFB805E53B61C6FD876059FD66" baseAuthorityName="Williston" baseAuthorityYear="1885" box="[672,821,543,569]" class="Insecta" family="Asilidae" genus="Efferia" kingdom="Animalia" order="Diptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="dubia">
<emphasis id="CDD59C7EB805E53B61C6FD876059FD66" box="[672,821,543,569]" italics="true" pageId="39" pageNumber="40">Efferia dubia</emphasis>
