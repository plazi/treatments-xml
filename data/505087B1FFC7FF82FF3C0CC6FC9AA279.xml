<taxonomicName id="1FF94D24FFC6FF82FF090C9BFE14A1B3" box="[185,397,1469,1495]" class="Insecta" family="Haliplidae" genus="Peltodytes" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="260" phylum="Arthropoda" rank="species" species="edentulus">
