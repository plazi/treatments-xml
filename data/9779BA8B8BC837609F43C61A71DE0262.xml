<taxonomicName id="322D43A99A57CB36C6EE04FD00A1D0EA" class="Chilopoda" family="Lithobiidae" genus="Lithobius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Lithobius tricuspis" order="Lithobiomorpha" pageId="6" pageNumber="21" phylum="Arthropoda" rank="species" species="tricuspis">Lithobius tricuspis</taxonomicName>
