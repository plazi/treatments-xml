<taxonomicName id="6087F9E27E6ED4C0508718C8C855B71E" class="Gastropoda" family="Camaenidae" genus="Bradybaena" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bradybaena linjun" order="Stylommatophora" pageId="10" pageNumber="139" phylum="Mollusca" rank="species" species="linjun">
