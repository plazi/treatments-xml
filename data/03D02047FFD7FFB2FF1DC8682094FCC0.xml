<taxonomicName id="4C79EAD2FFD7FFB1FBBECCB425EDF869" box="[1076,1201,1984,2006]" class="Liliopsida" family="Poaceae" genus="Arundinaria" kingdom="Plantae" order="Poales" pageId="4" pageNumber="295" phylum="Tracheophyta" rank="genus">
