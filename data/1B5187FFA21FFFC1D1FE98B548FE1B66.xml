<taxonomicName id="54F84D6AA21FFFC2D00298B54F501F84" authorityName="Bleeker" authorityYear="1860" box="[363,563,1413,1440]" class="Actinopterygii" family="Cyprinidae" genus="Pseudophoxinus" kingdom="Animalia" order="Cypriniformes" pageId="5" pageNumber="110" phylum="Chordata" rank="genus">
