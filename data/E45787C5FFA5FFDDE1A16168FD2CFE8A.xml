<taxonomicName id="ABFE4D50FFA7FFDDE1A8637EFEB9FEF5" box="[158,319,259,284]" class="Liliopsida" family="Bromeliaceae" kingdom="Plantae" order="Poales" pageId="38" pageNumber="39" phylum="Tracheophyta" rank="family">Bromeliaceae</taxonomicName>
).
