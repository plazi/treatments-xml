<taxonomicName id="ABFE4D50FFA7FFDDE42B629DFAECFF1E" box="[1309,1386,224,247]" class="Liliopsida" family="Bromeliaceae" genus="Vriesia" kingdom="Plantae" order="Poales" pageId="38" pageNumber="39" phylum="Tracheophyta" rank="genus">
