<taxonomicName id="38A13BEFB87BE545674AF9D4679EF939" authority="felis" authorityName="felis" box="[1068,1266,1612,1638]" class="Insecta" family="Asilidae" genus="Laphria" kingdom="Animalia" order="Diptera" pageId="89" pageNumber="90" phylum="Arthropoda" rank="subSpecies" species="felis" subSpecies="felis">
<emphasis id="CDD59C7EB87BE545674AF9D4679EF939" box="[1068,1266,1612,1638]" italics="true" pageId="89" pageNumber="90">Laphria felis felis</emphasis>
