<number id="2F6CA368FFFB2B3EFB15FEC648F647D9" box="[1162,1219,361,385]" pageId="32" pageNumber="233" value="1378.0">1378</number>
–1379
</date>
, 
<date id="ED1A978FFFFB2B3EFA88FEC649AD47D9" box="[1303,1432,361,385]" pageId="32" pageNumber="233" value="1409" valueMax="1410">1409–1410</date>
, 
<date id="ED1A978FFFFB2B3EFF08FE224D2347FE" box="[151,278,397,422]" pageId="32" pageNumber="233" value="1471" valueMax="1475">
