<emphasis id="B971EAE6FFD0FFA6FEA7FCF13B6AFC2F" bold="true" box="[344,525,886,907]" pageId="107" pageNumber="107">
<specimenCode id="DBA39E8FFFD0FFA6FEA7FCF13B6AFC2F" box="[344,525,886,907]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="107" pageNumber="107">AMNH 696239</specimenCode>
