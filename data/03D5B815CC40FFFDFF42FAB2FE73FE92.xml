<taxonomicName id="4C7C7280CC40FFF8FF42FAB2FDEAFA75" authority="Bravo &amp; Araújo, 2018" authorityName="Bravo &amp; Araújo" authorityYear="2018" box="[151,544,1384,1410]" class="Insecta" family="Psychodidae" genus="Maruina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="190" phylum="Arthropoda" rank="species" species="mucugensis" status="sp. nov." subGenus="Aculcina">
