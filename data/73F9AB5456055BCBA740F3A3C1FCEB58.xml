<taxonomicName id="7E041F6CF2D8A2FBBD545EA4863F5479" authorityName="Dejean" authorityYear="1821" class="Hexapoda" family="Carabidae" genus="Tachys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="450" pageNumber="451" phylum="Arthropoda" rank="genus">
