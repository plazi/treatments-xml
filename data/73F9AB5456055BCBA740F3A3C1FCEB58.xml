<taxonomicName id="F3B774B09A4DE69979D6F4E68CD709C2" authorityName="Kult" authorityYear="1961" class="Insecta" family="Carabidae" genus="Macrotachys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="450" pageNumber="451" phylum="Arthropoda" rank="genus">
