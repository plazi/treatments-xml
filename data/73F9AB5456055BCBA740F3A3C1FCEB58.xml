<taxonomicName id="3A6BBE55CC111196A300239E8A742D1D" authorityName="Dejean" authorityYear="1821" class="Hexapoda" family="Carabidae" genus="Tachys" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="450" pageNumber="451" phylum="Arthropoda" rank="genus">
