, 
<collectingCountry id="CB8876467C6F9B38FC9DF1E09B7CFC27" box="[877,1049,991,1016]" name="Guyana" pageId="28" pageNumber="29">British Guiana</collectingCountry>
, 
<collectingCountry id="CB8876467C6F9B38FBD9F1E09BF2FC27" box="[1065,1175,991,1016]" name="Suriname" pageId="28" pageNumber="29">Suriname</collectingCountry>
, 
<collectingRegion id="715BF8347C6F9B38FB58F1E09A60FC27" box="[1192,1285,991,1016]" country="Democratic Republic of the Congo" name="Equateur" pageId="28" pageNumber="29">
