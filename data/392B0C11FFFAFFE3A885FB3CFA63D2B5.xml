<number id="074AAF20FFFAFFE3AC5BF949FB83D240" box="[1097,1147,1777,1799]" pageId="3" pageNumber="404" value="2010.0">2010</number>
, 
<figureCitation id="29B9A182FFFAFFE3AC97F949FB46D240" box="[1157,1214,1777,1799]" captionStart="FIGURE 4" captionStartId="13.[151,250,1909,1931]" captionTargetBox="[156,1436,208,1887]" captionTargetId="figure@13.[151,1436,193,1888]" captionTargetPageId="13" captionText="FIGURE 4. Plega fasciatella: a, forewing; b, hindwing; c, pteropleura; d, head, frontal; e, forefemur, inner and outer surfaces; f, head, thorax and abdomen, dorsal." httpUri="https://zenodo.org/record/236819/files/figure.png" pageId="3" pageNumber="404">
