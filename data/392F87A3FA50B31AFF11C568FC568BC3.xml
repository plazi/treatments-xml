<taxonomicName id="76864D36FA51B31DFE5AC5E0FD8089A8" authorityName="Guerin-Meneville" authorityYear="1844" box="[476,565,1930,1951]" class="Insecta" family="Mogoplistidae" genus="Ornebius" higherTaxonomySource="CoL" kingdom="Animalia" order="Orthoptera" pageId="3" pageNumber="589" phylum="Arthropoda" rank="genus">
