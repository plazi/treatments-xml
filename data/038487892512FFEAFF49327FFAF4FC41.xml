<collectingRegion id="49E9F87D2512FFEAFF0B33F2FE4FFC41" box="[213,389,943,969]" country="South Africa" name="KwaZulu-Natal" pageId="7" pageNumber="41">KwaZulu-Natal</collectingRegion>
