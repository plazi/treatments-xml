<taxonomicName id="4C2D4D1C2512FFEAFB29333AFEA8FC2D" authority="Mayr, 1901" authorityName="Mayr" authorityYear="1901" class="Insecta" family="Formicidae" genus="Pheidole" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="41" phylum="Arthropoda" rank="species" species="tenuinodis">
