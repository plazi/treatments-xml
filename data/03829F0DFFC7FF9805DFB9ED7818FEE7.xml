<subSubSection id="C3317D90FFC7FF9B058FBF517E6FFBD0" box="[199,1135,1039,1065]" pageId="9" pageNumber="460" type="etymology">
<paragraph id="8B942E1BFFC7FF9B058FBF517E6FFBD0" blockId="9.[151,1437,894,2038]" box="[199,1135,1039,1065]" pageId="9" pageNumber="460">
<emphasis id="B95FF209FFC7FF9B058FBF517B4EFBD0" bold="true" box="[199,334,1039,1065]" pageId="9" pageNumber="460">Etymology.</emphasis>
Species named after 
<collectingCountry id="F33C6E8BFFC7FF9B0775BF5178EDFBD0" box="[573,749,1039,1065]" name="New Caledonia" pageId="9" pageNumber="460">New Caledonia</collectingCountry>
