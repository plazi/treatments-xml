<taxonomicName id="4C064D46C436B541FE52824DFD14B868" authorityName="G. M. Allen" authorityYear="1923" box="[455,659,322,348]" class="Mammalia" family="Rhinolophidae" genus="Rhinolophus" kingdom="Animalia" order="Chiroptera" pageId="9" pageNumber="79" phylum="Chordata" rank="species" species="rex">
