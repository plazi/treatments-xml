<taxonomicName id="9EA1A584FFD5FF973F72FBD5FCA5FBAA" authorityName="Last, Burgess, &amp; Seret" authorityYear="2002" box="[637,858,1057,1079]" class="Elasmobranchii" family="Etmopteridae" genus="Etmopterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squaliformes" pageId="10" pageNumber="207" phylum="Chordata" rank="species" species="dislineatus">
