<taxonomicName id="9EA1A584FFD5FF9739D4FD52FACBFD27" authorityName="Jordan &amp; Snyder" authorityYear="1902" box="[1243,1332,676,698]" class="Elasmobranchii" family="Etmopteridae" genus="Etmopterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squaliformes" pageId="10" pageNumber="207" phylum="Chordata" rank="species" species="lucifer">
