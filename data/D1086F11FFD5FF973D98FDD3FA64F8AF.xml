<taxonomicName id="9EA1A584FFD5FF9739C1FD35FAD1FD4A" baseAuthorityName="Whitley" baseAuthorityYear="1939" box="[1230,1326,705,727]" class="Elasmobranchii" family="Etmopteridae" genus="Etmopterus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squaliformes" pageId="10" pageNumber="207" phylum="Chordata" rank="species" species="molleri">
