<taxonomicName id="4816878713E27A9557A8B6959830C60A" authorityName="Weber" authorityYear="1801" class="Hexapoda" family="Carabidae" genus="Calosoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="genus">
<emphasis id="FA174F5A08139FCCBAF3FB1533E8DEC7" italics="true" pageId="169" pageNumber="170">Calosoma</emphasis>
</taxonomicName>
(about 170 species), and 
<taxonomicName id="995EB2C192A53D79F07542396AF3E147" authorityName="Linnaeus" authorityYear="1758" class="Hexapoda" family="Carabidae" genus="Carabus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="169" pageNumber="170" phylum="Arthropoda" rank="genus">
<emphasis id="9609D48021C56C56FF46104D34D52943" italics="true" pageId="169" pageNumber="170">Carabus</emphasis>
