<geoCoordinate id="EE0583FB1927FFCB6B5F943C1E54FBCE" box="[291,373,1074,1096]" degrees="14.55" direction="east" orientation="longitude" pageId="24" pageNumber="24" precision="555" value="14.55">14.55E</geoCoordinate>
