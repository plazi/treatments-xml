<subSubSection id="419452B3CFC4B4675FC2EB7B771E3531" pageId="19" pageNumber="20" type="ecology">
<paragraph id="178F2CA9BFFB1AB2D8EF9EF80CE96806" pageId="19" pageNumber="20">Ecology.</paragraph>
<paragraph id="DACC25B9178C01D06AC6FD51A3D434FB" pageId="19" pageNumber="20">
<taxonomicName id="114573E97E89A76FCAC0D3240AA27D34" authority="Weise, 1911" authorityName="Weise" authorityYear="1911" class="Insecta" family="Chrysomelidae" genus="Alocypha" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Alocypha bimaculata" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="bimaculata">Alocypha bimaculata</taxonomicName>
