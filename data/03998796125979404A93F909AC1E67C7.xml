<taxonomicName id="4C304D03125B79414B16FA3AAB6A63D7" authority="Mo &amp; Wang &amp; Li, 2020" authorityName="Mo &amp; Wang &amp; Li" authorityYear="2020" box="[274,515,1453,1474]" class="Insecta" family="Perlidae" genus="Hemacroneuria" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="398" phylum="Arthropoda" rank="species" species="trilobata" status="sp. nov.">
