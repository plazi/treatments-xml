<taxonomicName id="52A7C71E06269B16E6AABE2FCCF14971" authorityName="Latreille" authorityYear="1834" class="Insecta" family="Elateridae" genus="Campsosternus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="Campsosternus" order="Coleoptera" pageId="0" pageNumber="79" phylum="Arthropoda" rank="genus">
