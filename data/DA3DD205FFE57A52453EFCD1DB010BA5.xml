<subSubSection id="1A8E3098FFE67A52450EFB91DDC10B7D" box="[312,979,1124,1150]" pageId="86" pageNumber="87" type="etymology">
<paragraph id="522B6313FFE67A52450EFB91DDC10B7D" blockId="86.[264,1323,284,1190]" box="[312,979,1124,1150]" pageId="86" pageNumber="87">
<emphasis id="60E0BF01FFE67A52450EFB91DFAF0B7D" bold="true" box="[312,445,1124,1150]" pageId="86" pageNumber="87">Etymology</emphasis>
