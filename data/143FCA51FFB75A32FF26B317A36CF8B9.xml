<emphasis id="AEE2A755FFB75A32FF26B3DFA642FA7F" box="[151,283,1437,1460]" italics="true" pageId="6" pageNumber="39">Chirtothrips</emphasis>
<taxonomicName id="5B9600C4FFB75A32FE90B3DFA52AFA7F" authority="Girault, 1927:1" authorityName="Girault" authorityPageNumber="1" authorityYear="1927" box="[289,627,1437,1460]" class="Insecta" family="Thripidae" genus="Chirothrips" kingdom="Animalia" order="Thysanoptera" pageId="6" pageNumber="39" phylum="Arthropoda" rank="species" species="atricorpus">
(sic) 
<emphasis id="AEE2A755FFB75A32FEE6B3DFA69FFA7F" box="[343,454,1437,1460]" italics="true" pageId="6" pageNumber="39">atricorpus</emphasis>
<treatmentCitation id="1D375D56FFB75A32FE7CB3DFA52AFA7F" author="Girault" box="[461,627,1437,1460]" page="1" pageId="6" pageNumber="39" year="1927">
<bibRefCitation id="F80706B6FFB75A32FE7CB3DFA53FFA7F" author="Girault" box="[461,614,1437,1460]" pageId="6" pageNumber="42" refString="Girault, A. A. (1927) Thysanoptera Nova Australiensis from Queensland. Privately published, Brisbane, 1 p." type="book" year="1927">
Girault, 
<number id="2A5E6960FFB75A32FD97B3DFA53FFA7F" box="[550,614,1437,1460]" pageId="6" pageNumber="39" value="1927.0">1927</number>
</bibRefCitation>
: 
<number id="2A5E6960FFB75A32FDD8B3DFA52AFA7F" box="[617,627,1437,1460]" pageId="6" pageNumber="39" value="1.0">1</number>
</treatmentCitation>
</taxonomicName>
