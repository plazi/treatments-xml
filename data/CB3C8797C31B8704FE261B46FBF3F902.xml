<paragraph id="432A3681C31B8704FF8C1A21FE55FC8C" blockId="33.[125,502,771,795]" box="[125,502,771,795]" pageId="33" pageNumber="3576">
<taxonomicName id="84954D02C31B8704FF8C1A21FE55FC8C" authority="Salvadori." authorityName="Salvadori." box="[125,502,771,795]" class="Aves" family="Columbidae" genus="Macropygia" kingdom="Animalia" order="Columbiformes" pageId="33" pageNumber="3576" phylum="Chordata" rank="species" species="nigrirostris">
