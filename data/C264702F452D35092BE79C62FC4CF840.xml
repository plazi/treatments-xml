<typeStatus id="95767F9B452D35092BB79F00FEF1F928" box="[199,366,1641,1667]" pageId="23" pageNumber="474">Paralectotype</typeStatus>
.
</emphasis>
<specimenCount id="5CCB0AB0452D35092A0B9F00FE53F928" box="[379,460,1641,1667]" pageId="23" pageNumber="474" type="male">1 male</specimenCount>
: 5233 
<collectorName id="E738A4EF452D350929669F03FD02F928" box="[534,669,1641,1668]" pageId="23" pageNumber="474">L. Mulligan</collectorName>
(handwritten in red) | 
<taxonomicName id="8DCDBABA452D350928EC9F00FC61F928" box="[924,1022,1641,1667]" class="Insecta" family="Melolonthidae" genus="Rhopaea" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="474" phylum="Arthropoda" rank="genus">Rhopaea</taxonomicName>
