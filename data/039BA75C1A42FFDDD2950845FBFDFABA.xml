<taxonomicName id="4C326DC91A42FFDDD05D0C42FC9DFABA" authority="(Thunberg)" baseAuthorityName="Thunberg" baseAuthorityYear="1815" box="[516,782,1323,1340]" class="Insecta" family="Acrididae" genus="Tylotropidius" kingdom="Animalia" order="Orthoptera" pageId="28" pageNumber="339" phylum="Arthropoda" rank="species" species="didymus">
