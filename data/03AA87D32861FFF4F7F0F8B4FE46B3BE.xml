<subSubSection id="C319654E2861FFF4F7F0F81AFE46B3BE" lastPageId="35" lastPageNumber="36" pageId="33" pageNumber="34" type="description">
<paragraph id="8BBC36C52861FFF6F7F0F81AFE4DB788" blockId="33.[151,816,1949,2010]" box="[151,494,1949,1974]" pageId="33" pageNumber="34">
<taxonomicName id="4C034D462861FFF6F7F0F81AFF54B788" box="[151,247,1949,1974]" class="Insecta" family="Pieridae" kingdom="Animalia" order="Lepidoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="family">Pieridae</taxonomicName>
