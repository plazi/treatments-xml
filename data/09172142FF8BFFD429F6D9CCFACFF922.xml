<collectionCode id="E7AF0891FF8BFFD42BFFD87AFD47F8CD" box="[670,708,1842,1863]" country="France" name="Musee de Lectoure" pageId="5" pageNumber="50">ML</collectionCode>
0.6 6, 
<collectionCode id="E7AF0891FF8BFFD429F6D81AFF3DF8ED" box="[151,190,1874,1895]" country="Thailand" name="Paleontological Collections" pageId="5" pageNumber="50">PW</collectionCode>
