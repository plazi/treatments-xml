<paragraph id="4AB4EED04A54D1AE50336582D352EA6B" pageId="0" pageNumber="49">
<taxonomicName id="3EEAA33AEA581F03ECA604394FE644AE" class="Sordariomycetes" family="Nectriaceae" genus="Nectria" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Nectria cinnabarina" order="Hypocreales" pageId="0" pageNumber="49" phylum="Ascomycota" rank="species" species="cinnabarina">Nectria cinnabarina</taxonomicName>
