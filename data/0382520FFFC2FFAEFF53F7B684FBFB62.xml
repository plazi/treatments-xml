<taxonomicName id="4C2B989AFFC2FFAEFED3F7B6842AFC41" authority="Benesh, 1946" authorityName="Benesh" authorityYear="1946" box="[279,610,901,927]" class="Insecta" family="Lucanidae" genus="Platyceropsis" kingdom="Animalia" order="Coleoptera" pageId="21" pageNumber="367" phylum="Arthropoda" rank="genus">
<emphasis id="B95F3F0BFFC2FFAEFED3F7B687FEFC41" bold="true" box="[279,438,901,927]" italics="true" pageId="21" pageNumber="367">Platyceropsis</emphasis>
