<subSubSection id="628965362B37707C81DBFE00FE09F991" pageId="4" pageNumber="57" type="etymology">
<paragraph id="2A2C36BD2B37707C81DBFE00FE09F991" blockId="4.[151,1437,152,455]" pageId="4" pageNumber="57">
<emphasis id="18E7EAAF2B37707C81DBFE00FE98F9F2" bold="true" box="[197,338,394,420]" pageId="4" pageNumber="57">Etymology.</emphasis>
