</taxonomicName>
(
<emphasis id="BEB697249A9190E62629EF6DB7116FB1" bold="true" pageId="0" pageNumber="15">D-F</emphasis>
), and 
<taxonomicName id="D161E4951C33142599667751BE7A24F9" genus="I." lsidName="I. wardianum" pageId="0" pageNumber="15" rank="species" species="wardianum">
<emphasis id="BDE75E7962BD5CD04C1C1EC96CE8B513" italics="true" pageId="0" pageNumber="15">I. wardianum</emphasis>
