<taxonomicName id="B28C2FC3FFDBFFD1A8D9FE7C0B58FE65" authority="Razowski, 1960" authorityName="Razowski" authorityYear="1960" box="[151,539,399,425]" class="Insecta" family="Tortricidae" genus="Phalonidia" kingdom="Animalia" order="Lepidoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="tolli">
