<taxonomicName id="4C17FB9DFFE0FF9C66CD8DA8FDF7ABB7" authority="Hood, 1954: 32" authorityName="Hood" authorityYear="1954" box="[151,553,1697,1719]" class="Insecta" family="Phlaeothripidae" genus="Mystrothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="349" phylum="Arthropoda" rank="species" species="clavatoris">
<emphasis id="B9635C0CFFE0FF9C66CD8DA8FE5AABB7" box="[151,388,1697,1719]" italics="true" pageId="2" pageNumber="349">Mystrothrips clavatoris</emphasis>
