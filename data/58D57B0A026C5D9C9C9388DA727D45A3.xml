<taxonomicName id="0A8FBB2EC70919AF301E9D761F9441D7" LSID="58D57B0A-026C-5D9C-9C93-88DA727D45A3" class="Gastropoda" family="Ficidae" genus="Ficus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ficus elastica" order="Littorinimorpha" pageId="0" pageNumber="62878" phylum="Mollusca" rank="species" species="elastica">Ficus elastica</taxonomicName>
