<taxonomicName id="49B85AC1FFA6331AB22E68C8A8AB0F76" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[562,763,1787,1812]" class="Aves" family="Passeridae" genus="Passer" kingdom="Animalia" order="Passeriformes" pageId="36" pageNumber="37" phylum="Chordata" rank="species" species="montanus">
