<taxonomicName id="49B85AC1FFA6331AB41268C8AB450F5D" authority="(Scopoli, 1772)" baseAuthorityName="Scopoli" baseAuthorityYear="1772" class="Insecta" family="Philopteridae" genus="Philopterus" kingdom="Animalia" order="Psocodea" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="fringillae">
