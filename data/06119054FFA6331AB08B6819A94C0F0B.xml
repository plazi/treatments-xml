<taxonomicName id="49B85AC1FFA6331AB12E697CA9480F0B" authority="(Zlotorzycka, 1964)" baseAuthorityName="Zlotorzycka" baseAuthorityYear="1964" box="[306,792,1871,1897]" class="Insecta" family="Philopteridae" genus="Philopterus" kingdom="Animalia" order="Psocodea" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="montani">
