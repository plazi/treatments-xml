<taxonomicName id="AFD9A596122EFFA3FC73FE0FFB50FDD2" authority="Gmelin, 1791" authorityName="Gmelin" authorityYear="1791" box="[944,1269,510,535]" class="Bivalvia" family="Tellinidae" genus="Tellina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="84" pageNumber="85" phylum="Mollusca" rank="species" species="fabula">
