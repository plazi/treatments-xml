<taxonomicName id="AFD9A596122EFFA3FAF5FE0EFEF4FDFE" authority="Gronovius 1791" authorityName="Gronovius" authorityYear="1791" class="Bivalvia" family="Tellinidae" genus="Tellina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Veneroida" pageId="84" pageNumber="85" phylum="Mollusca" rank="species" species="fabula">
