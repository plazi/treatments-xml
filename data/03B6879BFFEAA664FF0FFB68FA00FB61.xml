<taxonomicName id="4C1F4D0EFFEBA664FB57FC02FAC1FC24" box="[1231,1373,926,947]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="species" species="ruficapitella">
