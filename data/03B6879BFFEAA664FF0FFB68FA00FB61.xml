<number id="3DD724AAFFEBA664FF79FCDAFF66FCCC" box="[225,250,838,859]" pageId="19" pageNumber="220" value="41.0">41</number>
</figureCitation>
)................................................................................... 
<taxonomicName id="4C1F4D0EFFEBA664FB75FCDAFAC1FCCC" box="[1261,1373,838,859]" class="Insecta" family="Nepticulidae" genus="Stigmella" kingdom="Animalia" order="Lepidoptera" pageId="19" pageNumber="220" phylum="Arthropoda" rank="species" species="saginella">
