<collectionCode id="C5FEAE739E48FF80FCF070C0FC5612A2" box="[835,928,1242,1263]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="76" pageNumber="76">AMNH</collectionCode>
as 
<emphasis id="919BEAA49E48FF80FC6F70C3FB8612BD" box="[988,1136,1241,1264]" italics="true" pageId="76" pageNumber="76">
<taxonomicName id="64EF4D359E48FF80FC6F70C3FB9B12BD" box="[988,1133,1241,1264]" class="Aves" family="Aves (awaiting allocation)" genus="Crucirostra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aves (awaiting allocation)" pageId="76" pageNumber="76" phylum="Chordata" rank="species" species="erythroptera">erythroptera</taxonomicName>
,
</emphasis>
unsexed, from 
<location id="A630606D9E48FF80FC9870ECFC371341" LSID="urn:lsid:plazi:treatment:2B4687A09E48FF80FF3D7299FBA01397:A630606D9E48FF80FC9870ECFC371341" box="[811,961,1270,1292]" name="Roda Valley" pageId="76" pageNumber="76">Roda Valley</location>
, 
<date id="D75110769E48FF80FC7E70EDFB871341" box="[973,1137,1270,1292]" pageId="76" pageNumber="76" value="1847-04-10">
<collectingDate id="C715E99E9E48FF80FC7E70EDFB871341" box="[973,1137,1270,1292]" pageId="76" pageNumber="76" value="1847-04-10">10 April 1847</collectingDate>
</date>
, was also exchanged to 
<collectionCode id="C5FEAE739E48FF80FC36710EFC281364" box="[901,990,1300,1321]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" lsid="urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="76" pageNumber="76" type="Museum">ZFMK</collectionCode>
