<collectionCode id="C5FEAE739E48FF80FCBF7085FC9A12F9" box="[780,876,1183,1204]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="76" pageNumber="76">AMNH</collectionCode>
</materialsCitation>
; 
<materialsCitation id="13873CEB9E48FF80FC327085FC611329" ID-GBIF-Occurrence="3059739002" collectingDate="1847-04-10" collectionCode="ZFMK, AMNH" location="Roda Valley" pageId="76" pageNumber="76" specimenCode="AMNH 457010" specimenCount="2" typeStatus="syntype">
