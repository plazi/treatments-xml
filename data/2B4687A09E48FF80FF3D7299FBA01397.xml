<collectionCode id="C5FEAE739E48FF80FC3270A7FC2D129F" box="[897,987,1213,1234]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" lsid="urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="76" pageNumber="76" type="Museum">ZFMK</collectionCode>
. 
<specimenCode id="F3499ECD9E48FF80FC5B70A6FB53129F" box="[1000,1189,1212,1234]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="76" pageNumber="76">AMNH 457010</specimenCode>
