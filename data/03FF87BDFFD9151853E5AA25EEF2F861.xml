<subSubSection id="C34C6520FFD9151653E5AA25EC1EFBB9" pageId="7" pageNumber="8" type="nomenclature">
<paragraph id="8BE936ABFFD9151653E5AA25EF05FB9C" blockId="7.[151,565,1079,1140]" box="[151,565,1079,1105]" pageId="7" pageNumber="8">
<heading id="D0A181C7FFD9151653E5AA25EF05FB9C" bold="true" box="[151,565,1079,1105]" fontSize="11" level="1" pageId="7" pageNumber="8" reason="1">
<emphasis id="B922EAB9FFD9151653E5AA25EF05FB9C" bold="true" box="[151,565,1079,1105]" pageId="7" pageNumber="8">
<taxonomicName id="4C564D28FFD9151653E5AA25ECDBFB9C" authority="Darling" authorityName="Darling" box="[151,491,1079,1105]" class="Insecta" family="Perilampidae" genus="Perilampus" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="pisticus" status="n. sp.">
