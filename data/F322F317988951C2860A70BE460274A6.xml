<taxonomicName id="B335BFAB52856473C933664CB2D9763A" authorityName="Jayasiri, E.B.G.Jones &amp; K.D.Hyde" authorityYear="2019" class="Dothideomycetes" family="Bambusicolaceae" genus="Leucaenicola" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Leucaenicola" order="Pleosporales" pageId="0" pageNumber="55957" phylum="Ascomycota" rank="genus">
<emphasis id="E19D526F7B3F26F7899E8902028C42ED" italics="true" pageId="0" pageNumber="55957">Leucaenicola</emphasis>
</taxonomicName>
, whereas 
<taxonomicName id="2FF660138A547B16AD3185E63CCC1CCB" class="Aves" family="Phasianidae" genus="Bambusicola" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Bambusicola" order="Galliformes" pageId="0" pageNumber="55957" phylum="Chordata" rank="genus">
<emphasis id="F4AD4D08E9FD6D7D5EFF92810752ADB7" italics="true" pageId="0" pageNumber="55957">Bambusicola</emphasis>
</taxonomicName>
