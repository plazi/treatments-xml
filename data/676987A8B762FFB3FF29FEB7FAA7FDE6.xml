<taxonomicName id="28C04D3DB763FFB0FECDF89CFD01F883" authority="Roback" authorityName="Roback" box="[371,655,1895,1917]" class="Insecta" family="Chironomidae" genus="Labrundinia" kingdom="Animalia" order="Diptera" pageId="97" pageNumber="98" phylum="Arthropoda" rank="species" species="opela">
