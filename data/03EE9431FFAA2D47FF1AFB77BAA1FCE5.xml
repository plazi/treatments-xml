<taxonomicName id="4C475EA4FFAA2D47FF1AFAB1BC60FC59" authority="Fieber, 1861" authorityName="Fieber" authorityYear="1861" box="[151,520,1326,1351]" class="Insecta" family="Miridae" genus="Hypsitylus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="prasinus">
