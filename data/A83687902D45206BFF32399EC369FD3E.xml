<taxonomicName id="E79F4D052D45206CFAFC3DB8C63FF8FC" authority="Bezzi" authorityName="Bezzi" authorityYear="1899" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="542" phylum="Arthropoda" rank="species" species="sibillina" subGenus="Kritempis">
