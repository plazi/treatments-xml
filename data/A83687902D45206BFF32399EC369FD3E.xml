<emphasis id="12EBEA942D45206CFDC13C5EC525F8FC" bold="true" box="[612,633,1820,1844]" pageId="11" pageNumber="542">9.</emphasis>
<taxonomicName id="E79F4D052D45206CFDDA3C5FC433F8FC" authority="Daugeron, 2009" authorityName="Daugeron" authorityYear="2009" box="[639,879,1821,1844]" class="Insecta" family="Empididae" genus="Empis" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="542" phylum="Arthropoda" rank="species" species="taffertensis" status="sp.nov." subGenus="Kritempis">
