<subScript id="17A6863DFF915C02FAA02E48FAC5FD99" attach="left" box="[1281,1289,568,582]" fontSize="6" pageId="4" pageNumber="42">2</subScript>
; four crossveins between M 
<subScript id="17A6863DFF915C02FEC32E13FEB7FDAE" attach="left" box="[354,379,611,625]" fontSize="6" pageId="4" pageNumber="42">1+2</subScript>
and M 
<subScript id="17A6863DFF915C02FE6D2E13FE29FDAE" attach="left" box="[460,485,611,625]" fontSize="6" pageId="4" pageNumber="42">3+4</subScript>
