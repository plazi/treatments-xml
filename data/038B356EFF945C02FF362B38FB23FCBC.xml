<subScript id="17A6863DFF915C02FECE2E48FEBBFD99" attach="left" box="[367,375,568,582]" fontSize="6" pageId="4" pageNumber="42">5</subScript>
; four cross-veins between R 
<subScript id="17A6863DFF915C02FD712E48FD14FD99" attach="left" box="[720,728,568,582]" fontSize="6" pageId="4" pageNumber="42">5</subScript>
and M 
<subScript id="17A6863DFF915C02FC912E48FCF4FD99" attach="left" box="[816,824,568,582]" fontSize="6" pageId="4" pageNumber="42">1</subScript>
; three cross-veins between M 
<subScript id="17A6863DFF915C02FB002E48FB65FD99" attach="left" box="[1185,1193,568,582]" fontSize="6" pageId="4" pageNumber="42">1</subScript>
