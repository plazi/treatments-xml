<taxonomicName id="4C3D4D295108FF8A9C229D12FD78F8AA" authority="(Keyserling, 1891)" authorityName="Oliveira &amp; Brescovit" authorityYear="2021" baseAuthorityName="Keyserling" baseAuthorityYear="1891" box="[347,727,1891,1915]" class="Arachnida" family="Anyphaenidae" genus="Patrera" kingdom="Animalia" order="Araneae" pageId="32" pageNumber="233" phylum="Arthropoda" rank="species" species="concolor">
