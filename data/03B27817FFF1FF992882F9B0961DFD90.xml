<subSubSection id="C3019A8AFFF1FF992882F957961DFD90" pageId="11" pageNumber="694" type="reference_group">
<paragraph id="8BA4C901FFF1FF992882F95791F2F92E" blockId="11.[167,777,1602,2091]" box="[168,748,1701,1721]" pageId="11" pageNumber="694">
<taxonomicName id="4C1BB282FFF1FF992882F95792DAF92E" authority="(Meek 1907)" baseAuthorityName="Meek" baseAuthorityYear="1907" box="[168,452,1701,1720]" class="Actinopterygii" family="Cichlidae" genus="Amphilophus" kingdom="Animalia" order="Perciformes" pageId="11" pageNumber="694" phylum="Chordata" rank="species" species="alfari">
<emphasis id="B96F1513FFF1FF992882F9579253F92E" box="[168,333,1701,1720]" italics="true" pageId="11" pageNumber="694">Amphilophus alfari</emphasis>
(
<bibRefCitation id="EF8AB4F0FFF1FF992972F95792A0F92E" author="Meek SE" box="[344,446,1701,1720]" pageId="11" pageNumber="694" pagination="133 - 157" refId="ref15366" refString="Meek SE (1907) Notes on fresh-water fishes from Mexico and Cen- tral America. Field Columbian Museum, Zoological Series 7 (5): 133 - 157." type="journal article" year="1907">Meek 1907</bibRefCitation>
