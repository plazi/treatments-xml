<paragraph id="D9FD329A38E1E3236E3F70C20507FEC6" pageId="null" pageNumber="304">
<pageBreakToken id="4D0555F1D22EE338B7229B40F4355622" pageId="null" pageNumber="304" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="337D0BB99EE8CA707CFACED7A7CD3645" class="Magnoliopsida" family="Rubiaceae" genus="Galium" kingdom="Plantae" order="Gentianales" pageId="null" pageNumber="304" phylum="Tracheophyta" rank="genus">Galium</taxonomicName>
