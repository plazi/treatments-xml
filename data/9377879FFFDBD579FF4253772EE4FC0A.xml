<typeStatus id="C465882BFFDBD579FD2152F02C26FC56" box="[756,804,933,959]" pageId="10" pageNumber="521">type</typeStatus>
locality, Hengduan Meili Mountains in 
<collectingRegion id="D91AF86BFFDBD579FB0852F02A9FFC57" box="[1245,1437,932,959]" country="China" name="Yunnan" pageId="10" pageNumber="521">Yunnan Province</collectingRegion>
of 
<collectingCountry id="63C97619FFDBD579FF63529D2FFBFC0B" box="[182,249,968,994]" name="China" pageId="10" pageNumber="521">China</collectingCountry>
(
<geoCoordinate id="7EEA504EFFDBD579FEDD529D2E6EFC0B" box="[264,364,968,994]" degrees="28" direction="north" minutes="06" orientation="latitude" pageId="10" pageNumber="521" precision="925" value="28.1">28Â°06â€™N</geoCoordinate>
; 
<geoCoordinate id="7EEA504EFFDBD579FEAD529D2EDBFC0A" box="[376,473,968,995]" degrees="98" direction="east" minutes="53" orientation="longitude" pageId="10" pageNumber="521" precision="925" value="98.88333">98Â°53â€™E</geoCoordinate>
