<taxonomicName id="78339AB5B0523D21ACA9347D169F02D1" authorityName="W.T.Wang" authorityYear="1984" class="Magnoliopsida" family="Urticaceae" genus="Elatostema" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Elatostema youyangense" order="Rosales" pageId="0" pageNumber="65" phylum="Tracheophyta" rank="species" species="youyangense">
