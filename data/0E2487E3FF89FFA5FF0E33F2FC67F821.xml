<subSubSection id="CE97657EFF89FFA5FF0E33B0FD23FBDA" box="[151,694,1015,1039]" pageId="21" pageNumber="22" type="type_taxon">
<paragraph id="863236F5FF89FFA5FF0E33B0FD23FBDA" blockId="21.[151,694,1015,1039]" box="[151,694,1015,1039]" pageId="21" pageNumber="22">
