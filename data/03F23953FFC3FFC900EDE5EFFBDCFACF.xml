<paragraph id="8BE48845FFC3FFC600EDE5EFFD93FD65" blockId="87.[177,595,705,728]" box="[177,595,705,728]" pageId="87" pageNumber="1100">
<heading id="D0AC3F29FFC3FFC600EDE5EFFD93FD65" box="[177,595,705,728]" fontSize="8" level="2" pageId="87" pageNumber="1100" reason="2">
<emphasis id="B92F5457FFC3FFC600EDE5EFFD93FD65" bold="true" box="[177,595,705,728]" pageId="87" pageNumber="1100">
<taxonomicName id="4C5BF3C6FFC3FFC600EDE5EFFEA1FD6A" authority="Haeselbarth, 2008" authorityName="Haeselbarth" authorityYear="2008" box="[177,353,705,727]" class="Insecta" family="Braconidae" genus="Perilitus" kingdom="Animalia" order="Hymenoptera" pageId="87" pageNumber="1100" phylum="Arthropoda" rank="species" species="thyellae" status="nov.sp.">
<emphasis id="B92F5457FFC3FFC600EDE5EFFEA1FD6A" bold="true" box="[177,353,705,727]" italics="true" pageId="87" pageNumber="1100">Perilitus thyellae</emphasis>
