<paragraph id="300BFF94DC4A5016FFB81584F10C1774" pageId="456" pageNumber="457">
<taxonomicName id="81C2922AE13D0A410DB61F390EC2CBE3" authorityName="Casey" authorityYear="1918" class="Insecta" family="Carabidae" genus="Anillinus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="456" pageNumber="457" phylum="Arthropoda" rank="species" species="carolinae">Anillinus carolinae</taxonomicName>
