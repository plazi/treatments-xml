<taxonomicName id="4C494D01FFC9FFF4FF5B7F44FDD0FC9D" authority="Pogue and Mickevich, 1990: 322" authorityName="Pogue and Mickevich" authorityPageNumber="322" authorityYear="1990" box="[151,561,835,860]" class="Insecta" family="Tortricidae" genus="Honca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="203" phylum="Arthropoda" rank="genus">
