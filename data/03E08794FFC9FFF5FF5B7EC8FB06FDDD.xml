<taxonomicName id="4C494D01FFC9FFF4FEEA7F06FDBFFCD8" authority="Busck, 1907" authorityName="Busck" authorityYear="1907" box="[294,606,773,798]" class="Insecta" family="Tortricidae" genus="Phalonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="203" phylum="Arthropoda" rank="species" species="grandis">
