<location id="BA57FE9CFFE47524FE5EFDCDFE5BE738" LSID="urn:lsid:plazi:treatment:37211951FFE37524FCDBF8CFFC85E0F2:BA57FE9CFFE47524FE5EFDCDFE5BE738" box="[420,467,535,558]" country="Brazil" county="The" municipality="Ventenat's" name="Two" pageId="20" pageNumber="107">Two</location>
