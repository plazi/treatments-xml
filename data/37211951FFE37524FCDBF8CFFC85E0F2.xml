<specimenCode id="EF2E003CFFE47524FE5DFC3BFDA5E6EF" box="[423,557,993,1017]" collectionCode="G" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="20" pageNumber="107" type="Herbarium">G00330754</specimenCode>
] and 
