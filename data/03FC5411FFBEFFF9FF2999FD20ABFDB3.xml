<emphasis id="B9213915FFBEFFFEFF2999FD20ADFF3E" bold="true" box="[151,931,151,177]" pageId="3" pageNumber="264">
<taxonomicName id="4C559E84FFBEFFFEFF2999FD2008FF3E" authority="Melnitsky &amp; Ivanov" authorityName="Melnitsky &amp; Ivanov" box="[151,774,151,177]" class="Insecta" family="Ecnomidae" genus="Archaeotinodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="3" pageNumber="264" phylum="Arthropoda" rank="species" species="regiomontana" status="new species">
<emphasis id="B9213915FFBEFFFEFF2999FD210AFF3E" bold="true" box="[151,516,151,177]" italics="true" pageId="3" pageNumber="264">Archaeotinodes regiomontana</emphasis>
Melnitsky &amp; Ivanov
</taxonomicName>
, 
<taxonomicNameLabel id="A212846EFFBEFFFEFCAD99FD20ADFF3E" box="[787,931,151,177]" pageId="3" pageNumber="264" rank="species">new species</taxonomicNameLabel>
</emphasis>
