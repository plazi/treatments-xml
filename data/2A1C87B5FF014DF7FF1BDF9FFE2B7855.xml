<paragraph id="A20A36A3FF0F4DF4FF10D99EFDF57C1D" blockId="153.[225,1423,1738,1819]" pageId="153" pageNumber="154">
<taxonomicName id="65B54D20FF0F4DF4FF10D99EFE567C07" authority="Baba, 2018" authorityName="Baba" authorityYear="2018" box="[225,409,1773,1793]" class="Malacostraca" family="Chirostylidae" genus="Uroptychus" kingdom="Animalia" order="Decapoda" pageId="153" pageNumber="154" phylum="Arthropoda" rank="species" species="cylindropus" status="n. sp.">
