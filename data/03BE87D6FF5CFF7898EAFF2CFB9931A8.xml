<quantity id="4CEF9B25FF5CFF789957FE9CFB31318F" box="[298,368,366,394]" metricMagnitude="1" metricUnit="m" metricValue="7.62" pageId="205" pageNumber="206" unit="ft" value="250.0">250 ft</quantity>
