<heading id="00318075DF2CFFACFF27FF215C2AFF46" bold="true" box="[151,521,151,177]" fontSize="11" level="1" pageId="29" pageNumber="30" reason="1">
<emphasis id="69B2EB0BDF2CFFACFF27FF215C2AFF46" bold="true" box="[151,521,151,177]" pageId="29" pageNumber="30">
<taxonomicName id="9CC64C9ADF2CFFACFF27FF215FA8FF46" box="[151,395,151,177]" class="Elasmobranchii" family="Carcharhinidae" genus="Carcharhinus" kingdom="Animalia" order="Carcharhiniformes" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="sealei">
<emphasis id="69B2EB0BDF2CFFACFF27FF215FA8FF46" bold="true" box="[151,395,151,177]" italics="true" pageId="29" pageNumber="30">Carcharhinus sealei</emphasis>
