; 
<geoCoordinate id="C3C550501B5AFFF38473D7FFFB6E36B5" box="[1069,1255,404,431]" degrees="75" direction="west" minutes="39" orientation="longitude" pageId="22" pageNumber="483" precision="1" seconds="46.26" value="-75.66285">75°39’46.26’’W</geoCoordinate>
