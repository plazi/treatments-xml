<collectingRegion id="6435F8751B5AFFF3852AD7FEFF5F36C8" country="Colombia" name="Caqueta" pageId="22" pageNumber="483">Caquetá</collectingRegion>
, 
<collectingCountry id="DEE676071B5AFFF380BCD7D3FEDA36C8" box="[226,339,440,466]" name="Colombia" pageId="22" pageNumber="483">Colombia</collectingCountry>
, 
<date id="D24F10571B5AFFF38100D7D3FD9636C8" box="[350,543,440,466]" pageId="22" pageNumber="483" value="2017-03-29" valueMax="2017-04-04" valueMin="2017-03-29">29.iii–04.iv.2017</date>
