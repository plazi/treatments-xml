<collectingCountry id="F351E9B92C3CFF8EFAF163F2FF700368" name="Saudi Arabia" pageId="37" pageNumber="38">Saudi Arabia</collectingCountry>
, 
<collectingCountry id="F351E9B92C3CFF8EFF5C6216FE700368" box="[241,486,798,824]" name="United Arab Emirates" pageId="37" pageNumber="38">United Arab Emirates</collectingCountry>
