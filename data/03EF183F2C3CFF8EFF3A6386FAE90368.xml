<collectingCountry id="F351E9B92C3CFF8EFB1663F3FA970345" box="[1211,1281,763,789]" name="Egypt" pageId="37" pageNumber="38">Egypt</collectingCountry>
, 
<collectingCountry id="F351E9B92C3CFF8EFAA663F2FAC40344" box="[1291,1362,762,788]" name="Oman" pageId="37" pageNumber="38">Oman</collectingCountry>
