<taxonomicName id="4C554D1EFFCEFFFBFC35FE1B51A6FDAC" baseAuthorityName="Gray" baseAuthorityYear="1850" box="[971,1155,510,535]" class="Mammalia" family="Delphinidae" genus="Stenella" kingdom="Animalia" order="Cetacea" pageId="18" pageNumber="2242" phylum="Chordata" rank="species" species="clymene">
<emphasis id="B921EA8FFFCEFFFBFC35FE1B51A6FDAC" box="[971,1155,510,535]" italics="true" pageId="18" pageNumber="2242">Stenella clymene</emphasis>
