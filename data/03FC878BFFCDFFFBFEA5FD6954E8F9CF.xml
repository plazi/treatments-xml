<emphasis id="B921EA8FFFCEFFFBFF5EFE3E55C4FE4F" bold="true" box="[160,225,475,500]" italics="true" pageId="18" pageNumber="2242">Hosts</emphasis>
</paragraph>
<paragraph id="8BEA369DFFCEFFFBFF5EFE1B5455FD81" blockId="18.[160,1155,475,570]" pageId="18" pageNumber="2242">
<taxonomicName id="4C554D1EFFCEFFFBFF5EFE1B54FAFDAC" authorityName="Gray" authorityYear="1846" box="[160,479,510,535]" class="Mammalia" family="Delphinidae" genus="Globicephala" kingdom="Animalia" order="Cetacea" pageId="18" pageNumber="2242" phylum="Chordata" rank="species" species="macrorhynchus">
<emphasis id="B921EA8FFFCEFFFBFF5EFE1B54FAFDAC" box="[160,479,510,535]" italics="true" pageId="18" pageNumber="2242">Globicephala macrorhynchus</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C554D1EFFCEFFFBFE10FE1B57C2FDAC" baseAuthorityName="Gray" baseAuthorityYear="1846" box="[494,743,510,535]" class="Mammalia" family="Delphinidae" genus="Peponocephala" kingdom="Animalia" order="Cetacea" pageId="18" pageNumber="2242" phylum="Chordata" rank="species" species="electra">
<emphasis id="B921EA8FFFCEFFFBFE10FE1B57C2FDAC" box="[494,743,510,535]" italics="true" pageId="18" pageNumber="2242">Peponocephala electra</emphasis>
