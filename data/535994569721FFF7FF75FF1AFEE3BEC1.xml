<taxonomicName id="1CF05EC39721FFF7FEC4FF53FD0CBFDB" authority="Pictet &amp; Saussure, 1891" authorityName="Pictet &amp; Saussure" authorityYear="1891" box="[294,754,222,244]" class="Insecta" family="Gryllacrididae" genus="Gryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="118" pageNumber="119" phylum="Arthropoda" rank="species" species="macilenta">
