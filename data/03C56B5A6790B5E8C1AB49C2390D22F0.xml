<taxonomicName id="4C6CA1CF6790B5E8C5A448323D312208" baseAuthorityName="Scopoli" baseAuthorityYear="1763" box="[1176,1323,834,857]" class="Insecta" family="Curculionidae" genus="Otiorhynchus" kingdom="Animalia" order="Coleoptera" pageId="453" pageNumber="454" phylum="Arthropoda" rank="species" species="gemmatus">
