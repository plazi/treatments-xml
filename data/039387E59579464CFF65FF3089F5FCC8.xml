and 
<taxonomicName id="4C3A4D709579464AFE09FC2788C8FC06" authorityName="Hara &amp; Pinto-Da-Rocha" authorityYear="2010" box="[507,670,896,922]" class="Arachnida" family="Gonyleptidae" genus="Eusarcus" kingdom="Animalia" order="Opiliones" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="sergipanus">
