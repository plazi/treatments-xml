<taxonomicName id="8FE048973B29FF89FEDDFB4AFD2D98C9" authority="L. Papp, 1979: 371" authorityName="L. Papp" authorityPageNumber="371" authorityYear="1979" box="[283,707,1181,1203]" class="Insecta" family="Sphaeroceridae" genus="Apterobiroina" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="103" phylum="Arthropoda" rank="species" species="australis">
