<taxonomicName id="58E24D4DFFFF3573FF0CFCC45326FCBF" box="[231,343,798,823]" class="Insecta" family="Staphylinidae" genus="Homalota" kingdom="Animalia" order="Coleoptera" pageId="22" pageNumber="323" phylum="Arthropoda" rank="genus">
