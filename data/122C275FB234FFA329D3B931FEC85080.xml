<quantity id="5D7D3BACB234FFA32846BE40FE43511E" box="[295,406,1028,1050]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" metricValueMax="3.7" metricValueMin="3.5" pageId="62" pageNumber="1663" unit="mm" value="3.6" valueMax="3.7" valueMin="3.5">3,5-3,7mm</quantity>
