<taxonomicName id="6ACB4D39FF9A2708FA02F81AE0AD8482" baseAuthorityName="Watts &amp; Humphreys" baseAuthorityYear="2003" box="[381,668,1928,1950]" class="Insecta" family="Dytiscidae" genus="Limbodessus" kingdom="Animalia" order="Coleoptera" pageId="95" pageNumber="96" phylum="Arthropoda" rank="species" species="yuinmeryensis">
<emphasis id="9FBFEAA8FF9A2708FA02F81AE0AD8482" box="[381,668,1928,1950]" italics="true" pageId="95" pageNumber="96">Limbodessus yuinmeryensis</emphasis>
