–; 
<date id="FFAB100AAF3FFFB3FDA5FD13FD2C4E61" box="[602,704,763,789]" pageId="2" pageNumber="3" value="1991-12-05">
<number id="3DDD24EDAF3FFFB3FDA5FD13FD854E61" box="[602,617,763,789]" pageId="2" pageNumber="3" value="5.0">5</number>
