; III: 
<number id="292F2487FFAFB57299D7FCA0FE6C4466" box="[406,445,848,874]" pageId="7" pageNumber="8" value="1.2">1.2</number>
, 
<number id="292F2487FFAFB572998AFCA0FE234466" box="[459,498,848,874]" pageId="7" pageNumber="8" value="0.3">0.3</number>
, 
<number id="292F2487FFAFB57299BEFCA0FDF74466" box="[511,550,848,874]" pageId="7" pageNumber="8" value="1.3">1.3</number>
, 
<number id="292F2487FFAFB5729A72FCA0FD8B4466" box="[563,602,848,874]" pageId="7" pageNumber="8" value="1.4">1.4</number>
