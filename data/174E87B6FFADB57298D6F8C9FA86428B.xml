<number id="292F2487FFAFB5729A29FCA0FD414466" box="[616,656,848,874]" pageId="7" pageNumber="8" value="0.4">0.4</number>
, 
<number id="292F2487FFAFB5729ADDFCA0FD154466" box="[668,708,848,874]" pageId="7" pageNumber="8" value="4.6">4.6</number>
