<number id="292F2487FFAFB5729D01FC86FA5F449C" box="[1344,1422,886,912]" pageId="7" pageNumber="8" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
</figureCitation>
). Bulb 
<number id="292F2487FFAFB5729896FC6DFEDB44BB" box="[215,266,925,951]" pageId="7" pageNumber="8" value="0.54">0.54</number>
