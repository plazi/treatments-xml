<taxonomicName id="D8FE25BF8E671111E6D8B075230A03B5" authority="Engel, Falin &amp; Batelka, 2019" authorityName="Engel, Falin &amp; Batelka" authorityYear="2019" class="Insecta" family="Ripiphoridae" genus="Zapotecotoma" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Zapotecotoma sumichrasti" order="Coleoptera" pageId="0" pageNumber="59" phylum="Arthropoda" rank="species" species="sumichrasti">Zapotecotoma sumichrasti</taxonomicName>
