<taxonomicName id="E3A7FE88FF1E83EEFD6E5CE5FEF8FB47" authority="Bunge. Furthermore" authorityName="Bunge. Furthermore" class="Magnoliopsida" family="Brassicaceae" genus="Isatis" kingdom="Plantae" order="Brassicales" pageId="163" pageNumber="163" phylum="Tracheophyta" rank="species" species="minima">
