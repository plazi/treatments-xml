<date id="860511CCFF9EB34AFBE0FB40C7EEFAAF" box="[1059,1227,1269,1292]" pageId="3" pageNumber="68" value="1985-04-04" valueMax="1985-04-07" valueMin="1985-04-04">
<number id="4473252BFF9EB34AFBE0FB40C76EFAAF" box="[1059,1099,1269,1292]" pageId="3" pageNumber="68" value="5.5" valueMax="7.0" valueMin="4.0">4–7</number>
