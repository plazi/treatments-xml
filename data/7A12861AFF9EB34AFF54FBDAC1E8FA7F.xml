<quantity id="35439AE9FF9EB34AFEF6FA17C24CFA1A" box="[309,361,1442,1465]" metricMagnitude="1" metricUnit="m" metricValue="2.98704" pageId="3" pageNumber="68" unit="ft" value="98.0">98 ft</quantity>
