<quantity id="35439AE9FF9EB34AFB0CFAADC631FA8C" box="[1231,1300,1304,1327]" metricMagnitude="4" metricUnit="m" metricValue="5.9" pageId="3" pageNumber="68" unit="km" value="59.0">59 km</quantity>
