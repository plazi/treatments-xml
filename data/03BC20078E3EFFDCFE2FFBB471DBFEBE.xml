<taxonomicName id="4C15EA928E3EFFDDFEB6FA1A721FFA25" authority="(Fabricius, 1781)" baseAuthorityName="Fabricius" baseAuthorityYear="1781" box="[331,829,1489,1512]" class="Insecta" family="Chrysomelidae" genus="Dorynota" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="659" phylum="Arthropoda" rank="species" species="bidens" subGenus="Dorynota">
<emphasis id="B9614D038E3EFFDDFEB6FA1A7093FA25" box="[331,433,1489,1512]" italics="true" pageId="2" pageNumber="659">Dorynota</emphasis>
(
<emphasis id="B9614D038E3EFFDDFE3DFA1A7304FA25" box="[448,550,1489,1512]" italics="true" pageId="2" pageNumber="659">Dorynota</emphasis>
) 
<emphasis id="B9614D038E3EFFDDFDCBFA1A735EFA25" box="[566,636,1489,1512]" italics="true" pageId="2" pageNumber="659">bidens</emphasis>
(
<bibRefCitation id="EF84ECE08E3EFFDDFD77FA1A7217FA25" author="FABRICIUS J. C." box="[650,821,1489,1512]" pageId="2" pageNumber="659" refId="ref18866" refString="FABRICIUS J. C. 1781: Species insectorum exhibentes eorvm differentias speci? cas, synonyma avctorvm, loca natalia, metamorphosin adiectis observationibvs, descriptionibvs. Tom. I. Carol. Ernest. Bohnii, Hamburgi et Kilionii, viii + 552 pp." type="book" year="1781">Fabricius, 1781</bibRefCitation>
