<date id="FF911048FFA7B863FB7775149973FB61" box="[1215,1404,296,322]" pageId="12" pageNumber="53" value="1987-08-18">
<number id="3DE724AFFFA7B863FB77751498D3FB61" box="[1215,1244,296,322]" pageId="12" pageNumber="53" value="18.0">18</number>
