<paragraph id="8BA1D645FFA5FFF477F9FA6CFB01FA3F" blockId="16.[151,1436,1410,1524]" pageId="16" pageNumber="17">
<taxonomicName id="4C1EADC6FFA5FFF477F9FA6CFED0FA53" box="[151,367,1410,1432]" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="militaris">
