st. 
<taxonomicName id="4C1EADC6FFA5FFF476FBFA6CFD72FA53" authority="Santschi, 1924: 222" authorityName="Santschi" authorityPageNumber="222" authorityYear="1924" box="[405,717,1410,1432]" class="Insecta" family="Formicidae" genus="Polyrhachis" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="epinotalis">
