<number id="3DC624E4FFF24F4AFE21FC40FE16A7E2" box="[464,478,950,974]" pageId="5" pageNumber="166" value="1.0">1</number>
male
</specimenCount>
(
<collectionCode id="ED1FAE06FFF24F4AFDC8FC40FD5AA7E2" box="[569,658,950,974]" pageId="5" pageNumber="166">MHBU</collectionCode>
), 
<collectingCountry id="F3197653FFF24F4AFD5CFC43FD3BA7E2" box="[685,755,949,974]" name="China" pageId="5" pageNumber="166">China</collectingCountry>
, Yunnan, Ruili, 
<number id="3DC624E4FFF24F4AFC4FFC40FC34A7E2" box="[958,1020,950,974]" pageId="5" pageNumber="166" value="2004.0">2004</number>
. 
<date id="FFB01003FFF24F4AFC0EFC40FB94A7E2" box="[1023,1116,950,974]" pageId="5" pageNumber="166" value="1917-05-18">
V. 
<number id="3DC624E4FFF24F4AFBE9FC40FB94A7E2" box="[1048,1116,950,974]" pageId="5" pageNumber="166" value="17.5" valueMax="18.0" valueMin="17.0">17–18</number>
</date>
, coll. Xiujuan 
