<collectionCode id="ED1FAE06FFF24F4AFE59FC64FE37A786" box="[424,511,914,938]" httpUri="http://grbio.org/cool/mci8-ehqk" name="Zoological Museum, University of Copenhagen" pageId="5" pageNumber="166">ZMUC</collectionCode>
