<heading id="D0982569FFEEFFA6FC16F921B2FE39AA" box="[1009,1258,1690,1711]" centered="true" fontSize="8" level="2" pageId="13" pageNumber="226" reason="8">
<taxonomicName id="4C6FE986FFEEFFA6FC16F921B2FE39AA" baseAuthorityName="Osgood" baseAuthorityYear="1907" box="[1009,1258,1690,1711]" class="Mammalia" family="Heteromyidae" genus="Chaetodipus" kingdom="Animalia" order="Rodentia" pageId="13" pageNumber="226" phylum="Chordata" rank="species" species="siccus">
