<taxonomicName id="4C556C8E9A745210FBFCFF18FB4288CB" authority="Rtt" authorityName="Reitter" authorityYear="1922" box="[1081,1228,151,177]" class="Insecta" family="Tenebrionidae" genus="Cylindrinotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="296" phylum="Arthropoda" rank="species" species="disgregus">
