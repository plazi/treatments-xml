<taxonomicName id="4C556C8E9A745210FC8EFF18FC1D88CA" authorityName="J. Frivaldszky" authorityYear="1894" box="[843,915,151,176]" class="Insecta" family="Tenebrionidae" genus="Helops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="296" phylum="Arthropoda" rank="species" species="gratus">
