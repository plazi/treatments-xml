<location id="8ED06031FFFA985D8FA2D6B5FC73FA40" LSID="urn:lsid:plazi:treatment:03A687FCFFFA985E8FA2D7AEFC44FE6E:8ED06031FFFA985D8FA2D6B5FC73FA40" box="[151,276,1481,1505]" country="Poland" name="Bartoszyce" pageId="16" pageNumber="451">Bartoszyce</location>
IG-1well, depth 
<quantity id="4CF79B0FFFFA985D8EE6D6B4FF58FA41" box="[467,575,1480,1505]" metricMagnitude="3" metricUnit="m" metricValue="1.6452" pageId="16" pageNumber="451" unit="m" value="1645.2">
