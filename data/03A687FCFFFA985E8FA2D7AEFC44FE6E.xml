<taxonomicName id="4C0F4D69FFFA985D8C08D6B4F948FA40" authority="Biozone." authorityName="Biozone." box="[829,1071,1480,1505]" class="Graptolithina" family="Graptoloidea (awaiting allocation)" genus="Colonograptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Graptoloidea" pageId="16" pageNumber="451" phylum="Hemichordata" rank="species" species="praedeubeli">
