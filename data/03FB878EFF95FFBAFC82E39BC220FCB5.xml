<taxonomicName id="4C524D1BFF95FFBAFBABE153C327FDD5" authority="DIERL, 1974" authorityName="DIERL" authorityYear="1974" box="[1100,1385,553,577]" pageId="8" pageNumber="333" rank="subSpecies" subSpecies="orientalis">
