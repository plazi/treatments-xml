species: 
<taxonomicName id="60BC37F8FFB7224E382900075BA3310C" authority="Karny, 1931" authorityName="Karny" authorityYear="1931" box="[294,686,320,342]" class="Insecta" family="Rhaphidophoridae" genus="Anoplophilus" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="392" phylum="Arthropoda" rank="species" species="longicercus">
