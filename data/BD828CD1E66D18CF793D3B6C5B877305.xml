H. 
<taxonomicName id="ED31D9C9054565B107FD567B3AE8333D" class="Magnoliopsida" family="Hypericaceae" genus="Androsaemum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Malpighiales" pageId="null" pageNumber="710" phylum="Tracheophyta" rank="genus">Androsaemum</taxonomicName>
