<taxonomicName id="4C5093EF9B37FFEFFF15FC9EF2C87D24" authority="(Fischer 1970)" baseAuthorityName="Fischer" baseAuthorityYear="1970" box="[151,648,834,861]" class="Insecta" family="Hydropsychidae" genus="Macrostemum" kingdom="Animalia" order="Trichoptera" pageId="2" pageNumber="440" phylum="Arthropoda" rank="species" species="brasiliense">
