<taxonomicName id="BBC84D137A0E005B634314E336875E1B" authority="Priesner" authorityName="Priesner" box="[1049,1247,498,521]" class="Insecta" family="Thripidae" genus="Odontothrips" kingdom="Animalia" order="Thysanoptera" pageId="4" pageNumber="488" phylum="Arthropoda" rank="species" species="karnyi">
