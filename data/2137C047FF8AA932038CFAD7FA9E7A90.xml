<taxonomicName id="6E9E0AD2FF8AA9320361F9B2FB8B7A3D" box="[832,1108,1611,1635]" class="Insecta" family="Perlodidae" genus="Hemimelaena" kingdom="Animalia" order="Plecoptera" pageId="2" pageNumber="19" phylum="Arthropoda" rank="species" species="flaviventris">
