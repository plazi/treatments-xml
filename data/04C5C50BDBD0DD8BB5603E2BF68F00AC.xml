<taxonomicName id="EF81909A75F9761D25F58075C360A271" class="Insecta" family="Vespidae" genus="Sphex" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sphex mimulus" order="Hymenoptera" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="mimulus">Sphex mimulus</taxonomicName>
