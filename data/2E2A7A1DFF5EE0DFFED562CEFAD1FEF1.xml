</taxonomicName>
() and 
<taxonomicName id="6183B088FF5FE0C0FCB86603FC23F8FF" box="[869,1002,1825,1848]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="sollertiae">
