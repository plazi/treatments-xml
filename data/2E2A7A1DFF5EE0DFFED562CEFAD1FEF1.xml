<taxonomicName id="6183B088FF5FE0C0FDF96603FD37F8FF" box="[548,766,1825,1848]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="ranae">
