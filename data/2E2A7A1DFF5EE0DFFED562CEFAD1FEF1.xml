<taxonomicName id="6183B088FF40E0DFFD6C603EFD3FFEF2" box="[689,758,284,309]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="144" pageNumber="145" phylum="Arthropoda" rank="species" species="ranae">
