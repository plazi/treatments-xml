<number id="104BD92CFF5FE0C0FDC665AEFDF4FB61" box="[539,573,1164,1190]" pageId="143" pageNumber="144" value="46.0">46</number>
</figureCitation>
): 
<taxonomicName id="6183B088FF5FE0C0FD8B65AEFC8FFB62" box="[598,838,1164,1189]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="143" pageNumber="144" phylum="Arthropoda" rank="species" species="ranae">
<emphasis id="94F71719FF5FE0C0FD8B65AEFC8FFB62" box="[598,838,1164,1189]" italics="true" pageId="143" pageNumber="144">Glyphidocera ranae</emphasis>
</taxonomicName>
