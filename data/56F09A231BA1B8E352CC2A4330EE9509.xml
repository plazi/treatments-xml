<taxonomicName id="C8E24F4CDCAB0AC16C4AF16701FFAD5B" class="Insecta" family="Diprionidae" genus="Neodiprion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Neodiprion huizeensis" order="Hymenoptera" pageId="13" pageNumber="68" phylum="Arthropoda" rank="species" species="huizeensis">Neodiprion huizeensis</taxonomicName>
