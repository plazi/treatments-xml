<taxonomicName id="03238E4B1A35821D9AE8C0F116A14D1D" class="Insecta" family="Diprionidae" genus="Diprion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diprion jingyuanensis" order="Hymenoptera" pageId="13" pageNumber="68" phylum="Arthropoda" rank="species" species="jingyuanensis">Diprion jingyuanensis</taxonomicName>
