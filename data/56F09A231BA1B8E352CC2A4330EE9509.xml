<taxonomicName id="3F4A28F2A6BEB66284CCDF551446F9A8" class="Insecta" family="Diprionidae" genus="Diprion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Diprion jingyuanensis" order="Hymenoptera" pageId="13" pageNumber="68" phylum="Arthropoda" rank="species" species="jingyuanensis">Diprion jingyuanensis</taxonomicName>
