<taxonomicName id="DF954AA48741596EFF06B953FD9BF887" authority=", USNM" authorityName="USNM" box="[247,525,1795,1816]" class="Anthozoa" family="Flabellidae" genus="Monomyces" kingdom="Animalia" order="Scleractinia" pageId="47" pageNumber="306" phylum="Cnidaria" rank="species" species="rubrum">
