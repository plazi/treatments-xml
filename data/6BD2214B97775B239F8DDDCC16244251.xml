<emphasis id="BEC3540A91EB0E4F06954B3BC62D2750" italics="true" pageId="0" pageNumber="99">Diplodiscus latifii</emphasis>
</taxonomicName>
(
<emphasis id="54F31FF9C0127455373F6E80CBB0F8B4" bold="true" pageId="0" pageNumber="99">A-C</emphasis>
) and 
<taxonomicName id="2204DE9A0E54657E64641C877F594996" genus="D." lsidName="D. longifolius" pageId="0" pageNumber="99" rank="species" species="longifolius">
