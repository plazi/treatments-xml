<taxonomicName id="290186972450C49E17A32A7443830794" LSID="urn:lsid:zoobank.org:act:2CE2BCE0-15BC-488A-A363-872B6BA232F0" class="Arachnida" family="Theridiosomatidae" genus="Ogulnius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ogulnius hapalus" order="Araneae" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="hapalus">Ogulnius hapalus</taxonomicName>
