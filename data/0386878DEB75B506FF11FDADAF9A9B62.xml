<taxonomicName id="4C2F4D18EB75B506FD30FBDDAE9F9B0E" authorityName="D. Selnekovic" baseAuthorityName="Schrank" baseAuthorityYear="1781" box="[694,903,1053,1079]" class="Insecta" family="Mordellidae" genus="Variimorda" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="45" phylum="Arthropoda" rank="species" species="villosa">
