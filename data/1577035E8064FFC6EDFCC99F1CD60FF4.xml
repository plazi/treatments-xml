<quantity id="5A261FAD8065FFC6EEEFCEC91A860E8B" box="[900,987,224,248]" metricMagnitude="1" metricUnit="m" metricValue="2.4" metricValueMax="4.1" metricValueMin="0.7" pageId="97" pageNumber="98" unit="m" value="24.0" valueMax="41.0" valueMin="7.0">7–41 m</quantity>
