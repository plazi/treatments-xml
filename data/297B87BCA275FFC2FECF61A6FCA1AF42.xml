<taxonomicName id="66D24D29A276FFC2FE6A67ABFD4AABC6" authority="Švec, 2012" authorityName="Švec" authorityYear="2012" box="[467,685,695,713]" class="Insecta" family="Leiodidae" genus="Zeadolopus" kingdom="Animalia" order="Coleoptera" pageId="11" pageNumber="422" phylum="Arthropoda" rank="species" species="punctiventris" status="sp. nov.">
