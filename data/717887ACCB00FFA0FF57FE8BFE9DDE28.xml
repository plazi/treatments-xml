<paragraph id="F96E36BACB00FFA7FF57FE1EFBD3D89C" blockId="2.[151,1043,448,470]" box="[151,1043,448,470]" pageId="2" pageNumber="503">
<taxonomicName id="3ED14D39CB00FFA7FF57FE1EFDEAD89F" authority="Banks, 1931: 412" authorityName="Banks" authorityPageNumber="412" authorityYear="1931" box="[151,554,448,470]" class="Insecta" family="Corydalidae" genus="Protohermes" kingdom="Animalia" order="Megaloptera" pageId="2" pageNumber="503" phylum="Arthropoda" rank="species" species="bellulus">
