<paragraph id="8B8A36F1FFC5C232FDB9B899FD760C85" blockId="21.[151,1437,1939,2036]" box="[151,685,1939,1963]" pageId="21" pageNumber="22">
Length. Male: 
<quantity id="4CCD9B14FFC5C232FC12B899FE640C85" box="[316,447,1939,1963]" metricMagnitude="-3" metricUnit="m" metricValue="3.8499999999999996" metricValueMax="3.9" metricValueMin="3.8" pageId="21" pageNumber="22" unit="mm" value="3.8499999999999996" valueMax="3.9" valueMin="3.8">3.8–3.9mm</quantity>
; female: 
<quantity id="4CCD9B14FFC5C232FF08B899FD720C85" box="[550,681,1939,1963]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" metricValueMax="4.5" metricValueMin="4.3" pageId="21" pageNumber="22" unit="mm" value="4.4" valueMax="4.5" valueMin="4.3">4.3–4.5mm</quantity>
