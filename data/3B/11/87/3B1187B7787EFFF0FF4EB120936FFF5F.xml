; MS = 
<number box="[301,336,1906,1931]" pageId="3" pageNumber="151" value="0.5">0.5</number>
<collectionCode LSID="urn:lsid:biocol.org:col:14382" box="[343,412,1907,1931]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="3" pageNumber="151">MOD</collectionCode>
