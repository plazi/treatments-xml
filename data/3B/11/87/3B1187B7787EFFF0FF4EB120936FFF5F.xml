. OOL = 
<number box="[1214,1249,1871,1895]" pageId="3" pageNumber="151" value="2.2">2.2</number>
<collectionCode LSID="urn:lsid:biocol.org:col:14382" box="[1257,1326,1871,1895]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:14382" name="University of Modena and Reggio Emilia, Department of Biology" pageId="3" pageNumber="151">MOD</collectionCode>
