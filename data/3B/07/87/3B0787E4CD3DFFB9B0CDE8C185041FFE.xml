<emphasis bold="true" box="[151,301,258,283]" pageId="9" pageNumber="524">Total length.</emphasis>
Male 
<typeStatus box="[372,470,258,283]" pageId="9" pageNumber="524">holotype</typeStatus>
<quantity box="[477,566,258,283]" metricMagnitude="-3" metricUnit="m" metricValue="9.5" pageId="9" pageNumber="524" unit="mm" value="9.5">9.5 mm</quantity>
