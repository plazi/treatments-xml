<taxonomicName authorityName="Taiti &amp; Checcucci" authorityYear="2009" box="[399,504,590,615]" class="Malacostraca" family="Bathytropidae" genus="Dumetoniscus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Isopoda" pageId="16" pageNumber="89" phylum="Arthropoda" rank="species" species="graniticus">
