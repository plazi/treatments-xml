<subSubSection pageId="12" pageNumber="41" type="nomenclature">
<paragraph pageId="12" pageNumber="41">
<taxonomicName authority="Hoser, 2004" authorityName="Hoser" authorityYear="2004" class="Reptilia" family="Boidae" genus="Chondropython" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Chondropython viridis" order="Squamata" pageId="12" pageNumber="41" phylum="Chordata" rank="subSpecies" species="viridis" subSpecies="shireenae">Chondropython viridis shireenae Hoser, 2004</taxonomicName>
<taxonomicNameLabel pageId="12" pageNumber="41">[synonym of Morelia viridis]</taxonomicNameLabel>
</paragraph>
</subSubSection>
<subSubSection pageId="12" pageNumber="41" type="holotype">
