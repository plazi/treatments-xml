<subSubSection pageId="18" pageNumber="19" type="description">
<paragraph blockId="18.[189,334,1174,1200]" box="[189,334,1174,1200]" pageId="18" pageNumber="19">
<heading bold="true" box="[189,334,1174,1200]" fontSize="11" level="3" pageId="18" pageNumber="19" reason="3">
