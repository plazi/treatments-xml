<paragraph blockId="2.[151,1436,151,860]" pageId="2" pageNumber="51">
<emphasis bold="true" box="[197,350,799,824]" inLineHeading="true" pageId="2" pageNumber="51" reason="1">Distribution.</emphasis>
<collectingCountry box="[358,422,800,823]" name="Japan" pageId="2" pageNumber="51">Japan</collectingCountry>
(Honshu, Shikoku, Kyushu), 
<collectingCountry box="[762,833,800,823]" name="South Korea" pageId="2" pageNumber="51">Korea</collectingCountry>
, 
<collectingCountry box="[846,913,799,823]" name="China" pageId="2" pageNumber="51">China</collectingCountry>
(southward to Fujan and 
<collectingRegion box="[1206,1294,800,823]" country="China" name="Yunnan" pageId="2" pageNumber="51">Yunnan</collectingRegion>
, eastward to 
<collectingRegion box="[151,214,836,860]" country="China" name="Xizang" pageId="2" pageNumber="51">Tibet</collectingRegion>
