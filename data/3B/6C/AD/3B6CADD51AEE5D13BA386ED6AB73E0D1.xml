<taxonomicName authorityName="Fabricius" authorityYear="1794" class="Hexapoda" family="Carabidae" genus="Cychrus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="152" pageNumber="153">Cychrus</emphasis>
</taxonomicName>
