<taxonomicName authorityName="Boileau" authorityYear="1901" class="Insecta" family="Carabidae" genus="Cychropsis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="genus">
