<taxonomicName authorityName="Dejean" authorityYear="1826" class="Insecta" family="Carabidae" genus="Scaphinotus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="genus">
<emphasis italics="true" pageId="152" pageNumber="153">Scaphinotus</emphasis>
</taxonomicName>
(55 species), and 
<taxonomicName authorityName="Dejean" authorityYear="1826" class="Insecta" family="Carabidae" genus="Sphaeroderus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="152" pageNumber="153" phylum="Arthropoda" rank="genus">
