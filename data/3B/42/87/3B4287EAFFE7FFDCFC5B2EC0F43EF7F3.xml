, 
<taxonomicName authority="Mendonca &amp; Nunes &amp; Fernandes, 2021" authorityName="Mendonca &amp; Nunes &amp; Fernandes" authorityYear="2021" box="[1229,1344,1548,1571]" class="Insecta" family="Pentatomidae" genus="Hypoxys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="305" phylum="Arthropoda" rank="species" species="servillei" status="sp. nov.">
