<subSubSection box="[982,1285,1909,1935]" pageId="1" pageNumber="336" type="nomenclature">
<paragraph blockId="1.[982,1285,1909,1935]" box="[982,1285,1909,1935]" pageId="1" pageNumber="336">
<heading bold="true" box="[982,1285,1909,1935]" centered="true" fontSize="10" level="4" pageId="1" pageNumber="336" reason="2">
<taxonomicName authority="Fuhrmann, 1916" authorityName="Fuhrmann" authorityYear="1916" box="[982,1285,1909,1935]" class="Cestoda" family="Monticelliidae" genus="Goezeella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Proteocephalidea" pageId="1" pageNumber="336" phylum="Platyhelminthes" rank="genus">
<emphasis bold="true" box="[982,1285,1909,1935]" pageId="1" pageNumber="336">
