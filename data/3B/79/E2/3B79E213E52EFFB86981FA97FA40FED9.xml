and 
<taxonomicName box="[590,784,1375,1401]" class="Insecta" family="Tenthredinidae" genus="Neocolochelyna" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hymenoptera" pageId="2" pageNumber="459" phylum="Arthropoda" rank="genus">
