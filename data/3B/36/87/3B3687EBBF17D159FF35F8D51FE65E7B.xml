<taxonomicName authority="Linnaeus" authorityName="Linnaeus" box="[299,742,1917,1943]" class="Sipunculidea" family="Sipunculidae" genus="Sipunculus" kingdom="Animalia" order="Sipunculiformes" pageId="1" pageNumber="52" phylum="Sipuncula" rank="subGenus" subGenus="Sipunculus">
