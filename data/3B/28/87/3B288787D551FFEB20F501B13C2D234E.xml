</emphasis>
</specimenCount>
, same as 
<typeStatus box="[522,620,1052,1077]" pageId="23" pageNumber="280">holotype</typeStatus>
(
<collectionCode box="[636,704,1052,1076]" httpUri="http://grbio.org/cool/9bph-kj5p" name="Institute of Zoology and Botany" pageId="23" pageNumber="280">IZBE</collectionCode>
); 
<specimenCount box="[725,805,1052,1077]" pageId="23" pageNumber="280" type="male">
<emphasis bold="true" box="[725,805,1052,1077]" pageId="23" pageNumber="280">
<number box="[725,739,1052,1077]" pageId="23" pageNumber="280" value="1.0">1</number>
male
</emphasis>
</specimenCount>
, same as 
<typeStatus box="[919,1017,1052,1077]" pageId="23" pageNumber="280">holotype</typeStatus>
