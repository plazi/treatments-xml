<heading bold="true" box="[151,426,151,178]" fontSize="11" level="1" pageId="10" pageNumber="267" reason="1">
<emphasis bold="true" box="[151,426,151,178]" pageId="10" pageNumber="267">
<taxonomicName box="[151,353,151,177]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="10" pageNumber="267" phylum="Arthropoda" rank="species" species="clivicola" status="sp. n.">
<emphasis bold="true" box="[151,353,151,177]" italics="true" pageId="10" pageNumber="267">Manota clivicola</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[360,426,152,178]" pageId="10" pageNumber="267" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
</heading>
