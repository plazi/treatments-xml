<collectionCode LSID="urn:lsid:biocol.org:col:34697" box="[311,395,548,572]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="25" pageNumber="282">BPBM</collectionCode>
