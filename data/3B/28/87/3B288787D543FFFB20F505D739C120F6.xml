<collectionCode box="[635,745,1957,1981]" pageId="7" pageNumber="264">GUINEA</collectionCode>
</collectingCountry>
, 
<collectionCode box="[756,791,1957,1981]" pageId="7" pageNumber="264">SE</collectionCode>
, Koroba, 
<quantity box="[899,970,1956,1981]" metricMagnitude="4" metricUnit="m" metricValue="4.0" pageId="7" pageNumber="264" unit="km" value="40.0">40 km</quantity>
W of Tari, 
<quantity box="[1097,1183,1956,1981]" metricMagnitude="3" metricUnit="m" metricValue="1.65" pageId="7" pageNumber="264" unit="m" value="1650.0">1650 m</quantity>
