. 
<collectingCountry box="[398,458,1956,1981]" name="Malta" pageId="7" pageNumber="264">Male</collectingCountry>
, 
<collectingCountry box="[469,745,1957,1981]" name="Papua New Guinea" pageId="7" pageNumber="264">
