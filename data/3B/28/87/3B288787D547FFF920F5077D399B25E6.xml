, 
<taxonomicName box="[508,651,620,644]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="262" phylum="Arthropoda" rank="species" species="hamulata">M. hamulata</taxonomicName>
and 
<taxonomicName box="[707,821,620,644]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="262" phylum="Arthropoda" rank="species" species="parilis">M. parilis</taxonomicName>
.
</emphasis>
