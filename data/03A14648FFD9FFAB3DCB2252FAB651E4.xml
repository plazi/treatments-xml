<taxonomicName id="4C088CDDFFD9FFAB3DB22252FE405116" authority="Ashmead" authorityName="Ashmead" box="[238,500,1308,1334]" class="Insecta" family="Encyrtidae" genus="Ooencyrtus" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="268" phylum="Arthropoda" rank="genus">
<emphasis id="B97C2B4CFFD9FFAB3DB22252FECD5116" bold="true" box="[238,377,1308,1334]" italics="true" pageId="9" pageNumber="268">Ooencyrtus</emphasis>
