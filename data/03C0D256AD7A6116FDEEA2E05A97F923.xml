<taxonomicName id="4C6918C3AD7B6116FF68A5A2599CFDB5" authority="Vervoort, 1993" authorityName="Vervoort" authorityYear="1993" box="[189,704,493,520]" class="Hydrozoa" family="Sertulariidae" genus="Thyroscyphus" kingdom="Animalia" order="Leptothecata" pageId="53" pageNumber="54" phylum="Cnidaria" rank="species" species="scorpioides">
<emphasis id="B91DBF52AD7B6116FF68A5A25AAAFDB5" box="[189,502,493,519]" italics="true" pageId="53" pageNumber="54">Thyroscyphus scorpioides</emphasis>
<bibRefCitation id="EFF81EB1AD7B6116FDD0A5A1599CFDB5" author="Vervoort" box="[517,704,493,520]" firstAuthor="Vervoort" pageId="53" pageNumber="54" pagination="89 - 298" refId="ref39987" refString="Vervoort W. 1993. Cnidaria, Hydrozoa, Hydroida: Hydroids from the western Pacific (Philippines, Indonesia and New Caledonia) I. Sertulariidae (Part 1). In: Crosnier A. (ed.) Resultats des Campagnes MUSORSTOM, Vol. 11. Memoires du Museum national d'histoire naturelle 158: 89 - 298." type="journal article" year="1993">Vervoort, 1993</bibRefCitation>
</taxonomicName>
, as 
<taxonomicName id="4C6918C3AD7B6116FD2BA5A25E53FDB5" authority="(Vervoort, 1993)" authorityName="Galea &amp; Schuchert" authorityYear="2019" baseAuthorityName="Vervoort" baseAuthorityYear="1993" box="[766,1295,493,519]" class="Hydrozoa" family="Symplectoscyphidae" genus="Tuberocaulus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Leptothecata" pageId="53" pageNumber="54" phylum="Cnidaria" rank="species" species="scorpioides" status="gen. et comb. nov.">
