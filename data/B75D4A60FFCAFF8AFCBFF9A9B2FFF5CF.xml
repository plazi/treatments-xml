, and 
<taxonomicName id="F8F480F5FFCAFF8BFE3AFDACB374F18E" baseAuthorityName="Sorensson" baseAuthorityYear="2007" box="[386,536,1710,1729]" class="Insecta" family="Ptiliidae" genus="Acrotrichis" kingdom="Animalia" order="Coleoptera" pageId="2" phylum="Arthropoda" rank="species" species="nana">
<emphasis id="0D802764FFCAFF8BFE3AFDACB374F18E" box="[386,536,1710,1729]" italics="true" pageId="2">Acrotrichis nana</emphasis>
</taxonomicName>
in Poland.
