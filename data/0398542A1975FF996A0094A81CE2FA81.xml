Inkerman, 
<emphasis id="B945392E1975FF9969FF92851B65FD26" bold="true" box="[899,1092,651,672]" pageId="74" pageNumber="74">
<specimenCode id="DB974D471975FF9969FF92851B65FD26" box="[899,1092,651,672]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH 673042</specimenCode>
</emphasis>
(2551), male, 
<emphasis id="B945392E1975FF99688C92A61C95FD3B" bold="true" box="[752,948,680,701]" pageId="74" pageNumber="74">
<specimenCode id="DB974D471975FF99688C92A61C95FD3B" box="[752,948,680,701]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH 673043</specimenCode>
</emphasis>
(2552), female, 
<date id="FF8FC3FC1975FF996EE792A61C13FD5D" pageId="74" pageNumber="74" value="1907-03-05">
<collectingDate id="EFCB3A141975FF996EE792A61C13FD5D" pageId="74" pageNumber="74" value="1907-03-05">5 March 1907</collectingDate>
</date>
, collected by 
<collectorName id="26C480EA1975FF99699F92CB1B1FFD5D" box="[995,1086,709,731]" pageId="74" pageNumber="74">Stalker.</collectorName>
<specimenCode id="DB974D471975FF996E3092C81DCCFD7F" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="74" pageNumber="74">AMNH 673045</specimenCode>
