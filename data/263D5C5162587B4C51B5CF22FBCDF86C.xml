<taxonomicName id="699496C462587B4C51E1CF22FD99FA9F" box="[195,540,1351,1377]" class="Insecta" family="Chrysomelidae" genus="Lema" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="honorata" subGenus="Petauristes">
