sp. n. (holotype) 
<normalizedToken id="5D4A451F7478B557394AC896A2C53340" originalValue="I–L">I-L</normalizedToken>
<taxonomicName id="F466E9376CA1BC33C5E349E21D4B4E91" class="Insecta" family="Melolonthidae" genus="Tetraserica" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tetraserica tianchiensis" order="Coleoptera" pageId="12" pageNumber="95" phylum="Arthropoda" rank="species" species="tianchiensis">Tetraserica tianchiensis</taxonomicName>
