<typeStatus id="54BD884CFF975C48FF29D72DFF18FCFC" box="[151,207,855,879]" pageId="1" pageNumber="119">Type</typeStatus>
species: 
<taxonomicName id="4C064D6DFF975C48FE8BD72DFD55FCFC" authority="Bolivar" authorityName="Bolivar" box="[309,642,854,879]" class="Insecta" family="Tetrigidae" genus="Paratettix" kingdom="Animalia" order="Orthoptera" pageId="1" pageNumber="119" phylum="Arthropoda" rank="species" species="personatus">
