<taxonomicName id="7CEC63741410F7664421CE81954382AB" class="Arachnida" family="Agelenidae" genus="Flexicoelotes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Flexicoelotes jiaohanyanensis" order="Araneae" pageId="3" pageNumber="44" phylum="Arthropoda" rank="species" species="jiaohanyanensis">Flexicoelotes jiaohanyanensis</taxonomicName>
