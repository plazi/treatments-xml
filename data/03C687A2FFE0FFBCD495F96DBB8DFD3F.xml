<taxonomicName id="4C6F4D37FFE0FFB2D495F96DB9AAF8A3" box="[151,471,1790,1816]" class="Insecta" family="Cicadellidae" genus="Limassolla" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="60" phylum="Arthropoda" rank="species" species="yingjianga" subGenus="L.">
<emphasis id="B91BEAA6FFE0FFB2D495F96DB966F8A3" bold="true" box="[151,283,1790,1816]" italics="true" pageId="7" pageNumber="60">Limassolla</emphasis>
(
<emphasis id="B91BEAA6FFE0FFB2D52EF96DB938F8A3" bold="true" box="[300,325,1790,1816]" italics="true" pageId="7" pageNumber="60">L.</emphasis>
