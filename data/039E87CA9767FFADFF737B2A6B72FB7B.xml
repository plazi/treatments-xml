<taxonomicName id="4C374D5F977AFFABFEFD7B436981FC4C" authority="Li" authorityName="Li" authorityYear="2017" box="[262,475,1639,1661]" class="Arachnida" family="Dictynidae" genus="Brommella" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="143" phylum="Arthropoda" rank="species" species="funaria" status="sp. nov.">
