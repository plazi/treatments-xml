, in regions 
<collectionCode id="4EF9AE4C5D0E8D61969CFC29FB55FC43" box="[1166,1196,992,1018]" pageId="8" pageNumber="56">IV</collectionCode>
, 
<collectionCode id="4EF9AE4C5D0E8D6196A7FC29FB30FC43" box="[1205,1225,992,1018]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="8" pageNumber="56" type="Museum">V</collectionCode>
, 
<collectionCode id="4EF9AE4C5D0E8D6196C6FC29FB0CFC43" box="[1236,1269,992,1018]" country="Norway" name="Mykotektet, National Veterinary Institute" pageId="8" pageNumber="56">VI</collectionCode>
, 
<collectionCode id="4EF9AE4C5D0E8D6196EDFC29FAD0FC43" box="[1279,1321,992,1018]" pageId="8" pageNumber="56">VII</collectionCode>
, 
<collectionCode id="4EF9AE4C5D0E8D619726FC29FA91FC43" box="[1332,1384,992,1018]" pageId="8" pageNumber="56">VIII</collectionCode>
