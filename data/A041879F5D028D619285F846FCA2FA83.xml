<taxonomicName id="EFE84D0A5D0E8D6190B1FC29FCE9FC40" authorityName="Koch" authorityYear="1962" box="[675,784,992,1017]" class="Insecta" family="Coleoptera (awaiting allocation)" genus="Gyrasida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="56" phylum="Arthropoda" rank="genus">
<emphasis id="1A9CEA9B5D0E8D6190B1FC29FCE9FC40" box="[675,784,992,1017]" italics="true" pageId="8" pageNumber="56">Gyrasida</emphasis>
