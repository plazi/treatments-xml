<specimenCount id="269B34BB6554FFCAFCC0FCB7FCDEE768" box="[794,809,812,838]" pageId="18" pageNumber="19" type="generic">9</specimenCount>
</number>
specimens (
<collectionCode id="568C67F76554FFCAFC63FCB7FBEDE768" LSID="urn:lsid:biocol.org:col:34988" box="[953,1050,812,838]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="18" pageNumber="19">MNHN</collectionCode>
