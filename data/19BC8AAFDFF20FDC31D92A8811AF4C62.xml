<taxonomicName id="525AF07B966A2D3B871B14662115F076" class="Insecta" family="Crambidae" genus="Catharylla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Catharylla" order="Lepidoptera" pageId="7" pageNumber="22" phylum="Arthropoda" rank="genus">Catharylla</taxonomicName>
