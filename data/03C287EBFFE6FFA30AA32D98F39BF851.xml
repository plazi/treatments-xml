<typeStatus id="54D0885FFFE6FFA30BEA2E32F234F9F9" box="[478,582,1626,1651]" pageId="13" pageNumber="550">holotype</typeStatus>
), 
<date id="FFD5103DFFE6FFA308692E33F283F9F9" box="[605,753,1626,1651]" pageId="13" pageNumber="550" value="1964-06-08">
