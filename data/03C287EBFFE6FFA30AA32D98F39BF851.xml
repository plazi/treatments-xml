<collectionCode id="ED7AAE38FFE6FFA30B1C2E33F10BF9F9" box="[296,377,1627,1651]" country="Italy" httpUri="http://grbio.org/cool/70mh-t5r1" name="Universita di Cagliari" pageId="13" pageNumber="550">MZUC</collectionCode>
