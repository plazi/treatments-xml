Known only from Qingyuan City, 
<collectingRegion id="F921F80FFFB4FF9CFD73FA21D6D9F986" box="[744,984,1554,1579]" country="China" name="Guangdong" pageId="3" pageNumber="251">Guangdong Province</collectingRegion>
, 
<collectingCountry id="43F2767DFFB4FF9CFC7FFA21D12AF986" box="[996,1067,1554,1579]" name="China" pageId="3" pageNumber="251">China</collectingCountry>
