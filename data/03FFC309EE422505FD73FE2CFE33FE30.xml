<heading id="D0A1C573EE42251AFD73FE2CFC43FDDA" box="[661,920,499,523]" centered="true" fontSize="10" level="2" pageId="15" pageNumber="711" reason="2">
<taxonomicName id="4C56099CEE42251AFD73FE2CFC8AFDDA" authority="HANSSON &amp; LASALLE, 2003" authorityName="HANSSON &amp; LASALLE" authorityYear="2003" box="[661,849,499,523]" class="Insecta" family="Eulophidae" genus="Cabeza" kingdom="Animalia" order="Hymenoptera" pageId="15" pageNumber="711" phylum="Arthropoda" rank="species" species="petiolata" status="sp. n.">
<emphasis id="B922AE0DEE42251AFD73FE2CFC8AFDDA" bold="true" box="[661,849,499,523]" italics="true" pageId="15" pageNumber="711">Cabeza petiolata</emphasis>
</taxonomicName>
