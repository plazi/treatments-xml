(
<figureCitation id="133A2A55A250FFF4B1DA3CE0B9A3FA60" box="[159,238,1507,1531]" captionStart="FIGURES 1 – 9" captionStartId="6.[151,264,1872,1895]" captionTargetBox="[170,1429,322,1857]" captionTargetId="figure@6.[170,1430,322,1858]" captionTargetPageId="6" captionText="FIGURES 1 – 9. Habitus of Thyestetha spp., dorsal aspect: (1) T. carbonaria, lectotype; (2) T. discolor sp. n., holotype; (3) T. emarginata sp. n., holotype; (4) T. glabra sp. n., holotype; (5) T. lubrica sp. n., holotype; (6) T. nitida; (7) T. oblita sp. n., holotype; (8) T. puncticollis sp. n., holotype; (9) T. splendens sp. n., holotype." httpUri="https://zenodo.org/record/214457/files/figure.png" pageId="7" pageNumber="8">Figs. 3</figureCitation>
, 
<figureCitation id="133A2A55A250FFF4B1BF3CE0B855FA60" box="[250,280,1507,1531]" captionStart="FIGURES 10 – 18" captionStartId="7.[151,264,1307,1330]" captionTargetBox="[151,1435,180,1286]" captionTargetId="figure@7.[151,1436,175,1287]" captionTargetPageId="7" captionText="FIGURES 10 – 18. Habitus of Thyestetha spp., lateral aspect: (10) T. carbonaria, lectotype; (11) T. discolor sp. n., holotype; (12) T. emarginata sp. n., holotype; (13) T. glabra sp. n., holotype; (14) T. lubrica sp. n., holotype; (15) T. nitida, lectotype; (16) T. oblita sp. n., holotype; (17) T. puncticollis sp. n., holotype; (18) T. splendens sp. n., holotype." httpUri="https://zenodo.org/record/214458/files/figure.png" pageId="7" pageNumber="8">12</figureCitation>
, 
<figureCitation id="133A2A55A250FFF4B0603CE0B820FA60" box="[293,365,1507,1531]" captionStart="FIGURES 55 – 60" captionStartId="16.[151,264,1661,1684]" captionTargetBox="[163,1410,182,1636]" captionTargetId="figure@16.[151,1436,175,1640]" captionTargetPageId="16" captionText="FIGURES 55 – 60. Female terminalia of Thyestetha spp.: (55 – 57) T. emarginata sp. n., holotype: (55) sternite VIII, (56) tergite VIII and (57) tergite VII. (58 – 60) T. nitida: (58) sternite VIII, (59) tergite VIII and (60) tergite VII." httpUri="https://zenodo.org/record/214463/files/figure.png" pageId="7" pageNumber="8">
<number id="3DC924F7A250FFF4B0603CE0B80CFA60" box="[293,321,1507,1531]" pageId="7" pageNumber="8" value="55.0">55</number>
–57
</figureCitation>
, 
<figureCitation id="133A2A55A250FFF4B03D3CE0B88CFA60" box="[376,449,1507,1531]" captionStart="FIGURES 61 – 65" captionStartId="18.[151,264,1783,1806]" captionTargetBox="[151,1434,491,1733]" captionTargetId="figure@18.[151,1436,483,1734]" captionTargetPageId="18" captionText="FIGURES 61 – 65. Female genitalia of Thyestetha spp.: (61 – 63) T. emarginata sp. n., paratype: (61) tripartite sclerite at insertion of spermathecal duct. T. emarginata sp. n., holotype: (62) Spermatheca. (64 – 65): T. nitida (64) ovipositor, lateral aspect." httpUri="https://zenodo.org/record/214464/files/figure.png" pageId="7" pageNumber="8">61–63</figureCitation>
, 
<figureCitation id="133A2A55A250FFF4B0893CE0B8A4FA60" box="[460,489,1507,1531]" captionStart="FIGURE 71" captionStartId="19.[151,250,2007,2030]" captionTargetBox="[151,1435,1301,1986]" captionTargetId="figure@19.[151,1435,1301,1986]" captionTargetPageId="19" captionText="FIGURE 71. Distribution of Thyestetha carbonaria, T. discolor sp. n., T. emarginata sp. n., T. nitida, and T. oblita sp. n.." httpUri="https://zenodo.org/record/214466/files/figure.png" pageId="7" pageNumber="8">71</figureCitation>
)
