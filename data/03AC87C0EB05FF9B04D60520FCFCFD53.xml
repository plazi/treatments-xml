<number id="3DCD24F1EB05FF9706AF0587FCF0FD6E" box="[750,811,689,715]" pageId="4" pageNumber="27" value="1999.0">1999</number>
) as &quot; 
<taxonomicName id="4C054D55EB05FF9707280587FB49FD6F" box="[873,1170,689,714]" class="Insecta" family="Geometridae" genus="Hoplosauris" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="fragmentata">
