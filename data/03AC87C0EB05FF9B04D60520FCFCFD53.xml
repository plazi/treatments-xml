<number id="3DCD24F1EB05FF97059D04F6FE30FC7F" box="[476,491,960,986]" pageId="4" pageNumber="27" value="2.0">2</number>
(
<taxonomicName id="4C054D55EB05FF9705BF04F6FDBFFC7C" box="[510,612,960,986]" class="Insecta" family="Geometridae" genus="Triptiloides" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="species" species="laeta">
