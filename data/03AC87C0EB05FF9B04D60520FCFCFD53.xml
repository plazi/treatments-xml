<taxonomicName id="4C054D55EB05FF9704D605EEFEF3FD54" box="[151,296,728,753]" class="Insecta" family="Geometridae" genus="Hoplosauris" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="27" phylum="Arthropoda" rank="genus">
