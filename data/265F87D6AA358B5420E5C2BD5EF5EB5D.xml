<collectingCountry id="D6E17650AA338B542115C6585BD7EAE0" box="[359,426,620,645]" name="China" pageId="13" pageNumber="274">China</collectingCountry>
(
<collectingRegion id="6C32F822AA338B5421CBC658585DEAE0" box="[441,544,620,645]" country="China" name="Guangxi" pageId="13" pageNumber="274">Guangxi</collectingRegion>
