<taxonomicName id="4C418CD108E9FE339F164FB0FD09FA6D" box="[571,693,1437,1456]" class="Magnoliopsida" family="Oxalidaceae" genus="Oxys" kingdom="Plantae" order="Oxalidales" pageId="440" pageNumber="391" phylum="Tracheophyta" rank="species" species="corymbosa">
<emphasis id="B9352B4008E9FE339F164FB0FD09FA6D" box="[571,693,1437,1456]" italics="true" pageId="440" pageNumber="391">O. corymbosa</emphasis>
