<taxonomicName id="B200A08059D992581418592D661E177C" class="Insecta" family="Staphylinidae" genus="Quedius" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Quedius richteri" order="Coleoptera" pageId="7" pageNumber="32" phylum="Arthropoda" rank="species" species="richteri">Quedius richteri</taxonomicName>
