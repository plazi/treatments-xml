<taxonomicName id="9780B1864D055A7A641E398BFE57F879" box="[294,507,1946,1968]" class="Malacostraca" family="Maeridae" genus="Hamimaera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="thijsseni" status="sp. nov.">
