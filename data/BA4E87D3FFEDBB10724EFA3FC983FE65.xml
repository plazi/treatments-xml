<taxonomicName id="F5E74D46FFEDBB16722EFA4FCB59F9BA" authority="Herbst, 1798" authorityName="Herbst" authorityYear="1798" box="[549,895,1520,1546]" class="Arachnida" family="Phalangiidae" genus="Opilio" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="fasciatus">
