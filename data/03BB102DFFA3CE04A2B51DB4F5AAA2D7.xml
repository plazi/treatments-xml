</bibRefCitation>
based on 
<taxonomicName id="4C12DAB8FFA3CE04A1881F7AF249A642" authority="Moore, 1885" authorityName="Moore" authorityYear="1885" box="[938,1312,601,626]" class="Insecta" family="Bombycidae" genus="Rondotia" kingdom="Animalia" order="Lepidoptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="menciana">
