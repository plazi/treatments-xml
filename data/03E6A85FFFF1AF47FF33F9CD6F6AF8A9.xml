<quantity id="4CB7B4ACFFF3AF47FAE7F8C66F99F8F2" box="[1347,1432,1796,1822]" metricMagnitude="3" metricUnit="m" metricValue="3.2" pageId="7" pageNumber="8" unit="m" value="3200.0">3200m</quantity>
