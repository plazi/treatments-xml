<subSubSection id="C3306555A85E040FFF22FECE0D14FE03" pageId="2" pageNumber="284" type="diagnosis">
<paragraph id="8B9536DEA85E040FFF22FECE0D14FE03" blockId="2.[151,1436,289,426]" pageId="2" pageNumber="284">
<emphasis id="B95EEACCA85E040FFF22FECE0E35FEB8" bold="true" box="[151,273,289,314]" pageId="2" pageNumber="284">Diagnosis.</emphasis>
