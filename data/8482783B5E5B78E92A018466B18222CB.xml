<paragraph id="4F9400AFC7E4EDAFBAA259C989B3CD1A" pageId="7" pageNumber="62">
<taxonomicName id="AA7C4D7A67F8C393DD3A35C9C878AE9E" class="Insecta" family="Staphylinidae" genus="Tachinus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Tachinus fimbriatus" order="Coleoptera" pageId="7" pageNumber="62" phylum="Arthropoda" rank="species" species="fimbriatus">Tachinus fimbriatus</taxonomicName>
