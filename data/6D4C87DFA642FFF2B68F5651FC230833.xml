(Colla 1836: 112) 
<bibRefCitation id="81744B38A642FFF2B24A523DFA5D0B3E" author="Fernandez Casas, F. J. &amp; Pizarro Dominguez, J. M." box="[1101,1415,1360,1387]" pageId="2" pageNumber="181" pagination="53 - 58" refId="ref14833" refString="Fernandez Casas, F. J. &amp; Pizarro Dominguez, J. M. (2002) Cnidoscolorum notulae (Euphorbiaceae), 6 - 7. Fontqueria 55: 53 - 58." type="journal article" year="2002">Fernández Casas (2002: 67)</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="22E54D4AA642FFF2B395523DFCF80BDA" authority="(Muller Argoviensis 1874: 502) Pax (1931: 166)" authorityName="Pax" authorityPageNumber="166" authorityYear="1931" baseAuthorityName="Muller Argoviensis" baseAuthorityPageNumber="502" baseAuthorityYear="1874" class="Magnoliopsida" family="Euphorbiaceae" genus="Cnidoscolus" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="181" phylum="Tracheophyta" rank="species" species="oligandrus">
<emphasis id="D791EADBA642FFF2B395523DFEDB0BDB" italics="true" pageId="2" pageNumber="181">C. oligandrus</emphasis>
(
<bibRefCitation id="81744B38A642FFF2B7085219FDBA0BDA" author="Muller Argoviensis, J." box="[271,608,1396,1423]" pageId="2" pageNumber="181" refId="ref15618" refString="Muller Argoviensis, J. (1874) Euphorbiaceae. In: Martius, C. F. P. von &amp; Eichler, A. W. (Eds.) Flora Brasiliensis 11 (2). Frid. Fleischer, Munchen, 752 pp." type="book" year="1874">Müller Argoviensis 1874: 502</bibRefCitation>
) 
<bibRefCitation id="81744B38A642FFF2B46A5218FCF80BDA" author="Pax, F. A. &amp; Hoffmann, K." box="[621,802,1396,1423]" pageId="2" pageNumber="181" pagination="11 - 233" refId="ref15854" refString="Pax, F. A. &amp; Hoffmann, K. (1931) Euphorbiaceae. In: Engler, A. &amp; Prantl, K. (Eds.) Die Naturlichen Pflanzenfamilien ed. 2, 19 c. Wilhelm Engelmann, Leipzig, pp. 11 - 233" type="book chapter" year="1931">Pax (1931: 166)</bibRefCitation>
</taxonomicName>
, 
<taxonomicName id="22E54D4AA642FFF2B52B5219FADE0BDB" authority="Klotzsch ex Pax (1910: 90)" authorityName="Klotzsch ex Pax" authorityPageNumber="90" authorityYear="1910" box="[812,1284,1396,1423]" class="Magnoliopsida" family="Euphorbiaceae" genus="Cnidoscolus" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="181" phylum="Tracheophyta" rank="species" species="sellowianus">
<emphasis id="D791EADBA642FFF2B52B5219FC150BDB" box="[812,975,1396,1422]" italics="true" pageId="2" pageNumber="181">C. sellowianus</emphasis>
Klotzsch ex 
<bibRefCitation id="81744B38A642FFF2B2585218FADE0BDB" author="Pax, F. A." box="[1119,1284,1396,1423]" pageId="2" pageNumber="181" pagination="1 - 148" refId="ref15754" refString="Pax, F. A. (1910) Euphorbiaceae - Jatropheae. In: Engler, A. (Ed.) Das Pflanzenreich IV. 147. I (Heft 42). Wilhelm Engelmann, Leipzig, pp. 1 - 148." type="book chapter" year="1910">Pax (1910: 90)</bibRefCitation>
</taxonomicName>
and 
<taxonomicName id="22E54D4AA642FFF2B33F5219FDD50BE6" authority="(Pax 1910: 104) Pax (1931: 166))" authorityName="Pax" authorityPageNumber="166" authorityYear="1931" baseAuthorityName="Pax" baseAuthorityPageNumber="104" baseAuthorityYear="1910" class="Magnoliopsida" family="Euphorbiaceae" genus="Cnidoscolus" kingdom="Plantae" order="Malpighiales" pageId="2" pageNumber="181" phylum="Tracheophyta" rank="species" species="urniger">
