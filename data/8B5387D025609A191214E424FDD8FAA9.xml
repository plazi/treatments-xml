<taxonomicName id="C4FA4D4525609A191174E4C2FD02FD1C" authority="Michaelsen, 1930: 495" authorityName="Michaelsen" authorityPageNumber="495" authorityYear="1930" box="[313,707,660,683]" class="Ascidiacea" family="Polycitoridae" genus="Polycitor" kingdom="Animalia" order="Aplousobranchia" pageId="13" pageNumber="1623" phylum="Chordata" rank="species" species="circes">
