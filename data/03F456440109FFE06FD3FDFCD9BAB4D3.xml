<quantity id="4CA54AB70109FFE76BD4FC9EDCD9B746" box="[1168,1253,821,847]" metricMagnitude="-3" metricUnit="m" metricValue="5.3" pageId="50" pageNumber="51" unit="mm" value="5.3">5.3mm</quantity>
