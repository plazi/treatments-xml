<quantity id="4CA54AB70109FFE76C29FC9EDBFFB746" box="[877,963,821,847]" metricMagnitude="-3" metricUnit="m" metricValue="8.1" pageId="50" pageNumber="51" unit="mm" value="8.1">8.1mm</quantity>
