<quantity id="4CA54AB70109FFE76FD3FCF1D8D0B77D" box="[151,236,858,884]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="50" pageNumber="51" unit="mm" value="2.4">2.4mm</quantity>
, pronotal length: 
<quantity id="4CA54AB70109FFE76EFDFCF1DA32B77D" box="[441,526,858,884]" metricMagnitude="-3" metricUnit="m" metricValue="2.4" pageId="50" pageNumber="51" unit="mm" value="2.4">2.4mm</quantity>
, pronotal width: 
<quantity id="4CA54AB70109FFE76D91FCF1DB17B77D" box="[725,811,858,884]" metricMagnitude="-3" metricUnit="m" metricValue="1.9" pageId="50" pageNumber="51" unit="mm" value="1.9">1.9mm</quantity>
, head width: 
<quantity id="4CA54AB70109FFE76C83FCF1DC20B77D" box="[967,1052,858,884]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="50" pageNumber="51" unit="mm" value="1.8">1.8mm</quantity>
