<taxonomicName id="99914D9CFFD451225EDAAA5DFCFAFAA7" authority="obovalis" box="[385,768,1287,1329]" genus="Ochrocarpus" kingdom="Plantae" pageId="36" pageNumber="109" rank="genus">Ochrocarpus obovalis</taxonomicName>
