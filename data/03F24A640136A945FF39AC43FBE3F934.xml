<taxonomicName id="4C5B80F10136A945FF39AC43FD12F984" authority="(Linnaeus, 1767)" authorityName="Linnaeus" authorityYear="1767" baseAuthorityName="Linnaeus" baseAuthorityYear="1767" box="[199,654,1594,1619]" class="Insecta" family="Pieridae" genus="Gonepteryx" kingdom="Animalia" order="Lepidoptera" pageId="9" pageNumber="137" phylum="Arthropoda" rank="species" species="cleopatra">
<emphasis id="B92F27600136A945FF39AC43FE5DF984" box="[199,449,1594,1619]" italics="true" pageId="9" pageNumber="137">Gonepteryx cleopatra</emphasis>
