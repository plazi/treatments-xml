locality: 
<location id="8EB56036FFD06568FEEC4572FE28C544" LSID="urn:lsid:plazi:treatment:03C387FBFFD06568FF43420BFE0FC6FC:8EB56036FFD06568FEEC4572FE28C544" box="[312,399,1046,1070]" country="Algeria" name="Sebdou" pageId="20" pageNumber="21">Sebdou</location>
, 
