<collectionCode id="ED29B99CFFFCFFA0F73EFD9B3D6BFDF7" box="[260,306,514,535]" country="Russia" httpUri="http://grbio.org/cool/kjjc-ziy5" name="Paleontological Institute, Russian Academy of Sciences" pageId="6" pageNumber="259">PIN</collectionCode>
