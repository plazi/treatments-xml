<collectionCode id="AFE0AE0C264E4227FB94FCC7FBBDFC9C" box="[1047,1145,794,818]" httpUri="http://grbio.org/cool/91g3-0mnw" name="Naturhistorisches Museum, Wien" pageId="16" pageNumber="44">NHMW</collectionCode>
