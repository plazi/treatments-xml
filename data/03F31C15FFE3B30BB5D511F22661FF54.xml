<heading id="D0AD1A6FFFE3B308B5D511F224C0FB4F" bold="true" box="[151,436,1191,1217]" fontSize="11" level="1" pageId="8" pageNumber="23" reason="1">
<taxonomicName id="4C5AD680FFE3B308B5D511F224C0FB4F" box="[151,436,1191,1217]" class="Insecta" family="Oligoneuriidae" genus="Oligoneuriella" kingdom="Animalia" order="Ephemeroptera" pageId="8" pageNumber="23" phylum="Arthropoda" rank="species" species="rhenana">
<emphasis id="B92E7111FFE3B308B5D511F224C0FB4F" bold="true" box="[151,436,1191,1217]" italics="true" pageId="8" pageNumber="23">Oligoneuriella rhenana</emphasis>
