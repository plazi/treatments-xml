<number id="0D1A5B79FFF4FFBC8033831D92979E62" box="[1139,1154,1887,1913]" pageId="2" pageNumber="59" value="9.0">9</number>
(
<number id="0D1A5B79FFF4FFBC80D5831D92B69E62" box="[1173,1187,1887,1913]" pageId="2" pageNumber="59" value="8.0">8</number>
– 
<number id="0D1A5B79FFF4FFBC80F6831D92D19E62" box="[1206,1220,1887,1913]" pageId="2" pageNumber="59" value="8.0">8</number>
