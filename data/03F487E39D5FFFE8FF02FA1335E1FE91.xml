(
<collectionCode id="ED4CAE309D5FFFE8FE0AF93830E4F93F" box="[416,479,1750,1774]" country="Canada" httpUri="http://grbio.org/cool/a5s1-0agm" name="Provincial Museum of Alberta" pageId="35" pageNumber="686">PMA</collectionCode>
