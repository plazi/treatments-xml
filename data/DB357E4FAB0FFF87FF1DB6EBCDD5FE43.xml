<taxonomicName id="949CB4DAAB0FFF86FF1DB6EBCD69F935" authorityName="Evans" authorityYear="1937" box="[151,299,1764,1790]" class="Insecta" family="Hesperiidae" kingdom="Animalia" order="Lepidoptera" pageId="0" pageNumber="471" phylum="Arthropoda" rank="subFamily" subFamily="Coeliadinae">
