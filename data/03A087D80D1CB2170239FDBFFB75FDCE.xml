<number id="3DC124E90D1CB211003FFAFCFCF0FAD2" box="[782,796,1359,1385]" pageId="2" pageNumber="3" value="1.0">1</number>
</tableCitation>
): Idiosoma 
<quantity id="4CF19B2B0D1CB211009AFAFCFBE8FAD2" box="[939,1028,1359,1385]" metricMagnitude="1" metricUnit="m" metricValue="3.25628" pageId="2" pageNumber="3" unit="in" value="1282.0">1282 in</quantity>
