<treatmentCitationGroup id="AB5AE27AFF8E1861E0AEFCDAFD95FC54" box="[151,580,893,916]" pageId="10" pageNumber="51">
<taxonomicName id="4C4ABED7FF8E1861E0AEFCDAFD95FC54" authority="Bagnall, 1924: 637" authorityName="Bagnall" authorityPageNumber="637" authorityYear="1924" box="[151,580,893,916]" class="Insecta" family="Phlaeothripidae" genus="Mesothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="10" pageNumber="51" phylum="Arthropoda" rank="species" species="froggatti">
<emphasis id="B93E1946FF8E1861E0AEFCDAFEA3FC54" box="[151,370,893,916]" italics="true" pageId="10" pageNumber="51">Mesothrips froggatti</emphasis>
<treatmentCitation id="0AEBE345FF8E1861E141FCDAFD95FC54" author="Bagnall" box="[376,580,893,916]" page="637" pageId="10" pageNumber="51" year="1924">
<bibRefCitation id="EFDBB8A5FF8E1861E141FCDAFDC3FC54" author="Bagnall" box="[376,530,893,916]" pageId="10" pageNumber="59" refString="Bagnall, R. S. (1924) Brief descriptions of new Thysanoptera. XIV. Annals and Magazine of Natural History, (9) 14, 625 - 640." type="journal article" year="1924">
Bagnall, 
<number id="3D82D773FF8E1861E1E0FCDAFDC3FC54" box="[473,530,893,916]" pageId="10" pageNumber="51" value="1924.0">1924</number>
</bibRefCitation>
