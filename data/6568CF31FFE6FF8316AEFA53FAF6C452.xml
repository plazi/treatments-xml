<quantity id="2A39D3C2FFE6FF8316D8F967FE50C5D8" box="[225,314,1701,1722]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" pageId="9" pageNumber="120" unit="mm" value="13.5">13.5 mm</quantity>
