(
<bibRefCitation id="895003D6FFE6FF831328F9D0FAFFC545" author="Uchida" box="[1297,1429,1554,1575]" pageId="9" pageNumber="120" refString="Uchida, T. (1956) Ueber die Gattung Spilophion Cameron. Insecta Matsumurana, 20, 17 - 18." type="journal article" year="1956">
Uchida, 
<number id="5B096C00FFE6FF83135DF9D0FAFFC545" box="[1380,1429,1554,1575]" pageId="9" pageNumber="120" value="1956.0">1956</number>
