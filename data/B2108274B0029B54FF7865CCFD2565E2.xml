<taxonomicName id="FDB948E1B0029B54FF7864B0FE1E62EA" authority="MOCSARY" authorityName="MOCSARY" box="[178,469,940,960]" class="Insecta" family="Masaridae" genus="Chrysis" kingdom="Animalia" order="Hymenoptera" pageId="57" pageNumber="62" phylum="Arthropoda" rank="species" species="peringuezi">
