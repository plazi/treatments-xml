<number id="3D9C24E5FFB83D72FAA2C61DFFB9FE98" box="[1371,1420,332,356]" pageId="15" pageNumber="516" value="0.21">0.21</number>
)/ 
<number id="3D9C24E5FFB83D72FF6EC621FB3CFE74" box="[151,265,368,392]" pageId="15" pageNumber="516" value="0.185" valueMax="0.19" valueMin="0.18">0.18–0.19</number>
, PME 
<number id="3D9C24E5FFB83D72FEACC63EFBF0FE74" box="[341,453,367,392]" pageId="15" pageNumber="516" value="0.055" valueMax="0.06" valueMin="0.05">0.05–0.06</number>
(
<number id="3D9C24E5FFB83D72FE2DC63EF833FE74" box="[468,518,367,392]" pageId="15" pageNumber="516" value="0.05">0.05</number>
