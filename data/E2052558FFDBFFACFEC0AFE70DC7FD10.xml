<collectingCountry id="12BBD4DEFFDBFFACFD78AC170F3BFDD0" box="[731,786,569,593]" name="India" pageId="6" pageNumber="639">India</collectingCountry>
recorded from 
<collectingRegion id="A8685AACFFDBFFACFC17AC17084DFDD0" box="[948,1124,569,593]" country="India" name="Andhra Pradesh" pageId="6" pageNumber="639">Andhra Pradesh</collectingRegion>
