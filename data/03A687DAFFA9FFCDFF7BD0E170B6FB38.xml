<taxonomicName id="4C0F4D4FFFA9FFCDFE04D15071B5FCDF" authority="madraspatanum (Fabricius)" baseAuthorityName="Fabricius" box="[488,1008,829,856]" class="Insecta" family="Sphecidae" genus="Sceliphron" kingdom="Animalia" order="Hymenoptera" pageId="17" pageNumber="78" phylum="Arthropoda" rank="subSpecies" species="madraspatanum" subSpecies="madraspatanum">
