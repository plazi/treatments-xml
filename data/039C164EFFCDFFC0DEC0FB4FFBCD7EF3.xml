<taxonomicName id="4C35DCDBFFCCFFC0D84EF989FD257ECE" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="badius">
