<subSubSection id="C32FF4D3FFCDFFC1DEC0FB4FFB1C7C73" box="[754,1158,1624,1647]" pageId="76" pageNumber="77" type="nomenclature">
<paragraph id="8B8AA758FFCDFFC1DEC0FB4FFB1C7C73" blockId="76.[754,1158,1624,1647]" box="[754,1158,1624,1647]" pageId="76" pageNumber="77">
<heading id="D0C21034FFCDFFC1DEC0FB4FFB1C7C73" box="[754,1158,1624,1647]" centered="true" fontSize="9" level="2" pageId="76" pageNumber="77" reason="2">
<taxonomicName id="4C35DCDBFFCDFFC1DEC0FB4FFC757C73" authorityName="Liu, Ronquist &amp; Nordlander" authorityYear="2007" box="[754,1007,1624,1647]" class="Insecta" family="Liopteridae" genus="Paramblynotus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="76" pageNumber="77" phylum="Arthropoda" rank="species" species="badius" status="new species">
<emphasis id="B9417B4AFFCDFFC1DEC0FB4FFC757C73" bold="true" box="[754,1007,1624,1647]" italics="true" pageId="76" pageNumber="77">Paramblynotus badius</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="A272C631FFCDFFC1DFCDFB4EFB1C7C73" box="[1023,1158,1625,1647]" pageId="76" pageNumber="77" rank="species">new species</taxonomicNameLabel>
