<taxonomicName id="4C35DCDBFFCCFFC0D838F988FBF07EA8" authority="Ragonot, 1890" authorityName="Ragonot" authorityYear="1890" box="[1034,1130,1183,1204]" class="Insecta" family="Pyralidae" genus="Papua" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="coruscus">
