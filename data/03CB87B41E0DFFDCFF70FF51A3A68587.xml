<taxonomicName id="4C624D211E0DFFDCFF07FE89A2898004" authority="LINSENMAIER, 1968" authorityName="LINSENMAIER" authorityYear="1968" box="[197,655,256,276]" class="Insecta" family="Chrysididae" genus="Hedychridium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="34" pageNumber="587" phylum="Arthropoda" rank="species" species="viridisulcatum" status="nov.syn.">
<emphasis id="B916EAB01E0DFFDCFF07FE89A1C98004" box="[197,463,256,276]" italics="true" pageId="34" pageNumber="587">Hedychridium viridisulcatum</emphasis>
