<emphasis id="A3D2EA87FF8AFF8BD4C5E09F562D1AD8" bold="true" box="[151,367,1736,1759]" pageId="86" pageNumber="87">FIGURES 254–261.</emphasis>
<taxonomicName id="56A64D16FF8AFF8BD524E09E55241AD9" box="[374,614,1737,1759]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="stannardi">
<emphasis id="A3D2EA87FF8AFF8BD524E09E55241AD9" box="[374,614,1737,1759]" italics="true" pageId="86" pageNumber="87">Holopothrips stannardi</emphasis>
</taxonomicName>
and 
<taxonomicName id="56A64D16FF8AFF8BD6C9E09D54451AD9" box="[667,775,1738,1759]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="striatus">
<emphasis id="A3D2EA87FF8AFF8BD6C9E09D54451AD9" box="[667,775,1738,1759]" italics="true" pageId="86" pageNumber="87">H. striatus</emphasis>
</taxonomicName>
. 254–256. 
<taxonomicName id="56A64D16FF8AFF8BD72FE09E532E1AD9" box="[893,1132,1737,1759]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="86" pageNumber="87" phylum="Arthropoda" rank="species" species="stannardi">
