<treatmentCitationGroup id="B1B611BBFF89FF88D4C5E17D55F91B46" box="[151,699,1834,1856]" pageId="85" pageNumber="86">
<taxonomicName id="56A64D16FF89FF88D4C5E17D55F41B46" authority="Cabrera &amp; Segarra, 2008: 232" authorityName="Cabrera &amp; Segarra" authorityPageNumber="232" authorityYear="2008" box="[151,694,1834,1856]" class="Insecta" family="Phlaeothripidae" genus="Holopothrips" higherTaxonomySource="GBIF" kingdom="Animalia" order="Thysanoptera" pageId="85" pageNumber="86" phylum="Arthropoda" rank="species" species="tabebuia">
