<quantity id="4CEF9B25FF86FFA29AB3FA44F92535D4" box="[718,868,1462,1489]" metricMagnitude="3" metricUnit="m" metricValue="1.6335" metricValueMax="2.905" metricValueMin="0.362" pageId="23" pageNumber="24" unit="m" value="1633.5" valueMax="2905.0" valueMin="362.0">362–2,905 m</quantity>
