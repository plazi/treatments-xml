<paragraph id="8BE236E7FF9EFF87FF22FDDCACCEFD7E" blockId="35.[151,647,619,645]" box="[151,647,619,645]" pageId="35" pageNumber="251">
<taxonomicName id="4C5D4D64FF9EFF87FF22FDDCAFE5FD7F" baseAuthorityName="Walker" baseAuthorityYear="1856" box="[151,428,619,645]" class="Insecta" family="Chironomidae" genus="Endochironomus" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="251" phylum="Arthropoda" rank="species" species="impar">
