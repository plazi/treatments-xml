sp. (
<taxonomicName id="24C44D59677A5E71FE7CF92FE9074FA1" box="[461,586,1759,1784]" class="Insecta" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="203" phylum="Arthropoda" rank="order">Hemiptera</taxonomicName>
: 
<taxonomicName id="24C44D59677A5E71FDE2F92FE9AA4FA1" box="[595,743,1759,1784]" class="Insecta" family="Eriococcidae" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="203" phylum="Arthropoda" rank="family">Eriococcidae</taxonomicName>
) (
<bibRefCitation id="87554B2B677A5E71FD4CF92FE8F94FA1" author="Blanchard" box="[765,948,1759,1784]" pageId="2" pageNumber="203" refString="Blanchard, E. E. (1940) Apuntes sobre encirtidos argentinos. Anales de la Sociedad Cientifica Argentina, 130 (3), 106 - 128." type="journal article" year="1940">
Blanchard 
<number id="550C24FD677A5E71FCC8F910E8F94FA1" box="[889,948,1760,1784]" pageId="2" pageNumber="203" value="1940.0">1940</number>
</bibRefCitation>
; 
<bibRefCitation id="87554B2B677A5E71FC71F910EF3D4FA1" author="De" box="[960,1136,1759,1784]" pageId="2" pageNumber="203" refString="De Santis, L. (1964 [1963]) Encirtidos de la Republica Argentina (Hymenoptera: Chalcidoidea). Anales de la Comision de Investigacion Cientifica de la Gobernacion de la Provincia de Buenos Aires, 4, 9 - 422." type="journal article" year="1964">
