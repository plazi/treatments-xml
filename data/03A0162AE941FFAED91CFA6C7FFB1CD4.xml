<taxonomicName id="4C09DCBFE940FFAEDE7FFE0D7FFA1FF2" baseAuthorityName="Lopushinskaya" baseAuthorityYear="1965" box="[1121,1247,458,484]" class="Strophomenata" family="Chilidiopsidae" genus="Morinorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Strophomenatida" pageId="7" pageNumber="80" phylum="Brachiopoda" rank="species" species="proprius">
<emphasis id="B97D7B2EE940FFAEDE7FFE0D7F5C1FF2" box="[1121,1145,458,483]" italics="true" pageId="7" pageNumber="80">M</emphasis>
. 
<emphasis id="B97D7B2EE940FFAEDE94FE0D7FFA1FF2" box="[1162,1247,458,483]" italics="true" pageId="7" pageNumber="80">proprius</emphasis>
</taxonomicName>
