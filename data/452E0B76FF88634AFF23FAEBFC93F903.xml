<specimenCount id="DB8171E9FF88634AFC34F934FC05F926" box="[896,990,1720,1746]" pageId="10" pageNumber="11" type="egg">
<number id="7B4FA847FF88634AFC34F934FC46F926" box="[896,925,1720,1746]" pageId="10" pageNumber="11" value="51.0">51</number>
eggs
</specimenCount>
