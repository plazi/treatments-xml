<quantity id="ED459B4A1F09FFF6FD73F858FCB1D7F5" box="[681,774,1939,1963]" metricMagnitude="-3" metricUnit="m" metricValue="3.7" pageId="8" pageNumber="503" unit="mm" value="3.7">3.7 mm</quantity>
