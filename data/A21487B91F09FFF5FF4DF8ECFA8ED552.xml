<subSubSection id="62A765241F0AFFF5FF1DFB4CFAD6D49B" pageId="11" pageNumber="506" type="description">
<paragraph id="2A0236AF1F0AFFF5FF1DFB4CFAD6D49B" blockId="11.[151,1437,151,1293]" pageId="11" pageNumber="506">
<emphasis id="18C9EABD1F0AFFF5FF1DFB4CFE82D4FF" bold="true" box="[199,309,1159,1184]" pageId="11" pageNumber="506">Variation</emphasis>
. Body length 
<quantity id="ED459B4A1F0AFFF5FE3AFB43FD3DD4FF" box="[480,650,1160,1184]" metricMagnitude="-3" metricUnit="m" metricValue="3.58" metricValueMax="3.83" metricValueMin="3.33" pageId="11" pageNumber="506" unit="mm" value="3.58" valueMax="3.83" valueMin="3.33">3.33–3.83 mm</quantity>
; forewing 
<quantity id="ED459B4A1F0AFFF5FCD2FB4CFC19D4FF" box="[776,942,1159,1184]" metricMagnitude="-3" metricUnit="m" metricValue="2.91" metricValueMax="3.16" metricValueMin="2.66" pageId="11" pageNumber="506" unit="mm" value="2.91" valueMax="3.16" valueMin="2.66">2.66–3.16 mm</quantity>
