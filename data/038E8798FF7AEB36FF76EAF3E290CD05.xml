<number id="3DEF24A9FF7BEB36FF1EE82CE29CCD05" box="[255,387,584,608]" pageId="5" pageNumber="542" value="1872.5" valueMax="1902.0" valueMin="1843.0">
<date id="FF99104EFF7BEB36FF1EE82CE29CCD05" box="[255,387,584,608]" pageId="5" pageNumber="542" value="1843" valueMax="1902">1843–1902</date>
</number>
).
</paragraph>
</subSubSection>
