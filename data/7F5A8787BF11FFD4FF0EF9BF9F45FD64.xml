<paragraph id="F74C3691BF10FFD4FF5EFDFA9EE9FD14" blockId="83.[151,1436,151,738]" box="[199,1276,631,658]" pageId="83" pageNumber="84">
<emphasis id="C587EA83BF10FFD4FF5EFDFA9BA7FD17" bold="true" box="[199,434,631,657]" pageId="83" pageNumber="84">World distribution</emphasis>
. 
<collectingCountry id="8FE47601BF10FFD4FE58FDF5982BFD14" box="[449,574,632,658]" name="Antarctica" pageId="83" pageNumber="84">Antarctica</collectingCountry>
, 
<collectingCountry id="8FE47601BF10FFD4FDD0FDF59885FD14" box="[585,656,632,658]" name="Brazil" pageId="83" pageNumber="84">Brazil</collectingCountry>
