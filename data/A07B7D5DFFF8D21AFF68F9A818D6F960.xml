. 
<taxonomicName id="EFD2B7C8FFF9D21AFED3F9C91836F9C6" authority="Marseul" authorityName="Marseul" authorityYear="1775" box="[300,626,1538,1560]" class="Insecta" family="Meloidae" genus="Mylabris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="madoni" subGenus="Mylabris">
<emphasis id="1AA61059FFF9D21AFED3F9C91BCCF9C6" box="[300,392,1538,1560]" italics="true" pageId="31" pageNumber="32">Mylabris</emphasis>
(
<emphasis id="1AA61059FFF9D21AFE66F9C81BF1F9C6" box="[409,437,1539,1560]" italics="true" pageId="31" pageNumber="32">M.</emphasis>
) 
