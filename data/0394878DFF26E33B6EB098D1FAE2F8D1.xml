<taxonomicName id="4C3D4D18FF20E3396F199EE3FD1BF847" authority="Gressitt, 1946" authorityName="Gressitt" authorityYear="1946" box="[318,705,1979,2001]" class="Insecta" family="Chrysomelidae" genus="Chlamisus" kingdom="Animalia" order="Coleoptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="ferrugineus">
