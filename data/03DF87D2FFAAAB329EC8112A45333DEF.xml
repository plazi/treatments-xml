<typeStatus id="54CD8866FFABAB329F96153844733D14" box="[457,558,1309,1335]" pageId="71" pageNumber="72">paratype</typeStatus>
of 
<taxonomicName id="4C764D47FFABAB329C09153845123D14" authority=", Anapolis" authorityName="Anapolis" box="[598,847,1309,1335]" class="Insecta" family="Corethrellidae" genus="Corethrella" kingdom="Animalia" order="Diptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="species" species="alticola">
