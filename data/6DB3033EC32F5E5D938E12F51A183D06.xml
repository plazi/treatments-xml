, 
<taxonomicName id="BF44501EED8CE3E6E8D292AB01321F45" class="Liliopsida" family="Orchidaceae" genus="Ophrys" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Ophrys iricolor" order="Asparagales" pageId="5" pageNumber="70" phylum="Arthropoda" rank="species" species="iricolor">
