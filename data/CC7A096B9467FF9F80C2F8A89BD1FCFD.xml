<taxonomicName id="83D3C3FE9467FF9C8663F4859B3CF4C4" authority="Dybas." authorityName="Dybas." box="[1086,1406,2824,2860]" class="Liliopsida" family="Pandanaceae" genus="Pandanus" kingdom="Plantae" order="Pandanales" pageId="13" pageNumber="102" phylum="Tracheophyta" rank="genus">
