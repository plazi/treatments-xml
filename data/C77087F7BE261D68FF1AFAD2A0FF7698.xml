<taxonomicName id="88D94D62BE261D68FADFFB7FA0FF7615" box="[1362,1436,311,332]" class="Malacostraca" family="Typhlotanaidae" genus="Hamatipeda" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="319" phylum="Arthropoda" rank="species" species="sima">
<emphasis id="7DADEAF3BE261D68FADFFB7FA0FF7615" box="[1362,1436,311,332]" italics="true" pageId="10" pageNumber="319">H. sima</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
</keyStep>
<keyStep id="F42D2E44BE261D68FF1AFB1BA0FF7698" pageId="10" pageNumber="319">
<paragraph id="4F6636E1BE261D68FF1AFB1BA0FE76DF" blockId="10.[151,1437,222,449]" pageId="10" pageNumber="319">
<keyLead id="F42895D4BE261D68FF1AFB1BA0FE76DF" pageId="10" pageNumber="319">
