<taxonomicName id="88D94D62BE261D68FAC5FB51A0FE7677" baseAuthorityName="Kudinova-Pasternak" baseAuthorityYear="1975" box="[1352,1437,281,302]" class="Malacostraca" family="Typhlotanaidae" genus="Hamatipeda" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="319" phylum="Arthropoda" rank="species" species="longa">
<emphasis id="7DADEAF3BE261D68FAC5FB51A0FE7677" box="[1352,1437,281,302]" italics="true" pageId="10" pageNumber="319">H. longa</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph id="4F6636E1BE261D68FF1AFB7EA0FF7615" blockId="10.[151,1437,222,449]" box="[151,1436,310,332]" pageId="10" pageNumber="319">
<keyLead id="F42895D4BE261D68FF1AFB7EA0FF7615" box="[151,1436,310,332]" pageId="10" pageNumber="319">
