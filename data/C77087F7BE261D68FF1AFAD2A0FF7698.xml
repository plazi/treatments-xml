<taxonomicName id="88D94D62BE261D68FA95FB39A0FE76DF" authorityName="Blazewicz-Paszkowycz" authorityYear="2007" box="[1304,1437,369,390]" class="Malacostraca" family="Typhlotanaidae" genus="Hamatipeda" kingdom="Animalia" order="Tanaidacea" pageId="10" pageNumber="319" phylum="Arthropoda" rank="species" species="trapezoida">
