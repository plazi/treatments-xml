<taxonomicName id="4C0616E3FF85027DFDBE9A2E18CA7066" authorityName="Wang, Liao and Lin" authorityYear="1981" box="[603,742,1370,1395]" class="Arachnida" family="Trombiculidae" genus="Leptotrombidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Actinedida" pageId="73" pageNumber="74" phylum="Arthropoda" rank="species" species="allosetum">
