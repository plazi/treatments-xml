, 
<taxonomicName id="3F8950CAFFBAD20FF380AA01FBBAFDD1" authority="Makarchenko et Semenchenko" authorityName="Makarchenko et Semenchenko" box="[641,1151,585,611]" class="Insecta" family="Chironomidae" genus="Diamesa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="590" phylum="Arthropoda" rank="species" species="alibaevae">
