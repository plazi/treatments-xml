<taxonomicName id="79D1D0CDFFD9FFC4FD7C1F51E0290D30" authority=", Ostinops" authorityName="Ostinops" box="[725,1074,323,347]" class="Aves" family="Thraupidae" genus="Paroaria" kingdom="Animalia" order="Passeriformes" pageId="5" pageNumber="266" phylum="Chordata" rank="species" species="dominicana">
