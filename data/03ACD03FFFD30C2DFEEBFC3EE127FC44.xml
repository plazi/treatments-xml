, 
<collectorName id="26F004FFFFD30C2BFCDEFB7BE3D8FACB" box="[828,943,1266,1288]" pageId="50" pageNumber="351">A. Smetana</collectorName>
[
<specimenCode id="DBA3C952FFD30C2BFC5EFB7BE38EFACB" box="[956,1017,1266,1288]" collectionCode="T" name="Tavera, Department of Geology and Geophysics" pageId="50" pageNumber="351">T160</specimenCode>
