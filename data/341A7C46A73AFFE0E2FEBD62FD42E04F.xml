<heading id="E7447A3CA73AFFE0E2FEBD62FD58E23A" bold="true" box="[151,732,1109,1135]" fontSize="11" level="1" pageId="4" pageNumber="5" reason="1">
<taxonomicName id="7BB3B6D3A73AFFE0E2FEBD62FD58E23A" authority="Aguiar et Ramos, 2011" authorityName="Aguiar et Ramos" authorityYear="2011" box="[151,732,1109,1135]" class="Insecta" family="Ichneumonidae" genus="Digonocryptus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="atrozyrix">
