: 
<number id="3D8B2494FF8A120AF1288924316EF7CE" box="[551,592,1146,1168]" pageId="35" pageNumber="36" value="345.0">345</number>
, 
<typeStatus id="54F88811FF8A120AF16F892531ACF7CE" box="[608,658,1147,1168]" pageId="35" pageNumber="36">Type</typeStatus>
