<taxonomicName id="4C434D30FF8A120AF1E3892430C9F7CE" authority="Gunther, 1859" authorityName="Gunther" authorityYear="1859" box="[748,1015,1146,1168]" class="Amphibia" family="Hylidae" genus="Pelodryas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anura" pageId="35" pageNumber="36" phylum="Chordata" rank="genus">
