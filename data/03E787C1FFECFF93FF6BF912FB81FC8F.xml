<taxonomicName id="4C4E4D54FFECFF9EFCE5F83FFCB7F812" box="[793,894,1950,1975]" class="Insecta" family="Archisargidae" genus="Calosargus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="antiquus">
