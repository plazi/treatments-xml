<taxonomicName id="6183B088FF81E01EFD8763DEFC82FCD2" box="[602,843,764,789]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="gazae">
