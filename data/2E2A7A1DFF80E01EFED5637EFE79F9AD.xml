<taxonomicName id="6183B088FF81E01EFD11670AFCD8F986" box="[716,785,1576,1601]" class="Insecta" family="Symmocidae" genus="Glyphidocera" kingdom="Animalia" order="Lepidoptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="gazae">
