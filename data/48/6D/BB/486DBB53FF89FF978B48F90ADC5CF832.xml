<paragraph blockId="19.[189,310,1820,1846]" box="[189,310,1820,1846]" pageId="19" pageNumber="20">
<heading bold="true" box="[189,310,1820,1846]" fontSize="11" level="3" pageId="19" pageNumber="20" reason="3">
