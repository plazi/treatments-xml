<collectingCountry box="[510,636,352,378]" name="Democratic Republic of the Congo" pageId="179" pageNumber="180">DR Congo</collectingCountry>
and NE 
<collectingCountry box="[738,824,352,378]" name="Angola" pageId="179" pageNumber="180">Angola</collectingCountry>
