<paragraph pageId="0" pageNumber="7935">
<taxonomicName authority="Cresson, 1887" authorityName="Cresson" authorityYear="1887" family="Trigonalidae" lsidName="" pageId="0" pageNumber="7935" rank="family">Trigonalidae Cresson, 1887</taxonomicName>
