<collectingRegion box="[203,341,1575,1601]" country="Canada" name="Nova Scotia" pageId="8" pageNumber="13">Nova Scotia</collectingRegion>
and 
<collectingRegion box="[399,493,1575,1601]" country="Canada" name="Ontario" pageId="8" pageNumber="13">Ontario</collectingRegion>
