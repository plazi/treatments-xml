<paragraph blockId="8.[140,1108,1363,1636]" pageId="8" pageNumber="13">
<taxonomicName box="[185,420,1540,1565]" class="Insecta" family="Staphylinidae" genus="Leptusa" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="13" phylum="Arthropoda" rank="species" species="gatineauensis">
<emphasis box="[185,420,1540,1565]" italics="true" pageId="8" pageNumber="13">Leptusa gatineauensis</emphasis>
