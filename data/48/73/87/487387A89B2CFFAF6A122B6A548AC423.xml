<paragraph blockId="20.[189,342,592,618]" box="[189,342,592,618]" pageId="20" pageNumber="21">
<heading bold="true" box="[189,342,592,618]" fontSize="11" level="3" pageId="20" pageNumber="21" reason="3">
<emphasis bold="true" box="[189,342,592,618]" pageId="20" pageNumber="21">Distribution</emphasis>
</heading>
</paragraph>
<paragraph blockId="20.[189,1398,638,700]" pageId="20" pageNumber="21">
<quantity box="[189,342,639,665]" metricMagnitude="3" metricUnit="m" metricValue="1.15" metricValueMax="2.0" metricValueMin="0.3" pageId="20" pageNumber="21" unit="m" value="1150.0" valueMax="2000.0" valueMin="300.0">300–2000 m</quantity>
depth. South 
<collectingCountry box="[521,591,638,664]" name="China" pageId="20" pageNumber="21">China</collectingCountry>
Sea, Kermadec Islands and 
<collectingCountry box="[951,1128,638,664]" name="Norfolk Island" pageId="20" pageNumber="21">Norfolk Island</collectingCountry>
(
<collectingCountry box="[1149,1318,638,665]" name="New Zealand" pageId="20" pageNumber="21">New Zealand</collectingCountry>
), 
