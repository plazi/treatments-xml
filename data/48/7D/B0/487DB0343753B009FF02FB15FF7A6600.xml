<emphasis bold="true" box="[932,955,1535,1555]" pageId="7" pageNumber="506">23</emphasis>
) male sternite VII; (
<emphasis bold="true" box="[230,253,1557,1577]" pageId="7" pageNumber="506">24</emphasis>
) male sternite VIII; (
<emphasis bold="true" box="[452,475,1557,1577]" pageId="7" pageNumber="506">25</emphasis>
