. 
<number box="[396,410,222,246]" pageId="19" pageNumber="20" value="1.0">1</number>
Ƥ, D. R. 
<collectingCountry box="[530,632,222,246]" name="Republic of the Congo" pageId="19" pageNumber="20">CONGO</collectingCountry>
, 
<taxonomicName box="[646,710,223,246]" class="Insecta" family="Hybotidae" genus="Elaphropeza" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="47" phylum="Arthropoda" rank="species" species="kona" status="sp. nov.">Kona</taxonomicName>
