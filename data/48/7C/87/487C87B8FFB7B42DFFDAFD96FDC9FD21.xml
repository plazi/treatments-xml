<collectingRegion country="Australia" name="New South Wales" pageId="32" pageNumber="290">New South Wales</collectingRegion>
, 
<collectingCountry box="[316,425,718,742]" name="Australia" pageId="32" pageNumber="290">Australia</collectingCountry>
(
<bibRefCitation author="Schicha E." box="[440,594,718,742]" pageId="32" pageNumber="290" refId="ref59797" refString="Schicha E. 1987 - Phytoseiidae of Australia and neighboring areas - Indira Publishing House, West Bloomfield, Michigan, USA, 187 pp." type="book" year="1987">Schicha 1987</bibRefCitation>
