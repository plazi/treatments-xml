<taxonomicName authority="Ramond" authorityName="Ramond" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="41" pageNumber="299" phylum="Tracheophyta" rank="species" species="uncinata">
