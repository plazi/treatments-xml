<paragraph blockId="3.[151,1436,151,429]" box="[199,1288,260,285]" pageId="3" pageNumber="468">
<emphasis bold="true" box="[199,419,260,285]" pageId="3" pageNumber="468">
European 
<collectingCountry box="[324,414,260,285]" name="Turkey" pageId="3" pageNumber="468">Turkey</collectingCountry>
