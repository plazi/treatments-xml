<paragraph blockId="11.[151,1437,151,526]" pageId="11" pageNumber="52">
<emphasis bold="true" box="[197,363,306,332]" pageId="11" pageNumber="52">Distribution.</emphasis>
<taxonomicName box="[372,608,306,331]" class="Insecta" family="Dolichopodidae" genus="Cheiromyia" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="52" phylum="Arthropoda" rank="species" species="laselva">
