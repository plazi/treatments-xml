<taxonomicName authorityName=": Hass" authorityYear="1991" box="[543,711,1947,1971]" class="Reptilia" family="Sphaerodactylidae" genus="Sphaerodactylus" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="167" phylum="Chordata" rank="genus">Sphaerodactylus</taxonomicName>
g. 
<taxonomicName authorityName="Stejneger" authorityYear="1904" box="[761,912,1947,1971]" class="Reptilia" family="Sphaerodactylidae" genus="Sphaerodactylus" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="167" phylum="Chordata" rank="species" species="grandisquamis">grandisquamis</taxonomicName>
