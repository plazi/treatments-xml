<quantity metricMagnitude="3" metricUnit="m" metricValue="1.15" metricValueMax="1.7" metricValueMin="0.6" pageId="18" pageNumber="297" unit="m" value="1150.0" valueMax="1700.0" valueMin="600.0">
<quantity metricMagnitude="3" metricUnit="m" metricValue="1.15" metricValueMax="1.7" metricValueMin="0.6" pageId="18" pageNumber="297" unit="m" value="1150.0" valueMax="1700.0" valueMin="600.0">600-1700 m,</quantity>
</quantity>
