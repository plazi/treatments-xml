<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[953,1036,958,982]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="4" pageNumber="284">USNM</collectionCode>
