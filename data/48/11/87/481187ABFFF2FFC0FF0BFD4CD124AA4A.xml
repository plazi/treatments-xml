<number box="[1157,1231,957,982]" pageId="4" pageNumber="284" value="64923.0">64923</number>
), allotype female (
<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[158,242,994,1018]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="4" pageNumber="284">USNM</collectionCode>
