<heading bold="true" box="[151,670,583,609]" fontSize="11" level="1" pageId="6" pageNumber="43" reason="1">
<taxonomicName authority="Murray, 1884" authorityName="Murray" authorityYear="1884" box="[151,670,583,609]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="6" pageNumber="43" phylum="Chordata" rank="species" species="kushmorensis">
<emphasis bold="true" box="[151,670,583,609]" pageId="6" pageNumber="43">
<emphasis bold="true" box="[151,492,583,609]" italics="true" pageId="6" pageNumber="43">Hemidactylus kushmorensis</emphasis>
Murray, 
<number box="[612,670,583,609]" pageId="6" pageNumber="43" value="1884.0">1884</number>
</emphasis>
</taxonomicName>
