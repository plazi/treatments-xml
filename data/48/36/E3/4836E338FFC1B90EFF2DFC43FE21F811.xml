), 
<taxonomicName authorityName="Bates" authorityYear="1885" box="[1292,1440,1534,1558]" class="Insecta" family="Cerambycidae" genus="Cacostola" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="3" phylum="Arthropoda" rank="species" species="rugicollis">
