/ 
<number box="[628,643,332,356]" pageId="5" pageNumber="34" value="4.0">4</number>
, toe III: 
<number box="[739,753,332,356]" pageId="5" pageNumber="34" value="5.0">5</number>
/ 
<number box="[762,777,332,356]" pageId="5" pageNumber="34" value="6.0">6</number>
, toe IV: 
<number box="[873,888,332,356]" pageId="5" pageNumber="34" value="7.0">7</number>
/ 
