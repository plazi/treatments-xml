/ 
<number box="[1369,1384,260,284]" pageId="5" pageNumber="34" value="4.0">4</number>
, finger IV: 
<number box="[235,249,295,319]" pageId="5" pageNumber="34" value="6.0">6</number>
/ 
<number box="[259,273,295,319]" pageId="5" pageNumber="34" value="6.0">6</number>
, and finger V: 
<number box="[437,451,295,319]" pageId="5" pageNumber="34" value="5.0">5</number>
/ 
<number box="[462,476,295,319]" pageId="5" pageNumber="34" value="5.0">5</number>
; toe I: 
