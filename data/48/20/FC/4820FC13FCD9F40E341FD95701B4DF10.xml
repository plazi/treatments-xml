Paratype 
<taxonomicName authorityName="Eggers" authorityYear="1940" class="Lecanoromycetes" family="Stereocaulaceae" genus="Xyleborus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Xyleborus metacuneolus" order="Lecanorales" pageId="0" pageNumber="1" phylum="Ascomycota" rank="species" species="metacuneolus">Xyleborus metacuneolus</taxonomicName>
