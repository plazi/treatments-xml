<collectingCountry box="[607,678,868,890]" name="Canada" pageId="30" pageNumber="201">Canada</collectingCountry>
of 
<taxonomicName authorityName="Casey" authorityYear="1906" box="[708,887,868,890]" class="Insecta" family="Staphylinidae" genus="Tachyusa" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="201" phylum="Arthropoda" rank="species" species="americana">
