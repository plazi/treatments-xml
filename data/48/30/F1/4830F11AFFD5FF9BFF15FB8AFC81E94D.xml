<paragraph blockId="7.[140,1108,1433,1671]" pageId="7" pageNumber="178">
<emphasis bold="true" box="[185,387,1504,1530]" pageId="7" pageNumber="178">
<emphasis box="[185,302,1504,1530]" inLineHeading="true" pageId="7" pageNumber="178" reason="1">Bionomic</emphasis>
Notes.
</emphasis>
In 
<collectingRegion box="[429,614,1504,1531]" country="Canada" name="New Brunswick" pageId="7" pageNumber="178">New Brunswick</collectingRegion>
, 
<taxonomicName authorityName="Mannerheim" authorityYear="1830" box="[627,728,1504,1531]" class="Insecta" family="Staphylinidae" genus="Aleochara" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="178" phylum="Arthropoda" rank="species" species="villosa">
<emphasis box="[627,728,1504,1531]" italics="true" pageId="7" pageNumber="178">A. villosa</emphasis>
</taxonomicName>
