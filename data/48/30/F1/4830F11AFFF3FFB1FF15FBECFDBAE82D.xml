, 
<collectingCountry box="[607,678,868,890]" name="Canada" pageId="34" pageNumber="205">Canada</collectingCountry>
of 
<taxonomicName authorityName="Casey" authorityYear="1911" box="[708,874,868,889]" class="Insecta" family="Staphylinidae" genus="Myllaena" kingdom="Animalia" order="Coleoptera" pageId="34" pageNumber="205" phylum="Arthropoda" rank="species" species="procidua">
