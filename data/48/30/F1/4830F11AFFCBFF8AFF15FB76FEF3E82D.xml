<paragraph blockId="25.[140,1108,941,1390]" pageId="25" pageNumber="196">
Brook, 
<geoCoordinate box="[225,356,941,968]" degrees="45.6860" direction="north" orientation="latitude" pageId="25" pageNumber="196" precision="5" value="45.686">45.6860°N</geoCoordinate>
, 
<geoCoordinate box="[368,501,941,967]" degrees="66.8668" direction="west" orientation="longitude" pageId="25" pageNumber="196" precision="5" value="-66.8668">66.8668°W</geoCoordinate>
, 
