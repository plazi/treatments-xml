<quantity box="[617,719,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="5.28" pageId="2" pageNumber="437" unit="mm" value="5.28">5.28 mm</quantity>
, PW: 
<quantity box="[786,888,259,286]" metricMagnitude="-3" metricUnit="m" metricValue="2.88" pageId="2" pageNumber="437" unit="mm" value="2.88">2.88 mm</quantity>
