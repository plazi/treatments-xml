<paragraph id="8BCA36888F6D204E38B3FD73F0F549CA" blockId="11.[151,1437,476,935]" box="[199,738,728,755]" pageId="11" pageNumber="527">
<taxonomicName id="4C754D0B8F6D204E38B3FD73F0F549CA" authority="Harris &amp; Armitage 2015" authorityName="Harris &amp; Armitage" authorityYear="2015" box="[199,738,728,755]" class="Insecta" family="Hydroptilidae" genus="Neotrichia" kingdom="Animalia" order="Trichoptera" pageId="11" pageNumber="527" phylum="Arthropoda" rank="species" species="parabullata">
