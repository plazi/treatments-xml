<taxonomicName id="DC987993FFBFFFC28E0BFB285A17FB37" authority="Latreille, 1794" authorityName="Latreille" authorityYear="1794" box="[352,626,1161,1185]" class="Insecta" family="Psychodidae" genus="Psychoda" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="207" phylum="Arthropoda" rank="genus">
<emphasis id="29ECDE02FFBFFFC28E0BFB2859A2FB37" box="[352,455,1161,1185]" italics="true" pageId="8" pageNumber="207">Psychoda</emphasis>
