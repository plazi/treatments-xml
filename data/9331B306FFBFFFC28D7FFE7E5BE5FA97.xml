, 
<taxonomicName id="DC987993FFBFFFC28C30FB2B5C02FB37" authority="(Quate, 1965)" authorityName="Ježek &amp; Harten" authorityYear="2005" baseAuthorityName="Quate" baseAuthorityYear="1965" box="[859,1127,1161,1185]" class="Insecta" family="Psychodidae" genus="Psychana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="207" phylum="Arthropoda" rank="species" species="moleva" status="comb. nov.">
