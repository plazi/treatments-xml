<paragraph id="7C8F50A7552755447C0AE648A1C1FBAF" pageId="257" pageNumber="258">
<taxonomicName id="FD834FB0F81D1D30D94C4936A2BF00EE" class="Insecta" family="Tachinidae" genus="Phorocera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Phorocera crassipalpis" order="Diptera" pageId="257" pageNumber="258" phylum="Arthropoda" rank="species" species="crassipalpis">Phorocera crassipalpis</taxonomicName>
