(
<taxonomicName id="7E8B813A2D3D27B8D2329EE10670337A" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33608" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole barbata Wheeler" lsidName-HNS="Pheidole barbata Wheeler" order="Hymenoptera" pageNumber="560" phylum="Arthropoda" rank="species" species="barbata">barbata</taxonomicName>
