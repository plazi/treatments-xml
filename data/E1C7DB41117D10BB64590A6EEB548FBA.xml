<taxonomicName id="5EC59FAF2C78FD2BE966964369946358" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:33944" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole psammophila Creighton &amp; Gregg" lsidName-HNS="Pheidole psammophila Creighton &amp; Gregg" order="Hymenoptera" pageNumber="560" phylum="Arthropoda" rank="species" species="psammophila">psammophila</taxonomicName>
