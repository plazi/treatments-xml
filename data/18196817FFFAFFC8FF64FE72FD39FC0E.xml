[
<geoCoordinate id="F584BFC6FFFAFFC8FACAFCA4FA17FC92" box="[1337,1432,834,859]" direction="south" orientation="latitude" pageId="4" pageNumber="5" precision="555" value="-16.59">16.59°S</geoCoordinate>
, 
