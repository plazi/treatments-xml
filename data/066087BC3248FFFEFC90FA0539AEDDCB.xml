<emphasis id="BCBDEAB83248FFFFFCEFFA673C00DDDB" bold="true" box="[850,1074,1499,1523]" pageId="10" pageNumber="543">Material examined.</emphasis>
<figureCitation id="16F22A2F3248FFFFFB87FA673CA7DDDB" box="[1082,1173,1499,1523]" captionStart="Figure 1" captionStartId="1.[168,230,1069,1089]" captionTargetBox="[168,1419,160,1045]" captionTargetId="figure-443@1.[168,1419,160,1045]" captionTargetPageId="1" captionText="Figure 1. Location of collection sites in Montenegro, Romania, and Serbia of fungus gnats." pageId="10" pageNumber="543">Figure 1</figureCitation>
. 
<collectingCountry id="F6DE763A3248FFFFFB1EFA673D18DDDB" box="[1187,1322,1499,1523]" name="Serbia" pageId="10" pageNumber="543">Montenegro</collectingCountry>
: Žabljak, Durmitor Mt., Crna lake, spruce forest along a small stream, 
<quantity id="49319B4F3248FFFFFC3FF9A23BE6DE1D" box="[898,980,1566,1590]" metricMagnitude="3" metricUnit="m" metricValue="1.448" pageId="10" pageNumber="543" unit="m" value="1448.0">1448 m</quantity>
a.s.l., 
<geoCoordinate id="EBFD506D3248FFFFFBA6F9A13CABDE1D" box="[1051,1177,1565,1589]" degrees="43.1486" direction="north" orientation="latitude" pageId="10" pageNumber="543" precision="5" value="43.1486">43.1486° N</geoCoordinate>
, 
<geoCoordinate id="EBFD506D3248FFFFFB18F9A23D1CDE1D" box="[1189,1326,1566,1590]" degrees="019.0882" direction="east" orientation="longitude" pageId="10" pageNumber="543" precision="5" value="19.0882">019.0882° E</geoCoordinate>
