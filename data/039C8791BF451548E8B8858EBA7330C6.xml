<taxonomicName id="4C354D04BF441548E91C81F1BC4B30C7" baseAuthorityName="Nickel" baseAuthorityYear="2003" box="[307,573,655,681]" class="Liliopsida" family="Poaceae" genus="Helictotrichon" kingdom="Plantae" order="Poales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="pratense">
