<collectingCountry id="F31C7638FF93A533CD93C168FD67A771" box="[702,766,312,338]" name="India" pageId="42" pageNumber="43">India</collectingCountry>
; 
<taxonomicName id="4C0B4D2BFF93A533CC21C168FC21A772" authorityName="A. Adams" authorityYear="1854" box="[780,952,312,337]" class="Gastropoda" family="Conidae" genus="Conus" kingdom="Animalia" order="Neogastropoda" pageId="42" pageNumber="43" phylum="Mollusca" rank="species" species="praecellens">
