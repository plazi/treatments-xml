<taxonomicName id="1ECDC834AEE3383186AEF24D03CC0E6B" class="Ascidiacea" family="Didemnidae" genus="Trididemnum" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Trididemnum symbioticum" order="Aplousobranchia" pageId="2" pageNumber="57" phylum="Chordata" rank="species" species="symbioticum">Trididemnum symbioticum</taxonomicName>
