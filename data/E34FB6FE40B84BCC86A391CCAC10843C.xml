<taxonomicName id="E479FD312E93A23B89C1096C2B5AAA45" LSID-ZBK="urn:lsid:zoobank.org:act:22A92A57-BBC3-4553-B0E3-A2C95E8F6BD4" class="Actinopterygii" family="Rivulidae" genus="Moema" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-ZBK="Moema Costa 1989:223" order="Cyprinodontiformes" pageNumber="2" phylum="Chordata" rank="genus">Moema Costa</taxonomicName>
