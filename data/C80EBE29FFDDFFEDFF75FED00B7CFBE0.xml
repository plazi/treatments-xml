= 204 (205), SC 
<subScript id="DC230D7AFFDCFFEDFEE3FCE60E1FFD51" attach="left" box="[257,269,673,687]" fontSize="6" pageId="27" pageNumber="28">
<specimenCount id="56A1C4B6FFDCFFEDFEE3FCE60E1FFD51" box="[257,269,673,687]" pageId="27" pageNumber="28" type="male">♂</specimenCount>
</subScript>
= 79 (77), 
<collectionCode id="26B697FAFFDCFFEDFE65FCD40EA9FD55" box="[391,443,659,683]" country="USA" lsid="urn:lsid:biocol.org:col:14844" name="Scripps Institution of Oceanography, University of California" pageId="27" pageNumber="28" type="Herbarium">SCR</collectionCode>
</materialsCitation>
<materialsCitation id="F0CF0562FFDCFFEDFE59FCE60B85FD54" ID-GBIF-Occurrence="2028140028" box="[443,1175,658,687]" collectionCode="IL" pageId="27" pageNumber="28" specimenCount="1" specimenCount-male="1" typeStatus="holotype">
<subScript id="DC230D7AFFDCFFEDFE59FCE60ED5FD51" attach="left" box="[443,455,673,687]" fontSize="6" pageId="27" pageNumber="28">
