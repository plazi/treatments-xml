<quantity id="4CFE9B6E195FFF986B5CA6BE71D6F967" box="[362,506,1753,1779]" metricMagnitude="-3" metricUnit="m" metricValue="7.05" metricValueMax="7.4" metricValueMin="6.7" pageId="127" pageNumber="128" unit="mm" value="7.05" valueMax="7.4" valueMin="6.7">6.7–7.4 mm</quantity>
, wing 
<quantity id="4CFE9B6E195FFF98687EA6BE72F4F967" box="[584,728,1753,1779]" metricMagnitude="-3" metricUnit="m" metricValue="5.5" metricValueMax="5.9" metricValueMin="5.1" pageId="127" pageNumber="128" unit="mm" value="5.5" valueMax="5.9" valueMin="5.1">5.1–5.9 mm</quantity>
