<taxonomicName id="50EC725E075ACB0A5200737089406710" authorityName="Chanabun &amp; Panha" authorityYear="2013" class="Clitellata" family="Almidae" genus="Glyphidrilus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Haplotaxida" pageId="44" pageNumber="45" phylum="Annelida" rank="species" species="vangthongensis">
<emphasis id="6C324EC898F120D081257E1C950014CE" italics="true" pageId="44" pageNumber="45">Glyphidrilus vangthongensis</emphasis>
</taxonomicName>
