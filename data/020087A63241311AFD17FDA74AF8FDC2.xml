(
<bibRefCitation id="EE384B41324E311BFE31FAFA4A5EFAC0" author="GROSS W." box="[453,563,1302,1319]" pageId="8" pageNumber="281" pagination="91 - 161" refId="ref7721" refString="GROSS W. 1947. - Die Agnathen und Acanthodier des obersilurischen Beyrichienkalks. Palaeontographica A 96: 91 - 161." type="journal article" year="1947">Gross, 1947</bibRefCitation>
)
</taxonomicName>
scale MMMC02553 in crown (anterior edge at bottom) and anterior views; 
<emphasis id="B8DDEAA2324E311BFF63FAC048C8FADA" bold="true" box="[151,165,1324,1341]" pageId="8" pageNumber="281">G</emphasis>
, 
<emphasis id="B8DDEAA2324E311BFF5BFAC048D0FADA" bold="true" box="[175,189,1324,1341]" pageId="8" pageNumber="281">H</emphasis>
, 
<taxonomicName id="4DA94D33324E311BFF3CFAC04920FADA" box="[200,333,1324,1341]" class="Pisces (awaiting allocation)" family="Pisces (awaiting allocation)" genus="Trundlelepis" kingdom="Animalia" order="Pisces (awaiting allocation)" pageId="8" pageNumber="281" phylum="Chordata" rank="species" species="undetermined">
