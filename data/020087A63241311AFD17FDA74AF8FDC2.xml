<taxonomicName id="4DA94D33324E311BFF3DFAFA4A54FAC0" authority="(Gross, 1947)" baseAuthorityName="Gross" baseAuthorityYear="1947" box="[201,569,1302,1319]" class="Pisces (awaiting allocation)" family="Pisces (awaiting allocation)" genus="Gomphonchoporus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pisces (awaiting allocation)" pageId="8" pageNumber="281" phylum="Chordata" rank="species" species="hoppei">
