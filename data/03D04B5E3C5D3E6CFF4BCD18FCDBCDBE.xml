<quantity id="4C8157AD3C5E3E6AFAFACF32FF4DCA63" metricMagnitude="-4" metricUnit="m" metricValue="8.05" metricValueMax="8.1" metricValueMin="8.0" pageId="2" pageNumber="61" unit="mm" value="0.805" valueMax="0.81" valueMin="0.8">0.80–0.81 mm</quantity>
(
<quantity id="4C8157AD3C5E3E6AFF08CF17FECACA63" box="[212,322,1637,1663]" metricMagnitude="-4" metricUnit="m" metricValue="8.1" pageId="2" pageNumber="61" unit="mm" value="0.81">0.81 mm</quantity>
). LP: 
<quantity id="4C8157AD3C5E3E6AFE5ACF17FDB8CA63" box="[390,560,1637,1663]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" metricValueMax="4.699999999999999" metricValueMin="4.3" pageId="2" pageNumber="61" unit="mm" value="0.44999999999999996" valueMax="0.47" valueMin="0.43">0.43–0.47 mm</quantity>
(
<quantity id="4C8157AD3C5E3E6AFDE2CF17FD25CA63" box="[574,685,1637,1663]" metricMagnitude="-4" metricUnit="m" metricValue="4.5" pageId="2" pageNumber="61" unit="mm" value="0.45">0.45 mm</quantity>
