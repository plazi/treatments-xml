(
<quantity id="4C8157AD3C5E3E6AFC69CF17FBACCA63" box="[949,1060,1637,1663]" metricMagnitude="-3" metricUnit="m" metricValue="1.28" pageId="2" pageNumber="61" unit="mm" value="1.28">1.28 mm</quantity>
