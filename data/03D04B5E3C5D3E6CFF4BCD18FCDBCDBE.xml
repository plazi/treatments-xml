<quantity id="4C8157AD3C5E3E6AFC6FCF32FBD4CA46" box="[947,1116,1600,1626]" metricMagnitude="-4" metricUnit="m" metricValue="9.25" metricValueMax="9.3" metricValueMin="9.2" pageId="2" pageNumber="61" unit="mm" value="0.925" valueMax="0.93" valueMin="0.92">0.92–0.93 mm</quantity>
(
<quantity id="4C8157AD3C5E3E6AFBB7CF32FB51CA46" box="[1131,1241,1600,1626]" metricMagnitude="-4" metricUnit="m" metricValue="9.2" pageId="2" pageNumber="61" unit="mm" value="0.92">0.92 mm</quantity>
