<collectionCode id="EECE7BF15A36FF981225FA19FA9EFA7B" box="[1304,1385,1414,1438]" country="0" httpUri="http://grbio.org/cool/jn0k-k005" name="Public Museum of Grand Rapids" pageId="23" pageNumber="324">GRPM</collectionCode>
