<taxonomicName id="4FDF98B75A36FF981618FAFEFD2EFA9F" authority="Brito &amp; Moreira" authorityName="Brito &amp; Moreira" authorityYear="2017" box="[293,729,1377,1402]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="23" pageNumber="324" phylum="Arthropoda" rank="species" species="selene">
