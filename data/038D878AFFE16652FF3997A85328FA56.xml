<heading id="D0D381F0FFE1665DFF3997A85229FB2F" bold="true" box="[151,612,1224,1251]" fontSize="11" level="1" pageId="7" pageNumber="428" reason="1">
<emphasis id="B950EA8EFFE1665DFF3997A85229FB2F" bold="true" box="[151,612,1224,1251]" pageId="7" pageNumber="428">
<taxonomicName id="4C244D1FFFE1665DFF3997A8512DFB2E" box="[151,352,1224,1250]" class="Insecta" family="Cicadellidae" genus="Evinus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="428" phylum="Arthropoda" rank="species" species="obscurus" status="sp. nov.">
<emphasis id="B950EA8EFFE1665DFF3997A8512DFB2E" bold="true" box="[151,352,1224,1250]" italics="true" pageId="7" pageNumber="428">Evinus obscurus</emphasis>
</taxonomicName>
