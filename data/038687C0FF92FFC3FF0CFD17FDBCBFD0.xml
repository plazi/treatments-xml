<number id="3DE724F1FF92FFC2FBD7F8F1FBDDB96A" box="[1100,1146,1874,1898]" pageId="14" pageNumber="490" value="230.0">230</number>
; width 
<number id="3DE724F1FF92FFC2FB48F8F1FAA7B96A" box="[1235,1280,1874,1898]" pageId="14" pageNumber="490" value="170.0">170</number>
