<taxonomicName id="20ED8892256EFFB18C2CFE64FB011943" box="[1078,1193,505,525]" class="Insecta" family="Miridae" genus="Aoplonema" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
