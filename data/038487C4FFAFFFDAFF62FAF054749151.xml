<number id="3DE524F5FFAFFFDBFBB0F803550C9797" box="[1093,1101,1940,1954]" pageId="4" pageNumber="133" value="1.0">1</number>
and cu 
<number id="3DE524F5FFAFFFDBFB55F80355E99797" box="[1184,1192,1940,1954]" pageId="4" pageNumber="133" value="1.0">1</number>
