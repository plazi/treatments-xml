<taxonomicName id="74BF4D01FF3DF8ABFE92F894B910E223" authority="Newstead." authorityName="Newstead." box="[287,563,1883,1905]" class="Insecta" family="Coccidae" genus="Ceroplastes" kingdom="Animalia" order="Hemiptera" pageId="183" pageNumber="184" phylum="Arthropoda" rank="species" species="quadrilineatus">
