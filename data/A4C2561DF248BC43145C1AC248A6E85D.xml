<taxonomicName id="01F27CFB56E3FBC2A7F013405051226A" class="Insecta" family="Chrysididae" genus="Loboscelidia" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Loboscelidia pasohana" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pasohana">Loboscelidia pasohana</taxonomicName>
