<materialsCitation id="80F03CA8FFDB556DB4A3F96FFB99A48B" ID-GBIF-Occurrence="1417439956" box="[151,1041,1687,1713]" collectionCode="ZJUH" location="Ma Yun" municipality="Xiamen" pageId="2" pageNumber="461" specimenCount="1" specimenCount-female="1" stateProvince="Fujian" typeStatus="paratype">
