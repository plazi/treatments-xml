<taxonomicName id="4C30B3ADFFAFFFB8FC69FB7AFAD57FA4" authority="Sheng, Sun &amp; Li" authorityName="Sheng, Sun &amp; Li" authorityYear="2020" box="[922,1301,1161,1188]" class="Insecta" family="Ichneumonidae" genus="Campodorus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="albilineatus" status="sp. nov.">
