<taxonomicName id="4C30B3ADFFAFFFB8FC6DFCB7FAD07861" authority="Sheng, Sun &amp; Li" authorityName="Sheng, Sun &amp; Li" authorityYear="2020" box="[926,1296,839,866]" class="Insecta" family="Ichneumonidae" genus="Campodorus" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="punctatus" status="sp. nov.">
