<collectionCode id="CEE4FDA3FFC2FFADD3DC2D4E1E1CFEB4" box="[508,529,277,299]" country="Switzerland" lsid="urn:lsid:biocol.org:col:15706" name="Conservatoire et Jardin botaniques de la Ville de Genève" pageId="50" pageNumber="1727" type="Herbarium">G</collectionCode>
