<subSubSection id="C37028A4FFD1FFF1FF4BF9F74990FE6D" lastPageId="6" lastPageNumber="7" pageId="4" pageNumber="5" type="diagnosis">
<paragraph id="8BD57B2FFFD1FFF3FF4BF9F74C16FB54" blockId="4.[189,311,1180,1206]" box="[189,311,1180,1206]" pageId="4" pageNumber="5">
<heading id="D09DCC43FFD1FFF3FF4BF9F74C16FB54" bold="true" box="[189,311,1180,1206]" fontSize="11" level="2" pageId="4" pageNumber="5" reason="2">
