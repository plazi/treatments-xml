</taxonomicName>
in 
<collectingCountry id="7CBB78B2C42EFF94FDC9FE0EFD58919A" box="[543,691,477,503]" name="South Korea" pageId="7" pageNumber="58">South Korea</collectingCountry>
(
<bibRefCitation id="603D45D3C42EFF94FD15FE0EFC02919A" author="Mikhaljova" box="[707,1001,477,503]" pageId="7" pageNumber="61" refString="Mikhaljova, E. V. &amp; Lim, K. Y. (2000) Millipede fauna (Diplopoda) of South Korea. Korean Journal of Systematic Zoology, 16 (2), 147 - 157." type="journal article" year="2000">
Mikhaljova &amp; Lim 
<number id="B2642A05C42EFF94FC7DFE0EFC02919A" box="[939,1001,477,503]" pageId="7" pageNumber="58" value="2000.0">2000</number>
</bibRefCitation>
; 
<bibRefCitation id="603D45D3C42EFF94FC20FE0EFB81919A" author="Lim" box="[1014,1130,477,503]" pageId="7" pageNumber="60" refString="Lim, K. Y. (2001) Taxonomy of millipedes (Arthropoda: Diplopoda) in Korea. Thesis of the degree of Doctor of Philosophy, Chonbuk National University, Jeonju, Korea, 256 pp." type="book" year="2001">
