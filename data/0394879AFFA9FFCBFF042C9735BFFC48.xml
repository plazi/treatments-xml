<quantity id="4CC59B69FFAAFFCBFD4C2CBC356DFF52" box="[735,837,188,212]" metricMagnitude="-4" metricUnit="m" metricValue="5.1" pageId="10" pageNumber="53" unit="mm" value="0.51">0.51 mm</quantity>
