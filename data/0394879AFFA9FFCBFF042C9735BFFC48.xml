<quantity id="4CC59B69FFAAFFCBFE762CBC3463FF52" box="[485,587,188,212]" metricMagnitude="-3" metricUnit="m" metricValue="1.22" pageId="10" pageNumber="53" unit="mm" value="1.22">1.22 mm</quantity>
