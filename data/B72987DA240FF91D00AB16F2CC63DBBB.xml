<caption id="6BFF66442409F91C00AB11BDCA67DEF0" box="[151,1389,2008,2032]" httpUri="https://zenodo.org/record/189068/files/figure.png" pageId="12" pageNumber="13" targetBox="[151,1436,197,1981]" targetPageId="12">
<paragraph id="3F3F36CC2409F91C00AB11BDCA67DEF0" blockId="12.[151,1389,2008,2032]" box="[151,1389,2008,2032]" pageId="12" pageNumber="13">
