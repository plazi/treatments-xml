<taxonomicName id="3EA04D828B72FFC6636DF8B8FD027466" authority="Robin, 1877" authorityName="Robin" authorityYear="1877" box="[317,719,1845,1871]" class="Arachnida" family="Pterolichidae" genus="Pterolichus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="delibatus">
