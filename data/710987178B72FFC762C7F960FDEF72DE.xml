<taxonomicName id="3EA04D828B73FFC761C7FE88FAEE7236" authority="(Norner, 1883)" baseAuthorityName="Norner" baseAuthorityYear="1883" box="[919,1315,261,287]" class="Arachnida" family="Gabuciniidae" genus="Gabucinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sarcoptiformes" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="urogalli">
