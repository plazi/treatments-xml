<taxonomicName id="3EA04D828B73FFC762B8FF14FE46739A" authority="Kaup" authorityName="Kaup" authorityYear="1829" box="[232,395,153,179]" class="Aves" family="Corvidae" genus="Coloeus" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="8" phylum="Chordata" rank="genus">
<emphasis id="CBD4EA138B73FFC762B8FF14FE89739A" box="[232,324,153,179]" italics="true" pageId="7" pageNumber="8">Coloeus</emphasis>
Kaup
</taxonomicName>
and 
<taxonomicName id="3EA04D828B73FFC76398FF17FD91739A" authority="Brisson" authorityName="Brisson" authorityYear="1760" box="[456,604,153,179]" class="Aves" family="Corvidae" genus="Pica" kingdom="Animalia" order="Passeriformes" pageId="7" pageNumber="8" phylum="Chordata" rank="genus">
