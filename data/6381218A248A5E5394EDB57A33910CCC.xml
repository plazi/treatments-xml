<taxonomicName id="E96EC0B6C657368F1BCD31B38271AE3D" genus="Agonum-" lsidName="" pageId="729" pageNumber="730" rank="genus">
<emphasis id="4F29A6BCFE4ED6DBB7EAD6510AC84E28" italics="true" pageId="729" pageNumber="730">Agonum-</emphasis>
</taxonomicName>
