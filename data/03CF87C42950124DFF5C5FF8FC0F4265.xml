<taxonomicName id="4C664D512951124DFCEE5B9BFBE34246" authorityName="Tovar-Hernandez and Salazar-Vallejo" authorityYear="2006" box="[805,1041,1268,1292]" class="Insecta" family="Carabidae" genus="Megalomma" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="48" phylum="Arthropoda" rank="species" species="perkinsi">
