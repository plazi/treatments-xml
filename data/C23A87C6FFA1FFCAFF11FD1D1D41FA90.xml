<date id="3E2D1010FFA1FFCAFE7EFCAB1E8CFCAC" box="[504,650,827,851]" pageId="18" pageNumber="19" value="1974-04-23">
<number id="FC5B24F7FFA1FFCAFE7EFCAB1E12FCAC" box="[504,532,827,851]" pageId="18" pageNumber="19" value="23.0">23</number>
Apr 
<number id="FC5B24F7FFA1FFCAFDC8FCAB1E8CFCAC" box="[590,650,827,851]" pageId="18" pageNumber="19" value="1974.0">1974</number>
</date>
