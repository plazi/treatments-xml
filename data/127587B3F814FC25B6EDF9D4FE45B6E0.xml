<taxonomicName id="5DDC4D26F81BFC25B7F3F8A6FDB8B67C" authorityName="Hasan, Islam, Kuramoto, Kurabayashi &amp; Sumida" authorityYear="2014" box="[393,553,1857,1883]" class="Amphibia" family="Microhylidae" genus="Microhyla" kingdom="Animalia" order="Anura" pageId="8" pageNumber="79" phylum="Chordata" rank="species" species="mukhlesuri">
