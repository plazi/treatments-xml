<taxonomicName id="5DDC4D26F81BFC25B58FF8F9FA8AB610" baseAuthorityName="Blyth" baseAuthorityYear="1855" box="[1013,1307,1821,1847]" class="Amphibia" family="Microhylidae" genus="Kalophrynus" kingdom="Animalia" order="Anura" pageId="8" pageNumber="79" phylum="Chordata" rank="species" species="interlineatus">
