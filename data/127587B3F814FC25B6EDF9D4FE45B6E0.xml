<taxonomicName id="5DDC4D26F81BFC25B778F8FAFE2DB610" box="[258,444,1821,1847]" class="Amphibia" family="Microhylidae" genus="Kaloula" kingdom="Animalia" order="Anura" pageId="8" pageNumber="79" phylum="Chordata" rank="species" species="indochinensis">
