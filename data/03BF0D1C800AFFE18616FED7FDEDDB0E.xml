<treatmentCitationGroup id="AB069B24800AFFE18616FE49FC9CDDE4" box="[264,866,385,409]" pageId="9" pageNumber="10">
<taxonomicName id="4C16C789800AFFE18616FE49FCA0DDE4" authority="Becker 1925 Becker (1925: 249)" authorityName="Becker 1925 Becker" authorityPageNumber="249" authorityYear="1925" box="[264,862,385,409]" class="Insecta" family="Asilidae" genus="Machimus" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="aurulentus">
<emphasis id="B9626018800AFFE18616FE49FE11DDE5" box="[264,495,386,409]" italics="true" pageId="9" pageNumber="10">Machimus aurulentus</emphasis>
<bibRefCitation id="EF87C1FB800AFFE186E9FE4AFD80DDE4" author="Becker" box="[503,638,385,408]" pageId="9" pageNumber="24" refString="Becker, T. (1925) H. Sauters Formosa Ausbeute: Asilinae III. (Diptera). Entomologische Mitteilungen, 14, 62 - 85." type="journal article" year="1925">
