, 
<collectingCountry id="B9D5761BFFBBFFF6FB17ADFA5D10F959" box="[1219,1287,1749,1773]" name="Brazil" pageId="12" pageNumber="617">Brazil</collectingCountry>
, 
<collectingCountry id="B9D5761BFFBBFFF6FAC5ADFA5D90F959" box="[1297,1415,1749,1773]" name="Guatemala" pageId="12" pageNumber="617">Guatemala</collectingCountry>
, 
<collectingCountry id="B9D5761BFFBBFFF6FCF9ADD85B96F8BB" box="[813,897,1783,1807]" name="Mexico" pageId="12" pageNumber="617">Mexico</collectingCountry>
, 
<collectingCountry id="B9D5761BFFBBFFF6FC59ADD85BEAF8BB" box="[909,1021,1783,1807]" name="Nicaragua" pageId="12" pageNumber="617">Nicaragua</collectingCountry>
, 
<collectingCountry id="B9D5761BFFBBFFF6FBDEADD85C76F8BA" box="[1034,1121,1783,1806]" name="Panama" pageId="12" pageNumber="617">Panama</collectingCountry>
, and 
<collectingCountry id="B9D5761BFFBBFFF6FB48ADD85D1AF8BB" box="[1180,1293,1783,1807]" name="Venezuela" pageId="12" pageNumber="617">Venezuela</collectingCountry>
