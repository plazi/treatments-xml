and 
<taxonomicName id="4C4C88F4FFFEFFA2FDC2FEBE0E94FEF6" box="[542,660,260,284]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="358" phylum="Tracheophyta" rank="species" species="dentata">
