<paragraph id="6132466AFFA6FFD7FF5C08ECFB122525" blockId="1.[151,821,220,280]" box="[151,274,255,280]" pageId="1" pageNumber="123">
(
<figureCitation id="F9B65AEFFFA6FFD7FF5408ECFB092525" box="[159,265,255,280]" captionStart-0="FIGURE 1" captionStart-1="FIGURE 2" captionStart-2="FIGURE 3 a, b" captionStart-3="FIGURE 4" captionStartId-0="1.[151,250,1956,1978]" captionStartId-1="2.[151,250,1060,1082]" captionStartId-2="3.[151,250,1665,1687]" captionStartId-3="4.[151,250,1979,2001]" captionTargetBox-0="[176,1399,1107,1896]" captionTargetBox-1="[209,1368,278,1005]" captionTargetBox-2="[259,1316,204,1615]" captionTargetId-0="figure@1.[150,1436,1058,1934]" captionTargetId-1="figure@2.[150,1436,193,1039]" captionTargetId-2="figure@3.[248,1339,193,1644]" captionTargetPageId-0="1" captionTargetPageId-1="2" captionTargetPageId-2="3" captionText-0="FIGURE 1. Cryptophasa watungi sp. nov. ♂" captionText-1="FIGURE 2. Cryptophasa watungi sp. nov. ♀" captionText-2="FIGURE 3 a, b. Male genitalia of Cryptophasa watungi sp. nov." captionText-3="FIGURE 4. Female genitalia of Cryptophasa watungi sp. nov." httpUri-0="https://zenodo.org/record/232067/files/figure.png" httpUri-1="https://zenodo.org/record/232068/files/figure.png" httpUri-2="https://zenodo.org/record/232069/files/figure.png" pageId="1" pageNumber="123">
