<taxonomicName id="1EE24D200129370E74DBF9E5E82F0257" authority="Ogloblin, 1955" authorityName="Ogloblin" authorityYear="1955" box="[681,1145,1554,1578]" class="Insecta" family="Mymaridae" genus="Dicopomorpha" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="macrocephala">
