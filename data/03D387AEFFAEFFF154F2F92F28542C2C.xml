</taxonomicName>
, 
<taxonomicName id="4C7A4D3BFFAFFFF657B0F9DC2D712B2A" box="[981,1104,1555,1580]" class="Insecta" family="Scelionidae" genus="Heptascelio" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="albipes">
