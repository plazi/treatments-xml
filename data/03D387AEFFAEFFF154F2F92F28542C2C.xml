</taxonomicName>
and 
<taxonomicName id="4C7A4D3BFFAFFFF650E9F9DC2C382B2A" box="[1164,1305,1555,1580]" class="Insecta" family="Scelionidae" genus="Heptascelio" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="anthonyi">
