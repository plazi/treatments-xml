<taxonomicName id="4C714D2281E0FF4CFCEFFD9BFBF4FD97" authority="K. Y. Lang" authorityName="K. Y. Lang" box="[867,1095,555,581]" class="Liliopsida" family="Orchidaceae" genus="Platanthera" kingdom="Plantae" order="Asparagales" pageId="207" pageNumber="208" phylum="Tracheophyta" rank="species" species="curvata">
