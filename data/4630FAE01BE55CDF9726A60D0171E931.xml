<emphasis id="ACCF9D44838C17B8A0259C226867EBAC" italics="true" pageId="0" pageNumber="53">Orbamia</emphasis>
</taxonomicName>
, 
<taxonomicName id="2AF08CC2AF84E99F3C948A128108B7DB" authority="Hausmann &amp; Tujuba, 2020" authorityName="Hausmann &amp; Tujuba" authorityYear="2020" class="Insecta" family="Geometridae" genus="Rabomia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rabomia" order="Lepidoptera" pageId="0" pageNumber="53" phylum="Arthropoda" rank="genus">
<emphasis id="27D11E764EE0382B83203417EF679680" italics="true" pageId="0" pageNumber="53">Rabomia</emphasis>
