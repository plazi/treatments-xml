<taxonomicName id="4C7B4D7AFF92E478AA7E506E424AFDF9" authority="(Spencer)" authorityName="Spencer" box="[524,841,512,536]" class="Insecta" family="Agromyzidae" genus="Liriomyza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="turneri">
