<collectionCode id="ED5BAE1FFFF7AB7A7F2AFD4AFEC7E9E5" box="[277,297,645,671]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15406" lsid="urn:lsid:biocol.org:col:15406" name="Harvard University - Arnold Arboretum" pageId="30" pageNumber="31" type="Herbarium">
