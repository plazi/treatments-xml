<number id="4F2B248AB24812377B1FFEFAFAABFE45" box="[1356,1395,373,397]" pageId="39" pageNumber="540" value="1.3">1.3</number>
; F i: 
<number id="4F2B248AB24812377EC4FE13FF66FE7C" box="[151,190,412,436]" pageId="39" pageNumber="540" value="3.9">3.9</number>
