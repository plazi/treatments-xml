<taxonomicName id="4C034D26FFCA350DFF79FC9A24CADC3B" authority="Joicey &amp; Talbot, 1921" authorityName="Joicey &amp; Talbot" authorityYear="1921" box="[151,713,834,860]" class="Insecta" family="Hesperiidae" genus="Ceratrichia" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="313" phylum="Arthropoda" rank="species" species="semlikensis">
