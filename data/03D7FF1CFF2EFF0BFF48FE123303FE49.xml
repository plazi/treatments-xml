<taxonomicName id="4C7E3589FF2EFF08FD0AFE8435FAF9F2" authority="Schaum, 1848" authorityName="Schaum" authorityYear="1848" box="[642,1158,1513,1538]" class="Insecta" family="Cetoniidae" genus="Gymnetis" kingdom="Animalia" order="Coleoptera" pageId="229" pageNumber="228" phylum="Arthropoda" rank="subSpecies" species="bajula" subSpecies="wollastonii">
