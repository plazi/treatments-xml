<normalizedToken id="7D38C22ABEB33A796A0B47645FD1250A" originalValue="Зайсан">Zaisan</normalizedToken>
] (
<geoCoordinate id="882A88BA9D432DC1BE30332813C7245B" direction="north" orientation="latitude" precision="925" value="47.566666">47°34'N</geoCoordinate>
, 
<geoCoordinate id="6DE572693AADB5CA2A46F6C1B711E750" direction="east" orientation="longitude" precision="925" value="84.65">84°39'E</geoCoordinate>
), 453 m a.s.l., 6 V 2017, 2♂♂, leg. RP; 1♀, leg. JH; 1♀, leg. KL; 5 km NE of Zaysan [
<normalizedToken id="59B0B4836629AF691014B6408C28086A" originalValue="Зайсан">Zaisan</normalizedToken>
] (
<geoCoordinate id="3272B74FE68733A5606952C68C74DADB" direction="north" orientation="latitude" precision="925" value="47.5">47°30'N</geoCoordinate>
