<taxonomicName id="4C0E4D19E37EFFCE51ECBEE180B5FC1D" box="[681,753,980,1004]" class="Insecta" family="Ceratopogonidae" genus="Forcipomyia" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="rivalis">
