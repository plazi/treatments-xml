<number id="3DC624BDE370FFC0539AB9D28362FB0E" box="[223,294,1255,1279]" pageId="6" pageNumber="7" value="8575.5" valueMax="17140.0" valueMin="11.0">11–17</number>
</figureCitation>
, 
<figureCitation id="13352A1FE370FFC05274B9D28318FB0E" box="[305,348,1255,1279]" captionStart="FIGURES 140 – 141" captionStartId="46.[151,264,2010,2033]" captionTargetBox="[284,1314,202,1982]" captionTargetId="figure@46.[276,1315,193,1983]" captionTargetPageId="46" captionText="FIGURES 140 – 141. Distribution map of species of Forcipomyia (Metaforcipomyia)." httpUri="https://zenodo.org/record/215052/files/figure.png" pageId="6" pageNumber="7">140</figureCitation>
