<taxonomicName id="F5AB4D51FF880E0FFDE969208EBA8491" box="[520,676,638,663]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="57" phylum="Arthropoda" rank="species" species="zhuxiaoyui">
