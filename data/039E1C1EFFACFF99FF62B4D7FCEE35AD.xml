<taxonomicName id="4C37D68BFFACFF99FF62B4D7FD1C37A5" authority="(Muller, 1776)" baseAuthorityName="Muller" baseAuthorityYear="1776" box="[151,691,1411,1438]" class="Arachnida" family="Unionicolidae" genus="Unionicola" kingdom="Animalia" order="Trombidiformes" pageId="13" pageNumber="114" phylum="Arthropoda" rank="species" species="crassipes" subGenus="Hexatax">
