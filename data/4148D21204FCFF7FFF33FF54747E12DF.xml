<quantity id="0E19CEE104FCFF7FFEFDFE1C77A711DB" box="[345,437,465,492]" metricMagnitude="0" metricUnit="m" metricValue="3.55" pageId="172" pageNumber="173" unit="m" value="3.55">3,550 m</quantity>
