<subSubSection id="C01B0B5D7969A086489CC3B669AFD16B" pageId="7" pageNumber="66" type="latin">
<paragraph id="6F38584CE37C2897D96BDCF874C36B1D" pageId="7" pageNumber="66">
<emphasis id="CD12E81CF465FCE5A5CAB07F31EC0512" italics="true" pageId="7" pageNumber="66">
<pageBreakToken id="A5118DABE2ACF90C5376F50FF1C0FC29" pageId="7" pageNumber="66" start="start">Haec</pageBreakToken>
