<taxonomicName id="008D4D0D6964EC7DFA89FEA5A5EEFE7F" box="[1305,1451,374,401]" class="Magnoliopsida" family="Asteraceae" genus="Centaurea" kingdom="Plantae" order="Asterales" pageId="2" pageNumber="245" phylum="Tracheophyta" rank="species" species="parlatoris">
