<taxonomicName id="4C0A4D49093C285EFE97A9EAFDCCD8FA" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[305,604,921,943]" class="Malacostraca" family="Cancridae" genus="Cancer" kingdom="Animalia" order="Decapoda" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="pulex">
<emphasis id="B97EEAD8093C285EFE97A9EAFE29D8FA" box="[305,441,921,943]" italics="true" pageId="2" pageNumber="3">Cancer pulex</emphasis>
