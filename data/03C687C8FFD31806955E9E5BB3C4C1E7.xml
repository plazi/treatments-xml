<taxonomicName id="4C6F4D5DFFDC1804963E9AC3B6C5C0DD" authority="Framenau &amp; Hudson, 2017" authorityName="Framenau &amp; Hudson" authorityYear="2017" box="[263,407,1930,1956]" class="Arachnida" family="Lycosidae" genus="Tetralycosa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="halophila" status="sp. nov.">
