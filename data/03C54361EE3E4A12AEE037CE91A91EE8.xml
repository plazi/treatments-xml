<typeStatus id="54D74CD5EE334A12AC8B36E793741F98" box="[764,812,729,755]" pageId="24" pageNumber="427">type</typeStatus>
locality, 
<collectingRegion id="49A83C95EE334A12ADE336E693B01F99" box="[916,1000,728,754]" country="China" name="Hainan" pageId="24" pageNumber="427">Hainan</collectingRegion>
, 
<collectingCountry id="F37BB2E7EE334A12AD8436E694611F99" box="[1011,1081,728,754]" name="China" pageId="24" pageNumber="427">China</collectingCountry>
