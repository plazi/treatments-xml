<taxonomicName id="50994D194A1FBF496842139A484FC7AF" box="[300,512,1232,1256]" family="Chaetonotidae" genus="Lepidodermella" kingdom="Animalia" order="Chaetonotida" pageId="4" pageNumber="233" phylum="Gastrotricha" rank="subSpecies" species="minor" subSpecies="chaetifer">
