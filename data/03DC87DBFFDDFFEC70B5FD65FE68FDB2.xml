<geoCoordinate id="EE41500AFFDDFFEE73EAF909FBA6FCC1" box="[968,1031,1679,1704]" degrees="2" direction="east" minutes="15" orientation="longitude" pageId="81" pageNumber="82" precision="925" value="2.25">
<emphasis id="B901EADFFFDDFFEE73EAF909FC43FCCE" box="[968,994,1680,1704]" italics="true" pageId="81" pageNumber="82">e2</emphasis>
