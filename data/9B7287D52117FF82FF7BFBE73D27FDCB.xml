<emphasis id="21AFEAD12117FF80FF08F9F2384FF9E2" box="[228,258,1540,1565]" italics="true" pageId="5" pageNumber="58">ft''</emphasis>
) 
<number id="A51324E42117FF80FEFFF9F2387DF9E1" box="[275,304,1540,1566]" pageId="5" pageNumber="58" value="38.0">38</number>
(
<number id="A51324E42117FF80FEADF9F238DDF9E1" box="[321,400,1540,1566]" pageId="5" pageNumber="58" value="38.5" valueMax="45.0" valueMin="32.0">32–45</number>
), paraxial unguinal tarsal setae (
<emphasis id="21AFEAD12117FF80FCFCF9F23A68F9E2" box="[784,805,1540,1565]" italics="true" pageId="5" pageNumber="58">u'</emphasis>
