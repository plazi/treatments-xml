(
<number id="A51324E42117FF80FB0AFA403C68FA2F" box="[1254,1317,1462,1488]" pageId="5" pageNumber="58" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
), paraxial tibial setae (
<emphasis id="21AFEAD12117FF80FECAFA2B3863FA09" box="[294,302,1501,1526]" italics="true" pageId="5" pageNumber="58">l</emphasis>
ʹ) absent; tarsus 
<number id="A51324E42117FF80FE01FA2B3B47FA08" box="[493,522,1501,1527]" pageId="5" pageNumber="58" value="13.0">13</number>
(
<number id="A51324E42117FF80FDF6FA2B3B2AFA08" box="[538,615,1501,1527]" pageId="5" pageNumber="58" value="12.5" valueMax="14.0" valueMin="11.0">11–14</number>
), paraxial fastigial tarsal setae (
<emphasis id="21AFEAD12117FF80FC36FA2B3ABDFA09" box="[986,1008,1501,1526]" italics="true" pageId="5" pageNumber="58">ft'</emphasis>
