<number id="742624B3FF94FFB7FF157140FF416F1D" box="[193,208,1124,1145]" pageId="4" pageNumber="211" value="2.0">2</number>
. 
<taxonomicName id="05EE4D17FF94FFB7FF0E7140FE6E6F1D" authority="Blkr" authorityName="Blkr" box="[218,511,1123,1146]" class="Actinopterygii" family="Peristediidae" genus="Peristedion" kingdom="Animalia" order="Scorpaeniformes" pageId="4" pageNumber="211" phylum="Chordata" rank="species" species="moluccense">
<emphasis id="F09AEA86FF94FFB7FF0E7140FE5A6F1E" box="[218,459,1124,1146]" italics="true" pageId="4" pageNumber="211">Peristedion moluccense</emphasis>
Blkr
