<subSubSection id="C32FC9E3BD22FF8AFF23FD35FA904178" box="[199,1300,764,789]" pageId="17" pageNumber="318" type="etymology">
<paragraph id="8B8A9A68BD22FF8AFF23FD35FA904178" blockId="17.[151,1437,151,896]" box="[199,1300,764,789]" pageId="17" pageNumber="318">
<emphasis id="B941467ABD22FF8AFF23FD35FECA4178" bold="true" box="[199,334,764,789]" pageId="17" pageNumber="318">Etymology.</emphasis>
