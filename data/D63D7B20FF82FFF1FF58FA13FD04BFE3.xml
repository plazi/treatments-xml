<paragraph id="5E2BCA36FF80FFF7FF3EFC53FBD0BA33" blockId="3.[184,1074,787,1038]" box="[222,1029,1014,1038]" pageId="3" pageNumber="38">
<materialsCitation id="EEFCC06BFF80FFF7FF3EFC53FBD4BA33" box="[222,1025,1014,1038]" collectionCode="NMBE, TLMI" collectorName="in den Sammlungen" pageId="3" pageNumber="38" specimenCount="1" typeStatus="paratype">
