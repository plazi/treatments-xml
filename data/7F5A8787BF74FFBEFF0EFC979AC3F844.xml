<taxonomicName id="30F34D12BF7BFFBFFCF0F8E29E28F800" authorityName="Cornelius" authorityYear="1982" box="[873,1085,1903,1926]" class="Hydrozoa" family="Campanulariidae" genus="Campanularia" kingdom="Animalia" order="Leptothecata" pageId="56" pageNumber="57" phylum="Cnidaria" rank="species" species="agas">
