<taxonomicName id="4C364D20FFAB4747FE9FFA87FD9DFADD" authority="Kuschel, 2008" authorityName="Kuschel, Moneo" authorityYear="2008" box="[272,599,1375,1398]" class="Insecta" family="Curculionidae" genus="Orthorhinoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="111" pageNumber="210" phylum="Arthropoda" rank="species" species="caledonicus" status="n. sp.">
