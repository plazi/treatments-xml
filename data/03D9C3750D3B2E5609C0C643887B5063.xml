<typeStatus id="54CBCCC10D3B2E5608E0C045886A530A" box="[439,548,1344,1369]" pageId="14" pageNumber="347">Holotype</typeStatus>
: ♂, 
<collectingCountry id="F36732F30D3B2E560B06C04588D6530A" box="[593,664,1344,1369]" name="China" pageId="14" pageNumber="347">China</collectingCountry>
: Yunnan Prov., Lancang, 
<quantity id="4C88DF860D3B2E560AE1C0458E4B530B" box="[950,1029,1344,1368]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="14" pageNumber="347" unit="m" value="1100.0">1100m</quantity>
, 
<date id="FFCE54A30D3B2E560D47C0458E33530B" box="[1040,1149,1344,1369]" pageId="14" pageNumber="347" value="1999-11-02">
<number id="3DB860440D3B2E560D47C0458E50530B" box="[1040,1054,1344,1368]" pageId="14" pageNumber="347" value="2.0">2</number>
xi 
<number id="3DB860440D3B2E560D15C0458E33530B" box="[1090,1149,1344,1368]" pageId="14" pageNumber="347" value="1999.0">1999</number>
