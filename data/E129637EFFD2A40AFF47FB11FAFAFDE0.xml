<taxonomicName id="AE80A9EBFFD3A40AFDFDFDDFFD38FDE7" authorityName="Rafinesque" authorityYear="1810" box="[557,674,513,539]" class="Actinopterygii" family="Clupeidae" kingdom="Animalia" order="Clupeiformes" pageId="73" pageNumber="74" phylum="Chordata" rank="family">Clupeidae</taxonomicName>
