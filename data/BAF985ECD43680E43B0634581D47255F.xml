<pageBreakToken id="D76B1661CD2CCB82330D86F1BB5B726A" pageId="null" pageNumber="437" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName id="B04DEC6511EA0A19459F1FB2D5F1CC53" authority="Bell." class="Magnoliopsida" family="Rosaceae" genus="Rubus" kingdom="Plantae" order="Rosales" pageId="null" pageNumber="437" phylum="Tracheophyta" rank="species" species="glandulosus">
