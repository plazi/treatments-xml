<paragraph id="E277EE7C0F3B2B1DB3BC4E609B4FB477" pageId="60" pageNumber="135">
<taxonomicName id="69F2A8897A05CC273A4DFA384ADCEBF0" authority="Chapin, 1945" authorityName="Chapin" authorityYear="1945" class="Insecta" family="Cleridae" genus="Onychotillus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Onychotillus" order="Coleoptera" pageId="60" pageNumber="135" phylum="Arthropoda" rank="genus">Onychotillus Chapin, 1945</taxonomicName>
