<taxonomicName id="91E45BF329523447FEF9FBF5FE75FB0E" authority="O. Targ. Tozz." authorityName="O. Targ. Tozz." box="[294,399,1134,1162]" class="Liliopsida" family="Araceae" genus="Arisarum" kingdom="Plantae" order="Alismatales" pageId="1" pageNumber="116" phylum="Tracheophyta" rank="species" species="vulgare">A. vulgare</taxonomicName>
