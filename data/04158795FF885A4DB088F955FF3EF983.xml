<subSubSection id="C4A66508FF8A5A4DB0D8FA1CFE4EFA13" pageId="6" pageNumber="453" type="etymology">
<paragraph id="8C033683FF8A5A4DB0D8FA1CFBE8FA36" blockId="6.[151,1437,151,1653]" box="[199,1064,1447,1472]" pageId="6" pageNumber="453">
<emphasis id="BEC8EA91FF8A5A4DB0D8FA1CFE8EFA36" bold="true" box="[199,334,1447,1472]" pageId="6" pageNumber="453">Etymology.</emphasis>
