<subSubSection id="73803CD28E199B47FF75FB817AB7FB13" pageId="33" pageNumber="34" type="etymology">
<paragraph id="3B256F598E199B47FF75FB817AB7FB13" blockId="33.[151,1436,1051,1227]" pageId="33" pageNumber="34">
<emphasis id="09EEB34B8E199B47FF75FB817B0CFB58" bold="true" box="[197,339,1126,1152]" pageId="33" pageNumber="34">Etymology.</emphasis>
<taxonomicName id="FC9A14DA8E199B47FEECFB807B88FB58" box="[348,471,1127,1152]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="quancong">
<emphasis id="09EEB34B8E199B47FEECFB807B88FB58" box="[348,471,1127,1152]" italics="true" pageId="33" pageNumber="34">Quancong</emphasis>
</taxonomicName>
, from Vietnamese “ 
<taxonomicName id="FC9A14DA8E199B47FD75FB81791EFB58" box="[709,833,1126,1152]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="species" species="quancong">quancong</taxonomicName>
