<taxonomicName id="FC9A14DA8E279B79FF27FA4C785FFA1D" box="[151,512,1451,1477]" class="Insecta" family="Polycentropodidae" genus="Pseudoneureclipsis" kingdom="Animalia" order="Trichoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="quancong" status="new species">Pseudoneureclipsis quancong</taxonomicName>
