, 
<materialsCitation id="3B525D46FFA0FF8CD0518902EE6F754B" ID-GBIF-Occurrence="1503739459" box="[630,870,1106,1131]" country="Oman" location="South" pageId="23" pageNumber="24" specimenCount="1" stateProvince="South">
<collectingRegion id="49FE99F9FFA0FF8CD0518902EFBE754B" box="[630,695,1106,1131]" country="Maldives" name="Dhekunu" pageId="23" pageNumber="24">South</collectingRegion>
coast of 
<collectingCountry id="F32D178BFFA0FF8CD1398903EE6F754B" box="[798,870,1107,1131]" name="Oman" pageId="23" pageNumber="24">Oman</collectingCountry>
</materialsCitation>
. 
<quantity id="4CC2FAFEFFA0FF8CD1578903EEC1754A" box="[880,968,1107,1131]" metricMagnitude="0" metricUnit="m" metricValue="9.0" metricValueMax="10.0" metricValueMin="8.0" pageId="23" pageNumber="24" unit="m" value="9.0" valueMax="10.0" valueMin="8.0">8–10 m</quantity>
