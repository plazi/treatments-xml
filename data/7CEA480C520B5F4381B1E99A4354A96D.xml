<taxonomicName id="FDDB4031F7E7328E2313FBC9E2E848A3" class="Chromadorea" family="Rhabditidae" genus="Rhabditis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Rhabditis" order="Rhabditida" pageId="0" pageNumber="15" phylum="Nematoda" rank="genus">
<emphasis id="69017F7F4C8E6C4569B39B09298F678D" italics="true" pageId="0" pageNumber="15">Rhabditis</emphasis>
</taxonomicName>
