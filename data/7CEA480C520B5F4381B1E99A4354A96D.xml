<taxonomicName id="AA89F542768D4D5B1A67C1BE2F8A20DE" class="Arachnida" family="Gamasidae" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Mesostigmata" pageId="0" pageNumber="15" phylum="Arthropoda" rank="family">Gamasidae</taxonomicName>
