<taxonomicName id="744788AFB4EFF070006BBB7886A18610" authorityName="Guerin-Meneville" authorityYear="1849" class="Insecta" family="Carabidae" genus="Dromochorus" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="217" pageNumber="218" phylum="Arthropoda" rank="genus">
<emphasis id="C21397AF6256660014CC57C6AD9BAD3F" italics="true" pageId="217" pageNumber="218">Dromochorus</emphasis>
</taxonomicName>
<normalizedToken id="814E31E47A1579956F80CCE13D2BCD49" originalValue="Guérin-Méneville">Guerin-Meneville</normalizedToken>
, 1849.
</paragraph>
</subSubSection>
