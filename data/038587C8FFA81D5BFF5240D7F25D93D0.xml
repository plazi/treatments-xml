<quantity id="4CD49B3BFFA81D5AFD16436CF37C9408" box="[723,804,1623,1647]" metricMagnitude="3" metricUnit="m" metricValue="2.5" pageId="6" pageNumber="57" unit="m" value="2500.0">2500m</quantity>
