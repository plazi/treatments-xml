</taxonomicName>
<taxonomicName id="4C564D6DFFAD2D3830342CDECAD11DAD" authority="Graeser (1890)" authorityName="Graeser" authorityYear="1890" box="[812,1195,152,178]" pageId="5" pageNumber="67" rank="variety" variety="nigricostata">
