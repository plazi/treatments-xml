<taxonomicName id="00F44D1E2567E04EFE66BFC2FC2CFA18" authority="Kobayashi, Jinbo et Hirowatari" authorityName="Kobayashi, Jinbo et Hirowatari" box="[392,961,1408,1435]" class="Insecta" family="Gracillariidae" genus="Phyllocnistis" kingdom="Animalia" order="Lepidoptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="species" species="gracilistylella">
