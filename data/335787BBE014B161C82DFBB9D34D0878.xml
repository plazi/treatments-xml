(
<collectionCode id="DDEFAE68E014B160CA87F9F3D4D60C4D" box="[930,1013,1577,1603]" httpUri="http://grbio.org/cool/nwhf-jc3k" name="Adams State College Collection" pageId="10" pageNumber="11">ASCC</collectionCode>
