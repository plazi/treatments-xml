<quantity id="991010C4BF55FFEBFDCAFB3FFD5CFAD2" box="[619,676,1266,1290]" metricMagnitude="1" metricUnit="m" metricValue="1.5" pageId="30" pageNumber="31" unit="m" value="15.0">15 m</quantity>
