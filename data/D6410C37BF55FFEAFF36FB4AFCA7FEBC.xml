<subSubSection id="16F2EEAABF55FFEBFF36FB3FFEC2FAF6" pageId="30" pageNumber="31" type="materials_examined">
<paragraph id="5E57BD21BF55FFEBFF36FB3FFEC2FAF6" blockId="30.[151,1437,1266,2010]" pageId="30" pageNumber="31">
<emphasis id="6C9C6133BF55FFEBFF36FB3FFE80FAD3" bold="true" box="[151,376,1266,1291]" pageId="30" pageNumber="31">Material examined</emphasis>
. 
<emphasis id="6C9C6133BF55FFEBFE27FB3FFE30FAD3" bold="true" box="[390,456,1266,1291]" pageId="30" pageNumber="31">Stn. 8</emphasis>
, 
<date id="2A569BE1BF55FFEBFE77FB3FFDBBFAD2" box="[470,579,1266,1290]" pageId="30" pageNumber="31" value="2012-01-31">31.i.2012</date>
