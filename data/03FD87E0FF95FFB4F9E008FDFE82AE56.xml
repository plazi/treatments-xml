<taxonomicName id="4C544D75FF95FFB5FA550FA8FD12A8B4" authorityName="Becker" authorityYear="1894" box="[607,714,1503,1521]" class="Insecta" family="Scathophagidae" genus="Acerocnema" kingdom="Animalia" order="Diptera" pageId="6" pageNumber="615" phylum="Arthropoda" rank="genus">
