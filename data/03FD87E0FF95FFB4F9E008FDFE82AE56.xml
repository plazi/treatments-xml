<collectorName id="26A15320FF95FFB5F9F60F03FDBBA8C2" box="[508,611,1396,1415]" pageId="6" pageNumber="615">N.E.Burma</collectorName>
, 
<location id="8E8B602DFF95FFB5FA610F04FD66A8C3" LSID="urn:lsid:plazi:treatment:03FD87E0FF95FFB4F9E008FDFE82AE56:8E8B602DFF95FFB5FA610F04FD66A8C3" box="[619,702,1395,1414]" country="Myanmar" name="Kombaiti" pageId="6" pageNumber="615">Kombaiti</location>
