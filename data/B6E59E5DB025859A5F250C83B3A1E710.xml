<taxonomicName id="E76897553BB46DAF42A8912A6FB8FADA" class="Insecta" family="Choreutidae" genus="Anthophila" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anthophila cryptarum" order="Lepidoptera" pageId="0" pageNumber="8050" phylum="Arthropoda" rank="species" species="cryptarum">cryptarum</taxonomicName>
