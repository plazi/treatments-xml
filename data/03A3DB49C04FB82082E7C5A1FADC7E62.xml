<taxonomicName id="4C0A11DCC04FB82083EBC632FD247E62" box="[411,645,533,558]" class="Elasmobranchii" family="Parascylliidae" genus="Parascyllium" kingdom="Animalia" order="Orectolobiformes" pageId="10" pageNumber="266" phylum="Chordata" rank="species" species="collare">
