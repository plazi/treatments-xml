<taxonomicName id="4C0A11DCC04FB8208057C5D5FD727E45" box="[551,723,497,521]" class="Elasmobranchii" family="Parascylliidae" genus="Cirrhoscyllium" kingdom="Animalia" order="Orectolobiformes" pageId="10" pageNumber="266" phylum="Chordata" rank="species" species="formosanum">
