<quantity id="04139B25FF8DDC36A259FE8CFC31FB21" box="[841,902,306,332]" metricMagnitude="1" metricUnit="m" metricValue="5.0" pageId="26" pageNumber="27" unit="m" value="50.0">50 m</quantity>
