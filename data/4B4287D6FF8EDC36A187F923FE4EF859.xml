<quantity id="04139B25FF8DDC36A3A2FEE7FC88FB1E" box="[690,831,345,371]" metricMagnitude="1" metricUnit="m" metricValue="9.7" metricValueMax="10.0" metricValueMin="9.4" pageId="26" pageNumber="27" unit="m" value="97.0" valueMax="100.0" valueMin="94.0">94 to 100 m</quantity>
