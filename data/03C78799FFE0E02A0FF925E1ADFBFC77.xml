<quantity id="4C969B6AFFE0E02B0FB42487AF55FA74" box="[255,366,1488,1509]" metricMagnitude="-3" metricUnit="m" metricValue="3.5" metricValueMax="3.7" metricValueMin="3.3" pageId="12" pageNumber="1563" unit="mm" value="3.5" valueMax="3.7" valueMin="3.3">3,3-3,7mm</quantity>
(Vorderkörperlänge: 
<quantity id="4C969B6AFFE0E02B0D002487ACACFA77" box="[587,663,1488,1510]" metricMagnitude="-3" metricUnit="m" metricValue="2.1" pageId="12" pageNumber="1563" unit="mm" value="2.1">2,1mm</quantity>
