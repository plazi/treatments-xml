<taxonomicName id="4C0B4D2BFFA1A501CE4AC090FD8DA6FA" baseAuthorityName="Roding" baseAuthorityYear="1798" box="[359,532,192,217]" class="Gastropoda" family="Conidae" genus="Conus" kingdom="Animalia" order="Neogastropoda" pageId="24" pageNumber="25" phylum="Mollusca" rank="species" species="biliosus">Conus biliosus</taxonomicName>
