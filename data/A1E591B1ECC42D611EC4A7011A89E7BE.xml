<normalizedToken id="C2B67891F1550C268F4DB35140BE0F68" originalValue="zwölfgliedrige">zwoelfgliedrige</normalizedToken>
<normalizedToken id="1A92A7A1DF106D9CEFF02CEE323C9D1B" originalValue="Fühler">Fuehler</normalizedToken>
. 
<taxonomicName id="C0B9F9AD500C23BB43D3C45F08AAE5AA" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145480" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName-HNS="Monomorium heyeri Forel" order="Hymenoptera" pageNumber="299" phylum="Arthropoda" rank="species" species="heyeri">Heyeri</taxonomicName>
