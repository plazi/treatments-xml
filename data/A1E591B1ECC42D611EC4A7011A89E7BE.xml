<taxonomicName id="18D34A48BA762464FBB1A5B98203F25D" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:145480" class="Insecta" family="Formicidae" genus="Monomorium" kingdom="Animalia" lsidName-HNS="Monomorium heyeri Forel" order="Hymenoptera" pageNumber="299" phylum="Arthropoda" rank="species" species="heyeri">Heyeri</taxonomicName>
