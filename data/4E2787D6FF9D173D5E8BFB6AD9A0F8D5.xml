<emphasis id="F4FAEAD2FF9D173D5EDBFA47DA55FA6E" bold="true" box="[199,348,1423,1450]" pageId="2" pageNumber="593">
<typeStatus id="19358862FF9D173D5EDBFA47DA0BFA6E" box="[199,258,1424,1450]" pageId="2" pageNumber="593">Type</typeStatus>
