<taxonomicName id="4C4F4D0CFFB2FFC4F212FF412FFE74EB" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[327,502,188,212]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="triplex">
<emphasis id="B93BEA9DFFB2FFC4F212FF412FFE74EB" box="[327,502,188,212]" italics="true" pageId="81" pageNumber="82">Rhotana triplex</emphasis>
