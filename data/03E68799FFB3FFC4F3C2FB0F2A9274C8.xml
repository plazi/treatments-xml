<taxonomicName id="4C4F4D0CFFB2FFC4F05EFF402D9D74EB" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[779,917,188,212]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="81" pageNumber="82" phylum="Arthropoda" rank="species" species="melichari" status="sp. nov.">
