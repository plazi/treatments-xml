<quantity id="8D6B9B35FFD6FFBDFC9AFCCC1F87FC8B" box="[796,897,860,885]" metricMagnitude="1" metricUnit="m" metricValue="2.15" metricValueMax="2.5" metricValueMin="1.8" pageId="101" pageNumber="102" unit="m" value="21.5" valueMax="25.0" valueMin="18.0">18–25 m</quantity>
