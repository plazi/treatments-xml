<quantity id="8D6B9B35FFD6FFBDFCB4FE3C1F69FE3B" box="[818,879,428,452]" metricMagnitude="1" metricUnit="m" metricValue="1.8" pageId="101" pageNumber="102" unit="m" value="18.0">18 m</quantity>
