<caption id="DF492F9CFF80792D1D6FF9DA392927F0" ID-DOI="http://doi.org/10.5281/zenodo.5065680" ID-Zenodo-Dep="5065680" httpUri="https://zenodo.org/record/5065680/files/figure.png" pageId="40" pageNumber="41" startId="40.[264,367,1642,1666]" targetBox="[282,1306,316,1581]" targetPageId="40">
