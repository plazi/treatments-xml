<collectingCountry id="F0F73894FF8BAE28FF6FFC613CD5F0DE" box="[207,248,920,939]" name="United States of America" pageId="41" pageNumber="42">USA</collectingCountry>
(
<collectingRegion id="4A24B6E6FF8BAE28FEA4FC613D66F0DE" box="[260,331,920,939]" country="United States of America" name="Florida" pageId="41" pageNumber="42">Florida</collectingRegion>
