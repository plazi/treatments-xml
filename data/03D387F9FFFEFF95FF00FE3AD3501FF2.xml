<taxonomicName id="4C7A4D6CFFFEFF95FA13FC43D03218D2" authorityName="Kingsley" authorityYear="1878" class="Malacostraca" family="Alpheidae" genus="Alpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="337" phylum="Arthropoda" rank="species" species="floridanus">
