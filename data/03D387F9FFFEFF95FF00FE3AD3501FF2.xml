<taxonomicName id="4C7A4D6CFFFEFF95FC98FC6FD5CD18D2" authority="Bracken-Grissom &amp; Felder, 2014" authorityName="Bracken-Grissom &amp; Felder" authorityYear="2014" box="[783,1269,937,962]" class="Malacostraca" family="Alpheidae" genus="Alpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="6" pageNumber="337" phylum="Arthropoda" rank="species" species="ulalae">
