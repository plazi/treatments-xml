and 
<taxonomicName id="64B04D37048AFF18FB04B5C8FE93F869" authority="Schroder" authorityName="Schroder" authorityYear="1959" class="Insecta" family="Cicadellidae" genus="Molomea" kingdom="Animalia" order="Hemiptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="consolida">
<emphasis id="91C4EAA6048AFF18FB04B5C8FF03F868" italics="true" pageId="229" pageNumber="230">Molomea consolida</emphasis>
Schröder
</taxonomicName>
(the latter two from sentinel eggs only) (
<taxonomicName id="64B04D37048AFF18FCDFB5B0FC77F869" box="[810,939,2007,2033]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="tribe" tribe="Proconiini">Proconiini</taxonomicName>
); also 
<taxonomicName id="64B04D37048AFF18FC01B5B0FAFCF869" authority="(Berg)" authorityName="Berg" baseAuthorityName="Berg" baseAuthorityYear="1884" box="[1012,1312,2007,2033]" class="Insecta" family="Cicadellidae" genus="Ciminius" kingdom="Animalia" order="Hemiptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="platensis">
<emphasis id="91C4EAA6048AFF18FC01B5B0FB11F868" box="[1012,1229,2007,2032]" italics="true" pageId="229" pageNumber="230">Ciminius platensis</emphasis>
(Berg)
</taxonomicName>
and 
<taxonomicName id="64B04D37048AFF1BFAA2B5B0FE34FF2A" authority="(Fowler)" authorityName="Fowler" baseAuthorityName="Fowler" baseAuthorityYear="1900" class="Insecta" family="Cicadellidae" genus="Plesiommata" kingdom="Animalia" lastPageId="230" lastPageNumber="231" order="Hemiptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="mollicella">
<emphasis id="91C4EAA6048AFF1BFAA2B5B0FEAAFF29" italics="true" lastPageId="230" lastPageNumber="231" pageId="229" pageNumber="230">Plesiommata mollicella</emphasis>
(Fowler)
</taxonomicName>
(
<taxonomicName id="64B04D370489FF1BFE09B2FFFD5DFF2A" box="[508,641,152,178]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="230" pageNumber="231" phylum="Arthropoda" rank="tribe" tribe="Cicadellini">Cicadellini</taxonomicName>
, from sentinel eggs only) (
<taxonomicName id="64B04D370489FF1BFC3AB2FFFBB9FF2A" box="[975,1125,152,178]" class="Insecta" family="Cicadellidae" kingdom="Animalia" order="Hemiptera" pageId="230" pageNumber="231" phylum="Arthropoda" rank="family">Cicadellidae</taxonomicName>
) (Triapitsyn 
<emphasis id="91C4EAA60489FF1BFAF3B2FFFA98FF29" box="[1286,1348,152,177]" italics="true" pageId="230" pageNumber="231">et al.</emphasis>
