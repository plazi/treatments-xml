<taxonomicName id="64B04D370489FF1BFC68B35FFB9CFEC9" baseAuthorityName="Germar" baseAuthorityYear="1821" box="[925,1088,312,337]" class="Insecta" family="Cicadellidae" genus="Homalodisca" kingdom="Animalia" order="Hemiptera" pageId="230" pageNumber="231" phylum="Arthropoda" rank="species" species="vitripennis">
