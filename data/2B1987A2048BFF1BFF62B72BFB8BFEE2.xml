<taxonomicName id="64B04D370489FF1BFDEBB35FFDA7FEC9" box="[542,635,312,337]" class="Insecta" family="Mymaridae" genus="Gonatocerus" kingdom="Animalia" order="Hymenoptera" pageId="230" pageNumber="231" phylum="Arthropoda" rank="species" species="virlai">
