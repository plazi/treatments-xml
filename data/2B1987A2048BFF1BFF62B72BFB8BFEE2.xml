<subSubSection id="EBAA653F048AFF1BFF33B5C9FF3FFEB2" lastPageId="230" lastPageNumber="231" pageId="229" pageNumber="230" type="biology_ecology">
<paragraph id="A30F36B4048AFF1BFF33B5C9FF3FFEB2" blockId="229.[151,1437,1326,2033]" lastBlockId="230.[151,1436,152,378]" lastPageId="230" lastPageNumber="231" pageId="229" pageNumber="230">
<emphasis id="91C4EAA6048AFF18FF33B5C9FEC8F850" bold="true" box="[198,276,1966,1992]" pageId="229" pageNumber="230">Hosts.</emphasis>
<taxonomicName id="64B04D37048AFF18FEE9B5C8FD10F851" authority="(Signoret)" authorityName="Signoret" baseAuthorityName="Signoret" baseAuthorityYear="1855" box="[284,716,1967,1993]" class="Insecta" family="Cicadellidae" genus="Tapajosa" kingdom="Animalia" order="Hemiptera" pageId="229" pageNumber="230" phylum="Arthropoda" rank="species" species="rubromarginata">
