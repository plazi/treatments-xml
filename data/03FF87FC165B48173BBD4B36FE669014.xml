<collectingCountry id="F341767A165A481739844D93FC9F919A" box="[693,779,166,192]" name="Canada" pageId="15" pageNumber="352">Canada</collectingCountry>
from 
<collectingRegion id="4992F808165A481738664D93FB8B919A" box="[855,1055,166,192]" country="Canada" name="British Columbia" pageId="15" pageNumber="352">British Columbia</collectingRegion>
