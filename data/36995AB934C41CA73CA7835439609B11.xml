<taxonomicName id="45FE389A94DC7AC985E81219FB9D7261" class="Gastropoda" family="Cyclophoridae" genus="Ditropopsis" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ditropopsis trachychilus" order="Architaenioglossa" pageId="11" pageNumber="12" phylum="Mollusca" rank="species" species="trachychilus">Ditropopsis trachychilus</taxonomicName>
