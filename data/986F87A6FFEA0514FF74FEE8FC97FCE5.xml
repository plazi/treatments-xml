<collectingCountry id="68D17620FFEA0514FD98FD68FD3FFD2C" box="[635,697,716,741]" name="Chad" pageId="4" pageNumber="255">Chad</collectingCountry>
, 
<collectingCountry id="68D17620FFEA0514FD27FD68FCDCFD2D" box="[708,858,716,741]" name="Burkina Faso" pageId="4" pageNumber="255">Burkina Faso</collectingCountry>
