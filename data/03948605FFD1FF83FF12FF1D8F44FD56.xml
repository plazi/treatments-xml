<quantity id="4CC59AF6FFD1FF84FEE1FCC68FA4FCB2" box="[356,451,844,870]" metricMagnitude="-2" metricUnit="m" metricValue="1.35" pageId="43" pageNumber="44" unit="mm" value="13.5">13.5mm</quantity>
