<specimenCount id="9D3BFC9AFFD1FF84FE51FCC68C1FFCB2" box="[468,632,844,870]" pageId="43" pageNumber="44" type="generic">
<number id="3DF52534FFD1FF84FE51FCC68F95FCB2" box="[468,498,844,870]" pageId="43" pageNumber="44" value="12.0">12</number>
