, 
<collectingCountry id="F32A7783FFD6FF83FE36FE7E8C27FDDA" box="[435,576,500,526]" name="Ivory Coast" pageId="44" pageNumber="45">Ivory Coast</collectingCountry>
, 
<collectingCountry id="F32A7783FFD6FF83FDCEFE7E8C8EFDDA" box="[587,745,500,526]" name="Mozambique" pageId="44" pageNumber="45">Mozambique</collectingCountry>
, 
<collectingCountry id="F32A7783FFD6FF83FD71FE7E8D3BFDDA" box="[756,860,500,526]" name="Namibia" pageId="44" pageNumber="45">Namibia</collectingCountry>
, 
<collectingCountry id="F32A7783FFD6FF83FCEDFE7E8A66FDDA" box="[872,1025,500,526]" name="South Africa" pageId="44" pageNumber="45">South Africa</collectingCountry>
, 
<collectingCountry id="F32A7783FFD6FF83FB88FE7E8A1EFDDA" box="[1037,1145,500,526]" name="Tanzania" pageId="44" pageNumber="45">Tanzania</collectingCountry>
