<emphasis id="B949EB01FFD1FF84FF12FCC18E9DFCB1" bold="true" box="[151,250,843,869]" pageId="43" pageNumber="44">Length:</emphasis>
<quantity id="4CC59AF6FFD1FF84FE84FCC68F33FCB2" box="[257,340,844,870]" metricMagnitude="-3" metricUnit="m" metricValue="8.6" pageId="43" pageNumber="44" unit="mm" value="8.6">8.6mm</quantity>
