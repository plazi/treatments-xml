<taxonomicName id="4C4F4D0CFF77FF01F75FFAC42A83716E" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[1034,1163,1337,1361]" class="Insecta" family="Derbidae" genus="Saccharodite" kingdom="Animalia" order="Hemiptera" pageId="148" pageNumber="149" phylum="Arthropoda" rank="species" species="lucifera">
<emphasis id="B93BEA9DFF77FF01F75FFAC42A83716E" box="[1034,1163,1337,1361]" italics="true" pageId="148" pageNumber="149">Sa. lucifera</emphasis>
