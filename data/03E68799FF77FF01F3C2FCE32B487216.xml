<collectingMunicipality id="6B94ACF5FF77FF01F24FFAA02FBC714A" box="[282,436,1373,1397]" pageId="148" pageNumber="149">North Borneo</collectingMunicipality>
(
<collectionCode id="ED5EAE4AFF77FF01F291FAA02C10714A" box="[452,536,1373,1397]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34697" lsid="urn:lsid:biocol.org:col:34697" name="Bishop Museum" pageId="148" pageNumber="149" type="Museum">BPBM</collectionCode>
