<materialsCitation id="3B273CD2FF77FF01F2D1FAE82C2C714A" collectionCode="EAST, BPBM" country="Malaysia" location="North Borneo" municipality="North Borneo" pageId="148" pageNumber="149" specimenCount="1" specimenCount-male="1" typeStatus="holotype">
<typeStatus id="54F4882DFF77FF01F2D1FAE82FE47112" box="[388,492,1301,1325]" pageId="148" pageNumber="149">Holotype</typeStatus>
<specimenCount id="9D49FD06FF77FF01F2A2FAEA2C017113" box="[503,521,1303,1324]" pageId="148" pageNumber="149" type="male">♂</specimenCount>
(forewing 
<quantity id="4CB79B6AFF77FF01F1DBFAE82CE47112" box="[654,748,1301,1325]" metricMagnitude="-3" metricUnit="m" metricValue="3.2" pageId="148" pageNumber="149" unit="mm" value="3.2">3.2 mm</quantity>
), 
<collectionCode id="ED5EAE4AFF77FF01F056FAE82D417112" box="[771,841,1301,1325]" pageId="148" pageNumber="149">EAST</collectionCode>
<collectingCountry id="F358761FFF77FF01F001FAE82DE27112" box="[852,1002,1301,1325]" name="Malaysia" pageId="148" pageNumber="149">MALAYSIA</collectingCountry>
: NORTH BORNEO; no data, among 
<location id="8E906054FF77FF01F3C2FAC42F39716E" LSID="urn:lsid:plazi:treatment:03E68799FF77FF01F3C2FCE32B487216:8E906054FF77FF01F3C2FAC42F39716E" box="[151,305,1337,1361]" country="Malaysia" municipality="North Borneo" name="North Borneo" pageId="148" pageNumber="149">North Borneo</location>
