<quantity id="8A5001D3FFD6FFF4FDE1FE56C4F9ADC7" box="[631,725,484,509]" metricMagnitude="-3" metricUnit="m" metricValue="3.6" pageId="13" pageNumber="414" unit="mm" value="3.6">3.6 mm</quantity>
