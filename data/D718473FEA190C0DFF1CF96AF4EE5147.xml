<quantity id="98495BCCEA190C0CFCD1F814F2E854CB" box="[858,922,1989,2012]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="6" pageNumber="7" unit="mm" value="4.0">4 mm</quantity>
