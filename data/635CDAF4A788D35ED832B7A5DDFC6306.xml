<geoCoordinate id="BC892184B2B9D0CD949EC50C3E491A82" direction="north" orientation="latitude" precision="925" value="30.116667">29°67'N</geoCoordinate>
, 
<geoCoordinate id="AEE6CF57C464F95F98D8F688ECD2FE15" direction="east" orientation="longitude" precision="925" value="96.166664">95°70'E</geoCoordinate>
