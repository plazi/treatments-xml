<taxonomicName id="EBED4D2F5975C94DFAA8FB74FA573A2C" box="[1290,1436,1259,1284]" class="Insecta" family="Endomychidae" genus="Lycoperdina" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="47" phylum="Arthropoda" rank="genus">
