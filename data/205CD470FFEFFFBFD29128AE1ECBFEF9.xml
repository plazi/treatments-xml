<quantity id="6F0DC883FFEFFF80D11D297F1F8FFAA7" box="[829,898,1316,1336]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="31" pageNumber="1708" unit="m" value="100.0">100 m</quantity>
