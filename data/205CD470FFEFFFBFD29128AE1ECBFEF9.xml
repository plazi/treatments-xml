<quantity id="6F0DC883FFEFFF80D16029671F71FACF" box="[832,892,1340,1360]" metricMagnitude="2" metricUnit="m" metricValue="1.65" pageId="31" pageNumber="1708" unit="m" value="165.0">165 m</quantity>
