<quantity id="6F0DC883FFEFFF80D3A129671DB6FACF" box="[385,443,1340,1360]" metricMagnitude="2" metricUnit="m" metricValue="7.0" pageId="31" pageNumber="1708" unit="m" value="700.0">700 m</quantity>
