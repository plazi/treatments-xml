<taxonomicName id="4C094EFCFFCDFFAEFB58E67DFAB87309" authority="Germann, 2017" authorityName="Germann" authorityYear="2017" box="[1259,1375,1116,1138]" class="Insecta" family="Curculionidae" genus="Amicromias" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="316" phylum="Arthropoda" rank="species" species="thracicus" status="sp. n.">
<emphasis id="B97DE96DFFCDFFAEFB58E67DFAB87309" box="[1259,1375,1116,1138]" italics="true" pageId="5" pageNumber="316">A. thracicus</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A24E5416FFCDFFAEFAD5E67DFA7B7309" box="[1382,1436,1116,1138]" pageId="5" pageNumber="316" rank="species">
<emphasis id="B97DE96DFFCDFFAEFAD5E67DFA7B7309" bold="true" box="[1382,1436,1116,1138]" pageId="5" pageNumber="316">sp. n.</emphasis>
</taxonomicNameLabel>
