<paragraph id="6961E949C60A1B3FFF6CF9B8FE6AF96B" blockId="12.[151,1436,1550,1862]" box="[199,495,1694,1719]" pageId="12" pageNumber="13">
<emphasis id="5BAA355BC60A1B3FFF6CF9B8FEAFF96A" bold="true" box="[199,298,1694,1719]" inLineHeading="true" pageId="12" pageNumber="13" reason="1">Habitat.</emphasis>
