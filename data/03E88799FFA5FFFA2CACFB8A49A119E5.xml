<taxonomicName id="4C414D0CFFA5FFF92E32F89D4FDD191C" authorityName="Anderson" authorityYear="2010" box="[875,992,1861,1886]" class="Insecta" family="Curculionidae" genus="Theognete" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="cristata">
<emphasis id="B935EA9DFFA5FFF92E32F89D4FDD191C" box="[875,992,1861,1886]" italics="true" pageId="25" pageNumber="26">T. cristata</emphasis>
</taxonomicName>
, 
<taxonomicName id="4C414D0CFFA5FFF92EB6F89D48B4191C" authorityName="Anderson" authorityYear="2010" box="[1007,1161,1861,1886]" class="Insecta" family="Curculionidae" genus="Theognete" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="tomneyeae">
<emphasis id="B935EA9DFFA5FFF92EB6F89D48B4191C" box="[1007,1161,1861,1886]" italics="true" pageId="25" pageNumber="26">T. tomneyeae</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C414D0CFFA5FFF9299AF89D494D191C" authorityName="Anderson" authorityYear="2010" box="[1219,1392,1861,1886]" class="Insecta" family="Curculionidae" genus="Theognete" kingdom="Animalia" order="Coleoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="species" species="baranowskii">
<emphasis id="B935EA9DFFA5FFF9299AF89D494D191C" box="[1219,1392,1861,1886]" italics="true" pageId="25" pageNumber="26">T. baranowskii</emphasis>
