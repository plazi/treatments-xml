<paragraph id="8BE8DA52860F1211FED188275DF97350" blockId="2.[284,1676,1341,1782]" box="[288,971,1341,1382]" pageId="2" pageNumber="203">
<taxonomicName id="4C57A1D1860F1211FED188275C0C7350" authority="Malloch, 1942" authorityName="Malloch" authorityYear="1942" box="[288,574,1341,1382]" class="Insecta" family="Trypetidae" genus="Cycasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="203" phylum="Arthropoda" rank="species" species="oculata" status="new species">Cycasia oculata</taxonomicName>
, 
<taxonomicNameLabel id="A210BB3B860F1211FDA688275D187350" box="[599,810,1341,1382]" pageId="2" pageNumber="203" rank="species">new species</taxonomicNameLabel>
(
<figureCitation id="136CC6D7860F1211FCBC88275D807350" box="[845,946,1341,1382]" captionStart="FIGURE 1" captionStartId="2.[689,796,1253,1287]" captionTargetBox="[436,1519,756,1215]" captionText="FIGURE 1.-vVing of C31casia oculata." figureDoi="http://doi.org/10.5281/zenodo.5163628" httpUri="https://zenodo.org/record/5163628/files/figure.png" pageId="2" pageNumber="203">fig. 1</figureCitation>
