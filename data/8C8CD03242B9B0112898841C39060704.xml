<taxonomicName id="00224FA9551298A28870A135D9A2804A" authority="Sharp, 1883" authorityName="Sharp" authorityYear="1883" class="Insecta" family="Staphylinidae" genus="Tinotus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Tinotus cavicollis" order="Coleoptera" pageId="1" pageNumber="82" phylum="Arthropoda" rank="species" species="cavicollis">Tinotus cavicollis</taxonomicName>
