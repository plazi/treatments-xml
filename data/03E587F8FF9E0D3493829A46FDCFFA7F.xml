<location id="8E936035FF9E0D3497389A6FFB99FCB7" LSID="urn:lsid:plazi:treatment:03E587F8FF9E0D3493829A46FDCFFA7F:8E936035FF9E0D3497389A6FFB99FCB7" box="[1032,1085,812,832]" country="Pakistan" name="Gilgit" pageId="17" pageNumber="1258">Gilgit</location>
