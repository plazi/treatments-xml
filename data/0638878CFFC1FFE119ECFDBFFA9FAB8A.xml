<taxonomicName id="49914D19FFCFFFE11DF8FAA0FABBA98A" authorityName="C. Willemse" authorityYear="1961" box="[1155,1280,1394,1417]" class="Insecta" family="Tettigoniidae" genus="Paramacroxiphus" kingdom="Animalia" order="Orthoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="aberrans">
