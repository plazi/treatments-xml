<subSubSection id="7C9121BF17D1A9FE98CFFE567A84FD83" pageId="224" pageNumber="225" type="materials_examined">
<paragraph id="3434723417D1A9FE98CFFE567929FDFB" blockId="224.[151,1436,152,719]" pageId="224" pageNumber="225">
<materialsCitation id="84E3786917D1A9FE98CFFE567929FDFB" collectingDate="1990-10-18" collectionCode="NMNH" collectorName="W. E. Steiner &amp; C. Kremen &amp; P. Daniels" country="Madagascar" elevation="900" location="Ranomafana" pageId="224" pageNumber="225" specimenCount="2" specimenCount-male="2" stateProvince="Fianarantsoa" typeStatus="holotype">
<emphasis id="06FFAE2617D1A9FE98CFFE567A37FE4B" bold="true" box="[197,628,413,439]" pageId="224" pageNumber="225">
