<taxonomicName id="F38B09B717EEA9C19988FA097B1AFA24" authority="Olah &amp; Johanson" authorityName="Olah &amp; Johanson" authorityYear="2008" box="[386,857,1473,1497]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="223" pageNumber="224" phylum="Arthropoda" rank="species" species="rantsoa" status="new species">
