<emphasis id="0BD7EF71FF863C528A91FF23FED7FF04" bold="true" box="[199,335,152,179]" pageId="41" pageNumber="42">Comments.</emphasis>
<taxonomicName id="FEA348E0FF863C528B0EFF22FD8BFF04" authorityName="Leschen &amp; Reid &amp; Nadein" authorityYear="2020" box="[344,531,153,179]" class="Insecta" family="Chrysomelidae" genus="Maurodus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="nunni">
