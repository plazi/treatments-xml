<number id="955324E9FF990428FAC46413857DD065" box="[1281,1342,312,338]" pageId="4" pageNumber="51" value="1967.0">1967</number>
</date>
(
<collectionCode id="458AAE0BFF990428FA94641385CCD065" LSID="urn:lsid:biocol.org:col:34700" box="[1361,1423,312,338]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="4" pageNumber="51">CAS</collectionCode>
