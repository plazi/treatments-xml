<number id="955324E9FF990428FE80641381C2D065" box="[325,385,312,338]" pageId="4" pageNumber="51" value="1967.0">1967</number>
</date>
(
<collectionCode id="458AAE0BFF990428FE5164138191D065" LSID="urn:lsid:biocol.org:col:34700" box="[404,466,312,338]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15690" name="California Academy of Sciences" pageId="4" pageNumber="51">CAS</collectionCode>
