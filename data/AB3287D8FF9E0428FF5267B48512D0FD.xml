<taxonomicName id="E49B4D4DFF990428FF5265EB81FAD1EE" box="[151,441,192,217]" class="Insecta" family="Dolichopodidae" genus="Mesorhaga" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="51" phylum="Arthropoda" rank="species" species="pallidicornis">
