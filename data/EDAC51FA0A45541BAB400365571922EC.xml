; “+” sign, 
<taxonomicName id="26E4E31D09B8AFFBA1C08FB8E44A0C9F" class="Actinopterygii" family="Cichlidae" genus="Cryptoheros" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="32" phylum="Chordata" rank="species" species="nanoluteus">Cr. nanoluteus</taxonomicName>
; stars, 
<taxonomicName id="90EE8C7F7DBEF8B3F3605416A0FFD3AC" class="Actinopterygii" family="Cichlidae" genus="Cryptoheros" higherTaxonomySource="CoL" kingdom="Animalia" order="Perciformes" pageNumber="32" phylum="Chordata" rank="species" species="sajica">Cr. sajica</taxonomicName>
