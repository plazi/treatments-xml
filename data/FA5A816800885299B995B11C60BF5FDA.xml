<geoCoordinate id="210B41369DF40C521A81453869F8A678" degrees="10" direction="north" minutes="12" orientation="latitude" precision="15" seconds="09" value="10.202499">10째12'09&quot;N</geoCoordinate>
, 
<geoCoordinate id="43895143587023CBE82AC4F6B8D17FED" degrees="103" direction="east" minutes="58" orientation="longitude" precision="15" seconds="06" value="103.96833">103째58'06&quot;E</geoCoordinate>
