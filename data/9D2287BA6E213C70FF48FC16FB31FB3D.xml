<bibRefCitation id="711A4B5D6E2C3C70FC6FFC42FBF6FC3A" author="Brown WJ" box="[974,1091,992,1015]" pageId="9" pagination="128 - 133" refId="ref4768" refString="Brown WJ. 1927. Four new species of Onthophagus (Coleoptera). Can Entomol. 59: 128 - 133. doi: 10.4039 / Ent 59128 - 6." type="journal article" year="1927">Brown 1927</bibRefCitation>
</taxonomicName>
(black triangle); 
<taxonomicName id="D28B4D2F6E2C3C70FF5CFC5FFE48FBD9" authority="Boucomont 1932" authorityName="Boucomont" authorityYear="1932" box="[253,509,1021,1044]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="9" phylum="Arthropoda" rank="species" species="fuscus">
<emphasis id="27FFEABE6E2C3C70FF5CFC5FFEE4FBD9" box="[253,337,1021,1044]" italics="true" pageId="9">O. fuscus</emphasis>
<bibRefCitation id="711A4B5D6E2C3C70FEF7FC5FFE48FBD9" author="Boucomont A" box="[342,509,1021,1044]" pageId="9" pagination="293 - 332" refId="ref4737" refString="Boucomont A. 1932. Synopsis des Onthophagus d ' Amerique du Sud (Col. Scarab.). Ann Soc Ent Fr. 101: 293 - 332." type="journal article" year="1932">Boucomont 1932</bibRefCitation>
</taxonomicName>
(white dot); and 
<taxonomicName id="D28B4D2F6E2C3C70FD02FC5FFB8AFBD9" authority="Zunino and Halffter 1988 a" authorityName="Zunino and Halffter" authorityYear="1988" box="[675,1087,1021,1044]" class="Insecta" family="Scarabaeidae" genus="Onthophagus" kingdom="Animalia" order="Coleoptera" pageId="9" phylum="Arthropoda" rank="species" species="pseudofuscus">
