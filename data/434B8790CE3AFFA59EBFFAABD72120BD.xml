<taxonomicName id="0CE24D05CE3AFFA59E89FACBD05C20BD" authorityName="Jacoby" authorityYear="1887" box="[711,787,1387,1408]" class="Insecta" family="Chrysomelidae" genus="Triarius" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="356" phylum="Arthropoda" rank="genus">
