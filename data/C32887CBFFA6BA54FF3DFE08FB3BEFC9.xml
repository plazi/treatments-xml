<taxonomicName id="8C814D5EFFA7BA54FB2DFAF1FAEBEF39" box="[1159,1286,1286,1307]" class="Mammalia" family="Muridae" genus="Psammomys" kingdom="Animalia" order="Rodentia" pageId="55" pageNumber="56" phylum="Chordata" rank="genus">Psammomys</taxonomicName>
<taxonomicName id="8C814D5EFFA7BA54FAA6FAF2FABCEF39" box="[1292,1361,1285,1307]" pageId="55" pageNumber="56" rank="species" species="obesus">obesus</taxonomicName>
