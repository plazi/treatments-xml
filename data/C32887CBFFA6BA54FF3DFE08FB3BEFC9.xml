<taxonomicName id="8C814D5EFFA7BA54FB2DFAF1FABCEF39" authorityName="Cretzschmar" authorityYear="1828" box="[1159,1361,1285,1307]" class="Mammalia" family="Muridae" genus="Psammomys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rodentia" pageId="55" pageNumber="56" phylum="Chordata" rank="species" species="obesus">
