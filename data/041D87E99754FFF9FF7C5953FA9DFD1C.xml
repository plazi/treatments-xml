<taxonomicName id="4BB44D7C9753FFF8FEC359ABFD6EF94F" authority="Coutiere, 1909" authorityName="Coutiere" authorityYear="1909" box="[296,685,1743,1765]" class="Malacostraca" family="Alpheidae" genus="Synalpheus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="townsendi">
