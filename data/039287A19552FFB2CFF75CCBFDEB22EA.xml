<emphasis id="B94FEAA59555FFB2CFF75AD3FDFB2097" bold="true" box="[151,514,222,249]" pageId="60" pageNumber="61">
<collectingCountry id="F32C76279555FFB2CFF75AD3FED62097" box="[151,303,222,248]" name="South Africa" pageId="60" pageNumber="61">South Africa</collectingCountry>
voucher material
</emphasis>
: 
<collectionCode id="ED2AAE729555FFB2CD715AD2FD9C2096" box="[529,613,223,249]" country="South Africa" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34984" lsid="urn:lsid:biocol.org:col:34984" name="South African Institute for Aquatic Biodiversity" pageId="60" pageNumber="61" type="Other type of research institution/biorepository">SAIAB</collectionCode>
25352, 
<collectionCode id="ED2AAE729555FFB2CDA05AD2FCED2096" box="[704,788,223,249]" country="South Africa" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34984" lsid="urn:lsid:biocol.org:col:34984" name="South African Institute for Aquatic Biodiversity" pageId="60" pageNumber="61" type="Other type of research institution/biorepository">SAIAB</collectionCode>
