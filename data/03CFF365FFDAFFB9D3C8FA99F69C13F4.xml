<number id="3DAE5054FFDAFFB9D79DF96DF7CA12A9" box="[1218,1226,1688,1709]" pageId="13" pageNumber="452" value="1.0">1</number>
/ 
<number id="3DAE5054FFDAFFB9D78DF96DF60A12A9" box="[1234,1290,1688,1709]" pageId="13" pageNumber="452" value="2.75" valueMax="3.0" valueMin="2.5">2.5–3</number>
(
