<quantity id="34479B29FFA0D869FDB77532FD53FAD8" box="[621,682,1310,1332]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="2" pageNumber="1397" unit="mm" value="4.0">4 mm</quantity>
