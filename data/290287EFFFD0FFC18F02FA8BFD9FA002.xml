<taxonomicName id="66AB4D7AFFD1FFC18D60FE24FF51A281" authority="(Wagler, 1824)" baseAuthorityName="Wagler" baseAuthorityYear="1824" class="Reptilia" family="Colubridae" genus="Oxybelis" kingdom="Animalia" order="Squamata" pageId="53" pageNumber="54" phylum="Chordata" rank="species" species="aeneus">
