<taxonomicName id="3AFE4D059C3CDD0DEB1DFA87FDDB745E" authority="Li, 2017" authorityName=", Latin" authorityYear="2017" box="[313,593,1331,1355]" class="Hexactinellida" family="Pheronematidae" genus="Pheronemoides" kingdom="Animalia" order="Amphidiscosida" pageId="2" phylum="Porifera" rank="species" species="fungosus" status="gen. et sp. nov.">
