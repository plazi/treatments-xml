<taxonomicName id="4C1D4D570053FFD6FDA3F8E0FD54150B" authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[623,762,1796,1823]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="dorrellae">
