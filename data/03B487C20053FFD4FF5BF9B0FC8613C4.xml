<quantity id="4CE59B310053FFD6FD63F8CDFD581556" box="[687,758,1832,1859]" metricMagnitude="-3" metricUnit="m" metricValue="4.0" pageId="65" pageNumber="66" unit="mm" value="4.0">4 mm</quantity>
