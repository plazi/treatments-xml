<quantity id="4CE59B310053FFD6FE77F8CDFDBD1556" box="[443,531,1832,1859]" metricMagnitude="-3" metricUnit="m" metricValue="4.5" pageId="65" pageNumber="66" unit="mm" value="4.5">4.5 mm</quantity>
