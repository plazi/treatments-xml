<taxonomicName id="4C1D4D570053FFD6FE32F904FD2614EF" authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[510,648,1760,1787]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="dorrellae">
<emphasis id="B969EAC60053FFD6FE32F904FDBC14EE" box="[510,530,1761,1787]" italics="true" pageId="65" pageNumber="66">O</emphasis>
