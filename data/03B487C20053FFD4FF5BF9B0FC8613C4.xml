<taxonomicName id="4C1D4D570053FFD6FCCEF958FCC514C3" authorityName="Archibald &amp; Cannings &amp; Erickson &amp; Bybee &amp; Mathewes" authorityYear="2021" box="[770,875,1724,1751]" class="Insecta" family="Dysagrionidae" genus="Okanagrion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="65" pageNumber="66" phylum="Arthropoda" rank="species" species="beardi">
<emphasis id="B969EAC60053FFD6FCCEF958FCB814C2" box="[770,790,1725,1751]" italics="true" pageId="65" pageNumber="66">O</emphasis>
