<taxonomicName id="88F2169C0292FBEAE56D468F22D17F0B" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:181901" class="Insecta" family="Formicidae" genus="Pheidole" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Pheidole micon Wilson" lsidName-HNS="Pheidole micon Wilson" order="Hymenoptera" pageNumber="454" phylum="Arthropoda" rank="species" species="micon">Pheidole micon</taxonomicName>
