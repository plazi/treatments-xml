<paragraph id="5355AC5A616BE21B7A1A17EBFB0DFA9D" blockId="1.[821,1199,1239,1296]" box="[821,1170,1272,1296]" pageId="1" pageNumber="2">
<treatmentCitationGroup id="73FA8B74616BE21B7A1A17EBFB0DFA9D" box="[821,1170,1272,1296]" pageId="1" pageNumber="2">
<taxonomicName id="94EAD7D9616BE21B7A1A17EBFC79FA9D" authorityName="Meixner" authorityYear="1907" box="[821,998,1272,1296]" class="Rhabditophora" family="Stylochoplanidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="1" pageNumber="2" phylum="Platyhelminthes" rank="family">Stylochoplanidae</taxonomicName>
