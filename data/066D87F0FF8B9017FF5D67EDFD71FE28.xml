<taxonomicName id="49C44D65FF8A9017FBB46772FA9EFEC4" box="[1150,1327,296,320]" class="Insecta" family="Phoridae" genus="Coniceromyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="leucomacula">
