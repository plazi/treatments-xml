<taxonomicName id="4C114D3EFFD9FFF1FE8DFF2EFDD4FF18" authority="Agassiz, 1833" authorityName="Agassiz" authorityYear="1833" box="[293,542,204,230]" class="Actinopterygii" family="Pycnodontidae" genus="Pycnodus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pycnodontiformes" pageId="13" pageNumber="152" phylum="Chordata" rank="genus">
