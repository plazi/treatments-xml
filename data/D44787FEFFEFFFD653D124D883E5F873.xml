<quantity id="9B169B0DFFEFFFD957EF253F8432F8EE" box="[1193,1299,1830,1855]" metricMagnitude="-3" metricUnit="m" metricValue="3.76" pageId="7" pageNumber="158" unit="mm" value="3.76">3.76 mm</quantity>
