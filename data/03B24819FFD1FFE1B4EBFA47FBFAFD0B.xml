<taxonomicName id="4C1B828CFFD3FFEFB56AF865FE5AF825" authorityName="Takeuchi" authorityYear="2010" box="[278,503,1987,2008]" class="Magnoliopsida" family="Rubiaceae" genus="Psychotria" kingdom="Plantae" order="Gentianales" pageId="6" pageNumber="31" phylum="Tracheophyta" rank="species" species="ternatifolia">
