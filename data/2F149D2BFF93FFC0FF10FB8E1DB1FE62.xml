<emphasis id="95C9F02FFF93FFC6FF40F8E81E61F8E7" box="[199,314,1881,1905]" italics="true" pageId="26" pageNumber="27">Antennula</emphasis>
</taxonomicName>
articles 
<number id="11753E1AFF93FFC6FE1CF8E81EF2F8E7" box="[411,425,1881,1905]" pageId="26" pageNumber="27" value="1.0">1</number>
and 
<number id="11753E1AFF93FFC6FE65F8E81EABF8E7" box="[482,496,1881,1905]" pageId="26" pageNumber="27" value="2.0">2</number>
