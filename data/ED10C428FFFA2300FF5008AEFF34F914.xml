<taxonomicName id="A2B90EBDFFFA2300FCC30BFDFC59F977" box="[772,909,1677,1704]" class="Hydrozoa" family="Lafoeidae" genus="Filellum" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="329" phylum="Cnidaria" rank="species" species="undetermined">
<emphasis id="57CDA92CFFFA2300FCC30BFDFCB7F978" box="[772,867,1677,1703]" italics="true" pageId="8" pageNumber="329">Filellum</emphasis>
sp.
</taxonomicName>
, 
<taxonomicName id="A2B90EBDFFFA2300FC5D0BFEFBFAF978" baseAuthorityName="Pena Cantero" baseAuthorityYear="2009" box="[922,1070,1677,1703]" class="Hydrozoa" family="Campanulinidae" genus="Lafoeina" kingdom="Animalia" order="Leptothecata" pageId="8" pageNumber="329" phylum="Cnidaria" rank="species" species="longitheca">
