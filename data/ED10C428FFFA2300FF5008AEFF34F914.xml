<quantity id="A241D8DBFFFA2300FBD50B19FB8EF95B" box="[1042,1114,1641,1668]" metricMagnitude="2" metricUnit="m" metricValue="1.27" pageId="8" pageNumber="329" unit="m" value="127.0">127 m</quantity>
