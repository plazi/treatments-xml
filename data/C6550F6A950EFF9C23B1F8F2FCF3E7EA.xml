<quantity id="89041399950EFF9C207AF8BBFC4DE70E" box="[860,962,1881,1907]" metricMagnitude="-2" metricUnit="m" metricValue="1.17" pageId="18" pageNumber="369" unit="mm" value="11.7">11.7 mm</quantity>
