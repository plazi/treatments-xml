<specimenCount id="9D7FFD67FF98FF8DAA9EFD02FEBCFD24" box="[212,294,728,754]" pageId="11" pageNumber="12" type="generic">
<quantity id="4C819B0BFF98FF8DAA9EFD02FEB8FD24" box="[212,290,728,754]" metricMagnitude="2" metricUnit="m" metricValue="2.1" pageId="11" pageNumber="12" unit="m" value="210.0">210 m</quantity>
