<quantity id="4C819B0BFF98FF8DA932FD68FC72FD1A" box="[888,1000,690,716]" metricMagnitude="-2" metricUnit="m" metricValue="9.48" pageId="11" pageNumber="12" unit="mm" value="94.8">94.8 mm</quantity>
