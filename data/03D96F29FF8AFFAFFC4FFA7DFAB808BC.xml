<taxonomicName id="4C70A5BCFF8AFFAFFC17FA7DFAE40BC4" authority="GUINOT, 1989" authorityName="Guinot" authorityYear="1989" box="[1021,1335,1473,1497]" class="Malacostraca" family="Bythograeidae" genus="Segonzacia" kingdom="Animalia" order="Decapoda" pageId="47" pageNumber="492" phylum="Arthropoda" rank="genus">
