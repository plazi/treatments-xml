<quantity id="4C9988D1FFDEDD2730F1FD19FB4BFC84" box="[1185,1273,762,788]" metricMagnitude="-2" metricUnit="m" metricValue="2.9" pageId="15" pageNumber="16" unit="mm" value="29.0">29 mm</quantity>
