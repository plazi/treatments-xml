<geoCoordinate id="EBFD506D3245FFF2FE86F83039FADF8B" box="[315,456,1932,1956]" degrees="018.8911" direction="east" orientation="longitude" pageId="7" pageNumber="540" precision="5" value="18.8911">018.8911° E</geoCoordinate>
, 
<date id="FA77106A3245FFF2FE65F8303A54DF8C" box="[472,614,1932,1956]" pageId="7" pageNumber="540" value="2014-05-03">3 May 2014</date>
