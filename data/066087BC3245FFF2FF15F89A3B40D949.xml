<taxonomicName id="49C94D293245FFF2FEAFF84D39FBD021" box="[274,457,2033,2057]" class="Magnoliopsida" family="Betulaceae" genus="Corylus" kingdom="Plantae" order="Fagales" pageId="7" pageNumber="540" phylum="Tracheophyta" rank="species" species="avellana">
