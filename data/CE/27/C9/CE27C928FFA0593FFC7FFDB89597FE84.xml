<taxonomicName authority="Karsch, 1878: 821" authorityName="Karsch" authorityPageNumber="821" authorityYear="1878" box="[987,1319,610,631]" class="Arachnida" family="Idiopidae" genus="Misgolas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="40" pageNumber="85" phylum="Arthropoda" rank="species" species="rapax">
