<geoCoordinate direction="north" orientation="latitude" precision="1" value="0.7143667">00°42.862'N</geoCoordinate>
<geoCoordinate direction="east" orientation="longitude" precision="1" value="124.4726">124°28.356'E</geoCoordinate>
