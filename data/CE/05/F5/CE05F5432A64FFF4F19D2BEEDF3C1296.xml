<taxonomicName box="[1189,1258,1426,1448]" pageId="16" pageNumber="87" rank="species" species="calopa">
<emphasis box="[1189,1258,1426,1448]" italics="true" pageId="16" pageNumber="87">calopa</emphasis>
</taxonomicName>
</taxonomicName>
, 50 
<taxonomicName box="[1303,1429,1426,1448]" class="Insecta" family="Veliidae" genus="Rhagovelia" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="87" phylum="Arthropoda" rank="species" species="vonprahli">
<emphasis box="[1303,1318,1427,1448]" italics="true" pageId="16" pageNumber="87">R</emphasis>
. 
<taxonomicName box="[1331,1429,1426,1448]" pageId="16" pageNumber="87" rank="species" species="vonprahli">
<emphasis box="[1331,1429,1426,1448]" italics="true" pageId="16" pageNumber="87">vonprahli</emphasis>
</taxonomicName>
</taxonomicName>
, 51 
<taxonomicName box="[182,300,1457,1479]" class="Insecta" family="Veliidae" genus="Rhagovelia" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="87" phylum="Arthropoda" rank="species" species="espriella">
<emphasis box="[182,197,1458,1479]" italics="true" pageId="16" pageNumber="87">R</emphasis>
. 
<taxonomicName box="[211,300,1457,1479]" pageId="16" pageNumber="87" rank="species" species="espriella">
<emphasis box="[211,300,1457,1479]" italics="true" pageId="16" pageNumber="87">espriella</emphasis>
</taxonomicName>
