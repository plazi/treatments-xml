/tarsal 
<number box="[469,482,1364,1389]" pageId="3" pageNumber="74" value="2.0">2</number>
/tarsal 
<number box="[561,578,1364,1388]" pageId="3" pageNumber="74" value="3.0">3</number>
: 
<number box="[593,640,1364,1388]" pageId="3" pageNumber="74" value="1.88">1.88</number>
/ 
<number box="[649,696,1364,1388]" pageId="3" pageNumber="74" value="1.48">1.48</number>
/ 
<number box="[704,754,1364,1388]" pageId="3" pageNumber="74" value="0.08">0.08</number>
/ 
<number box="[763,814,1364,1388]" pageId="3" pageNumber="74" value="0.82">0.82</number>
/ 
<number box="[822,873,1364,1388]" pageId="3" pageNumber="74" value="0.82">0.82</number>
; hind femur length-width/ tibia/ tarsal 
<number box="[1322,1331,1364,1389]" pageId="3" pageNumber="74" value="1.0">1</number>
/tarsal 
<number box="[1414,1427,1364,1388]" pageId="3" pageNumber="74" value="2.0">2</number>
/ tarsal 
<number box="[219,236,1400,1424]" pageId="3" pageNumber="74" value="3.0">3</number>
: 
<number box="[247,294,1399,1424]" pageId="3" pageNumber="74" value="1.28">1.28</number>
− 
<number box="[309,359,1399,1424]" pageId="3" pageNumber="74" value="0.36">0.36</number>
/ 
<number box="[368,416,1399,1424]" pageId="3" pageNumber="74" value="1.6">1.60</number>
/ 
