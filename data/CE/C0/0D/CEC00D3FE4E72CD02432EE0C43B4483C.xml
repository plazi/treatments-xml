<paragraph pageId="41" pageNumber="42">
<taxonomicName class="Gastropoda" family="Helicidae" genus="Helix" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Helix (Cochlitoma) regina" order="Stylommatophora" pageId="41" pageNumber="42" phylum="Mollusca" rank="species" species="regina" subGenus="Cochlitoma">Helix (Cochlitoma) regina</taxonomicName>
