<taxonomicName class="Agaricomycetes" family="Polyporaceae" genus="Polyporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Polyporus squamosus" order="Polyporales" pageId="0" pageNumber="1106" phylum="Basidiomycota" rank="species" species="squamosus">Polyporus squamosus</taxonomicName>
, 
<taxonomicName class="Agaricomycetes" family="Fomitopsidaceae" genus="Laetiporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Laetiporus sulphureus" order="Polyporales" pageId="0" pageNumber="1106" phylum="Basidiomycota" rank="species" species="sulphureus">Laetiporus sulphureus</taxonomicName>
, and 
<taxonomicName class="Agaricomycetes" family="Fomitopsidaceae" genus="Piptoporus" higherTaxonomySource="CoL" kingdom="Fungi" lsidName="Piptoporus betulinus" order="Polyporales" pageId="0" pageNumber="1106" phylum="Basidiomycota" rank="species" species="betulinus">Piptoporus betulinus</taxonomicName>
(
