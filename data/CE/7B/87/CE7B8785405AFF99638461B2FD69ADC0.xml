<emphasis bold="true" box="[151,287,577,603]" pageId="1" pageNumber="580">Comments.</emphasis>
<taxonomicName authority="Hong &amp; Wang, 1990" authorityName="Hong &amp; Wang" authorityYear="1990" box="[295,824,577,603]" class="Insecta" family="Pyrrhocorides" genus="Mesopyrrhocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="580" phylum="Arthropoda" rank="species" species="fasciata">
