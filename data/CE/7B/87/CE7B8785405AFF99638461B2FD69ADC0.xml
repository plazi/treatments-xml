<taxonomicName authorityName="Hong &amp; Wang" authorityYear="1990" box="[776,968,721,747]" class="Insecta" family="Pyrrhocorides" genus="Mesopyrrhocoris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="580" phylum="Arthropoda" rank="genus">
