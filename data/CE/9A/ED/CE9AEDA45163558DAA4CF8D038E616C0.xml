<taxonomicName authority="Huth, 1895" authorityName="Huth" authorityYear="1895" class="Magnoliopsida" family="Ranunculaceae" genus="Consolida" higherTaxonomySource="GBIF" kingdom="Plantae" lsidName="Consolida camptocarpa" order="Ranunculales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="species" species="camptocarpa">Consolida camptocarpa</taxonomicName>
