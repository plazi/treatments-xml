<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Amelanchier" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Amelanchier" order="Rosales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Amelanchier</taxonomicName>
