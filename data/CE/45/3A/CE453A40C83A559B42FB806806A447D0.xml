<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Prunus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Prunus" order="Rosales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Prunus</taxonomicName>
spp., 
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Crataegus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Crataegus" order="Rosales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Crataegus</taxonomicName>
