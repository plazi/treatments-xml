<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Prunus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Prunus" order="Rosales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Prunus</taxonomicName>
spp. (
<taxonomicName family="Rosaceae" lsidName="" pageId="0" pageNumber="7938" rank="family">Rosaceae</taxonomicName>
), 
<taxonomicName class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Vaccinium" order="Ericales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Vaccinium</taxonomicName>
spp. (
<taxonomicName family="Ericaceae" lsidName="" pageId="0" pageNumber="7938" rank="family">Ericaceae</taxonomicName>
) and 
<taxonomicName class="Magnoliopsida" family="Rosaceae" genus="Malus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Malus" order="Rosales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Malus</taxonomicName>
