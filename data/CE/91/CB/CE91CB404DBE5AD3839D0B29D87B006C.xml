<taxonomicName class="Magnoliopsida" family="Convolvulaceae" genus="Convolvulus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Convolvulus valentinus subsp. var. var. melliflorus" order="Solanales" pageId="115" pageNumber="116" phylum="Tracheophyta" rank="variety" species="valentinus" subSpecies="var." variety="melliflorus">Convolvulus valentinus var. melliflorus</taxonomicName>
