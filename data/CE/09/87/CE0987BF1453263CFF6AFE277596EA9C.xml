<taxonomicName authorityName="Boheman" authorityYear="1851" box="[943,1022,699,720]" class="Insecta" family="Cleridae" genus="Prosymnus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" phylum="Arthropoda" rank="species" species="livens">
