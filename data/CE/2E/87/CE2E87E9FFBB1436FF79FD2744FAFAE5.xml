</materialsCitation>
; 
<materialsCitation collectingDate="2016-03-28" collectionCode="MFLU, MFLUCC" collectorName="Ishani D. Goonasekara" country="Thailand" location="Mushroom Research Center" municipality="Mae Teang District" pageId="8" pageNumber="121" specimenCode="MFLU 22-0007, MFLUCC 16-0896" specimenCount="1" stateProvince="Chiang Mai Province">
