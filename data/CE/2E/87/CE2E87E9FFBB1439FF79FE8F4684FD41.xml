<taxonomicName authority="(Lib.) Lev." authorityName="Lev." baseAuthorityName="Lib." box="[782,1053,446,473]" class="Sordariomycetes" genus="Dinemasporium" kingdom="Fungi" pageId="7" pageNumber="120" phylum="Ascomycota" rank="species" species="graminum">
<emphasis box="[782,929,447,472]" italics="true" pageId="7" pageNumber="120">D. graminum</emphasis>
(Lib.) Lév.
</taxonomicName>
as the 
<typeStatus box="[1125,1173,447,473]" pageId="7" pageNumber="120">type</typeStatus>
species. 
<taxonomicName box="[1271,1451,447,472]" class="Sordariomycetes" genus="Dinemasporium" kingdom="Fungi" pageId="7" pageNumber="120" phylum="Ascomycota" rank="genus">
<emphasis box="[1271,1451,447,472]" italics="true" pageId="7" pageNumber="120">Dinemasporium</emphasis>
</taxonomicName>
