<paragraph pageId="0" pageNumber="77">
<taxonomicName authorityName="Druce" authorityYear="1889" class="Insecta" family="Thyrididae" genus="Dysodia" kingdom="Animalia" lsidName="Dysodia sica" order="Lepidoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="species" species="sica">
