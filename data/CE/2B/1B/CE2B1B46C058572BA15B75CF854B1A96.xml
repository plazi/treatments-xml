</taxonomicName>
(
<taxonomicName authorityName="Herrich-Schaeffer" authorityYear="1846" class="Insecta" family="Thyrididae" kingdom="Animalia" lsidName="" order="Lepidoptera" pageId="0" pageNumber="77" phylum="Arthropoda" rank="family">Thyrididae</taxonomicName>
) feeding on 
<taxonomicName class="Magnoliopsida" family="Piperaceae" genus="Piper" kingdom="Plantae" lsidName="Piper marginatum" order="Piperales" pageId="0" pageNumber="77" phylum="Tracheophyta" rank="species" species="marginatum">
