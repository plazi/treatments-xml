<taxonomicName id="A8F54D03FFEC37012998FB629159FB6B" authority="Dunn, 1982" authorityName="Dunn" authorityYear="1982" box="[151,576,1162,1188]" class="Anthozoa" family="Hormathiidae" genus="Paraphelliactis" kingdom="Animalia" order="Actiniaria" pageId="36" pageNumber="37" phylum="Cnidaria" rank="species" species="pabista">
