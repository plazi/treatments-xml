<taxonomicName id="BBE03DB6D569B22184C344ABEA19FB64" box="[474,621,1230,1251]" class="Insecta" family="Cetoniidae" genus="Euphoria" kingdom="Animalia" order="Coleoptera" pageId="75" pageNumber="76" phylum="Arthropoda" rank="species" species="exima">
