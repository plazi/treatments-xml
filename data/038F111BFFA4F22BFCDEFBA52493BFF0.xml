<taxonomicName id="4C26DB8EFFA4F22BFAB7FA922298BF30" class="Magnoliopsida" family="Polygonaceae" genus="Truellum" higherTaxonomySource="GBIF" kingdom="Plantae" order="Caryophyllales" pageId="35" pageNumber="190" phylum="Tracheophyta" rank="species" species="japonicum">
