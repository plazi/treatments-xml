<taxonomicName id="4C26DB8EFFA4F22BFC3EFA9225ACBF10" authority="L. Original" authorityName="L. Original" box="[961,1212,1339,1363]" class="Magnoliopsida" family="Polygonaceae" genus="Polygonum" kingdom="Plantae" order="Caryophyllales" pageId="35" pageNumber="190" phylum="Tracheophyta" rank="genus">
