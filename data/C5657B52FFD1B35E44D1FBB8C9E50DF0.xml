<bibRefCitation id="295DB7B5FFD1B35E46B4F99ECB7B0D43" author="Butler, T. H." box="[754,893,1623,1647]" pageId="77" pageNumber="78" pagination="1 - 280" refId="ref218371" refString="Butler, T. H. (1980) Shrimps of the Pacific Coast of Canada. Canadian Bulletin of Fisheries and Aquatic Science, 202, 1 - 280." type="journal article" year="1980">Butler 1980</bibRefCitation>
), 
<quantity id="8A3467A1FFD1B35E47D6F99ECC1B0D43" box="[912,1053,1623,1647]" metricMagnitude="2" metricUnit="m" metricValue="6.365" metricValueMax="11.629999999999999" metricValueMin="1.1" pageId="77" pageNumber="78" unit="m" value="636.5" valueMax="1163.0" valueMin="110.0">110–1163 m</quantity>
