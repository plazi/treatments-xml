<taxonomicName id="4C34EFF6BA6D9D04FDA0F9F1FF51F99E" authorityName="Fleming" authorityYear="1822" class="Aves" family="Muscicapidae" kingdom="Animalia" order="Passeriformes" pageId="24" pageNumber="25" phylum="Chordata" rank="family">Muscicapidae</taxonomicName>
