<taxonomicName id="4C34EFF6BA6D9D04FE8DF9F1FE52F9F0" authorityName="Vigors" authorityYear="1825" box="[354,493,1569,1592]" class="Aves" family="Muscicapidae" kingdom="Animalia" order="Passeriformes" pageId="24" pageNumber="25" phylum="Chordata" rank="subFamily" subFamily="Saxicolinae">Saxicolinae</taxonomicName>
