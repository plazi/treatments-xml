<location id="EA6E94F8FFE7FFA3FDD6FB67FD2D216A" LSID="urn:lsid:plazi:treatment:67187335FFE7FFA3FF3DFBF0FD652196:EA6E94F8FFE7FFA3FDD6FB67FD2D216A" box="[636,740,1257,1283]" country="Kenya" municipality="Gardner" name="Londiani" pageId="4" pageNumber="91">Londiani</location>
