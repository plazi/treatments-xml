<emphasis id="68BCF747FFF22A69498BA546FEECFE9F" bold="true" box="[199,273,295,321]" pageId="11" pageNumber="12">Hosts.</emphasis>
<taxonomicName id="9DC850D6FFF22A694855A546FE68FE9F" box="[281,405,295,321]" class="Magnoliopsida" family="Urticaceae" kingdom="Plantae" order="Rosales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="family">Urticaceae</taxonomicName>
