<taxonomicName id="E67F3E846E90329A30BFAF7CBE1458A3" LSID="http://zoobank.org/30B80D83-4B95-4093-988D-4C10DC078D42" authority="Klimaszewski &amp; Webster" class="Insecta" family="Staphylinidae" genus="Atheta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Atheta (Dimetrota) alphacrenuliventris" order="Coleoptera" pageId="15" pageNumber="100" phylum="Arthropoda" rank="species" species="alphacrenuliventris" subGenus="Dimetrota">Atheta (Dimetrota) alphacrenuliventris Klimaszewski &amp; Webster</taxonomicName>
<taxonomicNameLabel id="28894BB767DA37EF59934EDA81A1D436" pageId="15" pageNumber="100">sp. n.</taxonomicNameLabel>
