<materialsCitation id="3B3DE2400943C1243E46FAE9FE442CBC" collectingDate="1930-02" collectionCode="BMNH" pageId="42" pageNumber="2254" specimenCode="BMNH 1930.8" specimenCount="1">
Accra, 
<date id="FFEBCEDD0943C1243EF6FAE9FCBD2C9C" box="[653,817,1396,1420]" pageId="42" pageNumber="2254" value="1930-02">
<collectingDate id="EFAF37350943C1243EF6FAE9FCBD2C9C" box="[653,817,1396,1420]" pageId="42" pageNumber="2254" value="1930-02">February 1930</collectingDate>
