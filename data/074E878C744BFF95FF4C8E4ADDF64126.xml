<quantity id="481F9B7F744BFF95FEB88EAADECA40E0" box="[326,444,685,707]" metricMagnitude="3" metricUnit="m" metricValue="1.7" metricValueMax="1.9" metricValueMin="1.5" pageId="4" pageNumber="901" unit="m" value="1700.0" valueMax="1900.0" valueMin="1500.0">1500-1900 m</quantity>
