<taxonomicName id="4C754BB1180EA175FEDAFDF1FCFB6130" authority="Brunner de Wattenwyl, 1893" authorityName="Brunner de Wattenwyl" authorityYear="1893" box="[294,788,576,598]" class="Insecta" family="Hymenopodidae" genus="Pachymantis" kingdom="Animalia" order="Mantodea" pageId="5" pageNumber="248" phylum="Arthropoda" rank="species" species="nitida">
