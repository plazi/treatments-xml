<taxonomicName id="4C58F28C5A3AFF8D4438F92AFCF1F964" box="[516,831,1724,1749]" class="Insecta" family="Hesperiidae" genus="Pelopidas" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="325" phylum="Arthropoda" rank="subSpecies" species="conjuncta" subSpecies="narooa">
