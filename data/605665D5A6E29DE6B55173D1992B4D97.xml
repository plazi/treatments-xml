<taxonomicName id="25571C8A89033D8D4B7485B7BCF2AFD7" authority="Förster, 1853" authorityName="Förster" authorityYear="1853" class="Insecta" family="Chrysididae" genus="Notozus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Notozus komarowi" order="Hymenoptera" pageId="77" pageNumber="78" phylum="Arthropoda" rank="species" species="komarowi">Notozus komarowi</taxonomicName>
