<collectionCode id="ED7AAE20FFA2335AFF7AFE9FFEE5FEE5" box="[218,301,296,320]" httpUri="http://grbio.org/cool/m2jc-wmeq" name="University of California, Riverside" pageId="7" pageNumber="400">UCRC</collectionCode>
