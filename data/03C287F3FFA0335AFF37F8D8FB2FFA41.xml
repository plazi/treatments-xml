<number id="3DA324C2FFA2335AFAFDFF68FA50FF5D" box="[1373,1432,223,248]" pageId="7" pageNumber="400" value="2005.0">2005</number>
</date>
, H.-y. Hu group [
<number id="3DA324C2FFA2335AFEF6FEB3FEA1FEB9" box="[342,361,260,284]" pageId="7" pageNumber="400" value="2.0">2</number>
♀, 
<specimenCount id="9D6DFD6CFFA2335AFE2AFEB3FE67FEB8" box="[394,431,260,285]" pageId="7" pageNumber="400" type="generic">
<number id="3DA324C2FFA2335AFE2AFEB3FE51FEB8" box="[394,409,260,285]" pageId="7" pageNumber="400" value="6.0">6</number>
♂
</specimenCount>
, 
<collectionCode id="ED7AAE20FFA2335AFE1DFEB3FDC0FEB9" box="[445,520,260,284]" pageId="7" pageNumber="400">ICXU</collectionCode>
]. 
<emphasis id="B91FEAF7FFA2335AFDBBFEB3FD43FEB8" bold="true" box="[539,651,260,285]" pageId="7" pageNumber="400">
