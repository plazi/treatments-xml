<taxonomicName id="ED4BF9F877CD6AC26D0B9512DA138532" LSID="urn:lsid:zoobank.org:act:D261771E-D635-4AF6-8F96-8FA29579F25C" authorityName="Linnaeus" authorityYear="1758" class="Mammalia" family="Hominidae" genus="Homo" kingdom="Animalia" order="Primates" phylum="Chordata" rank="subSpecies" species="sapiens" status="subspec. nov." subSpecies="monstrosus" zbkClass="Mammalia" zbkKingdom="Animalia" zbkOrder="Primates">Homo sapiens subsp. monstrosus</taxonomicName>
