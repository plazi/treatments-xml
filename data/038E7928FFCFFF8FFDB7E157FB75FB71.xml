<taxonomicName id="4C27B3BDFFD0FF8FFC25E078FAE8FBC1" authority="Mull. Arg." authorityName="Mull. Arg." box="[921,1299,1112,1138]" class="Magnoliopsida" family="Euphorbiaceae" genus="Croton" kingdom="Plantae" order="Malpighiales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="billbergianus">
