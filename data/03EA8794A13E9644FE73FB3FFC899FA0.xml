<taxonomicName id="4C434D01A1259646FE87F9D8FDC29979" authority="Klie, 1929" authorityName="Klie" authorityYear="1929" box="[319,630,1552,1574]" class="Hexanauplia" family="Paramesochridae" genus="Remanea" kingdom="Animalia" order="Harpacticoida" pageId="16" pageNumber="2953" phylum="Arthropoda" rank="species" species="arenicola">
