<taxonomicName id="4C2590B1FFEFFF8FC387FBE8FE3CFBF4" box="[406,500,1049,1073]" class="Arachnida" family="Salticidae" genus="Colyttus" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="56" phylum="Arthropoda" rank="species" species="robustus">
