<taxonomicName id="5EAC4D264D134809FED4FBB64ACD3141" authorityName=": Rutzler" authorityYear="1974" box="[272,444,1089,1113]" class="Demospongiae" family="Clionaidae" genus="Cliona" kingdom="Animalia" order="Clionaida" pageId="24" pageNumber="25" phylum="Porifera" rank="species" species="dioryssa">Cliona dioryssa</taxonomicName>
