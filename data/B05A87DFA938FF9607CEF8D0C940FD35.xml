: 
<taxonomicName id="FFF34D4AA93AFF9606CCFEBDCA31FEB9" box="[405,584,297,323]" class="Magnoliopsida" family="Malvaceae" genus="Tilia" kingdom="Plantae" order="Malvales" pageId="30" pageNumber="231" phylum="Tracheophyta" rank="species" species="americana">
