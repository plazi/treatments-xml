), 
<collectingRegion id="FA37F82BA93AFF9607B3FE2DC94BFE29" box="[234,306,441,467]" country="United States of America" name="Maine" pageId="30" pageNumber="231">Maine</collectingRegion>
and 
<collectingRegion id="FA37F82BA93AFF960636FE2DCA50FE29" box="[367,553,441,467]" country="United States of America" name="New Hampshire" pageId="30" pageNumber="231">New Hampshire</collectingRegion>
(
<bibRefCitation id="5C624B38A93AFF960562FE2DCAA1FE29" author="Brower, A. E." box="[571,728,441,467]" pageId="30" pageNumber="231" pagination="1 - 70" refId="ref12798" refString="Brower, A. E. (1984) A list of the Lepidoptera of Maine. Part 2: The Microlepidoptera Section 2 Cosmopterigidae through Hepialidae. Maine Agricultural Experiment Station Technical Bulletin, 114, 1 - 70." type="journal article" year="1984">Brower 1984</bibRefCitation>
