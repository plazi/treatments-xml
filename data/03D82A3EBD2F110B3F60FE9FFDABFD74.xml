</taxonomicName>
in the 
<taxonomicName id="4C71E0ABBD2F110A3DA4F98CFDEDF90B" authorityName="Broun Desig. Park and Carlton" authorityYear="2013" box="[481,571,1663,1681]" class="Insecta" family="Staphylinidae" genus="Sagola" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="70" pageNumber="71" phylum="Arthropoda" rank="species" species="arboricola">
