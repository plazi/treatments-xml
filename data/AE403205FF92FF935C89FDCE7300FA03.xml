<taxonomicName id="E1E9F890FF91FF925D01F8A47321F891" box="[287,569,1806,1829]" class="Eutardigrada" family="Macrobiotidae" genus="Calcarobiotus" kingdom="Animalia" order="Parachela" pageId="2" pageNumber="26" phylum="Tardigrada" rank="species" species="parvicalcar" status="sp. n.">
<emphasis id="149D5F01FF91FF925D01F8A47321F891" box="[287,569,1806,1829]" italics="true" pageId="2" pageNumber="26">Calcarobiotus parvicalcar</emphasis>
</taxonomicName>
