<taxonomicName id="73C737563BE4472FF929CB9E16F27446" class="Insecta" family="Geometridae" genus="Sauris" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Sauris hirudinata" order="Lepidoptera" pageId="0" pageNumber="7938" phylum="Arthropoda" rank="species" species="hirudinata">Sauris hirudinata</taxonomicName>
<normalizedToken id="8BAFC6A93B7161478E439B5E7536ED0F" originalValue="GuenÃ©e">Guenee</normalizedToken>
