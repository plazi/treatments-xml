<taxonomicName id="9A7021AAC3ECAD481DC54886FCD165F7" class="Gastropoda" family="Ficidae" genus="Ficus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ficus prolixa" order="Littorinimorpha" pageId="0" pageNumber="7938" phylum="Mollusca" rank="species" species="prolixa">Ficus prolixa</taxonomicName>
