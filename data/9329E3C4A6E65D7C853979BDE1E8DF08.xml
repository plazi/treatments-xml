<taxonomicName id="B138AB143A4F563060B5CD637E98118B" class="Magnoliopsida" family="Lauraceae" genus="Alseodaphne" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Alseodaphne" order="Laurales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Alseodaphne</taxonomicName>
