<taxonomicName id="D645F39E4715D582F7AB50D3BFF58A6B" class="Magnoliopsida" family="Lauraceae" genus="Cinnamomum" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Cinnamomum" order="Laurales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Cinnamomum</taxonomicName>
