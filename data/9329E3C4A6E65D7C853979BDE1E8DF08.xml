<taxonomicName id="D5B7D6E6BFA3435D5FF3CD560422C89A" class="Magnoliopsida" family="Lythraceae" genus="Lagerstroemia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Lagerstroemia" order="Myrtales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Lagerstroemia</taxonomicName>
