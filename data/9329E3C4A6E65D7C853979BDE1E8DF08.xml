<taxonomicName id="A0312ABDFA3AE14BF856326881699635" class="Magnoliopsida" family="Loranthaceae" genus="Loranthus" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Loranthus" order="Santalales" pageId="0" pageNumber="7938" phylum="Tracheophyta" rank="genus">Loranthus</taxonomicName>
