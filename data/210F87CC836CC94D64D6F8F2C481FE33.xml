<taxonomicName id="6EA64D59836CC94E64D6F8F2C510F8F8" authority="(Hertwig, 1875)" baseAuthorityName="Hertwig" baseAuthorityYear="1875" box="[151,607,1832,1859]" class="Ciliatea" family="Ephelotidae" genus="Ephelota" higherTaxonomySource="GBIF" kingdom="Protozoa" order="Suctorida" pageId="1" pageNumber="548" phylum="Ciliophora" rank="species" species="gemmipara">
<emphasis id="9BD2EAC8836CC94E64D6F8F2C510F8F8" bold="true" box="[151,607,1832,1859]" pageId="1" pageNumber="548">
<emphasis id="9BD2EAC8836CC94E64D6F8F2C6DAF8F8" bold="true" box="[151,405,1832,1859]" italics="true" pageId="1" pageNumber="548">Ephelota gemmipara</emphasis>
(Hertwig, 1875)
</emphasis>
</taxonomicName>
</heading>
</paragraph>
