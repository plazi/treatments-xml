<taxonomicName id="C8B644C5DB3DFFEFFECBFF16FE30FF2D" box="[279,424,151,177]" class="Insecta" family="Staphylinidae" genus="Distichalius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="17" pageNumber="510" phylum="Arthropoda" rank="genus">
