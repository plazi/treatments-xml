<taxonomicName id="ADA14D22284F7F00F67DFCECB3F5EC10" box="[330,481,835,859]" class="Gastropoda" family="Pomatiidae" genus="Abbottella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Neotaenioglossa" pageId="16" pageNumber="17" phylum="Mollusca" rank="species" species="sosuaensis">
