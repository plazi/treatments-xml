<taxonomicName id="4C06E0B6FFD4F816FC65D6ACFBD3F87B" authorityName="Krefft" authorityYear="1871" box="[931,1074,1975,2001]" class="Chromadorea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="46" pageNumber="47" phylum="Nematoda" rank="genus">
