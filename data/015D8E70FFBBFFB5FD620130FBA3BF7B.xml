<paragraph id="894B3F66FFBAFFB5FF5A0616FBA3BF7B" blockId="49.[189,1398,1288,1350]" pageId="49" pageNumber="50">
<taxonomicName id="4EF444E5FFBAFFB5FF5A0616FE2DBF1E" authority="Castillo &amp; Rédei &amp; Weirauch, 2022" authorityName="Castillo &amp; Rédei &amp; Weirauch" authorityYear="2022" box="[189,385,1289,1315]" class="Insecta" family="Reduviidae" genus="Voconia" kingdom="Animalia" order="Hemiptera" pageId="49" pageNumber="50" phylum="Arthropoda" rank="species" species="fasciata" status="sp. nov.">
