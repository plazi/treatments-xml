<paragraph id="894B3F66FFBBFFB4FF5A07AEFEC0BEF7" blockId="48.[189,364,1200,1227]" box="[189,364,1200,1227]" pageId="48" pageNumber="49">
<heading id="D203880AFFBBFFB4FF5A07AEFEC0BEF7" bold="true" box="[189,364,1200,1227]" fontSize="11" level="3" pageId="48" pageNumber="49" reason="3">
<emphasis id="BB80E374FFBBFFB4FF5A07AEFEC0BEF7" bold="true" box="[189,364,1200,1227]" pageId="48" pageNumber="49">Type material</emphasis>
</heading>
</paragraph>
<paragraph id="894B3F66FFBBFFB4FF0D07C0FBE6BF7D" blockId="48.[189,1398,1247,1344]" pageId="48" pageNumber="49">
