<quantity id="4CCF4CD64546FFC8FBAAFD758DD5FD3C" box="[1101,1171,720,744]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" pageId="1" pageNumber="232" unit="mm" value="2.0">2 mm</quantity>
