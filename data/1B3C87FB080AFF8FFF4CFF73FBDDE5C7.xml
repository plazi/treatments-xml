<quantity id="546D9B080809FF8FFC69FEC1FC0DE55B" box="[946,1021,293,317]" metricMagnitude="4" metricUnit="m" metricValue="1.0" pageId="6" pageNumber="30" unit="km" value="10.0">10 km</quantity>
S Kangding), 
<quantity id="546D9B080809FF8FFB73FEC1FB0CE55B" box="[1192,1276,293,317]" metricMagnitude="3" metricUnit="m" metricValue="4.26" pageId="6" pageNumber="30" unit="m" value="4260.0">4260m</quantity>
