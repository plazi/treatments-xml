<number id="3DFA24E5FFBE10192450F877FBC9C489" box="[1039,1125,1940,1964]" pageId="4" value="93.0" valueMax="106.0" valueMin="80.0">80–106</number>
) (
<number id="3DFA24E5FFBE10192420F877FB21C489" box="[1151,1165,1940,1964]" pageId="4" value="8.0">8</number>
