<number id="3DFA24E5FFBE1019245CF83FFB8DC4D1" box="[1027,1057,2012,2036]" pageId="4" value="16.0">16</number>
), d = 
<number id="3DFA24E5FFBE1019243DF83FFBD2C4D1" box="[1122,1150,2012,2036]" pageId="4" value="10.0">10</number>
(
<number id="3DFA24E5FFBE101924D1F83FFB6BC4D1" box="[1166,1223,2012,2036]" pageId="4" value="8.5" valueMax="11.0" valueMin="6.0">6–11</number>
) (
<number id="3DFA24E5FFBE10192480F83FFB52C4D1" box="[1247,1278,2012,2036]" pageId="4" value="16.0">16</number>
), e = 
<number id="3DFA24E5FFBE10192562F83FFAE7C4D1" box="[1341,1355,2012,2036]" pageId="4" value="9.0">9</number>
(
<number id="3DFA24E5FFBE10192504F83FFA3FC4D1" box="[1371,1427,2012,2036]" pageId="4" value="9.5" valueMax="11.0" valueMin="8.0">8–11</number>
) (
<number id="3DFA24E5FFBF101820FFFF74FF13C38A" box="[160,191,151,175]" pageId="5" value="16.0">16</number>
