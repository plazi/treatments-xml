(
<number id="3DFA24E5FFBE1019235CF877FCC4C489" box="[771,872,1940,1964]" pageId="4" value="405.0" valueMax="463.0" valueMin="347.0">347–463</number>
) (
<number id="3DFA24E5FFBE101923DEF877FC23C489" box="[897,911,1940,1964]" pageId="4" value="8.0">8</number>
