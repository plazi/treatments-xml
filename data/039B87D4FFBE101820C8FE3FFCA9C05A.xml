<number id="3DFA24E5FFBE10192193F83FFE44C4D1" box="[460,488,2012,2036]" pageId="4" value="25.0">25</number>
(
<number id="3DFA24E5FFBE101921A8F83FFD93C4D1" box="[503,575,2012,2036]" pageId="4" value="25.5" valueMax="27.0" valueMin="24.0">24–27</number>
) (
<number id="3DFA24E5FFBE10192207F83FFDDBC4D1" box="[600,631,2012,2036]" pageId="4" value="16.0">16</number>
), b = 
<number id="3DFA24E5FFBE101922E8F83FFD7FC4D1" box="[695,723,2012,2036]" pageId="4" value="20.0">20</number>
(
<number id="3DFA24E5FFBE101922BCF83FFC86C4D1" box="[739,810,2012,2036]" pageId="4" value="20.5" valueMax="23.0" valueMin="18.0">18–23</number>
) (
<number id="3DFA24E5FFBE1019231CF83FFCCDC4D1" box="[835,865,2012,2036]" pageId="4" value="16.0">16</number>
), c = 
<number id="3DFA24E5FFBE101923FEF83FFC03C4D1" box="[929,943,2012,2036]" pageId="4" value="5.0">5</number>
(
<number id="3DFA24E5FFBE101923E1F83FFC47C4D1" box="[958,1003,2012,2036]" pageId="4" value="5.0" valueMax="6.0" valueMin="4.0">4–6</number>
