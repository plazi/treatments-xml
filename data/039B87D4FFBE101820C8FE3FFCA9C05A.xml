<number id="3DFA24E5FFBF10182168FF5FFEFFC3F1" box="[311,339,188,212]" pageId="5" value="14.0">14</number>
(
<number id="3DFA24E5FFBF1018213CFF5FFE07C3F1" box="[355,427,188,212]" pageId="5" value="14.0" valueMax="16.0" valueMin="12.0">12–16</number>
) (
<number id="3DFA24E5FFBF1018219BFF5FFE7FC3F1" box="[452,467,188,212]" pageId="5" value="8.0">8</number>
), h = 
<number id="3DFA24E5FFBF10182248FF5FFD9FC3F1" box="[535,563,188,212]" pageId="5" value="16.0">16</number>
(
<number id="3DFA24E5FFBF1018221CFF5FFD26C3F1" box="[579,650,188,212]" pageId="5" value="16.0" valueMax="19.0" valueMin="13.0">13–19</number>
) (
<number id="3DFA24E5FFBF101822FBFF5FFD6FC3F1" box="[676,707,188,212]" pageId="5" value="15.0">15</number>
