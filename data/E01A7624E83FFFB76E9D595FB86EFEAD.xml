<taxonomicName id="AFB3BCB1E83FFFB06F775E91B8F8FA3A" authorityName="COIFFAIT" authorityYear="1982" box="[344,454,1499,1521]" class="Insecta" family="Staphylinidae" genus="Atopolinus" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="1764" phylum="Arthropoda" rank="genus">
