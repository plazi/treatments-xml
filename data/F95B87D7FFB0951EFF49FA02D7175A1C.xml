<taxonomicName id="B6F24D42FFB0951DFEF7FA5BD0C85EE7" authority="Tang &amp; Skelley &amp; Pérez-Farrera, 2018" authorityName="Tang &amp; Skelley &amp; Pérez-Farrera" authorityYear="2018" box="[297,676,1488,1510]" class="Cycadopsida" family="Zamiaceae" genus="Ceratophila" higherTaxonomySource="GBIF" kingdom="Plantae" order="Cycadales" pageId="5" pageNumber="156" phylum="Tracheophyta" rank="subGenus" species="picipennis" status="n. sp." subGenus="Ceratophila">
