<taxonomicName id="EEDE15B0FFDC4A4A85B5FF02FC9AFF6D" authorityName="Harmer" authorityYear="1957" box="[650,799,188,212]" class="Gymnolaemata" family="Lanceoporidae" genus="Calyptotheca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomata" pageId="2" pageNumber="469" phylum="Bryozoa" rank="genus">
