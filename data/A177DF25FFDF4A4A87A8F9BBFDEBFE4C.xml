<taxonomicName id="EEDE15B0FFDF4A4984CDF811FBEAF87F" authority="Sebastian &amp; Cumming, 2016" authorityName="Sebastian &amp; Cumming" authorityYear="2016" box="[1010,1135,1966,1990]" class="Gymnolaemata" family="Lanceoporidae" genus="Calyptotheca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomata" pageId="1" pageNumber="468" phylum="Bryozoa" rank="species" species="wulguru" status="n. sp.">
<emphasis id="1BAAB221FFDF4A4984CDF811FBEAF87F" box="[1010,1135,1966,1990]" italics="true" pageId="1" pageNumber="468">C. wulguru</emphasis>
</taxonomicName>
<taxonomicNameLabel id="00990F5AFFDF4A498349F810FB30F87E" box="[1142,1205,1966,1991]" pageId="1" pageNumber="468" rank="species">
<emphasis id="1BAAB221FFDF4A498349F810FB30F87E" bold="true" box="[1142,1205,1966,1991]" pageId="1" pageNumber="468">n. sp.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName id="EEDE15B0FFDF4A4983D4F811FAD3F87F" authority="Sebastian &amp; Cumming, 2016" authorityName="Sebastian &amp; Cumming" authorityYear="2016" box="[1259,1366,1966,1990]" class="Gymnolaemata" family="Lanceoporidae" genus="Calyptotheca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cheilostomata" pageId="1" pageNumber="468" phylum="Bryozoa" rank="species" species="churro" status="n. sp.">
<emphasis id="1BAAB221FFDF4A4983D4F811FB7BF87F" box="[1259,1278,1967,1990]" italics="true" pageId="1" pageNumber="468">C</emphasis>
