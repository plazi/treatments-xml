</specimenCount>
of 
<taxonomicName id="E7D71BF48D394A042E60FB27FB24FB0B" box="[1101,1238,1154,1177]" class="Insecta" family="Eulophidae" genus="Cirrospilus" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="ingenuus">
