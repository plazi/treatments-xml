<taxonomicName id="DFB1C2C0C305913D79B78E61FCDA7C29" authorityName=": WINKLER (1932:" authorityYear="1452" box="[757,887,1042,1065]" class="Insecta" family="Curculionidae" genus="Asphalmus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="589" phylum="Arthropoda" rank="species" species="japonicus">
<emphasis id="2AC56551C305913D79B78E61FCDA7C29" box="[757,887,1042,1065]" italics="true" pageId="12" pageNumber="589">A. japonicus</emphasis>
</taxonomicName>
