<subSubSection id="C32C655AE57CFFCAFC30F9653638F8F4" box="[970,1297,1771,1795]" pageId="3" pageNumber="258" type="nomenclature">
<paragraph id="8B8936D1E57CFFCAFC30F9653638F8F4" blockId="3.[970,1297,1771,1795]" box="[970,1297,1771,1795]" pageId="3" pageNumber="258">
<heading id="D0C181BDE57CFFCAFC30F9653638F8F4" box="[970,1297,1771,1795]" centered="true" fontSize="10" level="2" pageId="3" pageNumber="258" reason="3">
<taxonomicName id="4C364D52E57CFFCAFC30F9653624F8F4" authority="Hope, 1841: 442" authorityName="Hope" authorityPageNumber="442" authorityYear="1841" box="[970,1293,1771,1795]" class="Insecta" family="Naucoridae" genus="Cheirochela" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="258" phylum="Arthropoda" rank="genus">
