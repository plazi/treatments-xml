<paragraph id="8BC75C02FFDDFFC1859E19DAFB02FE32" blockId="69.[128,654,213,1759]" lastBlockId="69.[701,1228,213,1186]" pageId="69" pageNumber="69">
FEMALE (
<typeStatus id="54C3E2A0FFDDFFC1841F19DBFE19FA53" box="[284,391,1444,1465]" pageId="69" pageNumber="69">paratype</typeStatus>
, PBI_ 
<accessionNumber id="942BC1E1FFDDFFC184E319DBFD1CFA50" box="[480,642,1444,1466]" httpUri="http://www.ncbi.nlm.nih.gov/protein/OON30669" pageId="69" pageNumber="69">OON 30669</accessionNumber>
