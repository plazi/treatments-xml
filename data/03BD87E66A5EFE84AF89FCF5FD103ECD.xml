<quantity id="4CEC9B156A20FE84ADECFB28FCA53E11" box="[754,891,834,861]" metricMagnitude="-3" metricUnit="m" metricValue="6.449999999999999" metricValueMax="6.8" metricValueMin="6.1" pageId="321" pageNumber="322" unit="mm" value="6.449999999999999" valueMax="6.8" valueMin="6.1">6.1–6.8 mm</quantity>
