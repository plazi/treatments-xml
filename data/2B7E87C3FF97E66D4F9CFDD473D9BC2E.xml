<elevation id="28FAD1E6FF97E66F4CEAFC347435BABA" box="[993,1052,906,933]" metricMagnitude="1" metricUnit="m" metricValue="9.2" pageId="125" pageNumber="126" unit="m" value="92.0">92 m</elevation>
</quantity>
, 
<date id="D7691015FF97E66F4B23FC34752CBABB" box="[1064,1285,906,932]" pageId="125" pageNumber="126" value="2019-11-14">
<collectingDate id="C72DE9FDFF97E66F4B23FC34752CBABB" box="[1064,1285,906,932]" pageId="125" pageNumber="126" value="2019-11-14">14 November 2019</collectingDate>
</date>
: 1 M 
<quantity id="642F9B30FF97E66F4A4DFC3475B4BABA" box="[1350,1437,906,933]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="125" pageNumber="126" unit="mm" value="1.7">1.7 mm</quantity>
(MNHN-IU-2014-20101)
