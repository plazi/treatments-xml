<taxonomicName id="7612477FDBCCE7A96430F4EA56CC4335" baseAuthorityName="Risso" baseAuthorityYear="1816" class="Malacostraca" family="Squillidae" genus="Meiosquilla" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Meiosquilla desmaresti" order="Stomatopoda" pageId="0" pageNumber="47183" phylum="Arthropoda" rank="species" species="desmaresti">Meiosquilla desmaresti</taxonomicName>
