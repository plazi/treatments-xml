<taxonomicName id="4C0F7AE98D1EFF23CD4AFE1BFE6D9E76" box="[268,405,496,521]" class="Actinopterygii" family="Congridae" genus="Gnathophis" kingdom="Animalia" order="Anguilliformes" pageId="6" pageNumber="7" phylum="Chordata" rank="genus">
