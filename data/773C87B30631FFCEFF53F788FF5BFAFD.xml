<taxonomicName id="38954D260631FFCEFD21F2D2FE99FB7D" authority="PACE, 2008" authorityName="PACE" authorityYear="2008" class="Insecta" family="Staphylinidae" genus="Gansiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="87" phylum="Arthropoda" rank="species" species="peruviana">
<emphasis id="CDE1EAB70631FFCEFD21F2D2FCB2FB5D" box="[657,793,1193,1217]" italics="true" pageId="4" pageNumber="87">G. peruviana</emphasis>
<bibRefCitation id="9B044B540631FFCEFF0DF2B2FE99FB7D" author="PACE, R." box="[189,306,1225,1249]" pageId="4" pageNumber="87" pagination="225 - 398" refId="ref4746" refString="PACE, R. 2008: New records of Aleocharinae from Ecuador and Peru, with the description of new species, new subgenera and new genera (Coleoptera, Staphylinidae): pp. 225 - 398. - In: GIACHINO P. M. (ed.) 2008: Biodiversity of South America I. - Memoirs on Biodiversity, World Biodiversity Association onlus, Verona 1: 496 pp." type="book chapter" year="2008">PACE, 2008</bibRefCitation>
</taxonomicName>
del 
<collectingCountry id="878276350631FFCEFEEDF2B2FE24FB7D" box="[349,399,1225,1249]" name="Peru" pageId="4" pageNumber="87">Perù</collectingCountry>
e 
<collectingCountry id="878276350631FFCEFE15F2B2FDF6FB7D" box="[421,605,1225,1249]" name="French Guiana" pageId="4" pageNumber="87">Guyana Francese</collectingCountry>
. È distinta da 
<emphasis id="CDE1EAB70631FFCEFD40F2B1FCB2FB7D" box="[752,793,1226,1249]" italics="true" pageId="4" pageNumber="87">essa</emphasis>
e da 
<taxonomicName id="38954D260631FFCEFF5EF292FEFFFA9D" authorityName="Pace" authorityYear="2014" box="[238,340,1257,1281]" class="Insecta" family="Staphylinidae" genus="Gansiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="87" phylum="Arthropoda" rank="species" species="granulosa">
<emphasis id="CDE1EAB70631FFCEFF5EF292FEFFFA9D" box="[238,340,1257,1281]" italics="true" pageId="4" pageNumber="87">granulosa</emphasis>
