<specimenCount id="DDA8FD5FA81DFFC2FCEAFE74FC4708AD" box="[895,1018,417,439]" pageId="5" pageNumber="5" type="male">four males</specimenCount>
and 
<specimenCount id="DDA8FD5FA81DFFC2FBA1FE74FB7A08AD" box="[1076,1223,417,439]" pageId="5" pageNumber="5" type="female">four females</specimenCount>
, collected at the type locality between 
<date id="BF101016A81DFFC2FBE2FE6BFC7F08EB" pageId="5" pageNumber="5" value="1905-07-22" valueMax="1905-11-02" valueMin="1905-07-22">
<collectingDate id="AF54E9FEA81DFFC2FBE2FE6BFC7F08EB" pageId="5" pageNumber="5" value="1905-07-22" valueMax="1905-11-02" valueMin="1905-07-22">22 July and 2 November 1905</collectingDate>
