<emphasis id="F9DAEAC4A81DFFC2FCEAFE56FB2B0882" bold="true" box="[895,1174,387,408]" pageId="5" pageNumber="5">
<specimenCode id="9B089EADA81DFFC2FCEAFE56FB2B0882" box="[895,1174,387,408]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="5" pageNumber="5">AMNH 716676–716682</specimenCode>
</emphasis>
and 
<emphasis id="F9DAEAC4A81DFFC2FD28FE74FCCC08AC" bold="true" box="[701,881,417,438]" pageId="5" pageNumber="5">
<specimenCode id="9B089EADA81DFFC2FD28FE74FCCC08AC" box="[701,881,417,438]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="5" pageNumber="5">AMNH 716684</specimenCode>
</emphasis>
