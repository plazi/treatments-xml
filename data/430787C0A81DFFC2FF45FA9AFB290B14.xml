<collectingCounty id="22704E5AA81DFFC2FBCCFEB3FB210866" box="[1113,1180,358,380]" pageId="5" pageNumber="5">There</collectingCounty>
are 
<specimenCount id="DDA8FD5FA81DFFC2FD28FE51FCCC0883" box="[701,881,388,410]" pageId="5" pageNumber="5" type="generic" typeStatus="paratypes">eight paratypes</specimenCount>
