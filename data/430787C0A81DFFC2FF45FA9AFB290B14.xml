<location id="CE71600DA81DFFC2FC20FEF9FBEA0858" LSID="urn:lsid:plazi:treatment:430787C0A81DFFC2FF45FA9AFB290B14:CE71600DA81DFFC2FC20FEF9FBEA0858" box="[949,1111,300,322]" county="There" municipality="Tsin-ling Mountains" name="Si Taipaishan" pageId="5" pageNumber="5">Si Taipaishan</location>
, 
<collectingMunicipality id="2B75ACACA81DFFC2FBF3FEF9FC830845" pageId="5" pageNumber="5">Tsin-ling Mountains</collectingMunicipality>
. 
<specimenCode id="9B089EADA81DFFC2FCD9FE9CFBBE0845" box="[844,1027,329,351]" collectionCode="AMNH" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="5" pageNumber="5">AMNH 716683</specimenCode>
