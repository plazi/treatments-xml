<subSubSection id="C31934BBFFF1FFA4FF025D9229152DD6" pageId="23" pageNumber="518" type="etymology">
<paragraph id="8BBC6730FFF1FFA4FF025D9229152DD6" blockId="23.[151,1437,151,968]" pageId="23" pageNumber="518">
<emphasis id="B977BB22FFF1FFA4FF025D922B132DBB" bold="true" box="[199,334,908,933]" pageId="23" pageNumber="518">Etymology.</emphasis>
