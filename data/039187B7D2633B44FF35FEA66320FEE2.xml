<taxonomicName id="4C384D22D2633B43FDC9FE5260AAFE56" baseAuthorityName="Kato" baseAuthorityYear="1944" box="[619,743,441,467]" class="Rhabditophora" family="Euryleptidae" genus="Parastylostomum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Polycladida" pageId="11" pageNumber="328" phylum="Platyhelminthes" rank="species" species="maculatum">
