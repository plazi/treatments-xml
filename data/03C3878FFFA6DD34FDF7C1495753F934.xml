<collectingCountry id="F37D7609FFA6DD34FBB5C75F57C1FADD" box="[1148,1265,1303,1329]" name="Mongolia" pageId="6" pageNumber="231">Mongolia</collectingCountry>
, 
<collectingCountry id="F37D7609FFA6DD34FEF4C77352E2FAB9" box="[317,466,1339,1365]" name="North Korea" pageId="6" pageNumber="231">North Korea</collectingCountry>
