; 10, 
<taxonomicName id="79B14D55FFE17E3C4707931B52F4062A" authority="de Meijere" authorityName="de Meijere" box="[794,1007,2062,2084]" class="Insecta" family="Hybotidae" genus="Syndyas" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="21" phylum="Arthropoda" rank="species" species="brevior">
