<subSubSection id="C369656A3B47FFB9FF55FB76FC26FAA9" pageId="45" pageNumber="46" type="biology_ecology">
<paragraph id="8BCC36E13B47FFB9FF55FB76FC26FAA9" blockId="45.[151,1437,1117,1538]" pageId="45" pageNumber="46">
<emphasis id="B907EAF33B47FFB9FF55FB76FE80FA85" bold="true" box="[199,295,1261,1286]" pageId="45" pageNumber="46">Biology.</emphasis>
<taxonomicName id="4C734D623B47FFB9FEA6FB76FD84FA85" box="[308,547,1261,1286]" class="Insecta" family="Eulophidae" genus="Pediobius" kingdom="Animalia" order="Hymenoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="susinellae">
