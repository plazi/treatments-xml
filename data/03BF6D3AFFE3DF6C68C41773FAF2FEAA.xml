sp. (Monterey Co., CA), 
<taxonomicName id="4C16A7AFFFE3DF6D6B1612E0FB11F87D" authority="(Hook.) Nutt." authorityName="(Hook.) Nutt." box="[837,1239,1953,1979]" class="Magnoliopsida" family="Rhamnaceae" genus="Ceanothus" kingdom="Plantae" order="Rosales" pageId="22" pageNumber="23" phylum="Tracheophyta" rank="species" species="cuneatus">
