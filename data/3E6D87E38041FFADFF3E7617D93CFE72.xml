<taxonomicName id="71C44D768040FFAFFB257238D921FE9E" box="[1164,1349,334,359]" class="Reptilia" family="Gekkonidae" genus="Lepidodactylus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="322" phylum="Chordata" rank="species" species="novaeguineae">
