<taxonomicName id="71C44D768040FFAFFA2F725CDD56FE9E" class="Reptilia" family="Gekkonidae" genus="Lepidodactylus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="322" phylum="Chordata" rank="species" species="novaeguineae">
