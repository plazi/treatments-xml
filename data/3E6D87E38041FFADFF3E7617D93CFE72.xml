<taxonomicName id="71C44D768040FFAFFD82725CDE82FEBA" box="[555,742,298,323]" class="Reptilia" family="Gekkonidae" genus="Lepidodactylus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="322" phylum="Chordata" rank="species" species="novaeguineae">
