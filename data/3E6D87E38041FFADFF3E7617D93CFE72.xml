<taxonomicName id="71C44D768040FFAFFA2F72CCDC9DFE0E" authority="Kraus, 2019" authorityName="Kraus" authorityYear="2019" class="Reptilia" family="Gekkonidae" genus="Lepidodactylus" kingdom="Animalia" order="Squamata" pageId="17" pageNumber="322" phylum="Chordata" rank="species" species="mitchelli" status="sp. nov.">
