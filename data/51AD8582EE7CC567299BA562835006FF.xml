, 
<taxonomicName id="0132732EFBDF44B26D9CDB5F59827829" baseAuthorityName="I.Sowerby" baseAuthorityYear="1833" class="Gastropoda" family="Bulimulidae" genus="Bostryx" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" lsidName="" order="Stylommatophora" pageId="144" pageNumber="145" phylum="Mollusca" rank="species" species="scabiosus">
