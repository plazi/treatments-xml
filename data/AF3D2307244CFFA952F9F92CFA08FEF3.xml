<typeStatus id="F82F2CB3244CFFA95359F88DFE05F84E" box="[317,416,1914,1938]" pageId="8" pageNumber="109">Holotype</typeStatus>
<specimenCode id="77323A6A244CFFA953C8F88DFD14F84E" box="[428,689,1914,1938]" collectionCode="HNHM-PBO" pageId="8" pageNumber="109">HNHM-PBO 2008.115</specimenCode>
