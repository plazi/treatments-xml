<quantity id="4D7CFAA3D934FFAC04B6FA0EFF31FA4B" metricMagnitude="-4" metricUnit="m" metricValue="7.949999999999999" metricValueMax="8.299999999999999" metricValueMin="7.6" pageId="14" pageNumber="119" unit="mm" value="0.7949999999999999" valueMax="0.83" valueMin="0.76">0.76–0.83 mm</quantity>
