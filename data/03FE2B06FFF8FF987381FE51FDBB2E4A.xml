<taxonomicName id="4C57E193FFF8FF987381FE51FEDF2F65" authorityName="Leite &amp; Haimovici" authorityYear="2008" box="[151,364,509,536]" class="Cephalopoda" family="Octopodidae" genus="Octopus" kingdom="Animalia" order="Octopoda" pageId="3" pageNumber="592" phylum="Mollusca" rank="species" species="insularis">
