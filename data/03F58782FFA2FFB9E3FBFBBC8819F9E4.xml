Known only to the type-locality, Trindade 
<collectingCountry id="F34B7604FFA8FFB9E033F9CC8A9EF999" box="[863,931,1556,1581]" name="Iceland" pageId="25" pageNumber="384">Island</collectingCountry>
(
<geoCoordinate id="EE685053FFA8FFB9E0DBF9CC8D03F998" box="[951,1086,1556,1580]" direction="south" orientation="latitude" pageId="25" pageNumber="384" precision="15" value="-20.524721">20°31’29”S</geoCoordinate>
, 
<geoCoordinate id="EE685053FFA8FFB9E722F9CC8DDCF998" box="[1102,1249,1556,1580]" direction="west" orientation="longitude" pageId="25" pageNumber="384" precision="15" value="-29.324722">29°19’29”W</geoCoordinate>
), 
<collectingRegion id="4998F876FFA8FFB9E795F9CC8CA1F998" box="[1273,1436,1556,1581]" country="Brazil" name="Espirito Santo" pageId="25" pageNumber="384">Espírito Santo</collectingRegion>
state, 
