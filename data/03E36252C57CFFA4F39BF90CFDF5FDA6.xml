<taxonomicName id="4C4AA8C7C57CFFA2F39BF90CFE29F921" authorityName="Heyden" authorityYear="1827" box="[151,428,1697,1723]" class="Reptilia" family="Gekkonidae" genus="Hemidactylus" kingdom="Animalia" order="Squamata" pageId="67" pageNumber="68" phylum="Chordata" rank="species" species="robustus">
<emphasis id="B93E0F56C57CFFA2F39BF90CFE29F921" bold="true" box="[151,428,1697,1723]" italics="true" pageId="67" pageNumber="68">Hemidactylus robustus</emphasis>
</taxonomicName>
