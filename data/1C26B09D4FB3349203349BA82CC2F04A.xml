<taxonomicName id="D40966C6A89D8BB9A9AEF213FAC47B8B" class="Insecta" family="Milichiidae" genus="Desmometopa" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Desmometopa" order="Diptera" pageId="4" pageNumber="95" phylum="Arthropoda" rank="genus">Desmometopa</taxonomicName>
