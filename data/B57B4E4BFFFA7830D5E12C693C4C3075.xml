<quantity id="FA2A52B8FFE77830D1AD29683D33342A" box="[1243,1342,583,608]" metricMagnitude="-1" metricUnit="m" metricValue="3.048" metricValueMax="3.302" metricValueMin="2.794" pageId="18" pageNumber="152" unit="in" value="12.0" valueMax="13.0" valueMin="11.0">11–13 in</quantity>
