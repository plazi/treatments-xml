<taxonomicName id="63341F4CB38C1514AE44BC386A3CEEEC" authority="Scopoli, 1777" authorityName="Scopoli" authorityYear="1777" class="Gastropoda" family="Bulimulidae" genus="Bulimus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Bulimus hartwegi" order="Stylommatophora" pageId="21" pageNumber="22" phylum="Mollusca" rank="species" species="hartwegi">Bulimus hartwegi</taxonomicName>
