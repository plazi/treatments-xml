<emphasis id="77946A5CD855FFAEFC9D43194759F98A" bold="true" box="[779,866,1552,1573]" pageId="69" pageNumber="70">
<taxonomicNameLabel id="6CA7D727D855FFAEFC9D4319477FF98A" box="[779,836,1552,1573]" pageId="69" pageNumber="70" rank="species">sp. n.</taxonomicNameLabel>
B.
</emphasis>
<taxonomicName id="82E0CDCDD855FFAEFCFE431847D8F989" box="[872,995,1553,1574]" class="Malacostraca" family="Melphidippidae" genus="Horniella" kingdom="Animalia" order="Amphipoda" pageId="69" pageNumber="70" phylum="Arthropoda" rank="species" species="smetanai" status="sp. n.">
<emphasis id="77946A5CD855FFAEFCFE43184741F989" box="[872,890,1553,1574]" italics="true" pageId="69" pageNumber="70">H</emphasis>
. 
<emphasis id="77946A5CD855FFAEFC11431847D8F989" box="[903,995,1553,1574]" italics="true" pageId="69" pageNumber="70">smetanai</emphasis>
</taxonomicName>
<emphasis id="77946A5CD855FFAEFC7F43194019F98A" bold="true" box="[1001,1058,1552,1573]" pageId="69" pageNumber="70">
