<location id="8E837392FF9D1E77FE5BFC20FD94C5A9" LSID="urn:lsid:plazi:treatment:03F5945FFF9D1E77FF5DFC8FFC76C2AE:8E837392FF9D1E77FE5BFC20FD94C5A9" box="[434,536,989,1011]" country="India" municipality="Wayanad" name="Kalpetta" pageId="54" pageNumber="428" stateProvince="Kerala">Kalpetta</location>
