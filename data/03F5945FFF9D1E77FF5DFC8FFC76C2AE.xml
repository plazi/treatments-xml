<materialsCitation id="3B342F14FF9D1E77FEBBFC42FE64C26A" collectionCode="SVL" country="India" location="Kalpetta" municipality="Wayanad" pageId="54" pageNumber="428" specimenCode="BNHS 4244, BNHS 4245, BNHS 4246" specimenCount="1" specimenCount-adult="1" stateProvince="Kerala" typeStatus="holotype">
<typeStatus id="54E79BEBFF9D1E77FEBBFC42FE37C58F" box="[338,443,959,981]" pageId="54" pageNumber="428">Holotype</typeStatus>
, 
<specimenCode id="DBFA8D32FF9D1E77FE21FC42FDD9C58E" box="[456,597,959,980]" collectionCode="BNHS" country="India" httpUri="http://grbio.org/cool/8g20-8f2r" name="Bombay Natural History Society" pageId="54" pageNumber="428">BNHS 4244</specimenCode>
