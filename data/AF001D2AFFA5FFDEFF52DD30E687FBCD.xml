<number id="9161BE1BFFABFFDBFF5ADD57E2C3F82B" box="[159,197,1914,1940]" pageId="9" pageNumber="10" value="3.6">3.6</number>
x), article 
<number id="9161BE1BFFABFFDBFE83DD57E353F82B" box="[326,341,1914,1940]" pageId="9" pageNumber="10" value="3.0">3</number>
shorter than article 
<number id="9161BE1BFFABFFDBFD86DD57E054F82B" box="[579,594,1914,1940]" pageId="9" pageNumber="10" value="1.0">1</number>
(
<number id="9161BE1BFFABFFDBFDA7DD57E08EF82B" box="[610,648,1914,1940]" pageId="9" pageNumber="10" value="0.3">0.3</number>
x); accessory flagellum with 
<number id="9161BE1BFFABFFDBFC22DD57E1F0F82B" box="[999,1014,1914,1940]" pageId="9" pageNumber="10" value="1.0">1</number>
