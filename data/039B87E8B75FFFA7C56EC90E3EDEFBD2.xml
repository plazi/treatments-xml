<quantity id="4CCA9B1BB75CFFA7C09ECB8E3E0EFC5A" metricMagnitude="-4" metricUnit="m" metricValue="5.699999999999999" pageId="6" pageNumber="7" unit="mm" value="0.57">0.57 mm</quantity>
, Edist 
