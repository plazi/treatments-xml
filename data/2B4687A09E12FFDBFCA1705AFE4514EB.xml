<geoCoordinate id="C6DB50719E12FFDAFC3B7110FC2E136D" box="[904,984,1290,1312]" degrees="18.05" direction="south" orientation="latitude" pageId="22" pageNumber="22" precision="555" value="-18.05">18.05S</geoCoordinate>
