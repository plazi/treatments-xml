<geoCoordinate id="C6DB50719E12FFDAFC577111FBC8136D" box="[996,1086,1291,1312]" degrees="39.34" direction="west" orientation="longitude" pageId="22" pageNumber="22" precision="555" value="-39.34">39.34W</geoCoordinate>
