<taxonomicName id="513A4B5B05FCD674BF3CBA7BAF2E259D" class="Insecta" family="Myrmecophilidae" genus="Myrmecophilus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Myrmecophilus albicinctus" order="Orthoptera" pageId="3" pageNumber="100" phylum="Arthropoda" rank="species" species="albicinctus">Myrmecophilus albicinctus</taxonomicName>
