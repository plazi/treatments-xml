<subSubSection id="C35C6559FF86FFD9FF35FE3CFAEB82DF" box="[197,1300,481,505]" pageId="15" pageNumber="16" type="etymology">
<paragraph id="8BF936D2FF86FFD9FF35FE3CFAEB82DF" blockId="15.[151,1437,151,542]" box="[197,1300,481,505]" pageId="15" pageNumber="16">
<emphasis id="B932EAC0FF86FFD9FF35FE3CFEB482DF" bold="true" box="[197,331,481,505]" pageId="15" pageNumber="16">Etymology.</emphasis>
