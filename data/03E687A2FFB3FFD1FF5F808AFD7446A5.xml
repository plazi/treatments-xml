<elevation id="0062D187FFB3FFD3FB608186FB7B41A9" box="[1192,1268,1446,1473]" metricMagnitude="2" metricUnit="m" metricValue="4.65" pageId="13" pageNumber="464" unit="m" value="465.0">465 m</elevation>
</quantity>
), coll. 
<collectorName id="26BA5362FFB3FFD3FA8D8187FF53418C" pageId="13" pageNumber="464">
RV 
<emphasis id="B93BEAA6FFB3FFD3FABA8187FF53418C" italics="true" pageId="13" pageNumber="464">Natureza</emphasis>
</collectorName>
, 
<date id="FFF11074FFB3FFD3FF2281EAFEE6418D" box="[234,361,1482,1509]" pageId="13" pageNumber="464" value="2000-03-25">
