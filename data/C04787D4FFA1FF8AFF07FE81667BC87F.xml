<taxonomicName id="8FEE4D41FFA1FF8AFF67FE816770CA06" authority="Cooley, 1945" authorityName="Cooley" authorityYear="1945" box="[247,563,294,321]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="72" pageNumber="73" phylum="Arthropoda" rank="species" species="tovari">
