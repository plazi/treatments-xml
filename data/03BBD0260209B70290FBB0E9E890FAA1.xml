<taxonomicName id="4C121AB30209B70291F1B0E9EF0EFD0C" authorityName="Hope" authorityYear="1838" box="[413,517,654,680]" class="Insecta" family="Carabidae" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="tribe" tribe="Broscini">Broscini</taxonomicName>
