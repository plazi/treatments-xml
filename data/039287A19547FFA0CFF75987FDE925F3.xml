<taxonomicName id="4C3B4D349547FFA0CCB95F37FB32253B" baseAuthorityName="Muller &amp; Henle" baseAuthorityYear="1838" box="[985,1227,1338,1364]" class="Elasmobranchii" family="Scyliorhinidae" genus="Scyliorhinus" kingdom="Animalia" order="Carcharhiniformes" pageId="46" pageNumber="47" phylum="Chordata" rank="species" species="capensis">
