<collectionCode id="86DBAE73FE9BFEFD5982FA9AFE596448" box="[412,490,1303,1329]" country="Italy" httpUri="http://grbio.org/cool/mvk4-k796" name="Museo Zoologico La Specola, Universita di Firenze" pageId="342" pageNumber="343">MZUF</collectionCode>
