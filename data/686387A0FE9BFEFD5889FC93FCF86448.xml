<taxonomicName id="27CA4D35FE9BFEFD5CB7FB7EFEDF6448" authority="Rondani, 1861" authorityName="Rondani" authorityYear="1861" class="Insecta" family="Tachinidae" genus="Phoricheta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="342" pageNumber="343" phylum="Arthropoda" rank="species" species="lacrimans">
