<emphasis id="A7002255B9A377B12C820F85D6FBE428" bold="true" pageId="9" pageNumber="10">F</emphasis>
<taxonomicName id="543F3D7BFAB53326B7376DAD3C7972A9" authorityName="Erichson" authorityYear="1842" class="Hexapoda" family="Trogossitidae" genus="Egolia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="variegata">
