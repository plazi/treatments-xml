<emphasis id="04256324B002399E6645361211C6F979" bold="true" pageId="9" pageNumber="10">G</emphasis>
<taxonomicName id="D3ED204EA7AE6C117A21AFD3DF2C9E75" authorityName="Crowson" authorityYear="1964" class="Insecta" family="Trogossitidae" genus="Necrobiopsis" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="" order="Coleoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="tasmanicus">
