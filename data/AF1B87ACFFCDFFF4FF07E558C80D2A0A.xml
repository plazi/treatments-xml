<number id="917A249DFFCDFFF4FBB8E52BCE522980" box="[1064,1131,1138,1164]" pageId="2" pageNumber="61" value="2009.0">2009</number>
. 
<date id="530C107AFFCDFFF4FBFEE52BCE8F2980" box="[1134,1206,1138,1164]" pageId="2" pageNumber="61" value="1922-09">
