<taxonomicName id="AEB2EE995968FD65E5B5A271BF8AE410" class="Insecta" family="Buprestidae" genus="Polycesta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Polycesta" order="Coleoptera" pageId="139" pageNumber="140" phylum="Arthropoda" rank="genus">Polycesta</taxonomicName>
