<elevation id="0028861AFFC80C30FD4CFEBBE348FE84" box="[686,831,306,327]" metricMagnitude="3" metricUnit="m" metricValue="2.2824999999999998" metricValueMax="2.5" metricValueMin="2.065" pageId="41" pageNumber="342" unit="m" value="2282.5" valueMax="2500.0" valueMin="2065.0">2065-2500 m</elevation>
