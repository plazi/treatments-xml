and 
<taxonomicName id="4C21DFC13E26FFC6FF78F82BFD89F857" authority="Tinkham, 1945" authorityName="Tinkham" authorityYear="1945" box="[199,572,1926,1950]" class="Insecta" family="Phaneropteridae" genus="Sinochlora" kingdom="Animalia" order="Orthoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="szechwanensis">
