<number id="5C1424DFFFE82A11DB43FCBA8E50B348" box="[1330,1383,836,860]" pageId="20" pageNumber="171" value="0.18">0.18</number>
; SL: 
<number id="5C1424DFFFE82A11DEE6FC998BFBB394" box="[151,204,871,896]" pageId="20" pageNumber="171" value="0.62">0.62</number>
; SI: 
<number id="5C1424DFFFE82A11DE8FFC998A2AB394" box="[254,285,871,896]" pageId="20" pageNumber="171" value="59.0">59</number>
