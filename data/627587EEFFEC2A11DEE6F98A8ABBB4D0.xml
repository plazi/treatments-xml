<number id="5C1424DFFFE82A11DEB2FD6E8A1EB2BC" box="[195,297,656,680]" pageId="20" pageNumber="171" value="104101.0">104, 101</number>
; EL: 
<number id="5C1424DFFFE82A11DF19FD718AABB2BC" box="[360,412,655,680]" pageId="20" pageNumber="171" value="0.16">0.16</number>
