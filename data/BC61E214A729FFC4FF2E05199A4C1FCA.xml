<number id="82004125A72BFFC3FC5404FE9ACA18A7" box="[1005,1022,1705,1729]" pageId="3" pageNumber="52" value="6.0">6</number>
: 
<number id="82004125A72BFFC3FBB804FE9D2418A7" box="[1025,1040,1705,1729]" pageId="3" pageNumber="52" value="5.0">5</number>
: 
<number id="82004125A72BFFC3FBAC04FE9D1018A7" box="[1045,1060,1705,1729]" pageId="3" pageNumber="52" value="5.0">5</number>
: 
<number id="82004125A72BFFC3FB9004FE9D0C18A7" box="[1065,1080,1705,1729]" pageId="3" pageNumber="52" value="5.0">5</number>
: 
<number id="82004125A72BFFC3FB8504FE9D6F18A7" box="[1084,1115,1705,1729]" pageId="3" pageNumber="52" value="13.0">13</number>
; pedicel 
<number id="82004125A72BFFC3FB7804FE9DD018A7" box="[1217,1252,1705,1729]" pageId="3" pageNumber="52" value="1.3">1.3</number>
X, anellus 
<number id="82004125A72BFFC3FAE704FE9CB618A7" box="[1374,1410,1705,1729]" pageId="3" pageNumber="52" value="0.3">0.3</number>
X, F 
<number id="82004125A72BFFC3FF13049A99801883" box="[170,180,1741,1765]" pageId="3" pageNumber="52" value="1.0">1</number>
<number id="82004125A72BFFC3FF02049A99EB1883" box="[187,223,1741,1765]" pageId="3" pageNumber="52" value="2.8">2.8</number>
X, F 
<number id="82004125A72BFFC3FEA9049A98291883" box="[272,285,1741,1765]" pageId="3" pageNumber="52" value="2.0">2</number>
<number id="82004125A72BFFC3FE9D049A987D1883" box="[292,329,1741,1765]" pageId="3" pageNumber="52" value="2.2">2.2</number>
X, F 
<number id="82004125A72BFFC3FEC3049A98B21883" box="[378,390,1741,1765]" pageId="3" pageNumber="52" value="3.0">3</number>
<number id="82004125A72BFFC3FE34049A98841883" box="[397,432,1741,1765]" pageId="3" pageNumber="52" value="1.5">1.5</number>
X, F 
<number id="82004125A72BFFC3FE5B049A98C41883" box="[482,496,1741,1765]" pageId="3" pageNumber="52" value="4.0">4</number>
<number id="82004125A72BFFC3FE4F049A9B2D1883" box="[502,537,1741,1765]" pageId="3" pageNumber="52" value="1.5">1.5</number>
X, F 
<number id="82004125A72BFFC3FDF5049A9B6D1883" box="[588,601,1741,1765]" pageId="3" pageNumber="52" value="5.0">5</number>
<number id="82004125A72BFFC3FDE6049A9BB61883" box="[607,642,1741,1765]" pageId="3" pageNumber="52" value="1.3">1.3</number>
X, F 
<number id="82004125A72BFFC3FD0C049A9BF61883" box="[693,706,1741,1765]" pageId="3" pageNumber="52" value="6.0">6</number>
