<collectionCode id="52D9CBC7A72BFFC3FF6604359822181F" LSID="urn:lsid:biocol.org:col:13900" box="[223,278,1634,1657]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:13900" name="Queen Ethelburga's School" pageId="3" pageNumber="52">HTE</collectionCode>
