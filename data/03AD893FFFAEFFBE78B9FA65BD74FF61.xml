<taxonomicName id="4C0443AAFFAEFFBD7CBEF903BA82F968" box="[1168,1279,1697,1720]" class="Arachnida" family="Psoroptidae" genus="Psoroptes" kingdom="Animalia" order="Astigmata" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
