<collectionCode id="96E6A1B1FF42AE433094F402FCD83CF5" box="[726,768,370,393]" pageId="27" pageNumber="1105">HL</collectionCode>
, 1.36–1.54 (1.49); mHW, 1.16–1.31 (1.27); 
<collectionCode id="96E6A1B1FF42AE43336AF4E1FEB13CD4" box="[296,361,401,424]" pageId="27" pageNumber="1105">LML</collectionCode>
, 0.88–0.94 (0.91); 
<collectionCode id="96E6A1B1FF42AE433005F4E1FDB43CD4" box="[583,620,401,424]" country="Czech Republic" name="Západoceské muzeum v Plzni" pageId="27" pageNumber="1105" type="Museum">PL</collectionCode>
