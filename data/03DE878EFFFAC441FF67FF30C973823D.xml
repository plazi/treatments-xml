<taxonomicName id="4C774D1BFFF8C440FD25FA38CF5F85B6" authority="Archibald &amp; Makarkin, 2021" authorityName="Archibald &amp; Makarkin" authorityYear="2021" box="[725,816,1439,1462]" class="Insecta" family="Raphidiidae" genus="Megaraphidia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Raphidioptera" pageId="14" pageNumber="55" phylum="Arthropoda" rank="species" species="ootsa" status="sp. nov.">
