<taxonomicName id="4C1EC3BBEE08FFFFA1ACDE88EC5C10A3" authority="Matile-Ferrero &amp; Le Ruyet" authorityName="Matile-Ferrero &amp; Le Ruyet" box="[665,1108,659,686]" class="Insecta" family="Coccidae" genus="Richardiella" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="59" phylum="Arthropoda" rank="genus">
<emphasis id="B96A642AEE08FFFFA1ACDE88EB1410A0" box="[665,796,659,685]" italics="true" pageId="2" pageNumber="59">Richardiella</emphasis>
