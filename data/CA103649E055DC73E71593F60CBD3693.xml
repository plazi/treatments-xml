<taxonomicName id="85B9FCDCE052DC72E78D919B0DFD34D4" authority="Turkay, 1975" authorityName="Turkay" authorityYear="1975" box="[400,703,1753,1774]" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="44" pageNumber="45" phylum="Arthropoda" rank="species" species="thieli">
