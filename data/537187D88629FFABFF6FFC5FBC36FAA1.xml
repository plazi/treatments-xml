<typeStatus id="0463886C8629FFABFF6FFC5FBF3EFC3F" box="[151,265,984,1010]" pageId="1" pageNumber="494">Holotype</typeStatus>
of 
<taxonomicName id="1CD84D4D8629FFABFEC8FC50BC68FC3F" authority="Franz" authorityName="Franz" box="[304,607,983,1010]" class="Insecta" family="Staphylinidae" genus="Eutheia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="494" phylum="Arthropoda" rank="species" species="siamensis">
<emphasis id="E9ACEADC8629FFABFEC8FC50BC3BFC3C" bold="true" box="[304,524,983,1009]" italics="true" pageId="1" pageNumber="494">Eutheia siamensis</emphasis>
