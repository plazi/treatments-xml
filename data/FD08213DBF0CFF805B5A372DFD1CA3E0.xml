<td id="8840FB15BF0C00765E47327EFBA1A54F" box="[1045,1106,1807,1833]" gridcol="5" gridrow="1" pageId="8" pageNumber="39">(now</td>
<td id="8840FB15BF0C00765E08327EFB40A54F" box="[1114,1203,1807,1833]" gridcol="6" gridrow="1" pageId="8" pageNumber="39">extinct)</td>
<td id="8840FB15BF0C00765EEE327EFB14A54F" box="[1212,1255,1807,1833]" gridcol="7" gridrow="1" pageId="8" pageNumber="39">and</td>
