, 
<taxonomicName id="4C334D69FFCE6162FCA1FEC2FC14FE9E" baseAuthorityName="Dettmann &amp; Clifford" baseAuthorityYear="1992" box="[808,940,343,370]" class="Polypodiopsida" family="Anemiaceae" kingdom="Plantae" order="Schizaeales" pageId="15" pageNumber="102" phylum="Tracheophyta" rank="family">Anemiaceae</taxonomicName>
