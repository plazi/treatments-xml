<paragraph id="FB2675A6CDF5DB39DEF1B7CFC1C061C9" pageId="8" pageNumber="95">
<taxonomicName id="1D4363EE8B689065D7F1FCDCBFC43489" LSID="urn:lsid:zoobank.org:act:BE699A24-2054-47FD-B1F2-D744ABBD67D0" authority="Sasaji, 1967" authorityName="Sasaji" authorityYear="1967" class="Insecta" family="Coccinellidae" genus="Shirozuella" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Shirozuella tibetina" order="Coleoptera" pageId="8" pageNumber="95" phylum="Arthropoda" rank="species" species="tibetina">Shirozuella tibetina</taxonomicName>
