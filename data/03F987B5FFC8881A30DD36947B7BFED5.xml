<taxonomicName id="4C504D20FFC9881A320234AC7CE2FF41" authority="L. The" authorityName="L. The" authorityYear="1990" box="[584,898,189,215]" class="Magnoliopsida" family="Fabaceae" genus="Hymenaea" kingdom="Plantae" order="Fabales" pageId="61" pageNumber="62" phylum="Tracheophyta" rank="species" species="courbaril">
