<taxonomicName id="4C44F6F0AC146146FF72F3C7B750FF15" box="[205,372,190,216]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="13" pageNumber="46" phylum="Tracheophyta" rank="family">Brassicaceae</taxonomicName>
), 
<taxonomicName id="4C44F6F0AC146146FE31F3C6B47AFF15" box="[398,606,191,216]" class="Magnoliopsida" family="Verbenaceae" genus="Lantana" kingdom="Plantae" order="Lamiales" pageId="13" pageNumber="46" phylum="Tracheophyta" rank="species" species="camara">
<emphasis id="B9305161AC146146FE31F3C6B47AFF15" box="[398,606,191,216]" italics="true" pageId="13" pageNumber="46">Lantana camara</emphasis>
</taxonomicName>
(
<taxonomicName id="4C44F6F0AC146146FDCFF3C7B533FF15" box="[624,791,190,216]" class="Magnoliopsida" family="Verbenaceae" kingdom="Plantae" order="Lamiales" pageId="13" pageNumber="46" phylum="Tracheophyta" rank="family">Verbenaceae</taxonomicName>
