<taxonomicName id="4C44F6F0AC146146FB87F3C7B2E9FF15" box="[1080,1229,190,216]" class="Magnoliopsida" family="Solanaceae" kingdom="Plantae" order="Solanales" pageId="13" pageNumber="46" phylum="Tracheophyta" rank="family">Solanaceae</taxonomicName>
