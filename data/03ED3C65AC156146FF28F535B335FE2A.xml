<taxonomicName id="4C44F6F0AC146146FE99F39CB414FF33" box="[294,560,229,254]" class="Magnoliopsida" family="Caryophyllaceae" genus="Dianthus" kingdom="Plantae" order="Caryophyllales" pageId="13" pageNumber="46" phylum="Tracheophyta" rank="species" species="caryophyllus">
