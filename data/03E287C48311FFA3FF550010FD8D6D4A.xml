<taxonomicName id="4C4B4D518316FFA2FF550639FCF16F0B" authority="Malicky &amp; Chantaramongkol 1997" authorityName="Malicky &amp; Chantaramongkol" authorityYear="1997" box="[151,805,260,285]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" kingdom="Animalia" order="Trichoptera" pageId="4" pageNumber="411" phylum="Arthropoda" rank="species" species="ceres">
