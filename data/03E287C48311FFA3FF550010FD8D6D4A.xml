: 
<collectingRegion id="498FF8308317FFA3FE6D0479FDD06D4A" box="[431,516,836,861]" country="India" name="Sikkim" pageId="5" pageNumber="412">Sikkim</collectingRegion>
. 
<collectingCountry id="F35C76428317FFA3FDCC0479FD816D4A" box="[526,597,836,861]" name="China" pageId="5" pageNumber="412">China</collectingCountry>
