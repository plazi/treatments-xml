<subSubSection id="53F7650FC56F750EFF6AFCA878F3FCF7" box="[199,1168,790,817]" pageId="7" pageNumber="280" type="etymology">
<paragraph id="1B523684C56F750EFF6AFCA878F3FCF7" blockId="7.[151,1437,151,817]" box="[199,1168,790,817]" pageId="7" pageNumber="280">
<emphasis id="2999EA96C56F750EFF6AFCA87D2BFCF7" bold="true" box="[199,328,790,816]" pageId="7" pageNumber="280">Etymology</emphasis>
