<quantity id="4CCB9B36FFB15B614CF3C87BC8CC217C" box="[686,752,536,560]" metricMagnitude="-3" metricUnit="m" metricValue="6.0" pageId="3" pageNumber="722" unit="mm" value="6.0">6 mm</quantity>
