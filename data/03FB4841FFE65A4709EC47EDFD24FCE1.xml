<emphasis id="B9262545FFE75A4708A2424FFD24FCE1" box="[473,679,837,863]" italics="true" pageId="1" pageNumber="294">
<taxonomicName id="4C5282D4FFE75A4708A2424FFDB8FCE1" baseAuthorityName="Linnavuori" baseAuthorityYear="1978" box="[473,571,837,863]" class="Insecta" family="Rhyparochromidae" genus="Stalaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="294" phylum="Arthropoda" rank="species" species="nysias" status="syn. nov.">S. nysias</taxonomicName>
