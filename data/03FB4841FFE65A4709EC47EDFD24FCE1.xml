<taxonomicName id="4C5282D4FFE75A470BFC432FFD6DFD81" authorityName="Slater &amp; Zheng" authorityYear="1985" box="[647,750,549,575]" class="Insecta" family="Rhyparochromidae" genus="Stalaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="294" phylum="Arthropoda" rank="species" species="lestoni">
<emphasis id="B9262545FFE75A470BFC432FFD6DFD81" box="[647,750,549,575]" italics="true" pageId="1" pageNumber="294">S. lestoni</emphasis>
</taxonomicName>
