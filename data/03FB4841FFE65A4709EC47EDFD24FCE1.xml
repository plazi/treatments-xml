<taxonomicName id="4C5282D4FFE75A470A0A4367FC55FD39" baseAuthorityName="Linnavuori" baseAuthorityYear="1978" box="[881,982,621,647]" class="Insecta" family="Rhyparochromidae" genus="Stalaria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="294" phylum="Arthropoda" rank="species" species="kisseis">
