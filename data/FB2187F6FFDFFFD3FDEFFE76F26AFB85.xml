<emphasis id="41FCEAF2FFDFFFD3FF51FC7CF76DFC25" box="[169,372,942,965]" italics="true" pageId="13" pageNumber="252">Material examined</emphasis>
</paragraph>
<paragraph id="733736E0FFDFFFD3FF31FC1BF4ACFC1A" blockId="13.[169,1154,749,1125]" pageId="13" pageNumber="252">
<materialsCitation id="C3E03CBDFFDFFFD3FF31FC1BF4ACFC1A" ID-GBIF-Occurrence="2564253661" collectionCode="BMNH" collectorName="W. C. Block &amp; Block, B. M." country="Uganda" latitude="0.48333332" location="Katabana" longLatPrecision="1308" longitude="32.616665" pageId="13" pageNumber="252" specimenCount="1" stateProvince="Kampala" typeStatus="holotype">
<typeStatus id="AC338842FFDFFFD3FF31FC1BF733FC3F" box="[201,298,969,991]" pageId="13" pageNumber="252">Holotype</typeStatus>
, 
<specimenCount id="658EFD69FFDFFFD3FECDFC1BF751FC3F" box="[309,328,969,991]" pageId="13" pageNumber="252">♀</specimenCount>
, 
