<location id="A630606D9E40FF88FD5A760DFCDE1460" LSID="urn:lsid:plazi:treatment:2B4687A09E40FF89FF0971D6FE0C1462:A630606D9E40FF88FD5A760DFCDE1460" box="[745,808,535,557]" county="Type'" municipality="Mearns. He" name="Allen" pageId="68" pageNumber="68" stateProvince="Arizona">Allen</location>
