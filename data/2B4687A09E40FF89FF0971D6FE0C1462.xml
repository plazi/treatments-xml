<collectionCode id="C5FEAE739E40FF88FD287648FD0E142A" box="[667,760,594,615]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" lsid="urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="68" pageNumber="68">AMNH</collectionCode>
