<typeStatus id="C40F88291F7AFFA96B684041C3F1F41C" box="[113,169,1291,1314]" pageId="3" pageNumber="96">Type</typeStatus>
species.
</emphasis>
<taxonomicName id="DCB44D081F7AFFA96A154041C15FF41C" authority="Geiser, 2017" authorityName="Geiser" authorityYear="2017" box="[268,519,1291,1315]" class="Insecta" family="Cantharidae" genus="Socotrasilis" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="96" phylum="Arthropoda" rank="species" species="socotrensis" status="sp. nov.">
