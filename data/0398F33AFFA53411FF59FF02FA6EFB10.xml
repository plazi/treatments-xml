<subSubSection id="C32B11A7FFA53411FF09FB5BFA6EFB10" box="[199,1437,1230,1255]" pageId="13" pageNumber="352" type="etymology">
<paragraph id="8B8E422CFFA53411FF09FB5BFA6EFB10" blockId="13.[151,1437,258,1255]" box="[199,1437,1230,1255]" pageId="13" pageNumber="352">
<emphasis id="B9459E3EFFA53411FF09FB5BFEBDFB10" bold="true" box="[199,334,1230,1255]" pageId="13" pageNumber="352">Etymology.</emphasis>
