<taxonomicName id="C397AFA2FFFBFFDAFDB68C93DE6BAFA8" authority="Fabricius, 1794" authorityName="Fabricius" authorityYear="1794" box="[633,1029,188,212]" class="Insecta" family="Issidae" genus="Falcidius" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="30" phylum="Arthropoda" rank="species" species="apterus">
