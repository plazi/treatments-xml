<number id="B25FC606FFFBFFDAFB658CB8DE81AFD3" box="[1194,1263,151,175]" pageId="6" pageNumber="30" value="22.5" valueMax="26.0" valueMin="19.0">19-26</number>
). 
<taxonomicName id="C397AFA2FFFBFFDAFACA8CB6DFF2AFCC" box="[1285,1436,152,176]" class="Insecta" family="Issidae" genus="Hysteropterum" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="30" phylum="Arthropoda" rank="species" species="striolatum">
