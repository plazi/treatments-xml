<number id="B25FC606FFFBFFDAFD328CB8D956AFD3" box="[765,824,151,175]" pageId="6" pageNumber="30" value="1979.0">1979</number>
</bibRefCitation>
, figs 
<number id="B25FC606FFFBFFDAFCB78CB8D9F1AFD3" box="[888,927,151,175]" pageId="6" pageNumber="30" value="5.0" valueMax="9.0" valueMin="1.0">1-9</number>
; 
<bibRefCitation id="6006A9D0FFFBFFDAFC628CB8DE05AFD3" author="Gnezdilov" box="[941,1131,151,175]" pageId="6" pageNumber="30" refString="Gnezdilov, V. M. (2002) New species of the genus Tshurtshurnella Kusnezov, 1927 (Homoptera, Cicadina, Issidae) from Turkey and Lebanon. Russian Entomological Journal, 11 (3), 233 - 240." type="journal article" year="2002">
Gnezdilov, 
<number id="B25FC606FFFBFFDAFBFF8CB8DE05AFD3" box="[1072,1131,151,175]" pageId="6" pageNumber="30" value="2002.0">2002</number>
</bibRefCitation>
