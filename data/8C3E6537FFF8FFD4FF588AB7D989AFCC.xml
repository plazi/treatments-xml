<taxonomicName id="C397AFA2FFF8FFD9FDFD8BE4DE75A89F" authority="Kirshbaum, 1868" authorityName="Kirshbaum" authorityYear="1868" box="[562,1051,1995,2019]" class="Insecta" family="Issidae" genus="Tshurtshurnella" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="29" phylum="Arthropoda" rank="species" species="zelleri">
