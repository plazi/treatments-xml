<taxonomicName id="97579C429EAC574114A98900A4E02CDF" LSID-ZBK="urn:lsid:zoobank.org:act:1C6AC061-0134-487B-9102-EFF719A47063" class="Actinopterygii" family="Sisoridae" genus="Pareuchiloglanis" kingdom="Animalia" lsidName-ZBK="Pareuchiloglanis Pellegrin 1936:245" order="Siluriformes" pageNumber="67" phylum="Chordata" rank="genus">Pareuchiloglanis</taxonomicName>
