<taxonomicName id="518B0627D56BD77D0E72DD6C4FC21D80" LSID-ZBK="urn:lsid:zoobank.org:act:F34DE64E-5530-4CC0-A5C5-3BD58430405D" class="Actinopterygii" family="Sisoridae" genus="Glyptosternon" kingdom="Animalia" lsidName-ZBK="Glyptosternon McClelland 1842:584" order="Siluriformes" pageNumber="67" phylum="Chordata" rank="genus">Glyptosternon</taxonomicName>
