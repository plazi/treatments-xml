<taxonomicName id="01E691AAFFBB8718FAB1C924FE39F42A" authority="Panzer. Comment" authorityName="Panzer. Comment" class="Magnoliopsida" family="Apocynaceae" genus="Anisotoma" kingdom="Plantae" order="Gentianales" pageId="64" pageNumber="65" phylum="Tracheophyta" rank="genus">
