<collectionCode id="EFD8B1C5AC1F3B78FCCDDB855F6CFC75" box="[796,815,960,980]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="83" pageNumber="476" type="Museum">V</collectionCode>
