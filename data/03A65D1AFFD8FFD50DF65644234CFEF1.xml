<taxonomicName id="4C0F978FFFD8FFD20C7656442052F994" authority="Muller, 1843" authorityName="Muller" authorityYear="1843" box="[279,585,1630,1656]" class="Actinopterygii" family="Exocoetidae" genus="Cheilopogon" kingdom="Animalia" order="Beloniformes" pageId="35" pageNumber="36" phylum="Chordata" rank="subGenus" subGenus="Ptenichthys">
