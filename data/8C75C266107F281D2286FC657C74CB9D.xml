</subSubSection>
<subSubSection id="4CC620FB107F281C2286FBD77D55CC1C" box="[151,809,1075,1100]" pageId="86" pageNumber="87" type="materials_examined">
<paragraph id="04637370107F281C2286FBD77D55CC1C" blockId="86.[151,1437,1075,2036]" box="[151,809,1075,1100]" pageId="86" pageNumber="87">
