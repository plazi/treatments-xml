<paragraph id="8BBE36F1CC0DFFFAFCAE818698E2F928" blockId="6.[826,1220,1727,1749]" box="[826,1220,1727,1749]" pageId="6" pageNumber="550">
<emphasis id="B975EAE3CC0DFFFAFCAE8186987BF928" box="[826,1117,1727,1748]" italics="true" pageId="6" pageNumber="550">Description: Body length:</emphasis>
<quantity id="4CF99B14CC0DFFFAFBF381869899F928" box="[1127,1215,1727,1749]" metricMagnitude="-3" metricUnit="m" metricValue="9.6" pageId="6" pageNumber="550" unit="mm" value="9.6">9.6 mm</quantity>
