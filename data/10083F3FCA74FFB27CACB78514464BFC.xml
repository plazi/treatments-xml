<number id="2E699C0ECA74FFB07E90B30213954D4C" box="[683,715,1890,1916]" pageId="5" pageNumber="48" value="38.0">38</number>
°06.5’S
</geoCoordinate>
, 
<geoCoordinate id="FD95E8EECA74FFB07F0BB30212E74D4C" box="[816,953,1890,1916]" direction="east" orientation="longitude" pageId="5" pageNumber="48" precision="92" value="144.28833">
<number id="2E699C0ECA74FFB07F0BB30212014D4C" box="[816,863,1890,1916]" pageId="5" pageNumber="48" value="144.0">144</number>
° 
<number id="2E699C0ECA74FFB07F52B30212FE4D4C" box="[873,928,1890,1916]" pageId="5" pageNumber="48" value="17.3">17.3</number>
’E
