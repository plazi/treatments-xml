<taxonomicName id="748559E0FFED5F58BD34CA2DFE71123F" box="[334,500,341,366]" class="Insecta" family="Apidae" genus="Ammobatoides" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="447" phylum="Arthropoda" rank="genus">
