<subSubSection id="8E72B8768C52FF9680B9D618E5B1AAAB" pageId="0" pageNumber="5" type="nomenclature">
<paragraph id="2B5C3122DB2E91F8C41E9993BD7D906D" pageId="0" pageNumber="5">
<taxonomicName id="7A4B9F7D9E0E8FEBC45533B46739C10F" LSID="http://zoobank.org/80494C1F-0D6A-4474-8A0C-B27312E5587D" authority="Awad" authorityName="Awad" authorityYear="2021" class="Insecta" family="Platygastridae" genus="Synopeas" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Synopeas occultum" order="Hymenoptera" pageId="0" pageNumber="5" phylum="Arthropoda" rank="species" species="occultum" status="sp. nov.">Synopeas occultum Awad</taxonomicName>
