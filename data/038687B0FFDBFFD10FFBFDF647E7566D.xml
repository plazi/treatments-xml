<taxonomicName id="4C2F4D25FFDBFFD10C30FC4944A451D5" box="[860,941,931,955]" class="Insecta" family="Drilidae" genus="Selasia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="567" phylum="Arthropoda" rank="genus">
