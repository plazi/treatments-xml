<taxonomicName id="4C2F4D25FFDBFFD10BB4FC2C422A51B1" box="[1240,1315,966,991]" class="Insecta" family="Elateridae" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="567" phylum="Arthropoda" rank="tribe" tribe="Drilini">Drilini</taxonomicName>
