<taxonomicName id="89BF4D0F4B78C06FFDE0FE00167AFE50" authority="Monostori 1991" authorityName="Monostori" authorityYear="1991" box="[636,1077,439,465]" class="Ostracoda" family="Cylindroleberididae" genus="Triadogigantocypris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="14" pageNumber="29" phylum="Arthropoda" rank="genus">
