<taxonomicName id="89BF4D0F4B78C06FFC4BFDB01769FDA0" authority="Kornicker 1958" authorityName="Kornicker" authorityYear="1958" box="[983,1318,519,545]" class="Ostracoda" family="Cylindroleberididae" genus="Actinoseta" kingdom="Animalia" order="Myodocopida" pageId="14" pageNumber="29" phylum="Arthropoda" rank="genus">
<emphasis id="7CCBEA9E4B78C06FFC4BFDB0161CFDA1" box="[983,1107,519,544]" italics="true" pageId="14" pageNumber="29">Actinoseta</emphasis>
<bibRefCitation id="2A2E4B7D4B78C06FFBFEFDB01769FDA0" author="Kornicker" box="[1122,1318,519,545]" lastPageId="14" lastPageNumber="52" pageId="14" pageNumber="51" refString="Kornicker, L. S. (1958) Ecology and taxonomy of Recent marine ostracodes in the Bimini area, Great Bahama Bank. Publications of the Institute of Marine Science, (The University of Texas), 5, 194 - 300." type="journal article" year="1958">
