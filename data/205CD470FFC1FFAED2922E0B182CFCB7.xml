<paragraph id="A84A6566FFC1FFAED2922EDB1ED9FD33" blockId="49.[178,1085,640,684]" pageId="49" pageNumber="1726">
<collectingRegion id="6A31AB84FFC1FFAED2922EDB1D0BFD0B" box="[178,262,640,660]" country="Austria" name="Salzburg" pageId="49" pageNumber="1726">Salzburg</collectingRegion>
