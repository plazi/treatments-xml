<quantity id="6F0DC883FFC1FFAED3A32EC31DB2FD33" box="[387,447,664,684]" metricMagnitude="2" metricUnit="m" metricValue="4.35" pageId="49" pageNumber="1726" unit="m" value="435.0">435 m</quantity>
