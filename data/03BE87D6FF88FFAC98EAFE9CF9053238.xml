<quantity id="4CEF9B25FF88FFAC9C05FE0DFEBE321C" box="[1144,1279,511,537]" metricMagnitude="2" metricUnit="m" metricValue="5.665" metricValueMax="11.1" metricValueMin="0.22999999999999998" pageId="25" pageNumber="26" unit="m" value="566.5" valueMax="1110.0" valueMin="23.0">23–1,110 m</quantity>
