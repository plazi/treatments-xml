<taxonomicName id="55E2C6EBFFE4FF98FD9DFAA3FD5827BF" box="[551,675,1383,1409]" class="Insecta" family="Curculionidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="family">Scolytidae</taxonomicName>
