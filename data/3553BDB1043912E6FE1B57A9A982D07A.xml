<taxonomicName id="2E5B0E6DACC75737117467A3D201F1A3" class="Pinopsida" family="Pinaceae" genus="Picea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Picea glauca" order="Pinales" pageId="33" pageNumber="64" phylum="Tracheophyta" rank="species" species="glauca">Picea glauca</taxonomicName>
