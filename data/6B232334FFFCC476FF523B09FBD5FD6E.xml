<collectingCountry id="9B9DD2B2FFFEC476FE9D3E8CFE2AFF55" box="[344,445,188,212]" name="United States of America" pageId="7" pageNumber="578">America</collectingCountry>
: 
<taxonomicName id="248AE9A1FFFEC476FE153E8DFD61FF55" authority="Martins, 1975" authorityName="Martins" authorityYear="1975" box="[464,758,188,212]" class="Insecta" family="Cerambycidae" genus="Euderces" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="578" phylum="Arthropoda" rank="species" species="dilutus">
<emphasis id="D1FE4E30FFFEC476FE153E8DFDD4FF55" box="[464,579,188,212]" italics="true" pageId="7" pageNumber="578">E. dilutus</emphasis>
Martins, 
<number id="55428005FFFEC476FD7B3E8CFD61FF55" box="[702,758,188,212]" pageId="7" pageNumber="578" value="1975.0">1975</number>
</taxonomicName>
