<collectingCountry id="9B9DD2B2FFFEC476FCC83E8CFCF2FF55" box="[781,869,188,212]" name="Bolivia" pageId="7" pageNumber="578">Bolivia</collectingCountry>
), 
<taxonomicName id="248AE9A1FFFEC476FC443E8DFB49FF55" authority="Chevrolat, 1862" authorityName="Chevrolat" authorityYear="1862" box="[897,1246,188,212]" class="Insecta" family="Cerambycidae" genus="Euderces" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="578" phylum="Arthropoda" rank="species" species="guerinii">
<emphasis id="D1FE4E30FFFEC476FC443E8DFB97FF55" box="[897,1024,189,212]" italics="true" pageId="7" pageNumber="578">E. guerinii</emphasis>
(Chevrolat, 
<number id="55428005FFFEC476FB583E8CFB41FF55" box="[1181,1238,188,212]" pageId="7" pageNumber="578" value="1862.0">1862</number>
)
</taxonomicName>
(
<collectingCountry id="9B9DD2B2FFFEC476FB313E8CFAFFFF55" box="[1268,1384,188,212]" name="Colombia" pageId="7" pageNumber="578">Colombia</collectingCountry>
), 
<taxonomicName id="248AE9A1FFFEC476FA413E8DFDE3FF76" authority="Giesbert &amp; Chemsak, 1997" authorityName="Giesbert &amp; Chemsak" authorityYear="1997" class="Insecta" family="Cerambycidae" genus="Euderces" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="578" phylum="Arthropoda" rank="species" species="paraposticus">
<emphasis id="D1FE4E30FFFEC476FA413E8DFEBFFF79" italics="true" pageId="7" pageNumber="578">E. paraposticus</emphasis>
