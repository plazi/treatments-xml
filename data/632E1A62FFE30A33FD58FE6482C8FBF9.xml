<taxonomicName id="2C87D0F7FFED0A30FF48F9908287F9E3" authority="Couvreur" authorityName="Couvreur" box="[194,460,1634,1652]" class="Magnoliopsida" family="Annonaceae" genus="Monodora" kingdom="Plantae" order="Magnoliales" pageId="9" pageNumber="252" phylum="Tracheophyta" rank="species" species="hastipetala">
<emphasis id="D9F37766FFED0A30FF48F990823CF9E4" box="[194,375,1635,1652]" italics="true" pageId="9" pageNumber="252">Monodora hastipetala</emphasis>
Couvreur
</taxonomicName>
: 
<emphasis id="D9F37766FFED0A30FE59F99082ABF9E4" bold="true" box="[467,480,1635,1652]" pageId="9" pageNumber="252">A</emphasis>
, flowering branch; 
<emphasis id="D9F37766FFED0A30FD0AF99081C6F9E4" bold="true" box="[640,653,1635,1652]" pageId="9" pageNumber="252">B</emphasis>
, mature flower (top view), outer petals opened; 
<emphasis id="D9F37766FFED0A30FB97F9908760F9E4" bold="true" box="[1053,1067,1635,1652]" pageId="9" pageNumber="252">C</emphasis>
, pedicel with sepals; 
<emphasis id="D9F37766FFED0A30FF14F98A83E7F91A" bold="true" box="[158,172,1657,1674]" pageId="9" pageNumber="252">D</emphasis>
, androecium and gynoecium; 
<emphasis id="D9F37766FFED0A30FE22F98A82FFF91A" bold="true" box="[424,436,1657,1674]" pageId="9" pageNumber="252">E</emphasis>
, stamen; 
<emphasis id="D9F37766FFED0A30FD8CF98A815AF91A" bold="true" box="[518,529,1657,1674]" pageId="9" pageNumber="252">F</emphasis>
