<taxonomicName id="4C1D4D068B306E1CFB50E15AFA0B910C" authority="(Chant)" authorityName="Chant" baseAuthorityName="Chant" baseAuthorityYear="1959" box="[1190,1433,1086,1113]" class="Arachnida" family="Phytoseiidae" genus="Amblyseius" kingdom="Animalia" order="Mesostigmata" pageId="2" pageNumber="529" phylum="Arthropoda" rank="species" species="herbicolus">
