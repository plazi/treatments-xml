<quantity id="5A261FAD80AEFF0DEE94C8C51D3F0978" box="[1023,1122,1772,1796]" metricMagnitude="1" metricUnit="m" metricValue="4.5" metricValueMax="8.0" metricValueMin="1.0" pageId="170" pageNumber="171" unit="m" value="45.0" valueMax="80.0" valueMin="10.0">10–80 m</quantity>
