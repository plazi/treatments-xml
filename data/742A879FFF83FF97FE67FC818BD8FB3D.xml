<taxonomicName id="3B834D0AFF87FF94FF51F9A98852F999" authority="Breuning, 1950" authorityName="Breuning" authorityYear="1950" box="[241,635,1590,1613]" class="Insecta" family="Cerambycidae" genus="Glenea" kingdom="Animalia" order="Coleoptera" pageId="37" pageNumber="242" phylum="Arthropoda" rank="species" species="albosignatipennis">
