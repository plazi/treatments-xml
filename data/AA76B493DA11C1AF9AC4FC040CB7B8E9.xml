<taxonomicName id="EABD93D1B13568FE159C26BD0C28E45E" LSID="http://zoobank.org/BC60CF21-AF03-4025-B272-251827BA2B87" authority="Cashatt" authorityName="Cashatt" authorityYear="2013" class="Hexapoda" family="Pyralidae" genus="Arta" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Arta brevivalvalis" order="Lepidoptera" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="brevivalvalis" status="sp. n.">Arta brevivalvalis Cashatt</taxonomicName>
<taxonomicNameLabel id="99323DD1A2BBD7E7F53CBDCB4268E8E0" pageId="4" pageNumber="59">sp. n.</taxonomicNameLabel>
