<taxonomicName id="0CAE4D55A817FFC8FD68FD03FB750BF2" authority="Dwight, 1918" authorityName="Dwight" authorityYear="1918" box="[765,1224,724,744]" class="Aves" family="Emberizidae" genus="Junco" kingdom="Animalia" order="Passeriformes" pageId="15" pageNumber="15" phylum="Chordata" rank="subSpecies" species="hyemalis" subSpecies="cismontanus">
