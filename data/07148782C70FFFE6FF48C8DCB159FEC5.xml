<paragraph id="8F023694C70FFFE1FF18CF07B69EF861" pageId="3" pageNumber="60">
<table id="FDBDC434C70F001DFF48CF07B69EF861" box="[151,1436,1868,2014]" gridcols="3" gridrows="4" pageId="3" pageNumber="60">
<tr id="318D34D6C70F001DFF48CF07B69EF8D9" box="[151,1436,1868,1894]" gridrow="0" pageId="3" pageNumber="60" rowspan-1="1" rowspan-2="1">
