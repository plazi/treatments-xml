<taxonomicName id="6DFD27A9BF54FFE85A14FDD334B84922" box="[363,470,564,590]" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="57" pageNumber="58" phylum="Tracheophyta" rank="species" species="undetermined">
