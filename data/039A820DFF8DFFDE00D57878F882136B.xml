<geoCoordinate id="EE0755DCFF8DFFDE038278B8FF6C10AB" box="[916,1068,1442,1466]" degrees="63.23713" direction="west" orientation="longitude" pageId="5" pageNumber="4" precision="1" value="-63.23713">63.23713°W</geoCoordinate>
