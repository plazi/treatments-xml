(
<geoCoordinate id="EE0755DCFF8DFFDE02E278BBF8C710A8" box="[756,903,1441,1465]" degrees="17.62452" direction="north" orientation="latitude" pageId="5" pageNumber="4" precision="1" value="17.62452">17.62452°N</geoCoordinate>
