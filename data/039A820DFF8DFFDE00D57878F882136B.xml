<quantity id="4CCB9EFEFF8DFFDE04CF78B8FE4F10AB" box="[1241,1295,1442,1466]" metricMagnitude="1" metricUnit="m" metricValue="2.0" pageId="5" pageNumber="4" unit="m" value="20.0">20m</quantity>
