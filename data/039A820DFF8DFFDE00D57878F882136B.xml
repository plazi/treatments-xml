<quantity id="4CCB9EFEFF8DFFDE049378B8FF8C10AB" box="[1157,1228,1442,1466]" metricMagnitude="2" metricUnit="m" metricValue="3.28" pageId="5" pageNumber="4" unit="m" value="328.0">328m</quantity>
