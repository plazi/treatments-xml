<emphasis id="69EBEAA8FF83E262FDB4D8D5FD4CF9AD" box="[586,683,1532,1554]" italics="true" pageId="3" pageNumber="18">enarotadi</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel id="72D857D3FF83E262FD46D8D4FCEFF9AD" box="[696,776,1533,1554]" pageId="3" pageNumber="18" rank="species">
