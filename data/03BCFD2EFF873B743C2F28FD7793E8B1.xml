<taxonomicName id="4C1537BBFF873B703F592A667721EE1F" baseAuthorityName="Gmelin" baseAuthorityYear="1788" box="[638,800,1847,1872]" class="Aves" family="Ramphastidae" genus="Lybius" kingdom="Animalia" order="Piciformes" pageId="2" pageNumber="61" phylum="Chordata" rank="species" species="dubius">
