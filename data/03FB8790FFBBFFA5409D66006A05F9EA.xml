<subSubSection id="C348650DFFBBFFA5410466D0682CFC0A" box="[113,800,905,928]" pageId="1" pageNumber="6" type="type_taxon">
<paragraph id="8BED3686FFBBFFA5410466D0682CFC0A" blockId="1.[113,1089,905,1600]" box="[113,800,905,928]" pageId="1" pageNumber="6">
<emphasis id="B926EA94FFBBFFA5410466D06A09FC0A" bold="true" box="[113,261,905,928]" pageId="1" pageNumber="6">
<typeStatus id="54E98824FFBBFFA5410466D06BA5FC0A" box="[113,169,905,928]" pageId="1" pageNumber="6">Type</typeStatus>
