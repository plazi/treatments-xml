<taxonomicName id="4C614D29FFD4FFA1FC84F849FCCEF817" authorityName="Von Heyden" authorityYear="1826" box="[780,867,1988,2014]" class="Arachnida" family="Cunaxidae" genus="Cunaxa" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="542" phylum="Arthropoda" rank="genus">
<emphasis id="B915EAB8FFD4FFA1FC84F849FCCEF817" box="[780,867,1988,2014]" italics="true" pageId="1" pageNumber="542">Cunaxa</emphasis>
</taxonomicName>
.
