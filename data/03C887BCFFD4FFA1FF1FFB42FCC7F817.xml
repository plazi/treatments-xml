<taxonomicName id="4C614D29FFD4FFA1FCFCF812FC51F870" baseAuthorityName="Den Heyer" baseAuthorityYear="2011" box="[884,1020,1951,1977]" class="Arachnida" family="Cunaxidae" genus="Rubroscirus" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="542" phylum="Arthropoda" rank="genus">
