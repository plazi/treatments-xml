<taxonomicName id="4C614D29FFD4FFA1FDE3F8F1FBECF85C" authority="(Muhammad &amp; Chaudri, 1993)" baseAuthorityName="Muhammad &amp; Chaudri" baseAuthorityYear="1993" box="[619,1089,1915,1942]" class="Arachnida" family="Cunaxidae" genus="Cunaxa" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="542" phylum="Arthropoda" rank="species" species="rasile">
