<taxonomicName id="4C614D29FFD4FFA1FBF7F8F1FE1FF870" authority="(Muhammad, Chaudri &amp; Akbar, 1989)" baseAuthorityName="Muhammad, Chaudri &amp; Akbar" baseAuthorityYear="1989" class="Arachnida" family="Cunaxidae" genus="Cunaxa" kingdom="Animalia" order="Trombidiformes" pageId="1" pageNumber="542" phylum="Arthropoda" rank="species" species="valentis">
