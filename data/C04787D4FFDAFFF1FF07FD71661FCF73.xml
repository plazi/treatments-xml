<taxonomicName id="8FEE4D41FFDAFFF1FF67FD7167C1C9B7" authority="Schulze, 1930" authorityName="Schulze" authorityYear="1930" box="[247,642,726,753]" class="Arachnida" family="Ixodidae" genus="Ixodes" kingdom="Animalia" order="Ixodida" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="nuttallianus">
<emphasis id="7A9AEAD0FFDAFFF1FF67FD716494C9B6" bold="true" box="[247,471,726,753]" italics="true" pageId="51" pageNumber="52">Ixodes nuttallianus</emphasis>
<bibRefCitation id="2C7F4B33FFDAFFF1FE4FFD7167C1C9B7" author="Schulze, P." box="[479,642,726,752]" pageId="51" pageNumber="52" pagination="294 - 303" refId="ref244388" refString="Schulze, P. (1930) Uber einige Verwandte von Ixodes ricinus L. aus Ostasien. Zoologischer Anzeiger, 90, 294 - 303." type="journal article" year="1930">Schulze, 1930</bibRefCitation>
