<emphasis id="CF8C52457B6EFFC9FF25EE0E91D9F9B9" box="[171,244,1575,1601]" italics="true" pageId="31" pageNumber="32">frigida</emphasis>
</taxonomicName>
, and 
<taxonomicName id="3AF8F5D47B6EFFC9FEBBEE0E90D1F9BA" authority="Hill" authorityName="Hill" authorityYear="1927" box="[309,508,1575,1602]" class="Insecta" family="Noctuidae" genus="Lasionycta" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="benjamini">
<emphasis id="CF8C52457B6EFFC9FEBBEE0E9068F9B8" box="[309,325,1575,1600]" italics="true" pageId="31" pageNumber="32">L</emphasis>
