<taxonomicNameLabel id="D4BFEF3E7B6EFFC9FB83EE2D9563F9E6" box="[1037,1102,1540,1566]" pageId="31" pageNumber="32" rank="species">sp. n.</taxonomicNameLabel>
, 
<taxonomicName id="3AF8F5D47B6EFFC9FF02EE0E91D9F9B9" authorityName="Crabo &amp; Lafontaine" authorityYear="2009" box="[140,244,1575,1601]" class="Insecta" family="Noctuidae" genus="Lasionycta" kingdom="Animalia" order="Lepidoptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="frigida">
<emphasis id="CF8C52457B6EFFC9FF02EE0E91B1F9B8" box="[140,156,1575,1600]" italics="true" pageId="31" pageNumber="32">L</emphasis>
