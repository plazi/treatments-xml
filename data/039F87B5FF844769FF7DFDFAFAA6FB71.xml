, 
<collectingCountry id="F3217633FF844768FE5DF98AFDE7F9C2" box="[466,557,1618,1641]" name="Vanuatu" pageId="64" pageNumber="163">Vanuatu</collectingCountry>
, 
<collectingCountry id="F3217633FF844768FDB8F98AFD14F9C1" box="[567,734,1618,1642]" name="New Caledonia" pageId="64" pageNumber="163">New Caledonia</collectingCountry>
