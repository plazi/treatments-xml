<subSubSection id="C36C6541577C8032FF3949E4FA87FF1B" box="[197,1357,188,213]" pageId="1" pageNumber="37" type="etymology">
<paragraph id="8BC936CA577C8032FF3949E4FA87FF1B" blockId="1.[197,1357,151,213]" box="[197,1357,188,213]" pageId="1" pageNumber="37">
<emphasis id="B902EAD8577C8032FF3949E4FE81FF1B" bold="true" box="[197,331,188,212]" pageId="1" pageNumber="37">Etymology.</emphasis>
