<taxonomicName id="4C764D49577D8033FF484F7DFDE7F9F0" authority="Shi, Bian &amp; Chang" authorityName="Shi, Bian &amp; Chang" box="[180,557,1573,1599]" class="Insecta" family="Tettigoniidae" genus="Sinodecma" kingdom="Animalia" order="Orthoptera" pageId="0" pageNumber="36" phylum="Arthropoda" rank="genus">
<emphasis id="B902EAD8577D8033FF484F7DFEF3F9F0" bold="true" box="[180,313,1573,1599]" italics="true" pageId="0" pageNumber="36">Sinodecma</emphasis>
