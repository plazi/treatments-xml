<taxonomicName id="03C8C4D3E46EFF82FA19FAA90C92B6F4" authority="(Bigot, 1884 a)" authorityName="Bigot" authorityYear="1884" baseAuthorityName="Bigot" baseAuthorityYear="1884" class="Insecta" family="Syrphidae" genus="Ocyptamus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="luctuosus">
