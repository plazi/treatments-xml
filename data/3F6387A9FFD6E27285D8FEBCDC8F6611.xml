to 
<geoCoordinate id="D2FE5078FFD6E2728044FD43D8566781" box="[1291,1368,656,680]" direction="south" orientation="latitude" pageId="59" pageNumber="60" precision="555" value="-7.91">7.91°S</geoCoordinate>
