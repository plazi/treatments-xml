<taxonomicName id="70CA4D3CFFD6E2728464FE0FDFFE64DC" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[299,752,476,501]" class="Hydrozoa" family="Eudendriidae" genus="Eudendrium" kingdom="Animalia" order="Anthoathecata" pageId="59" pageNumber="60" phylum="Cnidaria" rank="species" species="ramosum">
