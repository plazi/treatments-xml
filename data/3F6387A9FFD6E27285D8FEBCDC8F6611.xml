, at 
<geoCoordinate id="D2FE5078FFD6E27286C8FDBFDEDA67AD" box="[903,980,620,645]" direction="south" orientation="latitude" pageId="59" pageNumber="60" precision="555" value="-6.95">6.95°S</geoCoordinate>
<geoCoordinate id="D2FE5078FFD6E2728695FDBFD93167AD" box="[986,1087,620,644]" direction="west" orientation="longitude" pageId="59" pageNumber="60" precision="555" value="-80.7">80.70°W</geoCoordinate>
in 
<collectingCountry id="CFDD762FFFD6E272812EFDBFD98467AC" box="[1121,1162,620,645]" name="Iceland" pageId="59" pageNumber="60">Isla</collectingCountry>
Lobos de Afuera, 
<collectingCountry id="CFDD762FFFD6E2728017FDBFD89967AC" box="[1368,1431,620,645]" name="Chile" pageId="59" pageNumber="60">Chile</collectingCountry>
, at 
