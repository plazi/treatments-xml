<quantity id="DD971F8D76DE437B2B439498A2F1CC1B" metricMagnitude="-2" metricUnit="m" metricValue="7.05" metricValueMax="8.1" metricValueMin="6.0" unit="mm" value="70.5" valueMax="81.0" valueMin="60.0">60-81 mm</quantity>
