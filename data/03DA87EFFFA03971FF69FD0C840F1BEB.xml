<number id="3DBB24DEFFA03971FF69F9A586C61B23" box="[151,165,1611,1635]" pageId="3" pageNumber="322" value="2.0">2</number>
anterior and 
<number id="3DBB24DEFFA03971FEC1F9A4872F1B23" box="[319,332,1610,1635]" pageId="3" pageNumber="322" value="5.0">5</number>
/ 
<number id="3DBB24DEFFA03971FEABF9A487001B23" box="[341,355,1610,1635]" pageId="3" pageNumber="322" value="6.0">6</number>
