7. 
<taxonomicName id="037763CC7FB427D32D5484B6A9A676FE" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:233614" authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" class="Insecta" family="Mutillidae" genus="Mutilla" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName-HNS="Mutilla familiaris" order="Hymenoptera" pageNumber="84" phylum="Arthropoda" rank="species" species="familiaris">Mutilla familiaris</taxonomicName>
.
