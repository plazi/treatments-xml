<taxonomicName id="4C77C3B8FFFBD703FF13FC0D2C17D819" box="[151,338,1004,1030]" class="Chilopoda" family="Scolopocryptopidae" genus="Scolopocryptops" kingdom="Animalia" order="Scolopendromorpha" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
