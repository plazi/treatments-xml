<number id="2B3824AAFF827C71FD3AFCA731BE7B5F" box="[747,761,837,860]" pageId="7" pageNumber="54" value="7.0">7</number>
+ 0; 
<collectingCountry id="E5E7761DFF827C71FCFDFCA730147B5F" box="[812,851,837,860]" name="Mexico" pageId="7" pageNumber="54">Mx</collectingCountry>
IV = 
