<quantity id="4CA19B3158089714FC39FD0F3F7BFD1C" box="[960,1098,654,681]" metricMagnitude="-3" metricUnit="m" metricValue="4.85" metricValueMax="5.2" metricValueMin="4.5" pageId="11" pageNumber="368" unit="mm" value="4.85" valueMax="5.2" valueMin="4.5">4.5–5.2 mm</quantity>
