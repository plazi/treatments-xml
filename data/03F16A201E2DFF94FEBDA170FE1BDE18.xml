; 225, 226, 
<taxonomicName id="4C58A0B51E2AFF97FBD9A379FB3BDD45" authorityName="Duponchel" authorityYear="1830" box="[1091,1238,1855,1877]" class="Insecta" family="Geometridae" genus="Phasiane" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="53" pageNumber="310" phylum="Arthropoda" rank="species" species="rippertaria">
