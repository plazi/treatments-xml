<subSubSection id="C3683DCCA761FF88FF0DF8F1309D58C9" box="[151,1252,1908,1935]" pageId="56" pageNumber="57" type="etymology">
<paragraph id="8BCD6E47A761FF88FF0DF8F1309D58C9" blockId="56.[151,1436,1908,2042]" box="[151,1252,1908,1935]" pageId="56" pageNumber="57">
<emphasis id="B906B255A761FF88FF0DF8F1356758C9" bold="true" box="[151,286,1908,1934]" pageId="56" pageNumber="57">Etymology.</emphasis>
