<emphasis id="44F8EA89D644B077B56DA0D0FCC1F798" bold="true" box="[862,873,2033,2054]" pageId="10" pageNumber="277">c</emphasis>
<taxonomicName id="B18C4D18D644B077B55DA0D3FC53F799" box="[878,1019,2034,2055]" class="Magnoliopsida" family="Fabaceae" genus="Vigna" kingdom="Plantae" order="Fabales" pageId="10" pageNumber="277" phylum="Tracheophyta" rank="species" species="sinensis">Vigna sinensis</taxonomicName>
.
