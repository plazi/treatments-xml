<caption id="22F36613D644B077B6BBA0D0FC57F799" ID-DOI="http://doi.org/10.5281/zenodo.5888922" ID-Zenodo-Dep="5888922" box="[136,1023,2033,2055]" httpUri="https://zenodo.org/record/5888922/files/figure.png" pageId="10" pageNumber="277" startId="10.[136,229,2033,2055]" targetBox="[343,1246,1106,2004]" targetPageId="10">
