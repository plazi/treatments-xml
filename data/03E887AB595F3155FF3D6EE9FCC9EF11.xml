<treatmentCitationGroup id="AB511193595F3155FF3D6EB9FD89EBB1" box="[178,541,716,736]" pageId="4" pageNumber="663">
<emphasis id="B935EAAF595F3155FF3D6EB9FF6CEBB1" box="[178,248,716,736]" italics="true" pageId="4" pageNumber="663">Feronia</emphasis>
<treatmentCitation id="0AE010AC595F3155FF726EB9FD8DEBB1" author="CHAUDOIR M" box="[253,537,716,736]" page="251" pageId="4" pageNumber="663" year="1868">
