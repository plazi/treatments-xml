<collectionCode id="ED7AAE07870BAA1442BFFD3BDF9CA8C6" box="[1186,1276,713,739]" country="USA" httpUri="http://grbio.org/cool/142r-0w94" name="Smithsonian Institution, National Museum of Natural History" pageId="16" pageNumber="17" type="Museum">NMNH</collectionCode>
