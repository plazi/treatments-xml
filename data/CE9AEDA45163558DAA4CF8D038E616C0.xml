<taxonomicName id="D6FADADF366C59EB5FA2DBFE9C5E4706" class="Magnoliopsida" family="Ranunculaceae" genus="Delphinium" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Delphinium campocarpum subsp. var. var. leiocarpum" order="Ranunculales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="variety" species="campocarpum" subSpecies="var." variety="leiocarpum">Delphinium campocarpum Ledeb. var. leiocarpum</taxonomicName>
