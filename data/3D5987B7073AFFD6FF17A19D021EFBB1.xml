<materialsCitation id="05983CFC073AFFD6FF17A653021EFBB1" ID-GBIF-Occurrence="1455384298" location="A" pageId="69" pageNumber="70" specimenCount="1" stateProvince="A">
<emphasis id="8784EAB3073AFFD6FF17A6530165FBF9" bold="true" box="[151,266,1050,1075]" pageId="69" pageNumber="70">Diagnosis</emphasis>
