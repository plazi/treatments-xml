<taxonomicName id="4C184D42FFE74C1EFB0DDF15FA0DFEBE" box="[1255,1436,296,320]" class="Insecta" family="Tortricidae" genus="Andrioplecta" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="490" phylum="Arthropoda" rank="species" species="suboxystaura">
