<taxonomicName id="D4382C3947E0709C12441C09909F0611" authorityName="Cheek, Blumea 62: 147" authorityYear="2017" class="Magnoliopsida" family="Rubiaceae" genus="Inversodicraea" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Inversodicraea xanderi" order="Malpighiales" pageId="0" pageNumber="81" phylum="Tracheophyta" rank="species" species="xanderi">
