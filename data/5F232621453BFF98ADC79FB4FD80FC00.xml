<subSubSection id="9F90C4BC453AFF97AD979ADEFD25F9B0" box="[199,760,1532,1557]" pageId="7" pageNumber="469" type="etymology">
<paragraph id="D7359737453AFF97AD979ADEFD25F9B0" blockId="7.[151,1437,1532,2024]" box="[199,760,1532,1557]" pageId="7" pageNumber="469">
<emphasis id="E5FE4B25453AFF97AD979ADEFE93F9B1" bold="true" box="[199,334,1533,1557]" pageId="7" pageNumber="469">Etymology.</emphasis>
Named after the 
<typeStatus id="08312995453AFF97AF409ADEFD9DF9B1" box="[528,576,1533,1557]" pageId="7" pageNumber="469">type</typeStatus>
