<quantity id="4CEC9B51C66BA219F53FFC14F4B340F3" box="[2759,2908,910,949]" metricMagnitude="-2" metricUnit="m" metricValue="4.6" pageId="29" pageNumber="256" unit="mm" value="46.0">46 mm</quantity>
