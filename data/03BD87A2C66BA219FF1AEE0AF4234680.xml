<taxonomicName id="4C144D37C66BA219F40AFD83F3894106" authorityName="Temminck" authorityYear="1834" box="[3058,3174,537,576]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="29" pageNumber="256" phylum="Chordata" rank="species" species="bicolor">
