<taxonomicName id="4C144D37C66BA219F531FE7EF3B4414D" authorityName="K. Andersen" authorityYear="1918" box="[2761,3163,484,523]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="29" pageNumber="256" phylum="Chordata" rank="species" species="nequam">
