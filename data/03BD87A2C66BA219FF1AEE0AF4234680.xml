<quantity id="4CEC9B51C66BA219F364FCC3F2C240C6" box="[3228,3373,857,896]" metricMagnitude="-2" metricUnit="m" metricValue="1.9" pageId="29" pageNumber="256" unit="mm" value="19.0">19 mm</quantity>
