<taxonomicName id="0B9FCDDDB338F2CC418DDE0955254D37" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:26752" authorityName="Forel" authorityYear="1886" class="Insecta" family="Formicidae" genus="Camponotus" kingdom="Animalia" lsidName-HNS="Camponotus grandidieri Forel" order="Hymenoptera" pageNumber="39" phylum="Arthropoda" rank="species" species="grandidieri">Camponotus Grandidieri</taxonomicName>
