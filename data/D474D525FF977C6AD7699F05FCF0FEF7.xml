<taxonomicName id="9BDD1FB0FF977C68D5D79ADFFCB4F944" authorityName="Boucomont" authorityYear="1935" box="[724,822,1667,1689]" class="Insecta" family="Scarabaeidae" genus="Sisyphus" kingdom="Animalia" order="Coleoptera" pageId="12" pageNumber="13" phylum="Arthropoda" rank="species" species="alveatus">S. alveatus</taxonomicName>
