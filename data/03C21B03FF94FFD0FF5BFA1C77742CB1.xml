<taxonomicName id="4C6BD196FF92FFD1FE4FF89277DB283D" box="[387,511,1866,1888]" class="Adenophorea" family="Mononchidae" genus="Prionchulus" kingdom="Animalia" order="Dorylaimida" pageId="28" pageNumber="329" phylum="Nematoda" rank="genus">
<emphasis id="B91F7607FF92FFD1FE4FF89277DB283D" box="[387,511,1866,1888]" italics="true" pageId="28" pageNumber="329">Prionchulus</emphasis>
</taxonomicName>
(F), 
<taxonomicName id="4C6BD196FF92FFD1FDF8F8927493283D" box="[564,695,1866,1888]" class="Adenophorea" family="Mononchidae" genus="Mylonchulus" kingdom="Animalia" order="Dorylaimida" pageId="28" pageNumber="329" phylum="Nematoda" rank="genus">
<emphasis id="B91F7607FF92FFD1FDF8F8927493283D" box="[564,695,1866,1888]" italics="true" pageId="28" pageNumber="329">Mylonchulus</emphasis>
</taxonomicName>
(H), 
<taxonomicName id="4C6BD196FF92FFD1FD3CF892755E283D" box="[752,890,1866,1888]" class="Adenophorea" family="Mononchidae" genus="Sporonchulus" kingdom="Animalia" order="Dorylaimida" pageId="28" pageNumber="329" phylum="Nematoda" rank="genus">
<emphasis id="B91F7607FF92FFD1FD3CF892755E283D" box="[752,890,1866,1888]" italics="true" pageId="28" pageNumber="329">Sporonchulus</emphasis>
</taxonomicName>
(J), 
<taxonomicName id="4C6BD196FF92FFD1FC67F89375C7283D" box="[939,995,1867,1888]" class="Adenophorea" family="Mononchidae" genus="Actus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="28" pageNumber="329" phylum="Nematoda" rank="genus">
