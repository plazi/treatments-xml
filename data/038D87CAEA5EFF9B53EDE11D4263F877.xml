<treatmentCitationGroup id="AB3411F2EA5EFF9B53EDE1FD456DF8AA" box="[151,537,1776,1798]" pageId="4" pageNumber="85">
<taxonomicName id="4C244D5FEA5EFF9B53EDE1FD478EF8AA" authorityName="Froehlich" authorityYear="1969" box="[151,250,1776,1798]" class="Insecta" family="Gripopterygidae" genus="Tupiperla" kingdom="Animalia" order="Plecoptera" pageId="4" pageNumber="85" phylum="Arthropoda" rank="genus">
