(
<taxonomicName id="4C6CA1CF668EB4F6C32D4C3A3AA72632" box="[529,701,1866,1891]" class="Magnoliopsida" family="Plantaginaceae" kingdom="Plantae" order="Lamiales" pageId="219" pageNumber="220" phylum="Tracheophyta" rank="family">Plantaginaceae</taxonomicName>
