<taxonomicName id="05B34D6C6640473229B3FF72C775FF74" authority="Mound &amp; Walker" authorityName="Mound &amp; Walker" authorityYear="1986" box="[734,1136,222,244]" class="Insecta" family="Phlaeothripidae" genus="Deplorothrips" kingdom="Animalia" order="Thysanoptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="bassus">
