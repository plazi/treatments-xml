TYPE SPECIES: 
<taxonomicName id="C8FF4D49FF897B12FEE42371B4BEFB44" authority="Liu and Engel" authorityName="Liu and Engel" authorityYear="2007" class="Insecta" family="Liopteridae" genus="Proliopteron" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="species" species="redactus" status="new species">
<emphasis id="3D8BEAD8FF897B12FEE42371B658FBB9" box="[299,553,1114,1138]" italics="true" pageId="9" pageNumber="10">Proliopteron redactus</emphasis>
Liu and Engel
</taxonomicName>
, 
<taxonomicNameLabel id="26B857A3FF897B12FF13235CB512FB44" box="[220,355,1143,1167]" pageId="9" pageNumber="10" rank="species">new species</taxonomicNameLabel>
