(
<taxonomicName id="DF093F100FCFD355AC5CAC0321B03FFA" authority="" authorityName="" class="Insecta" family="Rhizophoraceae" genus="Acarina" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Acarina" order="Coleoptera" pageId="0" pageNumber="1" phylum="Arthropoda" rank="family">Rhizophoraceae</taxonomicName>
