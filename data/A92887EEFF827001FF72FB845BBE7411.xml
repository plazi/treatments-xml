<taxonomicName id="E6814D7BFF827001FC83FB425B4F75D3" authority="L. BENICK. Aedoeagus" authorityName="L. BENICK. Aedoeagus" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="47" pageNumber="1326" phylum="Arthropoda" rank="species" species="simulans">
