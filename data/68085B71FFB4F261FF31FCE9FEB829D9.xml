<taxonomicName id="27A191E4FFB4F261FE80FB9EFE262831" box="[294,412,1066,1089]" class="Insecta" family="Cerambycidae" genus="Ozineus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="63" phylum="Arthropoda" rank="species" species="striatus">
