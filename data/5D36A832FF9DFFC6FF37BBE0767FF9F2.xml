<quantity id="1267B4C1FF9DFFC6FB97B944767AFAA9" box="[1060,1151,1282,1302]" metricMagnitude="-2" metricUnit="m" metricValue="3.25" pageId="7" pageNumber="1234" unit="mm" value="32.5">32.5 mm</quantity>
