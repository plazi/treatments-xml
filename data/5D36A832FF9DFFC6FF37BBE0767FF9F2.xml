<quantity id="1267B4C1FF9DFFC6FB12B94476F8FAA9" box="[1185,1277,1282,1302]" metricMagnitude="-2" metricUnit="m" metricValue="2.55" pageId="7" pageNumber="1234" unit="mm" value="25.5">25.5 mm</quantity>
