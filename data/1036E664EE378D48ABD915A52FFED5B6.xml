<taxonomicName id="9578E82282728F3BD1D8D0C6DCFB0B5C" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:147972" class="Insecta" family="Formicidae" genus="Azteca" higherTaxonomySource="CoL" kingdom="Animalia" lsidName-HNS="Azteca trigona mediops Forel" order="Hymenoptera" pageNumber="24" phylum="Arthropoda" rank="species" species="mediops">mediops</taxonomicName>
