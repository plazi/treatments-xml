<taxonomicName id="819154F56F720664FE81FAC72C80C117" authority="Salgueiro-Sepúlveda &amp; Álvarez-Padilla, 2018" authorityName="Salgueiro-Sepúlveda &amp; Álvarez-Padilla" authorityYear="2018" box="[294,509,1336,1358]" class="Arachnida" family="Tetragnathidae" genus="Chrysometa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="18" pageNumber="319" phylum="Arthropoda" rank="species" species="sagicuta" status="n. sp.">
<emphasis id="74E5F3646F720664FE81FAC72C80C117" box="[294,509,1336,1358]" italics="true" pageId="18" pageNumber="319">Chrysometa sagicuta</emphasis>
