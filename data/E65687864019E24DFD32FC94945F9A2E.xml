<taxonomicName id="A9FF4D13401BE24DFE6CFD5797C99A69" authority="(K. Schum.) Koechl." authorityName="Koechl." baseAuthorityName="K. Schum." class="Liliopsida" family="Marantaceae" genus="Marantochloa" kingdom="Plantae" order="Zingiberales" pageId="7" pageNumber="44" phylum="Tracheophyta" rank="species" species="cordifolia">
