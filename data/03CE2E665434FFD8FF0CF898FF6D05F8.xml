<taxonomicName id="4C67E4F3542BFFD8FC77FF4DFBDC07C2" baseAuthorityName="Putnam" baseAuthorityYear="1883" box="[1004,1120,224,248]" class="Arachnida" family="Eremobatidae" genus="Eremorhax" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="16" pageNumber="459" phylum="Arthropoda" rank="species" species="striatus">
