<taxonomicName id="4C67E4F3542BFFD8FD12FF4DFC9907CD" baseAuthorityName="Kraepelin" baseAuthorityYear="1899" box="[649,805,224,248]" class="Arachnida" family="Eremobatidae" genus="Eremocosta" higherTaxonomySource="GBIF" kingdom="Animalia" order="Solifugae" pageId="16" pageNumber="459" phylum="Arthropoda" rank="species" species="spinipalpis">
