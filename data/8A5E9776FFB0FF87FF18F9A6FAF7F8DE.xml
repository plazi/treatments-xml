<taxonomicName id="C5F75DE3FFB0FF87FDF1F8E4FD6AF8DE" box="[638,759,1871,1896]" class="Magnoliopsida" family="Lamiaceae" kingdom="Plantae" order="Lamiales" pageId="10" pageNumber="245" phylum="Tracheophyta" rank="family">Lamiaceae</taxonomicName>
