<taxonomicName id="EDC3A9B4B81ADE09C931F9F7755D5713" box="[304,637,1581,1604]" class="Insecta" family="Tenthredinidae" genus="Pachyprotasis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="16" pageNumber="43" phylum="Arthropoda" rank="species" species="maculoannulata" status="sp. nov.">
