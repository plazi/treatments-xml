<taxonomicName id="D99747CDD1459363082CBE2B9377FED4" box="[771,856,291,315]" class="Insecta" family="Meloidae" genus="Apalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="50" pageNumber="49" phylum="Arthropoda" rank="genus">
