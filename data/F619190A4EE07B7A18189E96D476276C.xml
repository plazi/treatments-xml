<taxonomicName id="6808F3972C809DE4F60706570570B015" LSID="urn:lsid:zoobank.org:act:30939031-FE99-43C1-B4D4-62D9FED2D02F" class="Insecta" family="Mycetophilidae" genus="Acomoptera" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Acomoptera crispa" order="Diptera" pageId="4" pageNumber="45" phylum="Arthropoda" rank="species" species="crispa">Acomoptera crispa</taxonomicName>
