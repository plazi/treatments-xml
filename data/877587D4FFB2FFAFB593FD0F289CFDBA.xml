<collectionCode id="69CDAE07FFB2FFA0B663FCB72882FC9E" box="[871,952,804,828]" httpUri="http://grbio.org/cool/rsbi-vwmq" name="Swedish Museum of Natural History, Entomology Collections" pageId="7" pageNumber="543">NHRS</collectionCode>
