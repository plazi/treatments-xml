<keyLead id="C50A95A4FFB3842A1526D0C8090BFAC4" pageId="8" pageNumber="9">
<number id="C83324B6FFB3842A1526D0C80D36FAB4" box="[264,278,1279,1305]" pageId="8" pageNumber="9" value="2.0">2</number>
