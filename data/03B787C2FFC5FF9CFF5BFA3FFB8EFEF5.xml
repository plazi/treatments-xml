<taxonomicName id="4C1E4D57FFC4FF9CFBE8FF64FB64FF11" authorityName="Metcalf" authorityYear="1952" box="[1060,1179,223,248]" class="Insecta" family="Cicadidae" genus="Procollina" kingdom="Animalia" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="genus">
