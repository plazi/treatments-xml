</taxonomicName>
<emphasis id="B96AEAC6FFC5FF9DFDC5F86DFDB7F806" bold="true" box="[521,584,2006,2031]" pageId="22" pageNumber="23">
<taxonomicNameLabel id="A25957BDFFC5FF9DFDC5F86DFDB7F806" box="[521,584,2006,2031]" pageId="22" pageNumber="23" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C1E4D57FFC5FF9DFD9AF86CFCEBF806" authority="Sanborn, 2018" authorityName="Sanborn" authorityYear="2018" box="[598,788,2006,2031]" class="Insecta" family="Cicadidae" genus="Procollina" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="mesomaculata" status="n. sp.">
