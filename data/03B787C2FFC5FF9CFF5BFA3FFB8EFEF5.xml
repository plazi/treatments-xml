with specimens being reported previously from 
<collectingCountry id="F3097644FFC4FF9CFE7AFEBFFDF4FEF4" box="[438,523,260,285]" name="Mexico" pageId="23" pageNumber="24">
<collectingRegion id="49DAF836FFC4FF9CFE7AFEBFFDF4FEF4" box="[438,523,260,285]" country="Mexico" name="Mexico" pageId="23" pageNumber="24">Mexico</collectingRegion>
</collectingCountry>
and 
<collectingCountry id="F3097644FFC4FF9CFD8EFEBFFD43FEF4" box="[578,700,260,285]" name="Costa Rica" pageId="23" pageNumber="24">
<collectingRegion id="49DAF836FFC4FF9CFD8EFEBFFD7DFEF5" box="[578,642,260,284]" country="New Zealand" name="West Coast" pageId="23" pageNumber="24">Costa</collectingRegion>
Rica
</collectingCountry>
(
<bibRefCitation id="EF8F4B25FFC4FF9CFD06FEBFFC8DFEF4" box="[714,882,260,285]" pageId="23" pageNumber="24" refId="ref51260" refString="Metcalf, Z. P. (1963 a) General catalogue of the Homoptera, Fascicle VIII. Cicadoidea. Part 2. Tibicinidae. North Carolina State College Contribution, 1564, i - vi, 1 - 492." type="book chapter">Metcalf 1963a</bibRefCitation>
; 
