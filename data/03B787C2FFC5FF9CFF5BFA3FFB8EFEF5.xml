<taxonomicNameLabel id="A25957BDFFC5FF9DFBFBF86DFB89F806" box="[1079,1142,2006,2031]" pageId="22" pageNumber="23" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, 
<taxonomicName id="4C1E4D57FFC5FF9DFB48F86CFAAAF806" authority="Sanborn, 2018" authorityName="Sanborn" authorityYear="2018" box="[1156,1365,2006,2031]" class="Insecta" family="Cicadidae" genus="Procollina" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="tamaulipasensis" status="n. sp.">
