<paragraph id="FE0A80BB0B71E48522849AC02580D488" pageId="27" pageNumber="28">
<taxonomicName id="53F17CCF6D1698D073838D39C6C399CA" LSID="http://zoobank.org/9C6F7162-34A4-4403-BFD6-20E160151FBB" authority="Yoder" class="Insecta" family="Scelionidae" genus="Scelio" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Scelio copelandi" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="copelandi">Scelio copelandi Yoder</taxonomicName>
<taxonomicNameLabel id="A49D42E9A0E533DF2ADAAA8AEA928FC3" pageId="27" pageNumber="28">sp. n.</taxonomicNameLabel>
