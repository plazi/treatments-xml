<taxonomicName id="98B74D69FFF8FFDBFE25FD4CFDF153CD" authorityName="Peck and Cook" authorityYear="2014" box="[501,631,675,699]" class="Insecta" family="Leiodidae" genus="Zeadolopus" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="29" phylum="Arthropoda" rank="species" species="hatomayor">
