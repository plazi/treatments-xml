<taxonomicName id="4C4F4D0CFFB1FFC7F09CFB342A5E70DF" authority="Zelazny &amp; Webb, 2011" authorityName="Zelazny &amp; Webb" authorityYear="2011" box="[969,1110,1224,1248]" class="Insecta" family="Derbidae" genus="Rhotana" kingdom="Animalia" order="Hemiptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="tridentata" status="sp. nov.">
