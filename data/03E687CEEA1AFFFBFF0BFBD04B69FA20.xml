<taxonomicName id="4C4F4D5BEA1AFFF9FB3FF8E04D36F8A4" box="[1187,1309,1862,1885]" class="Insecta" family="Chironomidae" genus="Tanytarsus" kingdom="Animalia" order="Diptera" pageId="12" pageNumber="581" phylum="Arthropoda" rank="genus">
