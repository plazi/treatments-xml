<number id="3DB1EB1AFF89C500FE75F874FE4DF980" box="[419,485,1578,1604]" pageId="5" pageNumber="6" value="2008.0">2008</number>
”, “ 
<taxonomicName id="4C7982BEFF89C500FDDDF875FC25F980" authority="Faust, Gultekin" authorityName="Faust, Gultekin" box="[523,909,1578,1604]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="subulatus">
