<typeStatus id="54C2479FFF89C500FD84FBE4FD78FA10" box="[594,720,1466,1492]" pageId="5" pageNumber="6">Paratypus</typeStatus>
, 
<number id="3DB1EB1AFF89C500FD0BFBE4FCB5FA10" box="[733,797,1466,1492]" pageId="5" pageNumber="6" value="1909.0">1909</number>
, 
<taxonomicName id="4C7982BEFF89C500FCFCFBE5FB9CFA10" authority="Faust" authorityName="Faust" box="[810,1076,1466,1492]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="subulatus">
<emphasis id="B90D252FFF89C500FCFCFBE5FC41FA10" box="[810,1001,1467,1492]" italics="true" pageId="5" pageNumber="6">Lixus subulatus</emphasis>
Faust
</taxonomicName>
<taxonomicName id="4C7982BEFF89C500FBEAFBE4FACAFA10" authority="Reitter" authorityName="Reitter" box="[1084,1378,1466,1492]" pageId="5" pageNumber="6" rank="variety" variety="rubrolateralis">
v. 
<emphasis id="B90D252FFF89C500FB8CFBE5FAACFA10" box="[1114,1284,1467,1492]" italics="true" pageId="5" pageNumber="6">rubrolateralis</emphasis>
Reitter
</taxonomicName>
