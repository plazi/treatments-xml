des. 
<number id="3DB1EB1AFF89C500FD94F82BFD2AF94B" box="[578,642,1653,1679]" pageId="5" pageNumber="6" value="2008.0">2008</number>
”, “ 
<taxonomicName id="4C7982BEFF89C500FD73F82BFBB0F94B" authority="Faust, Gultekin" authorityName="Faust, Gultekin" box="[677,1048,1653,1679]" class="Insecta" family="Curculionidae" genus="Lixus" kingdom="Animalia" order="Coleoptera" pageId="5" pageNumber="6" phylum="Arthropoda" rank="species" species="subulatus">
