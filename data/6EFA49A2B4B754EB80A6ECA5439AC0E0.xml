<taxonomicName id="60CAB8A99CED4D4B45300D71F1865085" LSID-HNS="urn:lsid:biosci.ohio-state.edu:osuc_concepts:228148" class="Insecta" family="Formicidae" genus="Anochetus" genus.bestMatchDistance="2406" genus.bestMatchVote="2" genus.innerRound="1" genus.outerRound="1" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Anochetus madagascarensis var obscurata Santschi" lsidName-HNS="Anochetus madagascarensis var obscurata Santschi" order="Hymenoptera" pageNumber="265" phylum="Arthropoda" rank="variety" species="africanus" variety="obscurata">A. africanus Mayr var. obscurata Sants</taxonomicName>
