<location id="8EB9E2F4AA7DFFA2FBE0FE23FB8CFE61" LSID="urn:lsid:plazi:treatment:03CF0539AA7DFFA2FEF4FEE8FB5BFD78:8EB9E2F4AA7DFFA2FBE0FE23FB8CFE61" box="[1052,1098,471,497]" country="Trinidad &amp; Tobago, Venezuela, Ecuador" municipality="This" name="The" pageId="55" pageNumber="56" stateProvince="Sucumbios">The</location>
