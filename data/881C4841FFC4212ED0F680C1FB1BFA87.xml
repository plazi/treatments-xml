<taxonomicName id="C7B582D4FFC4212ED14580C1FB25FC83" authority="Durette-Desset, 1971" authorityName="Durette-Desset" authorityYear="1971" box="[776,1187,754,781]" class="Chromadorea" family="Heligmonellidae" genus="Neoheligmonella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Rhabditida" pageId="3" pageNumber="482" phylum="Nematoda" rank="genus">
