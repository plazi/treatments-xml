<taxonomicName id="4C16CFFBFFFFFFA5A5AFDFD69ECB0C21" box="[445,686,1769,1792]" class="Arachnida" family="Drymusidae" genus="Drymusa" kingdom="Animalia" order="Araneae" pageId="2" pageNumber="3" phylum="Arthropoda" rank="species" species="canhemabae" status="n. sp.">
