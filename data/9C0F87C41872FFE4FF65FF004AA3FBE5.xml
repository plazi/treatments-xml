long, 
<quantity id="D35E9B371872FFE4FCB3FD6C4BADFCC4" box="[833,918,763,787]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" pageId="5" pageNumber="491" unit="mm" value="1.7">1.7 mm</quantity>
wide across eyes and 
<quantity id="D35E9B371872FFE4FB7FFD6D4D28FCC4" box="[1165,1299,762,787]" metricMagnitude="-3" metricUnit="m" metricValue="1.55" metricValueMax="1.6" metricValueMin="1.5" pageId="5" pageNumber="491" unit="mm" value="1.55" valueMax="1.6" valueMin="1.5">1.5–1.6 mm</quantity>
