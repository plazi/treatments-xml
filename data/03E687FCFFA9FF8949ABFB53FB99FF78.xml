<quantity id="4CB79B0FFFA9FF8A4C2AFA19FB3DF9A3" box="[1070,1155,1530,1554]" metricMagnitude="3" metricUnit="m" metricValue="2.0" pageId="9" pageNumber="874" unit="m" value="2000.0">
