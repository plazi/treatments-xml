<collectingRegion id="498BF808FFA9FF8A4B8FFA19FBB7F9A3" box="[907,1033,1530,1554]" country="Tanzania" name="Kilimanjaro" pageId="9" pageNumber="874">Kilimanjaro</collectingRegion>
