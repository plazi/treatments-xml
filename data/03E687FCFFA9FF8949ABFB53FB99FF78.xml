<elevation id="0062D1D9FFA9FF8A4951F981FE16F9CB" box="[341,424,1634,1658]" metricMagnitude="2" metricUnit="m" metricValue="7.62" pageId="9" pageNumber="874" unit="ft" value="2500.0">2500 ft</elevation>
