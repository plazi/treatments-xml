<taxonomicName id="4C034D76FFA1FF9E76448F6DFBD3FA94" box="[1026,1090,1367,1390]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="297" phylum="Tracheophyta" rank="genus">
<emphasis id="B977EAE7FFA1FF9E76448F6DFBD3FA94" box="[1026,1090,1367,1390]" italics="true" pageId="18" pageNumber="297">Pinus</emphasis>
</taxonomicName>
, mixed forests with 
<taxonomicName id="4C034D76FFA1FF9E776A8F6DFAFDFA94" box="[1324,1388,1367,1390]" class="Pinopsida" family="Pinaceae" genus="Pinus" kingdom="Plantae" order="Pinales" pageId="18" pageNumber="297" phylum="Tracheophyta" rank="genus">
<emphasis id="B977EAE7FFA1FF9E776A8F6DFAFDFA94" box="[1324,1388,1367,1390]" italics="true" pageId="18" pageNumber="297">Pinus</emphasis>
</taxonomicName>
and 
<taxonomicName id="4C034D76FFA1FF9E72D18F41FF64FA68" box="[151,245,1403,1426]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="18" pageNumber="297" phylum="Tracheophyta" rank="genus">
<emphasis id="B977EAE7FFA1FF9E72D18F41FF64FA68" box="[151,245,1403,1426]" italics="true" pageId="18" pageNumber="297">Quercus</emphasis>
</taxonomicName>
.
</paragraph>
<paragraph id="8BBC36F5FFA1FF9E728F8FA4FDABFA4C" blockId="18.[151,1436,1293,1679]" box="[201,570,1438,1463]" pageId="18" pageNumber="297">
