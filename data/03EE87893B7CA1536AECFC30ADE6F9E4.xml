<taxonomicName id="4C474D1C3B7CA1536ACFFB6FAF2CFB5B" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[180,303,1233,1259]" class="Arachnida" family="Prodidomidae" genus="Eleleis" kingdom="Animalia" order="Araneae" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="haddadi" status="sp. nov.">
