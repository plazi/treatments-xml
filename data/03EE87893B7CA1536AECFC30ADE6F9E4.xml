<taxonomicName id="4C474D1C3B7CA1536BB3FB34AC7AFB13" authority="Rodrigues &amp; Rheims, 2020" authorityName="Rodrigues &amp; Rheims" authorityYear="2020" box="[456,633,1161,1187]" class="Arachnida" family="Prodidomidae" genus="Eleleis" kingdom="Animalia" order="Araneae" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="solitaria" status="sp. nov.">
