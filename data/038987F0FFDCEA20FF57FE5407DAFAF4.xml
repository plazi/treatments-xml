<quantity id="4CD89B03FFDDEA20FB48FC270063FC7F" box="[1160,1299,908,932]" metricMagnitude="-3" metricUnit="m" metricValue="1.7" metricValueMax="2.1" metricValueMin="1.3" pageId="3" pageNumber="36" unit="mm" value="1.7" valueMax="2.1" valueMin="1.3">1.3–2.1 mm</quantity>
