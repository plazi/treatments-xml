<quantity id="A241D8DBFFF82302FD070C52FCC6FEE3" box="[704,786,290,317]" metricMagnitude="-2" metricUnit="m" metricValue="8.5" pageId="10" pageNumber="331" unit="mm" value="85.0">85 mm</quantity>
