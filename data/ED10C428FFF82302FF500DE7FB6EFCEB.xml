<collectionCode id="03A8EDFBFFF82302FEE30C36FEBAFEBF" box="[292,366,326,352]" country="New Zealand" httpUri="http://grbio.org/cool/nyq3-3g6s" name="National Institute of Water and Atmospheric Research" pageId="10" pageNumber="331" type="Other institutional type">NIWA</collectionCode>
144243); 
<emphasis id="57CDA92CFFF82302FE270C35FD5DFE80" bold="true" box="[480,649,325,351]" pageId="10" pageNumber="331">TAN 0602/422</emphasis>
, one stem 
<quantity id="A241D8DBFFF82302FCC60C36FC84FEBF" box="[769,848,326,352]" metricMagnitude="-2" metricUnit="m" metricValue="9.0" pageId="10" pageNumber="331" unit="mm" value="90.0">90 mm</quantity>
