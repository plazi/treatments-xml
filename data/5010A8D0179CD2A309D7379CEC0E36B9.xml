<taxonomicName id="2BD4B6A7E6FFB597E0DDB0F550FA8CC6" family="Fagaceae" lsidName="" pageId="8" pageNumber="83" rank="family">Fagaceae</taxonomicName>
, 
<taxonomicName id="F007881A9B58E521D9731A62EA8439F8" family="Lauraceae" lsidName="" pageId="8" pageNumber="83" rank="family">Lauraceae</taxonomicName>
, 
<taxonomicName id="102FE79B6AE9F82620E6E31B7DE55C07" family="Theaceae" lsidName="" pageId="8" pageNumber="83" rank="family">Theaceae</taxonomicName>
, 
<taxonomicName id="DDD1CF2EC37BB9568B033BA5C580FD66" family="Magnoliaceae" lsidName="" pageId="8" pageNumber="83" rank="family">Magnoliaceae</taxonomicName>
, and 
<taxonomicName id="4B8F29CA0B0F70938EF7A21DCB931A01" family="Hamamelidaceae" lsidName="" pageId="8" pageNumber="83" rank="family">Hamamelidaceae</taxonomicName>
.
</paragraph>
</subSubSection>
