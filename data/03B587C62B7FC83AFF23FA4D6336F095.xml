<taxonomicName id="4C1C4D532B7DC83DFE8BF89063A3F7FF" authority="Macgowan, 2018" authorityName="Macgowan" authorityYear="2018" box="[319,482,1916,1938]" class="Insecta" family="Lonchaeidae" genus="Lonchaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="328" phylum="Arthropoda" rank="species" species="dama" status="sp. nov.">
<emphasis id="B968EAC22B7DC83DFE8BF89063A3F7FF" box="[319,482,1916,1938]" italics="true" pageId="3" pageNumber="328">Lonchaea dama</emphasis>
</taxonomicName>
<taxonomicNameLabel id="A25B57B92B7DC83DFE53F8916076F7FF" box="[487,567,1917,1938]" pageId="3" pageNumber="328" rank="species">
