<paragraph id="FD3136865219211DFF00FB14FBB1FAB4" blockId="3.[227,1125,1267,1294]" box="[227,1125,1267,1294]" pageId="3" pageNumber="4">
Southern 
<collectingCountry id="859976165219211DFEB7FB14FE42FAB4" box="[340,406,1267,1293]" name="Chile" pageId="3" pageNumber="4">Chile</collectingCountry>
, provinces 
<collectingRegion id="3F4AF8645219211DFDF9FB13FDA7FAB7" box="[538,627,1268,1294]" country="Chile" name="Araucania" pageId="3" pageNumber="4">Arauco</collectingRegion>
