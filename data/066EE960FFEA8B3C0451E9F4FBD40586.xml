<subSubSection id="C6DD0BFDFFE88B3C0461ED59FBD40586" pageId="27" pageNumber="28" type="discussion">
<paragraph id="8E785876FFE88B3C0461ED59FBD40586" blockId="27.[264,1323,284,430]" pageId="27" pageNumber="28">
<emphasis id="BCB38464FFE88B3C0461ED59FE190576" bold="true" box="[312,444,324,350]" italics="true" pageId="27" pageNumber="28">Discussion</emphasis>
: 
<taxonomicName id="49C723F5FFE88B3C0492ED59FCE40575" box="[459,833,324,350]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="gorirossiae" subGenus="Hearlea">
<emphasis id="BCB38464FFE88B3C0492ED59FD9F0575" box="[459,570,324,349]" italics="true" pageId="27" pageNumber="28">Simulium</emphasis>
(
<emphasis id="BCB38464FFE88B3C0712ED59FD0E0575" box="[587,683,324,349]" italics="true" pageId="27" pageNumber="28">Hearlea</emphasis>
) 
<emphasis id="BCB38464FFE88B3C07E4ED59FCE40575" box="[701,833,324,349]" italics="true" pageId="27" pageNumber="28">gorirossiae</emphasis>
</taxonomicName>
, 
<taxonomicName id="49C723F5FFE88B3C0609ED59FBB10575" box="[848,1044,324,349]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="larvispinosum">
<emphasis id="BCB38464FFE88B3C0609ED59FBB10575" box="[848,1044,324,349]" italics="true" pageId="27" pageNumber="28">S. larvispinosum</emphasis>
</taxonomicName>
, 
<taxonomicName id="49C723F5FFE88B3C017AED59FB650575" box="[1059,1216,324,349]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="menchacai">
<emphasis id="BCB38464FFE88B3C017AED59FB650575" box="[1059,1216,324,349]" italics="true" pageId="27" pageNumber="28">S. menchacai</emphasis>
</taxonomicName>
, 
<taxonomicName id="49C723F5FFE88B3C0197ED59FEE705AD" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="carolinae">
<emphasis id="BCB38464FFE88B3C0197ED59FEE705AD" italics="true" pageId="27" pageNumber="28">S. carolinae</emphasis>
</taxonomicName>
, 
<taxonomicName id="49C723F5FFE88B3C040AED71FE6B05AD" box="[339,462,364,389]" class="Insecta" family="Simuliidae" genus="Simulium" kingdom="Animalia" order="Diptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="johnson">
<emphasis id="BCB38464FFE88B3C040AED71FE6B05AD" box="[339,462,364,389]" italics="true" pageId="27" pageNumber="28">S. johnson</emphasis>
</taxonomicName>
, and 
