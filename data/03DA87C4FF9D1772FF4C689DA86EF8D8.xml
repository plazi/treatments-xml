</bibRefCitation>
and Smith (
<number id="3DBB24F5FF9D1772FD5D6C95A8DCF881" box="[646,704,1780,1804]" pageId="21" pageNumber="132" value="1991.0">1991</number>
) considered 
<taxonomicName id="4C734D51FF9D1772FC8D6C95A9C1F880" box="[854,989,1780,1805]" class="Arachnida" family="Aturidae" genus="Bharatalbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="21" pageNumber="132" phylum="Arthropoda" rank="genus">
