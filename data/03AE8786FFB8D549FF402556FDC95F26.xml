<taxonomicName id="4C074D13FFB8D545FF402556FD875973" authority="Downey 1971" authorityName="Downey. Examination" authorityYear="1971" box="[151,601,1089,1116]" class="Asteroidea" family="Pedicellasteridae" genus="Ampheraster" kingdom="Animalia" order="Forcipulatida" pageId="5" pageNumber="206" phylum="Echinodermata" rank="species" species="alaminos">
