(Song, in 
<bibRefCitation author="Song, D. &amp; Chen, G." box="[589,792,1876,1901]" pageId="89" pageNumber="90" pagination="406 - 424" refId="ref66067" refString="Song, D. &amp; Chen, G. (1976) Some parasitic copepods from marine fishes of China. Acta Zoologica Sinica, 22, 406 - 424." type="journal article" year="1976">Song &amp; Chen, 1976</bibRefCitation>
)
</taxonomicName>
<emphasis bold="true" box="[805,898,1877,1901]" pageId="89" pageNumber="90">
