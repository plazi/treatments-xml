<paragraph blockId="81.[151,491,405,431]" box="[151,491,405,431]" pageId="81" pageNumber="82">
<heading bold="true" box="[151,491,405,431]" fontSize="11" level="1" pageId="81" pageNumber="82" reason="1">
<emphasis bold="true" box="[151,491,405,431]" pageId="81" pageNumber="82">
Genus 
<taxonomicName authority="Wilson, 1913" authorityName="Wilson" authorityYear="1913" box="[238,491,405,431]" class="Maxillopoda" family="Lernanthropidae" genus="Sagum" kingdom="Animalia" order="Siphonostomatoida" pageId="81" pageNumber="82" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[238,323,405,431]" italics="true" pageId="81" pageNumber="82">Sagum</emphasis>
<bibRefCitation author="Wilson, C. B." box="[330,491,405,431]" pageId="81" pageNumber="82" pagination="189 - 227" refId="ref66717" refString="Wilson, C. B. (1913) Crustacean parasites of West Indian fishes and land crabs, with descriptions of new genera and species. Proceedings of the United States National Museum, 44 (1950), 189 - 227, pls. 18 - 53. https: // doi. org / 10.5479 / si. 00963801.44 - 1950.189" type="journal article" year="1913">Wilson, 1913</bibRefCitation>
</taxonomicName>
</emphasis>
