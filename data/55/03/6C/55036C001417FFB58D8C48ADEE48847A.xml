<subSubSection box="[197,1384,576,603]" pageId="6" pageNumber="63" type="etymology">
<paragraph blockId="6.[151,1437,152,603]" box="[197,1384,576,603]" pageId="6" pageNumber="63">
<emphasis bold="true" box="[197,338,576,602]" pageId="6" pageNumber="63">Etymology.</emphasis>
