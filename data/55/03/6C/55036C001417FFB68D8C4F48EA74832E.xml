<emphasis box="[724,786,1231,1256]" italics="true" pageId="5" pageNumber="62">
<taxonomicName box="[724,782,1231,1256]" class="Insecta" family="Hydroptilidae" genus="Ochrotrichia" kingdom="Animalia" order="Trichoptera" pageId="5" pageNumber="62" phylum="Arthropoda" rank="species" species="pora">porâ</taxonomicName>
