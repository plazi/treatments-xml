<subSubSection pageId="5" pageNumber="62" type="etymology">
<paragraph blockId="5.[151,1437,495,1295]" pageId="5" pageNumber="62">
<emphasis bold="true" box="[197,338,1230,1256]" pageId="5" pageNumber="62">Etymology.</emphasis>
