<emphasis box="[293,489,714,735]" italics="true" pageId="91" pageNumber="92">
Trimma 
<taxonomicName box="[379,489,714,735]" pageId="91" pageNumber="92" rank="species" species="unisquame">unisquame</taxonomicName>
