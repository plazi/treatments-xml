<pageBreakToken pageId="null" pageNumber="590" start="start">Artengruppe</pageBreakToken>
des 
<taxonomicName authority="L." class="Magnoliopsida" family="Asteraceae" genus="Hieracium" kingdom="Plantae" order="Asterales" pageId="null" pageNumber="590" phylum="Tracheophyta" rank="species" species="cymosum">
Hieracium 
