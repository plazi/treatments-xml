<location LSID="urn:lsid:plazi:treatment:553A87DDFF8FFFC6DCB6FD095EEE3F46:D84C6010FF8FFFC6D924FCBA587639DA" box="[1285,1363,877,902]" country="Spain" county="Huesca province" municipality="Jaca" name="Cantal" pageId="9" pageNumber="557">Cantal</location>
”, 
<collectingMunicipality box="[1388,1436,878,901]" pageId="9" pageNumber="557">Jaca</collectingMunicipality>
municipality, 
<collectingCounty box="[305,495,913,938]" pageId="9" pageNumber="557">Huesca province</collectingCounty>
, 
<collectingCountry box="[506,573,913,938]" name="Spain" pageId="9" pageNumber="557">Spain</collectingCountry>
