<quantity box="[826,979,862,883]" metricMagnitude="3" metricUnit="m" metricValue="2.105" metricValueMax="2.11" metricValueMin="2.1" pageId="21" pageNumber="248" unit="m" value="2105.0" valueMax="2110.0" valueMin="2100.0">
