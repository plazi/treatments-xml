<taxonomicName authority="Ito &amp; Nozaki" authorityName="Ito &amp; Nozaki" authorityYear="2018" box="[280,639,1878,1900]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trichoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="species" species="aira" status="sp. nov.">
<taxonomicName authority="Ito" authorityName="Ito" box="[280,535,1878,1900]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="species" species="aira">
<taxonomicName box="[280,501,1878,1900]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="species" species="aira">
<taxonomicName box="[280,453,1878,1900]" class="Insecta" family="Hydropsychidae" genus="Cheumatopsyche" higherTaxonomySource="CoL" kingdom="Animalia" order="Trichoptera" pageId="12" pageNumber="557" phylum="Arthropoda" rank="genus">
<emphasis box="[280,453,1878,1900]" italics="true" pageId="12" pageNumber="557">Cheumatopsyche</emphasis>
</taxonomicName>
<taxonomicName authority="Ito &amp; Nozaki" authorityName="Ito &amp; Nozaki" box="[459,501,1879,1900]" pageId="12" pageNumber="557" rank="species" species="aira" status="sp. nov.">
