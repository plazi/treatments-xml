<quantity box="[967,1103,1895,1920]" metricMagnitude="-3" metricUnit="m" metricValue="7.1" metricValueMax="7.3" metricValueMin="6.9" pageId="8" pageNumber="553" unit="mm" value="7.1" valueMax="7.3" valueMin="6.9">6.9–7.3 mm</quantity>
