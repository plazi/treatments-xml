<taxonomicName authority="Chobaut, 1899" authorityName="Chobaut" authorityYear="1899" class="Insecta" family="Elateridae" genus="Cebriognathus" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Cebriognathus" order="Coleoptera" pageId="146" pageNumber="147" phylum="Arthropoda" rank="genus">Cebriognathus</taxonomicName>
