<subSubSection pageId="1" pageNumber="154" type="etymology">
<paragraph pageId="1" pageNumber="154">Etymology.</paragraph>
<paragraph pageId="1" pageNumber="154">
After 
<normalizedToken originalValue="“Libano”">&quot;Libano&quot;</normalizedToken>
(Lebanese in Latin), and &quot; 
<taxonomicName class="Insecta" family="Psyllipsocidae" genus="Psyllipsocus" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Psyllipsocus" order="Psocodea" pageId="1" pageNumber="154" phylum="Arthropoda" rank="genus">Psyllipsocus</taxonomicName>
