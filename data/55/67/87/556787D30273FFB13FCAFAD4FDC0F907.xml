<taxonomicName authority="sculpturatus" authorityName="sculpturatus Broun" authorityYear="1908" class="Insecta" family="Carabidae" genus="Oopterus" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="44" phylum="Arthropoda" rank="subSpecies" species="sculpturatus" subSpecies="sculpturatus">
