<collectingCountry box="[762,866,1157,1183]" name="United States of America" pageId="9" pageNumber="10">America</collectingCountry>
, 
<collectingCountry box="[877,949,1157,1183]" name="Brazil" pageId="9" pageNumber="10">Brazil</collectingCountry>
(see 
<bibRefCitation author="Luther" box="[1010,1154,1157,1183]" pageId="9" pageNumber="17" refString="Luther, A. (1960) Die Turbellarien Ostfennoskandiens. Fauna Fennica, 7, 1 - 155." type="journal article" year="1960">
