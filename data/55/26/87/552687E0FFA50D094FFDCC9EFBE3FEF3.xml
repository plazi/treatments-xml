<subSubSection pageId="3" pageNumber="4" type="materials_examined">
<paragraph blockId="3.[151,1436,1663,2026]" pageId="3" pageNumber="4">
<emphasis bold="true" box="[196,312,1701,1727]" pageId="3" pageNumber="4">Material.</emphasis>
