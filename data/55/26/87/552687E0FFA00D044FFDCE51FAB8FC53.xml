<subSubSection box="[196,1072,263,290]" pageId="7" pageNumber="8" type="etymology">
<paragraph blockId="7.[151,1437,152,402]" box="[196,1072,263,290]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[196,337,263,289]" pageId="7" pageNumber="8">Etymology.</emphasis>
