<taxonomicName authorityName="Ishikawa &amp; Yano" authorityYear="2002" box="[426,631,291,314]" class="Insecta" family="Reduviidae" genus="Polytoxus" kingdom="Animalia" order="Hemiptera" pageId="12" pageNumber="27" phylum="Arthropoda" rank="subSpecies" species="rufinervis" subSpecies="ardens">
