<emphasis id="B1ACEAB3CD16FF9DFF28FBEEFEC1FB45" bold="true" box="[151,286,1130,1155]" pageId="3">Etymology.</emphasis>
<taxonomicName id="44D84D22CD16FF9DFE9AFBEFFEA3FB44" box="[293,380,1131,1154]" class="Insecta" family="Gomphidae" genus="Burmagomphus" kingdom="Animalia" order="Odonata" pageId="3" phylum="Arthropoda" rank="species" species="magnus">
