<materialsCitation id="3B38895F966CD229FF58FD12929FFCF4" collectingDate="1964-03-13" collectionCode="NMNH" elevation="2846" latitude="-60.9" longLatPrecision="1029" longitude="-56.966667" pageId="21" pageNumber="2279" specimenCode="NMNH 1154056" specimenCount="1" typeStatus="paratype">
