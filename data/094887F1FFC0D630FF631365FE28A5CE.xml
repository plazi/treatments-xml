<taxonomicName id="46E14D64FFC0D630FBEA10A7FB5BA5A2" box="[1054,1237,1694,1719]" class="Reptilia" family="Cheloniidae" genus="Chelonia" kingdom="Animalia" order="Testudines" pageId="5" pageNumber="206" phylum="Chordata" rank="species" species="mydas">
