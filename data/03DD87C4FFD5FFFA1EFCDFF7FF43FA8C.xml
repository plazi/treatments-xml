<taxonomicName id="4C744D51FFD5FFFA1EFCDFF7FE31FD78" box="[151,411,654,679]" class="Arachnida" family="Argasidae" genus="Ornithodoros" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="capensis">
<emphasis id="B900EAC0FFD5FFFA1EFCDFF7FE31FD78" bold="true" box="[151,411,654,679]" italics="true" pageId="13" pageNumber="14">Ornithodoros capensis</emphasis>
