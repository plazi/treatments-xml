0, in 
<taxonomicName id="AADF4D22FF965C3D5D4EFF7FFAD6E4DD" box="[1051,1373,188,212]" class="Insecta" family="Coreidae" genus="Homoeocerus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="584" phylum="Arthropoda" rank="species" species="limbatipennis">
