X 
<number id="DB172486FF965C3D5AE4FF09FC32E4D0" box="[945,953,203,217]" pageId="7" pageNumber="584" value="1.0">1</number>
</date>
<date id="19611061FF965C3D5AECFF7FFC5EE4D0" box="[953,981,189,217]" pageId="7" pageNumber="584">
X 
