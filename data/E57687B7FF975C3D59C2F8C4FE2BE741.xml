<number id="DB172486FF965C3D5C7CFF20FAE9E4F3" box="[1321,1378,226,250]" pageId="7" pageNumber="584" value="1935.0">1935</number>
) and in 
<taxonomicName id="AADF4D22FF965C3D59E1FEC8FE20E528" box="[180,427,265,289]" class="Insecta" family="Coreidae" genus="Homoeocerus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="584" phylum="Arthropoda" rank="species" species="serrifer">
<emphasis id="5FABEAB3FF965C3D59E1FEC8FE20E528" box="[180,427,265,289]" italics="true" pageId="7" pageNumber="584">Homoeocerus serrifer</emphasis>
</taxonomicName>
with 
<number id="DB172486FF965C3D58B8FECBFE70E528" box="[493,507,265,289]" pageId="7" pageNumber="584" value="2.0">2</number>
n= 
<number id="DB172486FF965C3D5B4CFECBFDB8E528" box="[537,563,265,289]" pageId="7" pageNumber="584" value="18.0">18</number>
