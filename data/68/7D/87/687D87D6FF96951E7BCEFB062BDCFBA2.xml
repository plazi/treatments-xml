<subSubSection pageId="2" pageNumber="265" type="etymology">
<paragraph blockId="2.[190,309,1033,1057]" box="[190,309,1033,1057]" pageId="2" pageNumber="265">
<emphasis bold="true" box="[190,309,1033,1057]" pageId="2" pageNumber="265">Etymology:</emphasis>
