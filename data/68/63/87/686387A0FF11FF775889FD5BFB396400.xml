<paragraph blockId="220.[151,1437,726,1401]" box="[151,337,726,752]" pageId="220" pageNumber="221">
<heading bold="true" box="[151,337,726,752]" fontSize="11" level="2" pageId="220" pageNumber="221" reason="3">
<taxonomicName baseAuthorityName="Papp" baseAuthorityYear="1984" box="[151,337,726,752]" class="Insecta" family="Sphaeroceridae" kingdom="Animalia" order="Diptera" pageId="220" pageNumber="221" phylum="Arthropoda" rank="family">
