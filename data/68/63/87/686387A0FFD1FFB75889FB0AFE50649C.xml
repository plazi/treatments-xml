described 
<emphasis box="[634,829,1411,1437]" italics="true" pageId="28" pageNumber="29">
<taxonomicName authorityName="Rondani" authorityYear="1875" box="[634,825,1411,1437]" class="Insecta" family="Agromyzidae" genus="Agromyza" kingdom="Animalia" order="Diptera" pageId="28" pageNumber="29" phylum="Arthropoda" rank="species" species="analis">Agromyza analis</taxonomicName>
