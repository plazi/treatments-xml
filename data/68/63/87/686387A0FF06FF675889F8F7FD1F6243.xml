</bibRefCitation>
described 
<taxonomicName authorityName="Rondani" authorityYear="1861" box="[628,863,619,645]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="204" pageNumber="205" phylum="Arthropoda" rank="species" species="privigna">
