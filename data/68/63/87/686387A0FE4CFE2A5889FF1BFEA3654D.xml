(
<specimenCount box="[467,489,942,968]" pageId="385" pageNumber="386" type="male">♂</specimenCount>
) and 
<typeStatus box="[554,716,942,968]" pageId="385" pageNumber="386">paralectotypes</typeStatus>
(
<specimenCount box="[735,778,942,969]" pageId="385" pageNumber="386" type="male">1 ♂</specimenCount>
, 
<specimenCount box="[791,853,942,969]" pageId="385" pageNumber="386" type="female">2 ♀♀</specimenCount>
