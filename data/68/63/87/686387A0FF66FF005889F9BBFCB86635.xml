<taxonomicName authorityName="Rondani" authorityYear="1875" box="[637,873,1770,1796]" class="Insecta" family="Lauxaniidae" genus="Piophila" kingdom="Animalia" order="Diptera" pageId="171" pageNumber="172" phylum="Arthropoda" rank="species" species="melanocera">
<emphasis box="[637,873,1770,1796]" italics="true" pageId="171" pageNumber="172">Piophila melanocera</emphasis>
</taxonomicName>
from more than 
<specimenCount box="[1065,1219,1770,1797]" pageId="171" pageNumber="172" type="generic">one specimen</specimenCount>
