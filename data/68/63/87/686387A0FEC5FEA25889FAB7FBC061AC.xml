<taxonomicName authority="(Fallen, 1817)" baseAuthorityName="Fallen" baseAuthorityYear="1817" box="[1050,1431,1554,1580]" class="Insecta" family="Syrphidae" genus="Pipiza" kingdom="Animalia" order="Diptera" pageId="264" pageNumber="265" phylum="Arthropoda" rank="species" species="campestris">
<emphasis box="[1050,1254,1555,1580]" italics="true" pageId="264" pageNumber="265">Pipiza campestris</emphasis>
(
<bibRefCitation author="Fallen, C. F." box="[1274,1424,1554,1580]" pageId="264" pageNumber="265" pagination="43 - 54" refId="ref350459" refString="Fallen, C. F. (1817) Syrphici. In: Fallen, C. F., Diptera Sveciae. Litteris Berlingianis, Lundae [= Lund], pp. 43 - 54." type="book chapter" year="1817">Fallén, 1817</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="(Meigen, 1822)" baseAuthorityName="Meigen" baseAuthorityYear="1822" box="[199,618,1590,1616]" class="Insecta" family="Syrphidae" genus="Pipiza" kingdom="Animalia" order="Diptera" pageId="264" pageNumber="265" phylum="Arthropoda" rank="species" species="maculipennis">
<emphasis box="[199,429,1590,1616]" italics="true" pageId="264" pageNumber="265">Pipiza maculipennis</emphasis>
(
<bibRefCitation author="Meigen, J. W." box="[447,610,1590,1616]" pageId="264" pageNumber="265" pagination="22 - 32" refId="ref359320" refString="Meigen, J. W. (1822) Systematische Beschreibung der bekannten europaischen zweiflugeligen Insekten. Dritter Theil. Schultz- Wundermann, Hamm, x + 416 pp., pls. 22 - 32." type="book chapter" year="1822">Meigen, 1822</bibRefCitation>
)
</taxonomicName>
