(
<emphasis box="[675,754,1410,1436]" italics="true" pageId="356" pageNumber="357">Adenia</emphasis>
) 
<emphasis box="[772,856,1410,1436]" italics="true" pageId="356" pageNumber="357">mimula</emphasis>
