<taxonomicName class="Liliopsida" family="Poaceae" genus="Sorghum" kingdom="Plantae" lsidName="Sorghum vulgare" order="Poales" pageId="0" pageNumber="1" phylum="Tracheophyta" rank="species" species="vulgare">
