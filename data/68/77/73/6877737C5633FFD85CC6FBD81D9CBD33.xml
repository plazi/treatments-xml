<quantity box="[1329,1423,1829,1852]" metricMagnitude="2" metricUnit="m" metricValue="1.585" metricValueMax="3.17" metricValueMin="0.0" pageId="8" pageNumber="28" unit="m" value="158.5" valueMax="317.0" valueMin="0.0">0–317 m</quantity>
</keyLead>
</paragraph>
