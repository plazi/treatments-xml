<normalizedToken originalValue="Lockerblütige" pageId="null" pageNumber="632">Lockerbluetige</normalizedToken>
<taxonomicName class="Liliopsida" family="Orchidaceae" genus="Orchis" kingdom="Plantae" order="Asparagales" pageId="null" pageNumber="632" phylum="Tracheophyta" rank="genus">Orchis</taxonomicName>
