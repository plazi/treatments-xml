<taxonomicName box="[1185,1394,525,550]" class="Magnoliopsida" family="Amaranthaceae" genus="Amaranthus" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="358" phylum="Tracheophyta" rank="species" species="albus">
<emphasis box="[1185,1394,525,550]" italics="true" pageId="19" pageNumber="358">Amaranthus albus</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Amaranthaceae" genus="Amaranthus" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="358" phylum="Tracheophyta" rank="species" species="blithoides">
<emphasis italics="true" pageId="19" pageNumber="358">A. blithoides</emphasis>
</taxonomicName>
, 
<taxonomicName box="[275,424,561,586]" class="Magnoliopsida" family="Amaranthaceae" genus="Amaranthus" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="358" phylum="Tracheophyta" rank="species" species="retroflexus">
<emphasis box="[275,424,561,586]" italics="true" pageId="19" pageNumber="358">A. retroflexus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[438,619,561,586]" class="Magnoliopsida" family="Amaranthaceae" genus="Atriplex" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="358" phylum="Tracheophyta" rank="species" species="sibirica">
<emphasis box="[438,619,561,586]" italics="true" pageId="19" pageNumber="358">Atriplex sibirica</emphasis>
</taxonomicName>
, 
<taxonomicName box="[633,849,561,586]" class="Hydrozoa" family="Abylidae" genus="Bassia" kingdom="Animalia" order="Siphonophorae" pageId="19" pageNumber="358" phylum="Cnidaria" rank="species" species="hyssopifolia">
<emphasis box="[633,849,561,586]" italics="true" pageId="19" pageNumber="358">Bassia hyssopifolia</emphasis>
</taxonomicName>
, 
<taxonomicName box="[863,991,562,585]" class="Hydrozoa" family="Abylidae" genus="Bassia" kingdom="Animalia" order="Siphonophorae" pageId="19" pageNumber="358" phylum="Cnidaria" rank="species" species="scoparia">
