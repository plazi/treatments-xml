, 
<taxonomicName box="[1005,1154,561,586]" class="Magnoliopsida" family="Amaranthaceae" genus="Beta" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="358" phylum="Tracheophyta" rank="species" species="vulgaris">
<emphasis box="[1005,1154,561,586]" italics="true" pageId="19" pageNumber="358">Beta vulgaris</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1168,1436,562,585]" class="Magnoliopsida" family="Amaranthaceae" genus="Ceratocarpus" kingdom="Plantae" order="Caryophyllales" pageId="19" pageNumber="358" phylum="Tracheophyta" rank="species" species="arenarius">
<emphasis box="[1168,1436,562,585]" italics="true" pageId="19" pageNumber="358">Ceratocarpus arenarius</emphasis>
</taxonomicName>
and 
<taxonomicName box="[203,355,597,622]" class="Actinopterygii" family="Chiasmodontidae" genus="Kali" kingdom="Animalia" order="Perciformes" pageId="19" pageNumber="358" phylum="Chordata" rank="species" species="turgidus">
