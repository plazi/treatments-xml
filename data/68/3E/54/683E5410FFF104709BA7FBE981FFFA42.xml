<taxonomicName authority="(Moron)" authorityName="Moron" box="[1257,1436,1315,1337]" class="Insecta" family="Scarabaeidae" genus="Chrysina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="200" phylum="Arthropoda" rank="species" species="quiche">
