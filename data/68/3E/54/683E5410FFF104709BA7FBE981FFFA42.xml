<location LSID="urn:lsid:plazi:treatment:683E5410FFF104709BA7FBE981FFFA42:E548B3DDFFF104709834FAD987F8FA9F" box="[772,924,1373,1395]" country="Mexico" name="Eastern Oaxaca" pageId="4" pageNumber="200" stateProvince="Eastern">
