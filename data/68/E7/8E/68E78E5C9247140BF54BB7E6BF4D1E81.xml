<subSubSection pageId="14" pageNumber="15" type="type species">
<paragraph pageId="14" pageNumber="15">Type species.</paragraph>
<paragraph pageId="14" pageNumber="15">
<taxonomicName class="Arachnida" family="Leptonetidae" genus="Leptoneta" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Leptoneta coeca" order="Araneae" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="coeca">Leptoneta coeca</taxonomicName>
