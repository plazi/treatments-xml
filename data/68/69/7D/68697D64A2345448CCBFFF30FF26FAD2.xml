<number box="[326,335,1021,1045]" pageId="4" pageNumber="397" value="1.0">1</number>
♀ 
<number box="[349,363,1021,1045]" pageId="4" pageNumber="397" value="2.0">2</number>
<emphasis box="[364,406,1020,1045]" italics="true" pageId="4" pageNumber="397">♂♂</emphasis>
