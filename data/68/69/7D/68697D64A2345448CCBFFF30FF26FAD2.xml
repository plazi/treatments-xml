<quantity box="[447,530,949,973]" metricMagnitude="3" metricUnit="m" metricValue="2.123" pageId="4" pageNumber="397" unit="m" value="2123.0">2123m</quantity>
), Medog County, Xizang, 
<number box="[830,891,949,973]" pageId="4" pageNumber="397" value="2013.0">2013</number>
. 
<date box="[895,969,949,973]" pageId="4" pageNumber="397" value="1921-07">
VII. 
<number box="[941,969,949,973]" pageId="4" pageNumber="397" value="21.0">21</number>
</date>
, leg. Pan Zhaohui, gen. slideno.WU0332 (
<collectionCode box="[159,246,984,1008]" httpUri="http://grbio.org/cool/crjh-4ne1" name="Institute of Zoology, Chinese Academy of Sciences" pageId="4" pageNumber="397">IZCAS</collectionCode>
