<quantity id="332E9B1AC31AA515FDD22DACB59F04F9" box="[583,676,660,686]" metricMagnitude="-3" metricUnit="m" metricValue="4.2" pageId="4" pageNumber="67" unit="mm" value="4.2">4.2 mm</quantity>
