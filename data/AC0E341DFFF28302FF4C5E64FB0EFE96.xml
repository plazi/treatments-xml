<taxonomicName id="E3A7FE88FFF28302FF2D5E54FE4FF9B7" authority="Buchenau." authorityName="Buchenau." box="[233,479,1588,1612]" class="Magnoliopsida" family="Brassicaceae" kingdom="Plantae" order="Brassicales" pageId="79" pageNumber="79" phylum="Tracheophyta" rank="tribe" tribe="Cochlearieae">
