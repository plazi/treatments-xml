<date id="E960948880AFFF0CE96BCCD51DEB0D68" box="[1024,1206,764,788]" pageId="171" pageNumber="172" value="2014-06-07" valueMax="2014-06-28" valueMin="2014-06-07">7–28 June 2014</date>
