<taxonomicName id="5ADEC9CB80AFFF0CEEA6CCA61DFB0CD4" authority="Fowler, 1904" authorityName="Fowler" authorityYear="1904" baseAuthorityName="Forster" baseAuthorityYear="1801" box="[973,1190,655,680]" class="Actinopterygii" family="Lethrinidae" genus="Lethrinella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="171" pageNumber="172" phylum="Chordata" rank="species" species="miniata">
