<typeStatus id="1F188868FF98FF8F256F1F3EFE20F9FC" box="[353,401,1603,1627]" pageId="2" pageNumber="55">type</typeStatus>
specimen of 
<taxonomicName id="07A34D49FF98FF8F26241F39FD23F9FC" box="[554,658,1604,1627]" class="Insecta" family="Buprestidae" genus="Hylaeogena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="species" species="cincta">
