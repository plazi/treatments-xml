<taxonomicName id="07A34D49FF98FF8F20101F39FB67F9FC" box="[1054,1238,1603,1627]" class="Insecta" family="Buprestidae" genus="Hylaeogena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="55" phylum="Arthropoda" rank="species" species="circumciliata">
