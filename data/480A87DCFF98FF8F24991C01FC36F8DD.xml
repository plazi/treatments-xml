<collectionCode id="A6B2AE0FFF98FF8F21131F3FFAC5F9FD" LSID="urn:lsid:biocol.org:col:35275" box="[1309,1396,1602,1626]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35275" name="Museo Nacional de Ciencias Naturales" pageId="2" pageNumber="55">MNCN</collectionCode>
