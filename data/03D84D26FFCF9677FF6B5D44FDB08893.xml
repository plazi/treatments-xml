<geoCoordinate id="EE459AF7FFCC9677FDD95D6AFD948876" box="[512,598,344,366]" degrees="25" direction="north" minutes="36" orientation="latitude" pageId="10" pageNumber="311" precision="925" value="25.6">25°36'N</geoCoordinate>
