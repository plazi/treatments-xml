<geoCoordinate id="EE459AF7FFCC9677FDBF5D6AFD018876" box="[614,707,344,366]" degrees="109" direction="east" minutes="55" orientation="longitude" pageId="10" pageNumber="311" precision="925" value="109.916664">109°55'E</geoCoordinate>
