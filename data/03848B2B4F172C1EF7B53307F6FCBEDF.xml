4. Vein 
<emphasis id="B959E62F4F172C1DF63632B6F242BBAF" box="[276,291,2015,2036]" italics="true" pageId="33" pageNumber="98">R</emphasis>
<subScript id="17A938784F172C1DF6013284F24BBBA2" attach="left" box="[291,298,2029,2041]" fontSize="5" pageId="33" pageNumber="98">
<emphasis id="B959E62F4F172C1DF6013284F24BBBA2" box="[291,298,2029,2041]" italics="true" pageId="33" pageNumber="98">3</emphasis>
</subScript>
distinctly oblique, veins 
<emphasis id="B959E62F4F172C1DF53932B6F14BBBAF" box="[539,554,2015,2036]" italics="true" pageId="33" pageNumber="98">R</emphasis>
<emphasis id="B959E62F4F172C1DF5083284F150BBA2" box="[554,561,2029,2041]" italics="true" pageId="33" pageNumber="98">
<subScript id="17A938784F172C1DF5083284F150BBA2" attach="left" box="[554,561,2029,2041]" fontSize="5" pageId="33" pageNumber="98">4</subScript>
</emphasis>
and 
<emphasis id="B959E62F4F172C1DF54432B6F114BBAF" box="[614,629,2015,2036]" italics="true" pageId="33" pageNumber="98">R</emphasis>
<emphasis id="B959E62F4F172C1DF5563284F11ABBA2" box="[628,635,2029,2041]" italics="true" pageId="33" pageNumber="98">
<subScript id="17A938784F172C1DF5563284F11ABBA2" attach="left" box="[628,635,2029,2041]" fontSize="5" pageId="33" pageNumber="98">5</subScript>
</emphasis>
