<taxonomicName id="4C00A6FAFFAEFFA449F2FBBC8650FB89" authorityName="" authorityYear="1829" box="[760,867,1088,1112]" class="Magnoliopsida" family="Fagaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Fagales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="family">Fagaceae</taxonomicName>
