<taxonomicName id="4C00A6FAFFAEFFA44980FB98861DFBAC" box="[650,814,1124,1149]" class="Magnoliopsida" family="Aquifoliaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Celastrales" pageId="24" pageNumber="25" phylum="Tracheophyta" rank="family">Aquifoliaceae</taxonomicName>
