<taxonomicName id="8DBA73A05A4E0A1429DFCE6D7AFF0319" authorityName="Sm., Spec. bot. New Holland 15. 1793, nom. cons. (fide Shepherd et al. 2017; Applequist" authorityYear="2019" class="Magnoliopsida" family="Goodeniaceae" genus="Goodenia" higherTaxonomySource="CoL" kingdom="Plantae" lsidName="Goodenia" order="Asterales" pageId="0" pageNumber="27" phylum="Tracheophyta" rank="genus">
