<emphasis id="068DEAD0FFFCFFB361F55C00CF7CFEAE" bold="true" box="[151,830,331,358]" pageId="10" pageNumber="91">
Key to 
<taxonomicName id="F3F94D41FFFCFFB361925C07CD74FEAE" box="[240,310,332,358]" class="Insecta" family="Aphididae" genus="Aphis" kingdom="Animalia" order="Aphidomorpha" pageId="10" pageNumber="91" phylum="Arthropoda" rank="genus">
