<subSubSection id="6FEC655EDE70FFECB1C3C0CFFAF495D4" box="[203,1364,187,212]" pageId="27" pageNumber="28" type="materials_examined">
<paragraph id="274936D5DE70FFECB1C3C0CFFAF495D4" blockId="27.[151,1437,151,607]" box="[203,1364,187,212]" pageId="27" pageNumber="28">
<emphasis id="1582EAC7DE70FFECB1C3C0CFFE1495D4" bold="true" box="[203,436,187,212]" inLineHeading="true" pageId="27" pageNumber="28" reason="1">Material examined.</emphasis>
