<quantity id="4CCEE9A8FFA6D95408488B4178F9FCE2" box="[1112,1215,789,817]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" pageId="1" pageNumber="146" unit="mm" value="2.65">2.65 mm</quantity>
, TL: 
