<taxonomicName id="6EF84D5D4C20B340FF1AFF4CF6D0D5CB" authorityName="Bernhauer" authorityYear="1906" box="[230,389,152,175]" class="Insecta" family="Staphylinidae" genus="Atopocentrum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="genus">
