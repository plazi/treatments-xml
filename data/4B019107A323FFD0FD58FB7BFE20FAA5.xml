<taxonomicName id="04A85B92A32CFFD1FF7AF9BDFE06F9A4" box="[232,402,1629,1646]" class="Liliopsida" family="Orchidaceae" genus="Disperis" kingdom="Plantae" order="Asparagales" pageId="8" pageNumber="63" phylum="Tracheophyta" rank="species" species="latigaleata">
