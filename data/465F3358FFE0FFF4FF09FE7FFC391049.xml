<quantity id="090E2FABFFE0FFF4FD38FD0CFD1B11B6" box="[678,750,653,678]" metricMagnitude="2" metricUnit="m" metricValue="5.0" pageId="38" pageNumber="39" unit="m" value="500.0">500 m</quantity>
