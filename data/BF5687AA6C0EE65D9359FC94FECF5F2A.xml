<taxonomicNameLabel id="1EB857D56C0EE65D90DFFAE4FC8E5F08" box="[654,779,1345,1367]" pageId="21" pageNumber="64" rank="species">comb. nov.</taxonomicNameLabel>
(
<taxonomicName id="F0FF4D3F6C0EE65D92FDFAC5FEBE5F2A" authorityName="Gibson &amp; Skevington" authorityYear="2013" baseAuthorityName="Gibson &amp; Skevington" baseAuthorityYear="1926" box="[172,315,1376,1397]" class="Insecta" family="Conopidae" genus="Physoconops" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="64" phylum="Arthropoda" rank="genus">
<emphasis id="058BEAAE6C0EE65D92FDFAC5FEBE5F2A" box="[172,315,1376,1397]" italics="true" pageId="21" pageNumber="64">Physoconops</emphasis>
</taxonomicName>
