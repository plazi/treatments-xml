<taxonomicNameLabel id="1EB857D56C0EE65D9098FA86FF545F08" pageId="21" pageNumber="64" rank="species">comb. nov.</taxonomicNameLabel>
(
<taxonomicName id="F0FF4D3F6C0EE65D92B6FAE4FEF35F09" authorityName="Gibson &amp; Skevington" authorityYear="2013" baseAuthorityName="Gibson &amp; Skevington" baseAuthorityYear="1926" box="[231,374,1345,1366]" class="Insecta" family="Conopidae" genus="Physoconops" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="64" phylum="Arthropoda" rank="genus">
<emphasis id="058BEAAE6C0EE65D92B6FAE4FEF35F09" box="[231,374,1345,1366]" italics="true" pageId="21" pageNumber="64">Physoconops</emphasis>
</taxonomicName>
); 
<taxonomicName id="F0FF4D3F6C0EE65D93C3FAE7FD055F09" authorityName="Gibson &amp; Skevington" authorityYear="2013" box="[402,640,1345,1367]" class="Insecta" family="Conopidae" genus="Gyroconops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="64" phylum="Arthropoda" rank="species" species="sylvosus" status="comb. nov.">
