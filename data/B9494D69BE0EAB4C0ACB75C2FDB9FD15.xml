<taxonomicName id="F6E087FCBE09AB4D0B7976F5FE73F993" box="[293,467,1517,1539]" class="Actinopterygii" family="Cepolidae" genus="Owstonia" kingdom="Animalia" order="Perciformes" pageId="60" pageNumber="61" phylum="Chordata" rank="species" species="lepiota">
