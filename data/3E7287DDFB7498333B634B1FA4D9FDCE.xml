<collectingCountry id="CECC765BFB74983C3BEA4BAFA608FE64" box="[227,294,469,493]" name="Sudan" pageId="7" pageNumber="370">Sudan</collectingCountry>
, 
<collectingCountry id="CECC765BFB74983C3A264BAFA6A2FE64" box="[303,396,469,493]" name="Tanzania" pageId="7" pageNumber="370">Tansania</collectingCountry>
, 
<collectingCountry id="CECC765BFB74983C3A9E4BAFA6C5FE64" box="[407,491,469,493]" name="Uganda" pageId="7" pageNumber="370">Uganda</collectingCountry>
