<subSubSection id="D6C27702F449BC0A517BC35A5727318E" pageId="0" pageNumber="49" type="materials_examined">
<paragraph id="81903C9C283128608D29AEDA2CD5631D" pageId="0" pageNumber="49">Material.</paragraph>
<paragraph id="A7EF8251CF1642AFF1B1634D44840B21" pageId="0" pageNumber="49">
