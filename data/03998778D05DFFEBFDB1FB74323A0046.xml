<paragraph id="8B8F366ED05CFFE8FEA1F954348B05AB" blockId="9.[313,1269,1518,1733]" box="[353,1269,1708,1733]" pageId="9" pageNumber="345">
<materialsCitation id="3B583C33D05CFFE8FEA1F954348F05AB" box="[353,1265,1708,1733]" collectionCode="R, V" pageId="9" pageNumber="345" specimenCount="1" typeStatus="holotype">
<emphasis id="B944EA7CD05CFFE8FEA1F954318F05AB" box="[353,497,1709,1733]" italics="true" pageId="9" pageNumber="345">
<typeStatus id="548B88CCD05CFFE8FEA1F95431E705AB" box="[353,409,1710,1733]" pageId="9" pageNumber="345">Type</typeStatus>
