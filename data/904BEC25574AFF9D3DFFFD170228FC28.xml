<taxonomicName id="DFE226B0574AFF9D3E7DFC8404FAFCEB" box="[831,975,865,889]" class="Insecta" family="Pentatomidae" genus="Mecocephala" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="343" phylum="Arthropoda" rank="genus">
