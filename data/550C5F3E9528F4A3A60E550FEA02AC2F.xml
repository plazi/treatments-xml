<paragraph id="610676D6FE95FF308D5B6B64040720C1" pageId="111" pageNumber="212">
<taxonomicName id="92D3A448E4CA5481428F36535B483B46" authorityName="Bates" authorityYear="1891" class="Insecta" family="Scarabaeidae" genus="Cyclocephala" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyclocephala rubescens" order="Coleoptera" pageId="111" pageNumber="212" phylum="Arthropoda" rank="species" species="rubescens">
<emphasis id="0C20C6EE71A502C7798D6BD2C4021062" italics="true" pageId="111" pageNumber="212">Cyclocephala rubescens</emphasis>
</taxonomicName>
