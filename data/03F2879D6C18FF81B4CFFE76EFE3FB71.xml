<taxonomicName id="4C5B4D086C18FF81B786FB2AEF4BFB15" baseAuthorityName="Gogala" baseAuthorityYear="2014" box="[990,1097,1158,1184]" class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="62" pageNumber="63" phylum="Tracheophyta" rank="family">Fabaceae</taxonomicName>
