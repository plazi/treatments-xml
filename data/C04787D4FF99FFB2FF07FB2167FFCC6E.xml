<taxonomicName id="8FEE4D41FF99FFB2FD1CF904664FCDFA" authority="Lahille, 1905" authorityName="Lahille" authorityYear="1905" baseAuthorityName="Kaufman" baseAuthorityYear="1972" box="[652,780,1699,1725]" class="Arachnida" family="Ixodidae" genus="Aponomma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ixodida" pageId="112" pageNumber="113" phylum="Arthropoda" rank="genus">
<emphasis id="7A9AEAD0FF99FFB2FD1CF904664FCDFA" box="[652,780,1699,1725]" italics="true" pageId="112" pageNumber="113">Aponomma</emphasis>
</taxonomicName>
) and Sun 
<emphasis id="7A9AEAD0FF99FFB2FC11F90466F9CDFA" box="[897,954,1699,1725]" italics="true" pageId="112" pageNumber="113">et al.</emphasis>
