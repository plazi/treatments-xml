<taxonomicName id="8FEE4D41FF99FFB2FDA5F9D86654CDDF" authorityName="Neumann" authorityYear="1910" box="[565,791,1663,1689]" class="Arachnida" family="Ixodidae" genus="Amblyomma" kingdom="Animalia" order="Ixodida" pageId="112" pageNumber="113" phylum="Arthropoda" rank="species" species="pattoni">
<emphasis id="7A9AEAD0FF99FFB2FDA5F9D86654CDDF" box="[565,791,1663,1689]" italics="true" pageId="112" pageNumber="113">Amblyomma pattoni</emphasis>
