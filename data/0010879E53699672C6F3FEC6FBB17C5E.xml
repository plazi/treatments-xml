<taxonomicNameLabel id="A1FE57E153699671C573FE3AFBDB7F89" box="[791,1065,439,463]" pageId="13" pageNumber="62">
<collectionCode id="EEA8AE4D53699671C573FE3AFCA57F88" box="[791,855,439,462]" httpUri="http://grbio.org/cool/93c9-w2q7" name="University of Newcastle" pageId="13" pageNumber="62">NEW</collectionCode>
