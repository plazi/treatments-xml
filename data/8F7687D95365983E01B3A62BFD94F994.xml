<geoCoordinate id="62EB50085360983E03BAA03DFB80FBD4" box="[980,1049,1114,1138]" degrees="0.321" direction="west" orientation="longitude" pageId="4" pageNumber="245" precision="55" value="-0.321">-0.321</geoCoordinate>
