<geoCoordinate id="62EB50085360983E0204A01DFD30FB34" box="[618,681,1146,1170]" degrees="0.283" direction="north" orientation="latitude" pageId="4" pageNumber="245" precision="55" value="0.283">0.283</geoCoordinate>
<geoCoordinate id="62EB50085360983E02C7A01DFD77FB34" box="[681,750,1146,1170]" degrees="0.291" direction="west" orientation="longitude" pageId="4" pageNumber="245" precision="55" value="-0.291">-0.291</geoCoordinate>
, humeral width 
<geoCoordinate id="62EB50085360983E03F9A01DFC4DFB34" box="[919,980,1146,1170]" degrees="0.581" direction="north" orientation="latitude" pageId="4" pageNumber="245" precision="55" value="0.581">0.581</geoCoordinate>
