<collectingCountry id="12A64584FFF55172FC5CFB1E4850CDAA" box="[952,1110,1168,1194]" name="Sierra Leone" pageId="6" pageNumber="7">Sierra Leone</collectingCountry>
). 
<collectingCountry id="12A64584FFF55172FB8DFB1E489BCDAA" box="[1129,1181,1168,1194]" name="India" pageId="6" pageNumber="7">Indo</collectingCountry>
