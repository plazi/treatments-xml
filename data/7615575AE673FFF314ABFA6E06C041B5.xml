<taxonomicName id="39BC9DCFE670FFF31075FD8C04DD4199" authority="Klunzinger, 1871" authorityName="Klunzinger" authorityYear="1871" class="Actinopterygii" family="Fistulariidae" genus="Fistularia" kingdom="Animalia" order="Syngnathiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="villosa">
<emphasis id="CCC83A5EE670FFF31075FD8C011D417D" box="[1097,1295,583,608]" italics="true" pageId="6" pageNumber="7">Fistularia villosa</emphasis>
Klunzinger, 
<number id="4874F46BE670FFF314ABFDA704DD4199" box="[151,207,620,644]" pageId="6" pageNumber="7" value="1871.0">1871</number>
</taxonomicName>
(see 
<bibRefCitation id="9A2D9BBDE670FFF31532FDA705A74199" author="Avdeev" box="[270,437,620,645]" pageId="6" pageNumber="7" refString="Avdeev, V. V. (1979 a) New species of the genus Cymothoa Fabricius (Isopoda, Cymothoidae) from the Indian Ocean. Parazitologiya, Leningrad, 13, 223 - 234." type="book chapter" year="1979" yearSuffix="a">
Avdeev 
<number id="4874F46BE670FFF31551FDA705BA4199" box="[365,424,620,644]" pageId="6" pageNumber="7" value="1979.0">1979</number>
a
</bibRefCitation>
; 
<bibRefCitation id="9A2D9BBDE670FFF315FEFDA706BD4199" author="Williams" box="[450,687,620,645]" pageId="6" pageNumber="7" refString="Williams, E. H. Jr., Bunkley-Williams, L. &amp; Pitlik, T. (2000) Three new records for Micronesia of cymothoid isopods (Crustacea) parasitic on fishes. Pacific Science, 54 (2), 157 - 158." type="journal article" year="2000">
Williams 
<emphasis id="CCC83A5EE670FFF3160DFDA6067E4198" box="[561,620,620,645]" italics="true" pageId="6" pageNumber="7">et al.</emphasis>
<number id="4874F46BE670FFF31648FDA706BD4199" box="[628,687,620,644]" pageId="6" pageNumber="7" value="2000.0">2000</number>
</bibRefCitation>
; 
<bibRefCitation id="9A2D9BBDE670FFF31681FDA707F44199" author="Rameshkumar" box="[701,998,620,645]" pageId="6" pageNumber="7" refString="Rameshkumar, G., Ravichandran, S., Sivasubramanian, K. &amp; Trilles, J. - P. (2013) New occurrence of parasitic isopods from Indian fishes. Journal of Parasitic Diseases, 37, 42 - 46." type="journal article" year="2013">
Rameshkumar 
<emphasis id="CCC83A5EE670FFF31754FDA607B14198" box="[872,931,620,645]" italics="true" pageId="6" pageNumber="7">et al.</emphasis>
<number id="4874F46BE670FFF31790FDA707F44199" box="[940,998,620,644]" pageId="6" pageNumber="7" value="2013.0">2013</number>
</bibRefCitation>
) and bluespotted cornetfish 
<taxonomicName id="39BC9DCFE670FFF31110FDA706DC41B5" authority="Ruppell, 1838 (Williams et al. 2000)" authorityName="Ruppell, 1838 (Williams et al." authorityYear="2000" class="Actinopterygii" family="Fistulariidae" genus="Fistularia" kingdom="Animalia" order="Syngnathiformes" pageId="6" pageNumber="7" phylum="Chordata" rank="species" species="commersonii">
<emphasis id="CCC83A5EE670FFF31110FDA7053A41B5" italics="true" pageId="6" pageNumber="7">Fistularia commersonii</emphasis>
Rüppell, 
<number id="4874F46BE670FFF315A9FD5B05DF41B5" box="[405,461,656,680]" pageId="6" pageNumber="7" value="1838.0">1838</number>
(
<bibRefCitation id="9A2D9BBDE670FFF315E0FD4406D441B5" author="Williams" box="[476,710,655,680]" pageId="6" pageNumber="7" refString="Williams, E. H. Jr., Bunkley-Williams, L. &amp; Pitlik, T. (2000) Three new records for Micronesia of cymothoid isopods (Crustacea) parasitic on fishes. Pacific Science, 54 (2), 157 - 158." type="journal article" year="2000">
Williams 
<emphasis id="CCC83A5EE670FFF31676FD5A069041B5" box="[586,642,655,680]" italics="true" pageId="6" pageNumber="7">et al.</emphasis>
<number id="4874F46BE670FFF316B5FD5B06D441B5" box="[649,710,656,680]" pageId="6" pageNumber="7" value="2000.0">2000</number>
</bibRefCitation>
)
</taxonomicName>
.
</paragraph>
</subSubSection>
