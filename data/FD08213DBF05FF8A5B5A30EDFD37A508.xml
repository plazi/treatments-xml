<taxonomicName id="B2A1EBA8BF06FF8B5B803688FCADA669" authority="Boisduval, 1835" authorityName="Boisduval" authorityYear="1835" box="[466,862,1016,1040]" class="Insecta" family="Phasmatidae" genus="Eurycantha" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="horrida">
