<taxonomicName id="B2A1EBA8BF06FF8B5B783111FE14A611" box="[298,487,1120,1143]" class="Insecta" family="Heteropterygidae" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="33" phylum="Arthropoda" rank="family">Heteropterygidae</taxonomicName>
