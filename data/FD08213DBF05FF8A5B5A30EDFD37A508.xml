<taxonomicName id="B2A1EBA8BF06FF8B5BA33110FC6DA611" authority="Parkinson, 1789" authorityName="Parkinson" authorityYear="1789" box="[497,926,1120,1144]" class="Insecta" family="Heteropterygidae" genus="Heteropteryx" kingdom="Animalia" order="Phasmida" pageId="2" pageNumber="33" phylum="Arthropoda" rank="species" species="dilatata">
<emphasis id="47D54C39BF06FF8B5BA33110FD25A61E" box="[497,726,1121,1144]" italics="true" pageId="2" pageNumber="33">Heteropteryx dilatata</emphasis>
(Parkinson, 
<number id="C369820CBF06FF8B590F3111FC65A611" box="[861,918,1120,1143]" pageId="2" pageNumber="33" value="1789.0">1789</number>
)
</taxonomicName>
