<taxonomicName id="D79309B06E9D39FEDEA3E71A259B6587" authority="Arrow, 1908" authorityName="Arrow" authorityYear="1908" class="Insecta" family="Scarabaeidae" genus="Ruteloryctes" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Ruteloryctes" order="Coleoptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="genus">
