<collectingCountry id="F3277653FFC32F02FF6EFB578CCA652A" box="[175,254,1277,1299]" name="Mexico" pageId="34" pageNumber="157">Mexico</collectingCountry>
, 
<collectingCountry id="F3277653FFC32F02FECBFB578D43652A" box="[266,375,1277,1299]" name="Nicaragua" pageId="34" pageNumber="157">Nicaragua</collectingCountry>
, 
<collectingCountry id="F3277653FFC32F02FE42FB578DDA652A" box="[387,494,1277,1299]" name="Venezuela" pageId="34" pageNumber="157">Venezuela</collectingCountry>
, and the 
<collectingCountry id="F3277653FFC32F02FD8EFB578EEF652A" box="[591,731,1277,1299]" name="United States of America" pageId="34" pageNumber="157">United States</collectingCountry>
