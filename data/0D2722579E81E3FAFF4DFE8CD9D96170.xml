<caption id="D1F1C3C99E81E3FBFF4DF950D836658E" pageId="228" pageNumber="229" startId="228.[151,250,1705,1728]" targetBox="[362,1227,644,1679]" targetPageId="228">
<paragraph id="853193419E81E3FBFF4DF950D836658E" blockId="228.[151,1436,1705,1759]" pageId="228" pageNumber="229">
<emphasis id="B7FA4F539E81E3FBFF4DF950DA106590" bold="true" box="[151,300,1705,1728]" pageId="228" pageNumber="229">FIGURE 193.</emphasis>
<taxonomicName id="428EE8C29E81E3FBFEE8F953D9856590" box="[306,697,1706,1728]" class="Insecta" family="Braconidae" genus="Aleiodes" kingdom="Animalia" order="Hymenoptera" pageId="228" pageNumber="229" phylum="Arthropoda" rank="species" species="vanachterbergi" status="sp. nov." subGenus="Arcaleiodes">
