<taxonomicName id="70CA4D3CFF9CE2388609FEF4DEAB6469" box="[838,933,295,320]" class="Hydrozoa" family="Rhodaliidae" genus="Rhodalia" kingdom="Animalia" order="Siphonophorae" pageId="113" pageNumber="176" phylum="Cnidaria" rank="species" species="miranda">
