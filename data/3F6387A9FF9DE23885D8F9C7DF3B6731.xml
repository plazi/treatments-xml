<taxonomicName id="70CA4D3CFF9DE2398751F9ACDEAA63B1" authority="Milstein, 1976" authorityName="Milstein" authorityYear="1976" box="[542,932,1663,1688]" class="Hydrozoa" family="Plumulariidae" genus="Plumularia" kingdom="Animalia" order="Leptothecata" pageId="112" pageNumber="113" phylum="Cnidaria" rank="species" species="spiralis">
