<taxonomicName id="AA934D6C5400FF90FB0274BFFAE7FC19" box="[1184,1387,905,928]" class="Insecta" family="Blaberidae" genus="Audreia" kingdom="Animalia" order="Blattodea" pageId="5" pageNumber="144" phylum="Arthropoda" rank="species" species="carinulata">
