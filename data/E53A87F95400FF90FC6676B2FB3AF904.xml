and 
<taxonomicName id="AA934D6C5400FF90FAB4747FFC53FC3B" authority="Shelford, 1910" authorityName="Shelford" authorityYear="1910" class="Insecta" family="Blaberidae" genus="Audreia" kingdom="Animalia" order="Blattodea" pageId="5" pageNumber="144" phylum="Arthropoda" rank="species" species="catharina">
