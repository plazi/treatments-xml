<normalizedToken id="545D2D1D465EA251C879810334408158" originalValue="nátans" pageId="null" pageNumber="195">natans</normalizedToken>
<authorityName id="CFD83C4D735CD1955FD28816116731BC" pageId="null" pageNumber="195">L.</authorityName>
