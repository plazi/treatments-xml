<taxonomicName id="13AD8C8CFF827C7209A2DFE8FCA4F966" authorityName="Schmutz" authorityYear="1913" box="[662,834,1726,1750]" class="Insecta" family="Thripidae" genus="Thrips" kingdom="Animalia" order="Thysanoptera" pageId="2" pageNumber="241" phylum="Arthropoda" rank="species" species="coloratus">
<emphasis id="E6D92B1DFF827C7209A2DFE8FCA4F966" bold="true" box="[662,834,1726,1750]" italics="true" pageId="2" pageNumber="241">Thrips coloratus</emphasis>
