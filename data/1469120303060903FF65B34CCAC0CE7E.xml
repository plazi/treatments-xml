<taxonomicName id="5BC0D89603010903FE59B59FCA55CDD2" authorityName="Pollet" authorityYear="2005" box="[404,532,1485,1506]" class="Insecta" family="Dolichopodidae" genus="Australachalcus" kingdom="Animalia" order="Diptera" pageId="37" pageNumber="64" phylum="Arthropoda" rank="species" species="robustus">
