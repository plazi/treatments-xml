<taxonomicName id="1EB94D5EFFA1270BFE65634DFC3EFA8A" authority="Hardy, 1944" authorityName="Hardy" authorityYear="1944" box="[416,963,1364,1404]" class="Insecta" family="Mydidae" genus="Heteromydas" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="86" phylum="Arthropoda" rank="genus">
<taxonomicName id="1EB94D5EFFA1270BFE65634DFCBDFA8A" authority="Hardy" authorityName="Hardy" box="[416,832,1364,1404]" class="Insecta" family="Mydidae" genus="Heteromydas" higherTaxonomySource="CoL" kingdom="Animalia" order="Diptera" pageId="47" pageNumber="86" phylum="Arthropoda" rank="genus">
