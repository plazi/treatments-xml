<taxonomicName id="48E048BAFFDB60096B31CE3ECA67AF2C" box="[321,579,1299,1324]" class="Insecta" family="Ecnomidae" genus="Ecnomina" kingdom="Animalia" order="Trichoptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="mesembria">
