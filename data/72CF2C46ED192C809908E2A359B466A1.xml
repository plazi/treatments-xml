<taxonomicName id="D7EE9482EC10260606F771B3506B80C4" class="Reptilia" family="Gekkonidae" genus="Cyrtopodion" higherTaxonomySource="CoL" kingdom="Animalia" lsidName="Cyrtopodion scabrum" order="Squamata" pageId="1" pageNumber="40" phylum="Chordata" rank="species" species="scabrum">Cyrtopodion scabrum</taxonomicName>
