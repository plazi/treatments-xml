<taxonomicName id="88B5BEBDFFE3FFCDF47C643EFE3FCB09" box="[297,500,1066,1090]" class="Magnoliopsida" family="Lauraceae" genus="Ocotea" kingdom="Plantae" order="Laurales" pageId="7" pageNumber="242" phylum="Tracheophyta" rank="species" species="auriculiformis">
