<paragraph id="347365DE53781A89FBC9DD6584C90E28" pageId="1" pageNumber="34">
<taxonomicName id="DA21DE9F115E50FCB373CF0A2CA9378E" authority="Carlgren, 1928" authorityName="Carlgren" authorityYear="1928" class="Anthozoa" family="Actiniidae" genus="Isadamsia" higherTaxonomySource="GBIF" kingdom="Animalia" lsidName="Isadamsia" order="Actiniaria" pageId="1" pageNumber="34" phylum="Cnidaria" rank="genus">Isadamsia</taxonomicName>
