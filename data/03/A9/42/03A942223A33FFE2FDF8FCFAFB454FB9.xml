<paragraph blockId="7.[189,335,1395,1421]" box="[189,335,1395,1421]" pageId="7" pageNumber="24">
<heading bold="true" box="[189,335,1395,1421]" fontSize="11" level="3" pageId="7" pageNumber="24" reason="3">
<emphasis bold="true" box="[189,335,1395,1421]" pageId="7" pageNumber="24">Description</emphasis>
