<taxonomicName authorityName="Filipjev" authorityYear="1927" box="[404,578,365,392]" class="Adenophorea" family="Thoracostomopsidae" genus="Mesacanthion" kingdom="Animalia" order="Enoplida" pageId="13" pageNumber="30" phylum="Nematoda" rank="genus">
