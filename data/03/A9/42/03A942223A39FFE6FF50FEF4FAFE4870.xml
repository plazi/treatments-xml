<taxonomicName authorityName="Filipjev" authorityYear="1927" box="[1200,1368,447,473]" class="Adenophorea" family="Thoracostomopsidae" genus="Mesacanthion" kingdom="Animalia" order="Enoplida" pageId="13" pageNumber="30" phylum="Nematoda" rank="genus">
<emphasis box="[1200,1368,447,473]" italics="true" pageId="13" pageNumber="30">Mesacanthion</emphasis>
</taxonomicName>
