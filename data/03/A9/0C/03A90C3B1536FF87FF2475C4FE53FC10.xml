<taxonomicName authorityName="Verhoeff" authorityYear="1895" box="[606,692,838,863]" class="Diplopoda" family="Haaseidae" genus="Haasea" kingdom="Animalia" order="Chordeumatida" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
