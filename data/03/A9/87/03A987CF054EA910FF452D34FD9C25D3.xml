<subSubSection box="[151,1248,597,622]" pageId="10" pageNumber="11" type="etymology">
<paragraph blockId="10.[151,1437,597,766]" box="[151,1248,597,622]" pageId="10" pageNumber="11">
<emphasis bold="true" box="[151,286,597,622]" pageId="10" pageNumber="11">Etymology.</emphasis>
