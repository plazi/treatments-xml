<taxonomicName authority=", Kucukcekmece" authorityName="Kucukcekmece" box="[210,554,1985,2003]" class="Mammalia" family="Equidae" genus="Hipparion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="6" pageNumber="231" phylum="Chordata" rank="species" species="giganteum">
