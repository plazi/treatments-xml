<taxonomicName authority="(WESMAEL, 1849)" baseAuthorityName="WESMAEL" baseAuthorityYear="1849" box="[358,703,1254,1278]" class="Insecta" family="Ichneumonidae" genus="Habronyx" kingdom="Animalia" order="Hymenoptera" pageId="4" pageNumber="253" phylum="Arthropoda" rank="species" species="heros">
