</collectingCountry>
(
<collectingRegion box="[587,651,188,213]" country="Angola" name="Zaire" pageId="30" pageNumber="31">
<collectingCountry box="[587,651,188,213]" name="Democratic Republic of the Congo" pageId="30" pageNumber="31">Zaire</collectingCountry>
</collectingRegion>
), 
<collectingCountry box="[671,759,188,213]" name="Rwanda" pageId="30" pageNumber="31">Ruanda</collectingCountry>
, 
<collectingCountry box="[770,857,188,213]" name="Uganda" pageId="30" pageNumber="31">Uganda</collectingCountry>
and 
