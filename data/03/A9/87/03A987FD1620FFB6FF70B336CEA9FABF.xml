<subSubSection pageId="25" pageNumber="476" type="biology_ecology">
<paragraph blockId="25.[151,1437,151,1365]" pageId="25" pageNumber="476">
<emphasis bold="true" box="[199,295,1231,1256]" pageId="25" pageNumber="476">Biology.</emphasis>
