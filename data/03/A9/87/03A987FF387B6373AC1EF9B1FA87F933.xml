<taxonomicName authority="Berlese, 1885" authorityName="Berlese" authorityYear="1885" box="[321,699,1599,1623]" class="Arachnida" family="Caligonellidae" genus="Caligonella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="3" pageNumber="89" phylum="Arthropoda" rank="species" species="robustus">
