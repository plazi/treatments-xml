<taxonomicName authorityName="Miyatake" authorityYear="1966" box="[284,388,1901,1925]" class="Insecta" family="Coccinellidae" genus="Nesolotis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
