and 
<taxonomicName authorityName="Hoang" authorityYear="1982" box="[445,601,1901,1925]" class="Insecta" family="Coccinellidae" genus="Paranesolotis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="genus">
