<taxonomicName authorityName="Reuscher, Fiege &amp; Imajima" authorityYear="2015" box="[987,1104,437,464]" class="Polychaeta" family="Ampharetidae" genus="Amage" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="ehlersi">
<emphasis box="[987,1104,437,464]" italics="true" pageId="4" pageNumber="5">A. ehlersi</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Moore" authorityYear="1923" box="[1170,1292,438,464]" class="Polychaeta" family="Ampharetidae" genus="Amage" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="scutata">
<emphasis box="[1170,1292,438,464]" italics="true" pageId="4" pageNumber="5">A. scutata</emphasis>
</taxonomicName>
have 11 thoracic uncinigers, 
<taxonomicName baseAuthorityName="Grube" baseAuthorityYear="1863" box="[436,620,472,499]" class="Polychaeta" family="Ampharetidae" genus="Amage" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="adspersa">
<emphasis box="[436,461,473,499]" italics="true" pageId="4" pageNumber="5">A.</emphasis>
cf. 
<emphasis box="[514,620,472,498]" italics="true" pageId="4" pageNumber="5">adspersa</emphasis>
</taxonomicName>
has 14 thoracic uncinigers. 
<taxonomicName baseAuthorityName="Grube" baseAuthorityYear="1863" box="[969,1153,472,499]" class="Polychaeta" family="Ampharetidae" genus="Amage" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="adspersa">
<emphasis box="[969,995,473,499]" italics="true" pageId="4" pageNumber="5">A.</emphasis>
cf. 
<emphasis box="[1047,1153,472,498]" italics="true" pageId="4" pageNumber="5">adspersa</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Malmgren" authorityYear="1866" box="[1172,1308,472,499]" class="Polychaeta" family="Ampharetidae" genus="Amage" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="auricula">
<emphasis box="[1172,1308,472,499]" italics="true" pageId="4" pageNumber="5">A. auricula</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Chamberlin" baseAuthorityYear="1919" class="Polychaeta" family="Ampharetidae" genus="Amage" kingdom="Animalia" order="Terebellida" pageId="4" pageNumber="5" phylum="Annelida" rank="species" species="delus">
<emphasis italics="true" pageId="4" pageNumber="5">A. delus</emphasis>
</taxonomicName>
