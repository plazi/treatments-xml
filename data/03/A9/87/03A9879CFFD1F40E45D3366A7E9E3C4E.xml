<subSubSection pageId="47" pageNumber="48" type="diagnosis">
<paragraph blockId="47.[151,1437,1028,2027]" pageId="47" pageNumber="48">
<emphasis bold="true" box="[199,321,1495,1520]" pageId="47" pageNumber="48">Diagnosis.</emphasis>
