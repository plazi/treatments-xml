<emphasis bold="true" box="[199,352,295,320]" pageId="29" pageNumber="30">Distribution.</emphasis>
S 
<collectingCountry box="[381,595,295,320]" name="Republic of the Congo" pageId="29" pageNumber="30">
Republic of 
<collectingRegion box="[520,595,296,320]" country="Republic of the Congo" name="Bas-Congo" pageId="29" pageNumber="30">Congo</collectingRegion>
</collectingCountry>
(
<collectingRegion box="[609,671,295,320]" country="Angola" name="Zaire" pageId="29" pageNumber="30">
<collectingCountry box="[609,671,295,320]" name="Democratic Republic of the Congo" pageId="29" pageNumber="30">Zaire</collectingCountry>
