<number box="[389,417,152,176]" pageId="7" pageNumber="135" value="19.0">19</number>
pairs of rather short (
<emphasis box="[666,678,153,176]" italics="true" pageId="7" pageNumber="135">c</emphasis>
<number box="[678,686,166,180]" pageId="7" pageNumber="135" value="1.0">1</number>
/ 
<emphasis box="[694,706,153,176]" italics="true" pageId="7" pageNumber="135">c</emphasis>
<number box="[706,714,166,180]" pageId="7" pageNumber="135" value="1.0">1</number>
