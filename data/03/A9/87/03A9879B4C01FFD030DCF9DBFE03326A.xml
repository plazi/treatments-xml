<number box="[648,656,205,219]" pageId="7" pageNumber="135" value="2.0">2</number>
remote from anterior notogastral border, setae 
<emphasis box="[1193,1205,192,215]" italics="true" pageId="7" pageNumber="135">c</emphasis>
<number box="[1206,1214,205,219]" pageId="7" pageNumber="135" value="2.0">2</number>
