<subSubSection pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[151,1437,258,1398]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[199,334,1338,1363]" pageId="21" pageNumber="22">Etymology.</emphasis>
