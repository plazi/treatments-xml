<collectionCode box="[882,978,924,950]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="34" pageNumber="35">MNHN</collectionCode>
