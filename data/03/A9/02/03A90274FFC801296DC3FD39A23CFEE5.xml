<paragraph blockId="6.[173,660,1567,1729]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[197,256,1567,1586]" pageId="6" pageNumber="7">Fig. 5.</emphasis>
<taxonomicName baseAuthorityName="Casey" baseAuthorityYear="1893" box="[278,499,1567,1586]" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbilicatus">
<emphasis box="[278,499,1567,1586]" italics="true" pageId="6" pageNumber="7">Margarinotus umbilicatus</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Casey" baseAuthorityYear="1893" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="umbrosus">
<emphasis italics="true" pageId="6" pageNumber="7">Margarinotus umbrosus</emphasis>
