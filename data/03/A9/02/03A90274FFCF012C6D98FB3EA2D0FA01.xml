<emphasis bold="true" box="[185,440,1159,1182]" pageId="1" pageNumber="2">KEY TO THE SPECIES OF</emphasis>
<taxonomicName authorityName="MARSEUL" authorityYear="1853" box="[449,604,1159,1182]" class="Insecta" family="Histeridae" genus="Margarinotus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="2" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[449,604,1159,1182]" italics="true" pageId="1" pageNumber="2">MARGARINOTUS</emphasis>
</taxonomicName>
</heading>
</paragraph>
<paragraph blockId="1.[185,604,1159,1235]" pageId="1" pageNumber="2">
<emphasis bold="true" box="[235,352,1186,1209]" pageId="1" pageNumber="2">KNOWN OR</emphasis>
<emphasis bold="true" box="[361,475,1186,1209]" pageId="1" pageNumber="2">SUSPECTED</emphasis>
<emphasis bold="true" box="[483,494,1186,1209]" pageId="1" pageNumber="2">(</emphasis>
†
<emphasis bold="true" pageId="1" pageNumber="2">) TO OCCUR IN</emphasis>
<emphasis bold="true" box="[387,514,1212,1235]" pageId="1" pageNumber="2">
<collectingRegion box="[387,514,1212,1235]" country="United States of America" name="California" pageId="1" pageNumber="2">CALIFORNIA</collectingRegion>
</emphasis>
</paragraph>
