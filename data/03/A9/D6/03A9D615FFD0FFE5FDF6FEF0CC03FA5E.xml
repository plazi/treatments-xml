<taxonomicName authority="Daday, 1905" authorityName="Daday" authorityYear="1905" box="[645,1048,926,953]" class="Ostracoda" family="Candonidae" genus="Candonopsis" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Podocopida" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="anisitsi">
