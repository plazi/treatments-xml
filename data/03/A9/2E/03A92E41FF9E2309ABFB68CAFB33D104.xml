<location LSID="urn:lsid:plazi:treatment:03A92E41FF9E2309ABFB68CAFB33D104:8EDFC98CFF9E2309AA1E6885FE4BD402" box="[333,427,1789,1813]" country="Ecuador" municipality="Playas Canton" name="Engabao" pageId="5" pageNumber="670" stateProvince="Guayas">Engabao</location>
