<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[824,906,592,615]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="1" pageNumber="64">MNHN</collectionCode>
