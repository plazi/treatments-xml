<subSubSection pageId="1" pageNumber="499" type="type_taxon">
<paragraph blockId="1.[173,660,884,1729]" pageId="1" pageNumber="499">
<emphasis bold="true" box="[197,333,885,906]" pageId="1" pageNumber="499">
<typeStatus box="[197,245,885,906]" pageId="1" pageNumber="499">Type</typeStatus>
Species.
</emphasis>
<taxonomicName authority="Miller and Short" authorityName="Miller and Short" authorityYear="2015" class="Insecta" family="Dytiscidae" genus="Belladessus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="499" phylum="Arthropoda" rank="species" species="femineus" status="new species">
<emphasis box="[347,550,884,906]" italics="true" pageId="1" pageNumber="499">Belladessus femineus</emphasis>
Miller and Short
