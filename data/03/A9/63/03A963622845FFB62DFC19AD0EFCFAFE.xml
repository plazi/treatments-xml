<taxonomicName authorityName="Antić &amp; Reip" authorityYear="2020" baseAuthorityName="Lohmander" baseAuthorityYear="1936" box="[856,1004,1235,1261]" class="Diplopoda" family="Julidae" genus="Leucogeorgia" kingdom="Animalia" order="Julida" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="abchasica">
<emphasis box="[856,1004,1235,1261]" italics="true" pageId="27" pageNumber="28">L. abchasica</emphasis>
</taxonomicName>
and an unidentified 
<taxonomicName authorityName="Verhoeff" authorityYear="1930" box="[1237,1399,1236,1261]" class="Diplopoda" family="Julidae" genus="Leucogeorgia" kingdom="Animalia" order="Julida" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
<emphasis box="[1237,1399,1236,1261]" italics="true" pageId="27" pageNumber="28">Leucogeorgia</emphasis>
</taxonomicName>
with modified mouthparts.
</paragraph>
