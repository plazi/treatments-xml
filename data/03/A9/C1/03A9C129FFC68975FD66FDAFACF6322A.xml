<taxonomicName authority="PEIRCE &amp; JAKOB-HOFF &amp; TWENTYMAN, 2003" authorityName="PEIRCE &amp; JAKOB-HOFF &amp; TWENTYMAN" authorityYear="2003" box="[1057,1232,1565,1586]" class="Aves" family="Apterygidae" genus="Hepatozoon" kingdom="Animalia" order="Apterygiformes" pageId="4" pageNumber="1800" phylum="Chordata" rank="species" species="kiwii" status="sp. nov.">
<emphasis box="[1057,1232,1565,1586]" italics="true" pageId="4" pageNumber="1800">Hepatozoon kiwii</emphasis>
</taxonomicName>
<taxonomicNameLabel pageId="4" pageNumber="1800" rank="species">sp. nov.</taxonomicNameLabel>
from 
<taxonomicName box="[491,749,1591,1613]" class="Aves" family="Apterygidae" genus="Apteryx" kingdom="Animalia" order="Apterygiformes" pageId="4" pageNumber="1800" phylum="Chordata" rank="subSpecies" species="australis" subSpecies="mantelli">
