), AZ, CO, CT, DC, FL, GA, IA (
<bibRefCitation author="Freese, E. L." box="[1071,1212,1195,1221]" pageId="7" pageNumber="8" pagination="78 - 80" refId="ref49425" refString="Freese, E. L. (2014) New state records for Perillus strigipes (Heteroptera: Pentatomidae: Asopinae). The Great Lakes Entomologist, 47 (1 - 2), 78 - 80." type="journal article" year="2014">Freese 2014</bibRefCitation>
), IL, IN, KS (
<bibRefCitation author="Freese, E. L." pageId="7" pageNumber="8" pagination="78 - 80" refId="ref49425" refString="Freese, E. L. (2014) New state records for Perillus strigipes (Heteroptera: Pentatomidae: Asopinae). The Great Lakes Entomologist, 47 (1 - 2), 78 - 80." type="journal article" year="2014">Freese 2014</bibRefCitation>
), KY, LA (
<bibRefCitation author="Freese, E. L." box="[329,470,1231,1257]" pageId="7" pageNumber="8" pagination="78 - 80" refId="ref49425" refString="Freese, E. L. (2014) New state records for Perillus strigipes (Heteroptera: Pentatomidae: Asopinae). The Great Lakes Entomologist, 47 (1 - 2), 78 - 80." type="journal article" year="2014">Freese 2014</bibRefCitation>
), MA, MD, ME (
<bibRefCitation author="Freese, E. L." box="[661,802,1231,1257]" pageId="7" pageNumber="8" pagination="78 - 80" refId="ref49425" refString="Freese, E. L. (2014) New state records for Perillus strigipes (Heteroptera: Pentatomidae: Asopinae). The Great Lakes Entomologist, 47 (1 - 2), 78 - 80." type="journal article" year="2014">Freese 2014</bibRefCitation>
), MO, MS (
<bibRefCitation author="Freese, E. L." box="[933,1074,1231,1257]" pageId="7" pageNumber="8" pagination="78 - 80" refId="ref49425" refString="Freese, E. L. (2014) New state records for Perillus strigipes (Heteroptera: Pentatomidae: Asopinae). The Great Lakes Entomologist, 47 (1 - 2), 78 - 80." type="journal article" year="2014">Freese 2014</bibRefCitation>
