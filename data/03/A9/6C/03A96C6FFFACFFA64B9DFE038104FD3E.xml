<taxonomicName authorityName="Crawford" authorityYear="1912" box="[474,646,692,717]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="fletcheri">
