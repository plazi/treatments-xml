<collectingCountry box="[311,373,1542,1567]" name="India" pageId="22" pageNumber="23">India</collectingCountry>
: 
<collectingRegion box="[383,525,1542,1567]" country="India" name="West Bengal" pageId="22" pageNumber="23">West Bengal</collectingRegion>
(
<bibRefCitation box="[539,689,1542,1567]" pageId="22" pageNumber="23" refId="ref20413" refString="Kieffer, J. J. (1905) Etude sur de nouveaux insectes et phytoptides gallicoles du Bengale. Annales de la Societe Scientifique de Bruxelles, 29, 143 - 200." type="journal article">Kieffer 1905</bibRefCitation>
