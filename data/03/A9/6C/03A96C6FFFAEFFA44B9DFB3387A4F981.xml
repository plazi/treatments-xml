<collectingCountry box="[318,380,1303,1328]" name="India" pageId="24" pageNumber="25">India</collectingCountry>
: 
<collectingRegion box="[397,522,1303,1328]" country="India" name="Meghalaya" pageId="24" pageNumber="25">Meghalaya</collectingRegion>
(
<bibRefCitation box="[543,1128,1303,1328]" pageId="24" pageNumber="25" refId="ref20621" refString="Lahiri, A. R. &amp; Biswas, S. (1979) On a collection of psyllids (Homoptera: Psyllidae) from Shillong, Khasi Hills. Bulletin of the Zoological Survey of India, 2, 61 - 67." type="journal article">
Lahiri &amp; Biswas 1979, as 
<taxonomicName authorityName="Mathur" authorityYear="1975" box="[871,1128,1305,1328]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="subSpecies" species="gigantea" subSpecies="curta">
<emphasis box="[871,1128,1305,1328]" italics="true" pageId="24" pageNumber="25">Trioza gigantea curta</emphasis>
</taxonomicName>
</bibRefCitation>
), 
<collectingRegion box="[1158,1298,1303,1328]" country="India" name="Tamil Nadu" pageId="24" pageNumber="25">Tamil Nadu</collectingRegion>
(
<bibRefCitation pageId="24" pageNumber="25" refId="ref22275" refString="Raman, A. &amp; Kandasamy, C. (1978) Leaf galls on Symplocos spicata Roxb. (Symplococeae) caused by Trioza sp. (Homoptera: Psyllidae). Current Science, 47, 127 - 128." type="journal article">Raman &amp; Kandasamy 1978</bibRefCitation>
, as 
<taxonomicName box="[391,461,1341,1364]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="genus">
<emphasis box="[391,461,1341,1364]" italics="true" pageId="24" pageNumber="25">Trioza</emphasis>
</taxonomicName>
sp.; 
<bibRefCitation box="[517,715,1340,1365]" pageId="24" pageNumber="25" refId="ref20263" refString="Kandasamy, C. (1980) A new leaf gall by Trioza gigantea Craw. (Homoptera: Psyllidae) on Faccinium neilgherrense W. (Vacciniaceae). Current Science, 49, 754." type="journal article">Kandasamy 1980</bibRefCitation>
, 
<bibRefCitation box="[726,785,1340,1365]" pageId="24" pageNumber="25" refId="ref20300" refString="Kandasamy, C. (1986) Taxonomy of South Indian psyllids. Records of the Zoological Survey of India, 84, 1 - 111." type="journal article">1986</bibRefCitation>
, as 
<taxonomicName authorityName="Mathur" authorityYear="1975" box="[827,1070,1341,1364]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="subSpecies" species="gigantea" subSpecies="curta">
<emphasis box="[827,1070,1341,1364]" italics="true" pageId="24" pageNumber="25">Trioza gigantea curta</emphasis>
</taxonomicName>
), 
<collectingRegion box="[1094,1235,1340,1365]" country="India" name="West Bengal" pageId="24" pageNumber="25">West Bengal</collectingRegion>
(
<bibRefCitation pageId="24" pageNumber="25" refId="ref21531" refString="Mathur, R. N. (1975) Psyllidae of the Indian subcontinent. Indian Council of Agricultural Research, New Delhi, 429 pp." type="book">
Mathur 1975, as 
<taxonomicName authorityName="Mathur" authorityYear="1975" box="[151,393,1377,1400]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="24" pageNumber="25" phylum="Arthropoda" rank="subSpecies" species="gigantea" subSpecies="curta">
<emphasis box="[151,393,1377,1400]" italics="true" pageId="24" pageNumber="25">Trioza gigantea curta</emphasis>
</taxonomicName>
</bibRefCitation>
