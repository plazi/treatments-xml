<taxonomicName authorityName="Mathur" authorityYear="1975" box="[505,665,1434,1459]" class="Insecta" family="Psyllidae" genus="Psylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="15" pageNumber="16" phylum="Arthropoda" rank="species" species="hyalina">
