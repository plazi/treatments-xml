</taxonomicName>
and 
<taxonomicName authority="(Cotes, 1893)" authorityName="Burckhardt &amp; Sharma &amp; Raman" authorityYear="2018" baseAuthorityName="Cotes" baseAuthorityYear="1893" box="[668,1050,1661,1686]" class="Insecta" family="Psyllidae" genus="Euphaleropsis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="species" species="isitis" status="comb. nov.">
<emphasis box="[668,889,1661,1686]" italics="true" pageId="16" pageNumber="17">Euphaleropsis isitis</emphasis>
(
<bibRefCitation box="[903,1041,1662,1686]" pageId="16" pageNumber="17" refId="ref18733" refString="Cotes, E. C. (1893) Psylla isitis sp. n. Indian Museum Notes, 2, 18." type="journal article">Cotes, 1893</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicNameLabel box="[1062,1190,1661,1686]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[1062,1190,1661,1686]" pageId="16" pageNumber="17">comb. nov.</emphasis>
</taxonomicNameLabel>
from 
<taxonomicName box="[1259,1329,1661,1686]" class="Insecta" family="Psyllidae" genus="Psylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="16" pageNumber="17" phylum="Arthropoda" rank="genus">
