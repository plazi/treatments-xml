<subSubSection pageId="8" pageNumber="9" type="biology_ecology">
<paragraph blockId="8.[151,1436,1230,1363]" pageId="8" pageNumber="9">
<emphasis bold="true" box="[199,326,1266,1291]" pageId="8" pageNumber="9">Host plant</emphasis>
