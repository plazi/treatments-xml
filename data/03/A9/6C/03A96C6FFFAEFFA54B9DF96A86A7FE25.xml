(
<bibRefCitation box="[360,516,476,501]" pageId="25" pageNumber="26" refId="ref21531" refString="Mathur, R. N. (1975) Psyllidae of the Indian subcontinent. Indian Council of Agricultural Research, New Delhi, 429 pp." type="book">Mathur, 1975</bibRefCitation>
)
</taxonomicName>
, 
<emphasis bold="true" box="[536,587,476,501]" pageId="25" pageNumber="26">stat.</emphasis>
and 
<emphasis bold="true" box="[641,769,476,501]" pageId="25" pageNumber="26">comb. nov.</emphasis>
from 
<taxonomicName box="[838,908,477,500]" class="Insecta" family="Psyllidae" genus="Trioza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
<emphasis box="[838,908,477,500]" italics="true" pageId="25" pageNumber="26">Trioza</emphasis>
