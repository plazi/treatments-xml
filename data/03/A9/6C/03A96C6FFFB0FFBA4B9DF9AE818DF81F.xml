<taxonomicName box="[337,485,1829,1854]" class="Magnoliopsida" family="Meliaceae" genus="Toona" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="ciliata">
<emphasis box="[337,485,1829,1854]" italics="true" pageId="6" pageNumber="7">Toona ciliata</emphasis>
</taxonomicName>
(
<taxonomicName box="[499,621,1829,1854]" class="Magnoliopsida" family="Meliaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="6" pageNumber="7" phylum="Magnoliopsida" rank="family">Meliaceae</taxonomicName>
); the host record of 
<taxonomicName authorityName="A. Juss" authorityYear="1830" box="[841,1066,1829,1854]" class="Magnoliopsida" family="Meliaceae" genus="Chukrasia" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="6" pageNumber="7" phylum="Tracheophyta" rank="species" species="tabularis">
<emphasis box="[841,1066,1829,1854]" italics="true" pageId="6" pageNumber="7">Chukrasia tabularis</emphasis>
</taxonomicName>
(
<taxonomicName box="[1079,1198,1829,1854]" class="Magnoliopsida" family="Meliaceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="6" pageNumber="7" phylum="Magnoliopsida" rank="family">Meliaceae</taxonomicName>
) by 
<bibRefCitation box="[1247,1410,1829,1854]" pageId="6" pageNumber="7" refId="ref21531" refString="Mathur, R. N. (1975) Psyllidae of the Indian subcontinent. Indian Council of Agricultural Research, New Delhi, 429 pp." type="book">Mathur (1975)</bibRefCitation>
