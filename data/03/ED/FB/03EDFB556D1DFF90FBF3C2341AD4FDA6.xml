(
<emphasis box="[1065,1078,295,318]" italics="true" pageId="3" pageNumber="588">S</emphasis>
.) 
<emphasis box="[1102,1194,296,319]" italics="true" pageId="3" pageNumber="588">nazarovi</emphasis>
Gusakov, 2017
</taxonomicName>
and 
<emphasis box="[1415,1430,296,319]" italics="true" pageId="3" pageNumber="588">L</emphasis>
. 
<taxonomicName authorityName="Kraatz" authorityYear="1883" box="[151,285,331,355]" class="Insecta" family="Geotrupidae" genus="Lethrus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="588" phylum="Arthropoda" rank="species" species="sulcatus" subGenus="Scelolethrus">
(
<emphasis box="[160,173,331,354]" italics="true" pageId="3" pageNumber="588">S</emphasis>
.) 
