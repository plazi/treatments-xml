<subSubSection pageId="10" pageNumber="55" type="etymology">
<paragraph blockId="10.[151,1052,263,328]" pageId="10" pageNumber="55">
<emphasis bold="true" box="[151,292,263,289]" pageId="10" pageNumber="55">Etymology.</emphasis>
