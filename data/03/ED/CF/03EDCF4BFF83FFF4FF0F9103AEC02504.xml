<subSubSection pageId="6" pageNumber="5" type="discussion">
<paragraph blockId="6.[192,1440,940,1254]" pageId="6" pageNumber="5">
<emphasis bold="true" box="[192,320,940,964]" pageId="6" pageNumber="5">Remarks.</emphasis>
<taxonomicName authority="Wappes &amp; Santos-Silva &amp; Galileo, 2019" authorityName="Wappes &amp; Santos-Silva &amp; Galileo" authorityYear="2019" box="[327,577,940,964]" class="Insecta" family="Cerambycidae" genus="Amphicnaeia" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="bezarki" status="new species">
<emphasis box="[327,577,940,964]" italics="true" pageId="6" pageNumber="5">Amphicnaeia bezarki</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[583,721,940,964]" pageId="6" pageNumber="5" rank="species">new species</taxonomicNameLabel>
