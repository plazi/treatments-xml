<geoCoordinate box="[205,267,1965,1991]" degrees="4" direction="south" minutes="45" orientation="latitude" pageId="98" pageNumber="99" precision="925" value="-4.75">s4 45</geoCoordinate>
(42–49), 
<geoCoordinate box="[383,445,1965,1991]" degrees="6" direction="south" minutes="49" orientation="latitude" pageId="98" pageNumber="99" precision="925" value="-6.8166666">s6 49</geoCoordinate>
(46–54), 
<geoCoordinate box="[561,629,1965,1991]" degrees="2" direction="south" minutes="55" orientation="latitude" pageId="98" pageNumber="99" precision="925" value="-2.9166667">S2 55</geoCoordinate>
