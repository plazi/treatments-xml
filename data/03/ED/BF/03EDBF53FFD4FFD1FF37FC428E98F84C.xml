<emphasis box="[898,1064,1892,1917]" italics="true" pageId="45" pageNumber="46">Typhlodromus</emphasis>
</taxonomicName>
<taxonomicName authority="Evans" authorityName="Evans" box="[1076,1381,1892,1918]" class="Arachnida" family="Phytoseiidae" genus="Clavidromus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="hartlandrowei" subGenus="Anthoseius">
