female from unknown plant, 
<location LSID="urn:lsid:plazi:treatment:03EDBF53FFD4FFD1FF37FC428E98F84C:8E9B589EFFD4FFD0FC49F8D78823F8E6" box="[1001,1137,1866,1892]" country="Ghana" name="Cape Coast" pageId="44" pageNumber="45">Cape Coast</location>
, 
<collectingCountry box="[1149,1228,1866,1892]" name="Ghana" pageId="44" pageNumber="45">Ghana</collectingCountry>
, 
<date box="[1240,1381,1866,1892]" pageId="44" pageNumber="45" value="1989-11-12">
<collectingDate box="[1240,1381,1866,1892]" pageId="44" pageNumber="45" value="1989-11-12">12-XI-1989</collectingDate>
</date>
