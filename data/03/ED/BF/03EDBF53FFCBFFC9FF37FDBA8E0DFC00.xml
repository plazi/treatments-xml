<geoCoordinate box="[1010,1072,1722,1748]" degrees="4" direction="south" minutes="17" orientation="latitude" pageId="51" pageNumber="52" precision="925" value="-4.2833333">s4 17</geoCoordinate>
(16–18), 
<geoCoordinate box="[1186,1248,1722,1748]" degrees="6" direction="south" minutes="17" orientation="latitude" pageId="51" pageNumber="52" precision="925" value="-6.2833333">s6 17</geoCoordinate>
(17–18), 
<geoCoordinate box="[1362,1430,1722,1748]" degrees="2" direction="south" minutes="17" orientation="latitude" pageId="51" pageNumber="52" precision="925" value="-2.2833333">S2 17</geoCoordinate>
