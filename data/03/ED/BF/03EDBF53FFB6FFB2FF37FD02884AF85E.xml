<quantity box="[1260,1339,1746,1772]" metricMagnitude="4" metricUnit="m" metricValue="7.8" pageId="78" pageNumber="79" unit="km" value="78.0">78 km</quantity>
<collectorName pageId="78" pageNumber="79">S. Chiwoma</collectorName>
