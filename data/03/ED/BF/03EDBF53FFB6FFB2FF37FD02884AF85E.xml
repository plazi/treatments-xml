and 
<specimenCount box="[733,962,1746,1772]" pageId="78" pageNumber="79" type="female" typeStatus="paratype">1 paratype females</specimenCount>
from 
<taxonomicName box="[1041,1241,1747,1772]" class="Magnoliopsida" family="Phyllanthaceae" genus="Uapaca" kingdom="Plantae" order="Malpighiales" pageId="78" pageNumber="79" phylum="Tracheophyta" rank="species" species="kirkiana">
<emphasis box="[1041,1241,1747,1772]" italics="true" pageId="78" pageNumber="79">Uapaca kirkiana</emphasis>
</taxonomicName>
