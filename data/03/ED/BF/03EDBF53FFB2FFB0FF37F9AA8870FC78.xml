<taxonomicName box="[892,1016,632,657]" class="Liliopsida" family="Poaceae" genus="Eragrostis" kingdom="Plantae" order="Poales" pageId="76" pageNumber="77" phylum="Tracheophyta" rank="species" species="guineensis">guineensis</taxonomicName>
</emphasis>
, 
<quantity box="[1030,1104,632,658]" metricMagnitude="4" metricUnit="m" metricValue="2.7" pageId="76" pageNumber="77" unit="km" value="27.0">27 km</quantity>
