<number box="[419,435,1922,1946]" pageId="1" pageNumber="419" value="9.0">9</number>
; A iii, 
<number box="[516,532,1921,1946]" pageId="1" pageNumber="419" value="6.0">6</number>
; P i, 
<number box="[593,624,1922,1946]" pageId="1" pageNumber="419" value="10.0">10</number>
; V ii, 
