<paragraph blockId="12.[151,1437,597,1343]" pageId="12" pageNumber="13">
<emphasis bold="true" box="[199,400,957,983]" pageId="12" pageNumber="13">Type conditions.</emphasis>
<typeStatus box="[411,515,957,983]" pageId="12" pageNumber="13">Holotype</typeStatus>
