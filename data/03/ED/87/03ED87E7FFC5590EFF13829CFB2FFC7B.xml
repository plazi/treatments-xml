Gottwald, 
<number box="[151,210,697,721]" pageId="7" pageNumber="40" value="1983.0">1983</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Gottwald, 1983" authorityName="Gottwald" authorityYear="1983" box="[221,529,697,721]" class="Ostracoda" family="Cobanocytheridae" genus="Cobanocythere" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="40" phylum="Arthropoda" rank="species" species="pacifica">
<emphasis box="[221,345,697,721]" italics="true" pageId="7" pageNumber="40">C. pacifica</emphasis>
<bibRefCitation author="Gottwald" box="[352,529,697,721]" pageId="7" pageNumber="40" refString="Gottwald, J. (1983) Interstitielle Fauna von Galapagos. XXX. Podocopida 1 (Ostracoda). Mikrofauna Meeresboden, 90, 1 - 187." type="journal article" year="1983">
Gottwald, 
<number box="[470,529,697,721]" pageId="7" pageNumber="40" value="1983.0">1983</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Gottwald, 1983" authorityName="Gottwald" authorityYear="1983" box="[541,924,697,721]" class="Ostracoda" family="Cobanocytheridae" genus="Cobanocythere" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="40" phylum="Arthropoda" rank="species" species="fernandinensis">
<emphasis box="[541,739,697,721]" italics="true" pageId="7" pageNumber="40">C. fernandinensis</emphasis>
<bibRefCitation author="Gottwald" box="[746,924,697,721]" pageId="7" pageNumber="40" refString="Gottwald, J. (1983) Interstitielle Fauna von Galapagos. XXX. Podocopida 1 (Ostracoda). Mikrofauna Meeresboden, 90, 1 - 187." type="journal article" year="1983">
Gottwald, 
<number box="[865,924,697,721]" pageId="7" pageNumber="40" value="1983.0">1983</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Gottwald, 1983" authorityName="Gottwald" authorityYear="1983" box="[935,1262,697,721]" class="Ostracoda" family="Cobanocytheridae" genus="Cobanocythere" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="40" phylum="Arthropoda" rank="species" species="hoodensis">
