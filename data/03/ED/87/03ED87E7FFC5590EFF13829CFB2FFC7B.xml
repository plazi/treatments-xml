</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Gottwald, 1983" authorityName="Gottwald" authorityYear="1983" class="Ostracoda" family="Cobanocytheridae" genus="Cobanocythere" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="40" phylum="Arthropoda" rank="species" species="psammophila">
