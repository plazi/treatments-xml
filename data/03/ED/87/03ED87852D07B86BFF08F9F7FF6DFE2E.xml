<emphasis bold="true" box="[693,785,1822,1846]" pageId="7" pageNumber="234">
<taxonomicNameLabel box="[693,751,1822,1846]" pageId="7" pageNumber="234" rank="species">n. sp.</taxonomicNameLabel>
4a
</emphasis>
, left lateral; 
<emphasis bold="true" box="[918,945,1822,1846]" pageId="7" pageNumber="234">4b</emphasis>
, phallus, left lateral; 
<emphasis bold="true" box="[1165,1189,1822,1846]" pageId="7" pageNumber="234">4c</emphasis>
, dorsal; 
