<quantity box="[784,955,1080,1106]" metricMagnitude="3" metricUnit="m" metricValue="1.1" metricValueMax="1.4" metricValueMin="0.8" pageId="44" pageNumber="45" unit="m" value="1100.0" valueMax="1400.0" valueMin="800.0">800 to 1400 m</quantity>
