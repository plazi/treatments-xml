<subSubSection box="[151,780,794,816]" pageId="1" pageNumber="582" type="materials_examined">
<paragraph blockId="1.[151,780,794,816]" box="[151,780,794,816]" pageId="1" pageNumber="582">
<emphasis bold="true" box="[151,293,794,816]" pageId="1" pageNumber="582">Type species:</emphasis>
<taxonomicName box="[299,568,794,816]" class="Insecta" family="Mantodea" genus="Paratheopompa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mantodea" pageId="1" pageNumber="582" phylum="Arthropoda" rank="species" species="siamensis" status="n. sp.">
<emphasis box="[299,568,794,816]" italics="true" pageId="1" pageNumber="582">Paratheopompa siamensis</emphasis>
</taxonomicName>
