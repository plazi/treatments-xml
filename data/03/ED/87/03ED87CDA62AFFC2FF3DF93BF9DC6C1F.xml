<number box="[863,978,260,284]" pageId="21" pageNumber="22" value="2.88" valueMax="3.33" valueMin="2.43">2.43–3.33</number>
, 
<number box="[992,1041,260,284]" pageId="21" pageNumber="22" value="2.8">2.80</number>
