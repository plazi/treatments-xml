<quantity box="[834,998,430,454]" metricMagnitude="-3" metricUnit="m" metricValue="2.21" metricValueMax="2.4" metricValueMin="2.02" pageId="35" pageNumber="36" unit="mm" value="2.21" valueMax="2.4" valueMin="2.02">2.02–2.40 mm</quantity>
