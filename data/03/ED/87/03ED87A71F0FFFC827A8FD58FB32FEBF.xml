and 
<collectingRegion box="[1010,1054,1599,1625]" country="Turkey" name="Van" pageId="10" pageNumber="283">Van</collectingRegion>
Provinces, SE 
<collectingCountry box="[1231,1311,1598,1624]" name="Turkey" pageId="10" pageNumber="283">Turkey</collectingCountry>
(
<figureCitation box="[1330,1400,1598,1625]" captionStart="FIGURE 7" captionStartId="9.[136,229,1832,1854]" captionTargetBox="[154,1419,1039,1778]" captionTargetId="figure-361@9.[151,1436,1001,1808]" captionTargetPageId="9" captionText="FIGURE 7. Distribution of Psephellus integrifolius (circles) and P. karduchorum (squares)." figureDoi="http://doi.org/10.5281/zenodo.5423482" httpUri="https://zenodo.org/record/5423482/files/figure.png" pageId="10" pageNumber="283">Fig. 7</figureCitation>
