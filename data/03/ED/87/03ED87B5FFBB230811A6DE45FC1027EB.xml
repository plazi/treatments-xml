<materialsCitation ID-GBIF-Occurrence="3428573303" collectingDate="2020-11-24" collectionCode="HKAS, KUMCC" country="China" location="Ruifang Xu" municipality="Xishuangbanna Tropical Botanical Garden" pageId="6" pageNumber="71" specimenCode="HKAS 115759, KUMCC 21-0086" specimenCount="1" stateProvince="Yunnan" typeStatus="holotype">
