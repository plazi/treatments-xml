</taxonomicName>
and 
<taxonomicName authority="(Kurian, 1955)" baseAuthorityName="Kurian" baseAuthorityYear="1955" box="[984,1403,880,907]" class="Insecta" family="Bethylidae" genus="Sclerodermus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="56" pageNumber="57" phylum="Arthropoda" rank="species" species="raonis">
