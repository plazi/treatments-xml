<paragraph blockId="6.[151,565,1364,1416]" box="[151,565,1394,1416]" pageId="6" pageNumber="529">
<taxonomicName authority="Anisyutkin" authorityName="Anisyutkin" authorityYear="2007" box="[151,466,1394,1416]" class="Insecta" family="Blaberidae" genus="Diploptera" kingdom="Animalia" order="Blattodea" pageId="6" pageNumber="529" phylum="Arthropoda" rank="species" species="pulchra" status="syn. nov.">
