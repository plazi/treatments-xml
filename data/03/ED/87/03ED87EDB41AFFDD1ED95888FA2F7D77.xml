<taxonomicName baseAuthorityName="Meyrick" baseAuthorityYear="1918" box="[1304,1435,727,748]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="macrodelta">
<emphasis box="[1304,1435,727,748]" italics="true" pageId="10" pageNumber="11">P. macrodelta</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph blockId="10.[151,1436,258,1394]" pageId="10" pageNumber="11">
<keyLead pageId="10" pageNumber="11">
