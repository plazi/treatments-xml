<taxonomicName baseAuthorityName="Meyrick" baseAuthorityYear="1922" box="[1325,1433,903,924]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="10" pageNumber="11" phylum="Arthropoda" rank="species" species="oxyprora">
