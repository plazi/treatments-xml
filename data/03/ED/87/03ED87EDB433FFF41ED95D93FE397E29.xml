<taxonomicName authorityName="Pylartis (Meyrick" authorityYear="1908" box="[757,880,1519,1544]" class="Insecta" family="Gelechiidae" genus="Palumbina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="genus">
