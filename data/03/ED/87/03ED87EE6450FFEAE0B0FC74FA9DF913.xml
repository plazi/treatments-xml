<subSubSection pageId="9" pageNumber="454" type="diagnosis">
<paragraph blockId="9.[151,1437,1014,1759]" pageId="9" pageNumber="454">
<emphasis bold="true" box="[199,321,1086,1111]" pageId="9" pageNumber="454">Diagnosis.</emphasis>
