<subSubSection lastPageId="3" lastPageNumber="28" pageId="2" pageNumber="27" type="nomenclature">
<paragraph blockId="2.[151,342,1678,1703]" box="[151,342,1678,1703]" pageId="2" pageNumber="27">
<heading bold="true" box="[151,342,1678,1703]" fontSize="10" level="2" pageId="2" pageNumber="27" reason="2">
<taxonomicName authority="Roewer" authorityName="Roewer" box="[151,342,1678,1703]" class="Arachnida" family="Gonyleptidae" genus="Thereza" higherTaxonomySource="GBIF" kingdom="Animalia" order="Opiliones" pageId="2" pageNumber="27" phylum="Arthropoda" rank="genus">
