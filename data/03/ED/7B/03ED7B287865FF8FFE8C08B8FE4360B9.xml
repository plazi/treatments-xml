. 
<taxonomicName authority="Evenhuis" authorityName="Evenhuis" class="Insecta" family="Bombyliidae" genus="Mythenteles" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="deemingi" status="sp. n.">
<taxonomicName genus="M." pageId="19" pageNumber="20" rank="species" species="deemingi">
