<emphasis box="[276,617,1145,1170]" italics="true" pageId="26" pageNumber="27">
<taxonomicName authority="Kaczmarek, 2017" authorityName="Kaczmarek" authorityYear="2017" box="[276,491,1145,1170]" class="Eutardigrada" family="Hypsibiidae" genus="Adropion" higherTaxonomySource="GBIF" kingdom="Animalia" order="Parachela" pageId="26" pageNumber="27" phylum="Tardigrada" rank="species" sensu="lato" species="scoticum" status="sensu lato">Adropion scoticum</taxonomicName>
