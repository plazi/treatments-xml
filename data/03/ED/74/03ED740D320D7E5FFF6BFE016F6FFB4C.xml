</taxonomicName>
<taxonomicNameLabel box="[199,338,1217,1242]" pageId="26" pageNumber="27" sensu="stricto">sensu stricto</taxonomicNameLabel>
<typeStatus box="[345,393,1218,1242]" pageId="26" pageNumber="27">type</typeStatus>
