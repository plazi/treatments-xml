<taxonomicName authorityName="Kaczmarek" authorityYear="2017" baseAuthorityName="Weglarska" baseAuthorityYear="1959" box="[286,396,940,965]" class="Eutardigrada" family="Hypsibiidae" genus="Astatumen" kingdom="Animalia" order="Parachela" pageId="27" pageNumber="28" phylum="Tardigrada" rank="species" species="bartosi">
