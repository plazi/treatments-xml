<collectingCountry box="[491,643,1759,1784]" name="Mozambique" pageId="53" pageNumber="54">Mozambique</collectingCountry>
: 1, 
<collectingCountry box="[682,770,1759,1784]" name="Tunisia" pageId="53" pageNumber="54">Tunisia</collectingCountry>
