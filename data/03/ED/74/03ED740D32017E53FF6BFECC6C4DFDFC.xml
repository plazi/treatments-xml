<taxonomicName baseAuthorityName="Binda" baseAuthorityYear="1969" box="[441,519,521,546]" class="Eutardigrada" family="Hypsibiidae" genus="Diphascon" kingdom="Animalia" order="Parachela" pageId="22" pageNumber="23" phylum="Tardigrada" rank="species" species="nobilei">
<emphasis box="[441,519,521,546]" italics="true" pageId="22" pageNumber="23">nobilei</emphasis>
