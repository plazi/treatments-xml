<collectingDate box="[736,866,965,986]" pageId="18" pageNumber="109" value="2002-08-22">22-VIII-2002</collectingDate>
</date>
, 
<collectorName box="[878,1001,965,986]" pageId="18" pageNumber="109">G. Nogueira</collectorName>
, fixed at 19 days age (
<collectionCode box="[383,445,991,1012]" pageId="18" pageNumber="109">IEXA</collectionCode>
)
</materialsCitation>
