<subSubSection lastPageId="9" lastPageNumber="479" pageId="8" pageNumber="478" type="materials_examined">
<paragraph blockId="8.[151,1436,2012,2037]" lastBlockId="9.[151,1436,151,357]" lastPageId="9" lastPageNumber="479" pageId="8" pageNumber="478">
<emphasis bold="true" box="[151,385,2012,2037]" pageId="8" pageNumber="478">Material examined.</emphasis>
