<taxonomicName authority="Castlenau" authorityName="Castlenau" box="[281,654,1418,1440]" class="Actinopterygii" family="Ambassidae" genus="Pseudoambassis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="2" pageNumber="567" phylum="Chordata" rank="species" species="macleayi">
