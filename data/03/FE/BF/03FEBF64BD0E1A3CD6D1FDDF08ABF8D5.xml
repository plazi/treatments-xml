<quantity box="[1109,1230,1018,1039]" metricMagnitude="-2" metricUnit="m" metricValue="4.191" metricValueMax="4.6482" metricValueMin="3.7338" pageId="1" pageNumber="52" unit="in" value="1.65" valueMax="1.83" valueMin="1.47">1.47–1.83 in</quantity>
