<quantity box="[878,1000,1018,1039]" metricMagnitude="-2" metricUnit="m" metricValue="3.683" metricValueMax="3.9878" metricValueMin="3.3782" pageId="1" pageNumber="52" unit="in" value="1.45" valueMax="1.57" valueMin="1.33">1.33–1.57 in</quantity>
