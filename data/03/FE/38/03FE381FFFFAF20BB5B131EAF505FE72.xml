<collectionCode LSID="urn:lsid:biocol.org:col:32981" box="[1226,1296,326,350]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="2" pageNumber="274">ANIC</collectionCode>
