<subSubSection box="[292,890,1818,1860]" pageId="8" pageNumber="209" type="nomenclature">
<paragraph blockId="8.[290,1688,1818,2495]" box="[292,890,1818,1860]" pageId="8" pageNumber="209">
<emphasis bold="true" box="[292,656,1818,1860]" pageId="8" pageNumber="209">
<taxonomicName authorityName="Malloch" authorityYear="1942" box="[292,649,1818,1860]" class="Insecta" family="Clusiidae" genus="Czernyola" kingdom="Animalia" order="Diptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="atrifrons" status="new species">Czernyola atrifrons</taxonomicName>
,
</emphasis>
<taxonomicNameLabel box="[670,885,1818,1860]" pageId="8" pageNumber="209" rank="species">new species</taxonomicNameLabel>
