<paragraph blockId="26.[151,1437,1455,2003]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[151,296,1455,1481]" pageId="26" pageNumber="27">Dimensions</emphasis>
. 
<typeStatus box="[311,425,1456,1482]" pageId="26" pageNumber="27">Holotype</typeStatus>
