</taxonomicName>
<taxonomicNameLabel box="[607,729,2033,2054]" pageId="4" pageNumber="301" rank="species">new species</taxonomicNameLabel>
(a), 
<taxonomicName authority="ski, 2009" authorityName="ski" authorityYear="2009" box="[134,430,2062,2083]" class="Insecta" family="Staphylinidae" genus="Horaeomorphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="301" phylum="Arthropoda" rank="species" species="samosirensis" status="new species">
