<paragraph blockId="3.[151,921,1288,1311]" box="[151,921,1288,1311]" pageId="3" pageNumber="188">
<emphasis bold="true" box="[151,288,1288,1311]" pageId="3" pageNumber="188">FIGURES 9.</emphasis>
<taxonomicName authority="Du, 2017" authorityName="Du" authorityYear="2017" box="[295,547,1289,1311]" class="Insecta" family="Leuctridae" genus="Rhopalopsole" kingdom="Animalia" order="Plecoptera" pageId="3" pageNumber="188" phylum="Arthropoda" rank="species" species="vespertilio" status="sp. nov.">
<emphasis box="[295,547,1289,1311]" italics="true" pageId="3" pageNumber="188">Rhopalopsole vespertilio</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[560,640,1289,1310]" pageId="3" pageNumber="188" rank="species">
<emphasis bold="true" box="[560,640,1289,1310]" pageId="3" pageNumber="188">sp. nov.</emphasis>
