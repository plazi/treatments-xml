<materialsCitation collectingDate="2009-06-02" collectionCode="UF" collectorName="J. Chase &amp; Three &amp; Buenavista &amp; Sincelejo &amp; Depto. &amp; E. Arjona &amp; The &amp; Ft. Lauderdale Research &amp; Education Center &amp; Florida &amp; Consumer Services &amp; Gainesville &amp; Arthropod Collection &amp; Instituto Alexander von Humboldt &amp; Boyac &amp; All &amp; Colombia's Caribbean &amp; Covenas &amp; N. W. San Jacinto &amp; Cienega &amp; Ortiz &amp; Salamina &amp; Sabanalurga &amp; Clemencia" country="Colombia" county="Lomas La Querella" elevation="113" lastPageId="4" lastPageNumber="67" latitude="10.57234" location="University of Florida Termite Collection" longLatPrecision="1" longitude="-75.3354" municipality="Villa de Leyva" pageId="3" pageNumber="66" specimenCode="JUNE09" specimenCount="4" stateProvince="Sucre" typeStatus="holotype">
