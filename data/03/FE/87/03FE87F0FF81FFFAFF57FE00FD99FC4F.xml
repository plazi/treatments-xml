, but 
<taxonomicName authorityName=": Schultz, 2011:" authorityYear="1242" box="[1099,1226,896,921]" class="Echinoidea" family="Trigonocidaridae" genus="Trigonocidaris" kingdom="Animalia" order="Camarodonta" pageId="38" pageNumber="39" phylum="Echinodermata" rank="species" species="monolini">
<emphasis box="[1099,1226,896,921]" italics="true" pageId="38" pageNumber="39">T. monolini</emphasis>
