to 
<location LSID="urn:lsid:plazi:treatment:03FE87F0FF8DFFF7FF57F8E1FBBAFD1B:8E88603DFF8CFFF7FDD9FE6FFD80FDFC" box="[537,585,504,527]" country="Japan" county="South Sea Islands" municipality="South Africa" name="East" pageId="43" pageNumber="44" stateProvince="Suez">East</location>
