<subSubSection pageId="5" pageNumber="87" type="distribution">
<paragraph blockId="5.[151,1437,1750,1957]" pageId="5" pageNumber="87">
<emphasis bold="true" box="[199,345,1750,1776]" pageId="5" pageNumber="87">Distribution</emphasis>
. Northeast 
<collectingCountry box="[472,530,1751,1777]" name="India" pageId="5" pageNumber="87">India</collectingCountry>
(
<collectingRegion box="[542,668,1751,1777]" country="India" name="Meghalaya" pageId="5" pageNumber="87">Meghalaya</collectingRegion>
and 
<collectingRegion box="[718,931,1751,1778]" country="India" name="Arunachal Pradesh" pageId="5" pageNumber="87">Arunachal Pradesh</collectingRegion>
) (
<bibRefCitation author="Hampson, G. F." box="[951,1124,1751,1777]" pageId="5" pageNumber="87" pagination="1 - 590" refId="ref5830" refString="Hampson, G. F. (1900) Catalogue of the Arctiadae (Nolinae, Lithosianae) in the British Museum. Catalogue of the Lepidoptera Phalaenae in the British Museum, 2, 1 - 590." type="journal article" year="1900">Hampson 1900</bibRefCitation>
; 
<bibRefCitation author="Kirti, J. S. &amp; Singh, N." box="[1134,1350,1750,1777]" pageId="5" pageNumber="87" refId="ref5990" refString="Kirti, J. S. &amp; Singh, N. (2016) Arctiid Moths of India. Vol. 2. Nature Books India, New Delhi, 214 pp." type="book" year="2016">Kirti &amp; Singh 2016</bibRefCitation>
), northern 
<collectingCountry box="[193,302,1787,1813]" name="Myanmar" pageId="5" pageNumber="87">Myanmar</collectingCountry>
(
<collectingRegion box="[316,438,1787,1813]" country="Myanmar" name="Shan" pageId="5" pageNumber="87">Shan State</collectingRegion>
) (
<bibRefCitation author="Hampson, G. F." box="[459,634,1787,1813]" pageId="5" pageNumber="87" refId="ref5795" refString="Hampson, G. F. (1894) The Fauna of British India, including Ceylon and Burma. Moths. Vol. 2. Taylor and Francis, London, 609 pp." type="book" year="1894">Hampson 1894</bibRefCitation>
), south-western 
<collectingCountry box="[816,883,1787,1813]" name="China" pageId="5" pageNumber="87">China</collectingCountry>
(
<collectingRegion box="[897,981,1787,1814]" country="China" name="Xizang" pageId="5" pageNumber="87">Xizang</collectingRegion>
) (
<bibRefCitation author="Huang, S. - Y. &amp; Volynkin, A. V. &amp; Miao, Z. - P. &amp; Tan, S. - Y. &amp; Wang, M. &amp; Fan, X. - L." box="[1003,1204,1787,1813]" pageId="5" pageNumber="87" refId="ref5869" refString="Huang, S. - Y., Volynkin, A. V., Miao, Z. - P., Tan, S. - Y., Wang, M. &amp; Fan, X. - L. (2021) Molecular phylogeny and classification of Nudariina (Lepidoptera: Erebidae) with emphasis on the genera Barsine Walker, Ammatho Walker and Ovipennis Hampson. Systematic Entomology. https: // doi. org / 10.1111 / syen. 12509" type="book" year="2021">
Huang 
<emphasis box="[1086,1141,1787,1813]" italics="true" pageId="5" pageNumber="87">et al.</emphasis>
2021
</bibRefCitation>
) and North 
<collectingCountry box="[1337,1437,1787,1813]" name="Thailand" pageId="5" pageNumber="87">Thailand</collectingCountry>
(
<collectingRegion box="[159,404,1823,1850]" country="Thailand" name="Chiang Mai" pageId="5" pageNumber="87">Chiang Mai Province</collectingRegion>
) (
<bibRefCitation author="Cerny, K. &amp; Pinratana, A." box="[428,710,1823,1851]" pageId="5" pageNumber="87" refId="ref5687" refString="Cerny, K. &amp; Pinratana, A. (2009) Moths of Thailand. Vol. 6. Arctiidae. Brothers of Saint Gabriel in Thailand, Bangkok, 283 pp." type="book" year="2009">Černý &amp; Pinratana 2009</bibRefCitation>
, partim). The records from the 
<collectingCountry box="[1067,1167,1823,1849]" name="Thailand" pageId="5" pageNumber="87">Thailand</collectingCountry>
provinces of 
<collectingRegion box="[1323,1432,1823,1849]" country="Thailand" name="Lampang" pageId="5" pageNumber="87">Lampang</collectingRegion>
, 
<collectingRegion box="[151,196,1859,1885]" country="Thailand" name="Tak" pageId="5" pageNumber="87">Tak</collectingRegion>
, 
<collectingRegion box="[207,387,1859,1885]" country="Thailand" name="Sakon Nakhon" pageId="5" pageNumber="87">Sakhon Nakhon</collectingRegion>
and 
<collectingRegion box="[441,667,1859,1885]" country="Thailand" name="Nakhon Ratchasima" pageId="5" pageNumber="87">Nakhon Ratchasima</collectingRegion>
by 
<bibRefCitation author="Cerny, K. &amp; Pinratana, A." box="[709,1000,1859,1887]" pageId="5" pageNumber="87" refId="ref5687" refString="Cerny, K. &amp; Pinratana, A. (2009) Moths of Thailand. Vol. 6. Arctiidae. Brothers of Saint Gabriel in Thailand, Bangkok, 283 pp." type="book" year="2009">Černý &amp; Pinratana (2009)</bibRefCitation>
