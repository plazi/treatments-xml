and 
<taxonomicName box="[817,910,1288,1311]" class="Insecta" family="Gelechiidae" genus="Telphusa" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
