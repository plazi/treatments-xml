<taxonomicName authority="Chambers, 1872" authorityName="Chambers" authorityYear="1872" box="[661,765,1288,1311]" class="Insecta" family="Gelechiidae" genus="Adrasteia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
