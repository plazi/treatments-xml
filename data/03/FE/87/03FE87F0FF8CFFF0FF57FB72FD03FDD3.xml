<taxonomicName baseAuthorityName="Blainville" baseAuthorityYear="1825" box="[632,887,191,216]" class="Echinoidea" family="Echinometridae" genus="Echinometra" kingdom="Animalia" order="Camarodonta" pageId="44" pageNumber="45" phylum="Echinodermata" rank="species" species="mathaei">
