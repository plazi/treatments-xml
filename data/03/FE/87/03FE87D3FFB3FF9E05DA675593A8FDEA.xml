<subSubSection pageId="8" pageNumber="57" type="materials_examined">
<paragraph blockId="8.[151,1437,152,570]" pageId="8" pageNumber="57">
<emphasis bold="true" box="[198,320,428,454]" pageId="8" pageNumber="57">
<typeStatus box="[198,315,428,454]" pageId="8" pageNumber="57">Holotype</typeStatus>
.
</emphasis>
<number box="[326,348,428,454]" pageId="8" pageNumber="57" value="3.0">3</number>
. 
<collectingCountry box="[363,524,428,454]" name="Colombia" pageId="8" pageNumber="57">COLOMBIA</collectingCountry>
: Putumayo, PNN La Paya, Cab. Viviano, 
<geoCoordinate box="[1011,1092,428,454]" direction="south" orientation="latitude" pageId="8" pageNumber="57" precision="555" value="-0.12">
<number box="[1011,1068,428,454]" pageId="8" pageNumber="57" value="0.12">0.12</number>
°S
</geoCoordinate>
