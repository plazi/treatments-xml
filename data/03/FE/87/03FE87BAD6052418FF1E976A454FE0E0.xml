<caption httpUri="https://zenodo.org/record/276422/files/figure.png" pageId="3" pageNumber="36" targetBox="[177,1404,211,1875]" targetPageId="3">
<paragraph blockId="3.[151,1436,1905,1960]" pageId="3" pageNumber="36">
<emphasis bold="true" box="[151,327,1905,1929]" pageId="3" pageNumber="36">FIGURES 1–5.</emphasis>
<taxonomicName authority="Feng" authorityName="Feng" box="[335,623,1905,1929]" class="Insecta" family="Cixiidae" genus="Indolipa" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="36" phylum="Arthropoda" rank="species" species="gansuensis" status="sp. nov.">
<emphasis box="[335,558,1906,1929]" italics="true" pageId="3" pageNumber="36">Indolipa gansuensis</emphasis>
Feng
</taxonomicName>
, 
<emphasis bold="true" box="[634,723,1905,1929]" pageId="3" pageNumber="36">
