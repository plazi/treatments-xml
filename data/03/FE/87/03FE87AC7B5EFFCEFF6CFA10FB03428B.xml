May 
<number box="[574,635,1601,1627]" pageId="6" pageNumber="45" value="1976.1">1976</number>
</date>
, 10 ex. mixed 
<taxonomicName box="[819,917,1601,1626]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="6" pageNumber="45" phylum="Tracheophyta" rank="genus">
