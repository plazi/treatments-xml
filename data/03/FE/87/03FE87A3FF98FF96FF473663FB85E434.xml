spp. (
<taxonomicName box="[1054,1238,1167,1192]" class="Insecta" family="Gelechiidae" genus="Evippe" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="prunifoliella">
