: 
<collectingRegion box="[560,659,1847,1870]" country="Canada" name="Nunavut" pageId="20" pageNumber="21">Nunavut</collectingRegion>
, 
<collectingRegion box="[671,851,1847,1871]" country="Canada" name="Yukon" pageId="20" pageNumber="21">Yukon Territory</collectingRegion>
, first
</materialsCitation>
