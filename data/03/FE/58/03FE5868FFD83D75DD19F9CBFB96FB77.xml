<taxonomicName authority="Lai, 1965" authorityName="Lai" authorityYear="1965" box="[455,780,623,650]" class="Cephalopoda" family="Lituitidae" genus="Lituites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthocerida" pageId="46" pageNumber="47" phylum="Mollusca" rank="species" species="ningkiangense">
<emphasis box="[455,655,623,649]" italics="true" pageId="46" pageNumber="47">L. ningkiangense</emphasis>
<bibRefCitation author="Lai C." box="[663,780,623,650]" pageId="46" pageNumber="47" pagination="308 - 342" refId="ref47918" refString="Lai C. 1965. Ordovician and Silurian cephalopods from the Hanzhung and Ningkiang of Shenzi. Acta Palaeontologica Sinica 13: 308 - 342." type="journal article" year="1965">Lai, 1965</bibRefCitation>
</taxonomicName>
); 
<taxonomicName authority="Lai, 1982" authorityName="Lai" authorityYear="1982" box="[801,1148,623,649]" class="Cephalopoda" family="Lituitidae" genus="Lituites" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthocerida" pageId="46" pageNumber="47" phylum="Mollusca" rank="species" species="xizangense">
