<subSubSection pageId="65" pageNumber="66" type="diagnosis">
<paragraph blockId="65.[189,310,1262,1288]" box="[189,310,1262,1288]" pageId="65" pageNumber="66">
<heading bold="true" box="[189,310,1262,1288]" fontSize="11" level="3" pageId="65" pageNumber="66" reason="3">
