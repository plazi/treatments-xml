<subSubSection pageId="11" pageNumber="12" type="diagnosis">
<paragraph blockId="11.[189,310,1415,1441]" box="[189,310,1415,1441]" pageId="11" pageNumber="12">
<heading bold="true" box="[189,310,1415,1441]" fontSize="11" level="3" pageId="11" pageNumber="12" reason="3">
