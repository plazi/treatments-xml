<number box="[1164,1172,1631,1645]" pageId="6" pageNumber="270" value="2.0">
<emphasis box="[1164,1172,1631,1645]" italics="true" pageId="6" pageNumber="270">2</emphasis>
</number>
absent). Setae 
<emphasis box="[1340,1354,1616,1641]" italics="true" pageId="6" pageNumber="270">d</emphasis>
<number box="[1354,1362,1631,1645]" pageId="6" pageNumber="270" value="1.0">
<emphasis box="[1354,1362,1631,1645]" italics="true" pageId="6" pageNumber="270">1</emphasis>
