<number box="[884,898,655,679]" pageId="2" pageNumber="260" value="1.0">1</number>
(
<number box="[915,1019,655,679]" pageId="2" pageNumber="260" value="11.735" valueMax="14.47" valueMin="9.0">9–14, 47</number>
