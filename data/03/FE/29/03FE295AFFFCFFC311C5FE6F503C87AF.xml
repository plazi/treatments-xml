<paragraph blockId="44.[144,596,532,556]" box="[144,596,532,556]" pageId="44" pageNumber="728">
<taxonomicName authority="Sharp, 1916" authorityName="Sharp" authorityYear="1916" box="[144,596,532,556]" class="Insecta" family="Helophoridae" genus="Atracthelophorus" kingdom="Animalia" order="Coleoptera" pageId="44" pageNumber="728" phylum="Arthropoda" rank="species" species="nevadensis">
