<location LSID="urn:lsid:plazi:treatment:03FE8632C5001A01FDDD7148FBED0D06:8E8861FFC5011A01FBE47531FB7D0D2B" box="[1056,1219,573,596]" country="Georgia" name="Accordingly" pageId="9" pageNumber="318">Accordingly</location>
