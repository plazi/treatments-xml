<caption pageId="108" pageNumber="109" startId="108.[151,264,1261,1285]" targetBox="[158,1428,187,1233]" targetPageId="108">
<paragraph blockId="108.[151,1436,1261,1321]" pageId="108" pageNumber="109">
<emphasis bold="true" box="[151,367,1261,1285]" pageId="108" pageNumber="109">FIGURES 350–353.</emphasis>
<taxonomicName authority="Beier, 1973" authorityName="Beier" authorityYear="1973" box="[376,704,1261,1285]" class="Arachnida" family="Chthoniidae" genus="Chthonius" kingdom="Animalia" order="Pseudoscorpiones" pageId="108" pageNumber="109" phylum="Arthropoda" rank="species" species="paolettii">
