<subSubSection box="[151,637,1777,1805]" pageId="0" pageNumber="591" type="nomenclature">
<paragraph blockId="0.[151,637,1741,1805]" box="[151,637,1777,1805]" pageId="0" pageNumber="591">
<heading box="[151,637,1777,1805]" level="2" pageId="0" pageNumber="591">
<emphasis bold="true" box="[151,519,1777,1804]" pageId="0" pageNumber="591">
<taxonomicName authority="Mo &amp; Wang &amp; Li, 2019" authorityName="Mo &amp; Wang &amp; Li" authorityYear="2019" box="[151,418,1777,1804]" class="Insecta" family="Perlidae" genus="Caroperla" kingdom="Animalia" order="Plecoptera" pageId="0" pageNumber="591" phylum="Arthropoda" rank="species" species="longilobata" status="sp. nov.">
<emphasis bold="true" box="[151,418,1777,1804]" italics="true" pageId="0" pageNumber="591">Caroperla longilobata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[425,519,1778,1804]" pageId="0" pageNumber="591" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
(Fig. a–f)
