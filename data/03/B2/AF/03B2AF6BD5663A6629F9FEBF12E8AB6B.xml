<collectingCountry box="[359,426,943,969]" name="China" pageId="9" pageNumber="266">China</collectingCountry>
(
<collectingRegion box="[441,548,943,970]" country="China" name="Zhejiang" pageId="9" pageNumber="266">Zhejiang</collectingRegion>
