<taxonomicName authorityName="Bates" authorityYear="1880" class="Insecta" family="Cerambycidae" genus="Parestola" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="560" phylum="Arthropoda" rank="species" species="zapotensis">
