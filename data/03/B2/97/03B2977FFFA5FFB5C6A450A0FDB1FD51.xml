<quantity box="[1072,1265,1868,1895]" metricMagnitude="-5" metricUnit="m" metricValue="5.699999999999999" metricValueMax="5.8999999999999995" metricValueMin="5.5" pageId="17" pageNumber="218" unit="mm" value="0.056999999999999995" valueMax="0.059" valueMin="0.055">
<geoCoordinate box="[1072,1134,1868,1895]" degrees="0.055" direction="north" orientation="latitude" pageId="17" pageNumber="218" precision="55" value="0.055">0.055</geoCoordinate>
