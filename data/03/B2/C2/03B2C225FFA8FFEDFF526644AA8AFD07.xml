<subSubSection box="[151,523,1111,1137]" pageId="19" pageNumber="20" type="nomenclature">
<paragraph blockId="19.[151,523,1111,1137]" box="[151,523,1111,1137]" pageId="19" pageNumber="20">
<heading bold="true" box="[151,523,1111,1137]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis bold="true" box="[151,523,1111,1137]" pageId="19" pageNumber="20">
<taxonomicName authority="Franz" authorityName="Franz" box="[151,443,1111,1137]" class="Insecta" family="Curculionidae" genus="Cotithene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="species" species="trigaea" status="sp. n.">
<emphasis bold="true" box="[151,360,1111,1137]" italics="true" pageId="19" pageNumber="20">Cotithene trigaea</emphasis>
Franz
</taxonomicName>
, 
<taxonomicNameLabel box="[457,523,1111,1137]" pageId="19" pageNumber="20" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
