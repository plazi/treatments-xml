<number box="[498,528,1641,1667]" pageId="3" pageNumber="38" value="24.0">24</number>
(
<number box="[545,623,1641,1667]" pageId="3" pageNumber="38" value="22.0" valueMax="24.0" valueMin="20.0">20–24</number>
