<quantity box="[1097,1233,1754,1778]" metricMagnitude="3" metricUnit="m" metricValue="1.25" metricValueMax="2.0" metricValueMin="0.5" pageId="0" pageNumber="215" unit="m" value="1250.0" valueMax="2000.0" valueMin="500.0">500–2000 m</quantity>
