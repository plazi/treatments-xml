<collectingCountry box="[365,435,432,458]" name="China" pageId="58" pageNumber="59">China</collectingCountry>
(
<collectingRegion box="[451,542,432,458]" country="China" name="Hainan" pageId="58" pageNumber="59">Hainan</collectingRegion>
