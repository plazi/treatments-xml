<taxonomicName authority="Benedek &amp; Volynkin &amp; Saldaitis &amp; Tóth, 2022" authorityName="Benedek &amp; Volynkin &amp; Saldaitis &amp; Tóth" authorityYear="2022" box="[473,583,511,536]" class="Insecta" family="Noctuidae" genus="Baimistra" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="448" phylum="Arthropoda" rank="genus" status="gen. n.">
