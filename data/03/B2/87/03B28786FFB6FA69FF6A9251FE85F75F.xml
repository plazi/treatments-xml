, 
<emphasis box="[1125,1148,1447,1473]" italics="true" pageId="10" pageNumber="374">j3</emphasis>
, 
<emphasis box="[1162,1183,1446,1472]" italics="true" pageId="10" pageNumber="374">j6</emphasis>
