, 
<emphasis box="[1277,1306,1447,1473]" italics="true" pageId="10" pageNumber="374">Z1</emphasis>
, 
<emphasis box="[1321,1349,1447,1473]" italics="true" pageId="10" pageNumber="374">S5</emphasis>
, 
<emphasis box="[1363,1389,1447,1473]" italics="true" pageId="10" pageNumber="374">r3</emphasis>
and 
<emphasis box="[151,183,1483,1509]" italics="true" pageId="10" pageNumber="374">R1</emphasis>
long, and 
<emphasis box="[300,325,1483,1509]" italics="true" pageId="10" pageNumber="374">s4</emphasis>
, 
<emphasis box="[339,367,1483,1509]" italics="true" pageId="10" pageNumber="374">S2</emphasis>
, 
<emphasis box="[381,411,1483,1509]" italics="true" pageId="10" pageNumber="374">Z4</emphasis>
and 
