<emphasis bold="true" box="[151,341,1196,1219]" pageId="7" pageNumber="388">FIGURES 14–17.</emphasis>
<taxonomicName authorityName="Davis" authorityYear="2017" box="[348,544,1197,1219]" class="Insecta" family="Curculionidae" genus="Coptus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="388" phylum="Arthropoda" rank="species" species="brevirostris">
<emphasis box="[348,544,1197,1219]" italics="true" pageId="7" pageNumber="388">
<taxonomicName box="[348,420,1198,1219]" class="Insecta" family="Curculionidae" genus="Coptus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="388" phylum="Arthropoda" rank="genus">Coptus</taxonomicName>
brevirostris
