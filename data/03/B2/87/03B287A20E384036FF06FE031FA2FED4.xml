species: 
<taxonomicName box="[294,567,541,563]" class="Malacostraca" family="Trachelipodidae" genus="Levantoniscus" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="bicostulatus" status="n. sp.">
<emphasis box="[294,567,541,563]" italics="true" pageId="1" pageNumber="2">Levantoniscus bicostulatus</emphasis>
</taxonomicName>
<emphasis bold="true" box="[573,631,541,562]" pageId="1" pageNumber="2">
