<paragraph blockId="9.[189,1210,514,576]" box="[189,1198,550,576]" pageId="9" pageNumber="10">
<specimenCount box="[189,213,550,576]" pageId="9" pageNumber="10" type="female">♀</specimenCount>
: L 
<quantity box="[251,336,550,576]" metricMagnitude="-2" metricUnit="m" metricValue="1.3" pageId="9" pageNumber="10" unit="mm" value="13.0">13 mm</quantity>
