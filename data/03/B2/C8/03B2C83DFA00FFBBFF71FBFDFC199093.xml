female &amp; 
<typeStatus box="[702,853,871,895]" pageId="34" pageNumber="35">paralectotype</typeStatus>
female, 
<emphasis bold="true" box="[954,1153,871,896]" pageId="34" pageNumber="35">
<collectingCountry box="[954,1045,871,896]" name="Japan" pageId="34" pageNumber="35">JAPAN</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:03B2C83DFA00FFBBFF71FBFDFC199093:8EC42FF0FA03FFBBFBC2FC9BFB4196B7" box="[1060,1153,872,896]" country="Japan" municipality="Kobe" name="Honshu" pageId="34" pageNumber="35">Honshu</location>
</emphasis>
, 
<collectingMunicipality box="[1169,1232,871,895]" pageId="34" pageNumber="35">Kobe</collectingMunicipality>
, 
<date box="[1245,1335,871,895]" pageId="34" pageNumber="35" value="1915-06">
<collectingDate box="[1245,1335,871,895]" pageId="34" pageNumber="35" value="1915-06">vi-1915</collectingDate>
</date>
, 
<collectorName pageId="34" pageNumber="35">J. E. A. Lewis</collectorName>
(
