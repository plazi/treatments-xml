). 
<emphasis bold="true" box="[324,414,907,932]" pageId="34" pageNumber="35">
<collectingCountry box="[324,414,907,932]" name="Japan" pageId="34" pageNumber="35">JAPAN</collectingCountry>
</emphasis>
, 
<emphasis bold="true" box="[428,521,908,932]" pageId="34" pageNumber="35">
<collectorName box="[428,521,908,932]" pageId="34" pageNumber="35">Honshu</collectorName>
</emphasis>
: 
<specimenCount box="[536,675,908,932]" pageId="34" pageNumber="35" type="female">106 females</specimenCount>
, 
<specimenCount box="[686,786,908,932]" pageId="34" pageNumber="35" type="male">25 males</specimenCount>
(
<collectionCode box="[801,865,908,932]" pageId="34" pageNumber="35">TUA</collectionCode>
