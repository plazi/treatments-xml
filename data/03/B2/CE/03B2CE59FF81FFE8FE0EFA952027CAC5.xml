<taxonomicName authority="Wollaston, 1865" authorityName="Wollaston" authorityYear="1865" box="[727,1180,1380,1404]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="2009" phylum="Arthropoda" rank="species" species="sulcirostris">
