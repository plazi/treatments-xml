<taxonomicName authorityName="Roudier" authorityYear="1958" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="2010" phylum="Arthropoda" rank="subSpecies" species="morio" subSpecies="vandeli">
