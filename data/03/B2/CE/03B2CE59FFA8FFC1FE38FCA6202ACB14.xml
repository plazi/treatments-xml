<taxonomicName authority="Wollaston, 1865" authorityName="Wollaston" authorityYear="1865" box="[719,1135,915,939]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="32" pageNumber="2032" phylum="Arthropoda" rank="species" species="obsitus">
