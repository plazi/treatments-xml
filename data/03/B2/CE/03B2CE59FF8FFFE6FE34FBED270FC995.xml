<taxonomicName authority="Machado, 2006" authorityName="Machado" authorityYear="2006" box="[148,280,1427,1450]" class="Insecta" family="Curculionidae" genus="Laparocerus" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="2007" phylum="Arthropoda" rank="genus" status="n.sp.">
<emphasis box="[148,280,1427,1450]" italics="true" pageId="7" pageNumber="2007">Laparocerus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[289,349,1427,1451]" pageId="7" pageNumber="2007" rank="species">n.sp.</taxonomicNameLabel>
]. MHN, Troyes. 2 exx [
<collectingRegion box="[636,724,1427,1451]" country="Portugal" name="Acores" pageId="7" pageNumber="2007">Azores</collectingRegion>
], 
<collectionCode box="[744,842,1427,1451]" country="France" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" lsid="urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="7" pageNumber="2007">MNHN</collectionCode>
