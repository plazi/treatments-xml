<taxonomicName authority="Hempel" authorityName="Hempel" authorityYear="1934" box="[703,1067,1002,1027]" class="Insecta" family="Eriococcidae" genus="Pseudotectococcus" kingdom="Animalia" order="Hemiptera" pageId="93" pageNumber="94" phylum="Arthropoda" rank="species" species="anonae">
