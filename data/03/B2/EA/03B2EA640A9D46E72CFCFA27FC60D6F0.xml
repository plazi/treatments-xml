<taxonomicName authorityName="Cockerell" authorityYear="1896" box="[822,999,1829,1854]" class="Insecta" family="Asterolecaniidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="179" pageNumber="180" phylum="Arthropoda" rank="family">Asterolecaniidae</taxonomicName>
