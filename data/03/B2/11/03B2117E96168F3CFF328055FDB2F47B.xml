<taxonomicName authorityName=", Stoll" authorityYear="1781" box="[291,400,943,969]" class="Insecta" family="Noctuidae" genus="Phalaena" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="239" phylum="Arthropoda" rank="species" species="euphemia">
