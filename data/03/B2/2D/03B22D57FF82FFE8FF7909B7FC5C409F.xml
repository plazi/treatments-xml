<taxonomicName authority=", Goddess" authorityName="Goddess" box="[568,760,1462,1488]" class="Malacostraca" family="Photidae" genus="Latigammaropsis" kingdom="Animalia" order="Amphipoda" pageId="14" pageNumber="785" phylum="Arthropoda" rank="species" species="hestia">
