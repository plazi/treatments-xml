<specimenCode box="[1310,1438,311,338]" collectionCode="P" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="4" pageNumber="117" type="Herbarium">P00721451</specimenCode>
), identified as 
<taxonomicName box="[957,1192,343,369]" class="Magnoliopsida" family="Ebenaceae" genus="Diospyros" kingdom="Plantae" order="Ericales" pageId="4" pageNumber="117" phylum="Tracheophyta" rank="species" species="cambodiana">
<emphasis box="[957,1192,343,369]" italics="true" pageId="4" pageNumber="117">Diospyros cambodiana</emphasis>
</taxonomicName>
