<location LSID="urn:lsid:plazi:treatment:03B27A3BFFF8FFC0FE9FF888FA4677F8:8EC49DF6FFF8FFC0FC44FEEBFC0174F9" box="[962,1003,375,401]" country="Cambodia" county="In" municipality="Arabic" name="The" pageId="4" pageNumber="117">The</location>
