<location LSID="urn:lsid:plazi:treatment:03B27A3BFFF8FFC0FE9FF888FA4677F8:8EC49DF6FFF8FFC0FB55FEC4FADB741A" box="[1235,1329,344,370]" country="Cambodia" county="In" municipality="Arabic" name="Lecomte" pageId="4" pageNumber="117">Lecomte</location>
