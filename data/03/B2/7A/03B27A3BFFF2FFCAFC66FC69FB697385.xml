<collectionCode box="[1073,1139,1620,1646]" country="Singapore" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15640" lsid="urn:lsid:biocol.org:col:15640" name="Singapore Botanic Gardens" pageId="14" pageNumber="127" type="Herbarium">SING</collectionCode>
