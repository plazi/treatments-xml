<collectionCode box="[923,986,1620,1646]" country="Japan" httpUri="http://grbio.org/cool/fv5j-intd" name="Hiroshima Botanical Garden" pageId="14" pageNumber="127">HBG</collectionCode>
, 
<collectionCode box="[998,1037,1620,1646]" country="USA" lsid="urn:lsid:biocol.org:col:15556" name="William and Lynda Steere Herbarium of the New York Botanical Garden" pageId="14" pageNumber="127" type="Herbarium">NY</collectionCode>
, 
<collectionCode box="[1048,1062,1620,1646]" country="France" lsid="urn:lsid:biocol.org:col:15763" name="Museum National d' Histoire Naturelle, Paris (MNHN) - Vascular Plants" pageId="14" pageNumber="127" type="Herbarium">P</collectionCode>
