<emphasis box="[813,869,1556,1582]" italics="true" pageId="14" pageNumber="127">7741</emphasis>
and 
<emphasis box="[918,974,1556,1582]" italics="true" pageId="14" pageNumber="127">8071</emphasis>
) from “ 
<collectingCountry box="[1057,1140,1556,1582]" name="Vietnam" pageId="14" pageNumber="127">Annam</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:03B27A3BFFF2FFCAFC66FC69FB697385:8EC49DF6FFF2FFCAFBFBF988FACE7346" box="[1149,1316,1556,1582]" country="Vietnam" name="Col des Nuages" pageId="14" pageNumber="127">Col des Nuages</location>
