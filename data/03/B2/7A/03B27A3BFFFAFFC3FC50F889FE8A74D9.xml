<location LSID="urn:lsid:plazi:treatment:03B27A3BFFFAFFC3FC50F889FE8A74D9:8EC49DF6FFFBFFC3FF03FE84FEB67459" box="[133,348,279,306]" country="Cambodia" name="Mont de l'Elephant" pageId="7" pageNumber="120">Mont de l’Eléphant</location>
