<subSubSection pageId="272" pageNumber="273" type="distribution">
<paragraph pageId="272" pageNumber="273">Distribution.</paragraph>
<paragraph pageId="272" pageNumber="273">North-central and central Texas; Brazos, Clay, Dallas, Denton, Erath, Lubbock, Potter, Robertson, Travis, Wichita</paragraph>
</subSubSection>
<subSubSection pageId="272" pageNumber="273" type="locality">
<paragraph pageId="272" pageNumber="273">Locality.</paragraph>
<paragraph pageId="272" pageNumber="273">Holmes Pecan Orchard, Wildcat Bluff Nature Center</paragraph>
</subSubSection>
<subSubSection pageId="272" pageNumber="273" type="time of activity">
<paragraph pageId="272" pageNumber="273">Time of activity.</paragraph>
<paragraph pageId="272" pageNumber="273">Male (January - May, November); female (February - June, December)</paragraph>
</subSubSection>
