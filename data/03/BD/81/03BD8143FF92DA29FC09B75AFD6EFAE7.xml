<taxonomicName authority="Constantin, 2020" authorityName="Constantin" authorityYear="2020" box="[441,674,890,915]" class="Insecta" family="Cantharidae" genus="Plectonotum" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="celendin" status="sp. nov.">
