<subSubSection pageId="1" pageNumber="144" type="etymology">
<paragraph blockId="1.[151,1437,903,2009]" pageId="1" pageNumber="144">
<emphasis bold="true" box="[199,334,1911,1936]" pageId="1" pageNumber="144">Etymology.</emphasis>
