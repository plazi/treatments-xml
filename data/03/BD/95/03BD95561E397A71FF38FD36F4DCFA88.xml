<collectingRegion box="[1143,1248,871,897]" country="Chile" name="Los Rios" pageId="10" pageNumber="461">Los Ríos</collectingRegion>
, 
<collectingRegion box="[1261,1383,871,897]" country="Chile" name="Los Lagos" pageId="10" pageNumber="461">Los Lagos</collectingRegion>
, and 
<collectingRegion box="[151,220,907,933]" country="Chile" name="Aysen" pageId="10" pageNumber="461">Aysén</collectingRegion>
of 
<collectingCountry box="[257,318,907,933]" name="Chile" pageId="10" pageNumber="461">Chile</collectingCountry>
(
<figureCitation box="[333,418,907,933]" captionStart="FIGURE 23" captionStartId="29.[151,250,1313,1337]" captionTargetBox="[226,1360,181,1288]" captionTargetId="figure-16@29.[226,1360,181,1288]" captionTargetPageId="29" captionText="FIGURE 23. Distribution of Argelodes gen. nov., Mucronotus gen. nov., and Solierodes gen. nov." figureDoi="http://doi.org/10.5281/zenodo.5556700" httpUri="https://zenodo.org/record/5556700/files/figure.png" pageId="10" pageNumber="461">Fig. 23</figureCitation>
