, 
<collectingCountry box="[676,743,403,429]" name="China" pageId="7" pageNumber="218">China</collectingCountry>
(
<collectingRegion box="[759,940,403,429]" country="China" name="Nei Mongol" pageId="7" pageNumber="218">Inner Mongolia</collectingRegion>
, 
<collectingRegion box="[952,1130,403,429]" country="China" name="Xinjiang Uygur" pageId="7" pageNumber="218">Xinjiang Uygur</collectingRegion>
Autonomous Region), 
<collectingCountry name="Kazakhstan" pageId="7" pageNumber="218">Kazakhstan</collectingCountry>
