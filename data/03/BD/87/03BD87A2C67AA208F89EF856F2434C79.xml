<quantity box="[3232,3371,2386,2425]" metricMagnitude="1" metricUnit="m" metricValue="1.5" metricValueMax="3.0" metricValueMin="0.0" pageId="12" pageNumber="239" unit="m" value="15.0" valueMax="30.0" valueMin="0.0">0-30 m</quantity>
