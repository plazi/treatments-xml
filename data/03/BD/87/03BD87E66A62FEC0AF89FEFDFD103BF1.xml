<taxonomicName authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" class="Insecta" family="Melolonthidae" genus="Maladera" kingdom="Animalia" order="Coleoptera" pageId="260" pageNumber="261" phylum="Arthropoda" rank="species" species="shangraoensis" status="sp. n.">
<emphasis box="[1093,1273,1909,1933]" italics="true" pageId="260" pageNumber="261">M. shangraoensis</emphasis>
Ahrens, Fabrizi &amp; Liu
</taxonomicName>
, 
<taxonomicNameLabel box="[223,276,1945,1968]" pageId="260" pageNumber="261" rank="species">sp. n.</taxonomicNameLabel>
(holotype); 
<emphasis bold="true" box="[399,446,1945,1969]" pageId="260" pageNumber="261">I–L:</emphasis>
<taxonomicName authority="Ahrens, Fabrizi &amp; Liu" authorityName="Ahrens, Fabrizi &amp; Liu" authorityYear="2021" box="[452,827,1945,1969]" class="Insecta" family="Melolonthidae" genus="Maladera" kingdom="Animalia" order="Coleoptera" pageId="260" pageNumber="261" phylum="Arthropoda" rank="species" species="hunanensis" status="sp. n.">
<emphasis box="[452,598,1945,1969]" italics="true" pageId="260" pageNumber="261">M. hunanensis</emphasis>
