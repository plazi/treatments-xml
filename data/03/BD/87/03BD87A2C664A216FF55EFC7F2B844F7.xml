<subSubSection pageId="18" pageNumber="245" type="discussion">
<paragraph blockId="18.[2713,3518,393,963]" pageId="18" pageNumber="245">
<taxonomicName authorityName="Aellen" authorityYear="1954" box="[2716,3086,553,592]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="18" pageNumber="245" phylum="Chordata" rank="species" species="marisae">
