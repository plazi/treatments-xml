; 
<bibRefCitation author="Zea" box="[1131,1254,284,310]" pageId="16" pageNumber="22" refString="Zea, S. (1987) Esponjas del Caribe Colombiano. Catalogo Cientifico, Bogota, 286 pp." type="book" year="1987">
Zea, 
<number box="[1192,1254,284,310]" pageId="16" pageNumber="17" value="1987.0">1987</number>
