<paragraph blockId="20.[189,1398,564,661]" pageId="20" pageNumber="21">
<taxonomicName baseAuthorityName="Cameron" baseAuthorityYear="1883" box="[189,438,564,591]" class="Insecta" family="Charipidae" genus="Alloxysta" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="21" phylum="Arthropoda" rank="species" species="curvicornis">
