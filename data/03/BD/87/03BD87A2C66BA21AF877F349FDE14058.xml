<subSubSection pageId="29" pageNumber="256" type="discussion">
<paragraph blockId="29.[2753,3553,3510,4075]" pageId="29" pageNumber="256">
<taxonomicName baseAuthorityName="Peters" baseAuthorityYear="1861" box="[2755,3164,3668,3707]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="29" pageNumber="256" phylum="Chordata" rank="species" species="obscurus">
