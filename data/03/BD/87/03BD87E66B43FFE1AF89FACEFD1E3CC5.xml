<quantity box="[754,891,330,357]" metricMagnitude="-3" metricUnit="m" metricValue="5.1" metricValueMax="5.6" metricValueMin="4.6" pageId="36" pageNumber="37" unit="mm" value="5.1" valueMax="5.6" valueMin="4.6">4.6–5.6 mm</quantity>
