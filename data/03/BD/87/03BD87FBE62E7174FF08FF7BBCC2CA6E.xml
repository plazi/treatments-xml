<emphasis box="[1078,1229,258,283]" italics="true" pageId="3" pageNumber="196">
<taxonomicName box="[1078,1225,258,283]" class="Secernentea" family="Gnathostomatidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Spirurida" pageId="3" pageNumber="196" phylum="Nematoda" rank="genus">Gnathosoma</taxonomicName>
