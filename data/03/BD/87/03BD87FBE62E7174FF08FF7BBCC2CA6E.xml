<number box="[1204,1218,367,391]" pageId="3" pageNumber="196" value="3.0">3</number>
sclerites, unpaired central and 
<number box="[280,294,403,427]" pageId="3" pageNumber="196" value="2.0">2</number>
narrow punctate lateral sclerites bearing bases of setae 
<emphasis box="[913,933,404,427]" italics="true" pageId="3" pageNumber="196">vi</emphasis>
, 
<emphasis box="[947,972,404,427]" italics="true" pageId="3" pageNumber="196">ve</emphasis>
, 
<emphasis box="[985,1004,404,427]" italics="true" pageId="3" pageNumber="196">si</emphasis>
and 
<emphasis box="[1057,1080,404,427]" italics="true" pageId="3" pageNumber="196">se</emphasis>
