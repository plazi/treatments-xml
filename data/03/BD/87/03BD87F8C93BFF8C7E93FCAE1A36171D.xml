<taxonomicName authority="(Marenzeller, 1885)" baseAuthorityName="Marenzeller" baseAuthorityYear="1885" box="[151,768,1877,1903]" class="Polychaeta" family="Serpulidae" genus="Omphalopomopsis" kingdom="Animalia" order="Sabellida" pageId="68" pageNumber="69" phylum="Annelida" rank="species" species="langerhansii">
