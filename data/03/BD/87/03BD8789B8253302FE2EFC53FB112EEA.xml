<paragraph blockId="2.[498,747,980,1005]" box="[498,747,980,1005]" pageId="2" pageNumber="1690">
<heading box="[498,747,980,1005]" centered="true" fontSize="36" level="2" pageId="2" pageNumber="1690" reason="7">
<taxonomicName authority="Ruta, 2011" authorityName="Ruta" authorityYear="2011" box="[498,640,980,1005]" class="Insecta" family="Scirtidae" genus="Chilarboreus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="1690" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[498,640,980,1005]" italics="true" pageId="2" pageNumber="1690">Chilarboreus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[647,747,981,1005]" pageId="2" pageNumber="1690" rank="genus">gen. nov.</taxonomicNameLabel>
</heading>
</paragraph>
