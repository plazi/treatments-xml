<quantity box="[429,559,761,785]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.5" metricValueMin="1.6" pageId="12" pageNumber="275" unit="mm" value="2.05" valueMax="2.5" valueMin="1.6">1.6±2.5 mm</quantity>
