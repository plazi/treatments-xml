<paragraph blockId="13.[1026,1829,2000,2569]" pageId="13" pageNumber="240">
<taxonomicName authorityName="Oey" authorityYear="1960" box="[1027,1393,2212,2251]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="13" pageNumber="240" phylum="Chordata" rank="species" species="sumbae">Hipposideros sumbae</taxonomicName>
