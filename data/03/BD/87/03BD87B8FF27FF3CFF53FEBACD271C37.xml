<collectionCode LSID="urn:lsid:biocol.org:col:33211" box="[434,481,755,778]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33211" name="Senckenberg Deutsches Entomologisches Institut" pageId="155" pageNumber="156">DEI</collectionCode>
