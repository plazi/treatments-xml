<subSubSection pageId="23" pageNumber="250" type="discussion">
<paragraph blockId="23.[2747,3548,1222,1794]" pageId="23" pageNumber="250">
<taxonomicName authorityName="Blyth" authorityYear="1853" box="[2749,3176,1382,1421]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="23" pageNumber="250" phylum="Chordata" rank="species" species="cineraceus">
