<paragraph blockId="18.[192,1440,1249,1306]" pageId="18" pageNumber="17">
<emphasis bold="true" box="[192,408,1249,1274]" pageId="18" pageNumber="17">Type specimen.</emphasis>
Male 
<typeStatus box="[489,594,1250,1274]" pageId="18" pageNumber="17">holotype</typeStatus>
(
<collectionCode box="[610,673,1250,1274]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:33791" lsid="urn:lsid:biocol.org:col:33791" name="Museum of Comparative Zoology" pageId="18" pageNumber="17" type="Museum">MCZ</collectionCode>
