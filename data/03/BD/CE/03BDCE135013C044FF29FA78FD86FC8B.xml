<collectingRegion box="[760,873,727,752]" country="Brazil" name="Sao Paulo" pageId="4" pageNumber="571">São Paulo</collectingRegion>
State, 
<collectingCountry box="[949,1021,727,752]" name="Brazil" pageId="4" pageNumber="571">Brazil</collectingCountry>
