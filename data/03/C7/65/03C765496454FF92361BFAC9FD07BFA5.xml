<subSubSection pageId="14" pageNumber="15" type="discussion">
<paragraph blockId="14.[264,1323,283,950]" pageId="14" pageNumber="15">
<emphasis bold="true" box="[312,433,403,429]" pageId="14" pageNumber="15">Remarks.</emphasis>
<taxonomicName authority="Forster, 1869" authorityName="Forster" authorityYear="1869" box="[444,783,404,430]" class="Insecta" family="Figitidae" genus="Trybliographa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="genus">
