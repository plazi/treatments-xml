<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[746,835,964,990]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="41" pageNumber="42">USNM</collectionCode>
