<date box="[854,1049,284,310]" pageId="30" pageNumber="31" value="1982-08-11" valueMax="1982-08-13" valueMin="1982-08-11">
<number box="[854,922,284,310]" pageId="30" pageNumber="31" value="12.0" valueMax="13.0" valueMin="11.0">11­13</number>
