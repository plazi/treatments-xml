<taxonomicName box="[264,342,364,390]" class="Insecta" family="Tortricidae" genus="Anopinella" kingdom="Animalia" order="Lepidoptera" pageId="30" pageNumber="90" phylum="Arthropoda" rank="species" species="cuzco">Cuzco</taxonomicName>
, 
<collectingCountry box="[353,411,364,390]" name="Peru" pageId="30" pageNumber="31">Peru</collectingCountry>
