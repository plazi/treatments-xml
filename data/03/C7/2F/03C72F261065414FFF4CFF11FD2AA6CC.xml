/ 
<number box="[151,205,1374,1398]" pageId="5" pageNumber="266" value="0.78">0.78</number>
; patella I 
<number box="[325,379,1374,1398]" pageId="5" pageNumber="266" value="0.24">0.24</number>
; tibia I 
<number box="[475,529,1374,1398]" pageId="5" pageNumber="266" value="0.72">0.72</number>
; metatarsus I 
<number box="[695,749,1374,1398]" pageId="5" pageNumber="266" value="0.72">0.72</number>
; tarsus I 
