, PLE 
<number box="[585,638,1305,1329]" pageId="5" pageNumber="266" value="0.09">0.09</number>
, AME-AME 
<number box="[792,845,1305,1329]" pageId="5" pageNumber="266" value="0.01">0.01</number>
, PME-PME 
<number box="[989,1042,1305,1329]" pageId="5" pageNumber="266" value="0.02">0.02</number>
, AME-ALE 
<number box="[1188,1241,1305,1329]" pageId="5" pageNumber="266" value="0.02">0.02</number>
