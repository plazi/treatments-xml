<number box="[1313,1432,562,586]" pageId="13" pageNumber="274" value="0.6100000000000001" valueMax="0.66" valueMin="0.56">0.56–0.66</number>
; femur I length 
<number box="[331,450,599,623]" pageId="13" pageNumber="274" value="0.81" valueMax="0.84" valueMin="0.78">0.78–0.84</number>
. Females (n= 
<number box="[612,642,599,623]" pageId="13" pageNumber="274" value="10.0">10</number>
): Total length 
<number box="[816,920,599,623]" pageId="13" pageNumber="274" value="1.6400000000000001" valueMax="1.78" valueMin="1.5">1.5–1.78</number>
; carapace length 
<number box="[1127,1246,599,623]" pageId="13" pageNumber="274" value="0.7" valueMax="0.74" valueMin="0.66">0.66–0.74</number>
; carapace width 
<number box="[151,255,637,661]" pageId="13" pageNumber="274" value="0.5700000000000001" valueMax="0.6" valueMin="0.54">0.54–0.6</number>
; femur I length 
<number box="[438,557,637,661]" pageId="13" pageNumber="274" value="0.73" valueMax="0.78" valueMin="0.68">0.68–0.78</number>
.
</paragraph>
