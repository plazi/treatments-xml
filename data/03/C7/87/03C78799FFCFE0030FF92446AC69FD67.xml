<quantity box="[587,702,315,337]" metricMagnitude="-3" metricUnit="m" metricValue="2.25" metricValueMax="2.3" metricValueMin="2.2" pageId="36" pageNumber="1587" unit="mm" value="2.25" valueMax="2.3" valueMin="2.2">2,2-2,3mm</quantity>
