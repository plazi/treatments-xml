(
<figureCitation box="[159,238,1725,1750]" captionStart="FIGURE 2" captionStartId="4.[151,250,1799,1821]" captionTargetBox="[163,1428,778,1761]" captionTargetId="figure@4.[151,1436,769,1778]" captionTargetPageId="4" captionText="FIGURE 2. Lannapsyche altilis n. sp., male genitalia and wings. 2 A – 2 D, male genitalia: 2 A, left lateral; 2 B, dorsal; 2 C, ventral; 2 D, phallus, left lateral; 2 Dv, phallus, ventral. 2 Fa – 2 Fp, right wings, dorsal: 2 Fa, forewing; 2 Fp, hind wing." httpUri="https://zenodo.org/record/891588/files/figure.png" pageId="3" pageNumber="84">Figs. 2</figureCitation>
, 
<figureCitation box="[250,265,1726,1750]" captionStart="FIGURES 3 – 4" captionStartId="5.[151,264,1966,1989]" captionTargetBox="[163,1420,1058,1949]" captionTargetId="figure@5.[151,1436,1045,1973]" captionTargetPageId="5" captionText="FIGURES 3 – 4. Lannapsyche spp., female genitalia. 3, L. setschuana Malicky &amp; Chantaramongkol 1995: 3 A, left lateral; 3 B, dorsal; 3 C, ventral. 4, L. altilis sp. nov.: 4 A, left lateral; 4 B, dorsal; 4 C, ventral." httpUri="https://zenodo.org/record/891590/files/figure.png" pageId="3" pageNumber="84">4</figureCitation>
)
