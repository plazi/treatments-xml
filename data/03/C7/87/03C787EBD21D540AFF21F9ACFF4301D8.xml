<number box="[870,918,296,320]" pageId="6" pageNumber="498" value="13.8">13.8</number>
/ 
<number box="[928,1027,296,320]" pageId="6" pageNumber="498" value="11.0" valueMax="13.2" valueMin="8.8">8.8–13.2</number>
; prothorax length, 
<number box="[1252,1337,295,320]" pageId="6" pageNumber="498" value="3.25" valueMax="3.4" valueMin="3.1">3.1–3.4</number>
/ 
<number box="[1347,1431,295,320]" pageId="6" pageNumber="498" value="2.8" valueMax="3.5" valueMin="2.1">2.1–3.5</number>
; greatest prothorax width, 
<number box="[438,525,332,357]" pageId="6" pageNumber="498" value="1.65" valueMax="1.7" valueMin="1.6">1.6–1.7</number>
/ 
<number box="[534,618,332,357]" pageId="6" pageNumber="498" value="1.5" valueMax="1.9" valueMin="1.1">1.1–1.9</number>
; elytral length, 8.0– 
<number box="[842,877,332,357]" pageId="6" pageNumber="498" value="8.9">8.9</number>
/ 
<number box="[886,970,332,357]" pageId="6" pageNumber="498" value="7.25" valueMax="8.8" valueMin="5.7">5.7–8.8</number>
