<taxonomicName baseAuthorityName="Moosa" baseAuthorityYear="1986" box="[999,1171,507,534]" class="Malacostraca" family="Tetrasquillidae" genus="Kasim" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stomatopoda" pageId="6" pageNumber="379" phylum="Arthropoda" rank="species" species="philippinensis">
