<taxonomicName authority="PUTHZ, 2015" authorityName="PUTHZ" authorityYear="2015" box="[277,586,543,565]" class="Insecta" family="Staphylinidae" genus="Stenus" kingdom="Animalia" order="Coleoptera" pageId="54" pageNumber="1605" phylum="Arthropoda" rank="species" species="chimalpopoca">
