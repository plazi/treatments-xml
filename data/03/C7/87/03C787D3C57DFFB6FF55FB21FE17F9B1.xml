, protarsus 
<number box="[421,473,1689,1714]" pageId="1" pageNumber="418" value="0.36">0.36</number>
, mesotarsus 
<number box="[616,668,1690,1714]" pageId="1" pageNumber="418" value="0.32">0.32</number>
, metatarsus 
