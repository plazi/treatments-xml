/ 
<number box="[862,898,1612,1637]" pageId="3" pageNumber="495" value="5.9">5.9</number>
; prothorax length, 
<number box="[1128,1164,1612,1637]" pageId="3" pageNumber="495" value="1.6">1.6</number>
/ 
<number box="[1173,1209,1612,1637]" pageId="3" pageNumber="495" value="1.2">1.2</number>
; greatest prothorax width, 
<number box="[228,261,1648,1672]" pageId="3" pageNumber="495" value="1.1">1.1</number>
