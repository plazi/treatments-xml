<subSubSection pageId="6" pageNumber="7" type="diagnosis">
<paragraph blockId="6.[189,457,403,429]" box="[189,457,403,429]" pageId="6" pageNumber="7">
<heading bold="true" box="[189,457,403,429]" fontSize="11" level="3" pageId="6" pageNumber="7" reason="3">
