<paragraph blockId="6.[192,1440,863,1055]" pageId="6" pageNumber="5">
<taxonomicName box="[192,391,897,921]" class="Insecta" family="Tineidae" genus="Tinea" kingdom="Animalia" order="Lepidoptera" pageId="6" pageNumber="5" phylum="Arthropoda" rank="species" species="pustulella">
<emphasis box="[192,391,897,921]" italics="true" pageId="6" pageNumber="5">Tinea pustulella</emphasis>
</taxonomicName>
