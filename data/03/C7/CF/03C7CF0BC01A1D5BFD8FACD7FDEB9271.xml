<subSubSection pageId="47" pageNumber="48" type="description">
<paragraph blockId="47.[189,427,709,735]" box="[189,427,709,735]" pageId="47" pageNumber="48">
<heading bold="true" box="[189,427,709,735]" fontSize="11" level="3" pageId="47" pageNumber="48" reason="3">
