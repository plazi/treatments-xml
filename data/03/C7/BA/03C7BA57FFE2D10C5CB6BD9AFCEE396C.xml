<number box="[947,961,1833,1857]" pageId="1" pageNumber="65" value="4.0">4</number>
and 
<number box="[1021,1062,1833,1857]" pageId="1" pageNumber="65" value="7.0" valueMax="8.0" valueMin="6.0">6–8</number>
