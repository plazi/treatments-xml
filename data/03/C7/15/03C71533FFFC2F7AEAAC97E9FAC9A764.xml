<subSubSection box="[199,479,294,321]" pageId="11" pageNumber="506" type="diagnosis">
<paragraph blockId="11.[151,1436,150,825]" box="[199,479,294,321]" pageId="11" pageNumber="506">
<emphasis bold="true" box="[199,322,294,320]" pageId="11" pageNumber="506">Diagnosis.</emphasis>
