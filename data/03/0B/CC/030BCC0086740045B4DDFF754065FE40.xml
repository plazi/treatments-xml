<taxonomicName baseAuthorityName="Wilkinson" baseAuthorityYear="1928" box="[225,385,979,1003]" class="Insecta" family="Braconidae" genus="Dolichogenidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="species" species="platyedrae">D. platyedrae</taxonomicName>
, 
<taxonomicName baseAuthorityName="Ashmead" baseAuthorityYear="1904" box="[398,528,979,1002]" class="Insecta" family="Braconidae" genus="Dolichogenidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="species" species="stantoni">D. stantoni</taxonomicName>
,
</emphasis>
and 
<taxonomicName authorityName="Fagan-Jeffries and Austin" authorityYear="2018" box="[591,754,979,1003]" class="Insecta" family="Braconidae" genus="Dolichogenidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="421" phylum="Arthropoda" rank="species" species="xenomorph">
