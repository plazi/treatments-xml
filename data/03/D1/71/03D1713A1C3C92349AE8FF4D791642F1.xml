<number box="[508,553,1533,1559]" pageId="16" pageNumber="17" value="4.5" valueMax="7.0" valueMin="2.0">2–7</number>
with 
<number box="[624,639,1533,1559]" pageId="16" pageNumber="17" value="1.0">1</number>
