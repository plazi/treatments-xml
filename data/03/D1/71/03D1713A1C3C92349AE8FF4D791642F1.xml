<number box="[151,195,1645,1671]" pageId="16" pageNumber="17" value="6.0" valueMax="7.0" valueMin="5.0">5–7</number>
with 
<number box="[261,276,1645,1671]" pageId="16" pageNumber="17" value="2.0">2</number>
