<number box="[1084,1136,1570,1596]" pageId="16" pageNumber="17" value="5.5" valueMax="7.0" valueMin="4.0">4–7</number>
), pereopods 
<number box="[1299,1347,1570,1596]" pageId="16" pageNumber="17" value="4.5" valueMax="7.0" valueMin="2.0">2–7</number>
with 
<number box="[1421,1437,1570,1596]" pageId="16" pageNumber="17" value="1.0">1</number>
