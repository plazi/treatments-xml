<taxonomicName authorityName="Grismado &amp; Ramírez" authorityYear="2013" baseAuthorityName="Grismado &amp; Ramírez" baseAuthorityYear="2013" box="[212,385,1033,1056]" class="Arachnida" family="Oonopidae" genus="Neotrops" kingdom="Animalia" order="Araneae" pageId="77" pageNumber="77" phylum="Arthropoda" rank="species" species="nigromaculatus">
