<paragraph blockId="50.[93,620,1352,1759]" lastBlockId="50.[667,1194,1352,1759]" pageId="50" pageNumber="50">
FEMALE (
<typeStatus box="[250,356,1708,1729]" pageId="50" pageNumber="50">paratype</typeStatus>
, PBI_ 
<accessionNumber box="[446,607,1707,1729]" httpUri="http://www.ncbi.nlm.nih.gov/protein/OON43357" pageId="50" pageNumber="50">OON 43357</accessionNumber>
