: 
<collectingRegion box="[623,711,1635,1661]" country="Colombia" name="Boyaca" pageId="59" pageNumber="60">Boyaca</collectingRegion>
: 
<collectingMunicipality box="[724,961,1634,1660]" pageId="59" pageNumber="60">Municipio Arcabuco</collectingMunicipality>
, c 
<quantity box="[995,1069,1634,1661]" metricMagnitude="4" metricUnit="m" metricValue="1.5" pageId="59" pageNumber="60" unit="km" value="15.0">15 km</quantity>
on the road towards 
<location LSID="urn:lsid:plazi:treatment:03D1E63498687074C5CDFD5CFD48F881:8EA701F998687074C066F9E0FAB4F9CC" box="[1315,1382,1634,1660]" country="Colombia" municipality="Municipio Arcabuco" name="Tunja" pageId="59" pageNumber="60" stateProvince="Boyaca">Tunja</location>
