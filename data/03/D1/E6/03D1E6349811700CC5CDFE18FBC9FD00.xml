<subSubSection pageId="66" pageNumber="67" type="description">
<paragraph blockId="66.[136,1452,486,580]" box="[136,1452,486,507]" pageId="66" pageNumber="67">
<emphasis box="[136,1452,486,507]" italics="true" pageId="66" pageNumber="67">Thallus corticolus, erectus. Laciniae intricate ramulosae, canaliculatae, pseudocyphelliis linearibus instructis. Pycnidia non visa.</emphasis>
</paragraph>
<paragraph blockId="66.[136,1452,486,580]" pageId="66" pageNumber="67">
<emphasis box="[189,1387,522,543]" italics="true" pageId="66" pageNumber="67">Apothecia subterminalia et lateralia vel terminalia et apicalia. Sporae ellipsoideae. Acida usnicum et psoromicum continens</emphasis>
. 
<emphasis bold="true" box="[136,186,558,579]" pageId="66" pageNumber="67">Type</emphasis>
:― 
<materialsCitation box="[214,1356,558,580]" collectingDate="1984-12-03" collectionCode="VEN" collectorName="L. Brako" country="Venezuela" elevation="1850" location="Cerro La Neblina" pageId="66" pageNumber="67" specimenCount="1" stateProvince="Amazonas" typeStatus="holotype">
<collectingCountry box="[214,362,558,579]" name="Venezuela" pageId="66" pageNumber="67">VENEZUELA</collectingCountry>
. 
<collectingRegion box="[370,475,558,579]" country="Venezuela" name="Amazonas" pageId="66" pageNumber="67">Amazonas</collectingRegion>
: 
<location LSID="urn:lsid:plazi:treatment:03D1E6349811700CC5CDFE18FBC9FD00:8EA701F99811700DC4A1FDACFD5CFDF4" box="[484,654,558,580]" country="Venezuela" name="Cerro La Neblina" pageId="66" pageNumber="67" stateProvince="Amazonas">Cerro La Neblina</location>
, 
<quantity box="[664,739,558,580]" metricMagnitude="3" metricUnit="m" metricValue="1.85" pageId="66" pageNumber="67" unit="m" value="1850.0">
<elevation box="[664,739,558,580]" metricMagnitude="3" metricUnit="m" metricValue="1.85" pageId="66" pageNumber="67" unit="m" value="1850.0">1850 m</elevation>
</quantity>
, 
<date box="[749,921,558,580]" pageId="66" pageNumber="67" value="1984-12-03">
<collectingDate box="[749,921,558,580]" pageId="66" pageNumber="67" value="1984-12-03">3 December 1984</collectingDate>
</date>
, 
<collectorName box="[931,1017,558,580]" pageId="66" pageNumber="67">L. Brako</collectorName>
7702 (
<typeStatus box="[1083,1169,558,580]" pageId="66" pageNumber="67">holotype</typeStatus>
<collectionCode box="[1174,1226,558,579]" country="Venezuela" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15643" lsid="urn:lsid:biocol.org:col:15643" name="Fundación Instituto Botánico de Venezuela" pageId="66" pageNumber="67" type="Herbarium">VEN</collectionCode>
