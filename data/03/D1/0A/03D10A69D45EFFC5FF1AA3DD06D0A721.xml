<emphasis box="[244,333,721,744]" italics="true" pageId="13" pageNumber="14">
pd 
<number box="[274,284,721,744]" pageId="13" pageNumber="14" value="1.0">1</number>
-pd 
<number box="[321,333,721,744]" pageId="13" pageNumber="14" value="3.0">3</number>
</emphasis>
