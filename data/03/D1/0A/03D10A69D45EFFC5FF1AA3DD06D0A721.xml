<emphasis box="[603,662,849,872]" italics="true" pageId="13" pageNumber="14">
J 
<number box="[618,629,849,872]" pageId="13" pageNumber="14" value="1.0">1</number>
-J 
<number box="[650,662,849,872]" pageId="13" pageNumber="14" value="4.0">4</number>
