.................................................................... 
<taxonomicName box="[1323,1339,721,744]" class="Arachnida" family="Laelapidae" genus="Anystipalpus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="13" pageNumber="14" phylum="Arthropoda" rank="species" species="livshitsi">A</taxonomicName>
