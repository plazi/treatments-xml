<taxonomicName authority="Dana, 1852" authorityName="Dana" authorityYear="1852" box="[857,1123,1041,1068]" class="Malacostraca" family="Mysidae" genus="Mysidium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mysida" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
