<subSubSection lastPageId="29" lastPageNumber="84" pageId="28" pageNumber="83" type="materials_examined">
<paragraph blockId="28.[718,1201,1378,1402]" box="[718,1201,1378,1402]" pageId="28" pageNumber="83">
