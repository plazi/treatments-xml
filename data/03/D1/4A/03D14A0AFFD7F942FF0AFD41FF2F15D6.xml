shares with the species 
<taxonomicName authority="(Kew, 1910)" authorityName="Kew" authorityYear="1910" baseAuthorityName="Kew" baseAuthorityYear="1910" box="[966,1281,1177,1201]" class="Arachnida" family="Neobisiidae" genus="Neobisium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pseudoscorpiones" pageId="7" pageNumber="208" phylum="Arthropoda" rank="species" species="carpenteri">
