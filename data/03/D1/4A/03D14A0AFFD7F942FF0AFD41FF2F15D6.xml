, 
<taxonomicName authority="Beier, 1939" authorityName="Beier" authorityYear="1939" box="[816,1239,1211,1236]" class="Arachnida" family="Neobisiidae" genus="Neobisium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pseudoscorpiones" pageId="7" pageNumber="208" phylum="Arthropoda" rank="subSpecies" species="ischyrum" subSpecies="balearicum">
