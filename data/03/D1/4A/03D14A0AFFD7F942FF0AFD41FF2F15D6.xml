and 
<taxonomicName authority="Beier, 1939" authorityName="Beier" authorityYear="1939" box="[673,945,1246,1271]" class="Arachnida" family="Neobisiidae" genus="Neobisium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pseudoscorpiones" pageId="7" pageNumber="208" phylum="Arthropoda" rank="species" species="ventalloi">
