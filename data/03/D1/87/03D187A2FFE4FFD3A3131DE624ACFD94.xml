<subSubSection pageId="16" pageNumber="39" type="etymology">
<paragraph blockId="16.[264,1323,283,630]" pageId="16" pageNumber="39">
<emphasis bold="true" box="[312,452,363,389]" pageId="16" pageNumber="39">Etymology.</emphasis>
