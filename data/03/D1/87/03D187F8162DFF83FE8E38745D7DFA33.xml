<collectingCountry box="[300,398,1479,1502]" name="Armenia" pageId="9" pageNumber="268">Armenia</collectingCountry>
, Azerbaijan), Central Asia (
<collectingCountry box="[688,841,1479,1502]" name="Turkmenistan" pageId="9" pageNumber="268">Turkmenistan</collectingCountry>
), 
<collectingCountry box="[858,929,1479,1502]" name="Russia" pageId="9" pageNumber="268">Russia</collectingCountry>
