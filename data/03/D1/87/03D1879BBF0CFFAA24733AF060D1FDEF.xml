<taxonomicName authorityName="Kirby" authorityYear="1802" box="[531,608,509,532]" class="Insecta" family="Stylopidae" genus="Stylops" kingdom="Animalia" order="Strepsiptera" pageId="5" pageNumber="310" phylum="Arthropoda" rank="genus">
