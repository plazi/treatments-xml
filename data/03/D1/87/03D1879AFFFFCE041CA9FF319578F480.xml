<collectingCountry box="[966,1015,354,377]" name="Italy" pageId="2" pageNumber="198">Italy</collectingCountry>
and 
<collectingRegion box="[1070,1132,354,377]" country="Italy" name="Sicilia" pageId="2" pageNumber="198">Sicily</collectingRegion>
(
<treatmentCitation author="Abbazzi, P. &amp; Maggini, L." page="74" pageId="2" pageNumber="198" year="2009">
<bibRefCitation author="Abbazzi, P. &amp; Maggini, L." box="[1148,1432,354,377]" pageId="2" pageNumber="198" pagination="29 - 216" refId="ref1642" refString="Abbazzi, P. &amp; Maggini, L. (2009) Elenco sistematico-faunistico dei Curculionoidea italiani, Scolytidae e Platypodidae esclusi (Insecta, Coleoptera). Aldrovandia, 5, 29 - 216." type="journal article" year="2009">Abbazzi &amp; Maggini 2009</bibRefCitation>
: 74
</treatmentCitation>
, as 
<taxonomicName authorityName="Motschulsky" authorityYear="1860" box="[220,400,389,412]" class="Insecta" family="Curculionidae" genus="Stephanocleonus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="198" phylum="Arthropoda" rank="genus">
<emphasis box="[220,400,389,412]" italics="true" pageId="2" pageNumber="198">Stephanocleonus</emphasis>
</taxonomicName>
<taxonomicName authorityName="Alonso-Zarazaga et al." authorityYear="2017" baseAuthorityName="Hoppe" baseAuthorityYear="1795" box="[406,579,389,412]" class="Insecta" family="Curculionidae" genus="Coniocleonus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="198" phylum="Arthropoda" rank="species" species="cicatricosus" subGenus="S.">
(
<emphasis box="[415,428,389,412]" italics="true" pageId="2" pageNumber="198">S</emphasis>
.) 
<emphasis box="[450,579,389,412]" italics="true" pageId="2" pageNumber="198">cicatricosus</emphasis>
</taxonomicName>
).
</treatmentCitationGroup>
</paragraph>
