; 
<collectionCode box="[1389,1436,301,327]" pageId="11" pageNumber="12">LIII</collectionCode>
<number box="[151,180,338,364]" pageId="11" pageNumber="12" value="60.0">60</number>
: 
<number box="[204,233,338,364]" pageId="11" pageNumber="12" value="18.0">18</number>
