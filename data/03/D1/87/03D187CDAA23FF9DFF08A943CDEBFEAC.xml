<taxonomicName authority="Wang &amp; Yang" authorityName="Wang &amp; Yang" box="[1191,1436,792,815]" class="Insecta" family="Dolichopodidae" genus="Chrysotus" kingdom="Animalia" order="Diptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="species" species="serratus">
