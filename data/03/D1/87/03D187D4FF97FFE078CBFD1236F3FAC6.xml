. 
<taxonomicName authorityName="Khaustov &amp; Hugo-Coetzee &amp; Ermilov &amp; Theron" authorityYear="2019" box="[280,559,1160,1184]" class="Arachnida" family="Microdispidae" genus="Sidorchukdispus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="3" pageNumber="107" phylum="Arthropoda" rank="species" species="ekaterinae">
