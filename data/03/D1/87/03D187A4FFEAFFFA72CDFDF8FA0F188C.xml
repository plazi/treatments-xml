</taxonomicName>
subgenus 
<taxonomicName authority="Gasparrini, 1844" authorityName="Gasparrini" authorityYear="1844" box="[829,931,1929,1950]" class="Magnoliopsida" family="Moraceae" genus="Urostigma" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="4" pageNumber="95" phylum="Tracheophyta" rank="genus">
