<quantity box="[346,462,1574,1598]" metricMagnitude="-2" metricUnit="m" metricValue="1.65" metricValueMax="2.0" metricValueMin="1.3" pageId="17" pageNumber="2202" unit="mm" value="16.5" valueMax="20.0" valueMin="13.0">13±20 mm</quantity>
