<quantity box="[801,888,1638,1662]" metricMagnitude="1" metricUnit="m" metricValue="2.75" metricValueMax="3.0" metricValueMin="2.5" pageId="17" pageNumber="2202" unit="m" value="27.5" valueMax="30.0" valueMin="25.0">
