<taxonomicName box="[252,397,427,451]" class="Sipunculidea" family="Aspidosiphonidae" genus="Aspidosiphon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Sipunculiformes" pageId="18" pageNumber="2203" phylum="Sipuncula" rank="genus">
<emphasis box="[252,397,427,451]" italics="true" pageId="18" pageNumber="2203">Aspidosiphon</emphasis>
</taxonomicName>
; thus, comparable with 
<taxonomicName authorityName="Oken" authorityYear="1815" box="[703,743,427,451]" class="Hirudinea" family="Hirudinoidea (awaiting allocation)" genus="Gol" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hirudinoidea (awaiting allocation)" pageId="18" pageNumber="2203" phylum="Annelida" rank="genus">
