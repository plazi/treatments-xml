Tielou gang, 
<date box="[415,588,1415,1439]" pageId="5" pageNumber="2190" value="1992-03-28">28 March 1992</date>
, 
<specimenCount box="[605,753,1415,1439]" pageId="5" pageNumber="2190" type="generic">16 specimens</specimenCount>
