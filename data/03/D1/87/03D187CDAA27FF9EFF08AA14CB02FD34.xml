<subSubSection box="[197,1113,263,290]" pageId="7" pageNumber="8" type="etymology">
<paragraph blockId="7.[151,1437,152,663]" box="[197,1113,263,290]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[197,337,263,289]" pageId="7" pageNumber="8">Etymology.</emphasis>
