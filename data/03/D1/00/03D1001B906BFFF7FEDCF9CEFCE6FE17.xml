. 
<taxonomicName authority="Fabricius, 1804" authorityName="Fabricius" authorityYear="1804" box="[323,730,1548,1574]" class="Insecta" family="Ichneumonidae" genus="Ophion" kingdom="Animalia" order="Hymenoptera" pageId="6" pageNumber="65" phylum="Arthropoda" rank="species" species="pennator">
<emphasis bold="true" box="[323,530,1548,1574]" italics="true" pageId="6" pageNumber="65">Ophion pennator</emphasis>
<bibRefCitation author="Fabricius" box="[538,730,1548,1574]" pageId="6" pageNumber="67" refString="Fabricius, J. C. (1804) Systema Piezatorum. Carolum Reichard, Brunsviga. 439 pp." type="book" year="1804">
