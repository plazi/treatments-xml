<emphasis bold="true" box="[151,299,464,487]" pageId="1" pageNumber="590">Type species.</emphasis>
<taxonomicName box="[305,572,463,487]" class="Insecta" family="Psocidiidae" genus="Hypopsylla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Permopsocida" pageId="1" pageNumber="590" phylum="Arthropoda" rank="species" species="belmontensis" status="sp. nov.">
<emphasis box="[305,572,463,487]" italics="true" pageId="1" pageNumber="590">Hypopsylla belmontensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[579,665,464,487]" pageId="1" pageNumber="590" rank="species">
<emphasis bold="true" box="[579,665,464,487]" pageId="1" pageNumber="590">sp. nov.</emphasis>
