<subSubSection box="[457,903,851,875]" pageId="10" pageNumber="2491" type="nomenclature">
<paragraph blockId="10.[457,903,851,875]" box="[457,903,851,875]" pageId="10" pageNumber="2491">
<heading box="[457,903,851,875]" centered="true" fontSize="10" level="2" pageId="10" pageNumber="2491" reason="4">
<emphasis bold="true" box="[457,814,851,875]" pageId="10" pageNumber="2491">
Genus 
<taxonomicName authority="Ohtsuka &amp; Nishida &amp; Machida, 2005" authorityName="Ohtsuka &amp; Nishida &amp; Machida" authorityYear="2005" box="[548,814,851,875]" class="Hexanauplia" family="Arietellidae" genus="Protoparamisophria" kingdom="Animalia" order="Calanoida" pageId="10" pageNumber="2491" phylum="Arthropoda" rank="genus" status="n. gen.">
<emphasis bold="true" box="[548,814,851,875]" italics="true" pageId="10" pageNumber="2491">Protoparamisophria</emphasis>
</taxonomicName>
</emphasis>
<taxonomicNameLabel box="[822,903,851,875]" pageId="10" pageNumber="2491" rank="genus">n. gen.</taxonomicNameLabel>
</heading>
</paragraph>
