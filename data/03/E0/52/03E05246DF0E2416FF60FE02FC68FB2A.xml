May 
<number box="[1129,1182,1042,1063]" pageId="1" pageNumber="43" value="2010.0">2010</number>
</date>
, coll. LI-CHAO 
<collectionCode box="[1369,1433,1042,1063]" pageId="1" pageNumber="43">TIAN</collectionCode>
. 
<typeStatus box="[151,244,1073,1094]" pageId="1" pageNumber="43">Paratype</typeStatus>
: 
<specimenCount box="[253,345,1072,1094]" pageId="1" pageNumber="43" type="female">
<number box="[253,266,1072,1093]" pageId="1" pageNumber="43" value="1.0">1</number>
female
</specimenCount>
, 
<collectingCountry box="[355,419,1072,1094]" name="China" pageId="1" pageNumber="43">China</collectingCountry>
