June 
<number box="[785,838,1072,1093]" pageId="1" pageNumber="43" value="1980.0">1980</number>
</date>
, coll. 
<collectionCode box="[900,948,1073,1094]" httpUri="http://grbio.org/cool/7g17-y511" name="Lycée Félix Esclangon, Région PACA" pageId="1" pageNumber="43">FEN</collectionCode>
<collectionCode box="[955,1000,1073,1094]" pageId="1" pageNumber="43">LIU</collectionCode>
