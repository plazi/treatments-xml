<collectionCode box="[1378,1433,682,706]" pageId="4" pageNumber="405">oval</collectionCode>
, 
<collectionCode box="[151,283,718,742]" pageId="4" pageNumber="405">comprising</collectionCode>
<collectionCode box="[295,435,718,742]" pageId="4" pageNumber="405">three-fourth</collectionCode>
