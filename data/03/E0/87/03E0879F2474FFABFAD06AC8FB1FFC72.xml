<number box="[774,786,974,998]" pageId="3" pageNumber="206" value="1.0">1</number>
) large size, up to at least 
<quantity box="[1073,1169,974,998]" metricMagnitude="-1" metricUnit="m" metricValue="9.9" pageId="3" pageNumber="206" unit="mm" value="990.0">990 mm</quantity>
; (
<number box="[1189,1204,974,998]" pageId="3" pageNumber="206" value="2.0">2</number>
) 
<number box="[1220,1247,974,998]" pageId="3" pageNumber="206" value="19.0">19</number>
â€“ 
<number box="[1263,1290,974,998]" pageId="3" pageNumber="206" value="19.0">19</number>
â€“ 
<number box="[1306,1333,974,998]" pageId="3" pageNumber="206" value="17.0">17</number>
DSR, all strongly keeled; (
<number box="[347,361,1009,1033]" pageId="3" pageNumber="206" value="3.0">3</number>
) 
<number box="[377,405,1009,1033]" pageId="3" pageNumber="206" value="29.0">29</number>
