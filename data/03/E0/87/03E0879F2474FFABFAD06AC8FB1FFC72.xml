; (
<number box="[996,1011,1043,1068]" pageId="3" pageNumber="206" value="6.0">6</number>
) 
<number box="[1027,1069,1044,1068]" pageId="3" pageNumber="206" value="8.5" valueMax="9.0" valueMin="8.0">8â€“9</number>
