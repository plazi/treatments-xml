<taxonomicName authority="Lundstrom 1912" authorityName="Lundstrom" authorityYear="1912" box="[698,1134,1324,1350]" class="Insecta" family="Mycetophilidae" genus="Exechia" kingdom="Animalia" order="Diptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="spinuligera">
