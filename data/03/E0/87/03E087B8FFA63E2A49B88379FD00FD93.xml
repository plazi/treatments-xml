(
<figureCitation box="[159,233,386,412]" captionStart="FIGURES 1 – 3" captionStartId="4.[151,269,1287,1311]" captionTargetBox="[253,1336,482,1249]" captionTargetId="figure@4.[240,1347,474,1263]" captionTargetPageId="4" captionText="FIGURES 1 – 3. Habitus. 1. Quadrastichus mendeli Ψ; 2. Selitrichodes kryceri “ dark ” specimen Ψ; 3. Selitrichodes kryceri “ light ” specimen Ψ" httpUri="https://zenodo.org/record/184547/files/figure.png" pageId="5" pageNumber="6">Figs 1</figureCitation>
, 
<figureCitation box="[248,293,386,412]" captionStart="FIGURES 4 – 9" captionStartId="6.[151,269,1810,1834]" captionTargetBox="[272,1299,196,1777]" captionTargetId="figure@6.[230,1337,112,1802]" captionTargetPageId="6" captionText="FIGURES 4 – 9. Quadrastichus mendeli Ψ. 4. Base of fore wing; 5. Antenna; 6. Head, frontal view; 7. Malar sulcus; 8. Mesosoma; 9. Gaster." httpUri="https://zenodo.org/record/184548/files/figure.png" pageId="5" pageNumber="6">
