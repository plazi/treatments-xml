<geoCoordinate box="[874,927,1405,1431]" degrees="48" direction="south" orientation="latitude" pageId="11" pageNumber="212" precision="55555" value="-48.0">48°S</geoCoordinate>
to 
<geoCoordinate box="[963,1016,1405,1432]" degrees="56" direction="south" orientation="latitude" pageId="11" pageNumber="212" precision="55555" value="-56.0">56°S</geoCoordinate>
reported in 
<collectingCountry box="[1148,1248,1405,1431]" name="Falkland Islands" pageId="11" pageNumber="212">Malvinas</collectingCountry>
/ 
<collectingCountry box="[1256,1436,1405,1431]" name="Falkland Islands" pageId="11" pageNumber="212">Falkland Islands</collectingCountry>
as 
<taxonomicName authorityName="Hartlaub" authorityYear="1901" box="[227,445,1441,1467]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" kingdom="Animalia" order="Leptothecata" pageId="11" pageNumber="212" phylum="Cnidaria" rank="species" species="allmani">
<emphasis box="[227,445,1441,1467]" italics="true" pageId="11" pageNumber="212">Sertularella allmani</emphasis>
