<subSubSection pageId="7" pageNumber="202" type="materials_examined">
<paragraph blockId="7.[151,1123,616,703]" box="[151,802,616,642]" pageId="7" pageNumber="202">
<typeStatus box="[151,201,618,642]" pageId="7" pageNumber="202">Type</typeStatus>
species: 
<taxonomicName authority="Busck, 1907" authorityName="Busck" authorityYear="1907" box="[294,652,616,641]" class="Insecta" family="Tortricidae" genus="Phalonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="202" phylum="Arthropoda" rank="species" species="hubbardana">
<emphasis box="[294,517,617,641]" italics="true" pageId="7" pageNumber="202">Phalonia hubbardana</emphasis>
<bibRefCitation author="Busck, A." box="[523,652,616,641]" pageId="7" pageNumber="202" pagination="19 - 36" refId="ref19735" refString="Busck, A. (1907) A review of the tortricid subfamily Phaloniinae with descriptions of new American species. Journal of the New York Entomological Society, 15, 19 - 36." type="journal article" year="1907">Busck, 1907</bibRefCitation>
</taxonomicName>
