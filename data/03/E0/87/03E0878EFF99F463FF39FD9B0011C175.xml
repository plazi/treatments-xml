<location LSID="urn:lsid:plazi:treatment:03E0878EFF99F463FF39FD9B0011C175:8E966043FF99F463FCB7FBA8070CC05D" box="[793,891,1125,1150]" country="Peru" county="Mississippi" municipality="Wirth" name="Mexican" pageId="11" pageNumber="412" stateProvince="Mexico by">Mexican</location>
