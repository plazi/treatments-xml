<taxonomicName authority="Jiang, 1988" authorityName="Jiang" authorityYear="1988" box="[111,513,859,883]" class="Insecta" family="Chrysomelidae" genus="Haplosomoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="190" phylum="Arthropoda" rank="species" species="nigricollis">
