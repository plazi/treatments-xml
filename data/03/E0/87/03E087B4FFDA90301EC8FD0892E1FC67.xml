<taxonomicName authority="Medvedev, 2000" authorityName="Medvedev" authorityYear="2000" box="[250,724,827,851]" class="Insecta" family="Chrysomelidae" genus="Haplosomoides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="190" phylum="Arthropoda" rank="species" species="nigricollis">
