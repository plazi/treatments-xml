<taxonomicName authorityName="Jacoby" authorityYear="1884" box="[823,944,860,883]" class="Insecta" family="Chrysomelidae" genus="Hoplasoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="190" phylum="Arthropoda" rank="genus">
