<emphasis box="[1052,1179,1592,1616]" italics="true" pageId="2" pageNumber="194">Carpophilus</emphasis>
</taxonomicName>
. A) 
<taxonomicName authority="Powell &amp; Hamilton" authorityName="Powell &amp; Hamilton" authorityYear="2019" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="194" phylum="Arthropoda" rank="species" species="jarijari" status="sp. nov.">
<emphasis box="[1226,1330,1592,1616]" italics="true" pageId="2" pageNumber="194">C. jarijari</emphasis>
Powell &amp; Hamilton
</taxonomicName>
, 
<emphasis bold="true" box="[260,340,1628,1652]" pageId="2" pageNumber="194">
<taxonomicNameLabel box="[260,340,1628,1652]" pageId="2" pageNumber="194" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
, B) 
<taxonomicName authority="Murray, C" authorityName="Murray, C" authorityYear="1864" box="[385,621,1628,1652]" class="Insecta" family="Nitidulidae" genus="Carpophilus" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="194" phylum="Arthropoda" rank="species" species="truncatus">
