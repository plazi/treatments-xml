<subSubSection pageId="2" pageNumber="3" type="discussion">
<paragraph blockId="2.[189,1399,1262,1578]" box="[189,303,1262,1288]" pageId="2" pageNumber="3">
<heading bold="true" box="[189,303,1262,1288]" fontSize="11" level="3" pageId="2" pageNumber="3" reason="3">
