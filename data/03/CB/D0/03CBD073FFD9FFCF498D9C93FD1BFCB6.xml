<paragraph blockId="7.[151,1436,151,789]" box="[199,400,367,392]" pageId="7" pageNumber="542">Female unknown.</paragraph>
<paragraph blockId="7.[151,1436,151,789]" box="[199,1223,404,429]" pageId="7" pageNumber="542">
Length in mm. Body 
<number box="[439,470,404,428]" pageId="7" pageNumber="542" value="20.0">20</number>
; body with wings 
<number box="[676,707,404,428]" pageId="7" pageNumber="542" value="28.0">28</number>
; pronotum 
<number box="[834,872,404,428]" pageId="7" pageNumber="542" value="3.1">3.1</number>
; tegmina 
<number box="[981,1012,404,428]" pageId="7" pageNumber="542" value="18.0">18</number>
; hind femora 
<number box="[1167,1218,404,429]" pageId="7" pageNumber="542" value="11.5">11.5</number>
