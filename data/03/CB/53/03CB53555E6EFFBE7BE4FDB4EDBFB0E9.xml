<taxonomicName authority="Reuter, 1884" authorityName="Reuter" authorityYear="1884" box="[653,936,1018,1042]" class="Insecta" family="Hemiptera (awaiting allocation)" genus="Oncozygidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="526" phylum="Arthropoda" rank="genus">
