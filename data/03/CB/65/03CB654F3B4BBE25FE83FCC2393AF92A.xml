<emphasis box="[422,452,1519,1544]" italics="true" pageId="2" pageNumber="23">
g 
<number box="[438,452,1519,1544]" pageId="2" pageNumber="23" value="2.0">2</number>
</emphasis>
<number box="[459,545,1519,1545]" pageId="2" pageNumber="23" value="1.65" valueMax="1.8" valueMin="1.5">1.5–1.8</number>
