<taxonomicName authority="Zlobin" authorityName="Zlobin" box="[151,565,1671,1697]" class="Insecta" family="Agromyzidae" genus="Phytobia" kingdom="Animalia" order="Diptera" pageId="0" pageNumber="61" phylum="Arthropoda" rank="species" species="pseudobetulivora" status="sp.n.">
<emphasis bold="true" box="[151,473,1671,1697]" italics="true" pageId="0" pageNumber="61">Phytobia pseudobetulivora</emphasis>
Zlobin
</taxonomicName>
, 
<taxonomicNameLabel box="[577,636,1671,1697]" pageId="0" pageNumber="61" rank="species">sp.n.</taxonomicNameLabel>
</emphasis>
