<geoCoordinate box="[1028,1184,1286,1310]" direction="east" orientation="longitude" pageId="11" pageNumber="440" precision="1" value="101.72366">101°43.420’E</geoCoordinate>
(
<emphasis box="[1200,1427,1285,1310]" italics="true" pageId="11" pageNumber="440">Sites &amp; Vitheepradit</emphasis>
) (all 
<collectionCode box="[195,269,1322,1346]" country="Australia" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="11" pageNumber="440">ANIC</collectionCode>
)
