species: 
<taxonomicName authority="(Douthitt, 1915) Baer, 1923" authorityName="(Douthitt, 1915) Baer" authorityYear="1923" box="[262,801,1068,1094]" class="Cestoda" family="Anoplocephalidae" genus="Anoplocephaloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="6" pageNumber="7" phylum="Platyhelminthes" rank="species" species="infrequens">
