), shorter cirrus sac (
<taxonomicName box="[823,1052,1157,1182]" class="Neoophora" family="Anoplocephalidae" genus="Flabelloskrjabinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="12" pageNumber="13" phylum="Platyhelminthes" rank="genus">
<emphasis box="[823,1052,1157,1182]" italics="true" pageId="12" pageNumber="13">Flabelloskrjabinia</emphasis>
</taxonomicName>
), presence of a cirrus retractor muscle (
<taxonomicName box="[250,399,1196,1221]" class="Cestoda" family="Anoplocephalidae" genus="Sciurotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="12" pageNumber="13" phylum="Platyhelminthes" rank="genus">
<emphasis box="[250,399,1196,1221]" italics="true" pageId="12" pageNumber="13">Sciurotaenia</emphasis>
</taxonomicName>
) and structure of the fully developed uterus (
<taxonomicName box="[925,1073,1196,1221]" class="Cestoda" family="Anoplocephalidae" genus="Sciurotaenia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="12" pageNumber="13" phylum="Platyhelminthes" rank="genus">
<emphasis box="[925,1073,1196,1221]" italics="true" pageId="12" pageNumber="13">Sciurotaenia</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1088,1306,1196,1221]" class="Neoophora" family="Anoplocephalidae" genus="Flabelloskrjabinia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="12" pageNumber="13" phylum="Platyhelminthes" rank="genus">
<emphasis box="[1088,1306,1196,1221]" italics="true" pageId="12" pageNumber="13">Flabelloskrjabinia</emphasis>
