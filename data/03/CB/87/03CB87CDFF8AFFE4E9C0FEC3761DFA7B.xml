<collectingRegion box="[733,908,1485,1511]" country="Turkey" name="Bursa" pageId="76" pageNumber="77">Bursa Province</collectingRegion>
, 
<collectingCountry box="[919,999,1485,1511]" name="Turkey" pageId="76" pageNumber="77">Turkey</collectingCountry>
(
<figureCitation box="[1014,1112,1485,1511]" captionStart="FIGURE 448" captionStartId="89.[151,250,1143,1168]" captionTargetBox="[151,1436,174,1123]" captionTargetId="figure@89.[151,1436,174,1124]" captionTargetPageId="89" captionText="FIGURE 448. Known distributions of Hoplopholcus trakyaensis and H. bursa. TL = type localities." figureDoi="http://doi.org/10.5281/zenodo.3613260" httpUri="https://zenodo.org/record/3613260/files/figure.png" pageId="76" pageNumber="77">Fig. 448</figureCitation>
).
</paragraph>
