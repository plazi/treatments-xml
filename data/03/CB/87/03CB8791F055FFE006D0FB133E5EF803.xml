<collectingCountry box="[1033,1117,1550,1574]" name="Guinea-Bissau" pageId="4" pageNumber="58">Guinea</collectingCountry>
, 
<collectingCountry box="[1130,1197,1551,1574]" name="Japan" pageId="4" pageNumber="58">Japan</collectingCountry>
