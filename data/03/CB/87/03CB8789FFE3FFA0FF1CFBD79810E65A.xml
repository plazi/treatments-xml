<taxonomicName authority=", Rob." authorityName="Rob." class="Insecta" family="Tachinidae" genus="Gouraldia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="82" pageNumber="83" phylum="Arthropoda" rank="species" species="binotata">
