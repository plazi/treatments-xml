/ 
<geoCoordinate box="[517,599,1209,1231]" degrees="27.56" direction="east" orientation="longitude" pageId="59" pageNumber="612" precision="555" value="27.56">27.56 E</geoCoordinate>
, 
<quantity box="[611,717,1209,1231]" metricMagnitude="2" metricUnit="m" metricValue="6.5" metricValueMax="7.5" metricValueMin="5.5" pageId="59" pageNumber="612" unit="m" value="650.0" valueMax="750.0" valueMin="550.0">
<elevation box="[611,717,1209,1231]" metricMagnitude="2" metricUnit="m" metricValue="6.5" metricValueMax="7.5" metricValueMin="5.5" pageId="59" pageNumber="612" unit="m" value="650.0" valueMax="750.0" valueMin="550.0">550-750m</elevation>
</quantity>
, 79/10, leg. 
<collectorName box="[845,953,1209,1231]" pageId="59" pageNumber="612">H. Rausch</collectorName>
) fand ich im 
<collectorName box="[178,532,1237,1259]" pageId="59" pageNumber="612">Biologiezentrum Linz. J. Schmidt</collectorName>
hat sie im 
<collectorName box="[660,702,1237,1259]" pageId="59" pageNumber="612">Jahr</collectorName>
1979 als &quot;? 
<taxonomicName authority="FORST." authorityName="FORSTER" authorityYear="1853" box="[844,1045,1237,1259]" class="Insecta" family="Chrysididae" genus="Cleptes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="612" phylum="Arthropoda" rank="species" species="aerosus">
