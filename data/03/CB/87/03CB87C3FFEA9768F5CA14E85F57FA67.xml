<collectingMunicipality box="[284,423,1366,1390]" pageId="49" pageNumber="1556">Weddell Sea</collectingMunicipality>
(oV 
<collectingCountry box="[481,605,1366,1390]" name="Heard Island and McDonald Islands" pageId="49" pageNumber="1556">McDonald</collectingCountry>
