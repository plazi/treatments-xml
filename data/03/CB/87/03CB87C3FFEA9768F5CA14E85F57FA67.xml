<taxonomicName authorityName="Broch Cantero Cantero" authorityYear="1948" box="[556,736,130,152]" class="Hydrozoa" family="Symplectoscyphidae" genus="Symplectoscyphus" kingdom="Animalia" order="Leptothecata" pageId="49" pageNumber="1556" phylum="Cnidaria" rank="genus">Symplectoscyphus</taxonomicName>
soae
