<subSubSection pageId="17" pageNumber="18" type="diagnosis">
<paragraph blockId="17.[227,348,1082,1108]" box="[227,348,1082,1108]" pageId="17" pageNumber="18">
<heading bold="true" box="[227,348,1082,1108]" fontSize="11" level="3" pageId="17" pageNumber="18" reason="3">
