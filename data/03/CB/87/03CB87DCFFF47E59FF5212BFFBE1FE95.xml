, 
<collectingCountry box="[419,467,331,357]" name="Iraq" pageId="3" pageNumber="454">Iraq</collectingCountry>
, 
<collectingCountry box="[478,576,331,357]" name="Pakistan" pageId="3" pageNumber="454">Pakistan</collectingCountry>
, 
<collectingCountry box="[586,732,331,357]" name="Saudi Arabia" pageId="3" pageNumber="454">Saudi Arabia</collectingCountry>
(
<bibRefCitation author="Amsel, H. G." box="[746,887,330,357]" pageId="3" pageNumber="454" pagination="255 - 326" refId="ref15603" refString="Amsel, H. G. (1954) Die Microlepidopteren der Brandt'schen Iran-Ausbeute. 4. Teil. Arkiv for Zoologi, New Series, 6 (16), 255 - 326, pls 1 - 33." type="journal article" year="1954">Amsel 1954</bibRefCitation>
; 
<bibRefCitation author="Roesler, R. - U." box="[899,1052,331,357]" pageId="3" pageNumber="454" pagination="1 - 137" refId="ref16720" refString="Roesler, R. - U. (1973) Phycitinae. Trifine Acrobasiina. In: Amsel, H. G., Gregor, F. &amp; Reisser, H. (Eds.), Microlepidoptera Palaearctica. 4 (1 - 2). Georg Fromme &amp; Co., Wien, pp. i-xvi + 1 - 752 &amp; pp. 1 - 137, pls 1 - 170." type="book chapter" year="1973">Roesler 1973</bibRefCitation>
).
</paragraph>
