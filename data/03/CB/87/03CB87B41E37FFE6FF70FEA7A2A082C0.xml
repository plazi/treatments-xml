<taxonomicName authority="(FORSTER, 1853) (sensu ARENS 2004 a)" authorityName="sensu ARENS" authorityYear="2004" baseAuthorityName="FORSTER" baseAuthorityYear="1853" box="[178,773,302,324]" class="Insecta" family="Chrysididae" genus="Holopyga" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="24" pageNumber="577" phylum="Arthropoda" rank="species" species="generosa">
