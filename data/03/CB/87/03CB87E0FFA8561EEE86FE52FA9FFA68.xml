<bibRefCitation author="Gurlt" box="[151,295,1114,1140]" pageId="20" pageNumber="29" refString="Gurlt, E. F. (1831) Lehrbuch der pathologischen Anatomie der Haus-Saugethiere. Erste Theil. G. Reimer, Berlin, 399 pp." type="book" year="1831">
Gurlt, 
<number box="[232,295,1114,1140]" pageId="20" pageNumber="21" value="1831.0">1831</number>
</bibRefCitation>
) 
<bibRefCitation author="Blanchard" box="[313,519,1114,1140]" pageId="20" pageNumber="29" refString="Blanchard, R. (1891) Notices helminthologiques (deuxieme serie). Memoires de la Societe Zoologique de France, 4, 420 - 466." type="journal article" year="1891">
Blanchard, 
<number box="[455,519,1114,1140]" pageId="20" pageNumber="21" value="1891.0">1891</number>
</bibRefCitation>
, 
<taxonomicName box="[533,908,1115,1140]" class="Cestoda" family="Anoplocephalidae" genus="Anoplocephaloides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclophyllidea" pageId="20" pageNumber="21" phylum="Platyhelminthes" rank="species" species="mamillana">
