<collectionCode LSID="urn:lsid:biocol.org:col:1008" box="[693,771,809,835]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:1008" name="American Entomological Institute" pageId="11" pageNumber="12">AEIC</collectionCode>
