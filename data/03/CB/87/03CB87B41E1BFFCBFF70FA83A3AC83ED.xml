<taxonomicName authorityName="LATREILLE" authorityYear="1809" box="[266,374,1577,1599]" class="Insecta" family="Chrysididae" genus="Euchroeus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="605" phylum="Arthropoda" rank="genus">
