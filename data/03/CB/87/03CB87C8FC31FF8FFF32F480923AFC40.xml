<heading bold="true" box="[151,826,1002,1028]" fontSize="11" level="1" pageId="22" pageNumber="23" reason="1">
<emphasis bold="true" box="[151,826,1002,1028]" pageId="22" pageNumber="23">
<taxonomicName authority="Zhaurova" authorityName="Zhaurova" box="[151,588,1002,1028]" class="Insecta" family="Ichneumonidae" genus="Physotarsus" kingdom="Animalia" order="Hymenoptera" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="emarginatus" status="n. sp.">
<emphasis bold="true" box="[151,455,1002,1028]" italics="true" pageId="22" pageNumber="23">Physotarsus emarginatus</emphasis>
Zhaurova
</taxonomicName>
, 
<taxonomicNameLabel box="[601,667,1002,1028]" pageId="22" pageNumber="23" rank="species">n. sp.</taxonomicNameLabel>
(
<figureCitation box="[682,818,1002,1028]" captionStart="FIGURES 29 – 32" captionStartId="23.[151,270,1911,1935]" captionTargetBox="[228,1356,742,1886]" captionTargetId="figure@23.[224,1362,735,1886]" captionTargetPageId="23" captionText="FIGURES 29 – 32. Habitus, face, and male genitalia. 29 – 31, Physotarsus emarginatus n. sp.; 29, habitus; 30, face; 31, male genitalia, ventral view. 32, P. flaivpennis n. sp., habitus." httpUri="https://zenodo.org/record/189763/files/figure.png" pageId="22" pageNumber="23">
Figs 
<number box="[743,818,1002,1028]" pageId="22" pageNumber="23" value="30.0" valueMax="31.0" valueMin="29.0">29–31</number>
</figureCitation>
)
</emphasis>
</heading>
</paragraph>
