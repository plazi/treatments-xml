<number box="[634,648,1807,1831]" pageId="1" pageNumber="65" value="5.0">5</number>
<emphasis box="[648,656,1808,1831]" italics="true" pageId="1" pageNumber="65">/</emphasis>
<number box="[656,672,1807,1831]" pageId="1" pageNumber="65" value="6.0">6</number>
, hind leg: 
<number box="[791,819,1807,1831]" pageId="1" pageNumber="65" value="53.0">53</number>
<emphasis box="[820,828,1808,1831]" italics="true" pageId="1" pageNumber="65">/</emphasis>
<date box="[827,913,1807,1831]" pageId="1" pageNumber="65" value="1914-07-10">
<number box="[827,855,1807,1831]" pageId="1" pageNumber="65" value="14.0">14</number>
