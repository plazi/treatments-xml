<taxonomicName box="[259,492,152,178]" class="Insecta" family="Anthomyiidae" kingdom="Animalia" order="Diptera" pageId="38" pageNumber="39" phylum="Arthropoda" rank="family">ANTHOMYIIDAE</taxonomicName>
