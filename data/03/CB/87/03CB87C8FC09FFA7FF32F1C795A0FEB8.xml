<heading bold="true" box="[151,790,1709,1735]" fontSize="11" level="1" pageId="46" pageNumber="47" reason="1">
<emphasis bold="true" box="[151,790,1709,1735]" pageId="46" pageNumber="47">
<taxonomicName authority="Zhaurova" authorityName="Zhaurova" box="[151,552,1709,1735]" class="Insecta" family="Ichneumonidae" genus="Physotarsus" kingdom="Animalia" order="Hymenoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="truncatus" status="n. sp.">
<emphasis bold="true" box="[151,420,1709,1735]" italics="true" pageId="46" pageNumber="47">Physotarsus truncatus</emphasis>
Zhaurova
</taxonomicName>
, 
<taxonomicNameLabel box="[565,631,1709,1735]" pageId="46" pageNumber="47" rank="species">n. sp.</taxonomicNameLabel>
(
<figureCitation box="[646,783,1709,1735]" captionStart="FIGURES 57 – 60" captionStartId="46.[151,272,1566,1590]" captionTargetBox="[221,1350,462,1523]" captionTargetId="figure@46.[219,1367,429,1543]" captionTargetPageId="46" captionText="FIGURES 57 – 60. Physotarsus truncatus n. sp. 57, habitus; 58, face; 59, male genitalia, ventral view; 60, head and mesonotum, Ψ, dorsal view." httpUri="https://zenodo.org/record/189770/files/figure.png" pageId="46" pageNumber="47">
Figs 
<number box="[707,783,1709,1735]" pageId="46" pageNumber="47" value="58.5" valueMax="60.0" valueMin="57.0">57–60</number>
</figureCitation>
)
</emphasis>
</heading>
</paragraph>
