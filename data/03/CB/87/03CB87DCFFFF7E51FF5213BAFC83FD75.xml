), 
<collectingCountry box="[629,674,619,645]" name="Iran" pageId="11" pageNumber="462">Iran</collectingCountry>
(
<emphasis bold="true" box="[690,826,618,645]" pageId="11" pageNumber="462">first record</emphasis>
).
</paragraph>
