<subSubSection pageId="16" pageNumber="17" type="etymology">
<paragraph blockId="16.[151,1437,152,1036]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[197,340,973,999]" pageId="16" pageNumber="17">Etymology.</emphasis>
