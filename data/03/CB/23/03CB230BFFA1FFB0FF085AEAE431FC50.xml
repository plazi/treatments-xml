<subSubSection box="[199,1229,833,858]" pageId="20" pageNumber="21" type="etymology">
<paragraph blockId="20.[151,1436,509,929]" box="[199,1229,833,858]" pageId="20" pageNumber="21">
<emphasis bold="true" box="[199,334,834,858]" pageId="20" pageNumber="21">Etymology.</emphasis>
