<paragraph blockId="6.[151,1436,151,933]" box="[199,1059,548,573]" pageId="6" pageNumber="392">
<materialsCitation ID-GBIF-Occurrence="1836204495" box="[199,1059,548,573]" location="Geographical Distribution. French Guiana" pageId="6" pageNumber="392" specimenCount="1" stateProvince="Geographical Distribution. French Guiana">
<collectingRegion box="[199,694,548,573]" inferredBySuffix="true" pageId="6" pageNumber="392">
<emphasis bold="true" box="[199,521,548,573]" pageId="6" pageNumber="392">
