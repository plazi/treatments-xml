0 
<geoCoordinate box="[249,266,702,730]" direction="south" orientation="latitude" pageId="13" pageNumber="268" precision="15" value="-1.4955555">
<number box="[249,260,706,730]" pageId="13" pageNumber="268" value="1.0">1</number>
o
