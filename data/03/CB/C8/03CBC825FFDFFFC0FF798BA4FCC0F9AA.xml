<emphasis bold="true" box="[508,521,788,807]" pageId="25" pageNumber="178">d</emphasis>
) ninth sternite of Ƌ; 
<emphasis bold="true" box="[737,747,788,807]" pageId="25" pageNumber="178">e</emphasis>
) first abdominal sternite; 
<emphasis bold="true" box="[1003,1011,788,807]" pageId="25" pageNumber="178">f</emphasis>
) sixth abdominal sternite of Ƌ; 
<emphasis bold="true" box="[479,491,812,831]" pageId="25" pageNumber="178">g</emphasis>
) sixth abdominal sternite of ♀; 
<emphasis bold="true" box="[795,808,812,831]" pageId="25" pageNumber="178">h</emphasis>
