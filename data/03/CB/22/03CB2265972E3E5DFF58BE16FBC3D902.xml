<quantity box="[1244,1406,945,970]" metricMagnitude="3" metricUnit="m" metricValue="1.275" metricValueMax="1.35" metricValueMin="1.2" pageId="2" pageNumber="567" unit="m" value="1275.0" valueMax="1350.0" valueMin="1200.0">1200–1350 m</quantity>
