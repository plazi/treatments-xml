<emphasis bold="true" box="[151,353,1726,1751]" pageId="6" pageNumber="571">Material studied</emphasis>
. 
<typeStatus box="[371,466,1728,1752]" pageId="6" pageNumber="571">Neotype</typeStatus>
(here designated)—male; 
<collectingCountry box="[776,833,1726,1751]" name="Laos" pageId="6" pageNumber="571">
<emphasis bold="true" box="[776,833,1726,1751]" pageId="6" pageNumber="571">Laos</emphasis>
