<geoCoordinate direction="north" orientation="latitude" precision="1" value="39.250168">39°15'0.6&quot;N</geoCoordinate>
, 
<geoCoordinate direction="east" orientation="longitude" precision="1" value="23.709002">23°42'32.4&quot;E</geoCoordinate>
