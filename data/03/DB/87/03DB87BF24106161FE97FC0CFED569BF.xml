<paragraph blockId="3.[264,1324,998,1905]" pageId="3" pageNumber="4">
<emphasis bold="true" box="[264,465,998,1024]" pageId="3" pageNumber="4">DIAGNOSIS —</emphasis>
Empodia 
<number box="[586,600,999,1025]" pageId="3" pageNumber="4" value="7.0">7</number>
