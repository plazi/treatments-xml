<quantity box="[422,565,180,202]" metricMagnitude="-3" metricUnit="m" metricValue="4.95" metricValueMax="5.2" metricValueMin="4.7" pageId="13" pageNumber="462" unit="mm" value="4.95" valueMax="5.2" valueMin="4.7">4.70–5.20 mm</quantity>
