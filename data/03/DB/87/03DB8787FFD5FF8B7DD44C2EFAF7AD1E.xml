....................... 
<taxonomicName authority="Klimov &amp; Oconnor &amp; Khaustov, 2019" authorityName="Klimov &amp; Oconnor &amp; Khaustov" authorityYear="2019" box="[1219,1373,476,498]" class="Arachnida" family="Acaridae" genus="Naiadacarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Astigmata" pageId="11" pageNumber="452" phylum="Arthropoda" rank="species" species="sidorchukae" status="sp. n.">
