<collectingRegion box="[469,563,1245,1267]" country="United States of America" name="Michigan" pageId="11" pageNumber="452">Michigan</collectingRegion>
, 
<collectingRegion box="[573,661,1245,1267]" country="United States of America" name="Missouri" pageId="11" pageNumber="452">Missouri</collectingRegion>
, 
<collectingRegion box="[670,774,1245,1267]" country="United States of America" name="Minnesota" pageId="11" pageNumber="452">Minnesota</collectingRegion>
, 
<collectingRegion box="[784,884,1245,1267]" country="United States of America" name="New York" pageId="11" pageNumber="452">New York</collectingRegion>
, 
<collectingRegion box="[893,1023,1245,1267]" country="United States of America" name="Pennsylvania" pageId="11" pageNumber="452">Pennsylvania</collectingRegion>
, 
<collectingRegion box="[1033,1116,1245,1267]" country="United States of America" name="Virginia" pageId="11" pageNumber="452">Virginia</collectingRegion>
). In water-filled tree holes................................................................................. 
