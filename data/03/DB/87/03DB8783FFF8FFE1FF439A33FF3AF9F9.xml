<taxonomicName authorityName="Assing &amp; Feldmann" authorityYear="2014" box="[340,571,1480,1504]" class="Insecta" family="Staphylinidae" genus="Domene" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="459" phylum="Arthropoda" rank="species" species="firmicornis">
