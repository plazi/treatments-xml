and 
<taxonomicName box="[196,344,1011,1035]" class="Insecta" family="Miridae" genus="Hoplomachus" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="562" phylum="Arthropoda" rank="genus">
