and 
<taxonomicName box="[1028,1135,729,752]" class="Insecta" family="Anthomyiidae" genus="Drymeia" kingdom="Animalia" order="Diptera" pageId="13" pageNumber="164" phylum="Arthropoda" rank="species" species="triseta" status="sp. nov.">
<emphasis box="[1028,1135,729,752]" italics="true" pageId="13" pageNumber="164">D. triseta</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1141,1230,727,752]" pageId="13" pageNumber="164" rank="species">
