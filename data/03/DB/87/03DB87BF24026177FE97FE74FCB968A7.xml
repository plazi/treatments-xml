<number box="[477,491,1039,1065]" pageId="17" pageNumber="18" value="5.0">5</number>
(
<number box="[509,557,1039,1065]" pageId="17" pageNumber="18" value="5.0" valueMax="6.0" valueMin="4.0">4–6</number>
), solenidion 
<number box="[710,725,1039,1065]" pageId="17" pageNumber="18" value="5.0">5</number>
(
<number box="[741,789,1039,1065]" pageId="17" pageNumber="18" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), strongly knobbed, empodium divided, each branch 
<number box="[352,365,1079,1105]" pageId="17" pageNumber="18" value="5.0">5</number>
­rayed. Leg 
<date box="[511,576,1079,1105]" pageId="17" pageNumber="18" value="1924-02">
II 
<number box="[547,576,1079,1105]" pageId="17" pageNumber="18" value="24.0">24</number>
</date>
(
<number box="[593,673,1079,1105]" pageId="17" pageNumber="18" value="25.5" valueMax="27.0" valueMin="24.0">24–27</number>
); femur 
<number box="[772,787,1079,1105]" pageId="17" pageNumber="18" value="9.0">9</number>
(
<number box="[804,866,1079,1105]" pageId="17" pageNumber="18" value="10.0" valueMax="11.0" valueMin="9.0">9–11</number>
), 
<emphasis box="[887,915,1079,1104]" italics="true" pageId="17" pageNumber="18">bv</emphasis>
missing; genu 
<number box="[1095,1110,1079,1105]" pageId="17" pageNumber="18" value="4.0">4</number>
(
<number box="[1127,1175,1079,1105]" pageId="17" pageNumber="18" value="3.5" valueMax="4.0" valueMin="3.0">3–4</number>
), 
<emphasis box="[1197,1221,1079,1104]" italics="true" pageId="17" pageNumber="18">l”</emphasis>
missing; tibia 
<number box="[325,342,1119,1145]" pageId="17" pageNumber="18" value="4.0">4</number>
(
<number box="[351,368,1119,1145]" pageId="17" pageNumber="18" value="4.0">4</number>
); tarsus 
<number box="[469,483,1119,1145]" pageId="17" pageNumber="18" value="7.0">7</number>
(
<number box="[503,550,1119,1145]" pageId="17" pageNumber="18" value="7.5" valueMax="8.0" valueMin="7.0">7–8</number>
