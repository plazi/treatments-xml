<number box="[1143,1158,1119,1145]" pageId="17" pageNumber="18" value="7.0">7</number>
(
<number box="[1176,1191,1119,1145]" pageId="17" pageNumber="18" value="7.0">7</number>
– 
<number box="[1209,1224,1119,1145]" pageId="17" pageNumber="18" value="6.0">6</number>
), empodium 
<number box="[333,347,1159,1185]" pageId="17" pageNumber="18" value="8.0">8</number>
(
<number box="[365,412,1159,1185]" pageId="17" pageNumber="18" value="8.5" valueMax="9.0" valueMin="8.0">8–9</number>
