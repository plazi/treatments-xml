<paragraph blockId="2.[151,599,1053,1079]" box="[151,599,1053,1079]" pageId="2" pageNumber="485">
<heading bold="true" box="[151,599,1053,1079]" fontSize="11" level="1" pageId="2" pageNumber="485" reason="1">
<taxonomicName authority="Joost, 1979" authorityName="Joost" authorityYear="1979" box="[151,599,1053,1079]" class="Insecta" family="Thaumaleidae" genus="Thaumalea" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="485" phylum="Arthropoda" rank="species" species="martinovskyi">
<emphasis bold="true" box="[151,599,1053,1079]" pageId="2" pageNumber="485">
<emphasis bold="true" box="[151,453,1053,1079]" italics="true" pageId="2" pageNumber="485">Thaumalea martinovskyi</emphasis>
<bibRefCitation author="Joost" box="[460,599,1053,1079]" pageId="2" pageNumber="485" refString="Joost, W. (1979) Thaumalea martinovskyi n. sp. aus dem Kaukasus. Entomologische Nachrichten, 6, 89 - 92." type="journal article" year="1979">
Joost, 
<number box="[540,599,1053,1079]" pageId="2" pageNumber="485" value="1979.0">1979</number>
</bibRefCitation>
</emphasis>
</taxonomicName>
</heading>
</paragraph>
