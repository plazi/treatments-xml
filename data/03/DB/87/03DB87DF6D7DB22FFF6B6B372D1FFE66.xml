Meigen, 
<number box="[340,394,866,888]" pageId="2" pageNumber="153" value="1826.0">1826</number>
</bibRefCitation>
: 
<number box="[406,446,866,888]" pageId="2" pageNumber="153" value="204.0">204</number>
</treatmentCitation>
</taxonomicName>
. Type-species: 
<taxonomicName authority="Meigen, 1826" authorityName="Meigen" authorityYear="1826" box="[602,864,866,888]" class="Insecta" family="Anthomyiidae" genus="Drymeia" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="153" phylum="Arthropoda" rank="species" species="obscura">
<emphasis box="[602,715,866,888]" italics="true" pageId="2" pageNumber="153">D. obscura</emphasis>
<bibRefCitation author="Meigen" box="[723,864,866,888]" pageId="2" pageNumber="153" refString="Meigen, J. W. (1826) Systematische Beschreibung der bekannten europaischen zweiflugeligen Insekten. Funfter Theil. Schultz, Hamm, xii + 412 pp." type="book" year="1826">
Meigen, 
<number box="[814,864,866,888]" pageId="2" pageNumber="153" value="1826.0">1826</number>
</bibRefCitation>
</taxonomicName>
[= 
<taxonomicName authority="Fallen, 1823" authorityName="Fallen" authorityYear="1823" box="[902,1190,866,888]" class="Insecta" family="Anthomyiidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="2" pageNumber="153" phylum="Arthropoda" rank="species" species="hamata">
