<location LSID="urn:lsid:plazi:treatment:03DB87EEFF829D23FF405B56FA90F1E9:8EAD6023FF859D23FF175C01FF5FF19D" box="[192,236,619,645]" county="Sangihe" municipality="Talaud Islands" name="The" pageId="116" pageNumber="117">The</location>
