<emphasis bold="true" box="[859,975,603,629]" pageId="13" pageNumber="14">Genitalia</emphasis>
<number box="[983,1012,604,630]" pageId="13" pageNumber="14" value="24.0">24</number>
(
<number box="[1029,1105,604,630]" pageId="13" pageNumber="14" value="23.5" valueMax="24.0" valueMin="23.0">23–24</number>
