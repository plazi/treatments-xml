<number box="[748,762,404,430]" pageId="13" pageNumber="14" value="7.0">7</number>
(
<number box="[782,829,404,430]" pageId="13" pageNumber="14" value="6.0" valueMax="7.0" valueMin="5.0">5–7</number>
