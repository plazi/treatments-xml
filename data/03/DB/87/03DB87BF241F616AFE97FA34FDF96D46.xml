<emphasis box="[993,1022,564,589]" italics="true" pageId="13" pageNumber="14">
<number box="[993,1007,564,589]" pageId="13" pageNumber="14" value="2.0">2</number>
a
</emphasis>
) 
<number box="[1042,1071,564,590]" pageId="13" pageNumber="14" value="20.0">20</number>
(
<number box="[1090,1168,564,590]" pageId="13" pageNumber="14" value="29.0" valueMax="38.0" valueMin="20.0">20–38</number>
