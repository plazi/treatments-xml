<number box="[1096,1143,404,430]" pageId="13" pageNumber="14" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
), solenidion in usual dorsal position, 
<number box="[528,557,444,470]" pageId="13" pageNumber="14" value="10.0">10</number>
(
<number box="[576,639,444,470]" pageId="13" pageNumber="14" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
), empodium 
<number box="[798,813,444,470]" pageId="13" pageNumber="14" value="5.0">5</number>
(
<number box="[831,878,444,470]" pageId="13" pageNumber="14" value="5.5" valueMax="6.0" valueMin="5.0">5–6</number>
