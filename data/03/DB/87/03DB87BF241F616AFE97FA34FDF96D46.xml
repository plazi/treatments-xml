<number box="[1191,1220,564,590]" pageId="13" pageNumber="14" value="23.0">23</number>
(
<number box="[1238,1315,564,590]" pageId="13" pageNumber="14" value="24.0" valueMax="26.0" valueMin="22.0">22–26</number>
