<collectingRegion box="[334,389,1036,1059]" country="Sri Lanka" name="Eastern" pageId="45" pageNumber="44">Este</collectingRegion>
autor cita a 
<taxonomicName box="[539,820,1035,1059]" class="Liliopsida" family="Amaryllidaceae" genus="Pancratium" kingdom="Plantae" order="Asparagales" pageId="45" pageNumber="44" phylum="Tracheophyta" rank="species" species="caribaeum">
