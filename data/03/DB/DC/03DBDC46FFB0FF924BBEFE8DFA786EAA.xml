<taxonomicName box="[271,391,1314,1335]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="509" phylum="Arthropoda" rank="species" species="biophilica">
<emphasis box="[271,391,1314,1335]" italics="true" pageId="8" pageNumber="509">F. biophilica</emphasis>
</taxonomicName>
.................................................................................. 
<taxonomicName box="[1382,1435,1314,1335]" class="Insecta" family="Formicidae" genus="Polyergus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="509" phylum="Arthropoda" rank="species" species="ruber">
