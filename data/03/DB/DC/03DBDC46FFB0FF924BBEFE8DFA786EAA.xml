<taxonomicName box="[1034,1142,580,601]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="509" phylum="Arthropoda" rank="species" species="japonica">F. japonica</taxonomicName>
, 
<taxonomicName box="[1151,1290,580,601]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="509" phylum="Arthropoda" rank="species" species="glabridorsis">F. glabridorsis</taxonomicName>
