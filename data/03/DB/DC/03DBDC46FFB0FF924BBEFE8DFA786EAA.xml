<number box="[856,883,1698,1719]" pageId="8" pageNumber="509" value="85.0">85</number>
; legs shorter, HFI &lt;
<number box="[1078,1116,1698,1719]" pageId="8" pageNumber="509" value="120.0">120</number>
, usually &lt;
<number box="[1220,1258,1698,1719]" pageId="8" pageNumber="509" value="110.0">110</number>
; hosts various, but apparently never 
<emphasis box="[391,459,1727,1748]" italics="true" pageId="8" pageNumber="509">
<taxonomicName box="[391,452,1727,1748]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="509" phylum="Arthropoda" rank="species" species="mok">F. mok</taxonomicName>
i
