<number box="[1190,1217,1991,2012]" pageId="8" pageNumber="509" value="79.0">79</number>
; legs longer, HFI 
<number box="[1388,1436,1991,2012]" pageId="8" pageNumber="509" value="114.5" valueMax="121.0" valueMin="108.0">108–</number>
</keyLead>
</paragraph>
