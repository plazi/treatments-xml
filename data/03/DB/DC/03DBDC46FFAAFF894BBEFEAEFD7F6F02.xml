(
<number box="[1033,1078,1137,1162]" pageId="18" pageNumber="519" value="145.0">145</number>
), LI 
<number box="[1132,1244,1137,1162]" pageId="18" pageNumber="519" value="3.9400000000000004" valueMax="4.2" valueMin="3.68">3.68–4.20</number>
(
<number box="[1259,1312,1138,1162]" pageId="18" pageNumber="519" value="3.97">3.97</number>
