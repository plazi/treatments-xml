<paragraph blockId="45.[624,957,332,356]" box="[624,957,332,356]" pageId="45" pageNumber="1959">
<heading box="[624,957,332,356]" centered="true" fontSize="10" level="2" pageId="45" pageNumber="1959" reason="3">
<taxonomicName authority="Freund &amp; Wagner, 2003" authorityName="Freund &amp; Wagner" authorityYear="2003" box="[624,858,332,356]" class="Insecta" family="Chrysomelidae" genus="Bonesioides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="1959" phylum="Arthropoda" rank="species" species="montana" status="sp. nov.">
<emphasis bold="true" box="[624,858,332,356]" italics="true" pageId="45" pageNumber="1959">Bonesioides montana</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[866,957,333,356]" pageId="45" pageNumber="1959" rank="species">sp. nov.</taxonomicNameLabel>
</heading>
</paragraph>
