<paragraph blockId="51.[610,972,906,930]" box="[610,972,906,930]" pageId="51" pageNumber="1965">
<heading box="[610,972,906,930]" centered="true" fontSize="10" level="2" pageId="51" pageNumber="1965" reason="3">
<taxonomicName authority="Freund &amp; Wagner, 2003" authorityName="Freund &amp; Wagner" authorityYear="2003" box="[610,871,906,930]" class="Insecta" family="Chrysomelidae" genus="Bonesioides" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="51" pageNumber="1965" phylum="Arthropoda" rank="species" species="trispiculata" status="sp. nov.">
<emphasis bold="true" box="[610,871,906,930]" italics="true" pageId="51" pageNumber="1965">Bonesioides trispiculata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[881,972,907,930]" pageId="51" pageNumber="1965" rank="species">sp. nov.</taxonomicNameLabel>
</heading>
