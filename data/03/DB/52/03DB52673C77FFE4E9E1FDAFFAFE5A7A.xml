<subSubSection pageId="7" pageNumber="45" type="discussion">
<paragraph blockId="7.[151,1437,741,1140]" pageId="7" pageNumber="45">
<emphasis bold="true" box="[151,276,741,767]" pageId="7" pageNumber="45">Remarks:</emphasis>
<taxonomicName box="[285,512,741,766]" class="Insecta" family="Fanniidae" genus="Fannia" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="45" phylum="Arthropoda" rank="species" species="itatiaiensis">
<emphasis box="[285,512,741,766]" italics="true" pageId="7" pageNumber="45">Fannia itatiaiensis</emphasis>
</taxonomicName>
