<subSubSection box="[199,1277,1992,2017]" pageId="4" pageNumber="28" type="etymology">
<paragraph blockId="4.[151,1437,1197,2017]" box="[199,1277,1992,2017]" pageId="4" pageNumber="28">
<emphasis bold="true" box="[199,334,1992,2016]" pageId="4" pageNumber="28">Etymology.</emphasis>
<taxonomicName box="[340,444,1993,2016]" class="Insecta" family="Psychodidae" genus="Trichomyia" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="28" phylum="Arthropoda" rank="species" species="sertaneja">
