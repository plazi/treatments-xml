</emphasis>
(in 
<collectionCode box="[611,699,266,292]" pageId="14" pageNumber="45">NRMS</collectionCode>
). N.E. 
<collectingCountry box="[788,868,266,292]" name="Myanmar" pageId="14" pageNumber="45">Burma</collectingCountry>
(
<collectingCountry box="[887,1007,266,292]" name="Myanmar" pageId="14" pageNumber="45">Myanmar</collectingCountry>
), Kambaiti, 
<quantity box="[1159,1253,266,292]" metricMagnitude="3" metricUnit="m" metricValue="2.1336" pageId="14" pageNumber="45" unit="ft" value="7000.0">7000 ft</quantity>
., 
<date box="[1273,1393,266,292]" pageId="14" pageNumber="45" value="1934-04-23">
<number box="[1273,1328,266,292]" pageId="14" pageNumber="45" value="23.4">23.4</number>
. 
