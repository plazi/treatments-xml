<quantity box="[1131,1266,1660,1686]" metricMagnitude="-2" metricUnit="m" metricValue="9.5" metricValueMax="12.0" metricValueMin="7.0" pageId="67" pageNumber="68" unit="mm" value="95.0" valueMax="120.0" valueMin="70.0">70–120 mm</quantity>
