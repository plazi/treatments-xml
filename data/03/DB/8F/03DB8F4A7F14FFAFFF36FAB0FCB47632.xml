<taxonomicName authority="" authorityName="Schmidt" authorityYear="1862" box="[628,853,152,179]" class="Demospongiae" family="Chalinidae" genus="Haliclona" higherTaxonomySource="GBIF" kingdom="Animalia" order="Haplosclerida" pageId="8" pageNumber="133" phylum="Porifera" rank="subGenus" subGenus="Reniera">
