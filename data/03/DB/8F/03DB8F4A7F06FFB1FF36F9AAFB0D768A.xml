<quantity box="[1055,1149,1947,1974]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" metricValueMax="8.0" metricValueMin="6.0" pageId="20" pageNumber="145" unit="mm" value="7.0" valueMax="8.0" valueMin="6.0">6‒8 mm</quantity>
