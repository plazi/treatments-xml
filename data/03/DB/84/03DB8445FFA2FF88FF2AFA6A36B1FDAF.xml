<subSubSection box="[151,979,1527,1552]" pageId="56" pageNumber="57" type="etymology">
<paragraph blockId="56.[151,1436,1527,2019]" box="[151,979,1527,1552]" pageId="56" pageNumber="57">
<emphasis bold="true" box="[151,289,1527,1551]" pageId="56" pageNumber="57">Etymology:</emphasis>
