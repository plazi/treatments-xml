<collectingCountry box="[812,892,1568,1592]" name="Peru" pageId="35" pageNumber="36">PERW</collectingCountry>
= 
<number box="[936,996,1568,1592]" pageId="35" pageNumber="36" value="2.69">2.69</number>
: 
<number box="[999,1009,1568,1592]" pageId="35" pageNumber="36" value="1.0">1</number>
