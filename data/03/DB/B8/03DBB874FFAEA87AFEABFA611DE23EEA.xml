<quantity box="[238,374,916,940]" metricMagnitude="-3" metricUnit="m" metricValue="8.85" metricValueMax="9.6" metricValueMin="8.1" pageId="33" pageNumber="1106" unit="mm" value="8.85" valueMax="9.6" valueMin="8.1">8.1–9.6 mm</quantity>
, length of elytra: 6.0– 
