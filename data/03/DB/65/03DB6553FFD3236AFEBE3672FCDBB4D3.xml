<quantity box="[919,1131,564,590]" metricMagnitude="5" metricUnit="m" metricValue="2.629045" metricValueMax="5.25441" metricValueMin="0.00368" pageId="3" pageNumber="42" unit="m" value="262904.5" valueMax="525441.0" valueMin="368.0">368–525, 441 m</quantity>
