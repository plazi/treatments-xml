<taxonomicName authorityName="Viets" authorityYear="1936" box="[291,449,475,501]" class="Arachnida" family="Halacaridae" genus="Copidognathus" kingdom="Animalia" order="Trombidiformes" pageId="8" pageNumber="309" phylum="Arthropoda" rank="species" species="bonairensis">
