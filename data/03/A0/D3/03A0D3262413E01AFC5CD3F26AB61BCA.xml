<taxonomicName authorityName="Okamoto and Ida" authorityYear="2002" box="[603,767,557,580]" class="Actinopterygii" family="Acropomatidae" genus="Acropoma" kingdom="Animalia" order="Perciformes" pageId="4" pageNumber="13" phylum="Chordata" rank="species" species="argentistigma">
