<taxonomicName authorityName="Assing" authorityYear="2012" box="[305,549,1175,1196]" class="Insecta" family="Staphylinidae" genus="Pseudolathra" kingdom="Animalia" order="Coleoptera" pageId="26" pageNumber="325" phylum="Arthropoda" rank="species" species="transversicollis">
