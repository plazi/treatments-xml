<paragraph blockId="1.[151,1437,820,2035]" pageId="1" pageNumber="396">
<emphasis bold="true" box="[151,297,820,847]" pageId="1" pageNumber="396">Description.</emphasis>
<emphasis box="[304,474,822,847]" italics="true" pageId="1" pageNumber="396">Measurements.</emphasis>
