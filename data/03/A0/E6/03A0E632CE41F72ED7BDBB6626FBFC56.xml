<emphasis box="[1141,1364,403,427]" italics="true" pageId="5" pageNumber="498">
<taxonomicName box="[1141,1360,403,427]" class="Branchiopoda" family="Ilyocryptidae" genus="Ilyocryptus" kingdom="Animalia" order="Diplostraca" pageId="5" pageNumber="498" phylum="Arthropoda" rank="species" species="spinifer">Ilyocryptus spinifer</taxonomicName>
