<taxonomicName authority="Korovchinsky, 1979" authorityName="Korovchinsky" authorityYear="1979" box="[896,1324,439,464]" class="Branchiopoda" family="Malvaceae" genus="Sida" kingdom="Animalia" order="Diplostraca" pageId="2" pageNumber="495" phylum="Arthropoda" rank="species" species="americana">
<emphasis box="[896,1078,440,464]" italics="true" pageId="2" pageNumber="495">Sida americana</emphasis>
Korovchinsky, 
<number box="[1268,1324,440,464]" pageId="2" pageNumber="495" value="1979.0">1979</number>
</taxonomicName>
