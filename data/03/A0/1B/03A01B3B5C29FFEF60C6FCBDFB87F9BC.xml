<date box="[1128,1339,1300,1324]" pageId="51" pageNumber="436" value="1801-06-14" valueMax="1801-06-28" valueMin="1801-06-14">
<collectingDate box="[1128,1339,1300,1324]" pageId="51" pageNumber="436" value="1801-06-14" valueMax="1801-06-28" valueMin="1801-06-14">14 and 28 June 1801</collectingDate>
</date>
. 
