<number box="[1074,1107,226,252]" pageId="27" pageNumber="28" value="18.0">18</number>
; 
<number box="[1121,1154,226,252]" pageId="27" pageNumber="28" value="38.0">38</number>
; 
<number box="[1167,1200,226,252]" pageId="27" pageNumber="28" value="60.0">60</number>
; 
<number box="[1214,1261,226,252]" pageId="27" pageNumber="28" value="125.0">125</number>
