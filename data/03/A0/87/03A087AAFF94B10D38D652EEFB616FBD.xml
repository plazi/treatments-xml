and not 
<taxonomicName box="[661,823,1053,1080]" class="Insecta" family="Aphididae" genus="Capitophorus" kingdom="Animalia" order="Aphidomorpha" pageId="8" pageNumber="583" phylum="Arthropoda" rank="species" species="hippophaes">
