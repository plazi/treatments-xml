<subScript attach="left" box="[1092,1113,1729,1741]" fontSize="5" pageId="4" pageNumber="585">4+5</subScript>
and M 
<subScript attach="right" box="[1183,1190,1730,1742]" fontSize="5" pageId="4" pageNumber="585">1</subScript>
