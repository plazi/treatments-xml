male, 
<date box="[222,352,1526,1552]" pageId="25" pageNumber="26" value="1998-03-23">
<number box="[222,253,1526,1552]" pageId="25" pageNumber="26" value="23.0">23</number>
.iii. 
<number box="[289,352,1526,1552]" pageId="25" pageNumber="26" value="1998.0">1998</number>
</date>
