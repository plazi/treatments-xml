of profemur: 
<number box="[1390,1432,1773,1799]" pageId="17" pageNumber="18" value="2.8">2.8</number>
. Palpomeres 
<taxonomicName box="[296,370,1810,1836]" class="Insecta" family="Chironomidae" genus="Corynoneurella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="species" species="length">length</taxonomicName>
(in µm): -; 
<number box="[511,543,1810,1836]" pageId="17" pageNumber="18" value="18.0">18</number>
; 
<number box="[556,589,1810,1836]" pageId="17" pageNumber="18" value="33.0">33</number>
; 
<number box="[602,635,1810,1836]" pageId="17" pageNumber="18" value="40.0">40</number>
; 
<number box="[648,681,1810,1836]" pageId="17" pageNumber="18" value="88.0">88</number>
. Palpomere 
<number box="[827,842,1810,1836]" pageId="17" pageNumber="18" value="2.0">2</number>
and 
<number box="[900,915,1810,1836]" pageId="17" pageNumber="18" value="3.0">3</number>
ellipsoid, 
<number box="[1039,1054,1810,1836]" pageId="17" pageNumber="18" value="4.0">4</number>
rectangular, 
<number box="[1209,1224,1810,1836]" pageId="17" pageNumber="18" value="5.0">5</number>
long and slender; palpomere 
<number box="[287,301,1848,1874]" pageId="17" pageNumber="18" value="5.0">5</number>
/ 
<number box="[320,339,1848,1874]" pageId="17" pageNumber="18" value="3.0">3</number>
: 
<number box="[352,394,1848,1874]" pageId="17" pageNumber="18" value="2.7">2.7</number>
. Gonocoxite 
<number box="[557,587,1848,1874]" pageId="17" pageNumber="18" value="75.0">75</number>
