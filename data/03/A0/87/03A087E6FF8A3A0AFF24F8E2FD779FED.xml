<number box="[539,583,1928,1954]" pageId="11" pageNumber="12" value="105.0">105</number>
µm long. Stipes 
<number box="[784,813,1928,1954]" pageId="11" pageNumber="12" value="75.0">75</number>
µm long. Palpomeres 
<taxonomicName box="[1080,1153,1928,1954]" class="Insecta" family="Chironomidae" genus="Corynoneurella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="length">length</taxonomicName>
