<subSubSection lastPageId="5" lastPageNumber="412" pageId="4" pageNumber="411" type="reference_group">
<paragraph blockId="4.[1408,2612,3394,3466]" box="[1409,2363,3396,3421]" pageId="4" pageNumber="411">
<emphasis bold="true" box="[1409,1564,3396,3421]" pageId="4" pageNumber="411">Taxonomy.</emphasis>
<taxonomicName authority="Osgood, 1932" authorityName="Osgood" authorityYear="1932" box="[1576,2097,3396,3421]" class="Mammalia" family="Cervidae" genus="Muntiacus" kingdom="Animalia" order="Artiodactyla" pageId="4" pageNumber="411" phylum="Chordata" rank="species" species="rooseveltorum">Muntiacus rooseveltorum Osgood, 1932</taxonomicName>
