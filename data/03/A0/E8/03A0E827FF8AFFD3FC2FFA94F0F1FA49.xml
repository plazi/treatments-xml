<subSubSection pageId="1" pageNumber="218" type="etymology">
<paragraph blockId="1.[831,1437,675,764]" pageId="1" pageNumber="218">
<emphasis bold="true" box="[831,965,676,700]" pageId="1" pageNumber="218">Etymology:</emphasis>
