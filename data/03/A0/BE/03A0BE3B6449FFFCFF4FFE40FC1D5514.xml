<paragraph blockId="67.[151,482,470,496]" box="[151,482,470,496]" pageId="67" pageNumber="68">
<heading bold="true" box="[151,482,470,496]" fontSize="11" level="1" pageId="67" pageNumber="68" reason="1">
<emphasis bold="true" box="[151,482,470,496]" pageId="67" pageNumber="68">
Genus 
<taxonomicName authority="Brisson, 1762" authorityName="Brisson" authorityYear="1762" box="[238,482,470,496]" class="Mammalia" family="Mustelidae" genus="Lutra" kingdom="Animalia" order="Carnivora" pageId="67" pageNumber="68" phylum="Chordata" rank="genus">
