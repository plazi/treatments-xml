<paragraph blockId="84.[151,594,548,574]" box="[151,594,548,574]" pageId="84" pageNumber="85">
<heading bold="true" box="[151,594,548,574]" fontSize="11" level="1" pageId="84" pageNumber="85" reason="1">
<emphasis bold="true" box="[151,594,548,574]" pageId="84" pageNumber="85">
Genus 
<taxonomicName authority="Severtzov, 1858" authorityName="Severtzov" authorityYear="1858" box="[238,594,548,574]" class="Mammalia" family="Felidae" genus="Prionailurus" kingdom="Animalia" order="Carnivora" pageId="84" pageNumber="85" phylum="Chordata" rank="genus">
