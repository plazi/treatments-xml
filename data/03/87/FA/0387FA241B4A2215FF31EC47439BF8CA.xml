<taxonomicName authority="Bouvier, 1922" authorityName="Bouvier" authorityYear="1922" box="[293,737,1329,1351]" class="Malacostraca" family="Paguridae" genus="Michelopagurus" kingdom="Animalia" order="Decapoda" pageId="38" pageNumber="489" phylum="Arthropoda" rank="species" species="atlanticus">
