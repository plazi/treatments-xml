</taxonomicName>
(
<collectingCountry box="[227,305,734,758]" name="Kenya" pageId="1" pageNumber="388">Kenya</collectingCountry>
), 
<taxonomicName authority="Voss, 1960" authorityName="Voss. In" authorityYear="1960" box="[330,580,733,758]" class="Insecta" family="Curculionidae" genus="Haptomerus" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="388" phylum="Arthropoda" rank="species" species="affinis">
<emphasis box="[330,440,733,757]" italics="true" pageId="1" pageNumber="388">H. affinis</emphasis>
