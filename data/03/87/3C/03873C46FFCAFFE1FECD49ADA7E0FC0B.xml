) and 
<taxonomicName authority="Van de Vijver in Taylor et al. (2014)" authorityName="Van de Vijver in Taylor" authorityYear="2014" box="[464,1270,813,840]" class="Bacillariophyceae" family="Achnanthidiaceae" genus="Achnanthidium" higherTaxonomySource="GBIF" kingdom="Chromista" order="Achnanthales" pageId="8" pageNumber="9" phylum="Bacillariophyta" rank="species" species="australexiguum">
