<subSubSection box="[199,834,1652,1677]" pageId="7" pageNumber="42" type="distribution">
<paragraph blockId="7.[151,1437,675,2037]" box="[199,834,1652,1677]" pageId="7" pageNumber="42">
<emphasis bold="true" box="[199,345,1652,1677]" pageId="7" pageNumber="42">Distribution</emphasis>
. 
<collectingCountry box="[359,426,1654,1677]" name="Japan" pageId="7" pageNumber="42">Japan</collectingCountry>
, 
<collectingCountry box="[437,508,1654,1677]" name="South Korea" pageId="7" pageNumber="42">Korea</collectingCountry>
