, 
<geoCoordinate box="[938,1055,1282,1308]" degrees="108" direction="east" minutes="26" orientation="longitude" pageId="8" pageNumber="139" precision="925" value="108.433334">E 108°26’</geoCoordinate>
, 
<date box="[1067,1215,1282,1308]" pageId="8" pageNumber="139" value="2011-08-14">
<collectingDate box="[1067,1215,1282,1308]" pageId="8" pageNumber="139" value="2011-08-14">2011.VIII.14</collectingDate>
