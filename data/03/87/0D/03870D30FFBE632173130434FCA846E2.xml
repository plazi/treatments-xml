<taxonomicName baseAuthorityName="Smith" baseAuthorityYear="1863" box="[859,1073,1263,1288]" class="Insecta" family="Apidae" genus="Partamona" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="cupira">
<emphasis box="[859,1073,1263,1288]" italics="true" pageId="41" pageNumber="42">Partamona cupira</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[1130,1295,1263,1288]" class="Insecta" family="Apidae" genus="Apis" kingdom="Animalia" order="Hymenoptera" pageId="41" pageNumber="42" phylum="Arthropoda" rank="species" species="mellifera">
<emphasis box="[1130,1295,1263,1288]" italics="true" pageId="41" pageNumber="42">Apis mellifera</emphasis>
