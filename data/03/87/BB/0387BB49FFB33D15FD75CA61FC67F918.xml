, 
<taxonomicName authority="(Hensel, 1862)" baseAuthorityName="Hensel" baseAuthorityYear="1862" box="[734,1090,1122,1139]" class="Mammalia" family="Equidae" genus="Hippotherium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perissodactyla" pageId="12" pageNumber="463" phylum="Chordata" rank="species" species="brachypus">
