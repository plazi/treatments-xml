<taxonomicName authorityName="Berlese" authorityYear="1913" box="[422,598,450,474]" class="Arachnida" family="Tenuipalpidae" kingdom="Animalia" order="Trombidiformes" pageId="5" pageNumber="237" phylum="Arthropoda" rank="family">Tenuipalpidae</taxonomicName>
