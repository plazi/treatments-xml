<taxonomicName authority="(Nomikou et al. 2003)" baseAuthorityName="Nomikou" baseAuthorityYear="2003" class="Insecta" family="Aleyrodidae" genus="Bemisia" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="238" phylum="Arthropoda" rank="species" species="tabaci">
