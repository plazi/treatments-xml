<taxonomicName baseAuthorityName="Hartman" baseAuthorityYear="1957" box="[323,631,1124,1150]" class="Polychaeta" family="Paraonidae" genus="Levinsenia" kingdom="Animalia" pageId="8" pageNumber="265" phylum="Annelida" rank="species" species="multibranchiata">
