<quantity box="[442,573,152,175]" metricMagnitude="-4" metricUnit="m" metricValue="2.5" metricValueMax="3.0" metricValueMin="2.0" pageId="2" pageNumber="73" unit="mm" value="0.25" valueMax="0.3" valueMin="0.2">0.2–0.3 mm</quantity>
, membrane without setae. VR 
<number box="[899,1009,152,175]" pageId="2" pageNumber="73" value="1.44" valueMax="1.48" valueMin="1.4">1.40–1.48</number>
. WW 
<number box="[1075,1185,152,175]" pageId="2" pageNumber="73" value="0.28500000000000003" valueMax="0.34" valueMin="0.23">0.23–0.34</number>
. Anal lobe weak. Costa 
<number box="[151,244,186,209]" pageId="2" pageNumber="73" value="815.5" valueMax="836.0" valueMin="795.0">795–836</number>
Μm long; R 
<number box="[380,388,201,213]" pageId="2" pageNumber="73" value="4.0">4</number>
+ 
<number box="[396,403,201,213]" pageId="2" pageNumber="73" value="5.0">5</number>
ending proximal to apex of M 
<number box="[744,751,202,214]" pageId="2" pageNumber="73" value="3.0">3</number>
+ 
<number box="[759,767,202,214]" pageId="2" pageNumber="73" value="4.0">4</number>
