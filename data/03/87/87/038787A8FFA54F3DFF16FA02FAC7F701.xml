; 
<bibRefCitation author="Davies, D. A. L." box="[637,821,1580,1608]" journalOrPublisher="Soc. int. Odonatol. rapid Comm." pageId="0" pageNumber="147" pagination="1 - 66" part="4" refId="ref2915" title="A synopsis of the extant genera of Odonata" type="journal article" year="1981">Davies, 1981</bibRefCitation>
