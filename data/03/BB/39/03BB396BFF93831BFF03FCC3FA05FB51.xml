<collectingRegion box="[1045,1144,866,890]" country="Canada" name="Ontario" pageId="13" pageNumber="12">Ontario</collectingRegion>
. 
<location LSID="urn:lsid:plazi:treatment:03BB396BFF93831BFF03FCC3FA05FB51:8ECDDEA6FF93831BFB42FC83FB10FCF2" box="[1153,1180,866,889]" county="Harfield County" municipality="In Canada" name="In" pageId="13" pageNumber="12" stateProvince="Ontario">In</location>
