<taxonomicName authorityName="Stark &amp; Sivec" authorityYear="2008" box="[703,776,1532,1556]" class="Insecta" family="Perlidae" genus="Togoperla" kingdom="Animalia" order="Plecoptera" pageId="16" pageNumber="224" phylum="Arthropoda" rank="species" species="clavata">
<emphasis box="[703,776,1532,1556]" italics="true" pageId="16" pageNumber="224">clavata</emphasis>
</taxonomicName>
</keyLead>
</paragraph>
<paragraph blockId="16.[184,776,392,1970]" pageId="16" pageNumber="224">
<keyLead pageId="16" pageNumber="224">
