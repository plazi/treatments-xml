<paragraph blockId="4.[151,1436,386,591]" pageId="4" pageNumber="287">
<emphasis bold="true" box="[151,227,386,411]" pageId="4" pageNumber="287">
<typeStatus box="[151,223,386,411]" pageId="4" pageNumber="287">Types</typeStatus>
