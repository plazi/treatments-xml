<taxonomicName authorityName="Ross" authorityYear="1835" box="[750,840,836,860]" class="Malacostraca" family="Crangonidae" genus="Sabinea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="11" phylum="Arthropoda" rank="genus">
