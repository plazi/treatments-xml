<materialsCitation ID-GBIF-Occurrence="2366589242" collectionCode="PIN" pageId="2" pageNumber="568" specimenCount="1" typeStatus="holotype">
<emphasis bold="true" box="[151,260,1305,1331]" pageId="2" pageNumber="568">
