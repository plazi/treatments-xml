<paragraph blockId="5.[151,1437,152,725]" pageId="5" pageNumber="62">
<taxonomicName authority="Blanchard, 1852" authorityName="Blanchard" authorityYear="1852" box="[198,722,351,377]" class="Insecta" family="Acanthosomatidae" genus="Ditomotarsus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="62" phylum="Arthropoda" rank="species" species="impluviatus">
