<collectingCountry box="[897,954,1871,1892]" name="China" pageId="2" pageNumber="591">China</collectingCountry>
(
<collectingRegion box="[967,1044,1871,1892]" country="China" name="Yunnan" pageId="2" pageNumber="591">Yunnan</collectingRegion>
