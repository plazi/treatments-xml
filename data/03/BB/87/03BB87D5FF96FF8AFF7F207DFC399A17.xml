<collectingCountry box="[992,1129,1510,1534]" name="Madagascar" pageId="2" pageNumber="3">Madagascar</collectingCountry>
, 
<collectingCountry box="[1140,1238,1510,1534]" name="Vietnam" pageId="2" pageNumber="3">Vietnam</collectingCountry>
, 
<collectingCountry box="[1250,1379,1510,1534]" name="Philippines" pageId="2" pageNumber="3">Philippines</collectingCountry>
, 
<collectingCountry box="[1390,1432,1510,1534]" name="Fiji" pageId="2" pageNumber="3">Fiji</collectingCountry>
, 
<collectingCountry box="[151,348,1546,1570]" name="French Polynesia" pageId="2" pageNumber="3">French Polynesia</collectingCountry>
, Hawaii. East Pacific: 
<collectingCountry box="[603,691,1546,1570]" name="Mexico" pageId="2" pageNumber="3">Mexico</collectingCountry>
, 
<collectingCountry box="[703,821,1546,1570]" name="Nicaragua" pageId="2" pageNumber="3">Nicaragua</collectingCountry>
, 
<collectingCountry box="[832,958,1546,1570]" name="Costa Rica" pageId="2" pageNumber="3">Costa Rica</collectingCountry>
, 
<collectingCountry box="[969,1060,1547,1570]" name="Panama" pageId="2" pageNumber="3">Panama</collectingCountry>
, 
<collectingCountry box="[1073,1187,1546,1570]" name="Colombia" pageId="2" pageNumber="3">Colombia</collectingCountry>
. West Atlantic: southeastern 
<collectingCountry box="[240,296,1582,1606]" name="United States of America" pageId="2" pageNumber="3">USA</collectingCountry>
(North Carolina, Florida, Louisiana), 
<collectingCountry box="[738,856,1582,1606]" name="Venezuela" pageId="2" pageNumber="3">Venezuela</collectingCountry>
, 
<collectingCountry box="[870,943,1582,1606]" name="Aruba" pageId="2" pageNumber="3">Aruba</collectingCountry>
, 
<collectingCountry box="[957,1048,1583,1606]" name="Panama" pageId="2" pageNumber="3">Panama</collectingCountry>
, 
