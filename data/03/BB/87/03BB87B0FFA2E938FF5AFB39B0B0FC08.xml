<subSubSection pageId="1" pageNumber="302" type="type_taxon">
<paragraph blockId="1.[151,679,1263,1318]" box="[151,661,1263,1287]" pageId="1" pageNumber="302">
<emphasis bold="true" box="[151,286,1263,1287]" pageId="1" pageNumber="302">
<typeStatus box="[151,204,1263,1287]" pageId="1" pageNumber="302">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName authority="Marchenko, 2022" authorityName="Marchenko" authorityYear="2022" box="[298,575,1263,1287]" class="Arachnida" family="Zerconidae" genus="Baikalozercon" kingdom="Animalia" order="Mesostigmata" pageId="1" pageNumber="302" phylum="Arthropoda" rank="species" species="dracunculus" status="sp. nov.">
