and 
<taxonomicName box="[634,913,1964,1986]" class="Amphibia" family="Rhacophoridae" genus="Polypedates" kingdom="Animalia" order="Anura" pageId="7" pageNumber="491" phylum="Chordata" rank="species" species="megacephalus">
