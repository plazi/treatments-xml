<subSubSection box="[151,429,1936,1963]" pageId="9" pageNumber="568" type="diagnosis">
<paragraph blockId="9.[151,1436,1936,2035]" box="[151,429,1936,1963]" pageId="9" pageNumber="568">
<emphasis bold="true" box="[151,274,1936,1962]" pageId="9" pageNumber="568">Diagnosis.</emphasis>
