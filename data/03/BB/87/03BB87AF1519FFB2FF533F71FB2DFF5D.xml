<collectingCountry box="[359,426,151,176]" name="China" pageId="17" pageNumber="520">China</collectingCountry>
(
<collectingRegion box="[441,542,151,176]" country="China" name="Guangxi" pageId="17" pageNumber="520">Guangxi</collectingRegion>
, 
