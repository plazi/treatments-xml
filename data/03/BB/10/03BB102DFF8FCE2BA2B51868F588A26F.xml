<emphasis box="[1018,1176,1354,1379]" italics="true" pageId="58" pageNumber="59">A. septilobum</emphasis>
Fang, 
<number box="[1263,1322,1355,1379]" pageId="58" pageNumber="59" value="1932.0">1932</number>
</taxonomicName>
, 
<taxonomicName authority="Maxim., 1857" authorityName="Maxim." authorityYear="1857" class="Magnoliopsida" family="Sapindaceae" genus="Acer" kingdom="Plantae" order="Sapindales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="species" species="mono">
