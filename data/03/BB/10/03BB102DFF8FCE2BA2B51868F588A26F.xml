</taxonomicName>
(all 
<taxonomicName box="[369,487,1391,1415]" class="Magnoliopsida" family="Aceraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Sapindales" pageId="58" pageNumber="59" phylum="Tracheophyta" rank="family">Aceraceae</taxonomicName>
