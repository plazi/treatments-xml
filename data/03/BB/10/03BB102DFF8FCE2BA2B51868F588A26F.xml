<collectionCode LSID="urn:lsid:biocol.org:col:34613" box="[1328,1412,1319,1343]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34613" name="Zoologisches Forschungsmuseum Alexander Koenig" pageId="58" pageNumber="59">ZFMK</collectionCode>
