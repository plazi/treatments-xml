<collectingCountry box="[483,550,1878,1903]" name="China" pageId="15" pageNumber="16">China</collectingCountry>
(Hunan, Guangdong, Guangxi, Yunnan) and Hainan, 
<collectingCountry box="[1189,1318,1878,1903]" name="Philippines" pageId="15" pageNumber="16">Philippines</collectingCountry>
, 
<collectingCountry box="[1334,1432,1878,1903]" name="Vietnam" pageId="15" pageNumber="16">Vietnam</collectingCountry>
, 
<collectingCountry box="[151,257,1911,1936]" name="Malaysia" pageId="15" pageNumber="16">Malaysia</collectingCountry>
, 
<collectingCountry box="[272,384,1911,1936]" name="Indonesia" pageId="15" pageNumber="16">Indonesia</collectingCountry>
, 
<collectingCountry box="[400,502,1911,1936]" name="Thailand" pageId="15" pageNumber="16">Thailand</collectingCountry>
, Sikkim, 
<collectingCountry box="[618,687,1911,1936]" name="Nepal" pageId="15" pageNumber="16">Nepal</collectingCountry>
, 
<collectingCountry box="[703,821,1911,1936]" name="Sri Lanka" pageId="15" pageNumber="16">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[836,896,1911,1936]" name="India" pageId="15" pageNumber="16">India</collectingCountry>
, vast region from Southeast Asia to Sumatra, Borneo and the 
<collectingCountry box="[329,455,1945,1970]" name="Philippines" pageId="15" pageNumber="16">Philippines</collectingCountry>
