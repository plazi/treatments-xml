<subSubSection pageId="23" pageNumber="174" type="discussion">
<paragraph blockId="23.[151,1436,151,285]" pageId="23" pageNumber="174">
<emphasis bold="true" box="[199,308,151,176]" pageId="23" pageNumber="174">Remarks</emphasis>
. The material studied match to 
<taxonomicName box="[680,930,151,176]" class="Demospongiae" family="Niphatidae" genus="Amphimedon" kingdom="Animalia" order="Haplosclerida" pageId="23" pageNumber="174" phylum="Porifera" rank="species" species="caribica">
<emphasis box="[680,930,151,176]" italics="true" pageId="23" pageNumber="174">Amphimedon caribica</emphasis>
</taxonomicName>
