<taxonomicName authority="Peters, 1864" authorityName="Peters" authorityYear="1864" box="[364,836,258,283]" class="Reptilia" family="Scincidae" genus="Scincopus" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="70" phylum="Chordata" rank="subSpecies" species="fasciatus" subSpecies="fasciatus">
