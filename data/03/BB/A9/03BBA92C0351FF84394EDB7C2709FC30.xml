, width 
<number box="[389,429,524,550]" pageId="16" pageNumber="57" value="1.6">1.6</number>
; length antennal segments I–IV: 
<number box="[812,866,524,550]" pageId="16" pageNumber="57" value="0.33">0.33</number>
, 
<number box="[878,932,524,550]" pageId="16" pageNumber="57" value="2.05">2.05</number>
, 
<number box="[944,998,524,550]" pageId="16" pageNumber="57" value="1.33">1.33</number>
, 1.0; length pronotum 
<number box="[1264,1318,524,550]" pageId="16" pageNumber="57" value="1.76">1.76</number>
, width 
<number box="[342,397,564,590]" pageId="16" pageNumber="57" value="3.05">3.05</number>
; length hemelytron 
<number box="[640,694,564,590]" pageId="16" pageNumber="57" value="6.67">6.67</number>
