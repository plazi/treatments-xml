<emphasis bold="true" box="[912,936,1082,1105]" pageId="10" pageNumber="11">C.</emphasis>
<taxonomicName authority="Stauffer, Bowers, Kellogg &amp; McKaye, 1997" authorityName="Stauffer, Bowers, Kellogg &amp; McKaye" authorityYear="1997" box="[942,1054,1083,1105]" class="Teleostei" family="Cichlidae" genus="Metriaclima" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="glaucos">
