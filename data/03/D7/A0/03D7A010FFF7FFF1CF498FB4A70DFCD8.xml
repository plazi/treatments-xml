): Panay Is, 
<collectingCountry box="[939,1074,564,590]" name="Philippines" pageId="8" pageNumber="31">Philippines</collectingCountry>
, 
<date box="[1085,1223,564,590]" pageId="8" pageNumber="31" value="1995-07-20">
<number box="[1085,1118,564,590]" pageId="8" pageNumber="31" value="20.0">20</number>
