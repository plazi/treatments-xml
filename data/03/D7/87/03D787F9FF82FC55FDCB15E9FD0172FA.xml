!; isotype 
<collectionCode box="[995,1072,1499,1525]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:15843" lsid="urn:lsid:biocol.org:col:15843" name="Botanical Research Institute of Texas" pageId="9" pageNumber="10" type="Herbarium">BRIT</collectionCode>
