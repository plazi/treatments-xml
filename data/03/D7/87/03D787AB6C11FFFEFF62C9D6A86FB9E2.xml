<typeStatus box="[151,211,821,847]" pageId="23" pageNumber="24">Type</typeStatus>
material
</emphasis>
. 
<emphasis bold="true" box="[345,423,821,847]" pageId="23" pageNumber="24">
Stn. 
<number box="[394,423,821,847]" pageId="23" pageNumber="24" value="10.0">10</number>
</emphasis>
: 0 
<date box="[455,578,821,847]" pageId="23" pageNumber="24" value="2009-02-12">
<number box="[455,511,821,847]" pageId="23" pageNumber="24" value="2.12">2.12</number>
