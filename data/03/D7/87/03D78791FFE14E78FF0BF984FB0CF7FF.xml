— 
<taxonomicName authorityName="Q. Xu, Y. B. Luo &amp; Z. J. Liu. A. Flowering" authorityYear="2014" box="[516,811,1911,1937]" class="Liliopsida" family="Orchidaceae" genus="Dendrobium" kingdom="Plantae" order="Asparagales" pageId="9" pageNumber="138" phylum="Tracheophyta" rank="species" species="wenshanense">
