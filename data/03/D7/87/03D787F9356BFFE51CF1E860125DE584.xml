<taxonomicName authority="O.F.Müller, 1773" authorityName="Kozhov" authorityYear="1936" box="[1049,1184,1911,1936]" class="Gastropoda" family="Valvatidae" genus="Valvata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ectobranchia" pageId="8" pageNumber="53" phylum="Mollusca" rank="species" species="brevicula">
