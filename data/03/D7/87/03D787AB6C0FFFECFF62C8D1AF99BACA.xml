</bibRefCitation>
); 
<taxonomicName authority="Bourne, 1890" authorityName="Bourne" authorityYear="1890" box="[751,1114,1600,1626]" class="Hydrozoa" family="Haleciidae" genus="Halecium" kingdom="Animalia" order="Leptothecata" pageId="9" pageNumber="10" phylum="Cnidaria" rank="species" species="lankesteri">
