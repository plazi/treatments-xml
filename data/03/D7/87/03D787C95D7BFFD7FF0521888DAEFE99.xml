115, 
<emphasis box="[1342,1366,622,647]" italics="true" pageId="1" pageNumber="139">ro</emphasis>
95, 
<emphasis box="[1416,1436,621,647]" italics="true" pageId="1" pageNumber="139">le</emphasis>
30, 
<emphasis box="[195,217,658,683]" italics="true" pageId="1" pageNumber="139">in</emphasis>
195; notogaster: length 550, width 415, height 400; setae: 
