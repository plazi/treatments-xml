<subScript attach="left" box="[1081,1089,675,689]" fontSize="6" pageId="1" pageNumber="139">1</subScript>
215, 
<emphasis box="[1155,1167,658,683]" italics="true" pageId="1" pageNumber="139">e</emphasis>
<subScript attach="left" box="[1168,1176,675,689]" fontSize="6" pageId="1" pageNumber="139">1</subScript>
215, 
<emphasis box="[1242,1256,657,683]" italics="true" pageId="1" pageNumber="139">h</emphasis>
<subScript attach="left" box="[1256,1264,675,689]" fontSize="6" pageId="1" pageNumber="139">1</subScript>
200, 
<emphasis box="[1329,1343,657,683]" italics="true" pageId="1" pageNumber="139">h</emphasis>
<subScript attach="left" box="[1343,1351,675,689]" fontSize="6" pageId="1" pageNumber="139">3</subScript>
90, 
