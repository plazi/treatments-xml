<emphasis bold="true" box="[151,356,1961,1985]" pageId="7" pageNumber="52">FIGURES 11–15.</emphasis>
<taxonomicName authority="(Kieffer, 1905)" authorityName="Azevedo &amp; Alencar" authorityYear="2009" baseAuthorityName="Kieffer" baseAuthorityYear="1905" box="[365,771,1961,1984]" class="Insecta" family="Bethylidae" genus="Pristocera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="52" phylum="Arthropoda" rank="species" species="levicollis" status="comb. nov.">
<emphasis box="[365,593,1961,1984]" italics="true" pageId="7" pageNumber="52">Pristocera levicollis</emphasis>
(
<bibRefCitation author="Kieffer, J. J." box="[609,762,1961,1984]" pageId="7" pageNumber="52" pagination="95 - 142" refId="ref4616" refString="Kieffer, J. J. (1905) Description de nouveaux Proctotrypides exotiques. Annales de la Societe Scientifique de Bruxelles, 29, 95 - 142." type="journal article" year="1905">Kieffer, 1905</bibRefCitation>
)
</taxonomicName>
<emphasis bold="true" box="[780,908,1961,1985]" pageId="7" pageNumber="52">
