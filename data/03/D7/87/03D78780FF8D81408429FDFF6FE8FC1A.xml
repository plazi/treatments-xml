<subSubSection box="[312,1303,844,870]" pageId="11" pageNumber="12" type="etymology">
<paragraph blockId="11.[264,1323,284,910]" box="[312,1303,844,870]" pageId="11" pageNumber="12">
<emphasis box="[312,435,845,870]" italics="true" pageId="11" pageNumber="12">Etymology</emphasis>
