<number box="[1399,1428,1759,1784]" pageId="12" pageNumber="381" value="26.0">26</number>
) as long as wide, F 
<number box="[355,369,1796,1820]" pageId="12" pageNumber="381" value="2.0">2</number>
<number box="[375,425,1796,1821]" pageId="12" pageNumber="381" value="1.46">1.46</number>
