<number box="[697,824,188,212]" pageId="5" pageNumber="439" value="1315.0" valueMax="1380.0" valueMin="1250.0">1250–1380</number>
; VIII, 
<number box="[900,944,188,212]" pageId="5" pageNumber="439" value="450.0">450</number>
/ 
<number box="[953,1009,188,212]" pageId="5" pageNumber="439" value="1050.0">1050</number>
