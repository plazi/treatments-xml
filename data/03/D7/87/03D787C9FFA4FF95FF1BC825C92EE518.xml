<collectionCode LSID="urn:lsid:biocol.org:col:34795" box="[1207,1299,1882,1908]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34795" name="Field Museum of Natural History" pageId="8" pageNumber="39">FMNH</collectionCode>
).
</paragraph>
<paragraph blockId="8.[151,1437,1611,2024]" pageId="8" pageNumber="39">
<emphasis bold="true" box="[197,780,1920,1946]" pageId="8" pageNumber="39">
