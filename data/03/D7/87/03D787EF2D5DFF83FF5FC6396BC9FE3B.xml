<taxonomicName authority="Collyer" authorityName="Collyer" box="[575,865,188,212]" class="Arachnida" family="Phytoseiidae" genus="Gynaeseius" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="brevisetosus">
<emphasis box="[575,592,189,212]" italics="true" pageId="7" pageNumber="8">G</emphasis>
. 
<emphasis box="[611,748,188,212]" italics="true" pageId="7" pageNumber="8">brevisetosus</emphasis>
(Collyer)
</taxonomicName>
, 
<taxonomicName authority="Schicha" authorityName="Schicha" box="[882,1151,188,212]" class="Arachnida" family="Phytoseiidae" genus="Gynaeseius" kingdom="Animalia" order="Mesostigmata" pageId="7" pageNumber="8" phylum="Arthropoda" rank="species" species="christinae">
