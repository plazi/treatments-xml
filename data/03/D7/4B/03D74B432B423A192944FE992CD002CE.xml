: 
<quantity box="[740,849,799,825]" metricMagnitude="-4" metricUnit="m" metricValue="5.6" pageId="2" pageNumber="37" unit="mm" value="0.56">0.56 mm</quantity>
; 
<emphasis bold="true" box="[862,919,798,824]" pageId="2" pageNumber="37">ASL</emphasis>
: 
<quantity box="[935,1044,799,825]" metricMagnitude="-4" metricUnit="m" metricValue="1.5" pageId="2" pageNumber="37" unit="mm" value="0.15">0.15 mm</quantity>
; 
