): 
<emphasis bold="true" box="[843,882,718,744]" pageId="2" pageNumber="37">BL</emphasis>
: 
<quantity box="[897,1004,719,745]" metricMagnitude="-3" metricUnit="m" metricValue="2.92" pageId="2" pageNumber="37" unit="mm" value="2.92">2.92 mm</quantity>
; 
<emphasis bold="true" box="[1016,1055,718,744]" pageId="2" pageNumber="37">EL</emphasis>
: 
<quantity box="[1069,1177,719,745]" metricMagnitude="-3" metricUnit="m" metricValue="1.8" pageId="2" pageNumber="37" unit="mm" value="1.8">1.80 mm</quantity>
; 
<emphasis bold="true" box="[1188,1256,718,744]" pageId="2" pageNumber="37">EWB</emphasis>
: 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="9.2" pageId="2" pageNumber="37" unit="mm" value="0.92">0.92 mm</quantity>
; 
<emphasis bold="true" box="[328,405,758,784]" pageId="2" pageNumber="37">EWW</emphasis>
: 
<quantity box="[423,534,759,785]" metricMagnitude="-3" metricUnit="m" metricValue="1.48" pageId="2" pageNumber="37" unit="mm" value="1.48">1.48 mm</quantity>
; 
<emphasis bold="true" box="[549,591,758,784]" pageId="2" pageNumber="37">EH</emphasis>
: 
<quantity box="[609,720,759,785]" metricMagnitude="-4" metricUnit="m" metricValue="8.4" pageId="2" pageNumber="37" unit="mm" value="0.84">0.84 mm</quantity>
; 
<emphasis bold="true" box="[735,794,758,784]" pageId="2" pageNumber="37">PNL</emphasis>
: 
<quantity box="[812,922,759,785]" metricMagnitude="-4" metricUnit="m" metricValue="8.4" pageId="2" pageNumber="37" unit="mm" value="0.84">0.84 mm</quantity>
; 
<emphasis bold="true" box="[937,1004,758,784]" pageId="2" pageNumber="37">PNW</emphasis>
: 
<quantity box="[1023,1133,759,785]" metricMagnitude="-3" metricUnit="m" metricValue="1.12" pageId="2" pageNumber="37" unit="mm" value="1.12">1.12 mm</quantity>
; 
<emphasis bold="true" box="[1148,1189,758,784]" pageId="2" pageNumber="37">RL</emphasis>
: 
<quantity box="[1207,1318,759,785]" metricMagnitude="-4" metricUnit="m" metricValue="4.1" pageId="2" pageNumber="37" unit="mm" value="0.41">0.41 mm</quantity>
; 
