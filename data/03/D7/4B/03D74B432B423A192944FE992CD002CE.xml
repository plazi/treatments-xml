; 
<emphasis bold="true" box="[469,537,798,824]" pageId="2" pageNumber="37">RWB</emphasis>
: 
<quantity box="[554,663,799,825]" metricMagnitude="-4" metricUnit="m" metricValue="3.4" pageId="2" pageNumber="37" unit="mm" value="0.34">0.34 mm</quantity>
; 
