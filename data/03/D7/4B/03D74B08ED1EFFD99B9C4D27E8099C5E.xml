<paragraph blockId="4.[151,1437,730,1552]" pageId="4" pageNumber="171">
Total length 
<quantity box="[342,445,1019,1043]" metricMagnitude="-3" metricUnit="m" metricValue="2.11" pageId="4" pageNumber="171" unit="mm" value="2.11">2.11 mm</quantity>
