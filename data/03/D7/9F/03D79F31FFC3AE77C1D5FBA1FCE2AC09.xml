<taxonomicName authority="Panzer, 1798" authorityName="Panzer" authorityYear="1798" class="Insecta" family="Muscidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="formosa">
