<taxonomicName authority="Latreille" authorityName="Latreille" box="[774,1081,1488,1514]" class="Insecta" family="Chloropidae" genus="Oscinis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="elegans">
