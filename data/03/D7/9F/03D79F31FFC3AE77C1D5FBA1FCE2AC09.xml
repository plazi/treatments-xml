<taxonomicName authority="Latreille, 1804" authorityName="Latreille" authorityYear="1804" class="Insecta" family="Ulidiidae" genus="Otites" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="genus">
