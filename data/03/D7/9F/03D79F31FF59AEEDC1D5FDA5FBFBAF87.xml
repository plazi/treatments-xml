<taxonomicName authorityName="Rondani" authorityYear="1856" box="[657,756,1253,1278]" class="Insecta" family="Tachinidae" genus="Spylosia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="170" pageNumber="171" phylum="Arthropoda" rank="genus">
