<taxonomicName authorityName="Burmeister" authorityYear="1847" box="[972,1194,987,1011]" class="Insecta" family="Dynastidae" genus="Podalgus" kingdom="Animalia" order="Coleoptera" pageId="207" pageNumber="206" phylum="Arthropoda" rank="species" species="exaratus">
