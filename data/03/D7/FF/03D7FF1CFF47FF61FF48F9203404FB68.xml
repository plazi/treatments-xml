<taxonomicName authority="sparsa Ohaus, 1903" authorityName="Ohaus" authorityYear="1903" class="Insecta" family="Rutelidae" genus="Lagochile" kingdom="Animalia" order="Coleoptera" pageId="140" pageNumber="139" phylum="Arthropoda" rank="subSpecies" species="sparsa" subSpecies="sparsa">
