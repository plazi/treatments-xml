<collectionCode box="[1134,1222,1538,1561]" country="Netherlands" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34992" lsid="urn:lsid:biocol.org:col:34992" name="National Museum of Natural History, Naturalis" pageId="230" pageNumber="229" type="Museum">RMNH</collectionCode>
