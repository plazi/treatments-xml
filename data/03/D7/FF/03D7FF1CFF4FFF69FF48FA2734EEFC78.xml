<taxonomicName authority="Soula, 2005" authorityName="Soula" authorityYear="2005" box="[736,1183,438,462]" class="Insecta" family="Rutelidae" genus="Chlorota" kingdom="Animalia" order="Coleoptera" pageId="132" pageNumber="131" phylum="Arthropoda" rank="subSpecies" species="surinama" subSpecies="iquitosensis">
<emphasis box="[736,1036,438,462]" italics="true" pageId="132" pageNumber="131">C. surinama iquitosensis</emphasis>
<bibRefCitation author="Soula" box="[1042,1183,438,462]" firstAuthor="Soula" pageId="132" pageNumber="131" pagination="297 - 409" refId="ref225600" refString="Soula, M. 2005. Rutelini 2, Revision des Anticheirina 3. Les Coleopteres du Monde 26 - 3: 297 - 409." type="journal article" year="2005">Soula, 2005</bibRefCitation>
</taxonomicName>
(recorded from 
<collectingCountry box="[1372,1431,438,462]" name="Peru" pageId="132" pageNumber="131">Peru</collectingCountry>
) and 
<taxonomicName authority="Soula, 2002" authorityName="Soula" authorityYear="2002" box="[246,656,470,494]" class="Insecta" family="Rutelidae" genus="Chlorota" kingdom="Animalia" order="Coleoptera" pageId="132" pageNumber="131" phylum="Arthropoda" rank="subSpecies" species="surinama" subSpecies="lemoulti">
<emphasis box="[246,507,470,494]" italics="true" pageId="132" pageNumber="131">C. surinama lemoulti</emphasis>
<bibRefCitation author="Soula, M." box="[513,656,470,494]" journalOrPublisher="Les Coleopteres du Monde 26" pageId="132" pageNumber="131" pagination="117 - 296" part="2" refId="ref225518" refString="Soula, M. 2002 a. Rutelini 2, Revision des Anticheirina 2. Les Coleopteres du Monde 26 - 2: 117 - 296." title="Rutelini 2, Revision des Anticheirina 2" type="journal article" year="2002">Soula, 2002</bibRefCitation>
</taxonomicName>
