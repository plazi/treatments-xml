<taxonomicName authority="calcaratus (Spinola, 1835)" authorityName="calcaratus (Spinola" authorityYear="1835" baseAuthorityName="Spinola" baseAuthorityYear="1835" box="[847,1321,1052,1077]" class="Insecta" family="Rutelidae" genus="Pseudodorysthetus" kingdom="Animalia" order="Coleoptera" pageId="152" pageNumber="151" phylum="Arthropoda" rank="subSpecies" species="calcaratus" subSpecies="calcaratus">
