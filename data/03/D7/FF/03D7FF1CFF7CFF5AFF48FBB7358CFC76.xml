<taxonomicName baseAuthorityName="Hohne" baseAuthorityYear="1923" class="Insecta" family="Dynastidae" genus="Mimeoma" kingdom="Animalia" order="Coleoptera" pageId="183" pageNumber="182" phylum="Arthropoda" rank="species" species="signatoides">
