<taxonomicName baseAuthorityName="Hohne" baseAuthorityYear="1923" box="[511,685,846,870]" class="Insecta" family="Dynastidae" genus="Mimeoma" kingdom="Animalia" order="Coleoptera" pageId="183" pageNumber="182" phylum="Arthropoda" rank="species" species="signatoides">
<emphasis box="[511,685,846,870]" italics="true" pageId="183" pageNumber="182">M. signatoides</emphasis>
</taxonomicName>
