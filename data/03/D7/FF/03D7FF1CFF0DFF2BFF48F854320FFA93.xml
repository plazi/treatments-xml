<taxonomicName authority="gyas (Herbst, 1785)" authorityName="gyas (Herbst" authorityYear="1785" baseAuthorityName="Herbst" baseAuthorityYear="1785" box="[723,1157,934,958]" class="Insecta" family="Dynastidae" genus="Megasoma" kingdom="Animalia" order="Coleoptera" pageId="198" pageNumber="197" phylum="Arthropoda" rank="subSpecies" species="gyas" subSpecies="gyas">
