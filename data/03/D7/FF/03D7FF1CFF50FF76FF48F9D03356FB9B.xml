<collectingCountry box="[448,545,1043,1067]" name="Guyana" pageId="155" pageNumber="154">Guyana</collectingCountry>
and 
<collectingCountry box="[606,730,1043,1067]" name="Suriname" pageId="155" pageNumber="154">Suriname</collectingCountry>
