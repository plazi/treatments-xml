, 
<collectingCountry box="[1249,1380,1962,1988]" name="Kyrgyzstan" pageId="51" pageNumber="52">Kyrgyzstan</collectingCountry>
, 
<collectingCountry name="Kazakhstan" pageId="51" pageNumber="52">Kazakhstan</collectingCountry>
.
