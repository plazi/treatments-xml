<quantity box="[931,1097,1286,1311]" metricMagnitude="-3" metricUnit="m" metricValue="6.39" metricValueMax="6.97" metricValueMin="5.81" pageId="6" pageNumber="287" unit="mm" value="6.39" valueMax="6.97" valueMin="5.81">5.81–6.97 mm</quantity>
