<paragraph blockId="1.[264,873,993,1019]" box="[264,873,993,1019]" pageId="1" pageNumber="2">
<emphasis box="[264,408,993,1018]" italics="true" pageId="1" pageNumber="2">
<typeStatus box="[264,316,993,1018]" pageId="1" pageNumber="2">Type</typeStatus>
species
</emphasis>
: 
<taxonomicName authority="Silvestri, 1899" authorityName="Silvestri" authorityYear="1899" box="[426,868,993,1019]" class="Chilopoda" family="Henicopidae" genus="Anopsobius" kingdom="Animalia" order="Lithobiomorpha" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="productus">
