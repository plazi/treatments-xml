<collectingCountry box="[715,932,977,1002]" name="Papua New Guinea" pageId="11" pageNumber="218">Papua New Guinea</collectingCountry>
and 
<collectingCountry box="[986,1054,978,1001]" name="Papua New Guinea" pageId="11" pageNumber="218">Papua</collectingCountry>
Province, 
<collectingCountry box="[1175,1288,977,1002]" name="Indonesia" pageId="11" pageNumber="218">Indonesia</collectingCountry>
