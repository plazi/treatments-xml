<collectingCountry box="[406,490,1218,1244]" name="Turkey" pageId="10" pageNumber="162">Turkey</collectingCountry>
(
<collectingRegion box="[505,652,1218,1245]" country="Turkey" name="Gumushane" pageId="10" pageNumber="162">Gümüşhane</collectingRegion>
