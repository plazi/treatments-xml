<collectingCountry box="[885,955,1884,1908]" name="China" pageId="10" pageNumber="49">China</collectingCountry>
; 
<collectingCountry box="[966,1033,1885,1908]" name="Japan" pageId="10" pageNumber="49">Japan</collectingCountry>
; 
<collectingCountry box="[1044,1116,1885,1908]" name="South Korea" pageId="10" pageNumber="49">Korea</collectingCountry>
. Sandy mud bottom 
<quantity box="[1345,1432,1884,1908]" metricMagnitude="1" metricUnit="m" metricValue="2.75" metricValueMax="5.0" metricValueMin="0.5" pageId="10" pageNumber="49" unit="m" value="27.5" valueMax="50.0" valueMin="5.0">
