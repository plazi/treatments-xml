<subSubSection box="[199,1250,476,501]" pageId="2" pageNumber="497" type="etymology">
<paragraph blockId="2.[151,1436,151,752]" box="[199,1250,476,501]" pageId="2" pageNumber="497">
<emphasis bold="true" box="[199,326,476,501]" pageId="2" pageNumber="497">Etymology</emphasis>
