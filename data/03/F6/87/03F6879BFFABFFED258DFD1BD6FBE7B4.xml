(
<taxonomicName box="[691,868,1586,1612]" class="Magnoliopsida" family="Symplocaceae" kingdom="Plantae" order="Ericales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="family">Symplocaceae</taxonomicName>
). (deposited at 
<collectionCode box="[1042,1132,1586,1612]" httpUri="http://grbio.org/cool/2nry-s1yg" name="National Museum of Natural Science" pageId="30" pageNumber="31">NMNS</collectionCode>
). 
<typeStatus box="[1151,1276,1586,1612]" pageId="30" pageNumber="31">
