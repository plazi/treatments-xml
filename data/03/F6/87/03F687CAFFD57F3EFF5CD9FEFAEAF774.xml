<subSubSection pageId="49" pageNumber="250" type="etymology">
<paragraph blockId="49.[151,1437,153,612]" pageId="49" pageNumber="250">
<emphasis bold="true" box="[199,342,333,359]" pageId="49" pageNumber="250">ETYMOLOGY.</emphasis>
