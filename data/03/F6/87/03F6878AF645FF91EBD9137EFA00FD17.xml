(
<bibRefCitation author="Liang" box="[1306,1422,1708,1730]" pageId="3" pageNumber="429" refString="Liang, G. Q. (1995) A new species of the genus Austrohancockia Gunther from Guangdong Province, China (Orthoptera: Tetrigoidea: Cladonotidae). Acta Entomologica Sinica, 38 (3), 332 - 333." type="journal article" year="1995">Liang, 1995</bibRefCitation>
)
</taxonomicName>
</keyLead>
