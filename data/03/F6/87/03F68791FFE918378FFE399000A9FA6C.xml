<quantity box="[710,883,1333,1358]" metricMagnitude="-3" metricUnit="m" metricValue="1.775" metricValueMax="1.85" metricValueMin="1.7" pageId="62" pageNumber="63" unit="mm" value="1.775" valueMax="1.85" valueMin="1.7">1.70–1.85 mm</quantity>
