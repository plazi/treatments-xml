<subSubSection box="[327,853,1134,1156]" pageId="9" type="nomenclature">
<paragraph blockId="9.[327,853,1134,1183]" box="[327,853,1134,1156]" pageId="9">
<heading box="[327,853,1134,1156]" centered="true" fontSize="9" level="2" pageId="9" reason="2">
<emphasis bold="true" box="[327,853,1134,1156]" pageId="9">
<taxonomicName authority="Aalbu and Thomas" authorityName="Aalbu and Thomas" authorityYear="2007" box="[327,725,1134,1156]" class="Insecta" family="Tenebrionidae" genus="Eschatomoxys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="9" phylum="Arthropoda" rank="species" species="paco" status="new species">
<emphasis bold="true" box="[327,525,1134,1156]" italics="true" pageId="9">Eschatomoxys paco</emphasis>
Aalbu and Thomas
</taxonomicName>
, 
<taxonomicNameLabel box="[738,853,1134,1156]" pageId="9" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
