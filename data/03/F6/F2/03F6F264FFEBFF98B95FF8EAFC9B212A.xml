<paragraph blockId="7.[314,865,949,998]" box="[314,865,949,971]" pageId="7">
<heading box="[314,865,949,971]" centered="true" fontSize="9" level="2" pageId="7" reason="2">
<emphasis bold="true" box="[314,865,949,971]" pageId="7">
<taxonomicName authority="Thomas and Pape" authorityName="Thomas and Pape" authorityYear="2007" box="[314,738,949,971]" class="Insecta" family="Tenebrionidae" genus="Eschatomoxys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" phylum="Arthropoda" rank="species" species="pholeter" status="new species">
<emphasis bold="true" box="[314,546,949,971]" italics="true" pageId="7">Eschatomoxys pholeter</emphasis>
Thomas and Pape
</taxonomicName>
, 
<taxonomicNameLabel box="[751,865,949,971]" pageId="7" rank="species">new species</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
