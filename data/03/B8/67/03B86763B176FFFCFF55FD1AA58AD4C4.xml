<quantity box="[835,963,1669,1696]" metricMagnitude="2" metricUnit="m" metricValue="4.5" metricValueMax="6.0" metricValueMin="3.0" pageId="30" pageNumber="431" unit="m" value="450.0" valueMax="600.0" valueMin="300.0">300–600 m</quantity>
