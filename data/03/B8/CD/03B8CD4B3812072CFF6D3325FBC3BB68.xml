; length of tegmen 
<number box="[1147,1176,619,642]" pageId="1" pageNumber="598" value="17.0">17</number>
; width of pronotum 
<number box="[1395,1431,619,642]" pageId="1" pageNumber="598" value="5.5">5.5</number>
; width of tegmen 
