<paragraph blockId="5.[151,773,295,322]" box="[151,773,295,322]" pageId="5" pageNumber="193">
<heading bold="true" box="[151,773,295,322]" fontSize="11" level="1" pageId="5" pageNumber="193" reason="1">
<emphasis bold="true" box="[151,773,295,322]" pageId="5" pageNumber="193">
<taxonomicName authority="Elsayed &amp; Tokuda" authorityName="Elsayed &amp; Shimizu-Kaya &amp; Itioka &amp; Meleng &amp; Yukawa &amp; Tokuda" authorityYear="2018" box="[151,672,295,322]" class="Insecta" family="Cecidomyiidae" genus="Macarangamyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="5" pageNumber="193" phylum="Arthropoda" rank="species" species="itiokai" status="sp. nov.">
<emphasis bold="true" box="[151,434,295,321]" italics="true" pageId="5" pageNumber="193">Macarangamyia itiokai</emphasis>
Elsayed &amp; Tokuda
</taxonomicName>
<taxonomicNameLabel box="[680,773,296,322]" pageId="5" pageNumber="193" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
