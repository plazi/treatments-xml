</taxonomicName>
as summarized by Ermilov 
<emphasis box="[975,1039,1344,1369]" italics="true" pageId="76" pageNumber="77">et al.</emphasis>
(
<number box="[1062,1120,1344,1368]" pageId="76" pageNumber="77" value="2013.0">2013</number>
<emphasis box="[1119,1133,1344,1369]" italics="true" pageId="76" pageNumber="77">d</emphasis>
) and subgenus 
<taxonomicName box="[1332,1436,1344,1369]" class="Arachnida" family="Galumnidae" genus="Galumna" kingdom="Animalia" order="Sarcoptiformes" pageId="76" pageNumber="77" phylum="Arthropoda" rank="genus">
<emphasis box="[1332,1436,1344,1369]" italics="true" pageId="76" pageNumber="77">Galumna</emphasis>
</taxonomicName>
(
<taxonomicName box="[160,303,1380,1405]" class="Arachnida" family="Galumnidae" genus="Neogalumna" kingdom="Animalia" order="Sarcoptiformes" pageId="76" pageNumber="77" phylum="Arthropoda" rank="genus">
<emphasis box="[160,303,1380,1405]" italics="true" pageId="76" pageNumber="77">Neogalumna</emphasis>
</taxonomicName>
