<collectingCountry box="[558,636,1866,1891]" name="Russia" pageId="39" pageNumber="40">Russia</collectingCountry>
; two 
<typeStatus box="[707,814,1867,1891]" pageId="39" pageNumber="40">paratypes</typeStatus>
