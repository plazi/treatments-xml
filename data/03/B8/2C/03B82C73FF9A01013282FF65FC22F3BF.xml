<collectingCountry box="[610,688,1830,1855]" name="Russia" pageId="39" pageNumber="40">Russia</collectingCountry>
; five 
<typeStatus box="[763,870,1832,1856]" pageId="39" pageNumber="40">paratypes</typeStatus>
