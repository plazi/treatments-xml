<number box="[151,195,674,700]" pageId="2" pageNumber="65" value="2.5" valueMax="4.0" valueMin="1.0">1–4</number>
length 
<number box="[286,405,674,700]" pageId="2" pageNumber="65" value="0.75" valueMax="0.8" valueMin="0.7">0.70–0.80</number>
, 
<number box="[419,538,674,700]" pageId="2" pageNumber="65" value="1.5350000000000001" valueMax="1.57" valueMin="1.5">1.50–1.57</number>
, 
<number box="[552,673,674,700]" pageId="2" pageNumber="65" value="1.375" valueMax="1.5" valueMin="1.25">1.25–1.50</number>
