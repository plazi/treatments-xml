<taxonomicName authority="(Porter 1989)" authorityName="Porter" authorityYear="1989" baseAuthorityName="Porter" baseAuthorityYear="1989" box="[654,937,1761,1786]" class="Insecta" family="Ichneumonidae" genus="Grotea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="196" phylum="Arthropoda" rank="species" species="eburnea">
