<taxonomicName baseAuthorityName="Porter" baseAuthorityYear="1989" box="[297,416,1833,1857]" class="Insecta" family="Ichneumonidae" genus="Grotea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="196" phylum="Arthropoda" rank="species" species="eburnea">
