<subSubSection box="[151,792,1082,1107]" pageId="10" pageNumber="447" type="etymology">
<paragraph blockId="10.[151,1436,1082,1830]" box="[151,792,1082,1107]" pageId="10" pageNumber="447">
<emphasis bold="true" box="[151,286,1082,1107]" pageId="10" pageNumber="447">Etymology.</emphasis>
<taxonomicName box="[292,410,1082,1107]" class="Insecta" family="Cercopidae" genus="Ambonga" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="447" phylum="Arthropoda" rank="species" species="lanceolata">
