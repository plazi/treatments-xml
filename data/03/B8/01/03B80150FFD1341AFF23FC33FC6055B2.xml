<subSubSection box="[151,984,1050,1075]" pageId="14" pageNumber="451" type="etymology">
<paragraph blockId="14.[151,1436,1050,1507]" box="[151,984,1050,1075]" pageId="14" pageNumber="451">
<emphasis bold="true" box="[151,286,1050,1075]" pageId="14" pageNumber="451">Etymology.</emphasis>
<emphasis box="[292,318,1050,1075]" italics="true" pageId="14" pageNumber="451">de</emphasis>
