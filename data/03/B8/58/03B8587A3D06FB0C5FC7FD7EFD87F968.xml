North East 
<collectingCountry box="[492,550,1582,1608]" name="India" pageId="26" pageNumber="27">India</collectingCountry>
(
<collectingRegion box="[565,646,1582,1608]" country="India" name="Assam" pageId="26" pageNumber="27">Assam</collectingRegion>
, 
<collectingRegion box="[659,783,1582,1608]" country="India" name="Meghalaya" pageId="26" pageNumber="27">Meghalaya</collectingRegion>
and 
<collectingRegion box="[841,946,1582,1609]" country="India" name="Mizoram" pageId="26" pageNumber="27">Mizoram</collectingRegion>
) (
<bibRefCitation author="Singh, J. &amp; Singh, N. &amp; Joshi, R." box="[969,1170,1582,1608]" pageId="26" pageNumber="27" pagination="1 - 76" refId="ref58626" refString="Singh, J., Singh, N. &amp; Joshi, R. (2014) A Checklist of subfamily Arctiinae (Erebidae: Noctuoidea: Lepidoptera) from India. Records of Zoological Survey of India, Occ. Paper No. 367, 1 - 76." type="book chapter" year="2014">
Singh 
<emphasis box="[1044,1102,1582,1608]" italics="true" pageId="26" pageNumber="27">et al.</emphasis>
2014
</bibRefCitation>
), South and South East 
<collectingCountry box="[151,218,1618,1644]" name="China" pageId="26" pageNumber="27">China</collectingCountry>
(
<bibRefCitation author="Hampson, G. F." box="[233,410,1618,1644]" pageId="26" pageNumber="27" pagination="1 - 590" refId="ref57367" refString="Hampson, G. F. (1900) Catalogue of the Arctiadae (Nolinae, Lithosianae) in the British Museum. Catalogue of the Lepidoptera Phalaenae in the British Museum, 2, 1 - 590." type="journal article" year="1900">Hampson 1900</bibRefCitation>
), North 
<collectingCountry box="[503,603,1618,1644]" name="Thailand" pageId="26" pageNumber="27">Thailand</collectingCountry>
(
<bibRefCitation author="Cerny, K. &amp; Pinratana, A." box="[618,898,1617,1644]" pageId="26" pageNumber="27" refId="ref56697" refString="Cerny, K. &amp; Pinratana, A. (2009) Moths of Thailand. Vol. 6. Arctiidae. Brothers of Saint Gabriel in Thailand, Bangkok, 283 pp." type="book" year="2009">Černý &amp; Pinratana 2009</bibRefCitation>
, as 
<taxonomicName baseAuthorityName="Rothschild" baseAuthorityYear="1912" box="[941,1067,1618,1645]" class="Insecta" family="Erebidae" genus="Cyana" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="flavalba">
<emphasis box="[941,1067,1618,1645]" italics="true" pageId="26" pageNumber="27">C. flavalba</emphasis>
</taxonomicName>
(part)) and 
<collectingCountry box="[1202,1266,1619,1645]" name="Japan" pageId="26" pageNumber="27">Japan</collectingCountry>
(Ryukyu, 
<collectingRegion country="Japan" name="Okinawa" pageId="26" pageNumber="27">Okinawa</collectingRegion>
and Iriomote islands) (
<bibRefCitation author="Inoue, H." box="[470,602,1653,1680]" pageId="26" pageNumber="27" pagination="241 - 245" refId="ref57636" refString="Inoue, H. (1965) One new genus, two new species and four unrecorded species of the Arctiidae from Japan (Lepidoptera). Kontyu, 33 (2), 241 - 245." type="journal article" year="1965">Inoue 1965</bibRefCitation>
).
</paragraph>
