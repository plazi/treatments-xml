<paragraph blockId="25.[277,1300,1780,1882]" lastBlockId="26.[264,1324,284,1230]" lastPageId="26" lastPageNumber="27" pageId="25" pageNumber="26">
Inner mandibular barbel length 
<number box="[849,896,1820,1843]" pageId="25" pageNumber="26" value="54.2">54.2</number>
<number box="[999,1106,1820,1843]" pageId="25" pageNumber="26" value="66.2" valueMax="78.2" valueMin="54.2">54.2–78.2</number>
