<emphasis bold="true" box="[199,358,798,825]" pageId="41" pageNumber="42">
<typeStatus box="[199,258,799,825]" pageId="41" pageNumber="42">Type</typeStatus>
locality.
</emphasis>
<location LSID="urn:lsid:plazi:treatment:03B887B16E10FFE548F7626C162D92F5:8ECE607C6E1EFFE5490C64271387940D" box="[364,509,799,825]" county="Gulf of Lion." latitude="42.77889" longLatPrecision="19" longitude="3.0558333" municipality="Western Mediterranean Sea" name="Gulf of Lion" pageId="41" pageNumber="42">Gulf of Lion</location>
, 
<collectingMunicipality box="[519,831,799,825]" pageId="41" pageNumber="42">Western Mediterranean Sea</collectingMunicipality>
, 
<collectingCounty box="[842,991,799,825]" pageId="41" pageNumber="42">Gulf of Lion.</collectingCounty>
