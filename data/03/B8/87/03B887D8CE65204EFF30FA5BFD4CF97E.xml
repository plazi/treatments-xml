<number box="[566,686,189,215]" pageId="8" pageNumber="53" value="1.615" valueMax="1.69" valueMin="1.54">1.54–1.69</number>
, 
<number box="[698,753,189,215]" pageId="8" pageNumber="53" value="1.62">1.62</number>
