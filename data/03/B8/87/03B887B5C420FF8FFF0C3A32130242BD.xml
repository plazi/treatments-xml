<collectionCode LSID="urn:lsid:biocol.org:col:32981" box="[459,531,368,392]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="4" pageNumber="573">ANIC</collectionCode>
