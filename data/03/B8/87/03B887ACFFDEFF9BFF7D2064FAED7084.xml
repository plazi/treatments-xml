<collectingCountry box="[745,828,1942,1966]" name="Algeria" pageId="8" pageNumber="9">Algeria</collectingCountry>
, 
<collectingCountry box="[839,905,1943,1967]" name="Egypt" pageId="8" pageNumber="9">Egypt</collectingCountry>
, 
<collectingCountry box="[915,1004,1942,1966]" name="Ukraine" pageId="8" pageNumber="9">Ukraine</collectingCountry>
, southern 
<collectingCountry box="[1113,1187,1942,1966]" name="Russia" pageId="8" pageNumber="9">Russia</collectingCountry>
, 
<collectingCountry box="[1197,1304,1942,1966]" name="Mongolia" pageId="8" pageNumber="9">Mongolia</collectingCountry>
, and 
<collectingCountry box="[1359,1426,1942,1966]" name="China" pageId="8" pageNumber="9">China</collectingCountry>
. 
<emphasis bold="true" box="[201,337,1978,2003]" pageId="8" pageNumber="9">Comments.</emphasis>
Recorded on plants such as 
<emphasis box="[656,752,1979,2003]" italics="true" pageId="8" pageNumber="9">Ephedra</emphasis>
sp., 
<taxonomicName box="[805,914,1979,2003]" class="Magnoliopsida" family="Asteraceae" genus="Artemisia" kingdom="Plantae" order="Asterales" pageId="8" pageNumber="9" phylum="Tracheophyta" rank="genus">
<emphasis box="[805,914,1979,2003]" italics="true" pageId="8" pageNumber="9">Artemisia</emphasis>
</taxonomicName>
sp., and 
<emphasis box="[1014,1104,1979,2003]" italics="true" pageId="8" pageNumber="9">Atriplex</emphasis>
sp. (
<bibRefCitation author="Linnavuori" box="[1158,1352,1978,2002]" pageId="8" pageNumber="9" refString="Linnavuori, R. E. (2007) Studies on the Lygaeidae s. lat. (Heteroptera) of Gilan and the adjacent provinces in northern Iran. Acta Entomologica Musei Nationalis Pragae, 47, 57 - 75." type="journal article" year="2007">
Linnavuori 
<number box="[1291,1352,1978,2002]" pageId="8" pageNumber="9" value="2007.0">2007</number>
</bibRefCitation>
