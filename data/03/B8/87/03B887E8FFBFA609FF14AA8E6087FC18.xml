<taxonomicName authorityName="Grum-Grshimailo" authorityYear="1891" box="[578,661,958,984]" class="Insecta" family="Pieridae" genus="Colias" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="458" phylum="Arthropoda" rank="species" species="lada">
