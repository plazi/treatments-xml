<number box="[151,166,511,536]" pageId="10" pageNumber="69" value="5.0">5</number>
. 
<taxonomicName box="[179,356,511,536]" class="Insecta" family="Cicadellidae" genus="Musbrnoia" kingdom="Animalia" order="Hemiptera" pageId="10" pageNumber="69" phylum="Arthropoda" rank="species" species="lata" status="sp. nov.">
