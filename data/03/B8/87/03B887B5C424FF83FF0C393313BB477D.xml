<taxonomicName class="Magnoliopsida" family="Lamiaceae" genus="Vitex" kingdom="Plantae" order="Lamiales" pageId="7" pageNumber="576" phylum="Tracheophyta" rank="species" species="acuminata">
<emphasis italics="true" pageId="7" pageNumber="576">Vitex acuminata</emphasis>
</taxonomicName>
, 
<date box="[283,410,1980,2005]" pageId="7" pageNumber="576" value="1996-12-22">
<number box="[283,313,1980,2005]" pageId="7" pageNumber="576" value="22.0">22</number>
.xii. 
<number box="[351,410,1980,2005]" pageId="7" pageNumber="576" value="1996.0">1996</number>
</date>
(LAM 
<number box="[491,549,1980,2004]" pageId="7" pageNumber="576" value="3079.0">3079</number>
), in 
<collectionCode LSID="urn:lsid:biocol.org:col:32981" box="[597,669,1980,2004]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:32981" name="Australian National Insect Collection" pageId="7" pageNumber="576">ANIC</collectionCode>
