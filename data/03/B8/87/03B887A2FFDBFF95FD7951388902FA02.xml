<collectingCountry box="[685,760,1523,1544]" name="Panama" pageId="3" pageNumber="78">Panama</collectingCountry>
, 
<collectingCountry box="[769,854,1523,1544]" name="Paraguay" pageId="3" pageNumber="78">Paraguay</collectingCountry>
, 
<collectingCountry box="[863,952,1523,1544]" name="Suriname" pageId="3" pageNumber="78">Suriname</collectingCountry>
