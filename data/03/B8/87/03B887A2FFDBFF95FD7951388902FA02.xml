<collectingCountry box="[754,846,1469,1490]" name="Colombia" pageId="3" pageNumber="78">Colombia</collectingCountry>
, 
<collectingCountry box="[858,963,1469,1490]" name="Costa Rica" pageId="3" pageNumber="78">Costa Rica</collectingCountry>
, 
<collectingCountry box="[974,1025,1469,1490]" name="Cuba" pageId="3" pageNumber="78">Cuba</collectingCountry>
, 
<collectingCountry box="[1037,1115,1469,1490]" name="Ecuador" pageId="3" pageNumber="78">Ecuador</collectingCountry>
, 
<collectingCountry name="Guadeloupe" pageId="3" pageNumber="78">Guadalupe</collectingCountry>
, 
<collectingCountry box="[768,869,1496,1517]" name="Guatemala" pageId="3" pageNumber="78">Guatemala</collectingCountry>
, 
<collectingCountry box="[887,962,1496,1517]" name="Jamaica" pageId="3" pageNumber="78">Jamaica</collectingCountry>
