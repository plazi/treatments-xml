<collectingCountry box="[1099,1168,1443,1464]" name="Bolivia" pageId="3" pageNumber="78">Bolivia</collectingCountry>
, 
<collectingCountry box="[685,742,1469,1490]" name="Brazil" pageId="3" pageNumber="78">Brazil</collectingCountry>
