<taxonomicName baseAuthorityName="Jaderholm" baseAuthorityYear="1917" box="[151,460,1658,1683]" class="Hydrozoa" family="Sertulariidae" genus="Symplectoscyphus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="32" pageNumber="33" phylum="Cnidaria" rank="species" species="curvatus">
