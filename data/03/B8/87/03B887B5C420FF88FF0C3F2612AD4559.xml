</taxonomicNameLabel>
</keyLead>
</paragraph>
</keyStep>
<keyStep pageId="3" pageNumber="572">
<paragraph blockId="3.[151,1436,942,1580]" pageId="3" pageNumber="572">
<keyLead pageId="3" pageNumber="572">
<number box="[151,165,1353,1374]" pageId="3" pageNumber="572" value="6.0">6</number>
