<number box="[1371,1395,942,963]" pageId="3" pageNumber="572" value="15.0">15</number>
</figureCitation>
)............................................................................................ 
<taxonomicName box="[1310,1382,972,993]" class="Insecta" family="Thripidae" genus="Dendrothrips" kingdom="Animalia" order="Thysanoptera" pageId="3" pageNumber="572" phylum="Arthropoda" rank="species" species="viticola" status="sp.n.">
