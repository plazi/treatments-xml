<taxonomicName authorityName="Ritchie" authorityYear="1907" box="[1210,1391,831,856]" class="Hydrozoa" family="Haleciidae" genus="Halecium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="16" pageNumber="17" phylum="Cnidaria" rank="species" species="interpolatum">H. interpolatum</taxonomicName>
