<taxonomicName authorityName="Hickson &amp; Gravely" authorityYear="1907" box="[327,463,868,893]" class="Hydrozoa" family="Sertulariidae" genus="Thuiaria" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="16" pageNumber="17" phylum="Cnidaria" rank="species" species="longstaffi">
