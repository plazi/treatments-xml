<number box="[151,165,239,260]" pageId="2" pageNumber="43" value="3.0">3</number>
. Vein R 
<number box="[291,298,253,265]" pageId="2" pageNumber="43" value="2.0">2</number>
+ 
<number box="[305,311,253,265]" pageId="2" pageNumber="43" value="3.0">3</number>
