<subSubSection box="[199,1015,295,321]" pageId="9" pageNumber="46" type="etymology">
<paragraph blockId="9.[151,1437,151,645]" box="[199,1015,295,321]" pageId="9" pageNumber="46">
<emphasis bold="true" box="[199,338,295,321]" pageId="9" pageNumber="46">Etymology:</emphasis>
