and 
<quantity box="[950,1063,1807,1832]" metricMagnitude="-2" metricUnit="m" metricValue="1.397" metricValueMax="1.524" metricValueMin="1.27" pageId="20" pageNumber="521" unit="in" value="0.55" valueMax="0.6" valueMin="0.5">0.5–0.6 in</quantity>
male 
<taxonomicName box="[1133,1248,1808,1832]" class="Insecta" family="Ichneumonidae" genus="Apophua" kingdom="Animalia" order="Hymenoptera" pageId="20" pageNumber="521" phylum="Arthropoda" rank="species" species="honmai">
<emphasis box="[1133,1150,1808,1831]" italics="true" pageId="20" pageNumber="521">A</emphasis>
. 
<emphasis box="[1164,1248,1808,1832]" italics="true" pageId="20" pageNumber="521">honmai</emphasis>
