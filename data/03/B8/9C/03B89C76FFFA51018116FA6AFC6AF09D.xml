(level 90), 
<location LSID="urn:lsid:plazi:treatment:03B89C76FFFA51018116FA6AFC6AF09D:8ECE7BBBFFFA511F86ADF9A1FA5DF332" box="[1236,1417,1544,1566]" country="Italy" county="Pizzaferri Collection" municipality="Emilia" name="Castell'Arquato" pageId="14" pageNumber="31">Castell’Arquato</location>
