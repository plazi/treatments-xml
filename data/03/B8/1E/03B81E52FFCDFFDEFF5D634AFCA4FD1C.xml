<taxonomicName authority="Prokofiev &amp; Klepadlo, 2019" authorityName="Prokofiev &amp; Klepadlo" authorityYear="2019" box="[280,563,1788,1810]" class="Actinopterygii" family="Stomiidae" genus="Photonectes" kingdom="Animalia" order="Stomiiformes" pageId="4" pageNumber="274" phylum="Chordata" rank="species" species="sphaerolampas" status="new species">
