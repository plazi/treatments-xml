<taxonomicName authorityName="André" authorityYear="1892" box="[857,1122,798,820]" class="Insecta" family="Formicidae" genus="Bothroponera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="126" pageNumber="127" phylum="Arthropoda" rank="species" species="gabonensis">
