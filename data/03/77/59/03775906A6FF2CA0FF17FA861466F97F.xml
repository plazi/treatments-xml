<taxonomicName box="[636,758,1514,1535]" class="Insecta" family="Formicidae" genus="Promyopias" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="128" pageNumber="129" phylum="Arthropoda" rank="genus">
