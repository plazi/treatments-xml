<taxonomicName authority="Fabricius, 1798: 280" authorityName="Fabricius. The" authorityPageNumber="280" authorityYear="1798" box="[772,1155,1374,1396]" class="Insecta" family="Formicidae" genus="Formica" kingdom="Animalia" order="Hymenoptera" pageId="123" pageNumber="124" phylum="Arthropoda" rank="species" species="tarsata">
