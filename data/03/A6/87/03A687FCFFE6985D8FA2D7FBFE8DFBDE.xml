<taxonomicName authority="(Holm, 1890)" authorityName="Holm" authorityYear="1890" baseAuthorityName="Holm" baseAuthorityYear="1890" box="[527,884,1839,1861]" class="Graptolithina" family="Retiolitidae" genus="Gothograptus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Graptoloidea" pageId="15" pageNumber="450" phylum="Hemichordata" rank="species" species="nassa">
