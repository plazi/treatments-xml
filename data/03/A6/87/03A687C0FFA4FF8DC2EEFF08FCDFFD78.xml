<quantity box="[916,970,400,420]" metricMagnitude="2" metricUnit="m" metricValue="4.3" pageId="75" pageNumber="230" unit="m" value="430.0">430m</quantity>
, 8244/1, 
<date box="[181,286,427,447]" pageId="75" pageNumber="230" value="2003-01-04">01.04.2003</date>
