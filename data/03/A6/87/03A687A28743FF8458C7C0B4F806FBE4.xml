</taxonomicName>
and 
<taxonomicName authority="Desf." authorityName="Desf." box="[984,1214,1004,1030]" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" kingdom="Plantae" order="Fabales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="species" species="langsdorffii">
<emphasis box="[984,1147,1004,1030]" italics="true" pageId="23" pageNumber="24">C. langsdorffii</emphasis>
Desf.
</taxonomicName>
(Leguminosae, 
<taxonomicName class="Magnoliopsida" family="Fabaceae" kingdom="Plantae" order="Fabales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="subFamily" subFamily="Detarioideae">Detarioideae</taxonomicName>
