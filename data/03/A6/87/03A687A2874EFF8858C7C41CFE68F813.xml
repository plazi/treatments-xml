<taxonomicName authorityName="White &amp; Hodkinson" authorityYear="1980" box="[873,1098,1843,1869]" class="Insecta" family="Psyllidae" genus="Acizzia" kingdom="Animalia" order="Hemiptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="ceplaciensis">
