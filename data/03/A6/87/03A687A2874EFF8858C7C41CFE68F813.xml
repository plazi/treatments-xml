<collectingCountry box="[360,428,1843,1869]" name="Brazil" pageId="27" pageNumber="28">Brazil</collectingCountry>
(
<collectingRegion box="[443,511,1843,1870]" country="Brazil" name="Bahia" pageId="27" pageNumber="28">Bahia</collectingRegion>
