(
<collectingCountry box="[439,527,978,1002]" name="Taiwan" pageId="28" pageNumber="29">Taiwan</collectingCountry>
), 
<collectingCountry box="[546,606,978,1002]" name="India" pageId="28" pageNumber="29">India</collectingCountry>
, N.E. Himalaya, 
<collectingCountry box="[799,905,978,1002]" name="Malaysia" pageId="28" pageNumber="29">Malaysia</collectingCountry>
, 
<collectingCountry box="[916,1029,978,1002]" name="Indonesia" pageId="28" pageNumber="29">Indonesia</collectingCountry>
. The species was reported for 
<collectingCountry box="[1369,1436,978,1002]" name="China" pageId="28" pageNumber="29">China</collectingCountry>
by 
<bibRefCitation author="Heppner" box="[186,466,1014,1038]" pageId="28" pageNumber="29" refString="Heppner, J. B. &amp; Inoue, H. (1992) Lepidoptera of Taiwan. Vol. 1, Part 2, Checklist. Scientific Publishers, Gainesville-Washington-Hamburg-Lima-Taipei-Tokyo, xlix + 276 pp." type="book" year="1992">
Heppner &amp; Inoue (
<number box="[397,458,1014,1038]" pageId="28" pageNumber="29" value="1992.0">1992</number>
