<collectingCountry box="[734,910,523,545]" name="Czech Republic" pageId="7" pageNumber="738">Czech Republic</collectingCountry>
, 
<collectingCountry box="[933,1003,523,545]" name="Poland" pageId="7" pageNumber="738">Poland</collectingCountry>
and 
<collectingCountry box="[151,272,553,575]" name="Switzerland" pageId="7" pageNumber="738">Switzerland</collectingCountry>
