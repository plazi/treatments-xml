sp. (
<taxonomicName box="[731,855,1052,1076]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecaceae" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="order">Arecaceae</taxonomicName>
); 
