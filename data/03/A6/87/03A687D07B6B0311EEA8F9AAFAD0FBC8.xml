</emphasis>
(
<taxonomicName box="[1046,1170,1016,1040]" class="Magnoliopsida" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecaceae" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="order">Arecaceae</taxonomicName>
); 
<taxonomicName box="[1189,1252,1017,1040]" class="Magnoliopsida" family="Moraceae" genus="Ficus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis box="[1189,1252,1017,1040]" italics="true" pageId="23" pageNumber="24">Ficus</emphasis>
</taxonomicName>
sp. (
<taxonomicName authorityName="" authorityYear="1835" box="[1308,1425,1016,1040]" class="Magnoliopsida" family="Moraceae" higherTaxonomySource="GBIF" kingdom="Plantae" order="Rosales" pageId="23" pageNumber="24" phylum="Magnoliopsida" rank="family">Moraceae</taxonomicName>
). 
<emphasis bold="true" box="[151,446,1052,1077]" pageId="23" pageNumber="24">Unconfirmed host plants</emphasis>
: 
<taxonomicName box="[461,561,1052,1077]" class="Magnoliopsida" family="Arecaceae" genus="Calamus" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis box="[461,561,1052,1077]" italics="true" pageId="23" pageNumber="24">Calamus</emphasis>
</taxonomicName>
sp., 
<taxonomicName authorityName="Adanson" authorityYear="1763" box="[613,677,1052,1077]" class="Magnoliopsida" family="Arecaceae" genus="Sabal" higherTaxonomySource="GBIF" kingdom="Plantae" order="Arecales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="genus">
<emphasis box="[613,677,1052,1077]" italics="true" pageId="23" pageNumber="24">Sabal</emphasis>
