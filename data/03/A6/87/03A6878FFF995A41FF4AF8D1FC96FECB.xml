<quantity box="[673,765,232,258]" metricMagnitude="1" metricUnit="m" metricValue="1.05" metricValueMax="1.2" metricValueMin="0.9" pageId="26" pageNumber="27" unit="m" value="10.5" valueMax="12.0" valueMin="9.0">
