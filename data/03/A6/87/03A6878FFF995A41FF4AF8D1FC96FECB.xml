<emphasis bold="true" box="[1380,1429,1939,1963]" pageId="25" pageNumber="26">mx1</emphasis>
, maxilla 1; 
<emphasis bold="true" box="[265,315,1973,1997]" pageId="25" pageNumber="26">mxp</emphasis>
