<quantity metricMagnitude="-3" metricUnit="m" metricValue="3.75" pageId="87" pageNumber="88" unit="mm" value="3.75">3.75 mm</quantity>
, width 
<quantity box="[279,382,1255,1280]" metricMagnitude="-3" metricUnit="m" metricValue="2.65" pageId="87" pageNumber="88" unit="mm" value="2.65">2.65 mm</quantity>
