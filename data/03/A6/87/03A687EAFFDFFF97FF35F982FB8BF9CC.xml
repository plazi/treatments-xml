<taxonomicName authority="GUO ET AL." authorityName="GUO ET AL." authorityYear="2022" box="[296,610,1628,1653]" class="Arachnida" family="Lagonomegopidae" genus="Scopomegops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="402" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[296,475,1628,1652]" italics="true" pageId="3" pageNumber="402">SCOPOMEGOPS</emphasis>
GUO 
<emphasis box="[540,604,1632,1652]" italics="true" pageId="3" pageNumber="402">ET AL</emphasis>
.
</taxonomicName>
<taxonomicNameLabel box="[617,733,1628,1653]" pageId="3" pageNumber="402" rank="genus">
