<taxonomicName baseAuthorityName="Green" baseAuthorityYear="1896" box="[1026,1195,1485,1508]" class="Insecta" family="Coccidae" genus="Paralecanium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="geometricum" status="syn. n.">
<emphasis box="[1026,1195,1485,1508]" italics="true" pageId="98" pageNumber="99">P. geometricum</emphasis>
</taxonomicName>
, 
<taxonomicNameLabel box="[1213,1291,1484,1509]" pageId="98" pageNumber="99">
<emphasis bold="true" box="[1213,1283,1484,1509]" pageId="98" pageNumber="99">syn. n</emphasis>
.
</taxonomicNameLabel>
</paragraph>
<paragraph blockId="98.[151,1437,151,1653]" pageId="98" pageNumber="99">
