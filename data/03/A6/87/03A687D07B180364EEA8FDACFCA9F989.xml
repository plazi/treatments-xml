<taxonomicName baseAuthorityName="Green" baseAuthorityYear="1896" box="[352,523,1521,1544]" class="Insecta" family="Coccidae" genus="Paralecanium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="98" pageNumber="99" phylum="Arthropoda" rank="species" species="geometricum">
