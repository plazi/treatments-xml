<paragraph blockId="64.[151,1437,151,2037]" pageId="64" pageNumber="65">
<materialsCitation ID-GBIF-Occurrence="1872522444" country="Malaysia" location="Sarawak" pageId="64" pageNumber="65" specimenCount="1" typeStatus="holotype">
