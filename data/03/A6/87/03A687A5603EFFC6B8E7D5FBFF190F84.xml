<paragraph blockId="9.[151,482,151,178]" box="[151,482,151,178]" pageId="9" pageNumber="558">
<heading bold="true" box="[151,482,151,178]" fontSize="11" level="1" pageId="9" pageNumber="558" reason="1">
<taxonomicName box="[151,482,151,178]" class="Malacostraca" family="Parabathynellidae" genus="Eobathynella" higherTaxonomySource="GBIF" isUncertain="true" kingdom="Animalia" order="Bathynellacea" pageId="9" pageNumber="558" phylum="Arthropoda" rank="species" species="yeojuensis">
<emphasis bold="true" box="[151,482,151,178]" pageId="9" pageNumber="558">
<emphasis bold="true" box="[151,312,151,178]" italics="true" pageId="9" pageNumber="558">Eobathynella</emphasis>
cf. 
<emphasis bold="true" box="[357,482,151,177]" italics="true" pageId="9" pageNumber="558">yeojuensis</emphasis>
</emphasis>
</taxonomicName>
</heading>
</paragraph>
