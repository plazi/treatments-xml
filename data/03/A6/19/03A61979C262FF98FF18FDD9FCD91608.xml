<quantity box="[738,907,1533,1559]" metricMagnitude="2" metricUnit="m" metricValue="8.59" metricValueMax="11.8" metricValueMin="5.38" pageId="65" pageNumber="66" unit="m" value="859.0" valueMax="1180.0" valueMin="538.0">538 to 1180 m</quantity>
