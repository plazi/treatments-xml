<quantity box="[900,986,512,532]" metricMagnitude="4" metricUnit="m" metricValue="4.0" metricValueMax="5.0" metricValueMin="3.0" pageId="4" pageNumber="1157" unit="km" value="40.0" valueMax="50.0" valueMin="30.0">30-50 km</quantity>
