, 
<collectingCountry box="[1220,1322,1805,1830]" name="Thailand" pageId="1" pageNumber="566">Thailand</collectingCountry>
, 
<collectingCountry box="[1334,1432,1805,1830]" name="Vietnam" pageId="1" pageNumber="566">Vietnam</collectingCountry>
, 
<collectingCountry box="[151,280,1841,1866]" name="Philippines" pageId="1" pageNumber="566">Philippines</collectingCountry>
