, 
<collectingCountry box="[923,1037,1805,1830]" name="Sri Lanka" pageId="1" pageNumber="566">Sri Lanka</collectingCountry>
, 
<collectingCountry box="[1048,1127,1806,1829]" name="Myanmar" pageId="1" pageNumber="566">Burma</collectingCountry>
, 
