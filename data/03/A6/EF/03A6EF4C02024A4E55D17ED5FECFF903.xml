<collectingCountry box="[498,588,1484,1508]" name="Guyana" pageId="5" pageNumber="6">Guyana</collectingCountry>
, 
<collectingCountry box="[600,671,1484,1508]" name="Brazil" pageId="5" pageNumber="6">Brazil</collectingCountry>
, 
<collectingCountry box="[682,736,1484,1507]" name="Peru" pageId="5" pageNumber="6">Peru</collectingCountry>
