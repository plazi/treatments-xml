<subSubSection pageId="19" pageNumber="20" type="nomenclature">
<paragraph blockId="19.[264,545,483,509]" box="[264,545,483,509]" pageId="19" pageNumber="20">
<heading bold="true" box="[264,545,483,509]" fontSize="11" level="1" pageId="19" pageNumber="20" reason="1">
<emphasis bold="true" box="[264,545,483,509]" pageId="19" pageNumber="20">
Family 
<taxonomicName box="[359,545,483,509]" class="Insecta" family="Hippoboscidae" kingdom="Animalia" order="Diptera" pageId="19" pageNumber="20" phylum="Arthropoda" rank="family">Hippoboscidae</taxonomicName>
</emphasis>
