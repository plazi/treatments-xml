(
<emphasis box="[553,746,1851,1873]" italics="true" pageId="4" pageNumber="247">Longipachylaelaps</emphasis>
) 
<emphasis box="[778,860,1852,1873]" italics="true" pageId="4" pageNumber="247">vicarius</emphasis>
</taxonomicName>
., female. 10. Spermathecal ducts. 
<taxonomicName authority="Şahin &amp; Özbek, 2018" authorityName="Şahin &amp; Özbek" authorityYear="2018" class="Arachnida" family="Pachylaelapidae" genus="Pachylaelaps" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mesostigmata" pageId="4" pageNumber="247" phylum="Arthropoda" rank="subGenus" species="bayburtensis" status="sp. nov." subGenus="Longipachylaelaps">
<emphasis box="[1300,1436,1851,1873]" italics="true" pageId="4" pageNumber="247">Pachylaelaps</emphasis>
(
<emphasis box="[159,352,1882,1904]" italics="true" pageId="4" pageNumber="247">Longipachylaelaps</emphasis>
) 
<emphasis box="[367,496,1882,1904]" italics="true" pageId="4" pageNumber="247">bayburtensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[502,583,1882,1903]" pageId="4" pageNumber="247">
