<subSubSection pageId="21" pageNumber="22" type="reference_group">
<paragraph blockId="21.[264,1323,604,870]" pageId="21" pageNumber="22">
<taxonomicName box="[312,528,764,789]" class="Insecta" family="Anthomyiidae" genus="Musca" kingdom="Animalia" order="Diptera" pageId="21" pageNumber="22" phylum="Arthropoda" rank="species" species="autumnalis">
