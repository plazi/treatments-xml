<taxonomicName authorityName="Suprapedicelar" box="[1036,1152,1830,1857]" class="Arachnida" family="Arachnida (awaiting allocation)" genus="Cinetomorpha" higherTaxonomySource="GBIF" kingdom="Animalia" order="Arachnida (awaiting allocation)" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="lorenzo">
