<subSubSection box="[151,1122,654,679]" pageId="2" pageNumber="43" type="etymology">
<paragraph blockId="2.[151,1437,654,1762]" box="[151,1122,654,679]" pageId="2" pageNumber="43">
<emphasis bold="true" box="[151,286,655,679]" pageId="2" pageNumber="43">Etymology.</emphasis>
