<subSubSection lastPageId="5" lastPageNumber="46" pageId="3" pageNumber="44" type="diagnosis">
<paragraph blockId="3.[151,1436,151,212]" lastBlockId="4.[151,1436,1510,1593]" lastPageId="4" lastPageNumber="45" pageId="3" pageNumber="44">
<emphasis bold="true" box="[197,319,151,176]" pageId="3" pageNumber="44">Diagnosis.</emphasis>
