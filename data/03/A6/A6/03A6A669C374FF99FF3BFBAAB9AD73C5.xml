<quantity box="[211,290,1432,1454]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" metricValueMax="6.0" metricValueMin="4.0" pageId="3" pageNumber="10" unit="mm" value="5.0" valueMax="6.0" valueMin="4.0">4–6 mm</quantity>
