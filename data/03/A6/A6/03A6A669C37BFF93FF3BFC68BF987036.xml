<caption ID-DOI="http://doi.org/10.5281/zenodo.6226925" ID-Zenodo-Dep="6226925" box="[136,1436,1490,1513]" httpUri="https://zenodo.org/record/6226925/files/figure.png" pageId="13" pageNumber="20" startId="13.[136,229,1490,1512]" targetBox="[157,1436,569,1466]" targetPageId="13">
<paragraph blockId="13.[136,1436,1490,1513]" box="[136,1436,1490,1513]" pageId="13" pageNumber="20">
