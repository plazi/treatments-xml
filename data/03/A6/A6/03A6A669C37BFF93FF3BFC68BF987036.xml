<taxonomicName authorityName="Stepanek &amp; Kirschner" authorityYear="2022" box="[259,484,1491,1512]" class="Magnoliopsida" family="Asteraceae" genus="Taraxacum" kingdom="Plantae" order="Asterales" pageId="13" pageNumber="20" phylum="Tracheophyta" rank="species" species="velebiticum">
