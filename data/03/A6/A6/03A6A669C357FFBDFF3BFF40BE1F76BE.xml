, ca. 
<quantity box="[915,1066,1778,1805]" metricMagnitude="3" metricUnit="m" metricValue="2.15" metricValueMax="2.2" metricValueMin="2.1" pageId="32" pageNumber="39" unit="m" value="2150.0" valueMax="2200.0" valueMin="2100.0">
<elevation box="[915,1066,1778,1805]" metricMagnitude="3" metricUnit="m" metricValue="2.15" metricValueMax="2.2" metricValueMin="2.1" pageId="32" pageNumber="39" unit="m" value="2150.0" valueMax="2200.0" valueMin="2100.0">2100-2200 m</elevation>
