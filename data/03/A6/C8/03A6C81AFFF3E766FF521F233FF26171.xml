<taxonomicName authority="Darkfin" authorityName="Darkfin" box="[1105,1379,562,584]" class="Actinopterygii" family="Kyphosidae" genus="Kyphosus" kingdom="Animalia" order="Perciformes" pageId="8" pageNumber="9" phylum="Chordata" rank="species" species="bigibbus">
