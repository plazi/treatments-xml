<tr box="[147,1414,273,295]" gridrow="1" pageId="5" pageNumber="246">
<th box="[147,381,273,295]" gridcol="0" gridrow="1" pageId="5" pageNumber="246">Stem</th>
<td box="[462,913,273,295]" gridcol="1" gridrow="1" pageId="5" pageNumber="246">densely hispid</td>
<td box="[954,1414,273,295]" gridcol="2" gridrow="1" pageId="5" pageNumber="246">sparsely pubescent</td>
</tr>
<tr box="[147,1414,327,385]" gridrow="2" pageId="5" pageNumber="246">
<th box="[147,381,327,385]" gridcol="0" gridrow="2" pageId="5" pageNumber="246">Leaf blade</th>
<td box="[462,913,327,385]" gridcol="1" gridrow="2" pageId="5" pageNumber="246">densely hispid, cordate to ovate, length usually equal to width</td>
<td box="[954,1414,327,385]" gridcol="2" gridrow="2" pageId="5" pageNumber="246">sparsely pubescent, narrowly cordate to ovate, length usually longer than width</td>
</tr>
<tr box="[147,1414,417,439]" gridrow="3" pageId="5" pageNumber="246">
<th box="[147,381,417,439]" gridcol="0" gridrow="3" pageId="5" pageNumber="246">Inflorescence</th>
<td box="[462,913,417,439]" gridcol="1" gridrow="3" pageId="5" pageNumber="246">scorpioid cymes</td>
<td box="[954,1414,417,439]" gridcol="2" gridrow="3" pageId="5" pageNumber="246">polychasia</td>
</tr>
<tr box="[147,1414,472,530]" gridrow="4" pageId="5" pageNumber="246">
<th box="[147,381,472,530]" gridcol="0" gridrow="4" pageId="5" pageNumber="246">Bracts</th>
<td box="[462,913,472,530]" gridcol="1" gridrow="4" pageId="5" pageNumber="246">small or absent</td>
<td box="[954,1414,472,530]" gridcol="2" gridrow="4" pageId="5" pageNumber="246">small, caducous, or large to 1–8×0.5–4 cm, usually persistent</td>
</tr>
<tr box="[147,1414,562,620]" gridrow="5" pageId="5" pageNumber="246">
<th box="[147,381,562,620]" gridcol="0" gridrow="5" pageId="5" pageNumber="246">Pedicel</th>
<td box="[462,913,562,620]" gridcol="1" gridrow="5" pageId="5" pageNumber="246">0.9–2.4 mm long, wing on angles, the width of wing 2–3 mm</td>
<td box="[954,1414,562,620]" gridcol="2" gridrow="5" pageId="5" pageNumber="246">0.9–1.9 mm long, carina on angles, the width of carina 0.2–0.5 mm</td>
</tr>
<tr box="[147,1414,653,711]" gridrow="6" pageId="5" pageNumber="246">
<th box="[147,381,653,711]" gridcol="0" gridrow="6" pageId="5" pageNumber="246">Anthers of long stamens</th>
<td box="[462,913,653,711]" gridcol="1" gridrow="6" pageId="5" pageNumber="246">decurrent connective without spurs abaxially</td>
<td box="[954,1414,653,711]" gridcol="2" gridrow="6" pageId="5" pageNumber="246">decurrent connective with a white or pink spur abaxially</td>
</tr>
<tr box="[147,1414,743,765]" gridrow="7" pageId="5" pageNumber="246">
<th box="[147,381,743,765]" gridcol="0" gridrow="7" pageId="5" pageNumber="246">Hypanthium</th>
<td box="[462,913,743,765]" gridcol="1" gridrow="7" pageId="5" pageNumber="246">wings on angles, 2–3 mm wide</td>
<td box="[954,1414,743,765]" gridcol="2" gridrow="7" pageId="5" pageNumber="246">carinas on angles, 0.2–0.5 mm wide</td>
</tr>
</table>
</paragraph>
