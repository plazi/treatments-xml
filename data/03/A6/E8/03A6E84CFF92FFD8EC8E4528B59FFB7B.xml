<taxonomicName authority="Hampson, 1898" authorityName="Hampson" authorityYear="1898" class="Insecta" family="Arctiidae" genus="Cercopimorpha" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="513" phylum="Arthropoda" rank="species" species="meterythra">
