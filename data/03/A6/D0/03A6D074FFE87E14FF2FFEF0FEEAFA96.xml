<subSubSection pageId="7" pageNumber="700" type="materials_examined">
<paragraph blockId="7.[112,647,368,445]" pageId="7" pageNumber="700">
<materialsCitation collectionCode="SPECIES" pageId="7" pageNumber="700" specimenCount="1" typeStatus="holotype">
