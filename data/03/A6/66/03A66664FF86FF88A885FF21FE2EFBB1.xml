<taxonomicName authority="(Say)" baseAuthorityName="Say" baseAuthorityYear="1835" box="[980,1172,1390,1411]" class="Insecta" family="Zopheridae" genus="Verodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="688" phylum="Arthropoda" rank="species" species="inaequalis">
