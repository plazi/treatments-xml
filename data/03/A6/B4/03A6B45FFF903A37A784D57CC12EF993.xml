<subSubSection pageId="5" pageNumber="181" type="etymology">
<paragraph blockId="5.[151,1436,153,215]" pageId="5" pageNumber="181">
<emphasis bold="true" box="[199,334,153,179]" pageId="5" pageNumber="181">Etymology.</emphasis>
