. 
<taxonomicName authority="MULSANT &amp; REY 1880" authorityName="MULSANT &amp; REY" authorityYear="1880" box="[656,1009,490,512]" class="Insecta" family="Staphylinidae" genus="Lesteva" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="995" phylum="Arthropoda" rank="species" species="omissa">
