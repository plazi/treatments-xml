<emphasis box="[1037,1094,1372,1397]" italics="true" pageId="15" pageNumber="16">Saka</emphasis>
era is 
<number box="[1177,1206,1372,1398]" pageId="15" pageNumber="16" value="20.0">20</number>
th 
<emphasis italics="true" pageId="15" pageNumber="16">Va is h akha</emphasis>
, 
<number box="[321,380,1412,1438]" pageId="15" pageNumber="16" value="1924.0">1924</number>
