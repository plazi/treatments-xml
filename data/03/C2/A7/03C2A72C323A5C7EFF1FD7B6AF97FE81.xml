<taxonomicName baseAuthorityName="Hendel" baseAuthorityYear="1915" box="[669,807,258,282]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="34" phylum="Arthropoda" rank="species" species="latifrons">
