<taxonomicName baseAuthorityName="Hendel" baseAuthorityYear="1915" box="[634,877,226,250]" class="Insecta" family="Tephritidae" genus="Bactrocera" kingdom="Animalia" order="Diptera" pageId="35" pageNumber="34" phylum="Arthropoda" rank="species" species="latifrons">
