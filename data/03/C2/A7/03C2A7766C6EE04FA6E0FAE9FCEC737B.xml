<taxonomicName box="[285,337,1985,2006]" class="Malacostraca" family="Aristiidae" genus="Boca" higherTaxonomySource="CoL" kingdom="Animalia" order="Amphipoda" pageId="4" pageNumber="433" phylum="Arthropoda" rank="genus">Boca</taxonomicName>
<taxonomicName box="[345,400,1985,2006]" pageId="4" pageNumber="433" rank="species" species="gurui" status="sp. nov.">gurui</taxonomicName>
