<accessionNumber box="[693,768,2004,2028]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/A33507" pageId="11" pageNumber="652">A33507</accessionNumber>
, 
<accessionNumber box="[813,892,217,241]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/B13448" pageId="11" pageNumber="652">B13448</accessionNumber>
, 
<accessionNumber box="[908,988,217,241]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/B13450" pageId="11" pageNumber="652">B13450</accessionNumber>
