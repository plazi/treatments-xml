<quantity box="[400,497,190,216]" metricMagnitude="-3" metricUnit="m" metricValue="3.0" metricValueMax="4.0" metricValueMin="2.0" pageId="11" pageNumber="12" unit="mm" value="3.0" valueMax="4.0" valueMin="2.0">2−4 mm</quantity>
