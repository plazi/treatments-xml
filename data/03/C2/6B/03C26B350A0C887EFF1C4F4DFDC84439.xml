<quantity box="[834,931,190,216]" metricMagnitude="-3" metricUnit="m" metricValue="7.0" metricValueMax="8.0" metricValueMin="6.0" pageId="11" pageNumber="12" unit="mm" value="7.0" valueMax="8.0" valueMin="6.0">6−8 mm</quantity>
long, 
<quantity box="[1005,1102,190,216]" metricMagnitude="-3" metricUnit="m" metricValue="5.0" metricValueMax="6.0" metricValueMin="4.0" pageId="11" pageNumber="12" unit="mm" value="5.0" valueMax="6.0" valueMin="4.0">4−6 mm</quantity>
wide. Petals 
<quantity box="[1257,1370,190,216]" metricMagnitude="-2" metricUnit="m" metricValue="1.45" metricValueMax="2.2" metricValueMin="0.7" pageId="11" pageNumber="12" unit="mm" value="14.5" valueMax="22.0" valueMin="7.0">7−22 mm</quantity>
long, 
<quantity box="[151,265,229,255]" metricMagnitude="-3" metricUnit="m" metricValue="8.0" metricValueMax="12.0" metricValueMin="4.0" pageId="11" pageNumber="12" unit="mm" value="8.0" valueMax="12.0" valueMin="4.0">4−12 mm</quantity>
