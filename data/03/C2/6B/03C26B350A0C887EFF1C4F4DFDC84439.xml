<quantity box="[974,1064,229,255]" metricMagnitude="-3" metricUnit="m" metricValue="6.5" pageId="11" pageNumber="12" unit="mm" value="6.5">6.5 mm</quantity>
long, and filament 
<quantity box="[1299,1435,229,255]" metricMagnitude="-3" metricUnit="m" metricValue="6.25" metricValueMax="11.0" metricValueMin="1.5" pageId="11" pageNumber="12" unit="mm" value="6.25" valueMax="11.0" valueMin="1.5">1.5−11 mm</quantity>
