<taxonomicName box="[447,619,225,250]" class="Insecta" family="Enicocephalidae" genus="Enicocephalus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="22" phylum="Arthropoda" rank="genus">
