<subSubSection box="[235,1007,1193,1250]" pageId="1" pageNumber="626" type="etymology">
<paragraph box="[235,358,1193,1218]" pageId="1" pageNumber="626">
<heading box="[235,358,1193,1218]" fontSize="10" level="4" pageId="1" pageNumber="626" reason="1">etymology</heading>
</paragraph>
<paragraph box="[235,1007,1226,1250]" pageId="1" pageNumber="626">
