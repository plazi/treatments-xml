<emphasis bold="true" box="[1193,1436,1100,1121]" pageId="1" pageNumber="116">
<taxonomicName authority="Han &amp; Achterberg &amp; Chen, 2021" authorityName="Han &amp; Achterberg &amp; Chen" authorityYear="2021" box="[1193,1354,1100,1121]" class="Insecta" family="Ichneumonidae" genus="Sinophorus" kingdom="Animalia" order="Hymenoptera" pageId="1" pageNumber="116" phylum="Arthropoda" rank="species" species="neimengensis" status="sp. nov.">
