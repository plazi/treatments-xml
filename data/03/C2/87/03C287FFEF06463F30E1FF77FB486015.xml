<collectingCountry box="[430,658,1440,1467]" name="Papua New Guinea" pageId="2" pageNumber="569">Papua New Guinea</collectingCountry>
</emphasis>
, 
<location LSID="urn:lsid:plazi:treatment:03C287FFEF06463F30E1FF77FB486015:8EB46032EF06463F32E8FA40FD0E63E4" box="[670,757,1440,1466]" country="Papua New Guinea" name="Koitaki" pageId="2" pageNumber="569">Koitaki</location>
, 
<typeStatus box="[767,865,1440,1466]" pageId="2" pageNumber="569">holotype</typeStatus>
female 
<typeStatus box="[952,1048,1441,1467]" pageId="2" pageNumber="569">paratype</typeStatus>
male without host data, 
<date box="[1316,1436,1440,1467]" pageId="2" pageNumber="569" value="1928-09-15">
<collectingDate box="[1316,1436,1440,1467]" pageId="2" pageNumber="569" value="1928-09-15">15.ix.1928</collectingDate>
