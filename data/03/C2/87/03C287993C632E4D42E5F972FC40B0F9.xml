<paragraph blockId="7.[151,1437,1848,2017]" lastBlockId="8.[151,1437,152,2024]" lastPageId="8" lastPageNumber="209" pageId="7" pageNumber="208">
<emphasis bold="true" box="[151,224,1849,1875]" italics="true" pageId="7" pageNumber="208">Types.</emphasis>
