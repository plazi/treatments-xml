sensu RDB in catalogue), 
<number box="[745,814,1699,1723]" pageId="13" pageNumber="550" value="11287.0">11287</number>
(morpho 
<emphasis box="[924,983,1699,1723]" italics="true" pageId="13" pageNumber="550">fusca</emphasis>
), 
<number box="[1007,1076,1698,1723]" pageId="13" pageNumber="550" value="11056.0">11056</number>
(adult female), 
<number box="[1255,1325,1699,1723]" pageId="13" pageNumber="550" value="12039.0">12039</number>
