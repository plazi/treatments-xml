<taxonomicName authority="(Milliere)" authorityName="Milliere" baseAuthorityName="Milliere" baseAuthorityYear="1858" class="Insecta" family="Tortricidae" genus="Avaria" kingdom="Animalia" order="Lepidoptera" pageId="11" pageNumber="58" phylum="Arthropoda" rank="species" species="hyerana">
