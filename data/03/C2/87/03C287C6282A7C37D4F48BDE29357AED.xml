<number box="[771,813,631,656]" pageId="33" pageNumber="434" value="662.0">662</number>
and 
<quantity box="[867,951,632,656]" metricMagnitude="3" metricUnit="m" metricValue="1.1199999999999999" pageId="33" pageNumber="434" unit="m" value="1120.0">1120 m</quantity>
(
