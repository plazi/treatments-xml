<number box="[1205,1263,907,931]" pageId="13" value="1930.0">1930</number>
” (
<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[1288,1380,907,931]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="13">MNHN</collectionCode>
