<collectorName box="[406,487,1015,1038]" pageId="33" pageNumber="34">Jammu</collectorName>
&amp; 
<collectorName box="[538,634,1014,1038]" pageId="33" pageNumber="34">Kashmir</collectorName>
— 
<collectorName box="[663,754,1014,1038]" pageId="33" pageNumber="34">Michael</collectorName>
&amp; 
<collectorName box="[805,958,1014,1038]" pageId="33" pageNumber="34">Sharma B.K.</collectorName>
