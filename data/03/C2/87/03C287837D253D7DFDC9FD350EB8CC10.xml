<paragraph blockId="1.[151,1437,715,1001]" box="[151,660,715,744]" pageId="1" pageNumber="312">
<emphasis box="[151,660,715,744]" italics="true" pageId="1" pageNumber="312">
Female (n = 7, 
<typeStatus box="[351,461,715,744]" pageId="1" pageNumber="312">holotype</typeStatus>
+ 
<specimenCount box="[499,651,715,744]" pageId="1" pageNumber="312" type="generic" typeStatus="paratypes">6 paratypes</specimenCount>
