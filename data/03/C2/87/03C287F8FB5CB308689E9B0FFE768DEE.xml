<paragraph blockId="8.[151,1436,1191,1504]" pageId="8" pageNumber="55">
<emphasis bold="true" box="[199,275,1407,1432]" pageId="8" pageNumber="55">Notes:</emphasis>
<taxonomicName baseAuthorityName="Spinola" baseAuthorityYear="1808" box="[286,482,1407,1432]" class="Insecta" family="Braconidae" genus="Choeras" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="55" phylum="Arthropoda" rank="species" species="dorsalis">
<emphasis box="[286,482,1407,1432]" italics="true" pageId="8" pageNumber="55">Choeras dorsalis</emphasis>
