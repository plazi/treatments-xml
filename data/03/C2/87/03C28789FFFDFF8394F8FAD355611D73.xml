<specimenCount box="[475,576,1408,1432]" pageId="23" pageNumber="390" type="generic" typeStatus="type">three type</specimenCount>
specimens from &quot;Rayon Moskowskij&quot; near 
<location LSID="urn:lsid:plazi:treatment:03C28789FFFDFF8394F8FAD355611D73:8EB46044FFFDFF8D97B0FA53521919A2" box="[1021,1092,1408,1432]" country="Tajikistan" municipality="The" name="Kuljab" pageId="23" pageNumber="390">Kuljab</location>
