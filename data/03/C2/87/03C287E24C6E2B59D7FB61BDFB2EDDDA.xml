; 
<collectingRegion box="[151,261,1759,1783]" country="India" name="Rajasthan" pageId="29" pageNumber="30">Rajasthan</collectingRegion>
— 
<bibRefCitation author="Venkataraman, K." box="[288,551,1759,1783]" pageId="29" pageNumber="30" pagination="166 - 168" refId="ref88685" refString="Venkataraman, K. (1990 b) New records of Cladocera of Keoladeo National Park, Bharatpur - 3. Journal of the Bombay Natural History Society, 87, 166 - 168." type="journal article" year="1990">Venkataraman (1990b)</bibRefCitation>
; 
<collectingRegion box="[567,703,1759,1783]" country="India" name="Tamil Nadu" pageId="29" pageNumber="30">Tamil Nadu</collectingRegion>
— 
<bibRefCitation author="Raghunathan, M. B. &amp; Suresh Kumar, R." box="[731,1164,1759,1783]" pageId="29" pageNumber="30" pagination="959 - 960" refId="ref78893" refString="Raghunathan, M. B. &amp; Suresh Kumar, R. (2002) Cladocera (Crustacea) of Tamil Nadu: Checklist and bibliography. Zoos' Print Journal, 17, 959 - 960." type="journal article" year="2002">Raghunathan &amp; Suresh Kumar (2002</bibRefCitation>
, 2009); 
<collectingRegion box="[1263,1407,1759,1783]" country="India" name="West Bengal" pageId="29" pageNumber="30">West Bengal</collectingRegion>
