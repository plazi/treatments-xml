<subSubSection box="[199,1299,726,753]" pageId="5" pageNumber="120" type="etymology">
<paragraph blockId="5.[151,1436,150,753]" box="[199,1299,726,753]" pageId="5" pageNumber="120">
<emphasis bold="true" box="[199,334,726,752]" pageId="5" pageNumber="120">Etymology.</emphasis>
