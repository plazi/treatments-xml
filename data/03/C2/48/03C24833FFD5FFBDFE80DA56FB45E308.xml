; EN, 
<number box="[1198,1215,1644,1670]" pageId="17" pageNumber="52" value="6.0">6</number>
; IN, 
<number box="[1274,1291,1644,1670]" pageId="17" pageNumber="52" value="8.0">8</number>
; E 
<collectingCountry box="[264,284,1684,1710]" name="Italy" pageId="17" pageNumber="52">Y</collectingCountry>
, 
<number box="[296,313,1684,1710]" pageId="17" pageNumber="52" value="9.0">9</number>
