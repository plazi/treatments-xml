<taxonomicName authority="Hewitson, 1870" authorityName="Hewitson" authorityYear="1870" box="[360,752,1285,1311]" class="Insecta" family="Hesperiidae" genus="Eudamus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="60" pageNumber="61" phylum="Arthropoda" rank="species" species="centrites">
