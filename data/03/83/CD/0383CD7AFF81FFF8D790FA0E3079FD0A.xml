<collectionCode box="[1247,1353,1805,1831]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="61" pageNumber="62" type="Museum">NHMUK</collectionCode>
