, M 
<subScript attach="left" box="[1279,1287,746,760]" fontSize="6" pageId="72" pageNumber="73">1</subScript>
–M 
<subScript attach="left" box="[1326,1334,746,760]" fontSize="6" pageId="72" pageNumber="73">2</subScript>
, M 
<subScript attach="left" box="[1374,1382,746,760]" fontSize="6" pageId="72" pageNumber="73">2</subScript>
–M 
<subScript attach="left" box="[1421,1429,746,760]" fontSize="6" pageId="72" pageNumber="73">3</subScript>
, M 
<subScript attach="left" box="[176,184,782,796]" fontSize="6" pageId="72" pageNumber="73">3</subScript>
–CuA 
<subScript attach="left" box="[251,259,782,796]" fontSize="6" pageId="72" pageNumber="73">1</subScript>
, CuA 
<subScript attach="left" box="[327,335,782,796]" fontSize="6" pageId="72" pageNumber="73">1</subScript>
