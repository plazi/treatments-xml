); HL= 
<quantity box="[956,1058,1289,1316]" metricMagnitude="-4" metricUnit="m" metricValue="7.9" pageId="1" pageNumber="127" unit="mm" value="0.79">0.79 mm</quantity>
, HW= 
<quantity box="[1137,1236,1289,1316]" metricMagnitude="-4" metricUnit="m" metricValue="6.6" pageId="1" pageNumber="127" unit="mm" value="0.66">0.66 mm</quantity>
(HW: HL= 0.83); ELL= 
<quantity box="[227,332,1325,1352]" metricMagnitude="-4" metricUnit="m" metricValue="2.2" pageId="1" pageNumber="127" unit="mm" value="0.22">0.22 mm</quantity>
; TL= 
<quantity box="[403,507,1325,1352]" metricMagnitude="-4" metricUnit="m" metricValue="3.5999999999999996" pageId="1" pageNumber="127" unit="mm" value="0.36">0.36 mm</quantity>
; PL= 
<quantity box="[578,681,1325,1352]" metricMagnitude="-4" metricUnit="m" metricValue="9.0" pageId="1" pageNumber="127" unit="mm" value="0.9">0.90 mm</quantity>
, PW= 
<quantity box="[761,863,1325,1352]" metricMagnitude="-4" metricUnit="m" metricValue="6.8" pageId="1" pageNumber="127" unit="mm" value="0.68">0.68 mm</quantity>
(PW: HW= 1.03); EL= 
<quantity box="[1142,1246,1325,1352]" metricMagnitude="-3" metricUnit="m" metricValue="1.02" pageId="1" pageNumber="127" unit="mm" value="1.02">1.02 mm</quantity>
