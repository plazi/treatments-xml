<subSubSection lastPageId="17" pageId="13" type="description">
<paragraph blockId="13.[151,1437,258,2011]" pageId="13">
<taxonomicName authority="Larrazábal-Filho &amp; Neres &amp; Esteves, 2017" authorityName="Larrazábal-Filho &amp; Neres &amp; Esteves" authorityYear="2017" box="[199,485,1627,1651]" class="Chromadorea" family="Desmodoridae" genus="Stygodesmodora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Desmodorida" pageId="13" phylum="Nematoda" rank="species" species="robusta" status="sp. n.">
<emphasis box="[199,485,1627,1651]" italics="true" pageId="13">Stygodesmodora robusta</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[502,575,1626,1651]" pageId="13" rank="species">
<emphasis bold="true" box="[502,575,1626,1651]" pageId="13">sp. n.</emphasis>
</taxonomicNameLabel>
