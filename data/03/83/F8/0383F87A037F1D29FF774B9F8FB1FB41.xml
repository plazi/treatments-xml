<collectingCountry box="[1324,1429,1034,1059]" name="Australia" pageId="32" pageNumber="283">Australia</collectingCountry>
) (
<bibRefCitation author="Avdeev" box="[158,325,1070,1095]" pageId="32" pageNumber="283" refString="Avdeev, V. V. (1979 a) New species of the genus Cymothoa (Isopoda, Cymothoidae) from the coastal regions of north-western Australia. Parazitologiya, 13, 50 - 55." type="book chapter" year="1979" yearSuffix="a">
Avdeev 
<number box="[252,312,1071,1095]" pageId="32" pageNumber="283" value="1979.0">1979</number>
a
</bibRefCitation>
).
</paragraph>
<paragraph blockId="32.[151,1437,350,1203]" pageId="32" pageNumber="283">
<emphasis bold="true" box="[199,266,1106,1131]" pageId="32" pageNumber="283">Hosts</emphasis>
. Reported from 
<taxonomicName authority="Bloch &amp; Schneider, 1801" authorityName="Bloch &amp; Schneider" authorityYear="1801" box="[459,1022,1106,1131]" class="Actinopterygii" family="Carangidae" genus="Pseudocaranx" kingdom="Animalia" order="Perciformes" pageId="32" pageNumber="283" phylum="Chordata" rank="species" species="dentex">
<emphasis box="[459,702,1106,1131]" italics="true" pageId="32" pageNumber="283">Pseudocaranx dentex</emphasis>
(Bloch &amp; Schneider, 
<number box="[956,1013,1107,1131]" pageId="32" pageNumber="283" value="1801.0">1801</number>
)
</taxonomicName>
(previously 
<taxonomicName authority="Whitley, 1931" authorityName="Whitley" authorityYear="1931" box="[1169,1427,1108,1131]" class="Actinopterygii" family="Carangidae" genus="Usacaranx" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="32" pageNumber="283" phylum="Chordata" rank="species" species="georgianus">
<emphasis box="[1169,1427,1108,1131]" italics="true" pageId="32" pageNumber="283">Usacaranx georgianus</emphasis>
</taxonomicName>
) (see 
<bibRefCitation author="Avdeev" box="[211,383,1142,1167]" pageId="32" pageNumber="283" refString="Avdeev, V. V. (1979 a) New species of the genus Cymothoa (Isopoda, Cymothoidae) from the coastal regions of north-western Australia. Parazitologiya, 13, 50 - 55." type="book chapter" year="1979" yearSuffix="a">
Avdeev 
<number box="[311,370,1143,1167]" pageId="32" pageNumber="283" value="1979.0">1979</number>
a
</bibRefCitation>
; 
<bibRefCitation author="Hine" box="[403,609,1142,1167]" pageId="32" pageNumber="283" refString="Hine, P. M., Jones, J. B. &amp; Diggles, B. K. (2000) A checklist of the parasites of New Zealand fishes, including previously unpublished records. NIWA Technical Report, 75, 1 - 95." type="journal article" year="2000">
Hine 
<emphasis box="[472,536,1142,1167]" italics="true" pageId="32" pageNumber="283">et al.</emphasis>
<number box="[551,609,1143,1167]" pageId="32" pageNumber="283" value="2000.0">2000</number>
