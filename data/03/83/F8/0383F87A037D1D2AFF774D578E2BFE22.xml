<paragraph blockId="34.[151,1436,1861,2030]" pageId="34" pageNumber="285">
<emphasis bold="true" box="[151,430,1861,1886]" pageId="34" pageNumber="285">
<typeStatus box="[151,220,1861,1886]" pageId="34" pageNumber="285">Types</typeStatus>
and 
