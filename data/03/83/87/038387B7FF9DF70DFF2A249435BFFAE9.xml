<paragraph blockId="12.[151,1437,1072,1387]" box="[199,849,1252,1279]" pageId="12" pageNumber="505">
<emphasis bold="true" box="[199,353,1252,1278]" pageId="12" pageNumber="505">Distribution.</emphasis>
<collectingCountry box="[359,430,1253,1279]" name="China" pageId="12" pageNumber="505">China</collectingCountry>
: 
<collectingRegion box="[441,540,1253,1279]" country="China" name="Guangxi" pageId="12" pageNumber="505">Guangxi</collectingRegion>
, 
<collectingRegion box="[552,683,1253,1279]" country="China" name="Guangdong" pageId="12" pageNumber="505">Guangdong</collectingRegion>
(
<emphasis bold="true" box="[698,833,1253,1279]" pageId="12" pageNumber="505">new record</emphasis>
