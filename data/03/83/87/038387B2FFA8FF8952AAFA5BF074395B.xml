– 
<accessionNumber box="[632,755,1822,1845]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AB828280" pageId="3" pageNumber="510">AB828280</accessionNumber>
, 
<accessionNumber box="[151,266,1854,1877]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AB828381" pageId="3" pageNumber="510">AB828381</accessionNumber>
