, 
<collectingRegion box="[136,234,705,731]" country="Turkey" name="Kutahya" pageId="10" pageNumber="28">Kütahya</collectingRegion>
, 
<collectingRegion box="[244,334,705,731]" country="Turkey" name="Kayseri" pageId="10" pageNumber="28">Kayseri</collectingRegion>
, 
