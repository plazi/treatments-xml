<collectingCountry box="[1148,1272,1298,1323]" name="Costa Rica" pageId="23" pageNumber="516">Costa Rica</collectingCountry>
(
<collectingRegion box="[1288,1380,1299,1323]" country="Costa Rica" name="Cartago" pageId="23" pageNumber="516">Cartago</collectingRegion>
) and new departments records from 
<collectingRegion box="[499,583,1335,1359]" country="Colombia" name="Arauca" pageId="23" pageNumber="516">Arauca</collectingRegion>
, 
<collectingRegion box="[594,673,1334,1359]" country="Colombia" name="Caldas" pageId="23" pageNumber="516">Caldas</collectingRegion>
, 
<collectingRegion box="[684,749,1335,1359]" country="Colombia" name="Cesar" pageId="23" pageNumber="516">Cesar</collectingRegion>
, 
<collectingRegion box="[761,925,1334,1359]" country="Colombia" name="Cundinamarca" pageId="23" pageNumber="516">Cundinamarca</collectingRegion>
