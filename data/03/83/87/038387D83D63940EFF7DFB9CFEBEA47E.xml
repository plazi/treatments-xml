(
<collectingCountry box="[622,738,1226,1251]" name="Colombia" pageId="23" pageNumber="516">Colombia</collectingCountry>
), 
<bibRefCitation author="Martinez" box="[758,941,1226,1251]" pageId="23" pageNumber="516" refString="Martinez, C. (2000) Escarabajos Longicornios (Coleoptera: Cerambycidae) de Colombia. Biota Colombiana, 1 (1), 76 - 105." type="journal article" year="2000">Martinez (2000)</bibRefCitation>
