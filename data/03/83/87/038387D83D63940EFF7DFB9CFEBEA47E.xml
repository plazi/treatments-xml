<collectingRegion box="[327,414,1299,1323]" country="Colombia" name="Boyaca" pageId="23" pageNumber="516">Boyacá</collectingRegion>
, 
<collectingRegion box="[427,551,1298,1323]" country="Colombia" name="Magdalena" pageId="23" pageNumber="516">Magdalena</collectingRegion>
