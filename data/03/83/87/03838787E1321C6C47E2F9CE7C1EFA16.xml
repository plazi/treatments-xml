<taxonomicName authorityName="Wang &amp; Wang" authorityYear="2008" box="[288,552,1133,1156]" class="Arachnida" family="Pteronyssidae" genus="Timalinyssus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Acari" pageId="2" pageNumber="67" phylum="Arthropoda" rank="species" species="longitarsus">
