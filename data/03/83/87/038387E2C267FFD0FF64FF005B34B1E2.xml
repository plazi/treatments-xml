mm; width 
<quantity box="[335,508,1603,1627]" metricMagnitude="-2" metricUnit="m" metricValue="1.2349999999999999" metricValueMax="1.25" metricValueMin="1.22" pageId="5" pageNumber="4" unit="mm" value="12.35" valueMax="12.5" valueMin="12.2">12.2-12.5 mm</quantity>
