. 
<taxonomicName box="[378,593,1059,1083]" class="Insecta" family="Dynastidae" genus="Orizabus" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="7" phylum="Arthropoda" rank="species" species="mezclus">
