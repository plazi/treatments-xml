<emphasis bold="true" box="[276,491,1941,1963]" italics="true" pageId="4" pageNumber="295">Meiyingia holzschuhi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[498,575,1941,1963]" pageId="4" pageNumber="295" rank="species">sp. nov.</taxonomicNameLabel>
♂ holotype: A
</emphasis>
, tergite VIII, sternite VIII &amp; IX (ventral view); 
<emphasis bold="true" box="[1197,1213,1941,1963]" pageId="4" pageNumber="295">B</emphasis>
, male genitalia (dorsal view); 
<emphasis bold="true" box="[218,235,1973,1995]" pageId="4" pageNumber="295">C</emphasis>
, male genitalia (ventral view); 
<emphasis bold="true" box="[536,553,1973,1995]" pageId="4" pageNumber="295">D</emphasis>
, male genitalia (lateral view); 
