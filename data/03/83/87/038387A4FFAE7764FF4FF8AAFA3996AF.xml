<materialsCitation ID-GBIF-Occurrence="3441300346" box="[151,1022,1904,1931]" collectionCode="RMNH" country="Indonesia" location="Makassar" pageId="18" pageNumber="119" specimenCount="1" stateProvince="Sulawesi Utara" typeStatus="holotype">
