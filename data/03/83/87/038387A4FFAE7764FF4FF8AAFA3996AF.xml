.INS.1256442 (
<collectingCountry box="[587,700,1904,1930]" name="Indonesia" pageId="18" pageNumber="119">Indonesia</collectingCountry>
, 
<collectingRegion box="[711,815,1904,1930]" country="Indonesia" name="Sulawesi Utara" pageId="18" pageNumber="119">Sulawesi</collectingRegion>
