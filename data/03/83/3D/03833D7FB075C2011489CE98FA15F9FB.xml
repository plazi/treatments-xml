<quantity box="[629,726,1414,1436]" metricMagnitude="-4" metricUnit="m" metricValue="7.5" metricValueMax="10.0" metricValueMin="5.0" pageId="1" pageNumber="24" unit="mm" value="0.75" valueMax="1.0" valueMin="0.5">0.5–1 mm</quantity>
