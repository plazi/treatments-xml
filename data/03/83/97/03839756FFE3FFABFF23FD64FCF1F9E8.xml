<subSubSection pageId="24" pageNumber="225" type="etymology">
<paragraph blockId="24.[151,1436,870,1543]" pageId="24" pageNumber="225">
<emphasis bold="true" box="[199,326,942,967]" pageId="24" pageNumber="225">Etymology</emphasis>
