<quantity box="[363,498,474,501]" metricMagnitude="-3" metricUnit="m" metricValue="6.35" metricValueMax="7.5" metricValueMin="5.2" pageId="31" pageNumber="32" unit="mm" value="6.35" valueMax="7.5" valueMin="5.2">5.2–7.5 mm</quantity>
