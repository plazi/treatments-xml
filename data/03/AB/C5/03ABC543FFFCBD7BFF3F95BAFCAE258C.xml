, second tarsomere 
<number box="[839,891,655,679]" pageId="4" pageNumber="30" value="0.07">0.07</number>
, third tarsomere 
<number box="[1079,1131,655,679]" pageId="4" pageNumber="30" value="0.06">0.06</number>
, pretarsal claw 
<number box="[1304,1356,655,679]" pageId="4" pageNumber="30" value="0.09">0.09</number>
; mesothoracic leg coxa 
