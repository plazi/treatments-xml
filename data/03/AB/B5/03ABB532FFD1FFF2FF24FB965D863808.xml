population collected in 2014 from rhizosphere of alfalfa in Darab region, 
<collectingRegion country="Iran" name="Fars" pageId="5" pageNumber="487">Fars province</collectingRegion>
, 
<collectingCountry box="[212,257,1953,1979]" name="Iran" pageId="5" pageNumber="487">Iran</collectingCountry>
(GPS coordinates: 
<geoCoordinate box="[475,621,1952,1979]" degrees="28" direction="north" minutes="41.167" orientation="latitude" pageId="5" pageNumber="487" precision="1" value="28.686117">28°41.167′N</geoCoordinate>
, 
<geoCoordinate box="[632,774,1952,1979]" degrees="54" direction="east" minutes="16.056" orientation="longitude" pageId="5" pageNumber="487" precision="1" value="54.2676">54°16.056′E</geoCoordinate>
, elevation 
<quantity box="[895,986,1953,1979]" metricMagnitude="3" metricUnit="m" metricValue="1.3669999999999998" pageId="5" pageNumber="487" unit="m" value="1367.0">1367 m</quantity>
.).
</paragraph>
