<taxonomicName authority="F. Muell. The" authorityName="F. Muell. The" class="Magnoliopsida" family="Myrtaceae" genus="Eucalyptus" kingdom="Plantae" order="Myrtales" pageId="23" pageNumber="24" phylum="Tracheophyta" rank="species" species="salmonophloia">
