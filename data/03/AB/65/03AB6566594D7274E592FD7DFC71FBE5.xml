<taxonomicName box="[1291,1408,1843,1869]" class="Liliopsida" family="Asparagaceae" genus="Lomandra" kingdom="Plantae" order="Asparagales" pageId="14" pageNumber="15" phylum="Tracheophyta" rank="genus">
