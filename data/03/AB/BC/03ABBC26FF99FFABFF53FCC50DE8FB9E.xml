<collectionCode box="[380,461,941,966]" pageId="3" pageNumber="70">GSMC</collectionCode>
004745– 
<number box="[561,589,941,966]" pageId="3" pageNumber="70" value="50.0">50</number>
), and 
<number box="[655,683,942,966]" pageId="3" pageNumber="70" value="29.0">29</number>
