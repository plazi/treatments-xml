<collectingCountry box="[1038,1116,1580,1604]" name="Republic of the Congo" pageId="8" pageNumber="75">Congo</collectingCountry>
, 
<collectingCountry box="[1130,1206,1581,1605]" name="Kenya" pageId="8" pageNumber="75">Kenya</collectingCountry>
, 
<collectingCountry box="[1220,1324,1580,1605]" name="Tanzania" pageId="8" pageNumber="75">Tanzania</collectingCountry>
, 
<collectingCountry box="[1337,1432,1580,1605]" name="Somalia" pageId="8" pageNumber="75">Somalia</collectingCountry>
, and 
<collectingCountry box="[204,294,1615,1640]" name="Uganda" pageId="8" pageNumber="75">Uganda</collectingCountry>
, but 
<taxonomicName box="[361,486,1615,1640]" class="Insecta" family="Tarachodidae" genus="Oxyelaea" kingdom="Animalia" order="Mantodea" pageId="8" pageNumber="75" phylum="Arthropoda" rank="species" species="elegans">
