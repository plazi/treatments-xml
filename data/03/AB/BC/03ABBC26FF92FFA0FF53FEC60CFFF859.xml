<collectingCountry box="[151,302,2012,2037]" name="Mozambique" pageId="8" pageNumber="75">Mozambique</collectingCountry>
, 
<collectingCountry box="[313,376,2012,2037]" name="South Africa" pageId="8" pageNumber="75">Natal</collectingCountry>
, 
<collectingCountry box="[387,482,2012,2037]" name="Somalia" pageId="8" pageNumber="75">Somalia</collectingCountry>
, 
<collectingCountry box="[493,597,2012,2037]" name="Tanzania" pageId="8" pageNumber="75">Tanzania</collectingCountry>
