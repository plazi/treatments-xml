<taxonomicName box="[185,409,1336,1360]" class="Bivalvia" family="Spondylidae" genus="Spondylus" kingdom="Animalia" order="Pectinoida" pageId="8" pageNumber="165" phylum="Mollusca" rank="species" species="americanus">Spondylus americanus</taxonomicName>
, 
<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[425,564,1336,1360]" class="Bivalvia" family="Spondylidae" genus="Spondylus" kingdom="Animalia" order="Pectinoida" pageId="8" pageNumber="165" phylum="Mollusca" rank="species" species="gaederopus">S. gaederopus</taxonomicName>
, 
<taxonomicName authority=", Pecten" authorityName="Pecten" class="Bivalvia" family="Spondylidae" genus="Spondylus" kingdom="Animalia" order="Pectinoida" pageId="8" pageNumber="165" phylum="Mollusca" rank="species" species="senegalensis">S. senegalensis, Pecten</taxonomicName>
gibbosus
</emphasis>
and 
<emphasis box="[402,565,1368,1392]" italics="true" pageId="8" pageNumber="165">
<taxonomicName baseAuthorityName="Roding" baseAuthorityYear="1798" box="[402,560,1368,1392]" class="Bivalvia" family="Pteriidae" genus="Pteria" kingdom="Animalia" order="Pterioida" pageId="8" pageNumber="165" phylum="Mollusca" rank="species" species="colymbus">Pteria colymbus</taxonomicName>
