<quantity metricMagnitude="3" metricUnit="m" metricValue="2.92" metricValueMax="5.34" metricValueMin="0.5" pageId="9" pageNumber="36" unit="m" value="2920.0" valueMax="5340.0" valueMin="500.0">500 to 5340 m</quantity>
