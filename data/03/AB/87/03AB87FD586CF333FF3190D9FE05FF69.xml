<collectingCountry box="[151,244,189,215]" name="United Kingdom" pageId="8" pageNumber="330">England</collectingCountry>
and 
<collectingCountry box="[301,478,189,215]" name="Netherlands" pageId="8" pageNumber="330">the Netherlands</collectingCountry>
(
