<collectingCountry box="[985,1056,1905,1931]" name="Brazil" pageId="67" pageNumber="68">Brazil</collectingCountry>
, and 
<collectingRegion box="[1114,1220,1905,1931]" country="Argentina" name="Misiones" pageId="67" pageNumber="68">Misiones</collectingRegion>
, 
<collectingRegion box="[1231,1328,1905,1932]" country="Argentina" name="Santa Fe" pageId="67" pageNumber="68">Santa Fe</collectingRegion>
and 
<collectingRegion country="Argentina" name="Ciudad Autonoma de Buenos Aires" pageId="67" pageNumber="68">Buenos Aires</collectingRegion>
