</collectingRegion>
, 
<collectingCountry box="[586,647,1777,1798]" name="Brazil" pageId="7">Brazil</collectingCountry>
, 
<collectingRegion box="[656,750,1777,1798]" country="Paraguay" name="Asuncion" pageId="7">
<collectingCountry box="[656,750,1777,1798]" name="Paraguay" pageId="7">Paraguay</collectingCountry>
