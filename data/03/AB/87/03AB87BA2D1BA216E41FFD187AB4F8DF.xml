<taxonomicName authority=", Persephone Pot, A" authorityName="Persephone Pot, A" box="[796,1220,1361,1383]" class="Gastropoda" family="Hydrobiidae" genus="Pseudotricula" kingdom="Animalia" order="Littorinimorpha" pageId="26" pageNumber="27" phylum="Mollusca" rank="species" species="elongata">
