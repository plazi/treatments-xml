from 
<location LSID="urn:lsid:plazi:treatment:03AB87CFFFEFE367FF7356C8FE5EFBDA:8EDD6002FFEFE364FCA756A1FC66F91C" box="[835,902,507,531]" country="Argentina" name="Tandil" pageId="33" pageNumber="34" stateProvince="Ciudad Autonoma de Buenos Aires">Tandil</location>
, 
