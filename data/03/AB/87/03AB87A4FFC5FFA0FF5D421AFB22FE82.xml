<number box="[1265,1292,1855,1879]" pageId="2" pageNumber="160" value="20.0">20</number>
(
<number box="[1310,1379,1855,1879]" pageId="2" pageNumber="160" value="19.0" valueMax="20.0" valueMin="18.0">18–20</number>
