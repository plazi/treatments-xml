<number box="[983,1010,1889,1913]" pageId="2" pageNumber="160" value="54.0">54</number>
(
<number box="[1028,1098,1889,1913]" pageId="2" pageNumber="160" value="47.5" valueMax="55.0" valueMin="40.0">40–55</number>
