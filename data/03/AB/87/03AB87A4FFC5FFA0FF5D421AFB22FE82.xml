<number box="[585,625,1855,1879]" pageId="2" pageNumber="160" value="175.0">175</number>
(
<number box="[644,739,1855,1879]" pageId="2" pageNumber="160" value="159.0" valueMax="188.0" valueMin="130.0">130–188</number>
