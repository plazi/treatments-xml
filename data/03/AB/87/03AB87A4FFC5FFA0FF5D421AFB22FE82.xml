<number box="[697,710,1890,1914]" pageId="2" pageNumber="160" value="6.0">6</number>
(
<number box="[730,774,1890,1914]" pageId="2" pageNumber="160" value="6.5" valueMax="7.0" valueMin="6.0">6–7</number>
