<geoCoordinate box="[151,305,1722,1747]" direction="north" orientation="latitude" pageId="8" pageNumber="423" precision="1" value="43.30172">43°18′06.2″N</geoCoordinate>
<geoCoordinate box="[319,487,1722,1747]" direction="east" orientation="longitude" pageId="8" pageNumber="423" precision="1" value="140.59877">140°35′55.6″E</geoCoordinate>
