<specimenCount box="[1089,1229,298,322]" pageId="5" pageNumber="46" type="male">seven males</specimenCount>
– 
<specimenCode box="[1265,1432,298,322]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="5" pageNumber="46">CHUNB 1160</specimenCode>
, 
<specimenCode box="[151,209,335,359]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="5" pageNumber="46">1169</specimenCode>
, 
<specimenCode box="[220,277,335,359]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="5" pageNumber="46">1192</specimenCode>
, 
<specimenCode box="[289,348,335,359]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="5" pageNumber="46">1220</specimenCode>
, 
<specimenCode box="[359,417,335,359]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="5" pageNumber="46">1247</specimenCode>
, 
<specimenCode box="[429,487,335,359]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="5" pageNumber="46">1249</specimenCode>
