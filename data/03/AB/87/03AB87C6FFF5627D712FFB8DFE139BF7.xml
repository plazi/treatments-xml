<taxonomicName authority="Polhemus &amp; Tran &amp; Polhemus, 2009" authorityName="Polhemus &amp; Tran &amp; Polhemus" authorityYear="2009" box="[826,937,2088,2109]" class="Insecta" family="Gerridae" genus="Eotrechus" kingdom="Animalia" order="Hemiptera" pageId="7" pageNumber="36" phylum="Arthropoda" rank="species" species="fansipan" status="new species">
