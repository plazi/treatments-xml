<caption ID-DOI="http://doi.org/10.5281/zenodo.5341562" ID-Zenodo-Dep="5341562" box="[109,1391,2088,2109]" httpUri="https://zenodo.org/record/5341562/files/figure.png" pageId="7" pageNumber="36" startId="7.[109,144,2088,2109]" targetBox="[121,1437,185,2051]" targetPageId="7">
<paragraph blockId="7.[109,1391,2088,2109]" box="[109,1391,2088,2109]" pageId="7" pageNumber="36">
