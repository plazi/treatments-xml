<collectingRegion box="[774,921,370,391]" country="Mexico" name="Baja California" pageId="3" pageNumber="594">
Baja 
<collectingRegion box="[824,921,370,391]" country="United States of America" name="California" pageId="3" pageNumber="594">California</collectingRegion>
</collectingRegion>
and Gulf of 
<collectingRegion box="[1043,1140,370,391]" country="United States of America" name="California" pageId="3" pageNumber="594">California</collectingRegion>
....................... 2
</keyLead>
