<emphasis box="[180,306,401,424]" italics="true" pageId="14" pageNumber="538">S. princesa</emphasis>
</taxonomicName>
<emphasis bold="true" box="[313,428,400,425]" pageId="14" pageNumber="538">
<taxonomicNameLabel box="[313,428,400,425]" pageId="14" pageNumber="538" rank="species">spec. nov.</taxonomicNameLabel>
</emphasis>
; a/b= 
<quantity box="[498,536,400,425]" metricMagnitude="-2" metricUnit="m" metricValue="2.54" pageId="14" pageNumber="538" unit="in" value="1.0">1 in</quantity>
