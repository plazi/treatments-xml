, 
<specimenCode box="[790,847,1117,1141]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="9" pageNumber="50">1191</specimenCode>
, 
<specimenCode box="[859,917,1117,1141]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="9" pageNumber="50">1193</specimenCode>
</materialsCitation>
, 
<materialsCitation box="[928,1340,1117,1141]" collectionCode="CHUNB" pageId="9" pageNumber="50" specimenCode="CHUNB 1253" specimenCount="1" specimenCount-male="1">
and 1255; 
<specimenCount box="[1046,1147,1117,1141]" pageId="9" pageNumber="50" type="male">one male</specimenCount>
– 
<specimenCode box="[1176,1340,1117,1141]" collectionCode="CHUNB" country="Brazil" httpUri="http://grbio.org/cool/tids-e3wr" name="University of Brasilia Herpetological Collection" pageId="9" pageNumber="50">CHUNB 1253</specimenCode>
</materialsCitation>
.
</paragraph>
