<taxonomicName authorityName="Goodey" authorityYear="1963" baseAuthorityName="Cobb" baseAuthorityYear="1920" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="cylindricus">
<emphasis italics="true" pageId="12" pageNumber="334">T. cylindricus</emphasis>
</taxonomicName>
, and 
<taxonomicName authorityName="Sauer" authorityYear="1968" baseAuthorityName="Andrassy" baseAuthorityYear="1961" box="[337,454,1120,1146]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="pileatus">
<emphasis box="[337,454,1120,1146]" italics="true" pageId="12" pageNumber="334">T. pileatus</emphasis>
</taxonomicName>
. See 
<taxonomicName authorityName="Sauer" authorityYear="1968" box="[515,665,1121,1146]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="constrictus">
<emphasis box="[515,665,1121,1146]" italics="true" pageId="12" pageNumber="334">T. constrictus</emphasis>
