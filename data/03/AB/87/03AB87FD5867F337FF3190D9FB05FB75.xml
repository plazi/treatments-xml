<taxonomicName authorityName="Sauer" authorityYear="1968" box="[1078,1231,1085,1110]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="constrictus">
