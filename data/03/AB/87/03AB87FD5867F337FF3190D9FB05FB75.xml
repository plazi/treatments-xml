</paragraph>
<paragraph blockId="12.[151,1437,1047,1147]" pageId="12" pageNumber="334">
<taxonomicName authorityName="Sauer" authorityYear="1968" box="[151,415,1084,1110]" class="Secernentea" family="Diplogasteridae" genus="Tylolaimophorus" kingdom="Animalia" order="Diplogasterida" pageId="12" pageNumber="334" phylum="Nematoda" rank="species" species="pugio">
