<emphasis bold="true" box="[965,978,1946,1970]" pageId="52" pageNumber="53">a</emphasis>
, habitus, dorsal view; 
<emphasis bold="true" box="[1210,1224,1946,1970]" pageId="52" pageNumber="53">b</emphasis>
