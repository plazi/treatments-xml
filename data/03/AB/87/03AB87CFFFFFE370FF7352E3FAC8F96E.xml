<emphasis bold="true" box="[151,162,1982,2006]" pageId="52" pageNumber="53">c</emphasis>
, same, lateral view; 
<emphasis bold="true" box="[371,385,1982,2006]" pageId="52" pageNumber="53">d</emphasis>
, prosoma, dorsal view; 
<emphasis bold="true" box="[627,638,1982,2006]" pageId="52" pageNumber="53">e</emphasis>
, same, ventral view; 
<emphasis bold="true" box="[854,862,1981,2005]" pageId="52" pageNumber="53">f</emphasis>
, same, lateral view; 
<emphasis bold="true" box="[1071,1084,1982,2006]" pageId="52" pageNumber="53">g</emphasis>
left palp, prolateral view; 
<emphasis bold="true" box="[1351,1365,1982,2006]" pageId="52" pageNumber="53">h</emphasis>
