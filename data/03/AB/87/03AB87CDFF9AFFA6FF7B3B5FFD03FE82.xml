<emphasis box="[800,924,740,761]" italics="true" pageId="3" pageNumber="211">
<taxonomicName box="[800,917,740,761]" class="Tremellomycetes" family="Tremellaceae" genus="Tremella" kingdom="Fungi" order="Tremellales" pageId="3" pageNumber="211" phylum="Basidiomycota" rank="species" species="salmonea">T. salmonea</taxonomicName>
