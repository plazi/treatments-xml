(
<figureCitation box="[159,245,870,895]" captionStart="FIGURES 10 – 22" captionStartId="3.[151,264,1855,1878]" captionTargetBox="[154,1433,193,1824]" captionTargetId="figure@3.[154,1433,193,1835]" captionTargetPageId="3" captionText="FIGURES 10 – 22. Hypopygium, dorsal view; 10. Trichiscus suzannae; 11. T. wittei; 12. T. noorti sp. nov.; 13. T. denisi sp. nov.; 14. T. briani sp. nov.; 15. T. copelandi sp. nov.; 16. T. mourei sp. nov.; 17. T. lynnae sp. nov.; 18. T. normani sp. nov.; 19. T. luboi sp. nov.; 20. T. zuparkoi sp. nov.; 21. T. ohli sp. nov.; 22. T. camargoi sp. nov. (Scale bars = 50 µm)." httpUri="https://zenodo.org/record/228359/files/figure.png" pageId="13" pageNumber="331">Figs 22</figureCitation>
, 
<figureCitation box="[257,329,870,895]" captionStart="FIGURES 63 – 71" captionStartId="14.[151,264,1942,1965]" captionTargetBox="[229,1353,209,1904]" captionTargetId="figure@14.[205,1381,193,1922]" captionTargetPageId="14" captionText="FIGURES 63 – 71. Male genitalia. 63 – 65 Trichiscus ohli sp. nov.; 63. dorsal view; 64. ventral view; 65. lateral view. 66 – 68 T. camargoi sp. nov.; 66 dorsal view; 67. ventral view; 68. lateral view. 69 – 71 T. jimi sp. nov.; 69. dorsal view; 70. ventral view; 71. lateral view. (Scale bars = 50 µm)." httpUri="https://zenodo.org/record/228364/files/figure.png" pageId="13" pageNumber="331">
66– 
<number box="[301,329,870,895]" pageId="13" pageNumber="331" value="68.0">68</number>
</figureCitation>
