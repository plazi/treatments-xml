<subSubSection box="[198,844,637,663]" pageId="16" pageNumber="17" type="distribution">
<paragraph blockId="16.[151,1437,152,1074]" box="[198,844,637,663]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[198,351,637,663]" pageId="16" pageNumber="17">Distribution</emphasis>
: Neotropical 
<emphasis bold="true" box="[512,527,637,663]" pageId="16" pageNumber="17">–</emphasis>
<taxonomicName box="[534,650,637,663]" class="Insecta" family="Mantidae" genus="Coptopteryx" kingdom="Animalia" order="Mantodea" pageId="16" pageNumber="29" phylum="Arthropoda" rank="species" species="argentina">
