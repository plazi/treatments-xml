<taxonomicName box="[1227,1433,928,954]" class="Gastropoda" family="Xanthonychidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="31" pageNumber="32" phylum="Mollusca" rank="family">Xanthonychidae</taxonomicName>
: 
<taxonomicName authority="Scott Hylton" authorityName="Scott Hylton" box="[151,673,965,991]" class="Gastropoda" family="Helminthoglyptidae" genus="Epiphragmophora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Stylommatophora" pageId="31" pageNumber="32" phylum="Mollusca" rank="species" species="hemiclausa">
<emphasis box="[151,513,965,990]" italics="true" pageId="31" pageNumber="32">Epiphragmophora hemiclausa</emphasis>
Scott Hylton
</taxonomicName>
(
<bibRefCitation author="Lopes" box="[689,851,965,991]" pageId="31" pageNumber="32" refString="Lopes, H. S. (1969 b) Neotropical Sarcophagidae reared from Gastropoda by Dr. W. Weyrauch (Diptera). Studia Entomolgica, 12, 133 - 160." type="journal article" year="1969" yearSuffix="b">
Lopes 
<number box="[772,835,965,991]" pageId="31" pageNumber="32" value="1969.0">1969</number>
b
</bibRefCitation>
). Gastropoda, 
<taxonomicName box="[1024,1175,965,991]" class="Gastropoda" family="Bulimulidae" kingdom="Animalia" order="Stylommatophora" pageId="31" pageNumber="32" phylum="Mollusca" rank="family">Bulimulidae</taxonomicName>
: 
<taxonomicName authority="Weyrauch" authorityName="Weyrauch" class="Mollusca" family="Mollusca" genus="Bulimulus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Mollusca" pageId="31" pageNumber="32" phylum="Mollusca" rank="species" species="vervoorsti">
<emphasis box="[1187,1436,965,990]" italics="true" pageId="31" pageNumber="32">Bulimulus vervoorsti</emphasis>
Weyrauch
</taxonomicName>
(
<bibRefCitation author="Lopes" box="[297,471,1002,1028]" pageId="31" pageNumber="32" refString="Lopes, H. S. (1969 b) Neotropical Sarcophagidae reared from Gastropoda by Dr. W. Weyrauch (Diptera). Studia Entomolgica, 12, 133 - 160." type="journal article" year="1969" yearSuffix="b">
Lopes 
<number box="[386,454,1002,1028]" pageId="31" pageNumber="32" value="1969.0">1969</number>
