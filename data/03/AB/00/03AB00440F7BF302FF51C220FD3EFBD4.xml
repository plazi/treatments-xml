<taxonomicName box="[347,473,928,954]" class="Insecta" family="Sarcophagidae" genus="Sarcophaga" kingdom="Animalia" order="Diptera" pageId="31" pageNumber="29" phylum="Arthropoda" rank="species" species="argentina">
<collectingCountry box="[347,473,928,954]" name="Argentina" pageId="31" pageNumber="32">Argentina</collectingCountry>
</taxonomicName>
