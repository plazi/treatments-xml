<subSubSection box="[192,1276,738,762]" pageId="8" pageNumber="7" type="etymology">
<paragraph blockId="8.[192,1276,738,762]" box="[192,1276,738,762]" pageId="8" pageNumber="7">
<emphasis bold="true" box="[192,348,738,762]" pageId="8" pageNumber="7">Etymology.</emphasis>
