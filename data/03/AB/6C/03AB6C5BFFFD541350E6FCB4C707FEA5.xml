— 
<number box="[968,977,1551,1575]" pageId="6" pageNumber="60" value="1.0">1</number>
<emphasis box="[984,1017,1551,1575]" italics="true" pageId="6" pageNumber="60">asl</emphasis>
, 
<number box="[1031,1045,1551,1575]" pageId="6" pageNumber="60" value="5.0">5</number>
