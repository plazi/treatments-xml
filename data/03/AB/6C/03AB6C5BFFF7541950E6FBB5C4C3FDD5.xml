<number box="[840,849,1901,1925]" pageId="12" pageNumber="66" value="1.0">1</number>
— 
<number box="[882,939,1901,1925]" pageId="12" pageNumber="66" value="1.5" valueMax="2.0" valueMin="1.0">1—2</number>
— 
<number box="[971,980,1901,1925]" pageId="12" pageNumber="66" value="1.0">1</number>
<emphasis box="[992,1021,1902,1925]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
; basifemora I–IV, 
<number box="[1246,1304,1901,1925]" pageId="12" pageNumber="66" value="5.0" valueMax="6.0" valueMin="4.0">4—6</number>
— 
<number box="[1335,1347,1901,1925]" pageId="12" pageNumber="66" value="3.0">3</number>
— 
<number box="[1378,1387,1901,1925]" pageId="12" pageNumber="66" value="1.0">1</number>
<emphasis box="[1398,1428,1902,1925]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
; telofemora I–IV, 
<number box="[342,354,1935,1959]" pageId="12" pageNumber="66" value="5.0">5</number>
— 
<number box="[384,396,1935,1959]" pageId="12" pageNumber="66" value="5.0">5</number>
— 
<number box="[426,440,1935,1959]" pageId="12" pageNumber="66" value="4.0">4</number>
— 
<number box="[470,482,1935,1959]" pageId="12" pageNumber="66" value="3.0">3</number>
<emphasis box="[489,518,1936,1959]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
; genua I–IV, {
<number box="[682,697,1935,1959]" pageId="12" pageNumber="66" value="1.0">1</number>
<emphasis box="[704,737,1935,1959]" italics="true" pageId="12" pageNumber="66">asl</emphasis>
, 
<number box="[750,764,1935,1959]" pageId="12" pageNumber="66" value="1.0">1</number>
<emphasis box="[771,800,1936,1959]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
}, 
<number box="[828,842,1935,1959]" pageId="12" pageNumber="66" value="3.0">3</number>
<emphasis box="[849,882,1935,1959]" italics="true" pageId="12" pageNumber="66">asl</emphasis>
, 
<number box="[895,909,1935,1959]" pageId="12" pageNumber="66" value="4.0">4</number>
<emphasis box="[916,946,1936,1959]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
— 
<number box="[975,988,1935,1959]" pageId="12" pageNumber="66" value="2.0">2</number>
<emphasis box="[994,1027,1935,1959]" italics="true" pageId="12" pageNumber="66">asl</emphasis>
, 
<number box="[1041,1055,1935,1959]" pageId="12" pageNumber="66" value="5.0">5</number>
<emphasis box="[1061,1091,1936,1959]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
— 
<number box="[1124,1133,1935,1959]" pageId="12" pageNumber="66" value="1.0">1</number>
<emphasis box="[1140,1173,1935,1959]" italics="true" pageId="12" pageNumber="66">asl</emphasis>
, 
<number box="[1187,1201,1935,1959]" pageId="12" pageNumber="66" value="5.0">5</number>
<emphasis box="[1207,1236,1936,1959]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
— 
<number box="[1266,1279,1935,1959]" pageId="12" pageNumber="66" value="2.0">2</number>
<emphasis box="[1285,1318,1935,1959]" italics="true" pageId="12" pageNumber="66">asl</emphasis>
, 
<number box="[1332,1346,1935,1959]" pageId="12" pageNumber="66" value="5.0">5</number>
<emphasis box="[1353,1382,1936,1959]" italics="true" pageId="12" pageNumber="66">sts</emphasis>
; tibiae I–IV, 
<number box="[254,268,1970,1994]" pageId="12" pageNumber="66" value="1.0">1</number>
<emphasis box="[276,308,1970,1994]" italics="true" pageId="12" pageNumber="66">asl</emphasis>
, 
