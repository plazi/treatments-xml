</taxonomicName>
<emphasis bold="true" box="[849,940,753,777]" pageId="3" pageNumber="264">
, 
<taxonomicNameLabel box="[860,940,753,777]" pageId="3" pageNumber="264" rank="species">sp. nov.</taxonomicNameLabel>
</emphasis>
; B. 
<taxonomicName authority="Smetana" authorityName="Smetana" authorityYear="2003" box="[982,1333,753,777]" class="Insecta" family="Staphylinidae" genus="Sphaerobulbus" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="264" phylum="Arthropoda" rank="species" species="yunnanus">
