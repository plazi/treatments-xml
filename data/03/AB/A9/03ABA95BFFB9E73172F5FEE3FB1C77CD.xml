<quantity box="[695,857,813,840]" metricMagnitude="3" metricUnit="m" metricValue="1.65" metricValueMax="1.8" metricValueMin="1.5" pageId="11" pageNumber="162" unit="m" value="1650.0" valueMax="1800.0" valueMin="1500.0">1500–1800 m</quantity>
