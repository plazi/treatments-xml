(
<figureCitation box="[159,273,188,212]" captionStart-0="FIGURES 1 – 8" captionStart-1="FIGURES 9 – 14" captionStartId-0="5.[151,264,1748,1771]" captionStartId-1="6.[151,264,1157,1180]" captionTargetBox-0="[173,1423,281,1690]" captionTargetBox-1="[151,1435,193,1135]" captionTargetId-0="figure@5.[151,1436,265,1693]" captionTargetId-1="figure@6.[151,1436,193,1136]" captionTargetPageId-0="5" captionTargetPageId-1="6" captionText-0="FIGURES 1 – 8. Bactrothrips elongatus n. sp. Female: 1, antenna; 2, dorsal view of head; 3, dorsal view of pronotum; 4, dorsal view of mesonotum and metanotum; 5, fore wing; 6, pelta; 7, abdominal tergite IX and tube. Male: 8, abdominal tergites VI – VIII showing the lateral tubercles. Scale bars = 100 μm." captionText-1="FIGURES 9 – 14. Bactrothrips elongatus n. sp. Female: 9, antenna, head, pronotum and fore legs; 10, dorsal view of pronotum; 11, dorsal view of mesonotum and metanotum; 12, hind leg; 13, abdominal tergite IX and tube. Male: 14, abdominal tergite IX and tube. Scale bars = 100 μm." httpUri-0="https://zenodo.org/record/214518/files/figure.png" httpUri-1="https://zenodo.org/record/214519/files/figure.png" pageId="4" pageNumber="51">
Figs 
<number box="[215,225,188,212]" pageId="4" pageNumber="51" value="1.0">1</number>
–14
</figureCitation>
, 
<figureCitation box="[285,315,188,212]" captionStart="FIGURES 31 – 37" captionStartId="10.[151,264,1790,1813]" captionTargetBox="[151,1435,193,1767]" captionTargetId="figure@10.[151,1436,193,1769]" captionTargetPageId="10" captionText="FIGURES 31 – 37. Bactrothrips spp. Dorsal view of head: 31, B. brevitubus; 32, B. flectoventris; 33, B. honoris; 34, B. pictipes; 35, B. quadrituberculatus; 36, B. elongatus n. sp.; 37, B. furvescrus n. sp. Scale bars = 100 μm." httpUri="https://zenodo.org/record/214522/files/figure.png" pageId="4" pageNumber="51">36</figureCitation>
, 
<figureCitation box="[327,356,188,212]" captionStart="FIGURES 38 – 43" captionStartId="11.[151,264,1566,1589]" captionTargetBox="[156,1424,193,1544]" captionTargetId="figure@11.[151,1436,193,1545]" captionTargetPageId="11" captionText="FIGURES 38 – 43. Bactrothrips spp. Abdominal tergites VI – VIII showing the lateral tubercles: 38 &amp; 39, B. brevitubus; 40, B. flectoventris; 41, B. quadrituberculatus; 42, B. elongatus n. sp.; 43, B. furvescrus n. sp. Scale bars = 100 μm." httpUri="https://zenodo.org/record/214523/files/figure.png" pageId="4" pageNumber="51">
