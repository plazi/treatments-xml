<specimenCount box="[343,456,576,598]" pageId="9" pageNumber="1946" type="female">one female</specimenCount>
from subalpine stands at 1600 and 
<quantity box="[828,905,576,598]" metricMagnitude="3" metricUnit="m" metricValue="1.4849999999999999" pageId="9" pageNumber="1946" unit="m" value="1485.0">1485 m</quantity>
