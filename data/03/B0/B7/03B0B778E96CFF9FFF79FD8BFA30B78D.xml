<number box="[775,830,1941,1967]" pageId="1" pageNumber="46" value="0.58">0.58</number>
; of mesoleg 
<number box="[984,1038,1941,1967]" pageId="1" pageNumber="46" value="2.77">2.77</number>
, 
<number box="[1052,1106,1941,1967]" pageId="1" pageNumber="46" value="2.88">2.88</number>
, and 
<number box="[1172,1227,1941,1967]" pageId="1" pageNumber="46" value="0.6">0.60</number>
