<location LSID="urn:lsid:plazi:treatment:03B05940FFBCFFB1FF50F972FC80D6C9:8EC6BE8DFFBCFFB3FF3BF882FE46D026" box="[252,394,1870,1895]" country="China" county="Guangzhou City" municipality="Conghua District" name="Mt. Guifeng" pageId="16" pageNumber="264" stateProvince="Guangdong">Mt. Guifeng</location>
