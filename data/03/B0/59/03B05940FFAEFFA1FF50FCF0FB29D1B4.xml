<collectingMunicipality box="[245,401,1357,1382]" pageId="2" pageNumber="250">Nanlyu Town</collectingMunicipality>
, 
<location LSID="urn:lsid:plazi:treatment:03B05940FFAEFFA1FF50FCF0FB29D1B4:8EC6BE8DFFAEFFA1FE5BFA80FD5ED227" box="[412,658,1357,1382]" country="China" county="Tunchang County" municipality="Nanlyu Town" name="Zhenghong Reservoir" pageId="2" pageNumber="250" stateProvince="Hainan">Zhenghong Reservoir</location>
