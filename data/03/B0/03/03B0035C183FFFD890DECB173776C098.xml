<paragraph blockId="19.[794,1422,939,986]" pageId="19" pageNumber="90">
<taxonomicName authority="Lewis, 1998 b" authorityName="Lewis" authorityYear="1998" box="[794,1129,939,960]" class="Malacostraca" family="Armadillidae" genus="Cubaris" kingdom="Animalia" order="Isopoda" pageId="19" pageNumber="90" phylum="Arthropoda" rank="species" species="granulatus">
<emphasis box="[794,987,939,960]" italics="true" pageId="19" pageNumber="90">Cubaris granulatus</emphasis>
<bibRefCitation author="Lewis, F" box="[995,1129,939,960]" pageId="19" pageNumber="90" pagination="743 - 777" refId="ref15619" refString="Lewis, F., 1998 b. Oniscidea (Isopoda) from Lord Howe Island. Crustaceana 71 (7): 743 - 777." type="journal article" year="1998">Lewis, 1998b</bibRefCitation>
</taxonomicName>
(not 
<taxonomicName authority="Collinge, 1915 b" authorityName="Collinge" authorityYear="1915" class="Malacostraca" family="Armadillidae" genus="Cubaris" kingdom="Animalia" order="Isopoda" pageId="19" pageNumber="90" phylum="Arthropoda" rank="species" species="granulatus">
<emphasis box="[1186,1323,939,960]" italics="true" pageId="19" pageNumber="90">C. granulatus</emphasis>
