<typeStatus box="[820,922,1386,1412]" pageId="33" pageNumber="34">holotype</typeStatus>
.
</paragraph>
</subSubSection>
<subSubSection box="[199,1214,1422,1449]" pageId="33" pageNumber="34" type="distribution">
<paragraph blockId="33.[151,1437,162,1449]" box="[199,1214,1422,1449]" pageId="33" pageNumber="34">
