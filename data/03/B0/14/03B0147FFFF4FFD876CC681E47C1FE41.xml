<taxonomicName authorityName="Cairns" authorityYear="2018" box="[382,465,622,647]" class="Anthozoa" family="Primnoidae" genus="Narella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Alcyonacea" pageId="26" pageNumber="27" phylum="Cnidaria" rank="species" species="virgosa">
