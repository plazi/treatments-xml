<emphasis bold="true" box="[596,705,1895,1920]" pageId="23" pageNumber="24">AFRICA</emphasis>
: 
<emphasis bold="true" box="[721,869,1895,1920]" pageId="23" pageNumber="24">
<collectingCountry box="[721,869,1895,1920]" name="Madagascar" pageId="23" pageNumber="24">Madagascar</collectingCountry>
