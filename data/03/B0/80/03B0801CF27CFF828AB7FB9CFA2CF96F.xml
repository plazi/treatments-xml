<taxonomicName authority="Demoulin 1966" authorityName="Demoulin" authorityYear="1966" box="[151,548,1123,1150]" class="Insecta" family="Baetidae" genus="Camelobaetidius" kingdom="Animalia" order="Ephemeroptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
