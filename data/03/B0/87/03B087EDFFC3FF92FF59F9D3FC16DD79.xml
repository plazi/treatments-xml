<quantity box="[410,488,1975,2002]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="10" pageNumber="471" unit="mm" value="20.0">20 µm</quantity>
; 
<collectionCode box="[500,600,1975,2002]" country="United Kingdom" httpUri="http://grbio.org/cool/2x42-q9ch" name="Ulster Museum, Belfast" pageId="10" pageNumber="471">BELUM</collectionCode>
