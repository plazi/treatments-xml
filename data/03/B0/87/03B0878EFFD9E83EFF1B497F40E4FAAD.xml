<taxonomicName authorityName="Xu and He" authorityYear="2006" box="[182,296,1421,1446]" class="Insecta" family="Bethylidae" genus="Odontepyris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="52" phylum="Arthropoda" rank="species" species="ovatus">
