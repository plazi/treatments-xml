<paragraph blockId="2.[151,783,1951,1975]" box="[151,783,1951,1975]" pageId="2" pageNumber="385">
<taxonomicName authority="Anderson 2009: 117" authorityName="Anderson" authorityPageNumber="117" authorityYear="2009" box="[151,561,1951,1975]" class="Insecta" family="Curculionidae" genus="Lymantes" kingdom="Animalia" order="Coleoptera" pageId="2" pageNumber="385" phylum="Arthropoda" rank="species" species="nadineae">
