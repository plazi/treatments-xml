<taxonomicName authority="Fabricius, 1787" authorityName="Fabricius" authorityYear="1787" box="[264,727,1708,1734]" class="Insecta" family="Reduviidae" genus="Manicocoris" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="rufipes">
<emphasis bold="true" box="[264,727,1708,1734]" pageId="23" pageNumber="24">
<emphasis bold="true" box="[264,507,1708,1734]" italics="true" pageId="23" pageNumber="24">Manicocoris rufipes</emphasis>
