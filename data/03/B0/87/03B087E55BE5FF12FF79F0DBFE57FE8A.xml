<quantity box="[1071,1160,223,247]" metricMagnitude="3" metricUnit="m" metricValue="1.22" pageId="129" pageNumber="130" unit="m" value="1220.0">1220 m</quantity>
) and McAdam National Park (
<quantity box="[216,346,260,284]" metricMagnitude="2" metricUnit="m" metricValue="8.0" metricValueMax="9.0" metricValueMin="7.0" pageId="129" pageNumber="130" unit="m" value="800.0" valueMax="900.0" valueMin="700.0">700–900 m</quantity>
