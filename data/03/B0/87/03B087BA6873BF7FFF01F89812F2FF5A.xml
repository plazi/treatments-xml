<taxonomicName authority="(Watts)" authorityName="Ruta &amp; Yoshitomi" authorityYear="2010" baseAuthorityName="Watts" baseAuthorityYear="2004" box="[572,858,152,178]" class="Insecta" family="Scirtidae" genus="Exochomoscirtes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="ruforotundus">
