<taxonomicName authorityName="Ruta &amp; Yoshitomi" authorityYear="2010" baseAuthorityName="Watts" baseAuthorityYear="2004" box="[151,335,1930,1955]" class="Insecta" family="Scirtidae" genus="Exochomoscirtes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="species" species="ruforotundus">
