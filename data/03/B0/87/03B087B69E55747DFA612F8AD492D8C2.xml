<taxonomicName authority="Kirkaldy, 1859" authorityName="Kirkaldy" authorityYear="1859" box="[395,731,1068,1094]" class="Insecta" family="Reduviidae" genus="Brontostoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="3" pageNumber="4" phylum="Arthropoda" rank="genus">
