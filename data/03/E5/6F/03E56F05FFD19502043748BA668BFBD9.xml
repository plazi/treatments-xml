<paragraph blockId="12.[264,1323,798,1868]" pageId="12" pageNumber="13">
Total length 
<number box="[461,581,919,945]" pageId="12" pageNumber="13" value="6.275" valueMax="6.35" valueMin="6.2">6.20–6.35</number>
, 
<quantity box="[594,702,919,945]" metricMagnitude="-3" metricUnit="m" metricValue="6.28" pageId="12" pageNumber="13" unit="mm" value="6.28">6.28 mm</quantity>
, head capsule 
