<number box="[1101,1133,1827,1853]" pageId="4" pageNumber="5" value="38.0">38</number>
; 
<number box="[1151,1319,1827,1853]" pageId="4" pageNumber="5" value="65118.5" valueMax="130127.0" valueMin="110.0">110–130, 127</number>
