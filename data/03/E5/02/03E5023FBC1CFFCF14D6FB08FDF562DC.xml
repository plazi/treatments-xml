<emphasis bold="true" box="[195,636,1244,1265]" pageId="7">
<taxonomicName authority="Gimmel" authorityName="Gimmel" authorityYear="2013" box="[195,505,1244,1265]" class="Gastropoda" family="Bulimulidae" genus="Nesiotus" kingdom="Animalia" order="Stylommatophora" pageId="7" phylum="Mollusca" rank="species" species="joebranhami" status="new species">
<emphasis bold="true" box="[195,413,1244,1265]" italics="true" pageId="7">Nesiotus joebranhami</emphasis>
Gimmel
</taxonomicName>
, 
<taxonomicNameLabel box="[518,636,1244,1265]" pageId="7" rank="species">new species</taxonomicNameLabel>
</emphasis>
