<quantity box="[678,787,1245,1269]" metricMagnitude="2" metricUnit="m" metricValue="5.175" metricValueMax="5.35" metricValueMin="5.0" pageId="53" pageNumber="320" unit="m" value="517.5" valueMax="535.0" valueMin="500.0">500-535 m</quantity>
, 
<date box="[795,908,1245,1269]" pageId="53" pageNumber="320" value="1989-03-10">10.III.1989</date>
