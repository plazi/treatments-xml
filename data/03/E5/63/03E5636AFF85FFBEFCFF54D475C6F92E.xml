<tr box="[113,1135,499,519]" gridrow="8" pageId="26" pageNumber="293" rowspan-1="1" rowspan-2="1">
<td box="[113,441,499,519]" gridcol="0" gridrow="8" pageId="26" pageNumber="293">Nematocysts</td>
</tr>
