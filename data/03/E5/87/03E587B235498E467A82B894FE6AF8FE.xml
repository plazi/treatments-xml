<materialsCitation ID-GBIF-Occurrence="3705866302" elevation="4000" location="Shigatse" municipality="Nyang Chu Valley" pageId="14" pageNumber="516" specimenCount="1" stateProvince="Xizang" typeStatus="holotype">
