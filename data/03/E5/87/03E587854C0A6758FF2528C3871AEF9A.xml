<taxonomicName class="Insecta" family="Tenebrionidae" genus="Xerolinus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="26" phylum="Arthropoda" rank="species" species="altutaceus">
