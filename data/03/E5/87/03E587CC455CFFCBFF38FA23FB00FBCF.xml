<collectionCode box="[368,428,1529,1555]" httpUri="http://grbio.org/cool/4b7r-7sec" name="Bavarian State Collection of Zoology" pageId="4" pageNumber="52">ZSM</collectionCode>
<number box="[436,493,1529,1555]" pageId="4" pageNumber="52" value="1592.0">1592</number>
/ 
<number box="[502,561,1529,1555]" pageId="4" pageNumber="52" value="2007.0">2007</number>
