<emphasis box="[317,375,869,894]" italics="true" pageId="7" pageNumber="66">et al.</emphasis>
, 
<number box="[390,453,869,895]" pageId="7" pageNumber="66" value="2006.0">2006</number>
</bibRefCitation>
