<taxonomicName box="[535,753,961,985]" class="Adenophorea" family="Belondiridae" genus="Syncheilaxonchium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="34" pageNumber="35" phylum="Nematoda" rank="genus">
<emphasis box="[535,753,961,985]" italics="true" pageId="34" pageNumber="35">Syncheilaxonchium</emphasis>
</taxonomicName>
; 
<emphasis box="[767,928,961,985]" italics="true" pageId="34" pageNumber="35">
<taxonomicName box="[767,924,961,985]" class="Adenophorea" family="Belondiridae" genus="Syncheilaxonchium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dorylaimida" pageId="34" pageNumber="35" phylum="Nematoda" rank="species" species="perplexans">A. perplexans</taxonomicName>
