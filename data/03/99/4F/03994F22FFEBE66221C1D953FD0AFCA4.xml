</taxonomicName>
; 
<taxonomicName authority="Siddiqi, 1964" authorityName="Siddiqi" authorityYear="1964" box="[1104,1436,784,809]" class="Adenophorea" family="Belondiridae" genus="Axonchium" kingdom="Animalia" order="Dorylaimida" pageId="34" pageNumber="35" phylum="Nematoda" rank="species" species="indicum" subGenus="Syncheilaxonchium">
