<subSubSection box="[2066,2623,1438,1467]" pageId="3" pageNumber="186" type="multiple">
<caption box="[2066,2623,1438,1467]" inLine="true" pageId="3" pageNumber="186" targetBox="[1450,2042,1288,1702]" targetPageId="3">
<paragraph blockId="3.[2066,2659,1285,1704]" box="[2066,2623,1438,1467]" pageId="3" pageNumber="186">
<emphasis bold="true" box="[2066,2242,1438,1467]" pageId="3" pageNumber="186">Distribution.</emphasis>
