<number box="[1239,1278,792,818]" pageId="27" pageNumber="28" value="1.5" valueMax="2.0" valueMin="1.0">1-2</number>
/ 
<number box="[1290,1332,792,818]" pageId="27" pageNumber="28" value="1.2">1, 2</number>
/ 
<number box="[1343,1381,792,818]" pageId="27" pageNumber="28" value="0.5" valueMax="1.0" valueMin="0.0">0-1</number>
.
</paragraph>
