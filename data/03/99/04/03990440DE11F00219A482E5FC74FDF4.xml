<number box="[982,1012,1910,1936]" pageId="6" pageNumber="7" value="26.0">26</number>
(
<number box="[1020,1096,1910,1936]" pageId="6" pageNumber="7" value="25.5" valueMax="26.0" valueMin="25.0">25–26</number>
