, 
<taxonomicName authorityName="Bharti &amp; Kumar" authorityYear="2013" box="[1061,1281,1415,1439]" class="Insecta" family="Formicidae" genus="Dilobocondyla" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="137" phylum="Arthropoda" rank="species" species="gasteroreticulata">
