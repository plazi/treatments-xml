</taxonomicName>
, 
<taxonomicName authorityName="Zettel &amp; Bruckner" authorityYear="2013" box="[467,595,1415,1438]" class="Insecta" family="Formicidae" genus="Dilobocondyla" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="137" phylum="Arthropoda" rank="species" species="carinata">
<emphasis box="[467,595,1415,1438]" italics="true" pageId="5" pageNumber="137">D. carinata</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Bharti &amp; Kumar" authorityYear="2013" box="[615,731,1415,1439]" class="Insecta" family="Formicidae" genus="Dilobocondyla" kingdom="Animalia" order="Hymenoptera" pageId="5" pageNumber="137" phylum="Arthropoda" rank="species" species="eguchii">
