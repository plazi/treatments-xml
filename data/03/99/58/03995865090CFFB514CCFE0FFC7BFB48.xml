<collectingRegion box="[151,323,852,878]" country="Brazil" name="Rio de Janeiro" pageId="45" pageNumber="46">Rio de Janeiro</collectingRegion>
, 
<collectingRegion box="[337,457,852,878]" country="Brazil" name="Sao Paulo" pageId="45" pageNumber="46">São Paulo</collectingRegion>
, 
<collectingRegion box="[471,550,852,878]" country="Brazil" name="Parana" pageId="45" pageNumber="46">Paraná</collectingRegion>
, Santa Catarina, 
<collectingRegion box="[750,971,852,879]" country="Brazil" name="Rio Grande do Sul" pageId="45" pageNumber="46">Rio Grande do Sul</collectingRegion>
) (
<bibRefCitation author="Rathbun, M. J." box="[996,1163,852,878]" pageId="45" pageNumber="46" pagination="1 - 461" refId="ref103657" refString="Rathbun, M. J. (1918) The grapsoid crabs of America. Bulletin of the United States National Museum, 97, 1 - 461. https: // doi. org / 10.5479 / si. 03629236.97. i" type="journal article" year="1918">Rathbun 1918</bibRefCitation>
; 
<bibRefCitation author="Crane, J." box="[1178,1314,852,879]" pageId="45" pageNumber="46" refId="ref90525" refString="Crane, J. (1975) Fiddler crabs of the world: Ocypodidae: genus Uca. Princeton University Press, Princeton, New Jersy, 766 pp." type="book" year="1975">Crane 1975</bibRefCitation>
; 
<bibRefCitation author="Barros, M. P. de &amp; Pimentel, F. R." pageId="45" pageNumber="46" pagination="15 - 41" refId="ref86601" refString="Barros, M. P. de &amp; Pimentel, F. R. (2001) A fauna de decapoda (Crustacea) do estado do Para, Brasil: lista preliminar das especies. Boletim do Museu Paraense Emilio Goeldi, Serie Zoologia, 17 (1), 15 - 41." type="journal article" year="2001">Barros &amp; Pimentel 2001</bibRefCitation>
; 
<bibRefCitation author="Bezerra, L. E. A." box="[329,483,888,915]" pageId="45" pageNumber="46" pagination="203 - 246" refId="ref87153" refString="Bezerra, L. E. A. (2012) The fiddler crabs (Crustacea: Brachyura: Ocypodidae: genus Uca) of the South Atlantic Ocean. Nauplius, 20 (2), 203 - 246. https: // doi. org / 10.1590 / S 0104 - 64972012000200011" type="journal article" year="2012">Bezerra 2012</bibRefCitation>
; 
<bibRefCitation author="Masunari, S. &amp; Martins, S. B. &amp; Anacleto, A. F. M." box="[493,730,888,915]" pageId="45" pageNumber="46" pagination="1 - 20" refId="ref98997" refString="Masunari, S., Martins, S. B. &amp; Anacleto, A. F. M. (2020) An illustrated key to the fiddler crabs (Crustacea, Decapoda, Ocypodidae) from the Atlantic coast of Brazil. ZooKeys, 943, 1 - 20. https: // doi. org / 10.3897 / zookeys. 943.52773" type="journal article" year="2020">
