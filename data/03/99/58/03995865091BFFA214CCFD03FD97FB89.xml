<paragraph blockId="58.[151,1437,824,1139]" pageId="58" pageNumber="59">
<emphasis bold="true" box="[199,353,860,886]" pageId="58" pageNumber="59">Distribution.</emphasis>
Western Atlantic—Brazil (
<collectingRegion box="[655,819,860,887]" country="Brazil" name="Espirito Santo" pageId="58" pageNumber="59">Espírito Santo</collectingRegion>
, 
<collectingRegion box="[829,993,860,886]" country="Brazil" name="Rio de Janeiro" pageId="58" pageNumber="59">Rio de Janeiro</collectingRegion>
, 
<collectingRegion box="[1003,1119,860,887]" country="Brazil" name="Sao Paulo" pageId="58" pageNumber="59">São Paulo</collectingRegion>
, 
<collectingRegion box="[1129,1208,861,887]" country="Brazil" name="Parana" pageId="58" pageNumber="59">Paraná</collectingRegion>
, 
<collectingRegion box="[1219,1385,860,887]" country="Cape Verde" name="Santa Catarina" pageId="58" pageNumber="59">Santa Catarina</collectingRegion>
, 
<collectingRegion country="Brazil" name="Rio Grande do Sul" pageId="58" pageNumber="59">Rio Grande do Sul</collectingRegion>
), mostly between 
<geoCoordinate box="[531,586,897,923]" degrees="19" direction="south" orientation="latitude" pageId="58" pageNumber="59" precision="55555" value="-19.0">19°S</geoCoordinate>
