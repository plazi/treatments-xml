<collectingCountry box="[465,553,649,675]" name="Mexico" pageId="30" pageNumber="31">Mexico</collectingCountry>
, and 
<collectingCountry box="[613,674,649,675]" name="Chile" pageId="30" pageNumber="31">Chile</collectingCountry>
(
<bibRefCitation author="Melo, G. A. S." box="[688,815,649,675]" pageId="30" pageNumber="31" refId="ref99429" refString="Melo, G. A. S. (1996) Manual de identificacao dos Brachyura (caranguejos e siris) do Litoral Brasileiro. Editora, Pleiade, Sao Paulo, 604 pp." type="book" year="1996">Melo 1996</bibRefCitation>
; Bertini &amp; Fransozo 2004; Bertini 
<emphasis box="[1207,1264,649,675]" italics="true" pageId="30" pageNumber="31">et al.</emphasis>
