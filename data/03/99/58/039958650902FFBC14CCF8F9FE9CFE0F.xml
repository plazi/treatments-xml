<collectingRegion box="[1275,1360,187,213]" country="Brazil" name="Amapa" pageId="36" pageNumber="37">Amapá</collectingRegion>
, Pará, 
<collectingRegion box="[151,294,223,249]" country="Brazil" name="Pernambuco" pageId="36" pageNumber="37">Pernambuco</collectingRegion>
, 
<collectingRegion box="[305,468,223,249]" country="Brazil" name="Rio de Janeiro" pageId="36" pageNumber="37">Rio de Janeiro</collectingRegion>
