<collectingCountry box="[1112,1227,542,568]" name="Argentina" pageId="37" pageNumber="38">Argentina</collectingCountry>
. Eastern Pacific— 
<collectingCountry box="[151,205,578,604]" name="United States of America" pageId="37" pageNumber="38">USA</collectingCountry>
(
<collectingRegion box="[219,336,578,605]" country="United States of America" name="California" pageId="37" pageNumber="38">California</collectingRegion>
) to 
<collectingCountry box="[377,491,578,604]" name="Colombia" pageId="37" pageNumber="38">Colombia</collectingCountry>
. Indo-West Pacific— 
<collectingCountry box="[733,800,578,604]" name="Japan" pageId="37" pageNumber="38">Japan</collectingCountry>
, 
<collectingCountry box="[810,881,578,604]" name="South Korea" pageId="37" pageNumber="38">Korea</collectingCountry>
