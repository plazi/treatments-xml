<taxonomicName baseAuthorityName="Fabricius" baseAuthorityYear="1793" box="[1224,1436,187,213]" class="Malacostraca" family="Raninidae" genus="Symethis" kingdom="Animalia" order="Decapoda" pageId="68" pageNumber="69" phylum="Arthropoda" rank="species" species="variolosa">
