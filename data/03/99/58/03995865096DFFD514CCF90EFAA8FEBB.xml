<paragraph blockId="76.[151,1436,1807,2014]" pageId="76" pageNumber="77">
<emphasis bold="true" box="[199,353,1843,1869]" pageId="76" pageNumber="77">Distribution.</emphasis>
WesternAtlantic—Bermuda, 
<collectingCountry box="[684,738,1844,1870]" name="United States of America" pageId="76" pageNumber="77">USA</collectingCountry>
(
<collectingRegion box="[751,922,1844,1870]" country="United States of America" name="North Carolina" pageId="76" pageNumber="77">North Carolina</collectingRegion>
, Florida), Gulf of Mexico, 
<collectingCountry box="[1215,1333,1844,1870]" name="Venezuela" pageId="76" pageNumber="77">Venezuela</collectingCountry>
, Antilles, 
<collectingCountry box="[151,261,1880,1906]" name="Suriname" pageId="76" pageNumber="77">Suriname</collectingCountry>
, and 
<collectingCountry box="[319,387,1880,1906]" name="Brazil" pageId="76" pageNumber="77">Brazil</collectingCountry>
(
<collectingRegion box="[402,491,1880,1906]" country="Brazil" name="Sergipe" pageId="76" pageNumber="77">Sergipe</collectingRegion>
, 
<collectingRegion box="[502,665,1880,1906]" country="Brazil" name="Espirito Santo" pageId="76" pageNumber="77">Espírito Santo</collectingRegion>
, 
<collectingRegion box="[675,836,1880,1906]" country="Brazil" name="Rio de Janeiro" pageId="76" pageNumber="77">Rio de Janeiro</collectingRegion>
to 
<collectingRegion box="[871,1081,1880,1907]" country="Brazil" name="Rio Grande do Sul" pageId="76" pageNumber="77">Rio Grande do Sul</collectingRegion>
