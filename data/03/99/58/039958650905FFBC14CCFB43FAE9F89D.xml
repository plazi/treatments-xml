<collectingCountry box="[151,250,1725,1751]" name="Uruguay" pageId="36" pageNumber="37">Uruguay</collectingCountry>
, and 
<collectingCountry box="[310,425,1724,1750]" name="Argentina" pageId="36" pageNumber="37">Argentina</collectingCountry>
