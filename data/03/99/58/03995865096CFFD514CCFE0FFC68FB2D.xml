<collectingRegion box="[404,569,1068,1094]" country="Brazil" name="Rio de Janeiro" pageId="77" pageNumber="78">Rio de Janeiro</collectingRegion>
, 
<collectingRegion box="[579,697,1068,1095]" country="Brazil" name="Sao Paulo" pageId="77" pageNumber="78">São Paulo</collectingRegion>
