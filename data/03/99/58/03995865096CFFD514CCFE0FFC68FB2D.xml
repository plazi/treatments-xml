<subSubSection pageId="77" pageNumber="78" type="distribution">
<paragraph blockId="77.[151,1437,564,1239]" pageId="77" pageNumber="78">
<emphasis bold="true" box="[199,353,996,1022]" pageId="77" pageNumber="78">Distribution.</emphasis>
WesternAtlantic—Bermuda, 
<collectingCountry box="[684,738,996,1022]" name="United States of America" pageId="77" pageNumber="78">USA</collectingCountry>
(Florida), 
<collectingCountry box="[855,960,996,1022]" name="Bahamas" pageId="77" pageNumber="78">Bahamas</collectingCountry>
, Gulf of Mexico, 
<collectingCountry box="[1151,1263,996,1022]" name="Honduras" pageId="77" pageNumber="78">Honduras</collectingCountry>
, 
<collectingCountry box="[1272,1364,997,1023]" name="Panama" pageId="77" pageNumber="78">Panama</collectingCountry>
, 
<collectingCountry name="Costa Rica" pageId="77" pageNumber="78">Costa Rica</collectingCountry>
, Antilles, 
<collectingCountry box="[322,435,1032,1058]" name="Colombia" pageId="77" pageNumber="78">Colombia</collectingCountry>
, and 
<collectingCountry box="[498,566,1032,1058]" name="Brazil" pageId="77" pageNumber="78">Brazil</collectingCountry>
(Fernando de Noronha, 
<collectingRegion box="[850,918,1032,1058]" country="Brazil" name="Ceara" pageId="77" pageNumber="78">Ceará</collectingRegion>
, 
<collectingRegion box="[931,1176,1032,1059]" country="Brazil" name="Rio Grande do Norte" pageId="77" pageNumber="78">Rio Grande do Norte</collectingRegion>
, 
<collectingRegion box="[1189,1276,1032,1058]" country="Brazil" name="Paraiba" pageId="77" pageNumber="78">Paraíba</collectingRegion>
, 
<collectingRegion box="[1289,1432,1032,1058]" country="Brazil" name="Pernambuco" pageId="77" pageNumber="78">Pernambuco</collectingRegion>
