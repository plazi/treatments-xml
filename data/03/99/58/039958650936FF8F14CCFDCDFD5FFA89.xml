, 
<collectingRegion box="[472,634,1225,1251]" country="Brazil" name="Rio de Janeiro" pageId="23" pageNumber="24">Rio de Janeiro</collectingRegion>
, 
<collectingRegion box="[644,759,1225,1251]" country="Brazil" name="Sao Paulo" pageId="23" pageNumber="24">São Paulo</collectingRegion>
