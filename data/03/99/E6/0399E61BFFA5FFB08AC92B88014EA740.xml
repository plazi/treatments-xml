<taxonomicName authority="Cobos, 1955" authorityName="Cobos" authorityYear="1955" box="[287,595,767,792]" class="Insecta" family="Eucnemidae" genus="Golbachia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="3" pageNumber="2" phylum="Arthropoda" rank="genus">
