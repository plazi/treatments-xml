<collectingRegion box="[695,908,637,661]" country="India" name="Arunachal Pradesh" pageId="75" pageNumber="1579">Arunachal Pradesh</collectingRegion>
(
<collectingCountry box="[923,985,637,661]" name="India" pageId="75" pageNumber="1579">India</collectingCountry>
