<typeStatus box="[528,576,296,320]" pageId="8" pageNumber="159">type</typeStatus>
locality in 
<collectionCode LSID="urn:lsid:biocol.org:col:35271" box="[701,786,296,320]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:35271" name="Royal Belgian Institute of Natural Sciences" pageId="8" pageNumber="159">RBINS</collectionCode>
, No. IG 
<number box="[881,954,296,320]" pageId="8" pageNumber="159" value="33082.0">33082</number>
, RIR 
<number box="[1017,1062,296,320]" pageId="8" pageNumber="159" value="247.0">247</number>
; 
<number box="[1073,1087,295,320]" pageId="8" pageNumber="159" value="5.0">5</number>
mounted 
