<collectingCountry box="[571,671,260,285]" name="Belgium" pageId="8" pageNumber="159">Belgium</collectingCountry>
, No. IG 
<number box="[766,839,260,284]" pageId="8" pageNumber="159" value="33082.0">33082</number>
, RIR 
<number box="[904,950,260,284]" pageId="8" pageNumber="159" value="217.0">217</number>
