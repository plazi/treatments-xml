), Brussels, 
<collectingCountry box="[571,671,798,823]" name="Belgium" pageId="10" pageNumber="161">Belgium</collectingCountry>
, No. IG 
<number box="[766,839,799,823]" pageId="10" pageNumber="161" value="33082.0">33082</number>
, RIR 
<number box="[904,950,799,823]" pageId="10" pageNumber="161" value="218.0">218</number>
