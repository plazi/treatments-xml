<paragraph blockId="10.[151,1437,474,2011]" pageId="10" pageNumber="161">
<emphasis bold="true" box="[199,326,834,859]" pageId="10" pageNumber="161">
<typeStatus box="[199,321,834,859]" pageId="10" pageNumber="161">Paratypes</typeStatus>
.
