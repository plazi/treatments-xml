<subSubSection pageId="28" pageNumber="179" type="etymology">
<paragraph blockId="28.[151,1436,759,1072]" pageId="28" pageNumber="179">
<emphasis bold="true" box="[199,334,1012,1037]" pageId="28" pageNumber="179">Etymology.</emphasis>
