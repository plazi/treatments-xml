<paragraph blockId="1.[151,1437,735,1642]" box="[151,706,735,762]" pageId="1" pageNumber="65">
<emphasis bold="true" box="[151,306,735,761]" pageId="1" pageNumber="65">
<typeStatus box="[151,212,735,761]" pageId="1" pageNumber="65">Type</typeStatus>
species
</emphasis>
. 
<taxonomicName box="[320,543,736,761]" class="Insecta" family="Cerambycidae" genus="Bocainella" kingdom="Animalia" order="Coleoptera" pageId="1" pageNumber="65" phylum="Arthropoda" rank="species" species="minima" status="new species">
