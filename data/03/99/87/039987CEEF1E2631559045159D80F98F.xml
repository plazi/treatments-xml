<tr box="[151,1436,1358,1384]" gridrow="1" pageId="5" pageNumber="6">
<th box="[151,721,1358,1384]" gridcol="0" gridrow="1" pageId="5" pageNumber="6">A common species living in graminaceous</th>
<td box="[729,889,1358,1384]" gridcol="1" gridrow="1" pageId="5" pageNumber="6">savannas and</td>
<td box="[896,1196,1358,1384]" gridcol="2" gridrow="1" pageId="5" pageNumber="6">areas covered by grassy</td>
<td box="[1203,1334,1358,1384]" gridcol="3" gridrow="1" pageId="5" pageNumber="6">vegetation.</td>
<td box="[1345,1369,1358,1384]" gridcol="4" gridrow="1" pageId="5" pageNumber="6">In</td>
<td box="[1381,1436,1358,1384]" gridcol="5" gridrow="1" pageId="5" pageNumber="6">Mali</td>
