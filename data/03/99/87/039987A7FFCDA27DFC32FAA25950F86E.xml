<paragraph blockId="9.[151,756,1890,2074]" pageId="9" pageNumber="188">
<emphasis bold="true" box="[151,295,1891,1914]" pageId="9" pageNumber="188">Description:</emphasis>
<emphasis box="[312,377,1890,1914]" italics="true" pageId="9" pageNumber="188">Male:</emphasis>
