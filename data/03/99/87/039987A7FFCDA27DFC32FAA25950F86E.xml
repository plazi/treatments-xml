<number box="[560,641,1891,1914]" pageId="9" pageNumber="188" value="5.0" valueMax="5.3" valueMin="4.7">4.7-5.3</number>
; prosoma length: 
<number box="[233,312,1923,1947]" pageId="9" pageNumber="188" value="2.6500000000000004" valueMax="3.1" valueMin="2.2">2.2-3.1</number>
, width: 2.0- 
<number box="[443,479,1923,1947]" pageId="9" pageNumber="188" value="2.4">2.4</number>
. Leg I: femur 
<number box="[637,673,1923,1947]" pageId="9" pageNumber="188" value="3.3">3.3</number>
, patella 1.0, tibia 
<number box="[263,299,1955,1979]" pageId="9" pageNumber="188" value="3.3">3.3</number>
, metatarsus 
<number box="[441,477,1955,1979]" pageId="9" pageNumber="188" value="2.3">2.3</number>
, tarsus 
<number box="[567,604,1955,1979]" pageId="9" pageNumber="188" value="1.3">1.3</number>
