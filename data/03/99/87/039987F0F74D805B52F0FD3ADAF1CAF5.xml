) and 
<taxonomicName box="[526,610,1362,1387]" class="Agaricomycetes" family="Pluteaceae" genus="Pluteus" higherTaxonomySource="GBIF" kingdom="Fungi" order="Agaricales" pageId="27" pageNumber="328" phylum="Basidiomycota" rank="genus">
