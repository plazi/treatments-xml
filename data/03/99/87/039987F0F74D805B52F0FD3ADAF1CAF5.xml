), 
<taxonomicName box="[151,252,1364,1387]" class="Agaricomycetes" family="Tricholomataceae" genus="Laccaria" higherTaxonomySource="GBIF" kingdom="Fungi" order="Agaricales" pageId="27" pageNumber="328" phylum="Basidiomycota" rank="genus">
