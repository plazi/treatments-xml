<subSubSection pageId="8" pageNumber="45" type="etymology">
<paragraph blockId="8.[151,1436,1301,2023]" pageId="8" pageNumber="45">
<emphasis bold="true" box="[197,348,1958,1984]" pageId="8" pageNumber="45">Etymology.</emphasis>
