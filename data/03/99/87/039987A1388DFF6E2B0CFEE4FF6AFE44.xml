<taxonomicName authorityName="Semenov" authorityYear="1891" box="[202,325,691,717]" class="Insecta" family="Tenebrionidae" genus="Reitterella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="159" pageNumber="160" phylum="Arthropoda" rank="genus">
