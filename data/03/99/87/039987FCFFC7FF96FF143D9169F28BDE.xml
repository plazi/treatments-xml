<number box="[1106,1120,1267,1291]" pageId="10" pageNumber="87" value="1.0">1</number>
macrochaeta (p 
<number box="[1299,1307,1281,1295]" pageId="10" pageNumber="87" value="3.0">3</number>
); Abd. I–V with 
<number box="[208,222,1306,1330]" pageId="10" pageNumber="87" value="3.0">3</number>
(m 
<number box="[260,269,1320,1334]" pageId="10" pageNumber="87" value="2.0">2</number>
, m 
<number box="[298,306,1320,1334]" pageId="10" pageNumber="87" value="3.0">3</number>
, m 
<number box="[341,349,1320,1334]" pageId="10" pageNumber="87" value="4.0">4</number>
), 
<number box="[373,387,1306,1330]" pageId="10" pageNumber="87" value="3.0">3</number>
(m 
<number box="[425,434,1320,1334]" pageId="10" pageNumber="87" value="2.0">2</number>
, m 
<number box="[463,471,1320,1334]" pageId="10" pageNumber="87" value="3.0">3</number>
, m 
<number box="[507,515,1320,1334]" pageId="10" pageNumber="87" value="4.0">4</number>
), 
<number box="[538,552,1306,1330]" pageId="10" pageNumber="87" value="4.0">4</number>
(m 
<number box="[590,599,1320,1334]" pageId="10" pageNumber="87" value="2.0">2</number>
, m 
<number box="[628,636,1320,1334]" pageId="10" pageNumber="87" value="6.0">6</number>
, p 
<number box="[664,672,1320,1334]" pageId="10" pageNumber="87" value="1.0">1</number>
, p 
<number box="[697,705,1320,1334]" pageId="10" pageNumber="87" value="6.0">6</number>
), 
<number box="[728,742,1306,1330]" pageId="10" pageNumber="87" value="1.0">1</number>
(E 
<number box="[776,784,1320,1334]" pageId="10" pageNumber="87" value="3.0">3</number>
) 
<number box="[800,814,1306,1330]" pageId="10" pageNumber="87" value="3.0">3</number>
(p 
<number box="[845,853,1320,1334]" pageId="10" pageNumber="87" value="2.0">2</number>
, p 
<number box="[881,889,1320,1334]" pageId="10" pageNumber="87" value="3.0">3</number>
, p 
<number box="[917,925,1320,1334]" pageId="10" pageNumber="87" value="4.0">4</number>
