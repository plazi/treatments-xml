(
<bibRefCitation author="Deane C." box="[542,695,1964,1990]" pageId="67" pageNumber="68" pagination="227 - 242" refId="ref59470" refString="Deane C. 1931. Trichopterygidae of Australia and Adjacent Islands. Descriptions of five new genera and twenty new species. Proceedings of the Linnean Society of New South Wales 227 (3): 227 - 242." type="journal article" year="1931">Deane, 1931</bibRefCitation>
)
