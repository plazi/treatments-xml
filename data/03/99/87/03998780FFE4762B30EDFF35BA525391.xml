<paragraph blockId="2.[166,621,175,202]" box="[166,621,175,202]" pageId="2" pageNumber="13">
<heading box="[166,621,175,202]" fontSize="36" level="2" pageId="2" pageNumber="13" reason="3">
<taxonomicName authority="(Dahlbom, 1843)" baseAuthorityName="Dahlbom" baseAuthorityYear="1843" box="[166,621,175,202]" class="Insecta" family="Crabronidae" genus="Psenulus" kingdom="Animalia" order="Hymenoptera" pageId="2" pageNumber="13" phylum="Arthropoda" rank="species" species="fuscipennis">
<emphasis bold="true" box="[166,430,175,202]" pageId="2" pageNumber="13">
<emphasis bold="true" box="[166,413,175,202]" italics="true" pageId="2" pageNumber="13">Psenulus fuscipennis</emphasis>
(
</emphasis>
Dahlbom, 1843
<emphasis bold="true" box="[611,621,175,201]" pageId="2" pageNumber="13">)</emphasis>
</taxonomicName>
