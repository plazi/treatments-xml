<subSubSection pageId="18" pageNumber="19" type="distribution">
<paragraph blockId="18.[264,1324,839,1824]" pageId="18" pageNumber="19">
<emphasis box="[312,455,1639,1664]" italics="true" pageId="18" pageNumber="19">Distribution</emphasis>
. 
<taxonomicName box="[472,769,1639,1664]" class="Insecta" family="Lycidae" genus="Dilophotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="lizipingensis" status="sp. n.">
<emphasis box="[472,769,1639,1664]" italics="true" pageId="18" pageNumber="19">Dilophotes lizipingensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[778,843,1638,1664]" pageId="18" pageNumber="19" rank="species">sp. n.</taxonomicNameLabel>
