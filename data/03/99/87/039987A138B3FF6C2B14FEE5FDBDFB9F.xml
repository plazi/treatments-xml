<taxonomicName authorityName="Darby" authorityYear="2015" box="[826,918,969,995]" class="Insecta" family="Ptiliidae" genus="Cissidium" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Coleoptera" pageId="162" pageNumber="163" phylum="Arthropoda" rank="species" species="lisae">
