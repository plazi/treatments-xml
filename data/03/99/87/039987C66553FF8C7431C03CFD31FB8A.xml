<subSubSection pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[264,1324,284,1070]" pageId="21" pageNumber="22">
<emphasis box="[312,435,1004,1029]" italics="true" pageId="21" pageNumber="22">Etymology</emphasis>
