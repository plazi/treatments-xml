<paragraph blockId="17.[151,1436,1896,1979]" pageId="17" pageNumber="126">
<emphasis bold="true" box="[151,290,1896,1918]" pageId="17" pageNumber="126">FIGURE 11.</emphasis>
<taxonomicName box="[301,517,1896,1918]" class="Maxillopoda" family="Pseudanthessiidae" genus="Pseudanthessius" kingdom="Animalia" order="Poecilostomatoida" pageId="17" pageNumber="126" phylum="Arthropoda" rank="species" species="imo" status="n. sp.">
<emphasis box="[301,517,1896,1918]" italics="true" pageId="17" pageNumber="126">Pseudanthessius imo</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[527,589,1896,1917]" pageId="17" pageNumber="126" rank="species">
