<taxonomicName box="[1258,1333,1234,1259]" class="Insecta" family="Notodontidae" genus="Nerice" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="121" phylum="Arthropoda" rank="subGenus" subGenus="Nerice">
