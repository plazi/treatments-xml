<emphasis bold="true" box="[932,1126,1162,1186]" pageId="14" pageNumber="143">
<taxonomicName authority="Löbl, 2015" authorityName="Löbl" authorityYear="2015" box="[932,1034,1162,1186]" class="Insecta" family="Staphylinidae" genus="Scaphisoma" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="143" phylum="Arthropoda" rank="species" species="tarsale" status="sp. nov.">
