<taxonomicName authority="BONAPARTE, 1838" authorityName="BONAPARTE" authorityYear="1838" box="[1005,1308,951,974]" class="Amphibia" family="Pelodytidae" genus="Pelodytes" kingdom="Animalia" order="Anura" pageId="3" pageNumber="56" phylum="Chordata" rank="genus">
