<subSubSection pageId="5" pageNumber="40" type="etymology">
<paragraph blockId="5.[151,1437,487,1480]" pageId="5" pageNumber="40">
<emphasis bold="true" box="[197,339,1415,1441]" pageId="5" pageNumber="40">Etymology.</emphasis>
