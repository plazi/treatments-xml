<paragraph blockId="16.[151,756,1988,2079]" lastBlockId="16.[831,1437,257,345]" pageId="16" pageNumber="195">
<subSubSection pageId="16" pageNumber="195" type="diagnosis">
<emphasis bold="true" box="[151,271,1989,2012]" pageId="16" pageNumber="195">Diagnosis:</emphasis>
Similar to 
<taxonomicName box="[408,564,1988,2012]" class="Arachnida" family="Thomisidae" genus="Cebrenninus" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="195" phylum="Arthropoda" rank="species" species="kalawitana">
<emphasis box="[408,564,1988,2012]" italics="true" pageId="16" pageNumber="195">C. kalawitana</emphasis>
</taxonomicName>
and 
<taxonomicName box="[627,749,1988,2012]" class="Arachnida" family="Thomisidae" genus="Cebrenninus" kingdom="Animalia" order="Araneae" pageId="16" pageNumber="195" phylum="Arthropoda" rank="species" species="rugosus">
<emphasis box="[627,749,1988,2012]" italics="true" pageId="16" pageNumber="195">C. rugosus</emphasis>
</taxonomicName>
