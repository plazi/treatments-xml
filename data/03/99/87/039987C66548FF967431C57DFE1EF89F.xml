<subSubSection pageId="15" pageNumber="16" type="etymology">
<paragraph blockId="15.[264,1325,905,1851]" pageId="15" pageNumber="16">
<emphasis box="[312,435,1786,1811]" italics="true" pageId="15" pageNumber="16">Etymology</emphasis>
