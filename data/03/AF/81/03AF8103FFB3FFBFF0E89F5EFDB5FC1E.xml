<number box="[1337,1389,152,176]" pageId="3" pageNumber="371" value="1.1">1.10</number>
, EL 
<number box="[151,204,188,213]" pageId="3" pageNumber="371" value="0.75">0.75</number>
. Clypeus height 
<number box="[397,449,188,212]" pageId="3" pageNumber="371" value="0.18">0.18</number>
