<number box="[956,1071,1966,1991]" pageId="2" pageNumber="370" value="3.79" valueMax="4.02" valueMin="3.56">3.56–4.02</number>
; one specimen measured, body length 
<number box="[232,286,2003,2027]" pageId="2" pageNumber="370" value="4.02">4.02</number>
: cephalothorax length 
<number box="[553,605,2002,2027]" pageId="2" pageNumber="370" value="2.25">2.25</number>
, width 
<number box="[695,747,2002,2027]" pageId="2" pageNumber="370" value="1.6">1.60</number>
; abdomen length 
<number box="[960,1011,2003,2027]" pageId="2" pageNumber="370" value="1.71">1.71</number>
, width 
<number box="[1103,1155,2003,2027]" pageId="2" pageNumber="370" value="1.03">1.03</number>
