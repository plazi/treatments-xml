and 
<taxonomicName box="[781,954,1510,1532]" class="Insecta" family="Bacillidae" genus="Bacillus" kingdom="Animalia" order="Phasmida" pageId="5" pageNumber="591" phylum="Arthropoda" rank="species" species="qassimus">
