: 
<collectingRegion box="[1205,1309,322,348]" country="China" name="Guizhou" pageId="12" pageNumber="82">Guizhou</collectingRegion>
, 
<collectingRegion box="[1320,1424,322,348]" country="China" name="Guangxi" pageId="12" pageNumber="82">Guangxi</collectingRegion>
, 
<collectingRegion box="[816,901,355,381]" country="China" name="Jiangxi" pageId="12" pageNumber="82">Jiangxi</collectingRegion>
and 
<collectingRegion box="[959,1049,355,381]" country="China" name="Yunnan" pageId="12" pageNumber="82">Yunnan</collectingRegion>
(
<bibRefCitation author="ZHANG, Y. Z." box="[1064,1218,355,381]" pageId="12" pageNumber="82" refId="ref13150" refString="ZHANG, Y. Z. (ed.). 1997. Distribution of mammalian species in China. China Forestry Publishing House, Beijing, 280 pp." type="book" year="1997">Zhang, 1997</bibRefCitation>
; 
<bibRefCitation author="WANG, Y. X." box="[1231,1378,355,381]" pageId="12" pageNumber="82" refId="ref12745" refString="WANG, Y. X. (ed.). 2003. A complete checklist of mammal species and subspecies in China: a taxonomic and geographic reference. China Forestry Publishing House, Beijing, 394 pp." type="book" year="2003">Wang, 2003</bibRefCitation>
).
</paragraph>
</subSubSection>
<subSubSection pageId="12" pageNumber="82" type="biology_ecology">
<paragraph blockId="12.[816,1428,423,517]" box="[816,1019,423,449]" pageId="12" pageNumber="82">
