<number box="[703,818,692,716]" pageId="7" pageNumber="44" value="0.275" valueMax="0.28" valueMin="0.27">0.27–0.28</number>
; AWV, 
<number box="[905,1020,692,716]" pageId="7" pageNumber="44" value="0.41000000000000003" valueMax="0.42" valueMin="0.4">0.40–0.42</number>
