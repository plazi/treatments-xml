<taxonomicName authority="Qin &amp; Wang &amp; Liu &amp; Li, 2018" authorityName="Qin &amp; Wang &amp; Liu &amp; Li" authorityYear="2018" box="[684,851,1587,1612]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="457" phylum="Arthropoda" rank="species" species="bilobata" status="sp. nov." subGenus="Tachycines">
<emphasis box="[684,697,1589,1612]" italics="true" pageId="6" pageNumber="457">T</emphasis>
. (
<emphasis box="[721,735,1589,1612]" italics="true" pageId="6" pageNumber="457">T</emphasis>
.) 
<emphasis box="[758,851,1587,1612]" italics="true" pageId="6" pageNumber="457">bilobata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[859,949,1587,1612]" pageId="6" pageNumber="457" rank="species">
<emphasis bold="true" box="[859,949,1587,1612]" pageId="6" pageNumber="457">sp. nov.</emphasis>
</taxonomicNameLabel>
and 
<taxonomicName authority="Qin &amp; Wang &amp; Liu &amp; Li, 2018" authorityName="Qin &amp; Wang &amp; Liu &amp; Li" authorityYear="2018" box="[1003,1148,1587,1612]" class="Insecta" family="Rhaphidophoridae" genus="Tachycines" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="6" pageNumber="457" phylum="Arthropoda" rank="species" species="karnyi" status="sp. nov." subGenus="Tachycines">
