<emphasis box="[1040,1053,1589,1612]" italics="true" pageId="6" pageNumber="457">T</emphasis>
.) 
<emphasis box="[1077,1148,1587,1612]" italics="true" pageId="6" pageNumber="457">karnyi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[1155,1245,1587,1612]" pageId="6" pageNumber="457" rank="species">
<emphasis bold="true" box="[1155,1245,1587,1612]" pageId="6" pageNumber="457">sp. nov.</emphasis>
</taxonomicNameLabel>
