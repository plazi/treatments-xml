<th box="[660,894,1127,1149]" gridcol="2" gridrow="0" pageId="4" pageNumber="359">
<taxonomicName box="[660,792,1127,1149]" class="Insecta" family="Plutellidae" genus="Rhigognostis" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="359" phylum="Arthropoda" rank="species" species="canariella">
<emphasis box="[660,792,1127,1149]" italics="true" pageId="4" pageNumber="359">R. canariella</emphasis>
</taxonomicName>
</th>
<th box="[915,1189,1127,1149]" gridcol="3" gridrow="0" pageId="4" pageNumber="359">
<taxonomicName box="[915,1070,1127,1149]" class="Insecta" family="Plutellidae" genus="Rhigognostis" kingdom="Animalia" order="Lepidoptera" pageId="4" pageNumber="359" phylum="Arthropoda" rank="species" species="schmaltzella">
<emphasis box="[915,1070,1127,1149]" italics="true" pageId="4" pageNumber="359">R. schmaltzella</emphasis>
</taxonomicName>
</th>
