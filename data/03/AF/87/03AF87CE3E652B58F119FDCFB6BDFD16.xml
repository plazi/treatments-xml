<paragraph blockId="29.[160,1156,480,644]" pageId="29" pageNumber="334">
<emphasis bold="true" box="[160,293,480,506]" italics="true" pageId="29" pageNumber="334">Comments.</emphasis>
<taxonomicName authorityName=", Pape" authorityYear="1996" box="[309,440,481,506]" class="Insecta" family="Sarcophagidae" genus="Titanogrypa" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="334" phylum="Arthropoda" rank="genus">
<emphasis box="[309,440,481,506]" italics="true" pageId="29" pageNumber="334">Titanogrypa</emphasis>
</taxonomicName>
<taxonomicName baseAuthorityName="Aldrich" baseAuthorityYear="1925" box="[449,572,481,506]" class="Insecta" family="Sarcophagidae" genus="Titanogrypa" kingdom="Animalia" order="Diptera" pageId="29" pageNumber="334" phylum="Arthropoda" rank="species" species="placida" subGenus="Cucullomyia">
