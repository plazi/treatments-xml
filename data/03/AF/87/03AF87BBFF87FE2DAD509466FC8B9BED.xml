<quantity box="[821,992,1724,1750]" metricMagnitude="-5" metricUnit="m" metricValue="5.5" metricValueMax="6.0" metricValueMin="5.0" pageId="3" pageNumber="4" unit="mm" value="0.055" valueMax="0.06" valueMin="0.05">0.05–0.06 mm</quantity>
