<paragraph blockId="21.[151,728,1663,1723]" box="[151,316,1698,1723]" pageId="21" pageNumber="22">
(
<figureCitation box="[159,307,1698,1723]" captionStart-0="FIGURES 92 – 97" captionStart-1="FIGURES 98 – 100" captionStartId-0="22.[151,264,1830,1853]" captionStartId-1="23.[151,264,1632,1655]" captionTargetBox-0="[157,1428,925,1803]" captionTargetBox-1="[161,1428,776,1605]" captionTargetId-0="figure@22.[150,1436,918,1809]" captionTargetId-1="figure@23.[151,1436,769,1611]" captionTargetPageId-0="22" captionTargetPageId-1="23" captionText-0="FIGURES 92 – 97. Minilimosina (Svarciella) tapiehella Su, sp. n.: 92, male fore leg, anterior; 93, male genitalia, posterior; 94, male genitalia, lateral; 95, gonostylus, anterior; 96, aedeagus and associated structures, lateral; 97, male sternite 5, ventral." captionText-1="FIGURES 98 – 100. Minilimosina (Svarciella) tapiehella Su, sp. n.: 98, female abdomen, dorsal; 99, female postabdomen, ventral; 100, spermathecae. (abbreviations: S, sternite; T, tergite)" httpUri-0="https://zenodo.org/record/289296/files/figure.png" httpUri-1="https://zenodo.org/record/289297/files/figure.png" pageId="21" pageNumber="22">
Figs. 
<number box="[222,307,1698,1722]" pageId="21" pageNumber="22" value="96.0" valueMax="100.0" valueMin="92.0">92–100</number>
</figureCitation>
)
