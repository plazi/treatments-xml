<taxonomicName authority="(Horsfield, 1823)" baseAuthorityName="Horsfield" baseAuthorityYear="1823" box="[886,1359,430,456]" class="Mammalia" family="Hipposideridae" genus="Hipposideros" kingdom="Animalia" order="Chiroptera" pageId="11" pageNumber="81" phylum="Chordata" rank="species" species="larvatus">
