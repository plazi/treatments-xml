<emphasis box="[974,1086,2062,2082]" italics="true" pageId="16" pageNumber="69">H. formula</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Orchymont" authorityYear="1932" box="[1102,1231,2061,2082]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="69" phylum="Arthropoda" rank="species" species="jacobsoni">
<emphasis box="[1102,1119,2062,2082]" italics="true" pageId="16" pageNumber="69">H</emphasis>
. 
<emphasis box="[1135,1231,2062,2082]" italics="true" pageId="16" pageNumber="69">jacobsoni</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Jäch &amp; Díaz &amp; Skale" authorityYear="2013" box="[1248,1388,2061,2082]" class="Insecta" family="Hydraenidae" genus="Hydraena" kingdom="Animalia" order="Coleoptera" pageId="16" pageNumber="69" phylum="Arthropoda" rank="species" species="paulmoritz">
<emphasis box="[1248,1265,2062,2082]" italics="true" pageId="16" pageNumber="69">H</emphasis>
. 
<emphasis box="[1281,1388,2062,2082]" italics="true" pageId="16" pageNumber="69">paulmoritz</emphasis>
