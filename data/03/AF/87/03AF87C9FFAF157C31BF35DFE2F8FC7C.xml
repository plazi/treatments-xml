<taxonomicName authority="Barra, 1968: 8" authorityName="Barra" authorityPageNumber="8" authorityYear="1968" box="[343,741,1932,1955]" class="Entognatha" family="Isotomidae" genus="Isotomiella" kingdom="Animalia" order="Collembola" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="delamarei">
