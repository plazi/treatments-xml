<subSubSection box="[151,572,151,176]" pageId="1" pageNumber="572" type="nomenclature">
<paragraph blockId="1.[151,572,151,176]" box="[151,572,151,176]" pageId="1" pageNumber="572">
<heading bold="true" box="[151,572,151,176]" fontSize="10" level="2" pageId="1" pageNumber="572" reason="2">
<emphasis bold="true" box="[151,572,151,176]" pageId="1" pageNumber="572">
Genus 
<taxonomicName box="[234,460,151,176]" class="Malacostraca" family="Pseudothelphusidae" genus="Colombiathelphusa" kingdom="Animalia" order="Decapoda" pageId="1" pageNumber="572" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[234,460,151,176]" italics="true" pageId="1" pageNumber="572">Colombiathelphusa</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[467,572,151,176]" pageId="1" pageNumber="572" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
