LENGTH. Body 
<quantity box="[362,455,676,702]" metricMagnitude="-3" metricUnit="m" metricValue="6.2" pageId="99" pageNumber="100" unit="mm" value="6.2">6.2 mm</quantity>
, wing 
<quantity box="[532,625,676,702]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="99" pageNumber="100" unit="mm" value="4.8">4.8 mm</quantity>
