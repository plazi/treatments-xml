<taxonomicName box="[945,1173,1358,1380]" class="Insecta" family="Miridae" genus="Dashymeniella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="tibialis">
