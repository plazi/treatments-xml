<taxonomicName box="[686,789,1419,1441]" class="Insecta" family="Miridae" genus="Dashymeniella" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="tibialis">
