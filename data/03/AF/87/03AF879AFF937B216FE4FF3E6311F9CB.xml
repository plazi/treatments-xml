species 
<taxonomicName authority="Li et Szwedo" authorityName="Li et Szwedo" box="[287,675,221,243]" class="Insecta" family="Fulgoridiidae" genus="Fenghuangor" higherTaxonomySource="CoL" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="54" phylum="Arthropoda" rank="species" species="imperator" status="sp. n.">
