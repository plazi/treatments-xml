<quantity box="[854,1026,488,514]" metricMagnitude="-4" metricUnit="m" metricValue="2.25" metricValueMax="2.6" metricValueMin="1.9" pageId="8" pageNumber="23" unit="mm" value="0.225" valueMax="0.26" valueMin="0.19">0.19–0.26 mm</quantity>
(n= 
<number box="[1077,1092,488,514]" pageId="8" pageNumber="23" value="6.0">6</number>
