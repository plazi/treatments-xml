) 
<emphasis box="[275,372,1929,1953]" italics="true" pageId="14" pageNumber="465">escalerae</emphasis>
(
<bibRefCitation author="Abeille de Perrin, E." box="[386,627,1929,1953]" pageId="14" pageNumber="465" pagination="206 - 224" refId="ref24142" refString="Abeille de Perrin, E. (1904) Buprestides. Boletin de la Real Sociedad Espanola de Historia Natural, 4, 206 - 224." type="journal article" year="1904">Abeille de Perrin, 1904</bibRefCitation>
)
</taxonomicName>
(lectotype). 23— 
<taxonomicName baseAuthorityName="Abeille de Perrin" baseAuthorityYear="1904" box="[815,985,1929,1953]" class="Insecta" family="Buprestidae" genus="Chalcophorella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="465" phylum="Arthropoda" rank="species" species="escalerae" subGenus="Stigmatophorella">
<emphasis box="[815,839,1929,1952]" italics="true" pageId="14" pageNumber="465">C.</emphasis>
(
<emphasis box="[854,873,1929,1952]" italics="true" pageId="14" pageNumber="465">S.</emphasis>
) 
<emphasis box="[888,985,1929,1953]" italics="true" pageId="14" pageNumber="465">escalerae</emphasis>
</taxonomicName>
(imago on 
