</taxonomicName>
from 
<collectingCountry box="[1222,1295,1773,1799]" name="Kenya" pageId="127" pageNumber="128">Kenya</collectingCountry>
[Rift Valley Province] and 
<taxonomicName authority="rueppelli" box="[314,610,1809,1835]" class="Reptilia" family="Agamidae" genus="Agama" kingdom="Animalia" order="Squamata" pageId="127" pageNumber="128" phylum="Chordata" rank="subSpecies" species="rueppelli" subSpecies="rueppelli">
