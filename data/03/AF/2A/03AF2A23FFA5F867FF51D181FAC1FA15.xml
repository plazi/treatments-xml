<emphasis box="[882,1042,905,930]" italics="true" pageId="95" pageNumber="96">
<taxonomicName authorityName="Merrem" authorityYear="1820" box="[882,1004,905,930]" class="Reptilia" family="Agamidae" genus="Uromastyx" kingdom="Animalia" order="Squamata" pageId="95" pageNumber="96" phylum="Chordata" rank="genus">Uromastyx</taxonomicName>
