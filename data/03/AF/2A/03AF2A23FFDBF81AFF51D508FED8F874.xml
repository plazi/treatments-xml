<taxonomicName authority="(Tragardh, 1905)" authorityName="Hirst" authorityYear="1926" baseAuthorityName="Tragardh" baseAuthorityYear="1905" box="[294,691,1125,1150]" class="Arachnida" family="Pterygosomatidae" genus="Pterygosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Prostigmata" pageId="34" pageNumber="35" phylum="Arthropoda" rank="species" species="inermis">
