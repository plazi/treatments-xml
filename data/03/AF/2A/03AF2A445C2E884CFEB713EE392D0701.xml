/ total 
<number box="[960,1015,1297,1323]" pageId="3" pageNumber="64" value="8.54">8.54</number>
; II 
<number box="[1057,1107,1297,1323]" pageId="3" pageNumber="64" value="2.01">2.01</number>
/ 
<number box="[1126,1177,1297,1323]" pageId="3" pageNumber="64" value="2.54">2.54</number>
