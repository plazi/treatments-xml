from 
<taxonomicName authority="(Stoliczka)" baseAuthorityName="Stoliczka" baseAuthorityYear="1871" box="[733,1060,1480,1507]" class="Reptilia" family="Agamidae" genus="Laudakia" kingdom="Animalia" order="Squamata" pageId="68" pageNumber="69" phylum="Chordata" rank="species" species="dayana">
<emphasis box="[733,929,1480,1506]" italics="true" pageId="68" pageNumber="69">Laudakia dayana</emphasis>
(Stoliczka)
</taxonomicName>
(Sauria: 
<taxonomicName box="[1162,1282,1480,1507]" class="Reptilia" family="Agamidae" kingdom="Animalia" order="Squamata" pageId="68" pageNumber="69" phylum="Chordata" rank="family">Agamidae</taxonomicName>
), 
<emphasis bold="true" box="[1301,1383,1479,1506]" pageId="68" pageNumber="69">
<collectingCountry box="[1301,1383,1479,1506]" name="India" pageId="68" pageNumber="69">INDIA</collectingCountry>
</emphasis>
: 
<collectingRegion country="India" name="Uttarakhand" pageId="68" pageNumber="69">Uttarakhand state</collectingRegion>
, Hardwar.
</paragraph>
<paragraph blockId="68.[151,1437,1191,1758]" box="[199,1187,1551,1579]" pageId="68" pageNumber="69">
