<taxonomicName box="[704,813,762,787]" class="Mammalia" family="Didelphidae" genus="Didelphis" kingdom="Animalia" order="Didelphimorphia" pageId="8" pageNumber="247" phylum="Chordata" rank="genus">
<emphasis box="[704,813,762,787]" italics="true" pageId="8" pageNumber="247">Didelphis</emphasis>
</taxonomicName>
sp. Carnivora—Canidae— 
<emphasis italics="true" pageId="8" pageNumber="247">
<taxonomicName box="[1122,1309,762,787]" class="Mammalia" family="Canidae" genus="Canis" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="247" phylum="Chordata" rank="species" species="familiaris">Canis familiaris</taxonomicName>
, 
<taxonomicName class="Mammalia" family="Canidae" genus="Lycalopex" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="247" phylum="Chordata" rank="species" species="gymnocercus">Lycalopex gymnocercus</taxonomicName>
, 
<taxonomicName box="[317,527,798,823]" class="Mammalia" family="Canidae" genus="Lycalopex" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="247" phylum="Chordata" rank="species" species="griseus">Lycalopex griseus</taxonomicName>
.
</emphasis>
<taxonomicName box="[543,630,798,823]" class="Mammalia" family="Felidae" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="247" phylum="Chordata" rank="family">Felidae</taxonomicName>
