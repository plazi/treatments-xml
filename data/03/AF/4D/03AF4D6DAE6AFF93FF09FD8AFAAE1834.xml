<emphasis box="[646,968,798,823]" italics="true" pageId="8" pageNumber="247">
<taxonomicName box="[646,776,798,823]" class="Mammalia" family="Felidae" genus="Felis" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="247" phylum="Chordata" rank="species" species="catus">Felis catus</taxonomicName>
, 
<taxonomicName box="[792,968,798,823]" class="Mammalia" family="Felidae" genus="Puma" kingdom="Animalia" order="Carnivora" pageId="8" pageNumber="247" phylum="Chordata" rank="species" species="concolor">Puma concolor</taxonomicName>
</emphasis>
