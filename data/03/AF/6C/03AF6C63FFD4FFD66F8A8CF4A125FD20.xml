, 
<taxonomicName authority="Hsueh, 2020" authorityName="Hsueh" authorityYear="2020" box="[631,796,333,359]" class="Polychaeta" family="Nereididae" genus="Nereis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="16" pageNumber="17" phylum="Annelida" rank="species" species="taiwanensis" status="n. sp.">
<emphasis box="[631,650,333,358]" italics="true" pageId="16" pageNumber="17">N</emphasis>
. 
<emphasis box="[664,796,333,358]" italics="true" pageId="16" pageNumber="17">taiwanensis</emphasis>
</taxonomicName>
<emphasis bold="true" box="[804,868,333,359]" pageId="16" pageNumber="17">
<taxonomicNameLabel box="[804,868,333,359]" pageId="16" pageNumber="17" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
, and 
<taxonomicName authority="Shen &amp; Sun in Sun, Wu &amp; Shen, 1978" authorityName="Shen &amp; Sun in Sun, Wu &amp; Shen" authorityYear="1978" class="Polychaeta" family="Nereididae" genus="Nereis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="16" pageNumber="17" phylum="Annelida" rank="species" species="zhongshaensis">
<emphasis box="[931,950,333,358]" italics="true" pageId="16" pageNumber="17">N</emphasis>
. 
<emphasis box="[965,1127,332,358]" italics="true" pageId="16" pageNumber="17">zhongshaensis</emphasis>
Shen &amp; Sun in 
<bibRefCitation author="Sun, R. &amp; Wu, B. L. &amp; Shen, S." pageId="16" pageNumber="17" pagination="133 - 168" refId="ref20874" refString="Sun, R., Wu, B. L. &amp; Shen, S. (1978) A preliminary report on the pelagic swarming polychaetes from the Zhongsha Islands, Guangdong Province, China. In: Zhongguo, K. (Ed.), Nanhai Haiyang Yanjiusuo, Wonguo yanjiu baobaoji [Nanhai Inst. Oceanogr. Acad. Sinica. Research report on invertebrates in marine biology in waters of Xisha and Zhongsha Islands, China. Science Press, Peking, pp. 133 - 168. [in Chinese with English abstract]" type="book chapter" year="1978">Sun, Wu &amp; Shen,1978</bibRefCitation>
