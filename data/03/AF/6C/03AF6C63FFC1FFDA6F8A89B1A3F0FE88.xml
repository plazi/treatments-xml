<emphasis bold="true" box="[151,240,1624,1649]" pageId="27" pageNumber="28">FIG. 21.</emphasis>
<taxonomicName authorityName="Linnaeus" authorityYear="1758" box="[245,310,1625,1648]" class="Polychaeta" family="Nereididae" genus="Nereis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phyllodocida" pageId="27" pageNumber="28" phylum="Annelida" rank="genus">
