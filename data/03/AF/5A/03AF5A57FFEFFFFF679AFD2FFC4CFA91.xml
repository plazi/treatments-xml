<subSubSection pageId="8" pageNumber="175" type="reference_group">
<paragraph blockId="8.[151,1437,826,881]" pageId="8" pageNumber="175">
<taxonomicName authority="Hreblay &amp; L. Ronkay, 1998" authorityName="Hreblay &amp; L. Ronkay" authorityYear="1998" box="[151,593,826,850]" class="Insecta" family="Noctuidae" genus="Charanyctycia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="8" pageNumber="175" phylum="Arthropoda" rank="genus">
