<number box="[804,847,1989,2015]" pageId="2" pageNumber="35" value="2.5">2.5</number>
, width 
<number box="[941,984,1989,2015]" pageId="2" pageNumber="35" value="1.7">1.7</number>
; abdomen length 
<number box="[1210,1254,1989,2015]" pageId="2" pageNumber="35" value="4.6">4.6</number>
