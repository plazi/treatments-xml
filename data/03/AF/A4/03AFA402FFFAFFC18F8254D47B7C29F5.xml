<caption httpUri="https://zenodo.org/record/1051914/files/figure.png" pageId="6" pageNumber="7" targetBox="[166,1409,225,1064]" targetPageId="6">
<paragraph blockId="6.[151,1436,1137,1224]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[151,344,1137,1161]" pageId="6" pageNumber="7">FIGURES 13–15</emphasis>
. 
<taxonomicName authorityName="Marinkovic-Gospodnetic" authorityYear="1971" box="[358,511,1138,1161]" class="Insecta" family="Limnephilidae" genus="Drusus" kingdom="Animalia" order="Trichoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="ramae">
