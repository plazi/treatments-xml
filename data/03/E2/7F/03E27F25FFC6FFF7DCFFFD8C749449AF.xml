<taxonomicName baseAuthorityName="Uchida &amp; Uchida" baseAuthorityYear="1929" box="[396,493,1651,1676]" class="Hydrozoa" family="Moerisiidae" genus="Moerisia" kingdom="Animalia" order="Anthoathecata" pageId="56" pageNumber="57" phylum="Cnidaria" rank="species" species="horii">
<emphasis box="[396,493,1651,1676]" italics="true" pageId="56" pageNumber="57">M. horii</emphasis>
