<quantity box="[1010,1088,848,874]" metricMagnitude="2" metricUnit="m" metricValue="5.83" pageId="9" pageNumber="10" unit="m" value="583.0">583 m</quantity>
, 
<date box="[1102,1228,848,874]" pageId="9" pageNumber="10" value="1902-04-01">01.iv.1902</date>
, one fragmentary colony, about 
<quantity box="[321,397,888,914]" metricMagnitude="-1" metricUnit="m" metricValue="1.5" pageId="9" pageNumber="10" unit="cm" value="15.0">15 cm</quantity>
high, without gonophores, 
<collectionCode box="[730,815,888,914]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="9" pageNumber="10" type="Museum">USNM</collectionCode>
22150 [
<typeStatus box="[914,1088,888,914]" pageId="9" pageNumber="10">LECTOTYPE</typeStatus>
; bottle also contains an antipatharian overgrown by an operculate hydroid].–Molokai: Albatross Stn. 3828, off south coast, 
<quantity box="[1348,1431,928,954]" metricMagnitude="2" metricUnit="m" metricValue="5.83" pageId="9" pageNumber="10" unit="m" value="583.0">583 m</quantity>
, 
<date box="[151,277,968,994]" pageId="9" pageNumber="10" value="1902-04-01">01.iv.1902</date>
