[
<taxonomicName box="[690,928,1378,1401]" class="Hydrozoa" family="Eudendriidae" genus="Eudendrium" kingdom="Animalia" order="Anthoathecata" pageId="37" pageNumber="38" phylum="Cnidaria" rank="species" species="ramosum">
