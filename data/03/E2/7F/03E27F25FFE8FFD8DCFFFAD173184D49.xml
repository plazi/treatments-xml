<taxonomicName baseAuthorityName="Peron &amp; Lesueur" baseAuthorityYear="1810" box="[747,959,1509,1532]" class="Agaricomycetes" family="Atheliaceae" genus="Amphinema" kingdom="Fungi" order="Atheliales" pageId="22" pageNumber="23" phylum="Basidiomycota" rank="species" species="dinema">
