<paragraph blockId="22.[120,421,1683,1733]" box="[120,421,1683,1706]" pageId="22" pageNumber="23">
<superScript attach="left" box="[120,127,1683,1697]" fontSize="6" pageId="22" pageNumber="23">a</superScript>
