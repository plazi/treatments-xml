<date box="[822,844,1452,1482]" pageId="1" pageNumber="30">
<emphasis box="[822,835,1452,1477]" italics="true" pageId="1" pageNumber="30">v</emphasis>
<number box="[836,844,1468,1482]" pageId="1" pageNumber="30" value="1.0">1</number>
</date>
