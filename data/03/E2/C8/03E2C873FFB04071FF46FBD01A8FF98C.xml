<number box="[720,764,268,294]" pageId="15" pageNumber="44" value="478.0">478</number>
(
<number box="[779,886,268,294]" pageId="15" pageNumber="44" value="426.5" valueMax="450.0" valueMin="403.0">403–450</number>
