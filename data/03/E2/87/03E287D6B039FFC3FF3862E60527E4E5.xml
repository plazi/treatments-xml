<paragraph blockId="1.[151,500,1227,1254]" box="[151,500,1227,1254]" pageId="1" pageNumber="393">
<heading bold="true" box="[151,500,1227,1254]" fontSize="11" level="1" pageId="1" pageNumber="393" reason="1">
<emphasis bold="true" box="[151,500,1227,1254]" pageId="1" pageNumber="393">
<taxonomicName box="[151,426,1227,1253]" class="Insecta" family="Psocidae" genus="Chaetoblaste" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocoptera" pageId="1" pageNumber="393" phylum="Arthropoda" rank="species" species="gonzalezi" status="n. sp.">
<emphasis bold="true" box="[151,426,1227,1253]" italics="true" pageId="1" pageNumber="393">Chaetoblaste gonzalezi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[434,500,1228,1254]" pageId="1" pageNumber="393" rank="species">n. sp.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
