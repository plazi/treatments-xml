<collectingCountry box="[1350,1440,539,558]" name="Mexico" pageId="17" pageNumber="16">Mexico</collectingCountry>
(
<collectingRegion box="[297,401,571,590]" country="Mexico" name="Hidalgo" pageId="17" pageNumber="16">Hidalgo</collectingRegion>
, 
