<collectingCountry box="[1318,1432,891,917]" name="Argentina" pageId="16" pageNumber="469">Argentina</collectingCountry>
, 
<collectingCountry box="[151,236,927,953]" name="Bolivia" pageId="16" pageNumber="469">Bolivia</collectingCountry>
, 
<collectingCountry box="[247,318,927,953]" name="Brazil" pageId="16" pageNumber="469">Brazil</collectingCountry>
, 
<collectingCountry box="[329,442,927,953]" name="Colombia" pageId="16" pageNumber="469">Colombia</collectingCountry>
, 
<collectingCountry box="[453,577,927,953]" name="Costa Rica" pageId="16" pageNumber="469">Costa Rica</collectingCountry>
, 
<collectingCountry box="[588,683,927,953]" name="Ecuador" pageId="16" pageNumber="469">Ecuador</collectingCountry>
, French Guiana, 
<collectingCountry box="[872,962,927,953]" name="Guyana" pageId="16" pageNumber="469">Guyana</collectingCountry>
, 
<collectingCountry box="[972,1090,927,953]" name="Nicaragua" pageId="16" pageNumber="469">Nicaragua</collectingCountry>
, 
<collectingCountry box="[1101,1193,927,953]" name="Panama" pageId="16" pageNumber="469">Panama</collectingCountry>
, 
<collectingCountry box="[1203,1308,927,953]" name="Paraguay" pageId="16" pageNumber="469">Paraguay</collectingCountry>
, 
<collectingCountry box="[1319,1373,927,953]" name="Peru" pageId="16" pageNumber="469">Peru</collectingCountry>
