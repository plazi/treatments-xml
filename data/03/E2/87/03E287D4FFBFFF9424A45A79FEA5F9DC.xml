<typeStatus box="[151,243,1225,1249]" pageId="19" pageNumber="338">paratype</typeStatus>
, 
<collectorName box="[261,468,1225,1249]" pageId="19" pageNumber="338">Alexey V. Shavrin</collectorName>
(currently 
<collectingRegion box="[598,719,1225,1249]" country="Latvia" name="Daugavpils" pageId="19" pageNumber="338">Daugavpils</collectingRegion>
, 
