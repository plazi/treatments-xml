; Hw, 
<number box="[815,853,980,1005]" pageId="10" pageNumber="508" value="2.5">2.5</number>
; ioD, 
<number box="[919,957,980,1004]" pageId="10" pageNumber="508" value="1.2">1.2</number>
; Lpron, 
<number box="[1050,1067,980,1004]" pageId="10" pageNumber="508" value="2.0">2</number>
; awpron, 
<number box="[1175,1213,980,1004]" pageId="10" pageNumber="508" value="3.2">3.2</number>
; pwpron, 
<number box="[1323,1361,980,1005]" pageId="10" pageNumber="508" value="2.6">2.6</number>
; LFW, 
<number box="[151,189,1016,1040]" pageId="10" pageNumber="508" value="4.9">4.9</number>
