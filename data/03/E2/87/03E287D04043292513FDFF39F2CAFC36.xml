, 
<collectingCountry box="[1327,1432,861,887]" name="Paraguay" pageId="15" pageNumber="468">Paraguay</collectingCountry>
, 
<collectingCountry box="[151,205,897,923]" name="Peru" pageId="15" pageNumber="468">Peru</collectingCountry>
