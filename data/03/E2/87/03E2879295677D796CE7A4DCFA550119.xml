<emphasis bold="true" box="[968,1351,417,439]" pageId="12" pageNumber="261">
<emphasis bold="true" box="[968,1226,417,439]" italics="true" pageId="12" pageNumber="261">
<taxonomicName authority="Agudelo-Zamora &amp; Ortega-Lara &amp; Taphorn, 2020" authorityName="Agudelo-Zamora &amp; Ortega-Lara &amp; Taphorn" authorityYear="2020" box="[968,1221,417,439]" class="Actinopterygii" family="Crenuchidae" genus="Characidium" kingdom="Animalia" order="Characiformes" pageId="12" pageNumber="261" phylum="Chordata" rank="species" species="chancoense" status="new species">Characidium chancoense</taxonomicName>
