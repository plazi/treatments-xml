<number box="[584,644,188,212]" pageId="2" pageNumber="53" value="1821.0">1821</number>
] 
<number box="[661,720,188,212]" pageId="2" pageNumber="53" value="1816.0">1816</number>
. Lafontaine (
<number box="[871,929,188,212]" pageId="2" pageNumber="53" value="1998.0">1998</number>
