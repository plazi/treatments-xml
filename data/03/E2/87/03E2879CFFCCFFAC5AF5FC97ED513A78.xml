, 
<taxonomicName box="[1065,1165,297,320]" class="Insecta" family="Noctuidae" genus="Estimata" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="53" phylum="Arthropoda" rank="genus">
<emphasis box="[1065,1165,297,320]" italics="true" pageId="2" pageNumber="53">Estimata</emphasis>
</taxonomicName>
and 
<emphasis box="[1229,1391,296,320]" italics="true" pageId="2" pageNumber="53">
<taxonomicName box="[1229,1387,296,320]" class="Insecta" family="Noctuidae" genus="Erebophasma" kingdom="Animalia" order="Lepidoptera" pageId="2" pageNumber="53" phylum="Arthropoda" rank="genus">Erebophasma</taxonomicName>
,
