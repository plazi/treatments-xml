. 
<collectingCountry box="[359,419,765,791]" name="United States of America" pageId="41" pageNumber="42">USA</collectingCountry>
. States of 
<collectingRegion box="[534,648,765,791]" country="United States of America" name="California" pageId="41" pageNumber="42">California</collectingRegion>
and 
<collectingRegion box="[702,786,765,791]" country="United States of America" name="Oregon" pageId="41" pageNumber="42">Oregon</collectingRegion>
(
<bibRefCitation author="Weld, L. H." box="[800,927,764,791]" pageId="41" pageNumber="42" pagination="107 - 122" refId="ref59157" refString="Weld, L. H. (1957) New American cynipid wasps from oak galls. Proceedings of the Unites States National Museum, 107 (3384), 107 - 122. https: // doi. org / 10.5479 / si. 00963801.107 - 3384.107" type="journal article" year="1957">Weld 1957</bibRefCitation>
