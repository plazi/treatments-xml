<taxonomicName authorityName="Stimpson" authorityYear="1871" box="[1093,1183,511,536]" class="Malacostraca" family="Acidopsidae" genus="Acidops" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
