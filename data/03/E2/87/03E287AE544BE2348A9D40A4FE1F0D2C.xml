<taxonomicName authorityName="Stimpson" authorityYear="1871" box="[1041,1131,439,464]" class="Malacostraca" family="Acidopsidae" genus="Acidops" kingdom="Animalia" order="Decapoda" pageId="15" pageNumber="16" phylum="Arthropoda" rank="genus">
