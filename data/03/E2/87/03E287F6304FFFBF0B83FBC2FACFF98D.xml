<number box="[264,350,1070,1094]" pageId="25" pageNumber="26" value="64100.0">64, 100</number>
, 
<number box="[362,664,1070,1094]" pageId="25" pageNumber="26" value="1.457097355E8" valueMax="2.11251261E8" valueMin="8.016821E7">80, 168, 210–211, 251,261</number>
