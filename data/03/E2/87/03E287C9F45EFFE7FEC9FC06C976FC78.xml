<taxonomicName authorityName="Carvalho &amp; Fontes" authorityYear="1968" class="Insecta" family="Miridae" genus="Amapacylapus" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="405" phylum="Arthropoda" rank="genus">
