<taxonomicName authority="Schmelz and Collado, 2005" authorityName="Schmelz and Collado" authorityYear="2005" box="[447,917,1798,1824]" class="Insecta" family="Pseudococcidae" genus="Antonina" kingdom="Animalia" order="Hemiptera" pageId="2" pageNumber="53" phylum="Arthropoda" rank="species" species="becki">
