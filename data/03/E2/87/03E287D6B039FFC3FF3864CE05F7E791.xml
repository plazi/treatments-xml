<typeStatus box="[199,258,1133,1158]" pageId="1" pageNumber="393">Type</typeStatus>
species.
</emphasis>
<taxonomicName box="[361,621,1133,1157]" class="Insecta" family="Psocidae" genus="Chaetoblaste" higherTaxonomySource="GBIF" kingdom="Animalia" order="Psocoptera" pageId="1" pageNumber="393" phylum="Arthropoda" rank="species" species="gonzalezi" status="n. sp.">
<emphasis box="[361,621,1133,1157]" italics="true" pageId="1" pageNumber="393">Chaetoblaste gonzalezi</emphasis>
