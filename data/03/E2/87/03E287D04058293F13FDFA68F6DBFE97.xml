<taxonomicName authority="(Degeer, 1773)" authorityName="Sanborn" authorityYear="2020" baseAuthorityName="Degeer" baseAuthorityYear="1773" box="[1047,1403,187,213]" class="Insecta" family="Cicadidae" genus="Alocha" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Hemiptera" pageId="21" pageNumber="474" phylum="Arthropoda" rank="species" species="collaris" status="n. comb.">
