<subSubSection pageId="4" pageNumber="5" type="description">
<paragraph blockId="4.[264,1326,799,1840]" pageId="4" pageNumber="5">
<emphasis bold="true" box="[312,454,1094,1120]" pageId="4" pageNumber="5">Coloration.</emphasis>
