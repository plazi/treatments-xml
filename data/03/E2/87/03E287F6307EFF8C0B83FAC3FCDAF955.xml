<subSubSection pageId="41" pageNumber="42" type="etymology">
<paragraph blockId="41.[151,1437,151,1688]" pageId="41" pageNumber="42">
<emphasis bold="true" box="[199,334,1628,1652]" pageId="41" pageNumber="42">Etymology.</emphasis>
