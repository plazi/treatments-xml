<emphasis box="[348,745,560,585]" italics="true" pageId="23" pageNumber="24">Aspidoscelis communis mariarum</emphasis>
(Günther)
</taxonomicName>
and 
<taxonomicName authority="Cope" authorityName="Cope" box="[925,1417,560,586]" class="Reptilia" family="Teiidae" genus="Aspidoscelis" kingdom="Animalia" order="Squamata" pageId="23" pageNumber="24" phylum="Chordata" rank="subSpecies" species="communis" subSpecies="communis">
