<number box="[915,965,741,764]" pageId="3" pageNumber="498" value="4.2">4.20</number>
. Carapace length 
<number box="[1153,1203,741,764]" pageId="3" pageNumber="498" value="2.05">2.05</number>
; carapace width 
<number box="[1381,1431,741,764]" pageId="3" pageNumber="498" value="0.95">0.95</number>
; carapace index 
<number box="[317,347,776,799]" pageId="3" pageNumber="498" value="46.0">46</number>
. Cephalic width 
<number box="[527,577,776,799]" pageId="3" pageNumber="498" value="0.8">0.80</number>
; cephalic index 
<number box="[750,780,776,799]" pageId="3" pageNumber="498" value="84.0">84</number>
. Sternum length 
<number box="[961,1011,776,799]" pageId="3" pageNumber="498" value="0.9">0.90</number>
; sternum width 
<number box="[1182,1232,776,799]" pageId="3" pageNumber="498" value="0.6">0.60</number>
; sternum index 
<number box="[1402,1432,776,799]" pageId="3" pageNumber="498" value="67.0">67</number>
. Abdomen length 2.00; abdomen width 
<number box="[578,628,810,833]" pageId="3" pageNumber="498" value="1.25">1.25</number>
; abdominal index 
<number box="[829,859,810,833]" pageId="3" pageNumber="498" value="63.0">63</number>
