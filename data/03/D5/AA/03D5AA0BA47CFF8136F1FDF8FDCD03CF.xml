species. 
<taxonomicName authority="Coffman et Roback, 1984: 131" authorityName="Coffman et Roback" authorityPageNumber="131" authorityYear="1984" box="[315,1105,680,703]" class="Insecta" family="Chironomidae" genus="Lopescladius" kingdom="Animalia" order="Diptera" pageId="1" pageNumber="40" phylum="Arthropoda" rank="species" species="hyporheicus" subGenus="Cordiella">
