<materialsCitation ID-GBIF-Occurrence="2831948312" collectingDate="1961-06" country="Taiwan" location="Labels" pageId="1" pageNumber="281" specimenCount="1" typeStatus="holotype">
<location LSID="urn:lsid:plazi:treatment:03D58377FFBBFF82C6D4F8ABFB42A1C8:8EA364BAFFBBFF81C684F8E3FEABA640" box="[199,278,1881,1907]" country="Taiwan" name="Labels" pageId="1" pageNumber="281">Labels</location>
: “ 
<collectingCountry box="[299,387,1881,1907]" name="Taiwan" pageId="1" pageNumber="281">Taiwan</collectingCountry>
, Tsugitakayama (= Xueshan, in Chinese character)/ 
<date box="[969,1088,1881,1908]" pageId="1" pageNumber="281" value="1961-06">
<collectingDate box="[969,1088,1881,1908]" pageId="1" pageNumber="281" value="1961-06">June 1961</collectingDate>
</date>
,” “ 
<taxonomicName authorityName="Scudder" authorityYear="1872" box="[1121,1227,1881,1907]" class="Insecta" family="Hesperiidae" genus="Ochlodes" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="281" phylum="Arthropoda" rank="genus">Ochlodes</taxonomicName>
/siva/yuchingkina/ 
<typeStatus box="[151,305,1917,1943]" pageId="1" pageNumber="281">HOLOTYPE</typeStatus>
.”
</materialsCitation>
