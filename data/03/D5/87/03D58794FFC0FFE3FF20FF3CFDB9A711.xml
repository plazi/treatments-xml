<collectingCountry box="[362,476,1089,1115]" name="Argentina" pageId="13" pageNumber="558">Argentina</collectingCountry>
, 
<collectingCountry box="[487,559,1089,1115]" name="Brazil" pageId="13" pageNumber="558">Brazil</collectingCountry>
.
</paragraph>
