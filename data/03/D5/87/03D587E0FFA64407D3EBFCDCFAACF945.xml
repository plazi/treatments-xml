<taxonomicName authorityName="Vitali &amp; Chemin" authorityYear="2022" baseAuthorityName="Breuning" baseAuthorityYear="1970" box="[213,374,1703,1728]" class="Insecta" family="Cerambycidae" genus="Nephelotus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="83" phylum="Arthropoda" rank="species" species="enganensis">
<emphasis box="[213,374,1703,1728]" italics="true" pageId="4" pageNumber="83">M. enganensis</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Breuning" authorityYear="1982" box="[429,566,1703,1729]" class="Insecta" family="Cerambycidae" genus="Anhammus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="83" phylum="Arthropoda" rank="species" species="luzonicus">
<emphasis box="[429,566,1703,1729]" italics="true" pageId="4" pageNumber="83">A. luzonicus</emphasis>
</taxonomicName>
