<number box="[574,603,1450,1476]" pageId="1" pageNumber="43" value="20.0">20</number>
(
<number box="[619,695,1450,1476]" pageId="1" pageNumber="43" value="21.0" valueMax="22.0" valueMin="20.0">20–22</number>
