<number box="[489,518,1330,1356]" pageId="1" pageNumber="43" value="19.0">19</number>
(
<number box="[536,614,1330,1356]" pageId="1" pageNumber="43" value="18.0" valueMax="19.0" valueMin="17.0">17–19</number>
