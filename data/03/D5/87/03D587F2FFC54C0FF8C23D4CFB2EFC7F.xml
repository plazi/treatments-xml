<paragraph blockId="10.[2714,3518,2284,2852]" box="[2714,3515,2813,2852]" pageId="10" pageNumber="359">
<taxonomicName authority="Dobson, 1878" authorityName="Dobson" authorityYear="1878" box="[2714,3279,2813,2852]" class="Magnoliopsida" family="Myrtaceae" genus="Etnoaaebnura" kingdom="Plantae" order="Myrtales" pageId="10" pageNumber="359" phylum="Tracheophyta" rank="subSpecies" subSpecies="raffrayana">
