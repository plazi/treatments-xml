<taxonomicName box="[136,470,551,573]" class="Dinophyceae" family="Peridiniaceae" genus="Peridinium" higherTaxonomySource="GBIF" kingdom="Chromista" order="Peridiniales" pageId="18" pageNumber="153" phylum="Myzozoa" rank="variety" species="cinctum" variety="gatunense">
