<location LSID="urn:lsid:plazi:treatment:03D587B5FF949D5CFF75CB27FCBFFAFA:8EA36078FF949D5DFBE1CAD1FBD3FC69" box="[1027,1145,978,1003]" country="Mexico" name="Xochipala" pageId="6" stateProvince="Mexico">Xochipala</location>
, 
<collectingDate box="[1156,1291,979,1003]" pageId="6" value="1989-11-17">
<date box="[1156,1291,979,1003]" pageId="6" value="1989-11-17">17-XI-1989</date>
</collectingDate>
, 
<collectorName box="[1302,1436,978,1003]" pageId="6">R. Turnbow</collectorName>
(
<collectionCode box="[159,240,1015,1039]" pageId="6">RHTC</collectionCode>
