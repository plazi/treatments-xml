<taxonomicName authority="Thery, 1934" authorityName="Thery" authorityYear="1934" class="Insecta" family="Buprestidae" genus="Agrilus" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="7" phylum="Arthropoda" rank="species" species="chembae" subGenus="Agrilus">
