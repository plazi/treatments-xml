<paragraph blockId="16.[140,1108,1187,1566]" pageId="16" pageNumber="17">
<emphasis bold="true" box="[185,455,1433,1459]" pageId="16" pageNumber="17">
<emphasis box="[185,324,1433,1459]" inLineHeading="true" pageId="16" pageNumber="17" reason="1">Differential</emphasis>
