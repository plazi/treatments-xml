<number box="[510,524,655,679]" pageId="10" pageNumber="329" value="2.0">2</number>
dorsal, 
<number box="[616,630,655,679]" pageId="10" pageNumber="329" value="1.0">1</number>
pro- and 
<number box="[743,757,655,679]" pageId="10" pageNumber="329" value="1.0">1</number>
retrolateral spines; tibia with 
<number box="[1100,1114,655,679]" pageId="10" pageNumber="329" value="2.0">2</number>
dorsal, 
<number box="[1206,1220,655,679]" pageId="10" pageNumber="329" value="1.0">1</number>
pro-, 
<number box="[1291,1305,655,679]" pageId="10" pageNumber="329" value="1.0">1</number>
retrolateral and 
<number box="[201,215,692,716]" pageId="10" pageNumber="329" value="5.0">5</number>
pairs of ventral spines metatarsus with 
