<paragraph blockId="7.[151,796,930,952]" box="[151,796,930,952]" pageId="7" pageNumber="478">
<taxonomicName authority="Fabricius 1775" authorityName="Fabricius" authorityYear="1775" box="[151,508,930,952]" class="Agaricostilbomycetes" family="Chionosphaeraceae" genus="Stilbum" kingdom="Fungi" order="Agaricostilbales" pageId="7" pageNumber="478" phylum="Basidiomycota" rank="species" species="splendidum">
<emphasis box="[151,351,930,952]" italics="true" pageId="7" pageNumber="478">Stilbum splendidum</emphasis>
Fabricius 
<number box="[457,508,930,952]" pageId="7" pageNumber="478" value="1775.0">1775</number>
</taxonomicName>
<taxonomicName authority="Navas 1910: 45" authorityName="Navas" authorityPageNumber="45" authorityYear="1910" box="[514,790,930,952]" pageId="7" pageNumber="478" rank="variety" variety="sicula">
var. 
<emphasis box="[558,618,930,952]" italics="true" pageId="7" pageNumber="478">sicula</emphasis>
: Navás 
<number box="[701,755,930,952]" pageId="7" pageNumber="478" value="1910.0">1910</number>
: 
<number box="[765,790,930,952]" pageId="7" pageNumber="478" value="45.0">45</number>
