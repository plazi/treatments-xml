and 
<taxonomicName authority="CASEY 1905" authorityName="CASEY" authorityYear="1905" box="[360,646,1308,1330]" class="Insecta" family="Staphylinidae" genus="Pseudolathra" kingdom="Animalia" order="Coleoptera" pageId="6" pageNumber="415" phylum="Arthropoda" rank="genus">
