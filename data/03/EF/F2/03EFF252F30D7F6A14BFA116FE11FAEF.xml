, 
<taxonomicName authority="Peringuey, 1904" authorityName="Peringuey" authorityYear="1904" class="Insecta" family="Carabidae" genus="Abacetus" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="210" phylum="Arthropoda" rank="species" species="optimus">
