, 
<taxonomicName authority="Basilewsky, 1968" authorityName="Basilewsky" authorityYear="1968" box="[649,1094,295,320]" class="Insecta" family="Carabidae" genus="Axinotoma" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="209" phylum="Arthropoda" rank="species" species="latipalpis">
