<paragraph blockId="6.[151,1437,151,789]" pageId="6" pageNumber="207">
<emphasis bold="true" box="[199,315,332,357]" pageId="6" pageNumber="207">Remarks.</emphasis>
<collectingRegion box="[322,342,332,356]" country="Nigeria" name="Rivers" pageId="6" pageNumber="207">A</collectingRegion>
species spread throughout 
<collectingCountry box="[649,1045,332,357]" name="Democratic Republic of the Congo" pageId="6" pageNumber="207">
Democratic Republic of the 
<collectingRegion box="[968,1045,332,356]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="6" pageNumber="207">Congo</collectingRegion>
</collectingCountry>
, 
<collectingCountry box="[1056,1179,332,357]" name="Zimbabwe" pageId="6" pageNumber="207">Zimbabwe</collectingCountry>
, 
<collectingCountry box="[1191,1276,332,357]" name="Angola" pageId="6" pageNumber="207">Angola</collectingCountry>
, 
<collectingCountry box="[1288,1389,332,357]" name="Tanzania" pageId="6" pageNumber="207">Tanzania</collectingCountry>
and 
<collectingCountry box="[151,299,367,392]" name="Mozambique" pageId="6" pageNumber="207">Mozambique</collectingCountry>
(
