<quantity box="[595,758,1210,1234]" metricMagnitude="-3" metricUnit="m" metricValue="1.095" metricValueMax="1.18" metricValueMin="1.01" pageId="33" pageNumber="234" unit="mm" value="1.095" valueMax="1.18" valueMin="1.01">1.01‒1.18 mm</quantity>
(female 
<typeStatus box="[856,946,1210,1235]" pageId="33" pageNumber="234">allotype</typeStatus>
and 
<typeStatus box="[999,1111,1210,1234]" pageId="33" pageNumber="234">paratypes</typeStatus>
), width: 
<quantity box="[1207,1307,1210,1234]" metricMagnitude="-4" metricUnit="m" metricValue="7.3" pageId="33" pageNumber="234" unit="mm" value="0.73">0.73 mm</quantity>
(
<typeStatus box="[1321,1425,1210,1235]" pageId="33" pageNumber="234">holotype</typeStatus>
), 
<quantity box="[151,319,1245,1270]" metricMagnitude="-4" metricUnit="m" metricValue="8.3" metricValueMax="9.1" metricValueMin="7.5" pageId="33" pageNumber="234" unit="mm" value="0.83" valueMax="0.91" valueMin="0.75">0.75‒0.91 mm</quantity>
(female 
<typeStatus box="[429,519,1245,1270]" pageId="33" pageNumber="234">allotype</typeStatus>
and 
<typeStatus box="[584,699,1246,1270]" pageId="33" pageNumber="234">paratypes</typeStatus>
