<taxonomicName baseAuthorityName="Boheman" baseAuthorityYear="1848" box="[403,575,1012,1037]" class="Insecta" family="Carabidae" genus="Prothymidia" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="224" phylum="Arthropoda" rank="species" species="angusticollis">
