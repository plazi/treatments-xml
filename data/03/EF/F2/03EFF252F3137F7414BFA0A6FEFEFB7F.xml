<taxonomicName baseAuthorityName="Quedenfeldt" baseAuthorityYear="1883" box="[419,575,1049,1072]" class="Insecta" family="Carabidae" genus="Lophyra" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="224" phylum="Arthropoda" rank="species" species="uncivittata">
