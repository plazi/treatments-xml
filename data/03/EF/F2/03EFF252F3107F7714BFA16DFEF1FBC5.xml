<collectingCountry box="[731,814,977,1002]" name="Angola" pageId="20" pageNumber="221">Angola</collectingCountry>
(
<bibRefCitation author="Harold" box="[828,975,977,1002]" pageId="20" pageNumber="221" refString="Harold, E. von (1879) Bericht uber die von den Herren A. von Homeyer und P. Pogge in Angola und in Lunda - Reiche gesammelten Coleopteren. Coleopterologische Hefte, 16, 1 - 224." type="journal article" year="1879">Harold 1879</bibRefCitation>
), but with a wide distribution throughout Africa (
<collectingCountry box="[241,326,1013,1038]" name="Guinea-Bissau" pageId="20" pageNumber="221">Guinea</collectingCountry>
, 
<collectingCountry box="[342,637,1013,1038]" name="Central African Republic" pageId="20" pageNumber="221">
<collectingRegion box="[342,425,1013,1038]" country="Zambia" name="Central" pageId="20" pageNumber="221">Central</collectingRegion>
African Republic
</collectingCountry>
, 
<collectingCountry box="[653,730,1014,1038]" name="Democratic Republic of the Congo" pageId="20" pageNumber="221">
<collectingRegion box="[653,730,1014,1038]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="20" pageNumber="221">Congo</collectingRegion>
</collectingCountry>
, D. R. of the 
<collectingCountry box="[901,979,1014,1038]" name="Democratic Republic of the Congo" pageId="20" pageNumber="221">
<collectingRegion box="[901,979,1014,1038]" country="Democratic Republic of the Congo" name="Bas-Congo" pageId="20" pageNumber="221">Congo</collectingRegion>
</collectingCountry>
, 
<collectingCountry box="[994,1079,1013,1038]" name="Angola" pageId="20" pageNumber="221">Angola</collectingCountry>
, 
<collectingCountry box="[1095,1218,1013,1038]" name="Zimbabwe" pageId="20" pageNumber="221">Zimbabwe</collectingCountry>
, 
<collectingCountry box="[1233,1327,1013,1038]" name="Burundi" pageId="20" pageNumber="221">Burundi</collectingCountry>
, 
