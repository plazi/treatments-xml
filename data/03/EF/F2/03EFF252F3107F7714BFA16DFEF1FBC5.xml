, 
<collectingCountry box="[313,386,1049,1074]" name="Sudan" pageId="20" pageNumber="221">Sudan</collectingCountry>
, 
<collectingCountry box="[398,501,1049,1074]" name="Tanzania" pageId="20" pageNumber="221">Tanzania</collectingCountry>
) (
<bibRefCitation author="Ferreira" box="[523,680,1049,1074]" pageId="20" pageNumber="221" refString="Ferreira, M. C. (1965) Catalogo dos Coleopteros de Angola. Revista de Entomologia de Mocambique, 8 (2), 415 - 1317." type="journal article" year="1965">Ferreira 1965</bibRefCitation>
, 
<bibRefCitation author="Anichtchenko" box="[692,915,1049,1074]" pageId="20" pageNumber="221" refString="Anichtchenko, A. V. (2016) Carabidae of the world. Available from: http: // carabid. org (accessed 9 October 2017)" type="journal article" year="2016">Anichtchenko 2016</bibRefCitation>
