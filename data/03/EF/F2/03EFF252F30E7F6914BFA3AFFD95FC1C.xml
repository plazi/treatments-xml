<taxonomicName authorityName="Dejean" authorityYear="1826" box="[609,708,836,859]" class="Insecta" family="Carabidae" genus="Dromica" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="211" phylum="Arthropoda" rank="genus">
<emphasis box="[609,708,836,859]" italics="true" pageId="10" pageNumber="211">Dromica</emphasis>
</taxonomicName>
(
<taxonomicName authorityName="Cassola" authorityYear="2003" box="[734,896,834,859]" class="Insecta" family="Carabidae" genus="Foveodromica" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="211" phylum="Arthropoda" rank="genus">
<emphasis box="[734,896,834,859]" italics="true" pageId="10" pageNumber="211">Foveodromica</emphasis>
</taxonomicName>
) sp., 
<taxonomicName authorityName="Chaudoir" authorityYear="1878" box="[978,1126,834,859]" class="Insecta" family="Carabidae" genus="Abacetus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="211" phylum="Arthropoda" rank="species" species="distinctus">
<emphasis box="[978,1126,834,859]" italics="true" pageId="10" pageNumber="211">
<collectingRegion box="[978,998,835,858]" country="Nigeria" name="Rivers" pageId="10" pageNumber="211">A</collectingRegion>
. distinctus
</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Bates" authorityYear="1886" box="[1150,1386,834,859]" class="Insecta" family="Carabidae" genus="Disphericus" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="211" phylum="Arthropoda" rank="species" species="tarsalis">
