<taxonomicName box="[429,484,1558,1580]" class="Insecta" family="Dilaridae" genus="Dilar" kingdom="Animalia" order="Neuroptera" pageId="2" phylum="Arthropoda" rank="genus">
<emphasis box="[429,484,1558,1580]" italics="true" pageId="2">Dilar</emphasis>
</taxonomicName>
spp. 1. 
<taxonomicName box="[569,686,1558,1581]" class="Insecta" family="Dilaridae" genus="Dilar" kingdom="Animalia" order="Neuroptera" pageId="2" phylum="Arthropoda" rank="species" species="hastatus" status="sp.nov.">
