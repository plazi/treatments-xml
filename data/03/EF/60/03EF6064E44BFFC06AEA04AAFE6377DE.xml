<number box="[993,1001,865,879]" pageId="32" pageNumber="333" value="1.0">1</number>
; 
<emphasis box="[1014,1028,850,874]" italics="true" pageId="32" pageNumber="333">h</emphasis>
<number box="[1028,1036,865,879]" pageId="32" pageNumber="333" value="3.0">3</number>
<number box="[1043,1071,850,874]" pageId="32" pageNumber="333" value="42.0">42</number>
μm, markedly shorter than 
<emphasis box="[1378,1392,850,874]" italics="true" pageId="32" pageNumber="333">h</emphasis>
<number box="[1392,1400,865,879]" pageId="32" pageNumber="333" value="2.0">2</number>
; 
