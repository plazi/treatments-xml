<number box="[773,833,703,727]" pageId="29" pageNumber="330" value="2000.0">2000</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="Michael, 1888" authorityName="Michael" authorityYear="1888" box="[852,1150,703,727]" class="Arachnida" family="Malaconothridae" genus="Malaconothrus" kingdom="Animalia" order="Sarcoptiformes" pageId="29" pageNumber="330" phylum="Arthropoda" rank="species" species="tardus">
<emphasis box="[852,961,703,727]" italics="true" pageId="29" pageNumber="330">M. tardus</emphasis>
(
<bibRefCitation author="Michael" box="[975,1142,703,727]" pageId="29" pageNumber="330" refString="Michael, A. D. (1888) British Oribatidae. Vol. II. Ray Society, London." type="book" year="1888">
Michael, 
<number box="[1083,1142,703,727]" pageId="29" pageNumber="330" value="1888.0">1888</number>
</bibRefCitation>
)
</taxonomicName>
, 
<taxonomicName authority="Iglesias, Palacios-Vargas &amp; Mahunka, 2001" authorityName="Iglesias, Palacios-Vargas &amp; Mahunka" authorityYear="2001" class="Arachnida" family="Malaconothridae" genus="Malaconothrus" kingdom="Animalia" order="Sarcoptiformes" pageId="29" pageNumber="330" phylum="Arthropoda" rank="species" species="almagrensis">
<emphasis box="[1162,1336,703,727]" italics="true" pageId="29" pageNumber="330">M. almagrensis</emphasis>
<bibRefCitation author="Iglesias" pageId="29" pageNumber="330" refString="Iglesias, R., Palacios-Vargas, J. &amp; Mahunka, S. (2001) New species of Trimalaconothrus from Mexico (Acari: Oribatei: Malaconothridae). Folia Entomologia Mexicana, 40, 67 - 81." type="journal article" year="2001">
Iglesias, Palacios-Vargas &amp; Mahunka, 
<number box="[493,549,739,763]" pageId="29" pageNumber="330" value="2001.0">2001</number>
</bibRefCitation>
