<subSubSection pageId="36" pageNumber="37" type="discussion">
<paragraph blockId="36.[106,395,1509,1535]" box="[106,395,1509,1535]" pageId="36" pageNumber="37">
<emphasis box="[106,395,1509,1535]" italics="true" pageId="36" pageNumber="37">Phylogenetic considerations</emphasis>
</paragraph>
<paragraph blockId="36.[106,1074,1561,1657]" pageId="36" pageNumber="37">
<taxonomicName authorityName="Grazia" authorityYear="1987" box="[106,328,1561,1587]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="dollingi">
<emphasis box="[106,328,1561,1587]" italics="true" pageId="36" pageNumber="37">Chloropepla dollingi</emphasis>
</taxonomicName>
and 
<taxonomicName baseAuthorityName="Grazia" baseAuthorityYear="1987" box="[390,555,1562,1588]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="36" pageNumber="37" phylum="Arthropoda" rank="species" species="tucuruiensis">
<emphasis box="[390,409,1562,1588]" italics="true" pageId="36" pageNumber="37">C</emphasis>
