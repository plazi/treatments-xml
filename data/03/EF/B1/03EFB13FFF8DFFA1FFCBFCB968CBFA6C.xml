<paragraph blockId="37.[98,1066,1434,1501]" pageId="37" pageNumber="38">
<taxonomicName authorityName="Grazia, Schwertner &amp; Greve" authorityYear="2008" box="[98,279,1434,1460]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="stysi">
<emphasis box="[98,279,1434,1460]" italics="true" pageId="37" pageNumber="38">Chloropepla stysi</emphasis>
</taxonomicName>
shares one synapomorphy with 
<taxonomicName authorityName="Caroline Greve" authorityYear="2013" box="[640,808,1435,1461]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="caxiuanensis">
<emphasis box="[640,808,1435,1461]" italics="true" pageId="37" pageNumber="38">C. caxiuanensis</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Grazia-Vieira" authorityYear="1973" box="[864,977,1434,1461]" class="Insecta" family="Pentatomidae" genus="Chloropepla" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="37" pageNumber="38" phylum="Arthropoda" rank="species" species="rolstoni">
<emphasis box="[864,977,1434,1461]" italics="true" pageId="37" pageNumber="38">C. rolstoni</emphasis>
</taxonomicName>
