<location LSID="urn:lsid:plazi:treatment:03EFB13FFFB2FF9FFFC3FC0B68B3FA5E:8E9956F2FFB2FF9EFE83FA1F6BC1FA12" box="[298,389,1420,1446]" country="Brazil" municipality="Gonocoxites" name="Capsula" pageId="26" pageNumber="27" stateProvince="Go">Capsula</location>
