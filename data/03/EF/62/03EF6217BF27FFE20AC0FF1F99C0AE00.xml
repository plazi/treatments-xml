<paragraph blockId="30.[151,604,151,177]" box="[151,604,151,177]" pageId="30" pageNumber="31">
<heading bold="true" box="[151,604,151,177]" fontSize="11" level="1" pageId="30" pageNumber="31" reason="1">
<emphasis bold="true" box="[151,604,151,177]" pageId="30" pageNumber="31">
<taxonomicName authorityName="Hita Garcia &amp; Fisher" authorityYear="2012" box="[151,428,151,177]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="bonibony">
<emphasis bold="true" box="[151,428,151,177]" italics="true" pageId="30" pageNumber="31">Tetramorium bonibony</emphasis>
</taxonomicName>
species group
</emphasis>
</heading>
