<paragraph blockId="8.[151,570,1482,1507]" box="[151,570,1482,1507]" pageId="8" pageNumber="9">
<heading bold="true" box="[151,570,1482,1507]" fontSize="10" level="2" pageId="8" pageNumber="9" reason="2">
<emphasis bold="true" box="[151,570,1482,1507]" pageId="8" pageNumber="9">
<taxonomicName authorityName="Forel" authorityYear="1891" box="[151,402,1482,1507]" class="Insecta" family="Formicidae" genus="Tetramorium" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="9" phylum="Arthropoda" rank="species" species="bessonii">
<emphasis bold="true" box="[151,402,1482,1507]" italics="true" pageId="8" pageNumber="9">Tetramorium bessonii</emphasis>
</taxonomicName>
species group
</emphasis>
