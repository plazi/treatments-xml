<collectingCountry box="[386,512,823,846]" name="Madagascar" pageId="4" pageNumber="337">Madagascar</collectingCountry>
and 
<collectingCountry box="[558,631,823,846]" name="Yemen" pageId="4" pageNumber="337">Yemen</collectingCountry>
