<collectingCountry box="[113,185,1516,1539]" name="Kenya" pageId="25" pageNumber="358">Kenya</collectingCountry>
, in addition to 
<collectingCountry box="[344,418,1516,1539]" name="Yemen" pageId="25" pageNumber="358">Yemen</collectingCountry>
and 
<collectingCountry box="[470,535,1516,1539]" name="Oman" pageId="25" pageNumber="358">Oman</collectingCountry>
(
