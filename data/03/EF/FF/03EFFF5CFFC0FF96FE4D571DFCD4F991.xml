<collectingCountry box="[518,631,1490,1513]" name="Cameroon" pageId="35" pageNumber="368">Cameroon</collectingCountry>
, 
<collectingCountry box="[639,1004,1490,1513]" name="Democratic Republic of the Congo" pageId="35" pageNumber="368">Democratic Republic of the Congo</collectingCountry>
, 
<collectingCountry box="[1013,1085,1490,1513]" name="Kenya" pageId="35" pageNumber="368">Kenya</collectingCountry>
, 
<collectingCountry box="[113,244,1522,1545]" name="Madagascar" pageId="35" pageNumber="368">Madagascar</collectingCountry>
, 
<collectingCountry box="[253,348,1522,1545]" name="Namibia" pageId="35" pageNumber="368">Namibia</collectingCountry>
, 
<collectingCountry box="[357,494,1522,1545]" name="South Africa" pageId="35" pageNumber="368">South Africa</collectingCountry>
