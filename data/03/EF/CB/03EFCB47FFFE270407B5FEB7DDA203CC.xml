<taxonomicName baseAuthorityName="Marsh" baseAuthorityYear="1889" box="[739,1080,298,334]" class="Reptilia" family="Ceratopsidae" genus="Ceratops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="2" pageNumber="83" phylum="Chordata" rank="species" species="paucidens">Ceratops paucidens</taxonomicName>
