<quantity box="[221,309,487,511]" metricMagnitude="1" metricUnit="m" metricValue="1.15" metricValueMax="1.8" metricValueMin="0.5" pageId="36" pageNumber="645" unit="m" value="11.5" valueMax="18.0" valueMin="5.0">5–18 m</quantity>
