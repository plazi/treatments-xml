<geoCoordinate box="[898,1033,1425,1451]" degrees="17" direction="north" minutes="07" orientation="latitude" pageId="6" pageNumber="643" precision="15" seconds="58" value="17.132778">17°07ʹ58ʺ N</geoCoordinate>
