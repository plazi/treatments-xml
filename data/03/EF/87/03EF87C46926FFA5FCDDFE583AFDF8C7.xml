<geoCoordinate box="[1043,1197,1425,1451]" degrees="092" direction="west" minutes="48" orientation="longitude" pageId="6" pageNumber="643" precision="15" seconds="19" value="-92.80528">092°48ʹ19ʺ W</geoCoordinate>
