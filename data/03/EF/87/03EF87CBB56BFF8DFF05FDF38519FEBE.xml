Of 
<typeStatus box="[388,486,823,848]" pageId="6" pageNumber="381">holotype</typeStatus>
female, 
<quantity box="[583,685,824,848]" metricMagnitude="-2" metricUnit="m" metricValue="1.08" pageId="6" pageNumber="381" unit="mm" value="10.8">10.8 mm</quantity>
