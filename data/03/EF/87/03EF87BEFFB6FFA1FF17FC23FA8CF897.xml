, 
<taxonomicName box="[332,471,1085,1109]" class="Reptilia" family="Boidae" genus="Ungaliophis" kingdom="Animalia" order="Squamata" pageId="5" pageNumber="254" phylum="Chordata" rank="genus">Ungaliophis</taxonomicName>
