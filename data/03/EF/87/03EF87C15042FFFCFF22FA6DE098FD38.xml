<subSubSection pageId="16" pageNumber="58" type="diagnosis">
<paragraph blockId="16.[151,1437,1590,2013]" pageId="16" pageNumber="58">
<emphasis bold="true" box="[199,314,1698,1724]" pageId="16" pageNumber="58">Diagnosis</emphasis>
