and 
<taxonomicName box="[1117,1291,504,527]" class="Demospongiae" family="Cladorhizidae" genus="Cladorhiza" kingdom="Animalia" order="Poecilosclerida" pageId="37" pageNumber="65" phylum="Porifera" rank="species" species="rectangularis">
