<number box="[334,399,454,478]" pageId="13" pageNumber="41" value="19.14">19.14</number>
' W
</geoCoordinate>
, 
<date box="[447,576,454,478]" pageId="13" pageNumber="41" value="2008-12-01">
<number box="[447,512,454,478]" pageId="13" pageNumber="41" value="12.01">12.01</number>
. 
<number box="[515,576,454,478]" pageId="13" pageNumber="41" value="2008.0">2008</number>
