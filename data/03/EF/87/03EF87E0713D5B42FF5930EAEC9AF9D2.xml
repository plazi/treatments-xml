<taxonomicName authority="Distant, 1905" authorityName="Distant" authorityYear="1905" box="[1128,1436,927,952]" class="Insecta" family="Cicadidae" genus="Hyalessa" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="435" phylum="Arthropoda" rank="species" species="stratoria">
