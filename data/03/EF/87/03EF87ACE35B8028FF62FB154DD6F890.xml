<collectingCountry box="[566,651,1619,1645]" name="Austria" pageId="39" pageNumber="40">Austria</collectingCountry>
, 
<collectingCountry box="[662,747,1619,1645]" name="Croatia" pageId="39" pageNumber="40">Croatia</collectingCountry>
