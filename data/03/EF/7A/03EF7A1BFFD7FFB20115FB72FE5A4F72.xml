<typeStatus box="[168,226,1287,1313]" pageId="52" pageNumber="53">TYPE</typeStatus>
SPECIES: 
<emphasis box="[338,565,1287,1313]" italics="true" pageId="52" pageNumber="53">
<taxonomicName authority="Vea &amp; Grimaldi, 2015" authorityName="Vea &amp; Grimaldi" authorityYear="2015" box="[338,561,1287,1313]" class="Insecta" family="Pseudococcidae" genus="Gilderius" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="eukrinops" status="n. sp.">Gilderius eukrinops</taxonomicName>
