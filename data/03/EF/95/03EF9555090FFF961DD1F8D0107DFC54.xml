<quantity box="[474,553,1187,1211]" metricMagnitude="3" metricUnit="m" metricValue="1.1" pageId="11" pageNumber="12" unit="m" value="1100.0">1100m</quantity>
, 2 ♀♀, beating 
<taxonomicName box="[729,869,1187,1211]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="11" pageNumber="12" phylum="Tracheophyta" rank="species" species="ilex">
