<subSubSection pageId="22" pageNumber="23" type="biology_ecology">
<paragraph blockId="22.[151,1437,151,573]" pageId="22" pageNumber="23">
<emphasis bold="true" box="[199,299,332,357]" pageId="22" pageNumber="23">Ecology.</emphasis>
<emphasis box="[308,546,332,356]" italics="true" pageId="22" pageNumber="23">
<taxonomicName authorityName="Mikhailov" authorityYear="1990" box="[308,493,332,356]" class="Arachnida" family="Clubionidae" genus="Clubiona" kingdom="Animalia" order="Araneae" pageId="22" pageNumber="23" phylum="Arthropoda" rank="species" species="pseudosimilis">C. pseudosimilis</taxonomicName>
