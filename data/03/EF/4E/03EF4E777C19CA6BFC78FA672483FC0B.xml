<taxonomicName baseAuthorityName="Loveridge" baseAuthorityYear="1945" box="[278,442,1520,1542]" class="Reptilia" family="Scincidae" genus="Lobulia" kingdom="Animalia" order="Squamata" pageId="16" pageNumber="236" phylum="Chordata" rank="species" species="lobulus">
