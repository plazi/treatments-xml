(
<figureCitation box="[159,251,778,804]" captionStart="FIGURE 1" captionStartId="54.[151,258,1750,1774]" captionTargetBox="[187,1403,201,1658]" captionTargetId="figure-85@54.[180,1407,197,1753]" captionTargetPageId="54" captionText="FIGURE 1 a–l. Pravistylus species. Pygofer, laterally. a, P. bidentidiscus sp. n.; b, P. brachyphysis sp. n.; c, P. brachyplacus sp. n.; d, P. caenophallus sp. n.; e, P. deltoplacus sp. n.; f–g, P. dentidiscus sp. n.; h, P. digitidiscus sp. n.; i–k, P.eductus(Naudé); l, P. exquadratus (Naudé)." pageId="30" pageNumber="31">
Figs 
<quantity box="[218,251,778,804]" metricMagnitude="3" metricUnit="kg" metricValue="1.0" pageId="30" pageNumber="31" unit="t" value="1.0">1 t</quantity>
</figureCitation>
; 
<figureCitation box="[264,279,778,804]" captionStart="FIGURE 2" captionStartId="57.[151,255,1711,1735]" captionTargetBox="[198,1389,205,1619]" captionTargetId="figure-63@57.[190,1396,194,1718]" captionTargetPageId="57" captionText="FIGURE 2 a–i. Pravistylus species. Plate, part or whole structure. a, P. varicudiscus sp. n.; b, P. mecistoplacus sp. n.; c, P. digitidiscus sp. n.; d, P. tanyoplacus sp. n.; e–g, P.eductus(Naudé); h–i, P. exquadratus (Naudé)." pageId="30" pageNumber="31">2</figureCitation>
