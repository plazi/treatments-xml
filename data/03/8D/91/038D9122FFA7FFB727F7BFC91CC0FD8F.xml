<taxonomicName box="[1252,1395,1846,1869]" class="Magnoliopsida" family="Crassulaceae" genus="Echeveria" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="craigiana">
<emphasis box="[1252,1395,1846,1869]" italics="true" pageId="1" pageNumber="540">E. craigiana</emphasis>
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Crassulaceae" genus="Echeveria" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="longissima">
<emphasis italics="true" pageId="1" pageNumber="540">E. longissima</emphasis>
</taxonomicName>
, 
<taxonomicName box="[288,390,1881,1905]" class="Magnoliopsida" family="Crassulaceae" genus="Echeveria" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="lurida">
<emphasis box="[288,390,1881,1905]" italics="true" pageId="1" pageNumber="540">E. lurida</emphasis>
</taxonomicName>
, 
<taxonomicName box="[407,548,1882,1905]" class="Magnoliopsida" family="Crassulaceae" genus="Echeveria" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="recurvata">
<emphasis box="[407,548,1882,1905]" italics="true" pageId="1" pageNumber="540">E. recurvata</emphasis>
</taxonomicName>
, 
<taxonomicName box="[564,721,1881,1905]" class="Magnoliopsida" family="Crassulaceae" genus="Echeveria" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="sessiliflora">
<emphasis box="[564,721,1881,1905]" italics="true" pageId="1" pageNumber="540">E. sessiliflora</emphasis>
</taxonomicName>
, 
<taxonomicName box="[737,908,1881,1905]" class="Magnoliopsida" family="Crassulaceae" genus="Sedum" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="palmeri">
<emphasis box="[737,908,1881,1905]" italics="true" pageId="1" pageNumber="540">Sedum palmeri</emphasis>
</taxonomicName>
and 
<taxonomicName box="[968,1228,1882,1905]" class="Magnoliopsida" family="Crassulaceae" genus="Sempervivum" kingdom="Plantae" order="Saxifragales" pageId="1" pageNumber="540" phylum="Tracheophyta" rank="species" species="tectorum">
<emphasis box="[968,1228,1882,1905]" italics="true" pageId="1" pageNumber="540">Sempervivum tectorum</emphasis>
</taxonomicName>
(McKenzie 
<number box="[1372,1431,1881,1906]" pageId="1" pageNumber="540" value="1967.0">1967</number>
; Williams &amp; Granara da Willink 
<number box="[509,568,1918,1942]" pageId="1" pageNumber="540" value="1992.0">1992</number>
; Malumphy 
<number box="[709,768,1918,1942]" pageId="1" pageNumber="540" value="1997.0">1997</number>
