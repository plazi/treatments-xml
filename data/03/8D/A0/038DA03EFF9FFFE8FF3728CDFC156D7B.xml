<taxonomicName authority="NCIP" authorityName="NCIP" class="Actinopterygii" family="Apogonidae" genus="Siphamia" kingdom="Animalia" order="Perciformes" pageId="29" pageNumber="30" phylum="Chordata" rank="species" species="cyanophthalma">
