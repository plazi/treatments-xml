<caption httpUri="https://zenodo.org/record/181517/files/figure.png" pageId="11" pageNumber="12" targetBox="[366,1242,222,1673]" targetPageId="11">
<paragraph blockId="11.[151,1436,1735,1794]" pageId="11" pageNumber="12">
<emphasis bold="true" box="[151,281,1735,1759]" pageId="11" pageNumber="12">FIGURE 6.</emphasis>
<taxonomicName box="[287,484,1736,1759]" class="Insecta" family="Mycetophilidae" genus="Manota" kingdom="Animalia" order="Diptera" pageId="11" pageNumber="12" phylum="Arthropoda" rank="species" species="macrodon" status="sp. n.">
<emphasis box="[287,484,1736,1759]" italics="true" pageId="11" pageNumber="12">Manota macrodon</emphasis>
