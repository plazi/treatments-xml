<subSubSection pageId="9" pageNumber="10" type="description">
<paragraph blockId="9.[151,1436,151,338]" pageId="9" pageNumber="10">
<emphasis box="[198,317,232,257]" italics="true" pageId="9" pageNumber="10">
<typeStatus box="[198,317,232,257]" pageId="9" pageNumber="10">Paratypes</typeStatus>
</emphasis>
, 
<specimenCount box="[332,422,232,258]" pageId="9" pageNumber="10" type="male">
<number box="[332,347,232,258]" pageId="9" pageNumber="10" value="2.0">2</number>
males
</specimenCount>
with same data as 
<typeStatus box="[644,750,232,258]" pageId="9" pageNumber="10">holotype</typeStatus>
. 
<specimenCount box="[761,851,232,258]" pageId="9" pageNumber="10" type="male">
<number box="[761,776,232,258]" pageId="9" pageNumber="10" value="2.0">2</number>
males
</specimenCount>
