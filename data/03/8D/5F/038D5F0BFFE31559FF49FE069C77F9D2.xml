<collectingCountry box="[348,415,1626,1650]" name="China" pageId="19" pageNumber="20">China</collectingCountry>
(
<collectingRegion box="[430,503,1626,1650]" country="China" name="Hebei" pageId="19" pageNumber="20">Hebei</collectingRegion>
).
</paragraph>
