<collectingCountry box="[359,427,1996,2022]" name="Brazil" pageId="33" pageNumber="34">Brazil</collectingCountry>
(
<collectingRegion box="[443,610,1996,2023]" country="Brazil" name="Santa Catarina" pageId="33" pageNumber="34">Santa Catarina</collectingRegion>
) (
