<taxonomicName authorityName="Decaisne" authorityYear="1841" box="[1070,1310,260,284]" class="Ulvophyceae" family="Halimedaceae" genus="Halimeda" kingdom="Plantae" order="Bryopsidales" pageId="72" pageNumber="73" phylum="Chlorophyta" rank="species" species="macroloba">
<emphasis box="[1070,1310,260,284]" italics="true" pageId="72" pageNumber="73">Halimeda macroloba</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Decaisne" authorityYear="1842" class="Ulvophyceae" family="Halimedaceae" genus="Halimeda" kingdom="Plantae" order="Bryopsidales" pageId="72" pageNumber="73" phylum="Chlorophyta" rank="species" species="cylindracea">
<emphasis italics="true" pageId="72" pageNumber="73">Halimeda cylindracea</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="C. Agardh" authorityYear="1817" baseAuthorityName="M. Vahl" box="[297,503,296,320]" class="Ulvophyceae" family="Caulerpaceae" genus="Caulerpa" kingdom="Plantae" order="Bryopsidales" pageId="72" pageNumber="73" phylum="Chlorophyta" rank="species" species="taxifolia">
<emphasis box="[297,503,296,320]" italics="true" pageId="72" pageNumber="73">Caulerpa taxifolia</emphasis>
