<quantity box="[709,815,1903,1927]" metricMagnitude="-3" metricUnit="m" metricValue="7.44" pageId="4" pageNumber="595" unit="mm" value="7.44">7.44 mm</quantity>
; head width 
<quantity box="[964,1070,1903,1927]" metricMagnitude="-3" metricUnit="m" metricValue="1.81" pageId="4" pageNumber="595" unit="mm" value="1.81">1.81 mm</quantity>
; crown length×width 0.71× 
<quantity metricMagnitude="-4" metricUnit="m" metricValue="7.199999999999999" pageId="4" pageNumber="595" unit="mm" value="0.72">0.72 mm</quantity>
