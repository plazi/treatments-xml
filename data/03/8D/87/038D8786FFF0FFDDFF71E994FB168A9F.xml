<subSubSection pageId="15" pageNumber="16" type="diagnosis">
<paragraph blockId="15.[151,1436,649,998]" pageId="15" pageNumber="16">
<emphasis bold="true" box="[201,323,793,818]" inLineHeading="true" pageId="15" pageNumber="16" reason="1">Diagnosis.</emphasis>
