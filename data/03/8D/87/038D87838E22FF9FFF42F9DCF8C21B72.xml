<taxonomicName authority="Verhoeff, 1899" authorityName="Verhoeff" authorityYear="1899" box="[296,753,1822,1844]" class="Chilopoda" family="Geophilidae" genus="Eurygeophilus" kingdom="Animalia" order="Geophilomorpha" pageId="37" pageNumber="338" phylum="Arthropoda" rank="species" species="multistiliger">
