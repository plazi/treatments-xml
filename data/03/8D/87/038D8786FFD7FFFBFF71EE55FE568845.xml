<quantity box="[698,800,188,212]" metricMagnitude="1" metricUnit="m" metricValue="7.5" metricValueMax="15.0" metricValueMin="0.0" pageId="41" pageNumber="42" unit="m" value="75.0" valueMax="150.0" valueMin="0.0">0–150 m</quantity>
