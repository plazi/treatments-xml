averaged 
<quantity box="[644,734,1358,1380]" metricMagnitude="-3" metricUnit="m" metricValue="4.61" pageId="3" pageNumber="336" unit="mm" value="4.61">4.61 mm</quantity>
in length (range, 
<quantity metricMagnitude="-3" metricUnit="m" metricValue="4.55" metricValueMax="5.4" metricValueMin="3.7" pageId="3" pageNumber="336" unit="mm" value="4.55" valueMax="5.4" valueMin="3.7">3.70– 5.40 mm</quantity>
); the 
<specimenCount box="[298,417,1385,1407]" pageId="3" pageNumber="336" type="male">three males</specimenCount>
, 
<quantity box="[429,518,1385,1407]" metricMagnitude="-3" metricUnit="m" metricValue="4.4" pageId="3" pageNumber="336" unit="mm" value="4.4">4.40 mm</quantity>
(range, 4.00– 
<quantity box="[660,754,1385,1407]" metricMagnitude="-3" metricUnit="m" metricValue="4.8" pageId="3" pageNumber="336" unit="mm" value="4.8">4.80 mm</quantity>
).
