<taxonomicName authority="Lonsdale, 1844" authorityName="Lonsdale" authorityYear="1844" box="[814,1090,343,369]" class="Stenolaemata" family="Stenoporidae" genus="Stenopora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trepostomatida" pageId="19" pageNumber="170" phylum="Bryozoa" rank="genus">
