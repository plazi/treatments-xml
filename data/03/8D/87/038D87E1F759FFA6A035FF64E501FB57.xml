<taxonomicName authorityName="Trizna" authorityYear="1958" box="[133,311,1140,1166]" class="Stenolaemata" family="Fistuliporidae" genus="Fistulipora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cystoporida" pageId="9" pageNumber="160" phylum="Bryozoa" rank="species" species="sana">
<emphasis box="[133,311,1140,1166]" italics="true" pageId="9" pageNumber="160">Fistulipora sana</emphasis>
</taxonomicName>
differs from 
<taxonomicName authority="Trizna, 1958" authorityName="Trizna" authorityYear="1958" box="[467,774,1140,1167]" class="Stenolaemata" family="Fistuliporidae" genus="Fistulipora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cystoporida" pageId="9" pageNumber="160" phylum="Bryozoa" rank="species" species="taidonensis">
