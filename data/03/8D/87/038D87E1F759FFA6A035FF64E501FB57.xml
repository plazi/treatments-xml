in 
<taxonomicName authorityName="Trizna" authorityYear="1958" box="[133,278,1236,1262]" class="Stenolaemata" family="Fistuliporidae" genus="Fistulipora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cystoporida" pageId="9" pageNumber="160" phylum="Bryozoa" rank="species" species="taidonensis">
