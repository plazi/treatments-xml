</taxonomicName>
and 
<taxonomicName baseAuthorityName="Quoy and Gaimard" baseAuthorityYear="1834" box="[1190,1389,1957,1981]" class="Ascidiacea" family="Styelidae" genus="Polycarpa" kingdom="Animalia" order="Stolidobranchia" pageId="51" pageNumber="52" phylum="Chordata" rank="species" species="aurata">
