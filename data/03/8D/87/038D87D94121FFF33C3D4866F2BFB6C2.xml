<emphasis bold="true" box="[812,918,1065,1091]" italics="true" pageId="12" pageNumber="429">Chicoreus</emphasis>
(
<emphasis bold="true" box="[934,1011,1063,1089]" italics="true" pageId="12" pageNumber="429">Siratus</emphasis>
) 
<emphasis bold="true" box="[1027,1150,1065,1091]" italics="true" pageId="12" pageNumber="429">hennequini</emphasis>
