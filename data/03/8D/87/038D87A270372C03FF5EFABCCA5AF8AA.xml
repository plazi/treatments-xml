<paragraph blockId="7.[151,596,1434,1487]" box="[151,596,1465,1487]" pageId="7" pageNumber="50">
<treatmentCitationGroup box="[151,596,1465,1487]" pageId="7" pageNumber="50">
<taxonomicName authority="Curtiss, 1938: 174" authorityName="Curtiss" authorityPageNumber="174" authorityYear="1938" box="[151,516,1465,1487]" class="Malacostraca" family="Cancridae" genus="Cancer" kingdom="Animalia" order="Decapoda" pageId="7" pageNumber="50" phylum="Arthropoda" rank="species" species="ballantei" status="n. syn.">
