<collectionCode box="[942,989,1367,1391]" country="Germany" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34838" lsid="urn:lsid:biocol.org:col:34838" name="Forschungsinstitut und Natur-Museum Senckenberg" pageId="7" pageNumber="158">SMF</collectionCode>
