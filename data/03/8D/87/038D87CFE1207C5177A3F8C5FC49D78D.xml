<quantity box="[445,581,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="1.8" metricValueMin="1.7" pageId="4" pageNumber="443" unit="mm" value="1.75" valueMax="1.8" valueMin="1.7">1.7–1.8 mm</quantity>
, wing length 
<quantity box="[733,871,152,176]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" metricValueMax="2.1" metricValueMin="1.9" pageId="4" pageNumber="443" unit="mm" value="2.0" valueMax="2.1" valueMin="1.9">1.9–2.1 mm</quantity>
