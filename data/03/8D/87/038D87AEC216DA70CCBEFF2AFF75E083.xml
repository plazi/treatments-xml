<taxonomicName authority="Lin &amp; Chan, 2011" authorityName="Lin &amp; Chan" authorityYear="2011" box="[658,1005,1065,1090]" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="413" phylum="Arthropoda" rank="species" species="aurantia">
