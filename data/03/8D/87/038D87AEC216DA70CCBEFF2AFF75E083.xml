Benedict, 
<number box="[444,500,1030,1054]" pageId="8" pageNumber="413" value="1902.0">1902</number>
</bibRefCitation>
</taxonomicName>
[off South Carolina, northwestern Atlantic], 
<taxonomicName authority="Ahyong, 2014" authorityName="Ahyong" authorityYear="2014" box="[1040,1342,1029,1054]" class="Malacostraca" family="Galatheidae" genus="Munidopsis" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="413" phylum="Arthropoda" rank="species" species="alcocki">
