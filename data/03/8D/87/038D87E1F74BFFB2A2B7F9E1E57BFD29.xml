<taxonomicName authorityName="Astrova &amp; Morozova" authorityYear="1956" class="Stenolaemata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cryptostomida" pageId="29" pageNumber="180" phylum="Bryozoa" rank="subOrder" subOrder="Rhabdomesina">Rhabdomesina</taxonomicName>
, 
<taxonomicName authorityName="Vine" authorityYear="1884" box="[245,408,534,560]" class="Stenolaemata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cryptostomata" pageId="29" pageNumber="180" phylum="Bryozoa" rank="order">Cryptostomata</taxonomicName>
), and put the genus 
<taxonomicName authority="Shishova, 1964" authorityName="Shishova" authorityYear="1964" class="Stenolaemata" family="Rhabdomesidae" genus="Neorhombopora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cryptostomida" pageId="29" pageNumber="180" phylum="Bryozoa" rank="genus">
<emphasis box="[611,772,534,560]" italics="true" pageId="29" pageNumber="180">Neorhombopora</emphasis>
<bibRefCitation author="SHISHOVA N. A." box="[132,298,566,592]" pageId="29" pageNumber="180" pagination="52 - 57" refId="ref29091" refString="SHISHOVA N. A. 1964. - New Late Permian Rhabdomesonidae of the Soviet Union. Paleontologicheskiy Zhurnal 1964 (3): 52 - 57 [in Russian]." type="journal article" year="1964">Shishova, 1964</bibRefCitation>
</taxonomicName>
to the genus 
<taxonomicName authority="Moore, 1929" authorityName="Moore" authorityYear="1929" box="[447,771,566,593]" class="Stenolaemata" family="Rhabdomesidae" genus="Megacanthopora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cryptostomida" pageId="29" pageNumber="180" phylum="Bryozoa" rank="genus">
<emphasis box="[447,617,566,592]" italics="true" pageId="29" pageNumber="180">Megacanthopora</emphasis>
<bibRefCitation author="MOORE R. C." box="[625,771,566,593]" pageId="29" pageNumber="180" pagination="121 - 156" refId="ref27370" refString="MOORE R. C. 1929. - A bryozoan faunule from the Upper Graham Formation, Pennsylvanian, of north central Texas. Journal of Paleontology 3: 1 - 27, 121 - 156." type="journal article" year="1929">Moore, 1929</bibRefCitation>
</taxonomicName>
, because the 
<typeStatus box="[259,305,598,624]" pageId="29" pageNumber="180">type</typeStatus>
species 
<taxonomicName authority="Ulrich, 1884" authorityName="Ulrich" authorityYear="1884" box="[386,724,598,624]" class="Stenolaemata" family="Rhabdomesidae" genus="Rhombopora" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cryptostomida" pageId="29" pageNumber="180" phylum="Bryozoa" rank="species" species="crassa">
<emphasis box="[386,580,598,624]" italics="true" pageId="29" pageNumber="180">Rhombopora crassa</emphasis>
<bibRefCitation author="ULRICH E. O." box="[586,724,598,624]" pageId="29" pageNumber="180" pagination="24 - 51" refId="ref29652" refString="ULRICH E. O. 1884. - American Palaeozoic Bryozoa. The Journal of the Cincinnati Society of Natural History 8: 24 - 51." type="journal article" year="1884">Ulrich, 1884</bibRefCitation>
</taxonomicName>
