<quantity box="[711,799,1722,1746]" metricMagnitude="1" metricUnit="m" metricValue="2.9" metricValueMax="5.5" metricValueMin="0.3" pageId="16" pageNumber="17" unit="m" value="29.0" valueMax="55.0" valueMin="3.0">
