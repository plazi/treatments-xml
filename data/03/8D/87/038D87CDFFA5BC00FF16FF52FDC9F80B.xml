<quantity box="[385,515,1915,1940]" metricMagnitude="2" metricUnit="m" metricValue="5.765" metricValueMax="7.68" metricValueMin="3.85" pageId="21" pageNumber="78" unit="m" value="576.5" valueMax="768.0" valueMin="385.0">
