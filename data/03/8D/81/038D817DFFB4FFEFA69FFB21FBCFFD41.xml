<taxonomicName authorityName="Miljutin &amp; Miljutina" authorityYear="2009" box="[318,500,306,331]" class="Nematoda incertae sedis" family="Benthimermithidae" genus="Trophomera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Benthimermithida" pageId="14" pageNumber="187" phylum="Nematoda" rank="species" species="senckenbergi" status="sp. n.">
<emphasis box="[318,500,306,331]" italics="true" pageId="14" pageNumber="187">T. senckenbergi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[508,570,306,332]" pageId="14" pageNumber="187" rank="species">sp. n.</taxonomicNameLabel>
</paragraph>
<paragraph blockId="14.[151,1436,152,719]" pageId="14" pageNumber="187">
<taxonomicName authorityName="Miljutin &amp; Miljutina" authorityYear="2009" box="[198,323,345,371]" class="Nematoda incertae sedis" family="Benthimermithidae" genus="Trophomera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Benthimermithida" pageId="14" pageNumber="187" phylum="Nematoda" rank="species" species="pacifica" status="sp. n.">
<emphasis box="[198,212,345,370]" italics="true" pageId="14" pageNumber="187">T</emphasis>
. 
<emphasis box="[228,323,345,370]" italics="true" pageId="14" pageNumber="187">pacifica</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[330,393,345,371]" pageId="14" pageNumber="187" rank="species">sp. n.</taxonomicNameLabel>
differs from 
<taxonomicName baseAuthorityName="Miljutin" baseAuthorityYear="2004" box="[550,744,345,370]" class="Nematoda incertae sedis" family="Benthimermithidae" genus="Trophomera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Benthimermithida" pageId="14" pageNumber="187" phylum="Nematoda" rank="species" species="pseudominuta">
