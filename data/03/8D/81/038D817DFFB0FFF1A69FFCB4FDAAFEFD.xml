<quantity box="[1065,1157,1702,1728]" metricMagnitude="-3" metricUnit="m" metricValue="1.5" pageId="14" pageNumber="187" unit="mm" value="1.5">1.5 mm</quantity>
vs. 
<quantity box="[1209,1356,1702,1728]" metricMagnitude="-1" metricUnit="m" metricValue="1.175" metricValueMax="1.7" metricValueMin="0.65" pageId="14" pageNumber="187" unit="mm" value="117.5" valueMax="170.0" valueMin="65.0">65–170 mm</quantity>
