<taxonomicNameLabel box="[1373,1436,1625,1651]" pageId="14" pageNumber="187" rank="species">sp. n.</taxonomicNameLabel>
and 
<taxonomicName authority="Miljutin, 2004" authorityName="Miljutin" authorityYear="2004" box="[200,570,1664,1690]" class="Nematoda incertae sedis" family="Benthimermithidae" genus="Trophomera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Benthimermithida" pageId="14" pageNumber="187" phylum="Nematoda" rank="species" species="pseudominuta">
