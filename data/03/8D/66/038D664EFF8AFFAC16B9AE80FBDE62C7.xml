, abactinal surface; 
<emphasis bold="true" box="[807,820,1498,1515]" pageId="20" pageNumber="937">B</emphasis>
, actinal surface; 
<emphasis bold="true" box="[961,975,1498,1515]" pageId="20" pageNumber="937">C</emphasis>
, abactinal arm disk surface; 
