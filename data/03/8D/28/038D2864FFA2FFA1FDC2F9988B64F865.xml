<subSubSection pageId="16" pageNumber="17" type="discussion">
<paragraph blockId="16.[189,303,1845,1871]" box="[189,303,1845,1871]" pageId="16" pageNumber="17">
<heading bold="true" box="[189,303,1845,1871]" fontSize="11" level="3" pageId="16" pageNumber="17" reason="3">
