<emphasis bold="true" box="[199,331,1049,1074]" pageId="8" pageNumber="539">Host plant.</emphasis>
<taxonomicName authority="(Miq.) Seem." authorityName="(Miq.) Seem." box="[338,628,1049,1074]" class="Magnoliopsida" family="Araliaceae" genus="Aralia" kingdom="Plantae" order="Apiales" pageId="8" pageNumber="539" phylum="Tracheophyta" rank="species" species="elata">
<emphasis box="[338,473,1049,1074]" italics="true" pageId="8" pageNumber="539">Aralia elata</emphasis>
(Miq.) Seem.
</taxonomicName>
(
<taxonomicName box="[643,769,1049,1074]" class="Magnoliopsida" family="Araliaceae" kingdom="Plantae" order="Apiales" pageId="8" pageNumber="539" phylum="Tracheophyta" rank="family">Araliaceae</taxonomicName>
