<collectingCountry box="[377,445,1750,1773]" name="South Korea" pageId="7" pageNumber="538">Korea</collectingCountry>
(
<collectionCode box="[460,547,1750,1774]" country="Switzerland" httpUri="http://grbio.org/cool/1d5a-4efs" name="Naturhistorisches Museum, Basel" pageId="7" pageNumber="538">NHMB</collectionCode>
