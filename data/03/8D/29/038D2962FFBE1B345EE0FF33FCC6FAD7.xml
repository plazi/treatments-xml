<collectingRegion box="[401,492,589,615]" country="United States of America" name="Arizona" pageId="37" pageNumber="74">Arizona</collectingRegion>
, [
<collectingCountry box="[510,573,588,615]" name="United States of America" pageId="37" pageNumber="74">USA</collectingCountry>
].
</paragraph>
