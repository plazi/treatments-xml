<collectionCode box="[468,557,484,510]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="37" pageNumber="74" type="Museum">USNM</collectionCode>
