<taxonomicName authorityName="Ehrman" authorityYear="1893" box="[319,504,906,932]" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="12" pageNumber="49" phylum="Arthropoda" rank="species" species="denussa">
<emphasis box="[319,504,906,932]" italics="true" pageId="12" pageNumber="49">Catocala denussa</emphasis>
</taxonomicName>
: the 
<typeStatus box="[562,661,906,932]" pageId="12" pageNumber="49">holotype</typeStatus>
male [
<collectionCode box="[738,837,906,932]" country="USA" httpUri="http://grbio.org/cool/daw7-8h44" name="The Cleveland Museum of Natural History" pageId="12" pageNumber="49" type="Museum">CMNH</collectionCode>
