<taxonomicName authority="Hulst" authorityName="Hulst. Th" authorityYear="1884" class="Insecta" family="Noctuidae" genus="Catocala" kingdom="Animalia" order="Lepidoptera" pageId="29" pageNumber="66" phylum="Arthropoda" rank="species" species="phoebe">
