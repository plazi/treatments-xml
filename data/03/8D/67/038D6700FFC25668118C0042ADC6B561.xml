<subSubSection pageId="126" pageNumber="127" type="etymology">
<paragraph blockId="126.[151,1436,1732,2008]" pageId="126" pageNumber="127">
<emphasis bold="true" box="[199,337,1805,1829]" pageId="126" pageNumber="127">Etymology:</emphasis>
