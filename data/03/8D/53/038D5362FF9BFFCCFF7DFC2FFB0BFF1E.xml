<subSubSection box="[151,853,1007,1032]" pageId="1" pageNumber="92" type="materials_examined">
<paragraph blockId="1.[151,1437,1007,2005]" box="[151,853,1007,1032]" pageId="1" pageNumber="92">
<emphasis bold="true" box="[151,306,1007,1032]" pageId="1" pageNumber="92">
<typeStatus box="[151,210,1007,1032]" pageId="1" pageNumber="92">Type</typeStatus>
species.
</emphasis>
<taxonomicName box="[313,596,1007,1032]" class="Malacostraca" family="Eriopisidae" genus="Paraflagitopisa" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="1" pageNumber="92" phylum="Arthropoda" rank="species" species="excavata" status="sp. nov.">
<emphasis box="[313,596,1007,1032]" italics="true" pageId="1" pageNumber="92">Paraflagitopisa excavata</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[602,691,1007,1032]" pageId="1" pageNumber="92" rank="species">
<emphasis bold="true" box="[602,691,1007,1032]" pageId="1" pageNumber="92">sp. nov.</emphasis>
