<emphasis bold="true" box="[151,432,1051,1078]" pageId="18" pageNumber="19">
<taxonomicName authority="Published, 2012" authorityName="Published" authorityYear="2012" box="[151,358,1052,1078]" class="Insecta" family="Crambidae" genus="Elophila" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="18" pageNumber="19" phylum="Arthropoda" rank="species" species="ealensis" status="sp. n.">
<emphasis bold="true" box="[151,358,1052,1078]" italics="true" pageId="18" pageNumber="19">Elophila ealensis</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[366,432,1051,1077]" pageId="18" pageNumber="19" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
