</materialsCitation>
<materialsCitation ID-GBIF-Occurrence="3342274566" box="[362,1093,1807,1832]" country="Iran" isTailEnumeration="true" location="Alborz Mountains" pageId="29" pageNumber="30" specimenCount="1" stateProvince="Ardabil">
