<paragraph blockId="31.[151,1436,289,1141]" box="[151,518,289,314]" pageId="31" pageNumber="32">
<typeStatus box="[151,207,290,314]" pageId="31" pageNumber="32">Type</typeStatus>
locality: D.R. 
<collectingCountry box="[372,450,289,313]" name="Democratic Republic of the Congo" pageId="31" pageNumber="32">Congo</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:03C387FBFFDA6563FF434295FEA4C51F:8EB56036FFDB6563FE194045FDA5C053" box="[461,514,289,313]" country="Democratic Republic of the Congo" name="Eala" pageId="31" pageNumber="32">Eala</location>
