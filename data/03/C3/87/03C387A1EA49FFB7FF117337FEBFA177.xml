<quantity box="[460,590,463,487]" metricMagnitude="-3" metricUnit="m" metricValue="5.4" metricValueMax="6.6" metricValueMin="4.2" pageId="3" pageNumber="148" unit="mm" value="5.4" valueMax="6.6" valueMin="4.2">4.2−6.6 mm</quantity>
, ♀ 6.0− 
<quantity box="[679,764,464,487]" metricMagnitude="-3" metricUnit="m" metricValue="7.3" pageId="3" pageNumber="148" unit="mm" value="7.3">7.3 mm</quantity>
