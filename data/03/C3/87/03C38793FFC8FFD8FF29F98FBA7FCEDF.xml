<subSubSection pageId="27" pageNumber="248" type="diagnosis">
<paragraph blockId="27.[136,1452,1742,2093]" pageId="27" pageNumber="248">
<emphasis bold="true" box="[136,261,1742,1768]" pageId="27" pageNumber="248">Diagnosis:</emphasis>
