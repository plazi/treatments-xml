<paragraph blockId="32.[151,1437,654,1650]" box="[151,430,654,679]" pageId="32" pageNumber="33">
<materialsCitation box="[151,430,654,679]" country="Nigeria" location="Nigeria" pageId="32" pageNumber="33" specimenCount="1" typeStatus="holotype">
<typeStatus box="[151,207,655,679]" pageId="32" pageNumber="33">Type</typeStatus>
locality: N. 
<collectingCountry box="[346,430,654,678]" name="Nigeria" pageId="32" pageNumber="33">Nigeria</collectingCountry>
