<taxonomicName box="[151,257,1737,1760]" class="Magnoliopsida" family="Tetradiclidaceae" genus="Peganum" kingdom="Plantae" order="Sapindales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="genus">
<emphasis box="[151,257,1737,1760]" italics="true" pageId="27" pageNumber="28">Peganum</emphasis>
</taxonomicName>
(
<taxonomicName box="[271,453,1735,1760]" class="Magnoliopsida" family="Zygophyllaceae" kingdom="Plantae" order="Zygophyllales" pageId="27" pageNumber="28" phylum="Tracheophyta" rank="family">Zygophyllaceae</taxonomicName>
