<taxonomicName authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Tetradiclidaceae" genus="Peganum" kingdom="Plantae" order="Sapindales" pageId="45" pageNumber="46" phylum="Tracheophyta" rank="species" species="harmala">
