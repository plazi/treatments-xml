TYPE LOCALITY. “ 
<location LSID="urn:lsid:plazi:treatment:03C387F2FFBCFF89FF5B4A7D5755F884:8EB5603FFFBCFF89FEA74A855429FA6C" box="[363,593,1447,1472]" country="Iran" name="Duz-ab in Kirmano" pageId="48" pageNumber="49" stateProvince="Balochistan">Duz-ab in Kirmano</location>
