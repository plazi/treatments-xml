<subSubSection pageId="16" pageNumber="237" type="diagnosis">
<paragraph blockId="16.[136,1452,158,1049]" pageId="16" pageNumber="237">
<emphasis bold="true" box="[136,289,158,184]" pageId="16" pageNumber="237">Diagnosis:—</emphasis>
