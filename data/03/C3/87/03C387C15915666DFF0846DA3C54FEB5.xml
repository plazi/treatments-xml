<subSubSection box="[199,1382,151,177]" pageId="11" pageNumber="468" type="etymology">
<paragraph blockId="11.[151,1436,151,285]" box="[199,1382,151,177]" pageId="11" pageNumber="468">
<emphasis bold="true" box="[199,334,151,177]" pageId="11" pageNumber="468">Etymology.</emphasis>
