(incl. 
<collectingRegion box="[555,657,1233,1259]" country="Italy" name="Sardegna" pageId="6" pageNumber="231">Sardinia</collectingRegion>
), 
<collectingCountry box="[681,759,1233,1259]" name="Latvia" pageId="6" pageNumber="231">Latvia</collectingCountry>
,? 
<collectingRegion box="[785,885,1233,1259]" country="Portugal" name="Madeira" pageId="6" pageNumber="231">Madeira</collectingRegion>
(
<collectingCountry box="[904,1010,1233,1259]" name="Portugal" pageId="6" pageNumber="231">Portugal</collectingCountry>
), 
<collectingCountry box="[1034,1105,1233,1259]" name="Malta" pageId="6" pageNumber="231">Malta</collectingCountry>
, 
