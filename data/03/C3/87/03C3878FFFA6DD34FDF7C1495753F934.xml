, 
<collectingCountry box="[552,635,1198,1224]" name="France" pageId="6" pageNumber="231">France</collectingCountry>
, 
<collectingCountry box="[650,762,1198,1224]" name="Germany" pageId="6" pageNumber="231">Germany</collectingCountry>
, 
<collectingCountry box="[778,938,1198,1224]" name="United Kingdom" pageId="6" pageNumber="231">Great Britain</collectingCountry>
, 
<collectingCountry box="[954,1040,1198,1224]" name="Greece" pageId="6" pageNumber="231">Greece</collectingCountry>
, 
<collectingCountry box="[1055,1161,1198,1224]" name="Hungary" pageId="6" pageNumber="231">Hungary</collectingCountry>
, 
<collectingCountry box="[1176,1265,1198,1224]" name="Iceland" pageId="6" pageNumber="231">Iceland</collectingCountry>
, 
<collectingCountry box="[317,403,1233,1259]" name="Ireland" pageId="6" pageNumber="231">Ireland</collectingCountry>
