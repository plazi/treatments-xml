<collectingCountry box="[1000,1151,1655,1679]" name="Mozambique" pageId="11" pageNumber="12">Mozambique</collectingCountry>
, 
<collectingCountry box="[1167,1317,1655,1679]" name="South Africa" pageId="11" pageNumber="12">South Africa</collectingCountry>
, 
<collectingCountry box="[1333,1432,1655,1679]" name="Namibia" pageId="11" pageNumber="12">Namibia</collectingCountry>
, 
<collectingCountry box="[151,228,1692,1716]" name="Democratic Republic of the Congo" pageId="11" pageNumber="12">Congo</collectingCountry>
