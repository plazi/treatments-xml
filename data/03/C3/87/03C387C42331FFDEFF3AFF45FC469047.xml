<taxonomicName box="[1340,1436,762,787]" class="Insecta" family="Cerambycidae" genus="America" kingdom="Animalia" order="Coleoptera" pageId="8" pageNumber="32" phylum="Arthropoda" rank="genus">
<collectingCountry box="[1340,1436,762,787]" name="United States of America" pageId="8" pageNumber="9">America</collectingCountry>
</taxonomicName>
