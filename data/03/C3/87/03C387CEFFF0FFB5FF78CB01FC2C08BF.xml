<taxonomicName authorityName="Distant" authorityYear="1911" class="Insecta" family="Coreidae" genus="Turrana" kingdom="Animalia" order="Hemiptera" pageId="6" pageNumber="91" phylum="Arthropoda" rank="species" species="abnormis">
