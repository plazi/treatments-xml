<taxonomicName authority="Chamberlin, 1913" authorityName="Chamberlin" authorityYear="1913" box="[359,923,566,590]" class="Chilopoda" family="Lithobiidae" genus="Lithobius" kingdom="Animalia" order="Lithobiomorpha" pageId="1" pageNumber="57" phylum="Arthropoda" rank="species" species="puritanus" subGenus="Sigibius">
