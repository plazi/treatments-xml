locality: D.R. 
<collectingCountry box="[372,449,1511,1535]" name="Democratic Republic of the Congo" pageId="43" pageNumber="44">Congo</collectingCountry>
, 
<location LSID="urn:lsid:plazi:treatment:03C387FBFFEF6550FF43400BFB45C09B:8EB56036FFEF6557FE194483FDC6C495" box="[461,609,1511,1535]" country="Democratic Republic of the Congo" name="Lubumbasha" pageId="43" pageNumber="44">Lubumbasha</location>
