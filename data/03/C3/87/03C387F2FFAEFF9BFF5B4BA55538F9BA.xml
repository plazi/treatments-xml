</taxonomicName>
, 
<taxonomicName box="[1260,1380,1383,1408]" class="Magnoliopsida" family="Euphorbiaceae" genus="Euphorbia" kingdom="Plantae" order="Malpighiales" pageId="34" pageNumber="35" phylum="Tracheophyta" rank="genus">
