<collectingCountry box="[325,373,1811,1834]" name="Iran" pageId="25" pageNumber="26">Iran</collectingCountry>
, 
<collectingCountry box="[384,543,1810,1835]" name="Turkmenistan" pageId="25" pageNumber="26">Turkmenistan</collectingCountry>
