<subSubSection box="[151,604,295,322]" pageId="23" pageNumber="24" type="nomenclature">
<paragraph blockId="23.[151,604,295,322]" box="[151,604,295,322]" pageId="23" pageNumber="24">
<heading bold="true" box="[151,604,295,322]" fontSize="11" level="1" pageId="23" pageNumber="24" reason="1">
<emphasis bold="true" box="[151,604,295,322]" pageId="23" pageNumber="24">
Genus 
<taxonomicName authority="Lyman, 1865" authorityName="Lyman" authorityYear="1865" box="[238,604,295,322]" class="Ophiuroidea" family="Amphiuridae" genus="Ophiophragmus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Ophiurida" pageId="23" pageNumber="24" phylum="Echinodermata" rank="genus">
<emphasis bold="true" box="[238,435,295,321]" italics="true" pageId="23" pageNumber="24">Ophiophragmus</emphasis>
<bibRefCitation author="Lyman, T." bookContentInfo="203 pp." box="[442,604,296,322]" journalOrPublisher="University Press: Welch, Bigelow, &amp; Company, Cambridge" pageId="23" pageNumber="24" refId="ref50591" refString="Lyman, T. (1865) Ophiuridae and Astrophytidae. University Press: Welch, Bigelow, &amp; Company, Cambridge, 203 pp." title="Ophiuridae and Astrophytidae" type="book" year="1865">Lyman, 1865</bibRefCitation>
</taxonomicName>
</emphasis>
