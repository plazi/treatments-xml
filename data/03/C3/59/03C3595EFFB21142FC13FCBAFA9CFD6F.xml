<emphasis italics="true" pageId="9" pageNumber="18">
<taxonomicName box="[1317,1409,337,360]" class="Magnoliopsida" family="Putranjivaceae" genus="Drypetes" kingdom="Plantae" order="Malpighiales" pageId="9" pageNumber="18" phylum="Tracheophyta" rank="genus">Drypetes</taxonomicName>
