'S
</geoCoordinate>
<geoCoordinate box="[245,352,223,247]" direction="east" orientation="longitude" pageId="5" pageNumber="27" precision="925" value="130.28334">
<number box="[245,291,223,247]" pageId="5" pageNumber="27" value="130.0">130</number>
° 
<number box="[301,329,223,247]" pageId="5" pageNumber="27" value="17.0">17</number>
'E
</geoCoordinate>
, 
