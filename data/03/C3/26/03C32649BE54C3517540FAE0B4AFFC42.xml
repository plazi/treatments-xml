species: 
<taxonomicName authority="Uhler, 1888: 82" authorityName="Uhler" authorityPageNumber="82" authorityYear="1888" box="[588,943,1419,1443]" class="Insecta" family="Cicadidae" genus="Prunasis" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="1591" phylum="Arthropoda" rank="species" species="venosa">
