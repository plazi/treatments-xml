Mironov, 
<number box="[434,487,1304,1326]" pageId="3" pageNumber="104" value="1989.0">1989</number>
</taxonomicName>
, 
<emphasis box="[500,665,1304,1326]" italics="true" pageId="3" pageNumber="104">Vestnik Zoologii</emphasis>
<emphasis bold="true" box="[673,724,1304,1325]" pageId="3" pageNumber="104">
<number box="[673,724,1304,1325]" pageId="3" pageNumber="104" value="1989.0">1989</number>
</emphasis>
(
<number box="[741,755,1304,1326]" pageId="3" pageNumber="104" value="3.0">3</number>
): 
<number box="[777,804,1304,1326]" pageId="3" pageNumber="104" value="14.0">14</number>
, figs 
<number box="[863,874,1304,1326]" pageId="3" pageNumber="104" value="3.0">3</number>
a–d. 
<typeStatus box="[926,1020,1304,1326]" pageId="3" pageNumber="104">Holotype</typeStatus>
♀ (
<collectionCode box="[1062,1115,1304,1326]" httpUri="http://grbio.org/cool/vmfx-g997" name="Zoological Institute, Russian Academy of Sciences" pageId="3" pageNumber="104">ZISP</collectionCode>
, examined), 
<collectingCountry box="[1249,1351,1304,1326]" name="Mongolia" pageId="3" pageNumber="104">Mongolia</collectingCountry>
