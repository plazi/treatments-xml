<collectingCountry box="[1171,1302,843,867]" name="Kyrgyzstan" pageId="12" pageNumber="113">Kyrgyzstan</collectingCountry>
, 
<collectingCountry box="[1319,1432,842,867]" name="Tajikistan" pageId="12" pageNumber="113">Tajikistan</collectingCountry>
