<number box="[1279,1307,817,839]" pageId="3" pageNumber="104" value="91.0">91</number>
. 
<typeStatus box="[1317,1411,817,839]" pageId="3" pageNumber="104">Holotype</typeStatus>
♀ (
<collectionCode box="[206,259,848,870]" httpUri="http://grbio.org/cool/vmfx-g997" name="Zoological Institute, Russian Academy of Sciences" pageId="3" pageNumber="104">ZISP</collectionCode>
