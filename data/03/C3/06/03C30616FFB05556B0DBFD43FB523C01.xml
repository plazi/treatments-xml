<paragraph blockId="3.[151,1436,817,870]" pageId="3" pageNumber="104">
<emphasis box="[151,410,817,839]" italics="true" pageId="3" pageNumber="104">
Eup[ithecia] 
<taxonomicName authority="Alpheraky, 1883" authorityName="Alpheraky" authorityYear="1883" box="[286,410,817,839]" class="Insecta" family="Geometridae" genus="Eupithecia" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="104" phylum="Arthropoda" rank="species" species="minusculata">minusculata</taxonomicName>
</emphasis>
Alphéraky, 
<number box="[533,586,817,839]" pageId="3" pageNumber="104" value="1883.0">1883</number>
, 
<emphasis box="[596,1020,817,839]" italics="true" pageId="3" pageNumber="104">Horae Societatis Entomologicae Rossicae</emphasis>
<emphasis bold="true" box="[1027,1053,817,838]" pageId="3" pageNumber="104">
<number box="[1027,1053,817,838]" pageId="3" pageNumber="104" value="17.0">17</number>
</emphasis>
(
<number box="[1066,1078,817,839]" pageId="3" pageNumber="104" value="3.0">3</number>
/ 
<number box="[1088,1103,817,839]" pageId="3" pageNumber="104" value="4.0">4</number>
): 
<number box="[1120,1160,817,839]" pageId="3" pageNumber="104" value="225.0">225</number>
, tab. 
<number box="[1214,1228,817,839]" pageId="3" pageNumber="104" value="9.0">9</number>
