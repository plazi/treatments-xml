Mironov, 
<number box="[442,495,1326,1348]" pageId="4" pageNumber="105" value="1989.0">1989</number>
</taxonomicName>
, 
<emphasis box="[508,675,1326,1348]" italics="true" pageId="4" pageNumber="105">Vestnik Zoologii</emphasis>
<emphasis bold="true" box="[684,735,1327,1348]" pageId="4" pageNumber="105">
<number box="[684,735,1327,1348]" pageId="4" pageNumber="105" value="1989.0">1989</number>
</emphasis>
(
<number box="[753,767,1326,1348]" pageId="4" pageNumber="105" value="3.0">3</number>
): 
<number box="[789,816,1326,1348]" pageId="4" pageNumber="105" value="17.0">17</number>
, figs 
<number box="[876,889,1326,1348]" pageId="4" pageNumber="105" value="4.0">4</number>
a–c, 
<number box="[939,951,1326,1348]" pageId="4" pageNumber="105" value="5.0">5</number>
a–b. 
<typeStatus box="[1004,1098,1326,1348]" pageId="4" pageNumber="105">Holotype</typeStatus>
♀ (
<collectionCode box="[1142,1195,1326,1348]" httpUri="http://grbio.org/cool/vmfx-g997" name="Zoological Institute, Russian Academy of Sciences" pageId="4" pageNumber="105">ZISP</collectionCode>
, examined), 
<collectingCountry box="[1330,1432,1326,1348]" name="Mongolia" pageId="4" pageNumber="105">Mongolia</collectingCountry>
: Bayan-Khongor aimak, 
<quantity box="[442,519,1357,1379]" metricMagnitude="5" metricUnit="m" metricValue="1.4" pageId="4" pageNumber="105" unit="km" value="140.0">140 km</quantity>
