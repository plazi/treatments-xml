<materialsCitation ID-GBIF-Occurrence="1977786285" county="Eastern Siberia" location="Suputinsky Nature Reserve" municipality="Russian Far East" pageId="11" pageNumber="12" specimenCount="2" stateProvince="Krasnoyarsk" typeStatus="syntype">
