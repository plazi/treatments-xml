; 
<bibRefCitation author="Senotrusova" box="[1209,1431,1560,1584]" pageId="5" pageNumber="6" refString="Senotrusova, V. G. (1987) Gamasid mites - parasites of wild animals in Kazakhstan. Nauka, Alma-Ata, 224 pp. [in Russian]" type="book" year="1987">Senotrusova, 1987</bibRefCitation>
; 
<bibRefCitation author="Goncharova" box="[151,424,1596,1621]" pageId="5" pageNumber="6" refString="Goncharova, A. A., Bondarchuk, A. S. &amp; Vershinina, O. N. (1991) Gamasid mites - Ectoparasites of Mammals in Transbaikalia. Chita State Medical University, Chita, 121 pp. [in Russian]" type="book" year="1991">
