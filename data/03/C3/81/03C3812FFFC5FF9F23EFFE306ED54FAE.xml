<number box="[539,591,1122,1146]" pageId="4" pageNumber="67" value="4.81">4.81</number>
, width 
<number box="[676,729,1122,1146]" pageId="4" pageNumber="67" value="2.39">2.39</number>
. Head length 
<number box="[886,938,1122,1146]" pageId="4" pageNumber="67" value="0.66">0.66</number>
, width 
<quantity box="[1023,1119,1122,1146]" metricMagnitude="-3" metricUnit="m" metricValue="1.1" pageId="4" pageNumber="67" unit="mm" value="1.1">1.10mm</quantity>
. Antenna length I–IV: 
<number box="[1380,1432,1122,1146]" pageId="4" pageNumber="67" value="0.38">0.38</number>
, 
<number box="[151,203,1158,1182]" pageId="4" pageNumber="67" value="1.07">1.07</number>
, 
<number box="[214,265,1158,1182]" pageId="4" pageNumber="67" value="0.75">0.75</number>
, 
<number box="[276,329,1158,1182]" pageId="4" pageNumber="67" value="0.72">0.72</number>
. Rostrum length 
<number box="[518,571,1158,1182]" pageId="4" pageNumber="67" value="2.92">2.92</number>
. Scutellum length 
<number box="[777,829,1158,1182]" pageId="4" pageNumber="67" value="0.85">0.85</number>
, width 
<number box="[910,962,1158,1182]" pageId="4" pageNumber="67" value="1.23">1.23</number>
. Length of fore leg: femur 
<number box="[1261,1313,1158,1182]" pageId="4" pageNumber="67" value="1.19">1.19</number>
, tibia 
<number box="[1380,1432,1158,1182]" pageId="4" pageNumber="67" value="1.23">1.23</number>
, tarsus 
<number box="[223,275,1194,1218]" pageId="4" pageNumber="67" value="0.63">0.63</number>
; length of middle leg: tibia 
<number box="[587,638,1194,1218]" pageId="4" pageNumber="67" value="1.51">1.51</number>
, tarsus 
<number box="[723,775,1194,1218]" pageId="4" pageNumber="67" value="0.75">0.75</number>
; length of hind leg: femur 
<number box="[1075,1126,1194,1218]" pageId="4" pageNumber="67" value="1.63">1.63</number>
, tibia 
<number box="[1195,1247,1194,1218]" pageId="4" pageNumber="67" value="2.75">2.75</number>
, tarsomere I–III: 
<number box="[151,203,1230,1254]" pageId="4" pageNumber="67" value="0.08">0.08</number>
, 
<number box="[214,266,1230,1254]" pageId="4" pageNumber="67" value="0.62">0.62</number>
, 
<number box="[277,330,1230,1254]" pageId="4" pageNumber="67" value="0.46">0.46</number>
. Length of hemelytra 
<number box="[575,627,1230,1254]" pageId="4" pageNumber="67" value="4.09">4.09</number>
, width 
<number box="[709,761,1230,1254]" pageId="4" pageNumber="67" value="1.29">1.29</number>
.
</paragraph>
