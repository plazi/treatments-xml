<taxonomicName authority="Robertson, 1918" authorityName="Robertson" authorityYear="1918" baseAuthorityName="Smith" baseAuthorityYear="2004" box="[637,873,211,232]" class="Insecta" family="Halictidae" genus="Seladonia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="3" pageNumber="1366" phylum="Arthropoda" rank="species" species="lucidipennis">
