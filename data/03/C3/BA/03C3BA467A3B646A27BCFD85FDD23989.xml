<collectingCountry box="[359,426,1737,1763]" name="China" pageId="10" pageNumber="411">China</collectingCountry>
(
<collectingRegion box="[441,537,1737,1764]" country="China" name="Shaanxi" pageId="10" pageNumber="411">Shaanxi</collectingRegion>
