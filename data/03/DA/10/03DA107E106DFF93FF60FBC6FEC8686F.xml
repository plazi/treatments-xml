; (9): most 
<taxonomicName baseAuthorityName="Cresson" baseAuthorityYear="1912" box="[274,377,1752,1778]" class="Insecta" family="Psilidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="family">Psilidae</taxonomicName>
; (13): Diopsinae, 
<taxonomicName authorityName="McAlpine" authorityYear="1997" box="[604,738,1752,1778]" class="Insecta" family="Gobryidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="family">Gobryidae</taxonomicName>
, 
<taxonomicName box="[752,900,1752,1778]" class="Insecta" family="Nothybidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="family">Nothybidae</taxonomicName>
; (15): some Diopsinae; (18): 
<taxonomicName authorityName="Seguy" authorityYear="1949" box="[1272,1435,1752,1777]" class="Insecta" family="Diopsidae" genus="Cladodiopsis" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="genus">
<emphasis box="[1272,1435,1752,1777]" italics="true" pageId="9" pageNumber="10">Cladodiopsis</emphasis>
</taxonomicName>
(
<taxonomicName box="[159,285,1790,1816]" class="Insecta" family="Diopsidae" kingdom="Animalia" order="Diptera" pageId="9" pageNumber="10" phylum="Arthropoda" rank="family">Diopsidae</taxonomicName>
).
</paragraph>
