<specimenCount box="[288,311,1427,1447]" pageId="45" pageNumber="46" type="male">♂</specimenCount>
, 
<collectionCode box="[326,416,1428,1454]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" lsid="urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="45" pageNumber="46" type="Museum">USNM</collectionCode>
)
</materialsCitation>
