<taxonomicName authority="Forest" authorityName="Forest" class="Magnoliopsida" family="Casuarinaceae" genus="Casuarina" kingdom="Plantae" order="Fagales" pageId="42" pageNumber="17" phylum="Tracheophyta" rank="genus">
