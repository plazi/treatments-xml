<collectingCountry box="[606,670,1301,1324]" name="Japan" pageId="9" pageNumber="390">Japan</collectingCountry>
and 
<collectingCountry box="[726,829,1300,1325]" name="Australia" pageId="9" pageNumber="390">Australia</collectingCountry>
