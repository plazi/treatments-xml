), 
<collectingCountry box="[735,815,1338,1364]" name="France" pageId="3" pageNumber="26">France</collectingCountry>
(Cannes), 
<collectingCountry box="[957,1041,1338,1364]" name="Greece" pageId="3" pageNumber="26">Greece</collectingCountry>
, 
<collectingCountry box="[1058,1161,1338,1364]" name="Portugal" pageId="3" pageNumber="26">Portugal</collectingCountry>
