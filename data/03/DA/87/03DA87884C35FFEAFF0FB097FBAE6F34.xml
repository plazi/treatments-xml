</taxonomicName>
; 
<taxonomicName authority="Reitter, 1887" authorityName="Reitter" authorityYear="1887" box="[795,1214,1620,1645]" class="Insecta" family="Rutelidae" genus="Phyllopertha" kingdom="Animalia" order="Coleoptera" pageId="10" pageNumber="177" phylum="Arthropoda" rank="species" species="semenowi">
