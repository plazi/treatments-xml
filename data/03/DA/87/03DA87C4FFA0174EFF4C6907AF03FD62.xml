<taxonomicName authority="Szalay, 1945" authorityName="Szalay" authorityYear="1945" box="[1112,1436,476,501]" class="Arachnida" family="Mideopsidae" genus="Nudomideopsis" kingdom="Animalia" order="Trombidiformes" pageId="41" pageNumber="152" phylum="Arthropoda" rank="genus">
