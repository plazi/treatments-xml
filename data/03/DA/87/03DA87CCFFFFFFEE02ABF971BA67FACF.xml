<collectionCode LSID="urn:lsid:biocol.org:col:34988" box="[341,436,1232,1258]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34988" name="Museum National d'Histoire Naturelle" pageId="29" pageNumber="30">MNHN</collectionCode>
