<paragraph blockId="43.[189,310,1046,1072]" box="[189,310,1046,1072]" pageId="43" pageNumber="44">
<heading bold="true" box="[189,310,1046,1072]" fontSize="11" level="3" pageId="43" pageNumber="44" reason="3">
