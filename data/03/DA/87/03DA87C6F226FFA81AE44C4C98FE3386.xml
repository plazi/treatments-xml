<quantity box="[838,926,727,751]" metricMagnitude="-3" metricUnit="m" metricValue="7.2" pageId="4" pageNumber="445" unit="mm" value="7.2">7.2 mm</quantity>
, width 
<quantity box="[1007,1111,726,751]" metricMagnitude="-2" metricUnit="m" metricValue="1.06" pageId="4" pageNumber="445" unit="mm" value="10.6">10.6 mm</quantity>
), 3°42′S 151°52E, at depth of about 
<quantity box="[220,307,763,787]" metricMagnitude="3" metricUnit="m" metricValue="1.94" pageId="4" pageNumber="445" unit="m" value="1940.0">1940 m</quantity>
