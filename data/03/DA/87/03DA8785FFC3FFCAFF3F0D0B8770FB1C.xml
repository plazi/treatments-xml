<subSubSection pageId="2" pageNumber="51" type="etymology">
<paragraph blockId="2.[151,1436,783,1204]" pageId="2" pageNumber="51">
<emphasis bold="true" box="[151,286,784,808]" pageId="2" pageNumber="51">Etymology.</emphasis>
