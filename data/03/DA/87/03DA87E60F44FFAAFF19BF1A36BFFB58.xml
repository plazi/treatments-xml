<collectionCode box="[600,708,981,1007]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34665" lsid="urn:lsid:biocol.org:col:34665" name="Natural History Museum, London" pageId="127" pageNumber="128" type="Museum">NHMUK</collectionCode>
