<quantity box="[1096,1268,1876,1902]" metricMagnitude="3" metricUnit="m" metricValue="1.48" metricValueMax="2.11" metricValueMin="0.85" pageId="15" pageNumber="16" unit="m" value="1480.0" valueMax="2110.0" valueMin="850.0">850 to 2110 m</quantity>
