<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[460,655,655,678]" class="Insecta" family="Philopteridae" genus="Lipeurus" kingdom="Animalia" order="Phthiraptera" pageId="32" pageNumber="33" phylum="Arthropoda" rank="species" species="caponis">
