<taxonomicName authorityName="Sánchez-Montes &amp; Colunga-Salas &amp; Álvarez-Castillo &amp; Guzmán-Cornejo &amp; Montiel-Parra" authorityYear="2018" box="[667,1089,1697,1722]" class="Insecta" family="Philopteridae" genus="Strongylocotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="subSpecies" species="complanatus" subSpecies="boucardi">
