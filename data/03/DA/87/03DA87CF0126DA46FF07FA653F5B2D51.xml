<taxonomicName authority="Sánchez-Montes &amp; Colunga-Salas &amp; Álvarez-Castillo &amp; Guzmán-Cornejo &amp; Montiel-Parra, 2018" authorityName="Sánchez-Montes &amp; Colunga-Salas &amp; Álvarez-Castillo &amp; Guzmán-Cornejo &amp; Montiel-Parra" authorityYear="2018" box="[163,596,1769,1794]" class="Insecta" family="Philopteridae" genus="Strongylocotes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="45" pageNumber="46" phylum="Arthropoda" rank="subSpecies" species="complanatus" status="new subspecies" subSpecies="boucardi">
