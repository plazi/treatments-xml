<taxonomicName authority="Tourn. ex Ruppius" authorityName="Tourn. ex Ruppius" box="[1098,1255,1719,1744]" class="Magnoliopsida" family="Solanaceae" genus="Lycopersicon" higherTaxonomySource="GBIF" kingdom="Plantae" order="Solanales" pageId="9" pageNumber="46" phylum="Tracheophyta" rank="genus">
