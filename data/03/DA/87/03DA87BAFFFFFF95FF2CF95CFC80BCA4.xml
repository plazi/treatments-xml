<emphasis bold="true" box="[151,275,1998,2023]" pageId="2" pageNumber="113">FIGURE 2.</emphasis>
<taxonomicName authority="Karsch" authorityName="Karsch" authorityYear="1890" box="[281,574,1999,2023]" class="Insecta" family="Phaneropteridae" genus="Ceratopompa" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="113" phylum="Arthropoda" rank="species" species="festiva">
