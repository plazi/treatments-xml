<subSubSection box="[199,563,968,995]" pageId="4" pageNumber="600" type="distribution">
<paragraph blockId="4.[151,1436,968,1138]" box="[199,563,968,995]" pageId="4" pageNumber="600">
<emphasis bold="true" box="[199,353,968,994]" pageId="4" pageNumber="600">Distribution.</emphasis>
<collectingRegion box="[358,444,968,994]" country="China" name="Yunnan" pageId="4" pageNumber="600">Yunnan</collectingRegion>
