; patellae + tibiae 
<number box="[1379,1428,1769,1794]" pageId="8" pageNumber="189" value="0.86">0.86</number>
/ 
<number box="[151,200,1805,1830]" pageId="8" pageNumber="189" value="0.63">0.63</number>
/ 
<number box="[209,259,1805,1830]" pageId="8" pageNumber="189" value="0.5">0.50</number>
/ 
<number box="[268,317,1805,1830]" pageId="8" pageNumber="189" value="0.63">0.63</number>
; metatarsi + tarsi 
<number box="[534,584,1805,1830]" pageId="8" pageNumber="189" value="0.84">0.84</number>
/ 
<number box="[592,641,1805,1830]" pageId="8" pageNumber="189" value="0.73">0.73</number>
/ 
<number box="[649,699,1805,1830]" pageId="8" pageNumber="189" value="0.54">0.54</number>
