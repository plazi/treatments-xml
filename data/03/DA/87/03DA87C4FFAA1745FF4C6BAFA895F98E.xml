<subSubSection pageId="34" pageNumber="145" type="diagnosis">
<paragraph blockId="34.[151,1437,722,1539]" box="[151,483,722,747]" pageId="34" pageNumber="145">
<emphasis bold="true" box="[151,357,722,747]" pageId="34" pageNumber="145">
