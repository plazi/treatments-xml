<emphasis box="[283,393,1014,1037]" italics="true" pageId="14" pageNumber="115">Juniperus</emphasis>
</taxonomicName>
spp. (junipers), 
<taxonomicName box="[584,675,1014,1037]" class="Pinopsida" family="Cupressaceae" genus="Sequoia" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="115" phylum="Tracheophyta" rank="genus">
<emphasis box="[584,675,1014,1037]" italics="true" pageId="14" pageNumber="115">Sequoia</emphasis>
</taxonomicName>
spp. (redwoods), 
<taxonomicName box="[884,1010,1013,1037]" class="Pinopsida" family="Cupressaceae" genus="Libocedrus" kingdom="Plantae" order="Pinales" pageId="14" pageNumber="115" phylum="Tracheophyta" rank="genus">
<emphasis box="[884,1010,1013,1037]" italics="true" pageId="14" pageNumber="115">Libocedrus</emphasis>
</taxonomicName>
sp. (incense-cedar), 
