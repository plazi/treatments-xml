<taxonomicName baseAuthorityName="Nelson &amp; Goldman" baseAuthorityYear="1929" box="[932,1308,764,789]" class="Mammalia" family="Geomyidae" genus="Orthogeomys" kingdom="Animalia" order="Rodentia" pageId="50" pageNumber="51" phylum="Chordata" rank="subSpecies" species="hispidus" subSpecies="chiapensis">
