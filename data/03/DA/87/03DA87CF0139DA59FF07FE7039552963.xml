<taxonomicName authority="Goldman" authorityName="Goldman" box="[703,1164,727,752]" class="Mammalia" family="Geomyidae" genus="Geomys" kingdom="Animalia" order="Rodentia" pageId="50" pageNumber="51" phylum="Chordata" rank="subSpecies" species="personatus" subSpecies="tropicalis">
