<number box="[151,203,1302,1327]" pageId="14" pageNumber="195" value="0.75">0.75</number>
, width 
<number box="[286,339,1302,1327]" pageId="14" pageNumber="195" value="0.58">0.58</number>
. Clypeus height 
<number box="[527,580,1303,1327]" pageId="14" pageNumber="195" value="0.37">0.37</number>
. Sternum length 
<number box="[771,823,1303,1327]" pageId="14" pageNumber="195" value="0.37">0.37</number>
, width 
<number box="[907,960,1303,1327]" pageId="14" pageNumber="195" value="0.39">0.39</number>
