<paragraph blockId="7.[675,1210,1547,1700]" box="[677,821,1547,1573]" pageId="7" pageNumber="12">DESCRIPTION</paragraph>
<paragraph blockId="7.[675,1210,1547,1700]" pageId="7" pageNumber="12">
Small 
<taxonomicName authorityName="Ehrenberg" authorityYear="1875" box="[742,856,1578,1604]" class="Polycystina" higherTaxonomySource="GBIF" kingdom="Chromista" order="Nassellaria" pageId="7" pageNumber="12" phylum="Radiozoa" rank="order">Nassellaria</taxonomicName>
