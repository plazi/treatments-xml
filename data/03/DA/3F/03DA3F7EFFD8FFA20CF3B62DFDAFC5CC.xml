<quantity box="[698,820,344,365]" metricMagnitude="-3" metricUnit="m" metricValue="2.05" metricValueMax="2.8" metricValueMin="1.3" pageId="19" pageNumber="606" unit="mm" value="2.05" valueMax="2.8" valueMin="1.3">1.3-2.8 mm</quantity>
