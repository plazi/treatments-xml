<quantity box="[699,819,520,541]" metricMagnitude="-3" metricUnit="m" metricValue="1.75" metricValueMax="2.2" metricValueMin="1.3" pageId="25" pageNumber="612" unit="mm" value="1.75" valueMax="2.2" valueMin="1.3">1.3-2.2 mm</quantity>
