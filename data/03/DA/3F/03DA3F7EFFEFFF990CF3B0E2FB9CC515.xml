) 
<taxonomicName box="[440,567,1527,1548]" class="Insecta" family="Cleridae" genus="Muisca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="626" phylum="Arthropoda" rank="species" species="octonotata">
<emphasis box="[440,567,1527,1548]" italics="true" pageId="39" pageNumber="626">M. octonotata</emphasis>
</taxonomicName>
, ungues. (
<emphasis bold="true" box="[661,684,1526,1547]" pageId="39" pageNumber="626">91</emphasis>
) 
<taxonomicName box="[697,825,1527,1548]" class="Insecta" family="Cleridae" genus="Muisca" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="39" pageNumber="626" phylum="Arthropoda" rank="species" species="octonotata">
<emphasis box="[697,825,1527,1548]" italics="true" pageId="39" pageNumber="626">M. octonotata</emphasis>
</taxonomicName>
, 4 
