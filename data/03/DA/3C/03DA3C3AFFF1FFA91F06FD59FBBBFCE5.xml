<collectingCountry box="[1012,1089,839,861]" name="Austria" pageId="10" pageNumber="771">Austria</collectingCountry>
, 
<collectingCountry box="[1103,1152,839,861]" name="Italy" pageId="10" pageNumber="771">Italy</collectingCountry>
, 
<collectingCountry box="[1166,1243,839,861]" name="Croatia" pageId="10" pageNumber="771">Croatia</collectingCountry>
