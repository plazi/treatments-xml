; 11— 
<taxonomicName class="Insecta" family="Dytiscidae" genus="Neptosternus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="209" phylum="Arthropoda" rank="species" species="hydaticoides">
<emphasis italics="true" pageId="4" pageNumber="209">N. hydaticoides</emphasis>
</taxonomicName>
; 12— 
<taxonomicName box="[345,461,1961,1982]" class="Insecta" family="Dytiscidae" genus="Neptosternus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="209" phylum="Arthropoda" rank="species" species="coomani">
<emphasis box="[345,461,1961,1982]" italics="true" pageId="4" pageNumber="209">N. coomani</emphasis>
</taxonomicName>
; 13— 
<taxonomicName box="[525,627,1960,1982]" class="Insecta" family="Dytiscidae" genus="Neptosternus" kingdom="Animalia" order="Coleoptera" pageId="4" pageNumber="209" phylum="Arthropoda" rank="species" species="strnadi">
