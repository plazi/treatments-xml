Do Heon Kwon &amp; Taiti, 
<number box="[1171,1232,1705,1731]" pageId="1" pageNumber="2" value="1993.0">1993</number>
</bibRefCitation>
</taxonomicName>
, 
<taxonomicName authority="Arcangeli, 1927" authorityName="Arcangeli" authorityYear="1927" class="Malacostraca" family="Armadillidae" genus="Spherillo" kingdom="Animalia" order="Isopoda" pageId="1" pageNumber="2" phylum="Arthropoda" rank="species" species="raffaelei">
