<taxonomicName baseAuthorityName="LINNAEUS" baseAuthorityYear="1758" box="[741,887,248,270]" class="Insecta" family="Megachilidae" genus="Anthidium" kingdom="Animalia" order="Hymenoptera" pageId="7" pageNumber="606" phylum="Arthropoda" rank="species" species="manicatum">
