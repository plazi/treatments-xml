<quantity box="[376,436,218,240]" metricMagnitude="2" metricUnit="m" metricValue="9.3" pageId="7" pageNumber="606" unit="m" value="930.0">930m</quantity>
, 
<date box="[450,554,218,240]" pageId="7" pageNumber="606" value="1967-07-14">14.7.1967</date>
, 3 3. Tacheng, Kuerkanlagishi, 
