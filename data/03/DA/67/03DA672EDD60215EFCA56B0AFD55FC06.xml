, 1. Provinz 
<collectingRegion box="[699,783,642,664]" country="China" name="Qinghai" pageId="7" pageNumber="606">Qinghai</collectingRegion>
, Yushubatang, 
<quantity box="[955,1027,642,664]" metricMagnitude="3" metricUnit="m" metricValue="3.9" pageId="7" pageNumber="606" unit="m" value="3900.0">3900m</quantity>
, 25.- 
<date box="[178,282,671,693]" pageId="7" pageNumber="606" value="1964-07-29">29.7.1964</date>
, 1 1. Provinz 
<collectingRegion box="[467,558,671,693]" country="China" name="Xinjiang Uygur" pageId="7" pageNumber="606">Xinjiang</collectingRegion>
, Zhaosu, 
<quantity box="[663,735,671,693]" metricMagnitude="3" metricUnit="m" metricValue="2.45" pageId="7" pageNumber="606" unit="m" value="2450.0">2450m</quantity>
, 
<date box="[749,853,671,693]" pageId="7" pageNumber="606" value="1978-07-23">23.7.1978</date>
, 6, 21.- 
<date box="[971,1081,671,693]" pageId="7" pageNumber="606" value="1978-07-22">22.7.1978</date>
, 9. Tashenkuenganhungqitafu, 
<quantity box="[527,658,700,722]" metricMagnitude="3" metricUnit="m" metricValue="4.5" metricValueMax="4.6" metricValueMin="4.4" pageId="7" pageNumber="606" unit="m" value="4500.0" valueMax="4600.0" valueMin="4400.0">4400-4600m</quantity>
