<taxonomicName authority="(FABRICIUS 1787)" baseAuthorityName="FABRICIUS" baseAuthorityYear="1787" box="[651,1053,1100,1122]" class="Insecta" family="Megachilidae" genus="Megachile" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="607" phylum="Arthropoda" rank="species" species="rotundata">
