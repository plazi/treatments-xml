<collectingCountry box="[825,1016,943,968]" name="Solomon Islands" pageId="16" pageNumber="469">Solomon Islands</collectingCountry>
, 
<collectingCountry box="[1028,1070,943,968]" name="Fiji" pageId="16" pageNumber="469">Fiji</collectingCountry>
