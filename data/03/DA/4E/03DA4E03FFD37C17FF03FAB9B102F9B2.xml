<collectingRegion box="[316,384,944,967]" country="Indonesia" name="Papua Barat" pageId="16" pageNumber="469">Papua</collectingRegion>
New Guinea
</collectingCountry>
east to 
<collectingCountry box="[623,813,943,968]" name="Marshall Islands" pageId="16" pageNumber="469">Marshall Islands</collectingCountry>
