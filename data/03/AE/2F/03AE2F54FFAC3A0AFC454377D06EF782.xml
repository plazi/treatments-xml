<taxonomicName authority="Hoberlandt &amp; Stys, 1979: 6" authorityName="Hoberlandt &amp; Stys" authorityPageNumber="6" authorityYear="1979" box="[935,1263,1977,1996]" class="Insecta" family="Aphelocheiridae" genus="Aphelocheirus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="22" pageNumber="373" phylum="Arthropoda" rank="species" species="asiaticus">
