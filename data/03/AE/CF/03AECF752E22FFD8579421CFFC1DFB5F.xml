<geoCoordinate box="[922,1059,1089,1115]" degrees="22" direction="south" minutes="17" orientation="latitude" pageId="4" pageNumber="576" precision="15" seconds="39" value="-22.294167">22°17’39”S</geoCoordinate>
; 
<geoCoordinate box="[1074,1220,1088,1115]" degrees="41" direction="west" minutes="40" orientation="longitude" pageId="4" pageNumber="576" precision="15" seconds="53" value="-41.68139">41°40’53”W</geoCoordinate>
