<paragraph blockId="30.[151,1437,1361,1782]" box="[199,1055,1397,1422]" pageId="30" pageNumber="31">
<emphasis bold="true" box="[199,401,1397,1422]" pageId="30" pageNumber="31">Included species.</emphasis>
<taxonomicName authorityName="Holmes" authorityYear="1908" box="[408,492,1398,1421]" class="Malacostraca" family="Lysianassidae" genus="Koroga" kingdom="Animalia" order="Amphipoda" pageId="30" pageNumber="31" phylum="Arthropoda" rank="genus">
<emphasis box="[408,492,1398,1421]" italics="true" pageId="30" pageNumber="31">Koroga</emphasis>
</taxonomicName>
includes one species: 
<taxonomicName authority="Holmes, 1908" authorityName="Holmes" authorityYear="1908" box="[743,1050,1397,1422]" class="Malacostraca" family="Lysianassidae" genus="Koroga" kingdom="Animalia" order="Amphipoda" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="megalops">
