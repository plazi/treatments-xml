<emphasis bold="true" box="[540,620,1953,1975]" pageId="54" pageNumber="55">
<taxonomicNameLabel box="[540,620,1953,1975]" pageId="54" pageNumber="55" rank="species">sp. nov.</taxonomicNameLabel>
