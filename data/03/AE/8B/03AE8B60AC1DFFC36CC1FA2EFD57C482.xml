<taxonomicName authorityName="Lowry &amp; Stoddart" authorityYear="1995" box="[408,497,1646,1669]" class="Malacostraca" family="Uristidae" genus="Gippsia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
