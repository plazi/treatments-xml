<taxonomicName authority="Lowry &amp; Stoddart, 1995" authorityName="Lowry &amp; Stoddart" authorityYear="1995" box="[721,1121,1645,1670]" class="Malacostraca" family="Uristidae" genus="Gippsia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="27" pageNumber="28" phylum="Arthropoda" rank="species" species="jonesae">
