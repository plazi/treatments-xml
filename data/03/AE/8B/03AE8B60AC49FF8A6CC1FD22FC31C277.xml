; 
<taxonomicName authority="J. L. Barnard, 1962" authorityName="J. L. Barnard" authorityYear="1962" class="Malacostraca" family="Uristidae" genus="Uristes" kingdom="Animalia" order="Amphipoda" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="mediator">
<emphasis box="[1300,1436,1641,1666]" italics="true" pageId="79" pageNumber="80">U. mediator</emphasis>
J.L. 
<bibRefCitation author="Barnard, J. L." box="[203,368,1677,1702]" pageId="79" pageNumber="80" pagination="1 - 78" refId="ref44876" refString="Barnard, J. L. (1962) South Atlantic abyssal amphipods collected by R. V. Vema. Abyssal Crustacea. Vema Research Series, 1, 1 - 78." type="journal article" year="1962">Barnard, 1962</bibRefCitation>
</taxonomicName>
; 
<taxonomicName authority="(Walker, 1903)" baseAuthorityName="Walker" baseAuthorityYear="1903" box="[383,706,1677,1702]" class="Malacostraca" family="Lysianassidae" genus="Tryphosites" kingdom="Animalia" order="Amphipoda" pageId="79" pageNumber="80" phylum="Arthropoda" rank="species" species="stebbingi">
<emphasis box="[383,523,1677,1702]" italics="true" pageId="79" pageNumber="80">U. stebbingi</emphasis>
(
<bibRefCitation author="Walker, A. O." box="[540,697,1677,1702]" pageId="79" pageNumber="80" pagination="38 - 64" refId="ref54350" refString="Walker, A. O. (1903) Amphipoda of the ' Southern Cross' Antarctic Expedition. Journal of the Linnean Society of London, Zoology, 29 (187), 38 - 64, pls. 37 - 11." type="journal article" year="1903">Walker, 1903</bibRefCitation>
