<subSubSection pageId="21" pageNumber="22" type="etymology">
<paragraph blockId="21.[151,1436,534,1127]" pageId="21" pageNumber="22">
<emphasis bold="true" box="[199,334,676,701]" pageId="21" pageNumber="22">Etymology.</emphasis>
