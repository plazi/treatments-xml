<taxonomicName authority="Dahl, 1945" authorityName="Dahl" authorityYear="1945" box="[820,1129,685,710]" class="Malacostraca" family="Lysianassidae" genus="Menigratopsis" kingdom="Animalia" order="Amphipoda" pageId="40" pageNumber="41" phylum="Arthropoda" rank="species" species="svennilssoni">
