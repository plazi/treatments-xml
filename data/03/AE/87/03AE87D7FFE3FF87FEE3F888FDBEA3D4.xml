<number box="[530,608,284,310]" pageId="10" pageNumber="229" value="24.5" valueMax="25.0" valueMin="24.0">24–25</number>
</figureCitation>
): Coxa with 
<number box="[779,823,284,310]" pageId="10" pageNumber="229" value="3.5" valueMax="5.0" valueMin="2.0">2–5</number>
ventral, 
<number box="[941,985,284,310]" pageId="10" pageNumber="229" value="4.5" valueMax="5.0" valueMin="4.0">4–5</number>
dorsal (mesal one largest) tubercles. Trochanter with 
<number box="[582,597,324,350]" pageId="10" pageNumber="229" value="1.0">1</number>
dorsal and 
<number box="[736,751,324,350]" pageId="10" pageNumber="229" value="2.0">2</number>
ventral tubercles. Femur slightly curved, with 
<number box="[1308,1323,324,350]" pageId="10" pageNumber="229" value="1.0">1</number>
ventrobasal tubercle. Tibia mesal IIiIi (
<number box="[720,736,364,390]" pageId="10" pageNumber="229" value="4.0">4</number>
&gt; 
<number box="[754,764,364,390]" pageId="10" pageNumber="229" value="1.0">1</number>
&gt; 
<number box="[782,798,364,390]" pageId="10" pageNumber="229" value="2.0">2</number>
&gt; 
<number box="[816,830,364,390]" pageId="10" pageNumber="229" value="5.0">5</number>
&gt; 
<number box="[849,863,364,390]" pageId="10" pageNumber="229" value="3.0">3</number>
), ectal IIiIi (
<number box="[1008,1018,364,390]" pageId="10" pageNumber="229" value="1.0">1</number>
&gt; 
<number box="[1036,1052,364,390]" pageId="10" pageNumber="229" value="2.0">2</number>
&gt; 
<number box="[1070,1087,364,390]" pageId="10" pageNumber="229" value="4.0">4</number>
&gt; 
<number box="[1105,1119,364,390]" pageId="10" pageNumber="229" value="3.0">3</number>
= 
<number box="[1137,1152,364,390]" pageId="10" pageNumber="229" value="5.0">5</number>
). Tarsus mesal Iiii (
<number box="[315,325,404,430]" pageId="10" pageNumber="229" value="1.0">1</number>
&gt; 
<number box="[343,359,404,430]" pageId="10" pageNumber="229" value="2.0">2</number>
= 
<number box="[377,391,404,430]" pageId="10" pageNumber="229" value="3.0">3</number>
= 
<number box="[410,427,404,430]" pageId="10" pageNumber="229" value="4.0">4</number>
), ectal IiIiIi (
<number box="[581,595,404,430]" pageId="10" pageNumber="229" value="3.0">3</number>
&gt; 
<number box="[612,622,404,430]" pageId="10" pageNumber="229" value="1.0">1</number>
&gt; 
<number box="[640,654,404,430]" pageId="10" pageNumber="229" value="5.0">5</number>
&gt; 
<number box="[673,689,404,430]" pageId="10" pageNumber="229" value="2.0">2</number>
&gt; 
