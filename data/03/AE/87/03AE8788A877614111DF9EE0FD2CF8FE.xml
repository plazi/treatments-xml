</taxonomicName>
, 
<emphasis box="[696,838,270,291]" italics="true" pageId="19" pageNumber="80">nom. illeg. non</emphasis>
<taxonomicName authority="Ahrendt (1941: 24)" authorityName="Ahrendt" authorityPageNumber="24" authorityYear="1941" box="[842,1159,270,292]" class="Magnoliopsida" family="Berberidaceae" genus="Berberis" kingdom="Plantae" order="Ranunculales" pageId="19" pageNumber="80" phylum="Tracheophyta" rank="species" species="formosana">
