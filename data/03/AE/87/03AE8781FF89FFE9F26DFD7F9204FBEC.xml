); 
<emphasis bold="true" box="[196,209,1657,1674]" pageId="18" pageNumber="905">A</emphasis>
, 
<emphasis bold="true" box="[219,231,1657,1674]" pageId="18" pageNumber="905">E</emphasis>
, 
<emphasis bold="true" box="[241,255,1657,1674]" pageId="18" pageNumber="905">H</emphasis>
, leg 17; 
<emphasis bold="true" box="[325,338,1657,1674]" pageId="18" pageNumber="905">B</emphasis>
, 
<emphasis bold="true" box="[348,359,1657,1674]" pageId="18" pageNumber="905">F</emphasis>
, 
<emphasis bold="true" box="[369,375,1657,1674]" pageId="18" pageNumber="905">I</emphasis>
, leg 18; 
