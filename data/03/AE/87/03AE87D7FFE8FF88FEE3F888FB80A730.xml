; chelicera: II 
<number box="[932,972,1825,1851]" pageId="4" pageNumber="223" value="2.7">2.7</number>
; III 
<number box="[1019,1059,1825,1851]" pageId="4" pageNumber="223" value="1.5">1.5</number>
; pedipalpus 
<number box="[1204,1259,1825,1851]" pageId="4" pageNumber="223" value="15.5">15.5</number>
