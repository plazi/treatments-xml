<collectingCountry box="[466,533,151,176]" name="China" pageId="9" pageNumber="534">China</collectingCountry>
(
<collectingRegion box="[547,625,151,176]" country="China" name="Fujian" pageId="9" pageNumber="534">Fujian</collectingRegion>
