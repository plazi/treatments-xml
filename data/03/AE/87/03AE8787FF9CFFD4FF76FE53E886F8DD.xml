<number box="[290,410,853,879]" pageId="30" pageNumber="31" value="0.8" valueMax="0.86" valueMin="0.74">0.74–0.86</number>
, WL 
<number box="[475,592,853,879]" pageId="30" pageNumber="31" value="1.8399999999999999" valueMax="1.9" valueMin="1.78">1.78–1.90</number>
