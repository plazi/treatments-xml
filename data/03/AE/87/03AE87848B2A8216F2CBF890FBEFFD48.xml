<number box="[782,819,584,608]" pageId="12" pageNumber="91" value="1.4">1.4</number>
, 
<number box="[830,867,584,608]" pageId="12" pageNumber="91" value="1.8">1.8</number>
, 
<number box="[879,916,584,608]" pageId="12" pageNumber="91" value="1.9">1.9</number>
