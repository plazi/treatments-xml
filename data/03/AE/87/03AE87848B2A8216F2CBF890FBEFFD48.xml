<number box="[958,1010,548,572]" pageId="12" pageNumber="91" value="0.2">0.20</number>
, 
<number box="[1022,1075,548,572]" pageId="12" pageNumber="91" value="0.14">0.14</number>
. Legs (
<number box="[1158,1195,548,572]" pageId="12" pageNumber="91" value="1.5" valueMax="2.0" valueMin="1.0">1-2</number>
- 
<number box="[1204,1221,548,572]" pageId="12" pageNumber="91" value="4.0">4</number>
- 
<number box="[1230,1245,548,572]" pageId="12" pageNumber="91" value="3.0">3</number>
): I: 
<number box="[1292,1327,548,572]" pageId="12" pageNumber="91" value="9.9">9.9</number>
(
<number box="[1343,1381,548,572]" pageId="12" pageNumber="91" value="2.9">2.9</number>
, 
<number box="[1394,1431,548,572]" pageId="12" pageNumber="91" value="1.7">1.7</number>
, 
<number box="[151,189,584,608]" pageId="12" pageNumber="91" value="2.4">2.4</number>
, 
<number box="[199,237,584,608]" pageId="12" pageNumber="91" value="2.2">2.2</number>
, 
<number box="[247,286,584,608]" pageId="12" pageNumber="91" value="0.7">0.7</number>
); II: 
<number box="[339,374,584,608]" pageId="12" pageNumber="91" value="9.8">9.8</number>
(3.0, 
<number box="[437,474,584,608]" pageId="12" pageNumber="91" value="1.8">1.8</number>
, 
<number box="[486,524,584,608]" pageId="12" pageNumber="91" value="2.1">2.1</number>
, 
<number box="[534,572,584,608]" pageId="12" pageNumber="91" value="2.2">2.2</number>
, 
<number box="[582,621,584,608]" pageId="12" pageNumber="91" value="0.7">0.7</number>
); III: 
<number box="[683,718,584,608]" pageId="12" pageNumber="91" value="7.3">7.3</number>
(
<number box="[733,771,583,608]" pageId="12" pageNumber="91" value="1.6">1.6</number>
