<number box="[277,292,296,320]" pageId="12" pageNumber="91" value="2.0">2</number>
- 
<number box="[301,335,296,320]" pageId="12" pageNumber="91" value="2.5" valueMax="4.0" valueMin="1.0">1-4</number>
- 
<number box="[344,357,296,320]" pageId="12" pageNumber="91" value="3.0">3</number>
