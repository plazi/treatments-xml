<materialsCitation box="[136,1029,1202,1229]" collectingDate="2011-08-13" collectorName="Lu" country="Taiwan" elevation="2550" location="Shalihsien Stream" pageId="31" pageNumber="92" specimenCount="1" stateProvince="Nantou">
