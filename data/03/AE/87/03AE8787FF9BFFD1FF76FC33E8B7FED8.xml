<heading bold="true" box="[151,593,946,972]" fontSize="11" level="1" pageId="25" pageNumber="26" reason="1">
<emphasis bold="true" box="[151,593,946,972]" pageId="25" pageNumber="26">
<taxonomicName box="[151,294,946,972]" class="Insecta" family="Formicidae" genus="Feroponera" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hymenoptera" pageId="25" pageNumber="26" phylum="Arthropoda" rank="genus">
