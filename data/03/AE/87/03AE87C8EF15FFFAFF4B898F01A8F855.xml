<collectingRegion box="[441,517,1746,1772]" country="China" name="Fujian" pageId="13" pageNumber="517">Fujian</collectingRegion>
, 
<collectingRegion box="[528,662,1746,1772]" country="China" name="Guangdong" pageId="13" pageNumber="517">Guangdong</collectingRegion>
, 
<collectingRegion box="[673,778,1746,1773]" country="China" name="Zhejiang" pageId="13" pageNumber="517">Zhejiang</collectingRegion>
