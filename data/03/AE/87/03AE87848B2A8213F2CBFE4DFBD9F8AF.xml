, 2.0, 
<number box="[151,188,1467,1491]" pageId="9" pageNumber="88" value="3.1">3.1</number>
, 
<number box="[201,238,1466,1491]" pageId="9" pageNumber="88" value="3.6">3.6</number>
, 
<number box="[251,290,1467,1491]" pageId="9" pageNumber="88" value="0.8">0.8</number>
); IV: 
<number box="[353,402,1466,1491]" pageId="9" pageNumber="88" value="16.9">16.9</number>
(
<number box="[419,457,1467,1491]" pageId="9" pageNumber="88" value="4.9">4.9</number>
