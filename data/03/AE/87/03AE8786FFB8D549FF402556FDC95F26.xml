<paragraph blockId="5.[151,601,1089,1151]" box="[151,601,1089,1116]" pageId="5" pageNumber="206">
<heading bold="true" box="[151,601,1089,1116]" fontSize="10" level="2" pageId="5" pageNumber="206" reason="2">
<taxonomicName authority="Downey 1971" authorityName="Downey. Examination" authorityYear="1971" box="[151,601,1089,1116]" class="Asteroidea" family="Pedicellasteridae" genus="Ampheraster" kingdom="Animalia" order="Forcipulatida" pageId="5" pageNumber="206" phylum="Echinodermata" rank="species" species="alaminos">
