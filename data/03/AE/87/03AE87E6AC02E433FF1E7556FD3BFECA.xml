<emphasis box="[431,460,1403,1428]" italics="true" pageId="5" pageNumber="30">
<number box="[431,444,1403,1428]" pageId="5" pageNumber="30" value="1.0">1</number>
a
</emphasis>
, 
<emphasis box="[475,502,1403,1428]" italics="true" pageId="5" pageNumber="30">
<number box="[475,488,1403,1428]" pageId="5" pageNumber="30" value="1.0">1</number>
c
</emphasis>
, 
<emphasis box="[516,546,1403,1428]" italics="true" pageId="5" pageNumber="30">
<number box="[516,530,1403,1428]" pageId="5" pageNumber="30" value="2.0">2</number>
a
</emphasis>
, 
<emphasis box="[560,590,1403,1428]" italics="true" pageId="5" pageNumber="30">
<number box="[560,575,1403,1428]" pageId="5" pageNumber="30" value="3.0">3</number>
a
</emphasis>
short, smooth; setae 
<emphasis box="[836,865,1403,1428]" italics="true" pageId="5" pageNumber="30">
<number box="[836,849,1403,1428]" pageId="5" pageNumber="30" value="1.0">1</number>
b
</emphasis>
, 
<emphasis box="[880,908,1403,1428]" italics="true" pageId="5" pageNumber="30">
