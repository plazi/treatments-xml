<taxonomicName authority="Herrich-Schaeffer, 1835" authorityName="Herrich-Schaeffer" authorityYear="1835" box="[237,673,1348,1372]" class="Insecta" family="Dipsocoridae" genus="Cryptostemma" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="382" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[237,673,1348,1372]" pageId="1" pageNumber="382">
<emphasis bold="true" box="[237,396,1348,1372]" italics="true" pageId="1" pageNumber="382">Cryptostemma</emphasis>
Herrich-Schaeffer, 1835
</emphasis>
</taxonomicName>
