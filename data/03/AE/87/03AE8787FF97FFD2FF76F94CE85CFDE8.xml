<number box="[575,629,1424,1450]" pageId="22" pageNumber="23" value="1.12">1.12</number>
, CI 
<number box="[678,722,1424,1450]" pageId="22" pageNumber="23" value="111.0">111</number>
, OI 
<number box="[773,805,1424,1450]" pageId="22" pageNumber="23" value="32.0">32</number>
