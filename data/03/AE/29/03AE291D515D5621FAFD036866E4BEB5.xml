<paragraph blockId="6.[151,1437,151,1737]" box="[151,1436,743,764]" pageId="6" pageNumber="62">
<keyLead box="[151,1436,743,764]" pageId="6" pageNumber="62">
<number box="[151,165,743,764]" pageId="6" pageNumber="62" value="7.0">7</number>
. Pronotum partially darkened............................................................................. 
<number box="[1424,1436,743,764]" pageId="6" pageNumber="62" value="8.0">8</number>
