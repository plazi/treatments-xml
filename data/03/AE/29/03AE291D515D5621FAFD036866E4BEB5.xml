<keyLead box="[151,1436,1966,1987]" pageId="5" pageNumber="61">
<number box="[151,165,1966,1987]" pageId="5" pageNumber="61" value="2.0">2</number>
. Head completely dark, elytra black........................................................................ 
<number box="[1424,1436,1966,1987]" pageId="5" pageNumber="61" value="3.0">3</number>
</keyLead>
</paragraph>
<paragraph blockId="5.[151,1436,1907,2016]" box="[151,1430,1995,2016]" pageId="5" pageNumber="61">
<keyLead box="[151,1430,1995,2016]" pageId="5" pageNumber="61">
