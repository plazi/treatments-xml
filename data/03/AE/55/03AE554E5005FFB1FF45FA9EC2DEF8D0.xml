<quantity box="[151,317,1714,1739]" metricMagnitude="-4" metricUnit="m" metricValue="5.25" metricValueMax="5.4" metricValueMin="5.1" pageId="3" pageNumber="344" unit="mm" value="0.525" valueMax="0.54" valueMin="0.51">0.51–0.54 mm</quantity>
