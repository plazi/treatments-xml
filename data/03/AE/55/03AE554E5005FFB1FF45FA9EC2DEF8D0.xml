HW: 
<quantity box="[262,430,1678,1703]" metricMagnitude="-4" metricUnit="m" metricValue="8.95" metricValueMax="9.399999999999999" metricValueMin="8.5" pageId="3" pageNumber="344" unit="mm" value="0.895" valueMax="0.94" valueMin="0.85">0.85–0.94 mm</quantity>
, PL: 
<quantity box="[495,663,1678,1703]" metricMagnitude="-4" metricUnit="m" metricValue="6.85" metricValueMax="7.0" metricValueMin="6.7" pageId="3" pageNumber="344" unit="mm" value="0.685" valueMax="0.7" valueMin="0.67">0.67–0.70 mm</quantity>
, PW: 
<quantity box="[736,904,1678,1703]" metricMagnitude="-4" metricUnit="m" metricValue="6.45" metricValueMax="6.8" metricValueMin="6.1" pageId="3" pageNumber="344" unit="mm" value="0.645" valueMax="0.68" valueMin="0.61">0.61–0.68 mm</quantity>
, EL: 
<quantity box="[970,1138,1678,1703]" metricMagnitude="-4" metricUnit="m" metricValue="7.05" metricValueMax="7.3" metricValueMin="6.8" pageId="3" pageNumber="344" unit="mm" value="0.705" valueMax="0.73" valueMin="0.68">0.68–0.73 mm</quantity>
