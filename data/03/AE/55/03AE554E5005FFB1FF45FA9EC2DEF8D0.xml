<quantity box="[1213,1381,1678,1703]" metricMagnitude="-4" metricUnit="m" metricValue="7.949999999999999" metricValueMax="8.6" metricValueMin="7.3" pageId="3" pageNumber="344" unit="mm" value="0.7949999999999999" valueMax="0.86" valueMin="0.73">0.73–0.86 mm</quantity>
