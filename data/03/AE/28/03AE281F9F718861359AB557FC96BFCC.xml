<taxonomicName authorityName="Baird" authorityYear="1846" box="[1206,1309,1433,1459]" class="Ostracoda" family="Cypridae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="7" pageNumber="24" phylum="Arthropoda" rank="family">Cypridae</taxonomicName>
