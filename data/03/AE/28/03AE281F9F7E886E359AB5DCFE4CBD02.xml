<taxonomicName authorityName="Nenninger" authorityYear="1948" box="[810,1052,1639,1665]" class="Oligohymenophorea" family="Vorticellidae" genus="Vorticella" higherTaxonomySource="GBIF" kingdom="Chromista" order="Peritrichida" pageId="8" pageNumber="25" phylum="Ciliophora" rank="species" species="crassicaulis">
