from 
<collectingRegion box="[826,918,513,539]" country="United Kingdom" name="Bedford" pageId="8" pageNumber="25">Bedford</collectingRegion>
College, 
<collectingCountry box="[1024,1120,513,539]" name="United Kingdom" pageId="8" pageNumber="25">England</collectingCountry>
