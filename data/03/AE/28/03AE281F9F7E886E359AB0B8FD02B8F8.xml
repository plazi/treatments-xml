<taxonomicName authority="Baird, 1859" authorityName="Baird" authorityYear="1859" box="[488,761,513,540]" class="Ostracoda" family="Darwinulidae" genus="Darwinula" higherTaxonomySource="GBIF" kingdom="Animalia" order="Podocopida" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="atkinsoni">
