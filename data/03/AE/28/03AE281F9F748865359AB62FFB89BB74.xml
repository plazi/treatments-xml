<taxonomicName baseAuthorityName="Muller" baseAuthorityYear="1906" box="[938,1054,1848,1875]" class="Ostracoda" family="Cypridinidae" genus="Cypridina" higherTaxonomySource="GBIF" kingdom="Animalia" order="Myodocopida" pageId="2" pageNumber="19" phylum="Arthropoda" rank="species" species="dentata">
