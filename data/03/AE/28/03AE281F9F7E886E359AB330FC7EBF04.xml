<taxonomicName authority="(Linnaeus, 1758)" baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[514,887,1233,1260]" class="Branchiopoda" family="Triopsidae" genus="Lepidurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Notostraca" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="apus">
