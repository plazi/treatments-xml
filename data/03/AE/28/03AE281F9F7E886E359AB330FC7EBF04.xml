<taxonomicName baseAuthorityName="Linnaeus" baseAuthorityYear="1758" box="[474,665,1125,1152]" class="Malacostraca" family="Gammaridae" genus="Gammarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="pulex">
<emphasis box="[474,665,1125,1152]" italics="true" pageId="8" pageNumber="25">Gammarus pulex</emphasis>
