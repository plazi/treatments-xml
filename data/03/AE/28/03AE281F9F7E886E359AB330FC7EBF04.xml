<taxonomicName baseAuthorityName="Jurine" baseAuthorityYear="1820" box="[719,901,1197,1223]" class="Hexanauplia" family="Cyclopidae" genus="Cyclops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="albidus">
