<taxonomicName authorityName="Sexton" authorityYear="1939" box="[683,900,1126,1152]" class="Malacostraca" family="Gammaridae" genus="Gammarus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="tigrinus">
