<taxonomicName authorityName="G. O. Sars" authorityYear="1895" class="Malacostraca" family="Corophiidae" genus="Corophium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Amphipoda" pageId="8" pageNumber="25" phylum="Arthropoda" rank="species" species="curvispinum">
<emphasis italics="true" pageId="8" pageNumber="25">Corophium curvispinum</emphasis>
</taxonomicName>
