<taxonomicName authorityName="Savigny" authorityYear="1816" box="[995,1094,1090,1115]" class="Malacostraca" family="Potamidae" genus="Potamon" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="8" pageNumber="25" phylum="Arthropoda" rank="genus">
<emphasis box="[995,1094,1090,1115]" italics="true" pageId="8" pageNumber="25">Potamon</emphasis>
