<subSubSection box="[1022,1287,1555,1578]" pageId="0" pageNumber="389" type="nomenclature">
<paragraph blockId="0.[1022,1287,1555,1578]" box="[1022,1287,1555,1578]" pageId="0" pageNumber="389">
<heading bold="true" box="[1022,1287,1555,1578]" centered="true" fontSize="10" level="2" pageId="0" pageNumber="389" reason="2">
<taxonomicName authority="Bott, 1969" authorityName="Bott" authorityYear="1969" box="[1022,1287,1555,1578]" class="Malacostraca" family="Gecarcinucidae" genus="Thelphusula" kingdom="Animalia" order="Decapoda" pageId="0" pageNumber="389" phylum="Arthropoda" rank="genus">
<emphasis bold="true" box="[1022,1287,1555,1578]" pageId="0" pageNumber="389">
<emphasis bold="true" box="[1022,1161,1555,1578]" italics="true" pageId="0" pageNumber="389">Thelphusula</emphasis>
<bibRefCitation author="Bott R" box="[1170,1287,1555,1578]" pageId="0" pageNumber="389" pagination="359 - 366" refId="ref3387" refString="Bott R (1969) Flusskrabben aus Asien und ihre Klassifikation (Crustacea, Decapoda). Senckenbergiana Biologica, 50 (5 - 6): 359 - 366." type="journal article" year="1969">Bott, 1969</bibRefCitation>
