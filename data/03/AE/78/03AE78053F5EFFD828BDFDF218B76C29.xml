<quantity metricMagnitude="3" metricUnit="m" metricValue="2.05" metricValueMax="2.4" metricValueMin="1.7" pageId="4" pageNumber="553" unit="m" value="2050.0" valueMax="2400.0" valueMin="1700.0">1700–2400 m</quantity>
