<subSubSection box="[199,1100,624,648]" pageId="3" pageNumber="64" type="etymology">
<paragraph blockId="3.[151,1436,151,786]" box="[199,1100,624,648]" pageId="3" pageNumber="64">
<emphasis bold="true" box="[199,327,624,648]" pageId="3" pageNumber="64">Etymology.</emphasis>
