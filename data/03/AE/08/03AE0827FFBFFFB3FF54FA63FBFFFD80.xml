<collectingRegion box="[246,332,342,368]" country="United States of America" name="Indiana" pageId="5" pageNumber="208">Indiana</collectingRegion>
(
<collectingCountry box="[347,405,343,369]" name="United States of America" pageId="5" pageNumber="208">USA</collectingCountry>
