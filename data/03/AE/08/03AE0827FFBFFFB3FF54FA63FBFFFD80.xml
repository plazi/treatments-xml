<collectingRegion country="United States of America" name="Pennsylvania" pageId="5" pageNumber="208">Pennsylvania</collectingRegion>
(
<collectingCountry box="[349,407,272,298]" name="United States of America" pageId="5" pageNumber="208">USA</collectingCountry>
