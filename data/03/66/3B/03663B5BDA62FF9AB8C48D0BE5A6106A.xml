<paragraph pageId="null" pageNumber="602">
<taxonomicName authority="DC." class="Magnoliopsida" family="Fabaceae" genus="Lathyrus" kingdom="Plantae" order="Fabales" pageId="null" pageNumber="602" phylum="Tracheophyta" rank="species" species="velutinus">
