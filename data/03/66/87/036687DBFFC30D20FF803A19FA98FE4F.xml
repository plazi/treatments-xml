<figureCitation box="[277,352,1464,1489]" captionStart="FIGURES 1 – 6" captionStartId="4.[151,264,1982,2005]" captionTargetBox="[167,1410,195,1931]" captionTargetId="figure@4.[167,1411,195,1933]" captionTargetPageId="4" captionText="FIGURES 1 – 6. Apsilocera spp., female, holotype, lateral view. 1 A. acuticristata; 2 A. bicristata; 3 A. cornuta; 4 A. dentata; 5 A. dupla; 6 A. elongata." httpUri="https://zenodo.org/record/248096/files/figure.png" pageId="6" pageNumber="454">
Figs 
<number box="[338,352,1465,1489]" pageId="6" pageNumber="454" value="1.19">1</number>
</figureCitation>
, 
<figureCitation box="[370,399,1465,1489]" captionStart="FIGURES 19 – 24" captionStartId="8.[151,264,1980,2003]" captionTargetBox="[168,1394,193,1903]" captionTargetId="figure@8.[168,1396,193,1905]" captionTargetPageId="8" captionText="FIGURES 19 – 24. Apsilocera spp., female, holotype, head in anterior view. 19 A. acuticristata; 20 A. bicristata; 21 A. cornuta; 22 A. dentata; 23 A. dupla; 24 A. elongata." httpUri="https://zenodo.org/record/248099/files/figure.png" pageId="6" pageNumber="454">19</figureCitation>
