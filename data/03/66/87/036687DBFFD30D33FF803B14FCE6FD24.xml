<number box="[770,801,258,282]" pageId="20" pageNumber="468" value="21.0">21</number>
; mesoscutum length: 
<number box="[1043,1074,257,282]" pageId="20" pageNumber="468" value="15.0">15</number>
; scutellum length: 
<number box="[1285,1316,258,282]" pageId="20" pageNumber="468" value="13.0">13</number>
