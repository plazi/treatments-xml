<emphasis bold="true" box="[140,478,553,580]" pageId="16" pageNumber="301">
<collectingRegion box="[140,323,553,580]" country="Canada" name="New Brunswick" pageId="16" pageNumber="301">New Brunswick</collectingRegion>
