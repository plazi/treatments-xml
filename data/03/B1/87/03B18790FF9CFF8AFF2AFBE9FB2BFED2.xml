<taxonomicName authority="Townsend, 1917" authorityName="Townsend" authorityYear="1917" box="[375,659,1238,1260]" class="Insecta" family="Psychodidae" genus="Xiphomyia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="24" pageNumber="175" phylum="Arthropoda" rank="genus">
