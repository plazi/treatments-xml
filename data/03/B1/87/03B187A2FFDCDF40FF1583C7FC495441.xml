<taxonomicName authority="(Osella, 1983)" authorityName="Osella" authorityYear="1983" baseAuthorityName="Osella" baseAuthorityYear="1983" class="Insecta" family="Curculionidae" genus="Otiorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="71" pageNumber="72" phylum="Arthropoda" rank="subSpecies" species="kahleni" subGenus="Baldorhynchus" subSpecies="valvestinus">
