<emphasis bold="true" box="[264,894,1364,1390]" pageId="14" pageNumber="15">
<taxonomicName box="[264,736,1364,1390]" class="Insecta" family="Helicopsychidae" genus="Helicopsyche" kingdom="Animalia" order="Trichoptera" pageId="14" pageNumber="15" phylum="Arthropoda" rank="species" species="neblinensis" status="new species" subGenus="Feropsyche">
<emphasis bold="true" box="[264,425,1364,1390]" italics="true" pageId="14" pageNumber="15">Helicopsyche</emphasis>
(
<emphasis bold="true" box="[442,582,1364,1390]" italics="true" pageId="14" pageNumber="15">Feropsyche</emphasis>
) 
<emphasis bold="true" box="[599,736,1364,1390]" italics="true" pageId="14" pageNumber="15">neblinensis</emphasis>
</taxonomicName>
