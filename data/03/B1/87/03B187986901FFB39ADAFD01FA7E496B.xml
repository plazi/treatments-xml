<subSubSection box="[192,1271,323,350]" pageId="5" pageNumber="4" type="distribution">
<paragraph blockId="5.[192,1271,323,350]" box="[192,1271,323,350]" pageId="5" pageNumber="4">
<emphasis bold="true" box="[192,346,323,349]" pageId="5" pageNumber="4">Distribution.</emphasis>
