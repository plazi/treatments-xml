Figs 
<number box="[1039,1081,476,500]" pageId="5" pageNumber="6" value="3.5" valueMax="5.0" valueMin="2.0">2–5</number>
</figureCitation>
and 
<figureCitation box="[1135,1149,477,500]" captionStart="FIGURE 7" captionStartId="14.[151,250,1153,1176]" captionTargetBox="[168,1414,388,1125]" captionTargetId="figure@14.[151,1436,373,1132]" captionTargetPageId="14" captionText="FIGURE 7. Catanduba canabrava sp. nov.: (A – C) Left palpal bulb: (A) prolateral view; (B) retrolateral view; (C) dorsal view. (D) Male left leg I tibial apophysis, ventral view. Scale bar = 1 mm." httpUri="https://zenodo.org/record/214022/files/figure.png" pageId="5" pageNumber="6">
<number box="[1135,1149,477,500]" pageId="5" pageNumber="6" value="7.0">7</number>
