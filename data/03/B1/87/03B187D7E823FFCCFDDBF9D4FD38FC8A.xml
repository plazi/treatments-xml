<subSubSection pageId="25" pageNumber="26" type="diagnosis">
<paragraph blockId="25.[189,454,1721,1747]" box="[189,454,1721,1747]" pageId="25" pageNumber="26">
<heading bold="true" box="[189,454,1721,1747]" fontSize="11" level="3" pageId="25" pageNumber="26" reason="3">
