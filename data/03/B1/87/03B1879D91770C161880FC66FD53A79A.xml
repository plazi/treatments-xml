<location LSID="urn:lsid:plazi:treatment:03B1879D91770C161880FC66FD53A79A:8EC7605091770C171CB9FB79FB42A2CA" box="[1198,1279,1166,1191]" country="India" county="This" municipality="Andrassy" name="Widely" pageId="16" pageNumber="17">Widely</location>
distributed in 
<location LSID="urn:lsid:plazi:treatment:03B1879D91770C161880FC66FD53A79A:8EC7605091770C171880FB43FEBFA2A6" box="[151,258,1204,1227]" country="India" county="This" municipality="Andrassy" name="European" pageId="16" pageNumber="17">European</location>
countries, registered in 
<location LSID="urn:lsid:plazi:treatment:03B1879D91770C161880FC66FD53A79A:8EC7605091770C171A37FB44FDEEA2A1" box="[544,595,1203,1228]" country="India" county="This" municipality="Andrassy" name="Asia" pageId="16" pageNumber="17">Asia</location>
(
<collectingCountry box="[614,676,1203,1228]" name="India" pageId="16" pageNumber="17">India</collectingCountry>
, 
<collectingCountry box="[691,823,1203,1228]" name="Bangladesh" pageId="16" pageNumber="17">Bangladesh</collectingCountry>
) and 
<location LSID="urn:lsid:plazi:treatment:03B1879D91770C161880FC66FD53A79A:8EC7605091770C171B69FB44FB97A2A1" box="[894,1066,1203,1228]" country="India" county="This" municipality="Andrassy" name="North America" pageId="16" pageNumber="17">North America</location>
(
<collectingCountry box="[1085,1174,1203,1228]" name="Canada" pageId="16" pageNumber="17">Canada</collectingCountry>
, USA) (
<bibRefCitation author="Pena-Santiago" pageId="16" pageNumber="17" refString="Pena-Santiago, R. (2006) Dorylaimida Part I: Superfamilies Belondiroidea, Nygolaimoidea and Tylencholaimoidea. In: Eyualem-Abebe, Traunspurger, W. &amp; Andrassy, I. (Eds.), Freshwater nematodes: ecology and taxonomy. CABI Publishing, Wallingford, pp. 326 - 391." type="book chapter" year="2006">
<location LSID="urn:lsid:plazi:treatment:03B1879D91770C161880FC66FD53A79A:8EC7605091770C171CEEFB44FA21A2A1" box="[1273,1436,1203,1228]" country="India" county="This" municipality="Andrassy" name="Pena-Santiago" pageId="16" pageNumber="17">Peña-Santiago</location>
2006
</bibRefCitation>
; 
<bibRefCitation author="Andrassy" box="[221,410,1238,1263]" pageId="16" pageNumber="17" refString="Andrassy, I. (2009 b) Free-living nematodes of Hungary (Nematoda Errantia). III. Pedozoologica Hungarica, No. 5, 1 - 608." type="book chapter" year="2009" yearSuffix="b">
<collectingMunicipality box="[221,327,1238,1263]" pageId="16" pageNumber="17">Andrássy</collectingMunicipality>
2009b
</bibRefCitation>
). 
<collectingCounty box="[428,478,1238,1263]" pageId="16" pageNumber="17">This</collectingCounty>
