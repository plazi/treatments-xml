<emphasis box="[151,292,1993,2017]" italics="true" pageId="5" pageNumber="492">S. densiflora</emphasis>
</taxonomicName>
, 
<taxonomicName box="[306,474,1993,2017]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="5" pageNumber="492" phylum="Tracheophyta" rank="species" species="macrophylla">
<emphasis box="[306,474,1993,2017]" italics="true" pageId="5" pageNumber="492">S. macrophylla</emphasis>
</taxonomicName>
, 
<taxonomicName box="[489,626,1993,2017]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="5" pageNumber="492" phylum="Tracheophyta" rank="species" species="parvifolia">
<emphasis box="[489,626,1993,2017]" italics="true" pageId="5" pageNumber="492">S. parvifolia</emphasis>
</taxonomicName>
, 
<taxonomicName box="[641,798,1993,2017]" class="Magnoliopsida" family="Dipterocarpaceae" genus="Shorea" kingdom="Plantae" order="Malvales" pageId="5" pageNumber="492" phylum="Tracheophyta" rank="species" species="platyclados">
