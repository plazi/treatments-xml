<subSubSection pageId="26" pageNumber="27" type="description">
<paragraph blockId="26.[151,1436,366,571]" box="[199,1066,402,427]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[199,643,402,427]" pageId="26" pageNumber="27">Population structure and abundance.</emphasis>
