<taxonomicName authority="(Osella, 1983)" authorityName="Osella" authorityYear="1983" baseAuthorityName="Osella" baseAuthorityYear="1983" box="[422,898,403,429]" class="Insecta" family="Curculionidae" genus="Baldorhynchus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="23" pageNumber="24" phylum="Arthropoda" rank="species" species="lessinicus">
