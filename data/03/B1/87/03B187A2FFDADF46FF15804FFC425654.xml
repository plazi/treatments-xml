<subSubSection box="[151,802,1130,1155]" pageId="65" pageNumber="66" type="materials_examined">
<paragraph blockId="65.[151,1437,1130,1661]" box="[151,802,1130,1155]" pageId="65" pageNumber="66">
<emphasis bold="true" box="[151,309,1130,1155]" pageId="65" pageNumber="66">Type locality.</emphasis>
“Cima Avez, 
<quantity box="[467,554,1131,1155]" metricMagnitude="3" metricUnit="m" metricValue="1.8399999999999999" pageId="65" pageNumber="66" unit="m" value="1840.0">1840 m</quantity>
