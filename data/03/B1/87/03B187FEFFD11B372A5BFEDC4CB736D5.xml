A 
<taxonomicName box="[888,1086,566,590]" class="Magnoliopsida" family="Gesneriaceae" genus="Primulina" kingdom="Plantae" order="Lamiales" pageId="1" pageNumber="10" phylum="Tracheophyta" rank="species" species="eburnea">Primulina eburnea</taxonomicName>
, 
<taxonomicName box="[1097,1182,566,590]" class="Magnoliopsida" family="Gesneriaceae" genus="Primulina" kingdom="Plantae" order="Lamiales" pageId="1" pageNumber="10" phylum="Tracheophyta" rank="species" species="dryas">P. dryas</taxonomicName>
, 
<taxonomicName box="[1192,1321,566,590]" class="Magnoliopsida" family="Gesneriaceae" genus="Primulina" kingdom="Plantae" order="Lamiales" pageId="1" pageNumber="10" phylum="Tracheophyta" rank="species" species="napoensis">P. napoensis</taxonomicName>
et P. spec. nov.
</emphasis>
<emphasis box="[865,1478,597,622]" italics="true" pageId="1" pageNumber="10">
differt bractis rhombiformis, sub angulo ca. 45 
<superScript attach="left" box="[1351,1357,597,611]" fontSize="6" pageId="1" pageNumber="10">°</superScript>
abientibus,
</emphasis>
<emphasis box="[866,1478,630,654]" italics="true" pageId="1" pageNumber="10">
calyx lobis late lanceolatis, corolla 
<quantity box="[1278,1399,630,654]" metricMagnitude="-2" metricUnit="m" metricValue="5.0" metricValueMax="5.5" metricValueMin="4.5" pageId="1" pageNumber="10" unit="cm" value="5.0" valueMax="5.5" valueMin="4.5">4.5-5.5 cm</quantity>
longa,
</emphasis>
<emphasis box="[866,1478,662,686]" italics="true" pageId="1" pageNumber="10">
filamentis ca. 
<quantity box="[1026,1102,662,686]" metricMagnitude="-2" metricUnit="m" metricValue="2.0" pageId="1" pageNumber="10" unit="mm" value="20.0">20 mm</quantity>
longis, pistillo 
<quantity box="[1280,1400,662,686]" metricMagnitude="-2" metricUnit="m" metricValue="3.85" metricValueMax="4.2" metricValueMin="3.5" pageId="1" pageNumber="10" unit="cm" value="3.85" valueMax="4.2" valueMin="3.5">3.5-4.2 cm</quantity>
