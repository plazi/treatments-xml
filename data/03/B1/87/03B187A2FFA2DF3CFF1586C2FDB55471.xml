<taxonomicName authority="Piva, 1984" authorityName="Piva" authorityYear="1984" box="[151,504,1015,1040]" class="Insecta" family="Leiodidae" genus="Neobathyscia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="leccei">
