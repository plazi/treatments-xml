<taxonomicName authority="(Dodero, 1904)" authorityName="Dodero" authorityYear="1904" baseAuthorityName="Dodero" baseAuthorityYear="1904" box="[1000,1431,980,1005]" class="Insecta" family="Leiodidae" genus="Neobathyscia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="fabianii">
<emphasis box="[1000,1247,980,1005]" italics="true" pageId="59" pageNumber="60">Neobathyscia fabianii</emphasis>
