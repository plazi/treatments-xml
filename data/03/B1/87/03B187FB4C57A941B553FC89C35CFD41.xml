sp. [
<taxonomicName box="[274,386,1607,1632]" class="Insecta" family="Cicadellidae" genus="Cuerna" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="alpina">
<emphasis box="[274,386,1607,1632]" italics="true" pageId="31" pageNumber="32">C. alpina</emphasis>
</taxonomicName>
<collectingCountry box="[396,468,1607,1633]" name="Oman" pageId="31" pageNumber="32">Oman</collectingCountry>
&amp; Beamer or 
<taxonomicName authority="Walker" authorityName="Walker" box="[648,977,1607,1633]" class="Insecta" family="Cicadellidae" genus="Cuerna" kingdom="Animalia" order="Hemiptera" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="septentrionalis">
