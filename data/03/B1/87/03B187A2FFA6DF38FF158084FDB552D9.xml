<taxonomicName authority="Leptodirinae" authorityName="Leptodirinae" box="[1152,1432,655,680]" class="Insecta" family="Cholevidae" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="63" pageNumber="64" phylum="Arthropoda" rank="family">Cholevidae Leptodirinae</taxonomicName>
