<date box="[849,907,1559,1583]" pageId="61" pageNumber="62" value="1939-05">39 V</date>
/VI) 
<quantity box="[962,1036,1559,1583]" metricMagnitude="2" metricUnit="m" metricValue="3.3" pageId="61" pageNumber="62" unit="m" value="330.0">330 m</quantity>
