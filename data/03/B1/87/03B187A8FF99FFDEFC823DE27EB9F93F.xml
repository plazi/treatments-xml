<subSubSection box="[808,1088,1201,1226]" pageId="31" type="nomenclature">
<paragraph blockId="31.[808,1088,1201,1226]" box="[808,1088,1201,1226]" pageId="31">
<heading box="[808,1088,1201,1226]" centered="true" fontSize="9" level="2" pageId="31" reason="2">
<taxonomicName authority="Grimaldi, 2018" authorityName="Grimaldi" authorityYear="2018" box="[808,958,1201,1225]" class="Insecta" family="Phoridae" genus="Alonchoptera" kingdom="Animalia" order="Diptera" pageId="31" phylum="Arthropoda" rank="genus" status="new genus">
<emphasis bold="true" box="[808,958,1201,1225]" italics="true" pageId="31">Alonchoptera</emphasis>
</taxonomicName>
