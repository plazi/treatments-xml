<location LSID="urn:lsid:plazi:treatment:03B1879D91700C1F1880FD9BFCB1A7C0:8EC7605091700C1018D0F997FE9BA014" box="[199,294,1632,1657]" country="Switzerland" county="Vinciguerra" municipality="Altherr" name="Ecology" pageId="23" pageNumber="24">Ecology</location>
