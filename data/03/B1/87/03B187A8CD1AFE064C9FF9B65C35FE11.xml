<collectingCountry box="[789,909,1920,1945]" name="Seychelles" pageId="12" pageNumber="237">Seychelles</collectingCountry>
(
<specimenCode box="[924,1102,1920,1945]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="237">
<collectionCode box="[924,1007,1921,1945]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="237">USNM</collectionCode>
385746
