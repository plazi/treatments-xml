examined by 
<collectorName box="[459,579,1848,1873]" pageId="12" pageNumber="237">McKinney</collectorName>
(
<specimenCode box="[594,771,1849,1873]" collectionCode="USNM" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="237">
<collectionCode box="[594,677,1849,1873]" country="USA" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="12" pageNumber="237">USNM</collectionCode>
220030
</specimenCode>
, 
<collectingRegion box="[783,879,1848,1873]" country="Sudan" name="Al Bahr al Ahmar" pageId="12" pageNumber="237">Red Sea</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:03B187A8CD1AFE064C9FF9B65C35FE11:8EC76065CD1DFE074F73F8F15D1AF890" box="[891,1057,1848,1873]" country="Seychelles" county="Ressource Island" municipality="St. Joseph Atoll" name="Gulf of Aqaba" pageId="12" pageNumber="237" stateProvince="Red Sea">Gulf of Aqaba</location>
, 
<collectorName box="[1068,1113,1849,1873]" pageId="12" pageNumber="237">Bay</collectorName>
at 
<collectorName box="[1149,1277,1848,1873]" pageId="12" pageNumber="237">El Himeira</collectorName>
, 
<specimenCount box="[1288,1429,1848,1873]" pageId="12" pageNumber="237" type="generic">4 specimens</specimenCount>
) was labeled as “ 
<taxonomicName box="[343,461,1885,1909]" class="Actinopterygii" family="Gobiidae" genus="Callogobius" kingdom="Animalia" order="Perciformes" pageId="12" pageNumber="237" phylum="Chordata" rank="species" species="aquilus">
<emphasis box="[343,461,1885,1909]" italics="true" pageId="12" pageNumber="237">C. aquilus</emphasis>
