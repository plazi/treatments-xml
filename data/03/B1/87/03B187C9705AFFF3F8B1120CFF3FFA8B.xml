<subSubSection lastPageId="16" lastPageNumber="191" pageId="14" pageNumber="189" type="description">
<paragraph blockId="14.[137,1059,953,1056]" pageId="14" pageNumber="189">
<taxonomicName baseAuthorityName="O. Pickard-Cambridge" baseAuthorityYear="1873" box="[137,370,984,1005]" class="Arachnida" family="Linyphiidae" genus="Walckenaeria" kingdom="Animalia" order="Araneae" pageId="14" pageNumber="189" phylum="Arthropoda" rank="species" species="karpinskii">
