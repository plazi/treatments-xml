<taxonomicName authority="(Allgen, 1933) Schuurmans Stekhoven, 1951" authorityName="(Allgen, 1933) Schuurmans Stekhoven" authorityYear="1951" box="[193,987,796,821]" class="Adenophorea" family="Cryptonchidae" genus="Cryptonchus" kingdom="Animalia" order="Enoplida" pageId="26" pageNumber="27" phylum="Nematoda" rank="species" species="abnormis">
