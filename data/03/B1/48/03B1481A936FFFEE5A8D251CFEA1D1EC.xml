, 
<geoCoordinate box="[151,295,1065,1091]" degrees="14" direction="north" minutes="16" orientation="latitude" pageId="5" pageNumber="426" precision="15" seconds="40" value="14.277778">14°16´40´´N</geoCoordinate>
<geoCoordinate box="[302,479,1065,1092]" degrees="102" direction="east" minutes="25" orientation="longitude" pageId="5" pageNumber="426" precision="1" seconds="28.5" value="102.42458">102°25´28.5´´E</geoCoordinate>
, 
<collectingMethod box="[491,595,1065,1092]" pageId="5" pageNumber="426">light trap</collectingMethod>
, A. 
<collectionCode box="[631,653,1066,1092]" country="Canada" lsid="urn:lsid:biocol.org:col:13946" name="Royal British Columbia Museum - Herbarium" pageId="5" pageNumber="426" type="Museum">V</collectionCode>
</materialsCitation>
. 
<materialsCitation box="[663,921,1065,1091]" collectionCode="CAV" collectorName="Korshunov" country="Thailand" pageId="5" pageNumber="426" specimenCount="1">
<collectorName box="[663,787,1065,1091]" pageId="5" pageNumber="426">Korshunov</collectorName>
leg. (
