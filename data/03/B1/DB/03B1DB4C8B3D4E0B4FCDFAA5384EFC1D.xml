<paragraph blockId="18.[189,1091,939,966]" box="[189,1091,939,966]" pageId="18" pageNumber="19">
<taxonomicName authorityName="Nurinsiyah &amp; Neiber &amp; Hausdorf" authorityYear="2019" baseAuthorityName="Leschke" baseAuthorityYear="1914" box="[189,428,939,965]" class="Gastropoda" family="Camaenidae" genus="Landouria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="18" pageNumber="19" phylum="Mollusca" rank="species" species="conoidea">
