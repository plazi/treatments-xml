<paragraph blockId="52.[189,1398,1410,1471]" pageId="52" pageNumber="53">
<taxonomicName authorityName="Nurinsiyah &amp; Neiber &amp; Hausdorf" authorityYear="2019" baseAuthorityName="Martens" baseAuthorityYear="1867" box="[189,476,1410,1436]" class="Gastropoda" family="Camaenidae" genus="Landouria" higherTaxonomySource="GBIF,CoL" kingdom="Animalia" order="Stylommatophora" pageId="52" pageNumber="53" phylum="Mollusca" rank="species" species="moussoniana">
