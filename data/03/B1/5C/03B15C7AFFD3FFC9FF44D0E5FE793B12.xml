<quantity box="[499,665,407,432]" metricMagnitude="-3" metricUnit="m" metricValue="5.225" metricValueMax="5.3" metricValueMin="5.15" pageId="3" pageNumber="138" unit="mm" value="5.225" valueMax="5.3" valueMin="5.15">5.15–5.30 mm</quantity>
