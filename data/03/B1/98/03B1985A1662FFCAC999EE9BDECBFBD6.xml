<taxonomicName box="[345,453,1866,1890]" class="Insecta" family="Ptiloneuridae" genus="Ptiloneura" higherTaxonomySource="CoL" kingdom="Animalia" order="Psocodea" pageId="34" pageNumber="435" phylum="Arthropoda" rank="genus">Ptiloneura</taxonomicName>
<taxonomicName box="[459,597,1867,1890]" pageId="34" pageNumber="435" rank="species" species="putumayensis" status="n. sp.">putumayensis</taxonomicName>
