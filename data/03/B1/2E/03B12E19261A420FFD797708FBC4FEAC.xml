<taxonomicName authorityName="Nomura &amp; Leschen" authorityYear="2015" baseAuthorityName="Nomura &amp; Leschen" baseAuthorityYear="2015" box="[285,448,863,882]" class="Insecta" family="Staphylinidae" genus="Parkerola" kingdom="Animalia" order="Coleoptera" pageId="15" phylum="Arthropoda" rank="species" species="gigantea">
