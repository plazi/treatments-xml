<emphasis box="[1322,1429,1823,1846]" italics="true" pageId="3" pageNumber="434">Maimuna</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[200,275,1859,1882]" class="Arachnida" family="Agelenidae" genus="Textrix" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus">
