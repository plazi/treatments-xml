<number box="[735,780,1714,1739]" pageId="3" pageNumber="434" value="4.0" valueMax="5.0" valueMin="3.0">3–5</number>
</figureCitation>
; both rows recurved in 
<taxonomicName box="[1044,1168,1714,1739]" class="Arachnida" family="Agelenidae" genus="Lycosoides" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus">
<emphasis box="[1044,1168,1714,1739]" italics="true" pageId="3" pageNumber="434">Lycosoides</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1182,1289,1715,1738]" class="Arachnida" family="Agelenidae" genus="Maimuna" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus">
<emphasis box="[1182,1289,1715,1738]" italics="true" pageId="3" pageNumber="434">Maimuna</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[1351,1426,1715,1738]" class="Arachnida" family="Agelenidae" genus="Textrix" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus">
