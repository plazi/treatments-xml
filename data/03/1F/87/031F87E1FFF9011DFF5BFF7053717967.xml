<emphasis box="[419,511,1749,1774]" italics="true" pageId="3" pageNumber="434">Agelena</emphasis>
Walckenaer, 
<number box="[662,718,1749,1774]" pageId="3" pageNumber="434" value="1805.0">1805</number>
</taxonomicName>
, 
<taxonomicName authority="Levy, 1996" authorityName="Levy" authorityYear="1996" box="[732,992,1749,1775]" class="Arachnida" family="Agelenidae" genus="Agelescape" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus">
<emphasis box="[732,860,1749,1774]" italics="true" pageId="3" pageNumber="434">Agelescape</emphasis>
Levy, 
<number box="[936,992,1749,1774]" pageId="3" pageNumber="434" value="1996.0">1996</number>
</taxonomicName>
