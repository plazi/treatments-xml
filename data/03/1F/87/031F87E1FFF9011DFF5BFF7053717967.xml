Simon, 
<number box="[915,974,1822,1846]" pageId="3" pageNumber="434" value="1898.0">1898</number>
</taxonomicName>
; AER recurved in 
<taxonomicName box="[1184,1308,1821,1846]" class="Arachnida" family="Agelenidae" genus="Lycosoides" kingdom="Animalia" order="Araneae" pageId="3" pageNumber="434" phylum="Arthropoda" rank="genus">
