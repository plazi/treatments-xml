, 
<collectingCountry box="[547,633,400,426]" name="Tunisia" pageId="32" pageNumber="33">Tunisia</collectingCountry>
, 
<collectingCountry box="[643,723,400,426]" name="Turkey" pageId="32" pageNumber="33">Turkey</collectingCountry>
and 
<collectingCountry box="[778,867,400,426]" name="Zambia" pageId="32" pageNumber="33">Zambia</collectingCountry>
