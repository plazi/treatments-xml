<taxonomicName authority="Itoh, 2001" authorityName="Itoh" authorityYear="2001" box="[339,667,1221,1247]" class="Insecta" family="Melolonthidae" genus="Miridiba" kingdom="Animalia" order="Coleoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="species" species="hirsuta">
