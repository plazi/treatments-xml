<figureCitation box="[550,579,1934,1956]" captionStart="FIGURE 5" captionStartId="11.[151,250,1804,1828]" captionTargetBox="[151,1436,196,1780]" captionTargetId="figure-21@11.[151,1436,181,1780]" captionTargetPageId="11" captionText="FIGURE 5. Embolemus huberi Olmi, 1997, holotype, male from Iran, (CNC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I. Mesosoma; F. Dorsal view; I. Lateral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view; K. Holotype labels." pageId="3" pageNumber="308">5E</figureCitation>
); eye large (
<figureCitation box="[702,785,1934,1956]" captionStart="FIGURE 4" captionStartId="9.[151,250,1772,1796]" captionTargetBox="[151,1436,181,1748]" captionTargetId="figure-21@9.[151,1436,181,1748]" captionTargetPageId="9" captionText="FIGURE 4. Embolemus harteni Olmi, 1997, male from Yemen, (MOLC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I, K. Mesosoma; F. Dorsal view; I. Lateral view; K. Ventral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view." pageId="3" pageNumber="308">Figs 4D</figureCitation>
, 
<figureCitation box="[796,828,1934,1956]" captionStart="FIGURE 5" captionStartId="11.[151,250,1804,1828]" captionTargetBox="[151,1436,196,1780]" captionTargetId="figure-21@11.[151,1436,181,1780]" captionTargetPageId="11" captionText="FIGURE 5. Embolemus huberi Olmi, 1997, holotype, male from Iran, (CNC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I. Mesosoma; F. Dorsal view; I. Lateral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view; K. Holotype labels." pageId="3" pageNumber="308">5D</figureCitation>
); TL short, OD= 3.0–4.0 (
<figureCitation box="[1088,1167,1934,1956]" captionStart="FIGURE 4" captionStartId="9.[151,250,1772,1796]" captionTargetBox="[151,1436,181,1748]" captionTargetId="figure-21@9.[151,1436,181,1748]" captionTargetPageId="9" captionText="FIGURE 4. Embolemus harteni Olmi, 1997, male from Yemen, (MOLC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I, K. Mesosoma; F. Dorsal view; I. Lateral view; K. Ventral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view." pageId="3" pageNumber="308">Figs 4E</figureCitation>
, 
<figureCitation box="[1179,1208,1934,1956]" captionStart="FIGURE 5" captionStartId="11.[151,250,1804,1828]" captionTargetBox="[151,1436,196,1780]" captionTargetId="figure-21@11.[151,1436,181,1780]" captionTargetPageId="11" captionText="FIGURE 5. Embolemus huberi Olmi, 1997, holotype, male from Iran, (CNC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I. Mesosoma; F. Dorsal view; I. Lateral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view; K. Holotype labels." pageId="3" pageNumber="308">5E</figureCitation>
); notaulus absent (
<figureCitation captionStart="FIGURE 4" captionStartId="9.[151,250,1772,1796]" captionTargetBox="[151,1436,181,1748]" captionTargetId="figure-21@9.[151,1436,181,1748]" captionTargetPageId="9" captionText="FIGURE 4. Embolemus harteni Olmi, 1997, male from Yemen, (MOLC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I, K. Mesosoma; F. Dorsal view; I. Lateral view; K. Ventral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view." pageId="3" pageNumber="308">Figs 4F</figureCitation>
, 
<figureCitation box="[261,290,1964,1986]" captionStart="FIGURE 5" captionStartId="11.[151,250,1804,1828]" captionTargetBox="[151,1436,196,1780]" captionTargetId="figure-21@11.[151,1436,181,1780]" captionTargetPageId="11" captionText="FIGURE 5. Embolemus huberi Olmi, 1997, holotype, male from Iran, (CNC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I. Mesosoma; F. Dorsal view; I. Lateral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view; K. Holotype labels." pageId="3" pageNumber="308">5F</figureCitation>
); metapectal-propodeal complex smooth, shiny, weakly rugose, with some irregular striae only near metanotum, lateral areas, and apically (
<figureCitation box="[416,497,1993,2015]" captionStart="FIGURE 4" captionStartId="9.[151,250,1772,1796]" captionTargetBox="[151,1436,181,1748]" captionTargetId="figure-21@9.[151,1436,181,1748]" captionTargetPageId="9" captionText="FIGURE 4. Embolemus harteni Olmi, 1997, male from Yemen, (MOLC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I, K. Mesosoma; F. Dorsal view; I. Lateral view; K. Ventral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view." pageId="3" pageNumber="308">Figs 4G</figureCitation>
, 
<figureCitation box="[506,537,1993,2015]" captionStart="FIGURE 5" captionStartId="11.[151,250,1804,1828]" captionTargetBox="[151,1436,196,1780]" captionTargetId="figure-21@11.[151,1436,181,1780]" captionTargetPageId="11" captionText="FIGURE 5. Embolemus huberi Olmi, 1997, holotype, male from Iran, (CNC). A. Body, dorsal view; B. Fore wing; C–E. Head; C. Frontal view; D. Lateral view; E. Dorsal view; F, I. Mesosoma; F. Dorsal view; I. Lateral view; G. Metapectal-propodeal complex, dorsal view; H, J. Metasoma; H. Dorsal view; J. Lateral view; K. Holotype labels." pageId="3" pageNumber="308">5G</figureCitation>
)......................................................................... 
