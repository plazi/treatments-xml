<subSubSection pageId="7" pageNumber="552" type="distribution">
<paragraph blockId="7.[151,1437,150,213]" pageId="7" pageNumber="552">
<emphasis bold="true" box="[199,353,150,176]" pageId="7" pageNumber="552">Distribution.</emphasis>
<collectingCountry box="[359,426,151,177]" name="China" pageId="7" pageNumber="552">China</collectingCountry>
(
<collectingRegion box="[440,537,151,178]" country="China" name="Sichuan" pageId="7" pageNumber="552">Sichuan</collectingRegion>
