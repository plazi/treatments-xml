<paragraph blockId="5.[151,1436,1425,1524]" pageId="5" pageNumber="30">
<taxonomicName authorityName="Kurz" authorityYear="1874" box="[199,364,1425,1451]" class="Branchiopoda" family="Moinidae" genus="Moina" kingdom="Animalia" order="Diplostraca" pageId="5" pageNumber="30" phylum="Arthropoda" rank="species" species="micrura">
