<location LSID="urn:lsid:plazi:treatment:03B55800FFE3B27386C9FE54FBC8FD26:8EC3BFCDFFE3B27384F1FDC8FCE3FDD6" box="[687,863,549,575]" country="Aruba" county="Boton. It" municipality="Westpunt" name="Lourdes Grotto" pageId="2" pageNumber="27">Lourdes Grotto</location>
