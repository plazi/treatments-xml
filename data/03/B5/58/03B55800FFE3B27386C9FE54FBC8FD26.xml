<location LSID="urn:lsid:plazi:treatment:03B55800FFE3B27386C9FE54FBC8FD26:8EC3BFCDFFE3B2738532FDC8FC4FFDD6" box="[876,1011,549,575]" country="Aruba" county="Boton. It" municipality="Westpunt" name="Seroe Preto" pageId="2" pageNumber="27">Seroe Preto</location>
