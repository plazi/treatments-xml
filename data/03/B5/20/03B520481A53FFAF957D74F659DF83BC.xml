<typeStatus box="[442,493,2001,2027]" pageId="12" pageNumber="13">type</typeStatus>
locality, off 
<collectingCountry box="[641,823,2001,2027]" name="New Caledonia" pageId="12" pageNumber="13">New Caledonia</collectingCountry>
