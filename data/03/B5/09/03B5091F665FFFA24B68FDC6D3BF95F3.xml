<taxonomicName authority="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy, 2020" authorityName="Kamalanathan &amp; Mohanraj &amp; Samuel &amp; Reddy" authorityYear="2020" box="[588,726,905,929]" class="Insecta" family="Scelionidae" genus="Baeus" kingdom="Animalia" order="Hymenoptera" pageId="91" pageNumber="903" phylum="Arthropoda" rank="species" species="giganteus" status="sp. n.">
