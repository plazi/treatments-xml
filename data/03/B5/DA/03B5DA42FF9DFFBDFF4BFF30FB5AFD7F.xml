se distingue facilement de 
<taxonomicName authority="sensu Sojak" authorityName="Sojak" class="Magnoliopsida" family="Rosaceae" genus="Potentilla" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="242" phylum="Tracheophyta" rank="species" species="philippinensis">
