<paragraph blockId="3.[714,1248,170,657]" pageId="3" pageNumber="242">
<taxonomicName authorityName="Danet" authorityYear="2003" box="[740,918,386,412]" class="Magnoliopsida" family="Rosaceae" genus="Potentilla" kingdom="Plantae" order="Rosales" pageId="3" pageNumber="242" phylum="Tracheophyta" rank="species" species="biloba">
<emphasis box="[740,918,386,412]" italics="true" pageId="3" pageNumber="242">Potentilla biloba</emphasis>
