<subSubSection box="[151,803,362,384]" pageId="1" pageNumber="292" type="materials_examined">
<paragraph blockId="1.[151,803,362,384]" box="[151,803,362,384]" pageId="1" pageNumber="292">
<emphasis bold="true" box="[151,290,362,384]" pageId="1" pageNumber="292">Type species.</emphasis>
<taxonomicName authority="Chen, 2018" authorityName="Chen" authorityYear="2018" box="[297,487,362,384]" class="Insecta" family="Leuctridae" genus="Euroleuctra" higherTaxonomySource="GBIF" kingdom="Animalia" order="Plecoptera" pageId="1" pageNumber="292" phylum="Arthropoda" rank="species" species="gillesi" status="gen. et sp. nov.">
