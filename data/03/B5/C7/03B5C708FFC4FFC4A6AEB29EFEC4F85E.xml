<paragraph blockId="1.[151,586,512,536]" box="[151,586,512,536]" pageId="1" pageNumber="284">
<typeStatus box="[151,201,512,536]" pageId="1" pageNumber="284">Type</typeStatus>
species: 
<taxonomicName authority="Byun, 2020" authorityName="Byun" authorityYear="2020" box="[294,501,512,536]" class="Insecta" family="Tortricidae" genus="Nabangana" kingdom="Animalia" order="Lepidoptera" pageId="1" pageNumber="284" phylum="Arthropoda" rank="species" species="koreana" status="sp. nov.">
