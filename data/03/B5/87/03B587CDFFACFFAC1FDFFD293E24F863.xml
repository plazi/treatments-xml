<paragraph blockId="8.[151,641,1100,1160]" pageId="8" pageNumber="207">
<taxonomicName authority="Casanova, 1996" authorityName="Casanova" authorityYear="1996" box="[151,641,1100,1125]" class="Malacostraca" family="Lophogastridae" genus="Gnathophausia" kingdom="Animalia" order="Lophogastrida" pageId="8" pageNumber="207" phylum="Arthropoda" rank="species" species="childressi">
