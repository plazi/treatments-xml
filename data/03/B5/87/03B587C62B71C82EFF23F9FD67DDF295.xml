<taxonomicName authority="McAlpine" authorityName="McAlpine" box="[1250,1436,591,613]" class="Insecta" family="Lonchaeidae" genus="Lonchaea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="16" pageNumber="341" phylum="Arthropoda" rank="species" species="dichaeta">
