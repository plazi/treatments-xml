and 
<taxonomicName authorityName="Cuvier" authorityYear="1829" box="[1107,1170,764,787]" class="Actinopterygii" family="Scombridae" genus="Auxis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Perciformes" pageId="45" pageNumber="46" phylum="Chordata" rank="genus">
