<collectingCountry box="[983,1047,609,632]" name="China" pageId="3" pageNumber="376">China</collectingCountry>
(
<collectingRegion box="[1063,1155,609,632]" country="China" name="Qinghai" pageId="3" pageNumber="376">Qinghai</collectingRegion>
