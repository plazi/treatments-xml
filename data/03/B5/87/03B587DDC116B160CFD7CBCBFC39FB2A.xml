<typeStatus box="[151,210,524,549]" pageId="44" pageNumber="45">Type</typeStatus>
species.
</emphasis>
<taxonomicName authority="Mocquard, 1905" authorityName="Mocquard" authorityYear="1905" box="[313,740,524,549]" class="Reptilia" family="Typhlopidae" genus="Typhlops" kingdom="Animalia" order="Squamata" pageId="44" pageNumber="45" phylum="Chordata" rank="species" species="grandidieri">
<emphasis box="[313,547,524,549]" italics="true" pageId="44" pageNumber="45">Typhlops grandidieri</emphasis>
