, 
<geoCoordinate box="[151,286,592,616]" direction="south" orientation="latitude" pageId="10" pageNumber="37" precision="1" value="-0.41889998">
0° 
<number box="[175,205,592,616]" pageId="10" pageNumber="37" value="25.0">25</number>
'8.04''S
</geoCoordinate>
, 
<geoCoordinate box="[299,457,592,616]" direction="west" orientation="longitude" pageId="10" pageNumber="37" precision="1" value="-79.0039">
<number box="[299,328,592,616]" pageId="10" pageNumber="37" value="79.0">79</number>
°0'14.04''W
</geoCoordinate>
, Reserva de Bosque Integral Otonga, near San Francisco de las Pampas, 
<quantity box="[1285,1436,592,616]" metricMagnitude="3" metricUnit="m" metricValue="2.1" metricValueMax="2.2" metricValueMin="2.0" pageId="10" pageNumber="37" unit="m" value="2100.0" valueMax="2200.0" valueMin="2000.0">2000-2200 m</quantity>
