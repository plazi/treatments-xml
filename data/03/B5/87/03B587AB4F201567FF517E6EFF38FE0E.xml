. 
<taxonomicName authority="Ketwetsuriya &amp; Karapunar &amp; Charoentitirat &amp; Nützel, 2020" authorityName="Ketwetsuriya &amp; Karapunar &amp; Charoentitirat &amp; Nützel" authorityYear="2020" box="[252,354,333,359]" class="Gastropoda" family="Araeonematidae" genus="Yunnania" higherTaxonomySource="CoL" kingdom="Fungi" order="Trochida" pageId="25" pageNumber="26" phylum="Ascomycota" rank="species" species="inflata" status="sp. nov.">
