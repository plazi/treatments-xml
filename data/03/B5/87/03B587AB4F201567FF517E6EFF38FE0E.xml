<taxonomicName authorityName="Nutzel &amp; Ketwetsuriya" authorityYear="2016" box="[807,925,225,251]" class="Gastropoda" family="Araeonematidae" genus="Yunnania" higherTaxonomySource="CoL" kingdom="Fungi" order="Trochida" pageId="25" pageNumber="26" phylum="Ascomycota" rank="species" species="pulchra">
