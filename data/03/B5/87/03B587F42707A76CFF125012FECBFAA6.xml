<number box="[214,256,776,799]" pageId="2" pageNumber="68" value="175.0">175</number>
, 
<quantity box="[267,341,776,799]" metricMagnitude="3" metricUnit="m" metricValue="2.9" pageId="2" pageNumber="68" unit="km" value="2.9">2.9 km</quantity>
S. La Esperanza, 
<number box="[534,594,776,799]" pageId="2" pageNumber="68" value="4900.0">4900</number>
’, cloud forest, at UV light, 
<date box="[883,1017,776,799]" pageId="2" pageNumber="68" value="1989-07-02">July 2, 1989</date>
, J. Rifkind, coll. 
<typeStatus box="[1206,1299,776,799]" pageId="2" pageNumber="68">Paratype</typeStatus>
