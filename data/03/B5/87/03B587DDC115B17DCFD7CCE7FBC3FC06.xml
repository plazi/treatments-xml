<number box="[816,845,908,933]" pageId="49" pageNumber="50" value="54.0">54</number>
%– 
<number box="[883,910,908,933]" pageId="49" pageNumber="50" value="85.0">85</number>
% in 
<taxonomicName box="[968,1130,908,933]" class="Reptilia" family="Typhlopidae" genus="Rhinotyphlops" kingdom="Animalia" order="Squamata" pageId="49" pageNumber="50" phylum="Chordata" rank="genus">
<emphasis box="[968,1130,908,933]" italics="true" pageId="49" pageNumber="50">Rhinotyphlops</emphasis>
