<taxonomicName box="[1026,1186,836,861]" class="Reptilia" family="Typhlopidae" genus="Madatyphlops" higherTaxonomySource="GBIF" kingdom="Animalia" order="Squamata" pageId="49" pageNumber="50" phylum="Chordata" rank="genus">
