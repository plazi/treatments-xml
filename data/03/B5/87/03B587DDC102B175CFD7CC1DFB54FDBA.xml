<paragraph blockId="56.[151,1436,1625,2010]" pageId="56" pageNumber="57">
<emphasis bold="true" box="[199,398,1661,1686]" pageId="56" pageNumber="57">Species content.</emphasis>
<emphasis box="[413,1325,1661,1686]" italics="true" pageId="56" pageNumber="57">
<taxonomicName box="[413,708,1661,1686]" class="Reptilia" family="Typhlopidae" genus="Acutotyphlops" kingdom="Animalia" order="Squamata" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="banaorum">Acutotyphlops banaorum</taxonomicName>
, 
<taxonomicName box="[728,923,1661,1686]" class="Reptilia" family="Typhlopidae" genus="Acutotyphlops" kingdom="Animalia" order="Squamata" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="infralabialis">Ac. infralabialis</taxonomicName>
, 
<taxonomicName box="[942,1128,1661,1686]" class="Reptilia" family="Typhlopidae" genus="Acutotyphlops" kingdom="Animalia" order="Squamata" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="kunuaensis">
<emphasis bold="true" box="[942,1128,1661,1686]" italics="true" pageId="56" pageNumber="57">Ac. kunuaensis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1152,1320,1661,1686]" class="Reptilia" family="Typhlopidae" genus="Acutotyphlops" kingdom="Animalia" order="Squamata" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="solomonis">Ac. solomonis</taxonomicName>
,
</emphasis>
and 
<emphasis italics="true" pageId="56" pageNumber="57">
<taxonomicName class="Reptilia" family="Typhlopidae" genus="Acutotyphlops" kingdom="Animalia" order="Squamata" pageId="56" pageNumber="57" phylum="Chordata" rank="species" species="subocularis">
<emphasis bold="true" italics="true" pageId="56" pageNumber="57">Ac. subocularis</emphasis>
</taxonomicName>
