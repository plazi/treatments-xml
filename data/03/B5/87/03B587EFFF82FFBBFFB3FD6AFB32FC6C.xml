<location LSID="urn:lsid:plazi:treatment:03B587EFFF82FFBBFFB3FD6AFB32FC6C:8EC36022FF82FFB8FC0DFD05FBA9FD78" box="[961,1058,660,680]" country="Madagascar" name="University" pageId="13" pageNumber="14" stateProvince="Antananarivo">University</location>
