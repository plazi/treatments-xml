<quantity box="[757,920,501,527]" metricMagnitude="-4" metricUnit="m" metricValue="8.35" metricValueMax="8.6" metricValueMin="8.1" pageId="23" pageNumber="396" unit="mm" value="0.835" valueMax="0.86" valueMin="0.81">0.81–0.86mm</quantity>
