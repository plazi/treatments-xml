<quantity box="[552,717,501,527]" metricMagnitude="-3" metricUnit="m" metricValue="1.335" metricValueMax="1.36" metricValueMin="1.31" pageId="23" pageNumber="396" unit="mm" value="1.335" valueMax="1.36" valueMin="1.31">1.31–1.36mm</quantity>
