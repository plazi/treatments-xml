<accessionNumber box="[1148,1298,960,986]" httpUri="http://www.uniprot.org/uniprot/ZMB13086" pageId="61" pageNumber="434">
<collectionCode box="[1148,1211,960,986]" httpUri="http://grbio.org/cool/8syf-d21i" name="Museum für Naturkunde Berlin (Zoological Collections)" pageId="61" pageNumber="434">ZMB</collectionCode>
<number box="[1218,1298,960,986]" pageId="61" pageNumber="434" value="13086.0">13086</number>
</accessionNumber>
