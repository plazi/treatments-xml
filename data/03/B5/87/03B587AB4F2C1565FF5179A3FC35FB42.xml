<subSubSection pageId="27" pageNumber="28" type="materials_examined">
<paragraph blockId="27.[151,1437,584,1223]" box="[151,637,584,611]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[151,262,584,611]" pageId="27" pageNumber="28">Material.</emphasis>
