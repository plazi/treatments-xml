<paragraph blockId="18.[151,1437,1290,2012]" pageId="18" pageNumber="66">
<taxonomicName box="[197,318,1909,1934]" class="Magnoliopsida" family="Apocynaceae" genus="Epigynum" kingdom="Plantae" order="Gentianales" pageId="18" pageNumber="66" phylum="Tracheophyta" rank="genus">
