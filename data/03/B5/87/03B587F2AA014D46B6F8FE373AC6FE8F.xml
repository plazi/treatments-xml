. 
<taxonomicName authority="Bassett-Smith, 1898" authorityName="Bassett-Smith" authorityYear="1898" box="[297,708,1934,1956]" class="Maxillopoda" family="Caligidae" genus="Caligus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Siphonostomatoida" pageId="109" pageNumber="110" phylum="Arthropoda" rank="species" species="platytarsis">
<taxonomicName box="[297,376,1934,1956]" class="Hexanauplia" family="Caligidae" genus="Caligus" higherTaxonomySource="CoL" kingdom="Animalia" order="Siphonostomatoida" pageId="109" pageNumber="110" phylum="Arthropoda" rank="genus">
<emphasis box="[297,376,1934,1956]" italics="true" pageId="109" pageNumber="110">Caligus</emphasis>
</taxonomicName>
<taxonomicName authority="Bassett-Smith, 1898" authorityName="Bassett-Smith" authorityYear="1898" box="[384,708,1934,1956]" pageId="109" pageNumber="110" rank="species" species="platytarsis">
