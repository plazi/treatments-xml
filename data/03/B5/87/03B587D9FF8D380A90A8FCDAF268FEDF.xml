Southern 
<collectingCountry box="[480,579,1826,1852]" name="Somalia" pageId="24" pageNumber="55">Somalia</collectingCountry>
, eastern 
<collectingCountry box="[681,757,1826,1852]" name="Kenya" pageId="24" pageNumber="55">Kenya</collectingCountry>
and northeastern 
<collectingCountry box="[966,1074,1826,1852]" name="Tanzania" pageId="24" pageNumber="55">Tanzania</collectingCountry>
