<quantity box="[1086,1223,1826,1852]" metricMagnitude="2" metricUnit="m" metricValue="8.1" metricValueMax="16.0" metricValueMin="0.2" pageId="24" pageNumber="55" unit="m" value="810.0" valueMax="1600.0" valueMin="20.0">20–1600 m</quantity>
