= 
<quantity box="[1082,1180,980,1005]" metricMagnitude="-1" metricUnit="m" metricValue="9.271" metricValueMax="10.668" metricValueMin="7.874" pageId="4" pageNumber="529" unit="in" value="36.5" valueMax="42.0" valueMin="31.0">31–42 in</quantity>
males; 
<emphasis box="[1266,1281,981,1004]" italics="true" pageId="4" pageNumber="529">L</emphasis>
