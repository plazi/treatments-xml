<taxonomicName authority="(Richardson, 1848)" authorityName="Richardson" authorityYear="1848" baseAuthorityName="Richardson" baseAuthorityYear="1848" box="[502,974,399,424]" class="Actinopterygii" family="Synodontidae" genus="Saurida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Aulopiformes" pageId="10" pageNumber="11" phylum="Chordata" rank="species" species="undosquamis">
