<number box="[594,624,1974,1998]" pageId="1" pageNumber="57" value="10.0">10</number>
, 
<number box="[638,781,1974,1998]" pageId="1" pageNumber="57" value="0.0475" valueMax="0.05" valueMin="0.045">0.045–0.050</number>
; seta 
<number box="[846,875,1974,1998]" pageId="1" pageNumber="57" value="11.0">11</number>
, 
<number box="[888,952,1974,1998]" pageId="1" pageNumber="57" value="0.095">0.095</number>
–0.100; ratio 
<number box="[1100,1127,1974,1998]" pageId="1" pageNumber="57" value="10.0">10</number>
