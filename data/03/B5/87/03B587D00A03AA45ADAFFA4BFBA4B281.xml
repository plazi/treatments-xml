<number box="[1137,1161,1974,1998]" pageId="1" pageNumber="57" value="11.0">11</number>
, 
<number box="[1174,1289,1974,1998]" pageId="1" pageNumber="57" value="0.475" valueMax="0.5" valueMin="0.45">0.45–0.50</number>
. Gula with 
<number box="[1422,1436,1974,1998]" pageId="1" pageNumber="57" value="4.0">4</number>
