</taxonomicName>
<taxonomicNameLabel box="[827,922,952,978]" pageId="55" pageNumber="428" rank="species">sp. nov.</taxonomicNameLabel>
differ from 
<taxonomicName authority="Brady, 1880 sensu Puri and Hulings" authorityName="Brady, 1880 sensu Puri and Hulings" class="Ostracoda" family="Bairdiidae" genus="Bythocypris" kingdom="Animalia" order="Podocopida" pageId="55" pageNumber="428" phylum="Arthropoda" rank="species" species="reniformis">
<emphasis box="[1076,1348,952,977]" italics="true" pageId="55" pageNumber="428">Bythocypris reniformis</emphasis>
<bibRefCitation author="Brady" pageId="55" pageNumber="443" refString="Brady, G. S. (1880) Report on the Ostracoda dredged by H. M. S. Challenger during the Years 1873 - 1876. Report on the scientific results of the voyage of H. M. S. Challenger. Zoology, 1 (3), 1 - 184." type="journal article" year="1880">
Brady, 
<number box="[151,210,992,1018]" pageId="55" pageNumber="428" value="1880.0">1880</number>
</bibRefCitation>
<emphasis box="[217,282,992,1017]" italics="true" pageId="55" pageNumber="428">sensu</emphasis>
<bibRefCitation author="Puri" box="[289,488,992,1018]" pageId="55" pageNumber="445" refString="Puri, H. S. &amp; Hulings, N. C. (1976) Designation of lectotypes of some ostracods from the Challenger expedition. Bulletin of the British Museum, 29 (5), 251 - 315." type="journal article" year="1976">Puri and Hulings</bibRefCitation>
</taxonomicName>
(
<number box="[504,568,992,1018]" pageId="55" pageNumber="428" value="1976.0">1976</number>
: Pl. 
<number box="[619,703,992,1018]" pageId="55" pageNumber="428" value="6.35" valueMax="10.0" valueMin="2.7">2.7–10</number>
, 
<figureCitation box="[714,781,992,1018]" captionStart="FIGURE 1" captionStartId="3.[151,255,1676,1700]" captionTargetBox="[165,1421,636,1611]" captionTargetId="figure@3.[151,1436,594,1653]" captionTargetPageId="3" captionText="FIGURE 1. Stations of the ANDEEP cruises containing Bairdioidea. Legend: Stations of the ANDEEP I cruise; Stations of the ANDEEP II cruise; Stations of the ANDEEP III cruise. For details on stations see Table 1." httpUri="https://zenodo.org/record/183821/files/figure.png" pageId="55" pageNumber="428">
