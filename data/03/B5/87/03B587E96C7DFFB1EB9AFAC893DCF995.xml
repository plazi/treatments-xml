<number box="[768,781,992,1018]" pageId="55" pageNumber="428" value="1.0">1</number>
</figureCitation>
) and 
<taxonomicName box="[848,993,992,1017]" class="Ostracoda" family="Bairdiidae" genus="Bythocypris" kingdom="Animalia" order="Podocopida" pageId="55" pageNumber="428" phylum="Arthropoda" rank="species" species="praerenis" status="sp. nov.">
