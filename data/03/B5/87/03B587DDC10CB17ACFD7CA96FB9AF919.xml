species.
</emphasis>
<taxonomicName authority="Werner, 1909" authorityName="Werner" authorityYear="1909" box="[313,754,222,247]" class="Reptilia" family="Typhlopidae" genus="Typhlops" kingdom="Animalia" order="Squamata" pageId="54" pageNumber="55" phylum="Chordata" rank="species" species="microcephalus">
