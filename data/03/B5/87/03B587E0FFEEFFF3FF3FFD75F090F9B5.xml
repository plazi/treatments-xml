material.
</emphasis>
<typeStatus box="[346,497,1698,1722]" pageId="6" pageNumber="41">HOLOTYPE</typeStatus>
, apt 
<number box="[564,585,1699,1723]" pageId="6" pageNumber="41" value="3.0">3</number>
, allotype apt Ƥ: 
<collectingCountry box="[804,956,1698,1722]" name="Colombia" pageId="6" pageNumber="41">COLOMBIA</collectingCountry>
, Nariño, Altaquer, Reserva Río Ñambí, 
<quantity box="[151,308,1734,1758]" metricMagnitude="3" metricUnit="m" metricValue="1.5" metricValueMax="1.9" metricValueMin="1.1" pageId="6" pageNumber="41" unit="m" value="1500.0" valueMax="1900.0" valueMin="1100.0">1100−1900 m</quantity>
