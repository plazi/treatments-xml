<paragraph blockId="11.[151,659,728,781]" pageId="11" pageNumber="210">
<taxonomicName box="[151,448,728,750]" class="Malacostraca" family="Lophogastridae" genus="Gnathophausia" kingdom="Animalia" order="Lophogastrida" pageId="11" pageNumber="210" phylum="Arthropoda" rank="subSpecies" species="elegans" subSpecies="fagei">
