<number box="[628,656,728,749]" pageId="11" pageNumber="210" value="88.0">88</number>
. 
<taxonomicName authority="Casanova, 1996a: 129" authorityName="Casanova" authorityPageNumber="129" authorityYear="1996" box="[151,595,758,781]" class="Malacostraca" family="Lophogastridae" genus="Gnathophausia" kingdom="Animalia" order="Lophogastrida" pageId="11" pageNumber="210" phylum="Arthropoda" rank="species" species="fagei">
