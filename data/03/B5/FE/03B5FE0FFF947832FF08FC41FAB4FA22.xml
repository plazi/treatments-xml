<paragraph blockId="4.[151,513,1069,1092]" box="[151,513,1069,1092]" pageId="4" pageNumber="5">
<typeStatus box="[151,204,1069,1092]" pageId="4" pageNumber="5">Type</typeStatus>
species: 
<taxonomicName authority="Stal." authorityName="Stal." box="[301,513,1069,1092]" class="Insecta" family="Cicadellidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="5" phylum="Arthropoda" rank="species" species="alacris">
