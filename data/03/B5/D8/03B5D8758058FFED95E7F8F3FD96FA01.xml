<subSubSection pageId="5" pageNumber="6" type="diagnosis">
<paragraph blockId="5.[189,310,267,293]" box="[189,310,267,293]" pageId="5" pageNumber="6">
<heading bold="true" box="[189,310,267,293]" fontSize="11" level="3" pageId="5" pageNumber="6" reason="3">
