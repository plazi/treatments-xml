<heading bold="true" box="[151,533,591,617]" fontSize="11" level="1" pageId="31" pageNumber="32" reason="1">
<emphasis bold="true" box="[151,533,591,617]" pageId="31" pageNumber="32">
<taxonomicName box="[151,459,591,617]" class="Branchiopoda" family="Holopediidae" genus="Holopedium" kingdom="Animalia" order="Diplostraca" pageId="31" pageNumber="32" phylum="Arthropoda" rank="species" species="acidophilum" status="n. sp.">
<emphasis bold="true" box="[151,459,591,617]" italics="true" pageId="31" pageNumber="32">Holopedium acidophilum</emphasis>
</taxonomicName>
