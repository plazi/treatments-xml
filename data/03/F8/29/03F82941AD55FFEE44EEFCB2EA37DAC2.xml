<number box="[1110,1181,994,1018]" pageId="24" pageNumber="25" value="43190.0">43190</number>
, “ 
<collectingCountry box="[1202,1308,994,1018]" name="Cameroon" pageId="24" pageNumber="25">Kamerun</collectingCountry>
, Manoka b. Duala, Bach, 
<date box="[297,418,1030,1054]" pageId="24" pageNumber="25" value="1911-11-11">
