<number box="[887,916,1696,1720]" pageId="4" pageNumber="55" value="29.0">29</number>
° 
<number box="[925,954,1696,1720]" pageId="4" pageNumber="55" value="32.0">32</number>
'54.83&quot;N
</geoCoordinate>
<geoCoordinate box="[1065,1222,1696,1720]" direction="west" orientation="longitude" pageId="4" pageNumber="55" precision="1" value="-8.880369">
<number box="[1065,1078,1696,1720]" pageId="4" pageNumber="55" value="8.0">8</number>
