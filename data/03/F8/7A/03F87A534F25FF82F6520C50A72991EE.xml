. 
<collectingCountry box="[284,369,987,1011]" name="Yemen" pageId="7" pageNumber="344">Socotra</collectingCountry>
. ................................................... 
<emphasis bold="true" box="[732,1089,986,1011]" pageId="7" pageNumber="344">
<taxonomicName authority="Švihla, 2012" authorityName="Švihla" authorityYear="2012" box="[732,996,986,1011]" class="Insecta" family="Oedemeridae" genus="Anisochrodes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="344" phylum="Arthropoda" rank="species" species="janavlada" status="sp. nov.">
