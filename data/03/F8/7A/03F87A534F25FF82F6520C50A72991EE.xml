. 
<collectingCountry box="[659,743,859,883]" name="Yemen" pageId="7" pageNumber="344">Socotra</collectingCountry>
, 
<collectingCountry box="[753,811,859,883]" name="United Arab Emirates" pageId="7" pageNumber="344">UAE</collectingCountry>
, S 
<collectingCountry box="[843,937,859,883]" name="Pakistan" pageId="7" pageNumber="344">Pakistan</collectingCountry>
