<paragraph blockId="27.[151,1436,1559,1621]" pageId="27" pageNumber="28">
<emphasis bold="true" box="[199,330,1559,1584]" pageId="27" pageNumber="28">Bionomics.</emphasis>
<taxonomicName box="[338,497,1560,1584]" class="Insecta" family="Formicidae" genus="Pachycondyla" kingdom="Animalia" order="Hymenoptera" pageId="27" pageNumber="28" phylum="Arthropoda" rank="genus">
<emphasis box="[338,497,1560,1584]" italics="true" pageId="27" pageNumber="28">Pachycondyla</emphasis>
</taxonomicName>
