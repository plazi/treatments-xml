<taxonomicName authorityName="Ermilov &amp; Liao" authorityYear="2020" box="[893,967,1119,1143]" class="Arachnida" family="Protoribatidae" genus="Perxylobates" kingdom="Animalia" order="Sarcoptiformes" pageId="7" pageNumber="618" phylum="Arthropoda" rank="species" species="hakkai">
