<collectingCountry box="[754,833,405,428]" name="France" pageId="81" pageNumber="82">France</collectingCountry>
, 
<collectingCountry box="[850,949,404,429]" name="Belgium" pageId="81" pageNumber="82">Belgium</collectingCountry>
