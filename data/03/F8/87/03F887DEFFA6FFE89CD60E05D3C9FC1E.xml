<collectingCountry box="[967,1031,404,429]" name="Spain" pageId="81" pageNumber="82">Spain</collectingCountry>
and 
<collectingCountry box="[1099,1197,404,429]" name="Portugal" pageId="81" pageNumber="82">Portugal</collectingCountry>
