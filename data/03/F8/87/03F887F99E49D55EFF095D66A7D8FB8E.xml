<subSubSection pageId="10" pageNumber="520" type="etymology">
<paragraph blockId="10.[151,1436,151,1040]" pageId="10" pageNumber="520">
<emphasis bold="true" box="[199,334,944,968]" pageId="10" pageNumber="520">Etymology.</emphasis>
