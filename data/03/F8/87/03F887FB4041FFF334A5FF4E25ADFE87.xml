<paragraph blockId="4.[159,1423,1480,1708]" lastBlockId="6.[151,1436,151,356]" lastPageId="6" lastPageNumber="42" pageId="4" pageNumber="40">
<taxonomicName box="[159,255,1686,1708]" class="Insecta" family="Fergusoninidae" genus="Fergusonina" kingdom="Animalia" order="Diptera" pageId="4" pageNumber="40" phylum="Arthropoda" rank="species" species="thornhilli">
<emphasis box="[159,255,1686,1708]" italics="true" pageId="4" pageNumber="40">thornhilli</emphasis>
</taxonomicName>
n/a 
<number box="[880,893,1687,1708]" pageId="4" pageNumber="40" value="7.0">7</number>
<number box="[928,973,1686,1707]" pageId="4" pageNumber="40" value="2.4">2.40</number>
