<taxonomicName authorityName="Zanclea Cocco" authorityYear="1832" box="[1228,1345,213,238]" class="Malacostraca" family="Portunidae" genus="Charybdis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="204" phylum="Arthropoda" rank="genus">
