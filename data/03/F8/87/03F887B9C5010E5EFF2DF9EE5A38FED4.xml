<taxonomicName baseAuthorityName="A. Milne-Edwards" baseAuthorityYear="1867" box="[884,1088,285,310]" class="Malacostraca" family="Portunidae" genus="Charybdis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="204" phylum="Arthropoda" rank="species" species="hellerii">
