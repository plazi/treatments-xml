<subSubSection box="[151,1429,1196,1221]" pageId="99" pageNumber="100" type="materials_examined">
<paragraph blockId="99.[151,1437,1196,2013]" box="[151,1429,1196,1221]" pageId="99" pageNumber="100">
<emphasis bold="true" box="[151,384,1196,1221]" pageId="99" pageNumber="100">Material examined.</emphasis>
HCUS-S 0 
<number box="[511,539,1196,1220]" pageId="99" pageNumber="100" value="77.0">77</number>
