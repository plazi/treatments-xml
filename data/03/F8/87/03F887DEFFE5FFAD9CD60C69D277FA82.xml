(De Vito 
<emphasis box="[1006,1064,1124,1149]" italics="true" pageId="20" pageNumber="21">et al.</emphasis>
<number box="[1073,1133,1124,1149]" pageId="20" pageNumber="21" value="2006.0">2006</number>
)
</taxonomicName>
, and 
<taxonomicName authority="Piraino et al. 2004" authorityName="Piraino et al." authorityYear="2004" class="Hydrozoa" family="Cytaeididae" genus="Perarella" kingdom="Animalia" order="Anthoathecata" pageId="20" pageNumber="21" phylum="Cnidaria" rank="species" species="schneideri">
