<paragraph blockId="33.[151,1436,1559,1621]" pageId="33" pageNumber="34">
<emphasis bold="true" box="[199,330,1559,1584]" pageId="33" pageNumber="34">Bionomics.</emphasis>
<taxonomicName box="[338,478,1561,1584]" class="Insecta" family="Formicidae" genus="Hypoponera" kingdom="Animalia" order="Hymenoptera" pageId="33" pageNumber="34" phylum="Arthropoda" rank="genus">
<emphasis box="[338,478,1561,1584]" italics="true" pageId="33" pageNumber="34">Hypoponera</emphasis>
