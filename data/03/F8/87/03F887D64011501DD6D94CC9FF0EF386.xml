<taxonomicName authority="Metcalf, 1963 e: 201" authorityName="Metcalf" authorityPageNumber="201" authorityYear="1963" box="[812,1396,151,177]" class="Insecta" family="Cicadidae" genus="Diceroprocta" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="30" phylum="Arthropoda" rank="variety" species="swalei" variety="davisi">
