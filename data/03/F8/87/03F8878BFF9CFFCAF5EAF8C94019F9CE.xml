Emery, 
<number box="[450,504,944,966]" pageId="30" pageNumber="31" value="1889.0">1889</number>
</taxonomicName>
. 
<typeStatus box="[513,563,945,966]" pageId="30" pageNumber="31">Type</typeStatus>
locality: 
<collectingCountry box="[659,762,944,966]" name="Vietnam" pageId="30" pageNumber="31">Cocincina</collectingCountry>
[southern 
<collectingCountry box="[870,962,944,966]" name="Vietnam" pageId="30" pageNumber="31">Vietnam</collectingCountry>
