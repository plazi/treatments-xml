<bibRefCitation author="Bonnevie" box="[738,922,871,896]" pageId="9" pageNumber="10" refString="Bonnevie, K. (1901) Hydroiden. Meeresfauna von Bergen. 1, 1 - 15. http: // dx. doi. org / 10.5962 / bhl. title. 10212" type="journal article" year="1901">
Bonnevie 
<number box="[863,922,872,896]" pageId="9" pageNumber="10" value="1901.0">1901</number>
</bibRefCitation>
; 
<bibRefCitation author="Vervoort" box="[943,1116,871,896]" pageId="9" pageNumber="10" refString="Vervoort, W. (1946) Hydrozoa (C 1) A. Hydropolypen. Fauna van Nederland, 14, 1 - 336." type="journal article" year="1946">
Vervoort 
<number box="[1057,1116,871,896]" pageId="9" pageNumber="10" value="1946.0">1946</number>
</bibRefCitation>
; 
