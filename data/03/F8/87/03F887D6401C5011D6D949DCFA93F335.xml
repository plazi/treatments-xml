<subSubSection pageId="9" pageNumber="34" type="etymology">
<paragraph blockId="9.[151,1436,439,538]" box="[151,865,439,466]" pageId="9" pageNumber="34">
<emphasis bold="true" box="[151,290,439,465]" pageId="9" pageNumber="34">Etymology:</emphasis>
