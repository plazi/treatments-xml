; 
<emphasis bold="true" box="[1138,1151,1431,1452]" pageId="11" pageNumber="412">5</emphasis>
, 
<taxonomicName authority="Kolencik &amp; Sychra &amp; Papousek &amp; Kuabara &amp; Valim &amp; Literak, 2018" authorityName="Kolencik &amp; Sychra &amp; Papousek &amp; Kuabara &amp; Valim &amp; Literak" authorityYear="2018" box="[1163,1347,1430,1452]" class="Insecta" family="Menoponidae" genus="Myrsidea" higherTaxonomySource="GBIF" kingdom="Animalia" order="Phthiraptera" pageId="11" pageNumber="412" phylum="Arthropoda" rank="species" species="zuzanae" status="n. sp.">
