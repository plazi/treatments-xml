<paragraph blockId="29.[151,1436,1702,1763]" pageId="29" pageNumber="30">
<emphasis bold="true" box="[199,330,1702,1727]" pageId="29" pageNumber="30">Bionomics.</emphasis>
<taxonomicName box="[342,474,1703,1726]" class="Insecta" family="Formicidae" genus="Cryptopone" kingdom="Animalia" order="Hymenoptera" pageId="29" pageNumber="30" phylum="Arthropoda" rank="genus">
<emphasis box="[342,474,1703,1726]" italics="true" pageId="29" pageNumber="30">Cryptopone</emphasis>
</taxonomicName>
