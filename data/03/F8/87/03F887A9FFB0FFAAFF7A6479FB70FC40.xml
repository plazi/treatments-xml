(mean 
<quantity box="[573,676,188,212]" metricMagnitude="-4" metricUnit="m" metricValue="2.9" pageId="8" pageNumber="419" unit="mm" value="0.29">0.29 mm</quantity>
), AnL 
<quantity box="[753,856,188,212]" metricMagnitude="-4" metricUnit="m" metricValue="5.0" pageId="8" pageNumber="419" unit="mm" value="0.5">0.50 mm</quantity>
; PL 
<quantity box="[906,1069,188,212]" metricMagnitude="-4" metricUnit="m" metricValue="3.7" metricValueMax="3.8" metricValueMin="3.5999999999999996" pageId="8" pageNumber="419" unit="mm" value="0.37" valueMax="0.38" valueMin="0.36">0.36–0.38 mm</quantity>
