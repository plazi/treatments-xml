(mean 
<quantity box="[278,382,223,247]" metricMagnitude="-4" metricUnit="m" metricValue="5.1" pageId="8" pageNumber="419" unit="mm" value="0.51">0.51 mm</quantity>
); EL 
<quantity box="[443,606,223,247]" metricMagnitude="-4" metricUnit="m" metricValue="6.65" metricValueMax="6.8" metricValueMin="6.5" pageId="8" pageNumber="419" unit="mm" value="0.665" valueMax="0.68" valueMin="0.65">0.65–0.68 mm</quantity>
