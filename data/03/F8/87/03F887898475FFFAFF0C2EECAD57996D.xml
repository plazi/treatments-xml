<quantity box="[510,698,1688,1715]" metricMagnitude="-3" metricUnit="m" metricValue="1.095" metricValueMax="1.15" metricValueMin="1.04" pageId="28" pageNumber="479" unit="mm" value="1.095" valueMax="1.15" valueMin="1.04">1.04 to 1.15 mm</quantity>
