<taxonomicName authority="Heer, 1853" authorityName="Heer" authorityYear="1853" box="[220,549,1374,1401]" class="Insecta" family="Cicadellidae" genus="Cicada" kingdom="Animalia" order="Hemiptera" pageId="4" pageNumber="29" phylum="Arthropoda" rank="species" species="bifasciata">
