<subSubSection pageId="9" pageNumber="34" type="etymology">
<paragraph blockId="9.[151,1436,1724,1823]" box="[151,691,1724,1751]" pageId="9" pageNumber="34">
<emphasis bold="true" box="[151,290,1725,1751]" pageId="9" pageNumber="34">Etymology:</emphasis>
Latin adjective 
<taxonomicName authorityName="Dmitriev" authorityYear="2020" box="[471,541,1725,1751]" class="Insecta" family="Cicadellidae" genus="Thamnotettix" kingdom="Animalia" order="Hemiptera" pageId="9" pageNumber="34" phylum="Arthropoda" rank="species" species="fuscus">
