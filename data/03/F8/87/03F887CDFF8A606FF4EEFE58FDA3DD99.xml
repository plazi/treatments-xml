<bibRefCitation author="Leech" box="[760,901,801,826]" pageId="35" pageNumber="36" refString="Leech, J. H. (1900) Lepidoptera Heterocera from northern China, Japan and corea. Part 3. Transactions of the Royal Entomological Society of London, 1900, 9 - 161." type="journal article" year="1900">
Leech, 
<number box="[844,901,802,826]" pageId="35" pageNumber="36" value="1900.0">1900</number>
</bibRefCitation>
)
</taxonomicName>
(p. 
<number box="[954,997,802,826]" pageId="35" pageNumber="36" value="219.0">219</number>
) and 
<taxonomicName authority="Leech, 1900" authorityName="Leech" authorityYear="1900" box="[1060,1399,801,826]" class="Insecta" family="Drepanidae" genus="Tethea" kingdom="Animalia" order="Lepidoptera" pageId="35" pageNumber="36" phylum="Arthropoda" rank="species" species="brunnea">
