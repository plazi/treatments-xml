<taxonomicName authority="Peters 1854" authorityName="Peters" authorityYear="1854" box="[419,697,1638,1665]" class="Amphibia" family="Rhacophoridae" genus="Chiromantis" kingdom="Animalia" order="Anura" pageId="39" pageNumber="40" phylum="Chordata" rank="genus">
