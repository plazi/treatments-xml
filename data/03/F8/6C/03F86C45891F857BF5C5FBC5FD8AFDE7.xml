, and 
<taxonomicName authority="Okazaki, 2012" authorityName="Okazaki" authorityYear="2012" box="[463,604,569,590]" class="Mammalia" family="Eomysticetidae" genus="Yamatocetus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cetacea" pageId="6" pageNumber="613" phylum="Chordata" rank="genus">
