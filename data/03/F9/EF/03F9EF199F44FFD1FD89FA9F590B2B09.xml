(
<bibRefCitation author="Sato M." box="[1125,1253,973,999]" pageId="19" pageNumber="20" pagination="173 - 177" refId="ref14802" refString="Sato M. 1994. Note on the genus Pseudeucinetus Heller and its new relative (Coleoptera, Limnichidae). Special Bulletin of the Essa Entomological Society 2: 173 - 177." type="journal article" year="1994">Satô, 1994</bibRefCitation>
)
