<taxonomicName authority="(Newman, 1838)" authorityName="Capnor" baseAuthorityName="Newman" baseAuthorityYear="1838" box="[151,490,414,436]" class="Insecta" family="Capniidae" genus="Capnia" kingdom="Animalia" order="Plecoptera" pageId="55" pageNumber="56" phylum="Arthropoda" rank="species" species="bifrons">
