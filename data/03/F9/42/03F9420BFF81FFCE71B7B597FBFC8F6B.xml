<taxonomicName authority="Walker 1851" authorityName="Walker" authorityYear="1851" box="[151,560,1429,1455]" class="Insecta" family="Cicadellidae" genus="Aulacizes" kingdom="Animalia" order="Hemiptera" pageId="1" pageNumber="212" phylum="Arthropoda" rank="species" species="conspersa">
