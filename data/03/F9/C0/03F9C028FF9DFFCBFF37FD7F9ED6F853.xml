<number box="[600,615,1016,1042]" pageId="9" pageNumber="48" value="2.0">2</number>
(
<number box="[634,679,1016,1042]" pageId="9" pageNumber="48" value="1.5" valueMax="2.0" valueMin="1.0">1–2</number>
