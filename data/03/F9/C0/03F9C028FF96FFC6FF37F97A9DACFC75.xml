<number box="[1406,1435,1866,1892]" pageId="2" pageNumber="41" value="10.0">10</number>
(
<number box="[160,237,1906,1932]" pageId="2" pageNumber="41" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
), gnathosoma projecting downwards. 
<emphasis bold="true" box="[689,894,1906,1932]" pageId="2" pageNumber="41">Prodorsal shield</emphasis>
<number box="[904,933,1906,1932]" pageId="2" pageNumber="41" value="35.0">35</number>
