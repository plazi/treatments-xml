<number box="[1213,1228,192,218]" pageId="3" pageNumber="42" value="7.0">7</number>
(
<number box="[1249,1296,192,218]" pageId="3" pageNumber="42" value="7.5" valueMax="8.0" valueMin="7.0">7–8</number>
