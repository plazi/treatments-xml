<number box="[464,493,1866,1892]" pageId="2" pageNumber="41" value="74.0">74</number>
(
<number box="[511,589,1866,1892]" pageId="2" pageNumber="41" value="73.5" valueMax="75.0" valueMin="72.0">72–75</number>
), pedipalp coxal setae (
<emphasis box="[870,898,1867,1892]" italics="true" pageId="2" pageNumber="41">ep</emphasis>
