<number box="[741,785,1826,1852]" pageId="2" pageNumber="41" value="396.0">396</number>
(
<number box="[802,910,1826,1852]" pageId="2" pageNumber="41" value="394.0" valueMax="408.0" valueMin="380.0">380–408</number>
