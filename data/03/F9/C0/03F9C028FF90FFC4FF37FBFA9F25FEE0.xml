), femur 
<number box="[151,179,1586,1612]" pageId="4" pageNumber="43" value="11.0">11</number>
(
<number box="[195,273,1586,1612]" pageId="4" pageNumber="43" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
