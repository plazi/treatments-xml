<number box="[926,970,1848,1874]" pageId="10" pageNumber="49" value="100.0">100</number>
(
<number box="[988,1093,1848,1874]" pageId="10" pageNumber="49" value="102.5" valueMax="105.0" valueMin="100.0">100–105</number>
) wide, 
<number box="[1185,1229,1848,1874]" pageId="10" pageNumber="49" value="105.0">105</number>
(
<number box="[1247,1351,1848,1874]" pageId="10" pageNumber="49" value="106.5" valueMax="113.0" valueMin="100.0">100–113</number>
) thick; light yellow. Gnathosoma 
<number box="[464,493,1888,1914]" pageId="10" pageNumber="49" value="70.0">70</number>
(
<number box="[511,589,1888,1914]" pageId="10" pageNumber="49" value="71.0" valueMax="75.0" valueMin="67.0">67–75</number>
), pedipalp coxal setae (
<emphasis box="[870,898,1889,1914]" italics="true" pageId="10" pageNumber="49">ep</emphasis>
) 
<number box="[916,931,1888,1914]" pageId="10" pageNumber="49" value="2.0">2</number>
(
<number box="[949,996,1888,1914]" pageId="10" pageNumber="49" value="2.5" valueMax="3.0" valueMin="2.0">2–3</number>
), dorsal pedipalp genual setae (
<emphasis box="[1373,1387,1889,1914]" italics="true" pageId="10" pageNumber="49">d</emphasis>
) 
<number box="[1406,1435,1888,1914]" pageId="10" pageNumber="49" value="15.0">15</number>
(
<number box="[160,237,1928,1954]" pageId="10" pageNumber="49" value="14.0" valueMax="15.0" valueMin="13.0">13–15</number>
), gnathosoma projecting downwards. 
<emphasis bold="true" box="[683,887,1928,1954]" pageId="10" pageNumber="49">Prodorsal shield</emphasis>
<number box="[894,923,1928,1954]" pageId="10" pageNumber="49" value="43.0">43</number>
(
<number box="[940,1018,1928,1954]" pageId="10" pageNumber="49" value="44.0" valueMax="45.0" valueMin="43.0">43–45</number>
