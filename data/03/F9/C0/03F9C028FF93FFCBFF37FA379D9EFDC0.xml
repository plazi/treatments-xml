<number box="[1298,1327,1661,1687]" pageId="7" pageNumber="46" value="10.0">10</number>
(
<number box="[1344,1422,1661,1687]" pageId="7" pageNumber="46" value="11.0" valueMax="12.0" valueMin="10.0">10–12</number>
