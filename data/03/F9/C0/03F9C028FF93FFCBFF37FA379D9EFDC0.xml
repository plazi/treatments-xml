<number box="[1121,1150,1621,1647]" pageId="7" pageNumber="46" value="60.0">60</number>
(
<number box="[1166,1242,1621,1647]" pageId="7" pageNumber="46" value="60.0" valueMax="65.0" valueMin="55.0">55–65</number>
