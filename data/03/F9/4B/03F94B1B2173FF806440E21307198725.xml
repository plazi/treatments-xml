<paragraph blockId="11.[151,760,917,1068]" pageId="11" pageNumber="162">
<emphasis bold="true" box="[151,222,917,940]" pageId="11" pageNumber="162">Notes.</emphasis>
Unlike the 
<typeStatus box="[351,407,917,940]" pageId="11" pageNumber="162">types</typeStatus>
from 
<collectingRegion box="[475,645,917,940]" country="Indonesia" name="Sulawesi Selatan" pageId="11" pageNumber="162">South Sulawesi</collectingRegion>
