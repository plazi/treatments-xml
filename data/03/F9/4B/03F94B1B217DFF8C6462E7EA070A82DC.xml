<subSubSection pageId="7" pageNumber="158" type="etymology">
<paragraph blockId="7.[151,761,190,469]" pageId="7" pageNumber="158">
<emphasis bold="true" box="[151,280,382,405]" pageId="7" pageNumber="158">Etymology.</emphasis>
