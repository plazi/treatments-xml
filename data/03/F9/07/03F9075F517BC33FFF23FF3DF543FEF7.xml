<subSubSection pageId="12" pageNumber="13" type="distribution">
<paragraph blockId="12.[151,1436,151,320]" box="[151,1423,187,212]" pageId="12" pageNumber="13">
<emphasis bold="true" box="[151,527,187,212]" pageId="12" pageNumber="13">
