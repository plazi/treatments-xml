<taxonomicName authority="Aiton" authorityName="Aiton" box="[1029,1381,332,356]" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" kingdom="Plantae" order="Ericales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="angustifolium">
<emphasis box="[1029,1307,332,356]" italics="true" pageId="16" pageNumber="17">Vaccinium angustifolium</emphasis>
Aiton
</taxonomicName>
, 
<taxonomicName class="Magnoliopsida" family="Ericaceae" kingdom="Plantae" order="Ericales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="family">Ericaceae</taxonomicName>
) and highbush blueberry (
<taxonomicName box="[527,795,368,392]" class="Magnoliopsida" family="Ericaceae" genus="Vaccinium" kingdom="Plantae" order="Ericales" pageId="16" pageNumber="17" phylum="Tracheophyta" rank="species" species="corymbosum">
