<taxonomicName authority="(Thunberg)" baseAuthorityName="Thunberg" baseAuthorityYear="1822" box="[783,1074,1900,1924]" class="Insecta" family="Braconidae" genus="Taphaeus" kingdom="Animalia" order="Hymenoptera" pageId="8" pageNumber="579" phylum="Arthropoda" rank="species" species="hiator">
