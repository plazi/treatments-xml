<subSubSection lastPageId="32" lastPageNumber="249" pageId="30" pageNumber="247" type="etymology">
<paragraph blockId="30.[839,1467,1386,1439]" pageId="30" pageNumber="247">
<emphasis bold="true" box="[839,962,1386,1410]" pageId="30" pageNumber="247">Etymology</emphasis>
