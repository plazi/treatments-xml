<subSubSection box="[199,917,187,213]" pageId="10" pageNumber="237" type="etymology">
<paragraph blockId="10.[151,1437,151,537]" box="[199,917,187,213]" pageId="10" pageNumber="237">
<emphasis bold="true" box="[199,334,187,213]" pageId="10" pageNumber="237">Etymology.</emphasis>
