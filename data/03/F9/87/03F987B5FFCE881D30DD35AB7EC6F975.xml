. 
<taxonomicName authorityName="Burckhardt &amp; Queiroz" authorityYear="2020" box="[342,597,1593,1619]" class="Insecta" family="Psyllidae" genus="Platycorypha" kingdom="Animalia" order="Hemiptera" pageId="58" pageNumber="59" phylum="Arthropoda" rank="species" species="atrifrons">
