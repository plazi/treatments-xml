. (
<quantity box="[571,695,223,247]" metricMagnitude="2" metricUnit="m" metricValue="5.82" metricValueMax="6.459999999999999" metricValueMin="5.18" pageId="24" pageNumber="25" unit="m" value="582.0" valueMax="646.0" valueMin="518.0">518–646m</quantity>
