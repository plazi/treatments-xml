<collectingCountry box="[872,970,1459,1483]" name="Thailand" pageId="6" pageNumber="285">Thailand</collectingCountry>
, 
<collectingCountry box="[983,1087,1460,1484]" name="Myanmar" pageId="6" pageNumber="285">Myanmar</collectingCountry>
