<taxonomicName authority="Mart." authorityName="Mart." box="[591,923,1413,1439]" class="Magnoliopsida" family="Fabaceae" genus="Copaifera" kingdom="Plantae" order="Fabales" pageId="42" pageNumber="43" phylum="Tracheophyta" rank="species" species="oblongifolia">
