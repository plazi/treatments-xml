<quantity box="[572,715,958,982]" metricMagnitude="2" metricUnit="m" metricValue="8.145" metricValueMax="10.129999999999999" metricValueMin="6.16" pageId="24" pageNumber="25" unit="m" value="814.5" valueMax="1013.0" valueMin="616.0">616–1013 m</quantity>
