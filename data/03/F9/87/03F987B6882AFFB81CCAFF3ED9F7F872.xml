Body length: 
<quantity box="[542,709,654,679]" metricMagnitude="-2" metricUnit="m" metricValue="2.435" metricValueMax="2.68" metricValueMin="2.19" pageId="19" pageNumber="20" unit="mm" value="24.35" valueMax="26.8" valueMin="21.9">21.9â€“26.8 mm</quantity>
. CL: 0.92; EL: 3.48; 
<collectionCode box="[949,1003,655,679]" country="United Kingdom" httpUri="http://biocol.org/urn:lsid:biocol.org:col:12573" name="Aldenham School" pageId="19" pageNumber="20">ELS</collectionCode>
