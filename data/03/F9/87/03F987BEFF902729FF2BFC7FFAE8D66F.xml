<taxonomicName authority="Amyot &amp; Serville" authorityName="Amyot &amp; Serville" authorityYear="1843" box="[151,521,945,972]" class="Insecta" family="Phlaeothripidae" genus="Hoplothrips" kingdom="Animalia" order="Thysanoptera" pageId="1" pageNumber="302" phylum="Arthropoda" rank="genus">
