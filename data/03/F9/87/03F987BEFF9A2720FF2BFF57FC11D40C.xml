<geoCoordinate box="[628,694,873,899]" degrees="1" direction="south" minutes="27" orientation="latitude" pageId="11" pageNumber="312" precision="925" value="-1.45">S1 27</geoCoordinate>
, 
<geoCoordinate box="[706,773,873,899]" degrees="2" direction="south" minutes="27" orientation="latitude" pageId="11" pageNumber="312" precision="925" value="-2.45">S2 27</geoCoordinate>
, 
<geoCoordinate box="[785,853,873,899]" degrees="3" direction="south" minutes="20" orientation="latitude" pageId="11" pageNumber="312" precision="925" value="-3.3333333">S3 20</geoCoordinate>
