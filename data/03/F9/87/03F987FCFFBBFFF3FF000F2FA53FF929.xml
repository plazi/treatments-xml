° 
<number box="[821,851,1186,1212]" pageId="12" pageNumber="63" value="15.0">15</number>
'S
</geoCoordinate>
, 
<geoCoordinate box="[884,989,1186,1212]" direction="west" orientation="longitude" pageId="12" pageNumber="63" precision="925" value="-66.416664">
<number box="[884,915,1186,1212]" pageId="12" pageNumber="63" value="66.0">66</number>
° 
