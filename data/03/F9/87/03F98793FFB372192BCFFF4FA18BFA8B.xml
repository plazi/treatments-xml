<quantity box="[342,534,1706,1732]" metricMagnitude="2" metricUnit="m" metricValue="1.754082" pageId="39" pageNumber="40" unit="mm" value="175408.2">175408, 20 mm</quantity>
preserved), 
<date box="[686,741,1706,1732]" pageId="39" pageNumber="40" value="1935-10">
<number box="[686,715,1706,1732]" pageId="39" pageNumber="40" value="35.0">35</number>
