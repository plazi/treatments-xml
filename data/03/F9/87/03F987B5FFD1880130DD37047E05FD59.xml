<collectionCode box="[1107,1182,1294,1320]" country="Brazil" httpUri="http://grbio.org/cool/9yp6-zxp9" name="Sao Paulo, Museu de Zoologia da Universidade de Sao Paulo" pageId="37" pageNumber="38" type="Museum">MZSP</collectionCode>
, 
<collectionCode box="[1193,1280,1295,1321]" country="0" httpUri="http://grbio.org/cool/1d5a-4efs" name="Natural History Museum Bucharest" pageId="37" pageNumber="38">NHMB</collectionCode>
