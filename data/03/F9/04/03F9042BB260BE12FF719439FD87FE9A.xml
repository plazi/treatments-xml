<subSubSection box="[160,933,142,203]" pageId="37" pageNumber="2642" type="etymology">
<paragraph blockId="37.[160,933,142,203]" box="[160,285,142,168]" pageId="37" pageNumber="2642">
<emphasis bold="true" box="[160,285,142,168]" italics="true" pageId="37" pageNumber="2642">Etymology</emphasis>
</paragraph>
<paragraph blockId="37.[160,933,142,203]" box="[160,933,178,203]" pageId="37" pageNumber="2642">
