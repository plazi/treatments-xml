<taxonomicName box="[429,707,1518,1542]" class="Malacostraca" family="Cambaridae" genus="Cambarus" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="ludovicianus">
<emphasis box="[429,707,1518,1542]" italics="true" pageId="34" pageNumber="135">Cambarus ludovicianus</emphasis>
</taxonomicName>
, 
<taxonomicName box="[733,925,1519,1542]" class="Malacostraca" family="Cambaridae" genus="Orconectes" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="nais">
<emphasis box="[733,925,1519,1542]" italics="true" pageId="34" pageNumber="135">Orconectes nais</emphasis>
</taxonomicName>
, 
<taxonomicName box="[950,1160,1518,1542]" class="Malacostraca" family="Cambaridae" genus="Orconectes" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="virilis">
<emphasis box="[950,1160,1518,1542]" italics="true" pageId="34" pageNumber="135">Orconectes virilis</emphasis>
</taxonomicName>
, 
<taxonomicName box="[1186,1429,1518,1542]" class="Malacostraca" family="Cambaridae" genus="Procambarus" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="clarkii">
<emphasis box="[1186,1429,1518,1542]" italics="true" pageId="34" pageNumber="135">Procambarus clarkii</emphasis>
</taxonomicName>
, 
<taxonomicName box="[151,394,1555,1579]" class="Malacostraca" family="Cambaridae" genus="Procambarus" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="gracilis">
<emphasis box="[151,394,1555,1579]" italics="true" pageId="34" pageNumber="135">Procambarus gracilis</emphasis>
</taxonomicName>
, and 
<taxonomicName box="[456,714,1555,1579]" class="Malacostraca" family="Cambaridae" genus="Procambarus" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="135" phylum="Arthropoda" rank="species" species="simulans">
<emphasis box="[456,714,1555,1579]" italics="true" pageId="34" pageNumber="135">Procambarus simulans</emphasis>
</taxonomicName>
.
</paragraph>
<paragraph blockId="34.[151,1436,582,1723]" pageId="34" pageNumber="135">
<emphasis bold="true" box="[199,451,1590,1615]" pageId="34" pageNumber="135">Conservation status.</emphasis>
