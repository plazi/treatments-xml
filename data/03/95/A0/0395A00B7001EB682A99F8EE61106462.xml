<collectingCountry box="[359,419,402,428]" name="United States of America" pageId="36" pageNumber="37">USA</collectingCountry>
: *MA, MI; 
<collectingCountry box="[552,640,402,428]" name="Canada" pageId="36" pageNumber="37">Canada</collectingCountry>
