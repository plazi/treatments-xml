, 
<taxonomicName authorityName="Linnaeus" authorityYear="1753" class="Magnoliopsida" family="Caprifoliaceae" genus="Lonicera" kingdom="Plantae" order="Dipsacales" pageId="19" pageNumber="20" phylum="Tracheophyta" rank="species" species="tatarica">
