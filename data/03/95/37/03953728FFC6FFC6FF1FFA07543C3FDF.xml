<caption httpUri="https://zenodo.org/record/183550/files/figure.png" pageId="4" pageNumber="59" targetBox="[199,1386,201,1789]" targetPageId="4">
<paragraph blockId="4.[151,1436,1814,1907]" pageId="4" pageNumber="59">
<emphasis bold="true" box="[151,336,1814,1838]" pageId="4" pageNumber="59">FIGURES 9–16.</emphasis>
<emphasis box="[343,517,1815,1838]" italics="true" pageId="4" pageNumber="59">Trigounuropoda</emphasis>
<taxonomicName box="[524,837,1815,1838]" class="Arachnida" family="Trigonuropodidae" genus="Trigonuropoda" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Mesostigmata" pageId="4" pageNumber="59" phylum="Arthropoda" rank="species" species="dominicana" status="sp. nov." subGenus="Baloghiatrigon">
(
<emphasis box="[532,695,1815,1838]" italics="true" pageId="4" pageNumber="59">Baloghiatrigon</emphasis>
) 
<emphasis box="[711,837,1815,1838]" italics="true" pageId="4" pageNumber="59">dominicana</emphasis>
</taxonomicName>
