. 
<emphasis box="[294,449,1445,1470]" italics="true" pageId="5" pageNumber="19">melanoptera</emphasis>
(Distant)
</taxonomicName>
, 
<taxonomicName authority="Distant" authorityName="Distant" authorityYear="2010" box="[584,853,1445,1471]" class="Insecta" family="Cicadidae" genus="Sonata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="19" phylum="Arthropoda" rank="species" species="obnubila">
<emphasis box="[584,605,1445,1470]" italics="true" pageId="5" pageNumber="19">O</emphasis>
. 
<emphasis box="[622,731,1445,1470]" italics="true" pageId="5" pageNumber="19">obnubila</emphasis>
(Distant)
</taxonomicName>
, 
<taxonomicName authority="Distant" authorityName="Distant" authorityYear="2010" box="[866,1113,1445,1471]" class="Insecta" family="Cicadidae" genus="Sonata" higherTaxonomySource="GBIF" kingdom="Animalia" order="Hemiptera" pageId="5" pageNumber="19" phylum="Arthropoda" rank="species" species="stratoria">
