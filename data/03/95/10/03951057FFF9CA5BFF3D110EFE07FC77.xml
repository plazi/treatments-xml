53. 
<taxonomicName authority="Takaoka &amp; Davies, 1995" authorityName="Takaoka &amp; Davies" authorityYear="1995" box="[192,870,260,285]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="48" pageNumber="49" phylum="Arthropoda" rank="species" species="malayense" subGenus="Simulium">
<emphasis bold="true" box="[192,306,260,285]" italics="true" pageId="48" pageNumber="49">Simulium</emphasis>
(
<emphasis bold="true" box="[323,437,260,285]" italics="true" pageId="48" pageNumber="49">Simulium</emphasis>
) 
<emphasis bold="true" box="[452,574,260,285]" italics="true" pageId="48" pageNumber="49">malayense</emphasis>
Takaoka &amp; Davies, 1995
</taxonomicName>
</emphasis>
</heading>
</paragraph>
