</elevation>
wide, 
<elevation metricMagnitude="3" metricUnit="m" metricValue="1.315" pageId="19" pageNumber="20" unit="m" value="1315.0">
<quantity metricMagnitude="3" metricUnit="m" metricValue="1.315" pageId="19" pageNumber="20" unit="m" value="1315.0">1,315 m</quantity>
</elevation>
in elevation) in 
<location LSID="urn:lsid:plazi:treatment:03951057FFDACA7AFF3D1605FD4DF8F7:8EE3F79AFFDACA7AFECE1695FD9FF96A" box="[356,523,1695,1720]" name="Simpang Pulai" pageId="19" pageNumber="20" stateProvince="Pahang">Simpang Pulai</location>
