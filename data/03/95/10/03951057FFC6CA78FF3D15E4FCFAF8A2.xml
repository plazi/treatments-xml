(
<emphasis bold="true" box="[323,493,1518,1543]" italics="true" pageId="15" pageNumber="16">Gomphostilbia</emphasis>
) 
<emphasis bold="true" box="[508,626,1518,1543]" italics="true" pageId="15" pageNumber="16">tahanense</emphasis>
