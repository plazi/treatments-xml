<taxonomicName authority="Takaoka &amp; Davies, 1995: 155" authorityName="Takaoka &amp; Davies" authorityPageNumber="155" authorityYear="1995" box="[151,826,188,213]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="46" pageNumber="47" phylum="Arthropoda" rank="species" species="rudnicki" subGenus="Simulium">
