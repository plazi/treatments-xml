<paragraph blockId="50.[151,1437,1220,1784]" pageId="50" pageNumber="51">
<taxonomicName box="[151,485,1292,1316]" class="Insecta" family="Simuliidae" genus="Simulium" higherTaxonomySource="GBIF" kingdom="Animalia" order="Diptera" pageId="50" pageNumber="51" phylum="Arthropoda" rank="species" species="vanluni" subGenus="Simulium">
