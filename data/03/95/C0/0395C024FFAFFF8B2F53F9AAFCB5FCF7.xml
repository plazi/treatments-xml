khankaensis
</emphasis>
</taxonomicName>
<emphasis bold="true" box="[848,914,1782,1806]" pageId="3" pageNumber="54">
<taxonomicNameLabel box="[848,914,1782,1806]" pageId="3" pageNumber="54" rank="species">sp. n.</taxonomicNameLabel>
</emphasis>
: 
<emphasis bold="true" box="[935,948,1782,1806]" pageId="3" pageNumber="54">4</emphasis>
, male genitalia, ventral view; 
<emphasis bold="true" box="[1302,1315,1782,1806]" pageId="3" pageNumber="54">5</emphasis>
, aedeagus, lateral view; 
<emphasis bold="true" box="[514,527,1817,1841]" pageId="3" pageNumber="54">6</emphasis>
, female genitalia; 
<emphasis bold="true" box="[726,739,1817,1841]" pageId="3" pageNumber="54">7</emphasis>
, 
<taxonomicName authority="Inoue" authorityName="Inoue" box="[753,993,1818,1841]" class="Insecta" family="Geometridae" genus="Scopula" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="vojnitsi">
<taxonomicName box="[753,924,1818,1841]" class="Insecta" family="Geometridae" genus="Scopula" higherTaxonomySource="CoL" kingdom="Animalia" order="Lepidoptera" pageId="3" pageNumber="54" phylum="Arthropoda" rank="species" species="vojnitsi">
