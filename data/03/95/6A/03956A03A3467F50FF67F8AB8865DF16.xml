<taxonomicName authority="Schmidt, 2017" authorityName="Schmidt" authorityYear="2017" box="[376,738,1139,1161]" class="Insecta" family="Carabidae" genus="Trichotichnus" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="346" phylum="Arthropoda" rank="species" species="martensi" status="sp. n." subGenus="Bottchrus">
<taxonomicName box="[376,516,1139,1161]" class="Insecta" family="Carabidae" genus="Trichotichnus" higherTaxonomySource="CoL" kingdom="Animalia" order="Coleoptera" pageId="45" pageNumber="346" phylum="Arthropoda" rank="genus">
