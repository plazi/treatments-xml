<subSubSection pageId="5" pageNumber="193" type="etymology">
<paragraph blockId="5.[151,1437,152,250]" pageId="5" pageNumber="193">
<emphasis bold="true" box="[199,334,189,215]" pageId="5" pageNumber="193">Etymology.</emphasis>
