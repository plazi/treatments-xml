<taxonomicName box="[1126,1263,1782,1808]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="douglasii">
<emphasis box="[1126,1263,1782,1808]" italics="true" pageId="30" pageNumber="31">Q. douglasii</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Burks" baseAuthorityYear="1979" box="[1276,1396,1782,1808]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="dumosa">
<emphasis box="[1276,1396,1782,1808]" italics="true" pageId="30" pageNumber="31">Q. dumosa</emphasis>
</taxonomicName>
, 
<taxonomicName baseAuthorityName="Burks" baseAuthorityYear="1979" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="garryana">
<emphasis italics="true" pageId="30" pageNumber="31">Q. garryana</emphasis>
</taxonomicName>
, 
<taxonomicName authority="(Burks 1979)" baseAuthorityName="Burks" baseAuthorityYear="1979" box="[269,534,1818,1844]" class="Magnoliopsida" family="Fagaceae" genus="Quercus" kingdom="Plantae" order="Fagales" pageId="30" pageNumber="31" phylum="Tracheophyta" rank="species" species="lobata">
