<number box="[1320,1348,1194,1218]" pageId="5" pageNumber="20" value="10.0">10</number>
(
<number box="[1363,1424,1194,1218]" pageId="5" pageNumber="20" value="9.0" valueMax="10.0" valueMin="8.0">8–10</number>
). Coxal seta I (
<emphasis box="[305,333,1230,1254]" italics="true" pageId="5" pageNumber="20">
<number box="[305,318,1230,1254]" pageId="5" pageNumber="20" value="1.0">1</number>
b
</emphasis>
) absent; coxal seta II (
<emphasis box="[596,624,1230,1254]" italics="true" pageId="5" pageNumber="20">
<number box="[596,608,1230,1254]" pageId="5" pageNumber="20" value="1.0">1</number>
a
