spiculiform, 
<number box="[872,902,325,351]" pageId="19" pageNumber="20" value="45.0">45</number>
(
<number box="[918,996,325,351]" pageId="19" pageNumber="20" value="42.5" valueMax="45.0" valueMin="40.0">40–45</number>
) long; setae 
<emphasis box="[1150,1180,325,350]" italics="true" pageId="19" pageNumber="20">
d 
<number box="[1166,1180,325,350]" pageId="19" pageNumber="20" value="2.0">2</number>
