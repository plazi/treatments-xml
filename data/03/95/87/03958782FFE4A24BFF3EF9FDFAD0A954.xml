<collectingCountry box="[624,733,1647,1673]" name="Myanmar" pageId="7" pageNumber="84">Myanmar</collectingCountry>
(N. 
<collectingRegion box="[783,932,1646,1672]" country="Myanmar" name="Kachin" pageId="7" pageNumber="84">Kachin State</collectingRegion>
).
