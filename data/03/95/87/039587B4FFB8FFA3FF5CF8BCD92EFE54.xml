<collectingCountry box="[361,498,330,356]" name="Madagascar" pageId="8" pageNumber="333">Madagascar</collectingCountry>
, 
<collectingRegion box="[509,655,330,356]" country="Madagascar" name="Fianarantsoa" pageId="8" pageNumber="333">Fianarantsoa</collectingRegion>
, 
<location LSID="urn:lsid:plazi:treatment:039587B4FFB8FFA3FF5CF8BCD92EFE54:8EE36079FFB6FFA3FD52FEE4DB88FEE1" box="[665,969,330,356]" country="Madagascar" name="Ranomafana National Park" pageId="8" pageNumber="333" stateProvince="Fianarantsoa">Ranomafana National Park</location>
, ex 
<taxonomicName box="[1012,1154,331,357]" class="Magnoliopsida" family="Lauraceae" genus="Cryptocarya" kingdom="Plantae" order="Laurales" pageId="8" pageNumber="333" phylum="Tracheophyta" rank="genus">
<emphasis box="[1012,1154,331,357]" italics="true" pageId="8" pageNumber="333">Cryptocarya</emphasis>
</taxonomicName>
