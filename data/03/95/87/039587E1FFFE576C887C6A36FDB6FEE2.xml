<accessionNumber box="[783,918,673,699]" httpUri="http://www.ncbi.nlm.nih.gov/nucleotide/AM051230" pageId="3" pageNumber="22">
AM 
<date box="[830,918,673,699]" pageId="3" pageNumber="22" value="30-05-12">051230</date>
</accessionNumber>
– 
<number box="[932,964,673,699]" pageId="3" pageNumber="22" value="32.0">32</number>
, mitochondrial 
<collectionCode box="[1153,1213,673,699]" pageId="3" pageNumber="22">COII</collectionCode>
