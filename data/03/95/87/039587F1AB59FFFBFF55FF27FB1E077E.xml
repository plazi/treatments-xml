Figs. 
<number box="[1351,1365,223,247]" pageId="14" pageNumber="209" value="4.0">4</number>
D 
<emphasis bold="true" box="[1385,1399,222,247]" pageId="14" pageNumber="209">–</emphasis>
F
</figureCitation>
).
</paragraph>
<paragraph blockId="14.[151,1436,222,535]" box="[199,896,258,283]" pageId="14" pageNumber="209">
<emphasis bold="true" box="[199,295,258,283]" pageId="14" pageNumber="209">Species:</emphasis>
<taxonomicName box="[302,551,259,283]" class="Insecta" family="Elmidae" genus="Macrelmis" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="209" phylum="Arthropoda" rank="species" species="sandersoni">
<emphasis box="[302,551,259,283]" italics="true" pageId="14" pageNumber="209">Macrelmis sandersoni</emphasis>
</taxonomicName>
, 
<taxonomicName box="[565,675,259,282]" class="Insecta" family="Elmidae" genus="Macrelmis" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="209" phylum="Arthropoda" rank="species" species="striata">
<emphasis box="[565,675,259,282]" italics="true" pageId="14" pageNumber="209">M. striata</emphasis>
</taxonomicName>
and 
<emphasis box="[730,896,259,283]" italics="true" pageId="14" pageNumber="209">
<taxonomicName box="[730,892,259,283]" class="Insecta" family="Elmidae" genus="Macrelmis" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="209" phylum="Arthropoda" rank="species" species="striatoidea">M. striatoidea</taxonomicName>
