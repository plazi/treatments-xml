<taxonomicName authority="Leach, 1820" authorityName="Leach" authorityYear="1820" box="[303,386,1899,1922]" class="Malacostraca" family="Galatheidae" genus="Munida" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="34" pageNumber="35" phylum="Arthropoda" rank="genus">Munida</taxonomicName>
redacta
</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[481,567,1899,1923]" pageId="34" pageNumber="35" rank="species">
