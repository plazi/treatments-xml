<taxonomicName authority="Pic, 1903" authorityName="Pic" authorityYear="1903" box="[265,553,1469,1488]" class="Insecta" family="Anthicidae" genus="Andrahomanus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="19" pageNumber="186" phylum="Arthropoda" rank="species" species="luteipes">
