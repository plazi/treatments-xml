= ostium; 
<emphasis box="[568,604,1911,1932]" italics="true" pageId="5" pageNumber="185">st 8</emphasis>
= sternite 8; 
<emphasis box="[737,774,1911,1932]" italics="true" pageId="5" pageNumber="185">st 9</emphasis>
