<emphasis box="[943,966,1941,1964]" italics="true" pageId="2" pageNumber="551">vs</emphasis>
<quantity box="[973,1137,1940,1965]" metricMagnitude="-3" metricUnit="m" metricValue="2.465" metricValueMax="2.76" metricValueMin="2.17" pageId="2" pageNumber="551" unit="mm" value="2.465" valueMax="2.76" valueMin="2.17">2.17–2.76 mm</quantity>
in females, 
<quantity box="[1274,1436,1940,1964]" metricMagnitude="-3" metricUnit="m" metricValue="2.195" metricValueMax="2.28" metricValueMin="2.11" pageId="2" pageNumber="551" unit="mm" value="2.195" valueMax="2.28" valueMin="2.11">2.11–2.28 mm</quantity>
<emphasis box="[151,174,1977,2000]" italics="true" pageId="2" pageNumber="551">vs</emphasis>
<quantity box="[181,345,1976,2000]" metricMagnitude="-3" metricUnit="m" metricValue="2.0" metricValueMax="2.01" metricValueMin="1.99" pageId="2" pageNumber="551" unit="mm" value="2.0" valueMax="2.01" valueMin="1.99">1.99–2.01 mm</quantity>
in males), and a shorter and broader tail (c= 
<number box="[840,890,1976,2000]" pageId="2" pageNumber="551" value="20.3">20.3</number>
–34.0 
<emphasis box="[963,986,1977,2000]" italics="true" pageId="2" pageNumber="551">vs</emphasis>
<number box="[993,1105,1976,2001]" pageId="2" pageNumber="551" value="18.6" valueMax="20.3" valueMin="16.9">16.9–20.3</number>
and c'= 2.0–3.0 
<emphasis box="[1292,1315,1977,2000]" italics="true" pageId="2" pageNumber="551">vs</emphasis>
<quantity box="[1323,1436,1976,2001]" metricMagnitude="-2" metricUnit="m" metricValue="9.906" metricValueMax="10.414" metricValueMin="9.398" pageId="2" pageNumber="551" unit="in" value="3.9" valueMax="4.1" valueMin="3.7">3.7–4.1 in</quantity>
females and c= 
<number box="[327,439,2012,2037]" pageId="2" pageNumber="551" value="30.85" valueMax="36.2" valueMin="25.5">25.5–36.2</number>
<emphasis box="[446,469,2013,2036]" italics="true" pageId="2" pageNumber="551">vs</emphasis>
<number box="[477,525,2012,2037]" pageId="2" pageNumber="551" value="17.5">17.5</number>
–18.0 and c'= 
<number box="[684,768,2012,2037]" pageId="2" pageNumber="551" value="2.1" valueMax="2.6" valueMin="1.6">1.6–2.6</number>
<emphasis box="[774,797,2013,2036]" italics="true" pageId="2" pageNumber="551">vs</emphasis>
<quantity box="[805,918,2012,2037]" metricMagnitude="-1" metricUnit="m" metricValue="1.1683999999999999" metricValueMax="1.1938" metricValueMin="1.143" pageId="2" pageNumber="551" unit="in" value="4.6" valueMax="4.7" valueMin="4.5">4.5–4.7 in</quantity>
