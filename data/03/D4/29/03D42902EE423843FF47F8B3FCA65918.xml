<paragraph pageId="2" pageNumber="383">
<table box="[159,1423,734,1004]" gridcols="8" gridrows="7" pageId="2" pageNumber="383">
<tr box="[159,1423,734,756]" gridrow="0" pageId="2" pageNumber="383">
<th box="[159,397,734,756]" gridcol="0" gridrow="0" pageId="2" pageNumber="383">Voucher</th>
<th box="[420,584,734,756]" gridcol="1" gridrow="0" pageId="2" pageNumber="383">Species</th>
