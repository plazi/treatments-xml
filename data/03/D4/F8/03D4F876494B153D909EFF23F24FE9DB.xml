<taxonomicName authority="(Muller)" baseAuthorityName="Muller" box="[518,889,914,940]" class="Gastropoda" family="Helicidae" genus="Eobania" kingdom="Animalia" order="Stylommatophora" pageId="18" pageNumber="584" phylum="Mollusca" rank="species" species="vermiculata">
