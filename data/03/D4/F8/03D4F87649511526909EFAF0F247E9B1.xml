</taxonomicName>
and 
<taxonomicName authority="(Muller)" baseAuthorityName="Muller" box="[205,495,809,835]" class="Gastropoda" family="Helicidae" genus="Theba" kingdom="Animalia" order="Stylommatophora" pageId="9" pageNumber="575" phylum="Mollusca" rank="species" species="pisana">
