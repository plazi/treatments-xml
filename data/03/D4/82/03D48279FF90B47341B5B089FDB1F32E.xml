<emphasis box="[1029,1071,2007,2033]" italics="true" pageId="4" pageNumber="376">gd2</emphasis>
, 
<emphasis box="[1085,1127,2006,2032]" italics="true" pageId="4" pageNumber="376">gd6</emphasis>
