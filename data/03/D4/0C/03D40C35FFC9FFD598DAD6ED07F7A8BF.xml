<emphasis bold="true" box="[984,1011,1636,1660]" pageId="12" pageNumber="13">14</emphasis>
</number>
, Dorsal habitus; 
<number box="[1191,1218,1636,1660]" pageId="12" pageNumber="13" value="15.0">
<emphasis bold="true" box="[1191,1218,1636,1660]" pageId="12" pageNumber="13">15</emphasis>
</number>
, Lateral habitus; 
<number box="[1403,1430,1636,1660]" pageId="12" pageNumber="13" value="16.0">
<emphasis bold="true" box="[1403,1430,1636,1660]" pageId="12" pageNumber="13">16</emphasis>
</number>
, 
<taxonomicName box="[151,269,1676,1699]" class="Crustacea" family="Crustacea" genus="Mesosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Crustacea" pageId="12" pageNumber="4" phylum="Arthropoda" rank="genus">Mesosoma</taxonomicName>
, dorsal view; 
<number box="[417,444,1676,1700]" pageId="12" pageNumber="13" value="17.0">
<emphasis bold="true" box="[417,444,1676,1700]" pageId="12" pageNumber="13">17</emphasis>
