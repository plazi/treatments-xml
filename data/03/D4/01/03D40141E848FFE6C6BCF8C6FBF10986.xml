<taxonomicName box="[297,533,1975,1997]" class="Insecta" family="Staphylinidae" genus="Lathrobium" kingdom="Animalia" order="Coleoptera" pageId="14" pageNumber="259" phylum="Arthropoda" rank="species" species="liyangense">
<emphasis box="[297,533,1975,1997]" italics="true" pageId="14" pageNumber="259">
Lathrobium 
<taxonomicName box="[425,533,1975,1997]" pageId="14" pageNumber="259" rank="species" species="liyangense">liyangense</taxonomicName>
</emphasis>
</taxonomicName>
