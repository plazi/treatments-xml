and 
<collectingRegion box="[281,402,295,320]" country="Brazil" name="Sao Paulo" pageId="18" pageNumber="19">São Paulo</collectingRegion>
) (
<bibRefCitation box="[430,772,295,320]" pageId="18" pageNumber="19" refId="ref34354" refString="Redighieri, E. S. &amp; Azevedo, C. O. (2006) Fauna de Dissomphalus Ashmead (Hymenoptera, Bethylidae) da Mata Atlantica Brasileira, com descricao de 23 especies novas. Revista Brasileira de Entomologia, 50, 297 - 334. https: // doi. org / 10.1590 / S 0085 - 56262006000300001" type="journal article">Redighieri &amp; Azevedo 2006</bibRefCitation>
