<taxonomicName class="Arachnida" family="Aturidae" genus="Albiella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="63" pageNumber="64" phylum="Arthropoda" rank="species" species="lundbladi">
