<taxonomicName authority="A. Smith" authorityName="A. Smith" box="[1109,1376,781,805]" class="Mammalia" family="Galagidae" genus="Galago" kingdom="Animalia" order="Primates" pageId="20" pageNumber="284" phylum="Chordata" rank="species" species="moholi">
