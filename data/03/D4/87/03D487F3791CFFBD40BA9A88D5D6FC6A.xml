<taxonomicName box="[491,619,816,840]" class="Mammalia" family="Galagidae" kingdom="Animalia" order="Primates" pageId="20" pageNumber="284" phylum="Chordata" rank="family">Galagidae</taxonomicName>
), 
<collectingCountry box="[639,757,816,840]" name="Rwanda" pageId="20" pageNumber="284">RWANDA</collectingCountry>
