<number box="[737,857,484,510]" pageId="31" pageNumber="32" value="1.67" valueMax="1.77" valueMin="1.57">1.57–1.77</number>
, HV 
<number box="[918,1040,484,510]" pageId="31" pageNumber="32" value="2.65" valueMax="2.79" valueMin="2.51">2.51–2.79</number>
.
