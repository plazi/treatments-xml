<taxonomicName authority="(White)" baseAuthorityName="White" class="Malacostraca" family="Raninidae" genus="Cosmonotus" kingdom="Animalia" order="Decapoda" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
