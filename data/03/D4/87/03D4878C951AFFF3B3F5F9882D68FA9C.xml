<subSubSection pageId="3" pageNumber="2" type="etymology">
<paragraph blockId="3.[830,1440,659,743]" pageId="3" pageNumber="2">
<emphasis bold="true" box="[874,998,659,682]" pageId="3" pageNumber="2">
<emphasis bold="true" box="[874,991,659,682]" italics="true" pageId="3" pageNumber="2">Etymology</emphasis>
.
</emphasis>
