<collectingCountry box="[1043,1166,1271,1295]" name="Democratic Republic of the Congo" pageId="14" pageNumber="278">DR Congo</collectingCountry>
, 
<collectingCountry box="[1178,1276,1271,1295]" name="Tanzania" pageId="14" pageNumber="278">Tanzania</collectingCountry>
, and 
<emphasis box="[1332,1350,1272,1295]" italics="true" pageId="14" pageNumber="278">L</emphasis>
