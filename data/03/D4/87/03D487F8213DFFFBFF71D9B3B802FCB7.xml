<paragraph blockId="26.[151,1437,151,862]" pageId="26" pageNumber="27">
<emphasis bold="true" box="[199,501,294,321]" pageId="26" pageNumber="27">Habitat and distribution.</emphasis>
<taxonomicName baseAuthorityName="Wood-Mason in Wood-Mason &amp; Alcock" baseAuthorityYear="1891" box="[509,718,295,321]" class="Malacostraca" family="Leucosiidae" genus="Tanaoa" kingdom="Animalia" order="Decapoda" pageId="26" pageNumber="27" phylum="Arthropoda" rank="species" species="pustulosus">
<emphasis box="[509,718,295,321]" italics="true" pageId="26" pageNumber="27">Tanaoa pustulosus</emphasis>
</taxonomicName>
is distributed along the 
<collectionCode box="[993,1017,295,321]" pageId="26" pageNumber="27">IP</collectionCode>
(
<collectingCountry box="[1033,1076,295,321]" name="Fiji" pageId="26" pageNumber="27">Fiji</collectingCountry>
, 
<collectingCountry box="[1089,1266,295,321]" name="New Caledonia" pageId="26" pageNumber="27">New Caledonia</collectingCountry>
, 
<collectingCountry box="[1278,1432,295,321]" name="New Zealand" pageId="26" pageNumber="27">New Zealand</collectingCountry>
, 
<collectingCountry box="[151,218,331,357]" name="Japan" pageId="26" pageNumber="27">Japan</collectingCountry>
, 
<collectingCountry box="[239,324,331,357]" name="Taiwan" pageId="26" pageNumber="27">Taiwan</collectingCountry>
, 
<collectingCountry box="[345,457,331,357]" name="Indonesia" pageId="26" pageNumber="27">Indonesia</collectingCountry>
, 
<collectingCountry box="[479,608,331,357]" name="Philippines" pageId="26" pageNumber="27">Philippines</collectingCountry>
, Andaman Sea, 
<collectingCountry box="[819,942,331,357]" name="Seychelles" pageId="26" pageNumber="27">Seychelles</collectingCountry>
, 
<collectingCountry box="[963,1154,331,357]" name="Comoros" pageId="26" pageNumber="27">Comoro Islands</collectingCountry>
