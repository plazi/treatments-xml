<quantity box="[629,736,366,393]" metricMagnitude="2" metricUnit="m" metricValue="5.31" metricValueMax="9.77" metricValueMin="0.85" pageId="26" pageNumber="27" unit="m" value="531.0" valueMax="977.0" valueMin="85.0">85–977m</quantity>
