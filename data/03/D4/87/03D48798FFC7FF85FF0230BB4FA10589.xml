<emphasis bold="true" box="[314,392,1114,1139]" italics="true" pageId="13" pageNumber="14">
(
<taxonomicName authorityName="Dejean" authorityYear="1833" box="[321,386,1114,1139]" class="Insecta" family="Orphnidae" genus="Hybalus" kingdom="Animalia" order="Coleoptera" pageId="13" pageNumber="14" phylum="Arthropoda" rank="genus">Hybalus</taxonomicName>
