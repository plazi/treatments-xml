<number box="[799,814,1719,1745]" pageId="51" pageNumber="52" value="2.0">2</number>
–4, 3 
<taxonomicName box="[888,950,1719,1745]" class="Insecta" family="Chironomidae" genus="Litocladius" kingdom="Animalia" order="Diptera" pageId="51" pageNumber="52" phylum="Arthropoda" rank="species" species="setae">setae</taxonomicName>
