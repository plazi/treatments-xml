<subSubSection pageId="2" pageNumber="43" type="reference_group">
<paragraph blockId="2.[151,1436,1662,1715]" pageId="2" pageNumber="43">
<taxonomicName authority="Rentz, Invertebrate Taxonomy 1990: 1123" authorityName="Rentz, Invertebrate Taxonomy 1990:" authorityPageNumber="1123" authorityYear="1123" box="[151,783,1662,1685]" class="Insecta" family="Gryllacrididae" genus="Chauliogryllacris" higherTaxonomySource="GBIF" kingdom="Animalia" order="Orthoptera" pageId="2" pageNumber="43" phylum="Arthropoda" rank="genus">
