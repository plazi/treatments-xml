</bibRefCitation>
</taxonomicName>
(
<collectingCountry box="[946,1009,692,717]" name="India" pageId="39" pageNumber="40">India</collectingCountry>
), 
<taxonomicName authority="Cook, 1967" authorityName="Cook" authorityYear="1967" box="[1031,1271,692,717]" class="Arachnida" family="Aturidae" genus="Javalbia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="39" pageNumber="40" phylum="Arthropoda" rank="species" species="punya">
