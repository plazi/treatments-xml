<paragraph blockId="63.[264,1326,1029,1895]" pageId="63" pageNumber="64">
Total length 
<number box="[468,588,1549,1575]" pageId="63" pageNumber="64" value="1.75" valueMax="1.86" valueMin="1.64">1.64–1.86</number>
, 
<quantity box="[605,717,1549,1575]" metricMagnitude="-3" metricUnit="m" metricValue="1.71" pageId="63" pageNumber="64" unit="mm" value="1.71">1.71 mm</quantity>
. Wing length 
<number box="[894,1014,1549,1575]" pageId="63" pageNumber="64" value="1.1400000000000001" valueMax="1.24" valueMin="1.04">1.04–1.24</number>
, 
