</taxonomicNameLabel>
and 
<taxonomicName authority="Wiedemann" authorityName="Wiedemann" box="[894,1339,1229,1251]" class="Insecta" family="Calliphoridae" genus="Huascaromusca" kingdom="Animalia" order="Diptera" pageId="7" pageNumber="258" phylum="Arthropoda" rank="species" species="aeneiventris">
