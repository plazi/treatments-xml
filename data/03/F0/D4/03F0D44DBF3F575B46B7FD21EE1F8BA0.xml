</taxonomicName>
and 
<taxonomicName box="[469,590,836,861]" class="Aves" family="Threskiornithidae" genus="Bostrychia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Pelecaniformes" pageId="6" pageNumber="227" phylum="Chordata" rank="genus">
<emphasis box="[469,590,836,861]" italics="true" pageId="6" pageNumber="227">Bostrychia</emphasis>
</taxonomicName>
