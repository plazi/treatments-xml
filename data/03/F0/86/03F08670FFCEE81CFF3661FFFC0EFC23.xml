</taxonomicName>
, 
<taxonomicName baseAuthorityName="Cresson" baseAuthorityYear="1865" box="[1088,1245,657,683]" class="Insecta" family="Ichneumonidae" genus="Acrotaphus" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="fascipennis">
<emphasis box="[1088,1105,657,683]" italics="true" pageId="59" pageNumber="60">A</emphasis>
. 
<emphasis box="[1119,1245,657,683]" italics="true" pageId="59" pageNumber="60">fascipennis</emphasis>
</taxonomicName>
, 
<taxonomicName authorityName="Gauld" authorityYear="1991" box="[1259,1390,657,683]" class="Insecta" family="Ichneumonidae" genus="Acrotaphus" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="chedelae">
<emphasis box="[1259,1390,657,683]" italics="true" pageId="59" pageNumber="60">A. chedelae</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName="Higa &amp; Penteado-Dias" authorityYear="2019" box="[151,228,693,719]" class="Insecta" family="Ichneumonidae" genus="Acrotaphus" kingdom="Animalia" order="Hymenoptera" pageId="59" pageNumber="60" phylum="Arthropoda" rank="species" species="japi">
<emphasis box="[151,228,693,719]" italics="true" pageId="59" pageNumber="60">A. japi</emphasis>
</taxonomicName>
