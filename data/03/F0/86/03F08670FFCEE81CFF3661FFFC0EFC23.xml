<subSubSection pageId="59" pageNumber="60" type="etymology">
<paragraph blockId="59.[151,1437,152,899]" pageId="59" pageNumber="60">
<emphasis bold="true" box="[199,334,585,611]" pageId="59" pageNumber="60">Etymology.</emphasis>
