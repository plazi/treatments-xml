<subSubSection pageId="45" pageNumber="46" type="etymology">
<paragraph blockId="45.[151,1437,153,467]" pageId="45" pageNumber="46">
<emphasis bold="true" box="[199,334,405,431]" pageId="45" pageNumber="46">Etymology.</emphasis>
