<taxonomicName authority="(Cameron)" authorityName="Cameron" baseAuthorityName="Cameron" baseAuthorityYear="1886" class="Insecta" family="Ichneumonidae" genus="Acrotaphus" kingdom="Animalia" order="Hymenoptera" pageId="52" pageNumber="53" phylum="Arthropoda" rank="species" species="tibialis">
