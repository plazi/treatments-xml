<taxonomicName authority="Ramazzotti and Maucci, 1983" authorityName="Ramazzotti and Maucci" authorityYear="1983" box="[485,1124,654,678]" class="Eutardigrada" family="Hypsibiidae" genus="Hypsibius" kingdom="Animalia" order="Parachela" pageId="4" pageNumber="43" phylum="Tardigrada" rank="species" species="camelopardalis">
