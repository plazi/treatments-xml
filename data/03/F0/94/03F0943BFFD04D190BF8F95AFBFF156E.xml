<taxonomicName baseAuthorityName="Lamouroux" baseAuthorityYear="1821" box="[790,978,285,310]" class="Hydrozoa" family="Sertulariidae" genus="Sertularella" higherTaxonomySource="GBIF" kingdom="Animalia" order="Leptothecata" pageId="13" pageNumber="14" phylum="Cnidaria" rank="species" species="gayi">
