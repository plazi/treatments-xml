<paragraph blockId="19.[151,1436,815,1597]" box="[151,1138,815,840]" pageId="19" pageNumber="20">
<emphasis bold="true" box="[151,489,815,840]" pageId="19" pageNumber="20">Material examined. 54907#3</emphasis>
, four stems up to 
<quantity box="[691,770,816,840]" metricMagnitude="-2" metricUnit="m" metricValue="1.8" pageId="19" pageNumber="20" unit="mm" value="18.0">18 mm</quantity>
