<subSubSection pageId="11" pageNumber="12" type="etymology">
<paragraph blockId="11.[189,1398,977,1084]" box="[189,325,977,1003]" pageId="11" pageNumber="12">
<heading bold="true" box="[189,325,977,1003]" fontSize="11" level="2" pageId="11" pageNumber="12" reason="6">
