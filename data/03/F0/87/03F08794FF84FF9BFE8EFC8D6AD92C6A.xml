<taxonomicName authorityName="Hampson" authorityYear="1898" box="[151,250,1031,1052]" class="Insecta" family="Erebidae" genus="Pseudothyretes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="29" phylum="Arthropoda" rank="species" species="carnea">
