, 
<taxonomicName authorityName="Hampson" authorityYear="1901" box="[264,404,1030,1052]" class="Insecta" family="Erebidae" genus="Pseudothyretes" higherTaxonomySource="GBIF" kingdom="Animalia" order="Lepidoptera" pageId="7" pageNumber="29" phylum="Arthropoda" rank="species" species="erubescens">
