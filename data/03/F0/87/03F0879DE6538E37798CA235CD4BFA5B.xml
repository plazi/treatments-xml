<number box="[560,612,265,290]" pageId="38" pageNumber="295" value="0.55">0.55</number>
, 
<number box="[623,672,265,290]" pageId="38" pageNumber="295" value="0.51">0.51</number>
(
<number box="[687,805,265,290]" pageId="38" pageNumber="295" value="0.515" valueMax="0.55" valueMin="0.48">0.48–0.55</number>
