<number box="[577,696,151,176]" pageId="38" pageNumber="295" value="0.515" valueMax="0.53" valueMin="0.5">0.50–0.53</number>
); III 
<number box="[753,805,151,176]" pageId="38" pageNumber="295" value="0.46">0.46</number>
, 
