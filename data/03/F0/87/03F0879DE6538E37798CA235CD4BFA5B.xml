(
<number box="[277,395,1956,1981]" pageId="37" pageNumber="294" value="0.515" valueMax="0.55" valueMin="0.48">0.48–0.55</number>
); eye-ocellar space 
<number box="[614,666,1957,1981]" pageId="37" pageNumber="294" value="0.23">0.23</number>
, 
<number box="[677,726,1957,1981]" pageId="37" pageNumber="294" value="0.23">0.23</number>
(
<number box="[741,859,1956,1981]" pageId="37" pageNumber="294" value="0.225" valueMax="0.25" valueMin="0.2">0.20–0.25</number>
); eye length 
<number box="[1000,1052,1956,1981]" pageId="37" pageNumber="294" value="0.57">0.57</number>
, 
<number box="[1063,1112,1956,1981]" pageId="37" pageNumber="294" value="0.54">0.54</number>
