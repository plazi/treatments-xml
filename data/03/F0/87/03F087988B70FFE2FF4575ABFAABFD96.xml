<subSubSection box="[199,1035,404,429]" pageId="5" pageNumber="36" type="etymology">
<paragraph blockId="5.[151,1436,151,536]" box="[199,1035,404,429]" pageId="5" pageNumber="36">
<emphasis bold="true" box="[199,334,404,428]" pageId="5" pageNumber="36">Etymology.</emphasis>
