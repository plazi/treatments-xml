<paragraph blockId="21.[151,1436,151,783]" pageId="21" pageNumber="472">
<taxonomicName authorityName="Krefft" authorityYear="1871" box="[199,342,576,600]" class="Arachnida" family="Syringophilidae" genus="Gnathosoma" higherTaxonomySource="GBIF" kingdom="Animalia" order="Trombidiformes" pageId="21" pageNumber="472" phylum="Arthropoda" rank="genus">
<emphasis box="[199,342,576,600]" italics="true" pageId="21" pageNumber="472">Gnathosoma</emphasis>
</taxonomicName>
