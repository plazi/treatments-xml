<taxonomicName authorityName="Macleay" authorityYear="1877" box="[1193,1429,1488,1511]" class="Insecta" family="Curculionidae" genus="Heteropus" kingdom="Animalia" order="Coleoptera" pageId="30" pageNumber="31" phylum="Arthropoda" rank="species" species="variegatus">
