<collectingRegion box="[803,877,1523,1546]" country="Pakistan" name="Punjab" pageId="0" pageNumber="271">Punjab</collectingRegion>
region where the 
<typeStatus box="[1070,1116,1523,1546]" pageId="0" pageNumber="271">type</typeStatus>
