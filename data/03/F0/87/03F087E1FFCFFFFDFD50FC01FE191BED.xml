<paragraph blockId="3.[173,659,1071,1730]" pageId="3">
<emphasis bold="true" box="[197,389,1230,1251]" pageId="3">
Female 
<typeStatus box="[281,386,1230,1251]" pageId="3">Paratypes</typeStatus>
