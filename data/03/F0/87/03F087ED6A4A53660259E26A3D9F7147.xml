<taxonomicName authority="Hsiao" authorityName="Hsiao" box="[449,694,1494,1517]" class="Insecta" family="Reduviidae" genus="Panthous" higherTaxonomySource="GBIF" kingdom="Metazoa" order="Hemiptera" pageId="3" pageNumber="64" phylum="Arthropoda" rank="species" species="ruber">
