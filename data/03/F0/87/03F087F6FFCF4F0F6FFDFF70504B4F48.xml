<subSubSection box="[151,1389,350,375]" pageId="2" pageNumber="571" type="distribution">
<paragraph blockId="2.[151,1436,350,519]" box="[151,1389,350,375]" pageId="2" pageNumber="571">
<emphasis bold="true" box="[151,304,350,375]" pageId="2" pageNumber="571">Distribution.</emphasis>
<collectingCountry box="[311,375,351,374]" name="Japan" pageId="2" pageNumber="571">Japan</collectingCountry>
