<paragraph blockId="34.[193,1399,621,824]" pageId="34" pageNumber="35">
<taxonomicName authorityName="Haworth" authorityYear="1825" box="[193,299,621,647]" class="Malacostraca" family="Mysidae" kingdom="Animalia" order="Mysida" pageId="34" pageNumber="35" phylum="Arthropoda" rank="family">Mysidae</taxonomicName>
