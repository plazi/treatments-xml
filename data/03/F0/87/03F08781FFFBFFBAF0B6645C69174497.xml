<location LSID="urn:lsid:plazi:treatment:03F08781FFFBFFBAF0B6645C69174497:8E86604CFFFBFFBBF50865696C16411B" box="[1096,1146,791,815]" country="Israel" name="This" pageId="4" pageNumber="1717">This</location>
