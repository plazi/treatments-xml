); III 
<number box="[748,800,260,285]" pageId="16" pageNumber="273" value="0.57">0.57</number>
, 
<number box="[811,860,260,285]" pageId="16" pageNumber="273" value="0.52">0.52</number>
(
<number box="[876,995,260,285]" pageId="16" pageNumber="273" value="0.48" valueMax="0.5" valueMin="0.46">0.46–0.50</number>
); IV. 
<number box="[1055,1107,260,285]" pageId="16" pageNumber="273" value="0.46">0.46</number>
, 
<number box="[1119,1168,260,285]" pageId="16" pageNumber="273" value="0.45">0.45</number>
(
<number box="[1184,1303,260,284]" pageId="16" pageNumber="273" value="0.41000000000000003" valueMax="0.42" valueMin="0.4">0.40–0.42</number>
). Antennae, length of segments: I 
<number box="[403,455,295,320]" pageId="16" pageNumber="273" value="0.35">0.35</number>
, 
<number box="[468,517,295,320]" pageId="16" pageNumber="273" value="0.36">0.36</number>
(
<number box="[534,653,296,320]" pageId="16" pageNumber="273" value="0.32" valueMax="0.34" valueMin="0.3">0.30–0.34</number>
); II 
<number box="[702,754,295,320]" pageId="16" pageNumber="273" value="0.75">0.75</number>
, 
<number box="[768,817,295,320]" pageId="16" pageNumber="273" value="0.76">0.76</number>
(
<number box="[834,952,295,320]" pageId="16" pageNumber="273" value="0.71" valueMax="0.78" valueMin="0.64">0.64–0.78</number>
); III 
<number box="[1011,1063,295,320]" pageId="16" pageNumber="273" value="0.5">0.50</number>
, 
<number box="[1076,1125,295,320]" pageId="16" pageNumber="273" value="0.45">0.45</number>
(
<number box="[1143,1261,295,320]" pageId="16" pageNumber="273" value="0.505" valueMax="0.55" valueMin="0.46">0.46–0.55</number>
); IV 
<number box="[1322,1374,295,320]" pageId="16" pageNumber="273" value="0.64">0.64</number>
, 
