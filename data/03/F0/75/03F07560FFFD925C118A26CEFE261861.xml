<taxonomicName authority="Kurz, 1875" authorityName="Kurz" authorityYear="1875" box="[151,620,1668,1694]" class="Branchiopoda" family="Lepidoziaceae" genus="Kurzia" kingdom="Animalia" order="Diplostraca" pageId="0" pageNumber="293" phylum="Arthropoda" rank="species" sensu="lato" species="latissima" subGenus="Kurzia">
<emphasis bold="true" box="[151,456,1668,1694]" italics="true" pageId="0" pageNumber="293">Kurzia (Kurzia) latissima</emphasis>
(Kurz, 
<number box="[552,612,1668,1694]" pageId="0" pageNumber="293" value="1875.0">1875</number>
)
</taxonomicName>
<taxonomicNameLabel box="[628,692,1668,1694]" pageId="0" pageNumber="293" sensu="lato">
<emphasis bold="true" box="[628,685,1668,1694]" italics="true" pageId="0" pageNumber="293">s. lat</emphasis>
.
