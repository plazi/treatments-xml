<collectionCode LSID="urn:lsid:biocol.org:col:34094" box="[865,942,799,823]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34094" name="Ohio State University Acarology Laboratory" pageId="10" pageNumber="311">OSAL</collectionCode>
.
