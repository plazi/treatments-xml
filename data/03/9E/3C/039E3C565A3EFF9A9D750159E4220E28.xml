<quantity box="[1303,1380,1947,1973]" metricMagnitude="4" metricUnit="m" metricValue="4.8" pageId="22" pageNumber="23" unit="km" value="48.0">48 km</quantity>
<superScript attach="right" box="[1383,1392,1946,1960]" fontSize="6" pageId="22" pageNumber="23">2</superScript>
