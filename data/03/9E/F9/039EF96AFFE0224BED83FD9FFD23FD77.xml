, 
<taxonomicName authorityName="Vieillot" authorityYear="1823" box="[410,567,1192,1219]" class="Aves" family="Accipitridae" genus="Buteo" kingdom="Animalia" order="Accipitriformes" pageId="27" pageNumber="28" phylum="Chordata" rank="species" species="platypterus">
