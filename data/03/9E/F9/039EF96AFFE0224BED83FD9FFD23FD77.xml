and 
<taxonomicName authorityName="Gmelin" authorityYear="1788" box="[621,783,1193,1219]" class="Aves" family="Accipitridae" genus="Buteo" kingdom="Animalia" order="Accipitriformes" pageId="27" pageNumber="28" phylum="Chordata" rank="species" species="jamaicensis">
<emphasis box="[621,638,1193,1218]" italics="true" pageId="27" pageNumber="28">B</emphasis>
. 
<emphasis box="[651,783,1193,1218]" italics="true" pageId="27" pageNumber="28">jamaicensis</emphasis>
</taxonomicName>
). 
