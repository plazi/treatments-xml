<taxonomicName authority="Linnaeus, 1758" authorityName="Linnaeus" authorityYear="1758" box="[239,669,841,868]" class="Aves" family="Anatidae" genus="Dendrocygna" kingdom="Animalia" order="Anseriformes" pageId="5" pageNumber="6" phylum="Chordata" rank="species" species="arborea">
