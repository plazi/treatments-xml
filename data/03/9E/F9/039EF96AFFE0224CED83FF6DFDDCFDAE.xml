<taxonomicName authority="Gmelin, 1788" authorityName="Gmelin" authorityYear="1788" box="[182,550,400,426]" class="Aves" family="Accipitridae" genus="Buteo" kingdom="Animalia" order="Accipitriformes" pageId="27" pageNumber="28" phylum="Chordata" rank="species" species="jamaicensis">
