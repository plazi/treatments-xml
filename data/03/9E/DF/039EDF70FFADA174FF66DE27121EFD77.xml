<quantity box="[859,997,1842,1866]" metricMagnitude="2" metricUnit="m" metricValue="2.5" metricValueMax="4.0" metricValueMin="1.0" pageId="19" pageNumber="170" unit="m" value="250.0" valueMax="400.0" valueMin="100.0">
100‚Äì 
<specimenCount box="[914,997,1842,1866]" pageId="19" pageNumber="170" type="generic">400 m</specimenCount>
</quantity>
. Only 
<taxonomicName box="[1084,1212,1841,1865]" class="Crinoidea" family="Comasteridae" genus="Comissia" kingdom="Animalia" order="Comatulida" pageId="19" pageNumber="170" phylum="Echinodermata" rank="species" species="luetkeni">
<emphasis box="[1084,1212,1841,1865]" italics="true" pageId="19" pageNumber="170">C. luetkeni</emphasis>
</taxonomicName>
