<collectingCountry box="[833,918,1805,1830]" name="Mexico" pageId="19" pageNumber="170">
<collectingRegion box="[833,918,1805,1830]" country="Mexico" name="Mexico" pageId="19" pageNumber="170">Mexico</collectingRegion>
</collectingCountry>
