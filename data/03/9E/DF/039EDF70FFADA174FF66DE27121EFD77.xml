<specimenCount box="[370,450,1878,1902]" pageId="19" pageNumber="170" type="generic">
<quantity box="[370,445,1878,1902]" metricMagnitude="2" metricUnit="m" metricValue="1.0" pageId="19" pageNumber="170" unit="m" value="100.0">100 m</quantity>
.
</specimenCount>
<bibRefCitation author="Rowe" box="[457,706,1877,1902]" pageId="19" pageNumber="170" refString="Rowe, F. W. E. &amp; Gates, J. (1995) Echinodermata. Zoological Catalogue of Australia. CSIRO, Melbourne, 510 pp." type="book" year="1995">Rowe &amp; Gates (1995)</bibRefCitation>
