</taxonomicName>
[= 
<taxonomicName box="[1082,1282,1491,1515]" class="Crinoidea" family="Comasteridae" genus="Comanthus" kingdom="Animalia" order="Comatulida" pageId="23" pageNumber="174" phylum="Echinodermata" rank="species" species="plectrophorum">
