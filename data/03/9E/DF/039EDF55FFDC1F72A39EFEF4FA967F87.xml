<taxonomicName authority="Awasthi" authorityName="Awasthi" box="[273,565,500,523]" class="Lecanoromycetes" family="Physciaceae" genus="Pyxine" kingdom="Fungi" order="Teloschistales" pageId="17" pageNumber="49" phylum="Ascomycota" rank="species" species="keralensis">
