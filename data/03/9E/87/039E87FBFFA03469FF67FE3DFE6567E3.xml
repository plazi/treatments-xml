<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[506,596,658,684]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="3" pageNumber="4">USNM</collectionCode>
