, 
<collectingRegion box="[406,630,930,958]" country="Iran" name="Kerman" pageId="6" pageNumber="253">Kerman province</collectingRegion>
, southern 
<collectingCountry box="[759,810,930,958]" name="Iran" pageId="6" pageNumber="253">Iran</collectingCountry>
(
<figureCitation box="[827,909,930,958]" captionStart="Map 1" captionStartId="5.[226,283,1941,1966]" captionTargetBox="[226,1359,1001,1916]" captionTargetId="figure-251@5.[226,1359,1390,1916]" captionTargetPageId="5" captionText="Map 1: Type localities of Lathrobium bucharense KOCH (triangle) and L. bananum nov.sp. (circle)." figureDoi="http://doi.org/10.5281/zenodo.6518393" httpUri="https://zenodo.org/record/6518393/files/figure.png" pageId="6" pageNumber="253">Map 1</figureCitation>
