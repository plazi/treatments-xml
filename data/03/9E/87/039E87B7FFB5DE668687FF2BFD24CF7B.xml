<subSubSection box="[199,878,330,355]" pageId="7" pageNumber="585" type="etymology">
<paragraph blockId="7.[151,1437,258,1147]" box="[199,878,330,355]" pageId="7" pageNumber="585">
<emphasis bold="true" box="[199,334,330,355]" pageId="7" pageNumber="585">Etymology.</emphasis>
