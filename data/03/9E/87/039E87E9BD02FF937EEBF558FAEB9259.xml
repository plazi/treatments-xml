<subSubSection pageId="6" pageNumber="7" type="etymology">
<paragraph blockId="6.[151,1437,151,1147]" box="[198,1281,928,954]" pageId="6" pageNumber="7">
<emphasis bold="true" box="[198,339,928,954]" pageId="6" pageNumber="7">Etymology.</emphasis>
