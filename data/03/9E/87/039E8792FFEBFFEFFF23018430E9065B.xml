<subSubSection box="[199,1378,151,176]" pageId="17" pageNumber="18" type="biology_ecology">
<paragraph blockId="17.[151,1436,151,608]" box="[199,1378,151,176]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[199,266,151,176]" pageId="17" pageNumber="18">Hosts</emphasis>
