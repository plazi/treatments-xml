<collectingCountry box="[477,533,188,212]" name="United States of America" pageId="17" pageNumber="18">USA</collectingCountry>
(
<collectingRegion box="[548,631,188,213]" country="United States of America" name="Illinois" pageId="17" pageNumber="18">Illinois</collectingRegion>
