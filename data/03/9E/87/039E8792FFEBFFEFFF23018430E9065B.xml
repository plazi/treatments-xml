<taxonomicName box="[516,655,151,176]" class="Insecta" family="Tettigoniidae" genus="Orchelimum" kingdom="Animalia" order="Orthoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="genus">
<emphasis box="[516,655,151,176]" italics="true" pageId="17" pageNumber="18">Orchelimum</emphasis>
</taxonomicName>
sp. (
<taxonomicName box="[708,836,151,176]" class="Insecta" kingdom="Animalia" order="Orthoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="order">Orthoptera</taxonomicName>
: 
<taxonomicName box="[846,995,151,176]" class="Insecta" family="Tettigoniidae" kingdom="Animalia" order="Orthoptera" pageId="17" pageNumber="18" phylum="Arthropoda" rank="family">Tettigoniidae</taxonomicName>
) on 
<taxonomicName box="[1045,1209,153,176]" class="Magnoliopsida" family="Apiaceae" genus="Daucus" kingdom="Plantae" order="Apiales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="species" species="carota">
<emphasis box="[1045,1209,153,176]" italics="true" pageId="17" pageNumber="18">Daucus carota</emphasis>
</taxonomicName>
L. (
<taxonomicName box="[1255,1366,151,176]" class="Magnoliopsida" family="Apiaceae" kingdom="Plantae" order="Apiales" pageId="17" pageNumber="18" phylum="Tracheophyta" rank="family">Apiaceae</taxonomicName>
).
</paragraph>
</subSubSection>
<subSubSection box="[199,809,188,213]" pageId="17" pageNumber="18" type="distribution">
<paragraph blockId="17.[151,1436,151,608]" box="[199,809,188,213]" pageId="17" pageNumber="18">
<emphasis bold="true" box="[199,345,188,213]" pageId="17" pageNumber="18">Distribution</emphasis>
