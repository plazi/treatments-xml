, 
<collectingCountry box="[518,616,1383,1408]" name="Vietnam" pageId="10" pageNumber="557">Vietnam</collectingCountry>
, Khanh Hoa Province, Khế River near the inflow of Cai River (
<geoCoordinate direction="north" orientation="latitude" pageId="10" pageNumber="557" precision="1" value="12.2790165">
N 
<number box="[151,180,1420,1445]" pageId="10" pageNumber="557" value="12.0">12</number>
° 
<number box="[189,274,1420,1445]" pageId="10" pageNumber="557" value="16741.0">16.741</number>
’
</geoCoordinate>
, 
<geoCoordinate box="[292,460,1420,1445]" direction="east" orientation="longitude" pageId="10" pageNumber="557" precision="1" value="108.91115">
E 
<number box="[317,360,1420,1445]" pageId="10" pageNumber="557" value="108.0">108</number>
° 
<number box="[370,456,1420,1445]" pageId="10" pageNumber="557" value="54669.0">54.669</number>
’
