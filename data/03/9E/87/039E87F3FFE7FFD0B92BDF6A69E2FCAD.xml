</taxonomicName>
than to 
<emphasis box="[417,675,387,407]" italics="true" pageId="2" pageNumber="3">
<taxonomicName authorityName="Osborn" authorityYear="1905" box="[417,550,387,407]" class="Reptilia" family="Tyrannosauridae" genus="Albertosaurus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Dinosauria" pageId="2" pageNumber="3" phylum="Chordata" rank="genus">Albertosaurus</taxonomicName>
