<subSubSection box="[151,707,1060,1085]" pageId="1" pageNumber="388" type="etymology">
<paragraph blockId="1.[151,1436,1060,2037]" box="[151,707,1060,1085]" pageId="1" pageNumber="388">
<emphasis bold="true" box="[151,286,1060,1085]" pageId="1" pageNumber="388">Etymology.</emphasis>
