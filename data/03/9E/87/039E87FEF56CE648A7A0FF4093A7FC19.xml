<taxonomicName authorityName="Song and Ahn" authorityYear="2019" box="[1013,1089,835,860]" class="Insecta" family="Staphylinidae" genus="Iotarphia" higherTaxonomySource="GBIF" kingdom="Animalia" order="Coleoptera" pageId="7" pageNumber="424" phylum="Arthropoda" rank="species" species="magna">
