<collectionCode LSID="urn:lsid:biocol.org:col:34871" box="[521,611,152,178]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34871" name="Smithsonian Institution, National Museum of Natural History" pageId="11" pageNumber="12">USNM</collectionCode>
