<emphasis box="[651,814,547,571]" italics="true" pageId="2" pageNumber="202">Allips concolor</emphasis>
</taxonomicName>
and 
<taxonomicName authorityName=": Smith" authorityYear="1994" box="[866,1068,547,571]" class="Actinopterygii" family="Ophichthidae" genus="Apterichtus" higherTaxonomySource="GBIF" kingdom="Animalia" order="Anguilliformes" pageId="2" pageNumber="202" phylum="Chordata" rank="species" species="moseri">
<emphasis box="[866,1068,547,571]" italics="true" pageId="2" pageNumber="202">Apterichtus moseri</emphasis>
