<taxonomicName class="Insecta" family="Baetidae" genus="Callibaetis" kingdom="Animalia" order="Ephemeroptera" pageId="11" pageNumber="511" phylum="Arthropoda" rank="species" species="jocosus" subGenus="Callibaetis">
