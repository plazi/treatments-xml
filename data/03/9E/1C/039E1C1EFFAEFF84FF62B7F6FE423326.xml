<emphasis bold="true" box="[151,695,1698,1725]" pageId="15" pageNumber="116">
<emphasis bold="true" box="[151,280,1698,1725]" italics="true" pageId="15" pageNumber="116">Unionicola</emphasis>
(
<emphasis bold="true" box="[297,395,1699,1725]" italics="true" pageId="15" pageNumber="116">Hexatax</emphasis>
) 
<emphasis bold="true" box="[411,481,1699,1725]" italics="true" pageId="15" pageNumber="116">tenuis</emphasis>
