<taxonomicName authority=", Udothea" authorityName="Udothea" box="[777,945,402,429]" class="Ulvophyceae" family="Ulvaceae" genus="Ulva" kingdom="Plantae" order="Ulvales" pageId="6" pageNumber="107" phylum="Chlorophyta" rank="genus">
<emphasis box="[777,831,402,428]" italics="true" pageId="6" pageNumber="107">Ulva</emphasis>
