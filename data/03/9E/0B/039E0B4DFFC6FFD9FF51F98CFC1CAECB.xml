<quantity box="[417,574,1699,1725]" metricMagnitude="-3" metricUnit="m" metricValue="9.75" metricValueMax="11.3" metricValueMin="8.2" pageId="35" pageNumber="36" unit="mm" value="9.75" valueMax="11.3" valueMin="8.2">8.2–11.3 mm</quantity>
