<taxonomicName baseAuthorityName="Tschudi" baseAuthorityYear="1845" box="[810,971,1718,1742]" class="Mammalia" family="Didelphidae" genus="Marmosops" kingdom="Animalia" order="Didelphimorphia" pageId="55" pageNumber="56" phylum="Chordata" rank="species" species="impavidus">
