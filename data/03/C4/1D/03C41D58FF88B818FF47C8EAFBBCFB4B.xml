<number box="[651,665,1849,1873]" pageId="11" pageNumber="36" value="4.0">4</number>
(Fig, 
<number box="[736,767,1849,1873]" pageId="11" pageNumber="36" value="17.0">17</number>
). Dorsal setae fine j 
<number box="[999,1010,1849,1873]" pageId="11" pageNumber="36" value="1.0">1</number>
<number box="[1019,1047,1849,1873]" pageId="11" pageNumber="36" value="43.0">43</number>
(
<number box="[1064,1138,1849,1873]" pageId="11" pageNumber="36" value="44.0" valueMax="47.0" valueMin="41.0">41–47</number>
), other anterior setae long anteriorly and on margins; j 
<number box="[473,487,1885,1909]" pageId="11" pageNumber="36" value="2.0">2</number>
<number box="[497,525,1885,1909]" pageId="11" pageNumber="36" value="75.0">75</number>
(
<number box="[543,618,1885,1909]" pageId="11" pageNumber="36" value="67.0" valueMax="75.0" valueMin="59.0">59–75</number>
), r 
<number box="[651,664,1885,1909]" pageId="11" pageNumber="36" value="3.0">3</number>
<number box="[674,702,1885,1909]" pageId="11" pageNumber="36" value="76.0">76</number>
(
<number box="[720,795,1885,1909]" pageId="11" pageNumber="36" value="77.0" valueMax="78.0" valueMin="76.0">76–78</number>
), S 
<number box="[836,847,1885,1909]" pageId="11" pageNumber="36" value="1.0">1</number>
<number box="[857,885,1885,1909]" pageId="11" pageNumber="36" value="88.0">88</number>
(
<number box="[903,978,1885,1909]" pageId="11" pageNumber="36" value="79.0" valueMax="88.0" valueMin="70.0">70–88</number>
), S 
<number box="[1015,1030,1885,1909]" pageId="11" pageNumber="36" value="4.0">4</number>
<number box="[1040,1068,1886,1909]" pageId="11" pageNumber="36" value="77.0">77</number>
(
<number box="[1086,1161,1885,1909]" pageId="11" pageNumber="36" value="74.5" valueMax="77.0" valueMin="72.0">72–77</number>
), S 
<number box="[1199,1213,1885,1909]" pageId="11" pageNumber="36" value="5.0">5</number>
<number box="[1223,1251,1885,1909]" pageId="11" pageNumber="36" value="73.0">73</number>
(
<number box="[1269,1344,1885,1909]" pageId="11" pageNumber="36" value="71.0" valueMax="77.0" valueMin="65.0">65–77</number>
), Z 
<number box="[1385,1398,1885,1909]" pageId="11" pageNumber="36" value="5.0">5</number>
<number box="[1408,1436,1885,1909]" pageId="11" pageNumber="36" value="71.0">71</number>
(
<number box="[159,235,1921,1945]" pageId="11" pageNumber="36" value="67.5" valueMax="77.0" valueMin="58.0">58–77</number>
). Shorter in mid-podonotal region; j 
<number box="[638,651,1921,1945]" pageId="11" pageNumber="36" value="5.0">5</number>
