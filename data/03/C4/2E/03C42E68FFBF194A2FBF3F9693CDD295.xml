<collectingCountry box="[959,1055,1897,1921]" name="Andorra" pageId="18" pageNumber="269">Andorra</collectingCountry>
</taxonomicName>
, 
<collectingCountry box="[1067,1120,1897,1921]" name="Italy" pageId="18" pageNumber="269">Italy</collectingCountry>
, 
