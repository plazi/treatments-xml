(f), 
<taxonomicName authorityName="Zerche" authorityYear="1998" box="[160,288,1102,1125]" class="Insecta" family="Staphylinidae" genus="Pseudopsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="942" phylum="Arthropoda" rank="species" species="gansuensis">
<emphasis box="[160,288,1102,1125]" italics="true" pageId="9" pageNumber="942">P. gansuensis</emphasis>
</taxonomicName>
(c), 
<taxonomicName authority="Yin, 2021" authorityName="Yin" authorityYear="2021" box="[332,409,1102,1125]" class="Insecta" family="Staphylinidae" genus="Pseudopsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="942" phylum="Arthropoda" rank="species" species="pengi" status="sp. nov.">
<emphasis box="[332,409,1102,1125]" italics="true" pageId="9" pageNumber="942">P. pengi</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[416,494,1102,1125]" pageId="9" pageNumber="942" rank="species">sp. nov.</taxonomicNameLabel>
(d), and 
<taxonomicName authority="Yin, 2021" authorityName="Yin" authorityYear="2021" box="[583,673,1102,1125]" class="Insecta" family="Staphylinidae" genus="Pseudopsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="942" phylum="Arthropoda" rank="species" species="serrata" status="sp. nov.">
