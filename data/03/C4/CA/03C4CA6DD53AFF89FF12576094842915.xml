<typeStatus box="[199,258,801,826]" pageId="40" pageNumber="41">Type</typeStatus>
locality.
</emphasis>
Werribee, Port Phillip Bay, Victoria, 
<collectingCountry box="[777,879,801,826]" name="Australia" pageId="40" pageNumber="41">Australia</collectingCountry>
(
<geoCoordinate box="[894,1045,801,826]" direction="south" orientation="latitude" pageId="40" pageNumber="41" precision="1" value="-37.951416">
<number box="[894,923,801,826]" pageId="40" pageNumber="41" value="37.0">37</number>
° 
<number box="[932,1017,801,826]" pageId="40" pageNumber="41" value="57.085">57.085</number>
’ S
</geoCoordinate>
, 
<geoCoordinate box="[1058,1224,801,825]" direction="east" orientation="longitude" pageId="40" pageNumber="41" precision="1" value="144.78546">
<number box="[1058,1100,801,825]" pageId="40" pageNumber="41" value="144.0">144</number>
° 
<number box="[1109,1193,801,825]" pageId="40" pageNumber="41" value="47.128">47.128</number>
