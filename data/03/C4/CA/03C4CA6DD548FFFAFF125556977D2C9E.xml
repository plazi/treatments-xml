d 
<number box="[1102,1113,1088,1112]" pageId="88" pageNumber="89" value="1.0">1</number>
-2, 
<date box="[1155,1182,1088,1112]" pageId="88" pageNumber="89">2v</date>
<number box="[1183,1220,1088,1112]" pageId="88" pageNumber="89" value="2.0" valueMax="3.0" valueMin="1.0">1-3</number>
, and IId. Elements 
<number box="[151,165,1124,1149]" pageId="88" pageNumber="89" value="2.0">2</number>
d 
<number box="[179,216,1125,1149]" pageId="88" pageNumber="89" value="1.5" valueMax="2.0" valueMin="1.0">1-2</number>
