<quantity box="[1152,1269,728,755]" metricMagnitude="-2" metricUnit="m" metricValue="3.6" metricValueMax="3.7" metricValueMin="3.5" pageId="4" pageNumber="319" unit="mm" value="36.0" valueMax="37.0" valueMin="35.0">35–37 µm</quantity>
