<quantity box="[450,567,728,755]" metricMagnitude="-2" metricUnit="m" metricValue="3.9" metricValueMax="4.3" metricValueMin="3.5" pageId="4" pageNumber="319" unit="mm" value="39.0" valueMax="43.0" valueMin="35.0">35–43 µm</quantity>
long, 
<quantity box="[643,761,728,755]" metricMagnitude="-2" metricUnit="m" metricValue="3.65" metricValueMax="3.8" metricValueMin="3.5" pageId="4" pageNumber="319" unit="mm" value="36.5" valueMax="38.0" valueMin="35.0">35–38 µm</quantity>
