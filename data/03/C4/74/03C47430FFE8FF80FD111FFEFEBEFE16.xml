<taxonomicName authorityName="Chevrolat" authorityYear="1862" class="Insecta" family="Cerambycidae" genus="Elateropsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="quinquenotatus">
<emphasis italics="true" pageId="9" pageNumber="362">Elateropsis quinquenotatus</emphasis>
</taxonomicName>
, female, d) 
<taxonomicName authorityName="Chevrolat" authorityYear="1862" box="[382,591,1655,1673]" class="Insecta" family="Cerambycidae" genus="Elateropsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="sericeiventris">
<emphasis box="[382,591,1655,1673]" italics="true" pageId="9" pageNumber="362">Elateropsis sericeiventris</emphasis>
</taxonomicName>
, male, e) 
<taxonomicName baseAuthorityName="Cazier and Lacey" baseAuthorityYear="1952" box="[677,888,1655,1673]" class="Insecta" family="Cerambycidae" genus="Elateropsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="trimarginatus">
<emphasis box="[677,888,1655,1673]" italics="true" pageId="9" pageNumber="362">Elateropsis trimarginatus</emphasis>
</taxonomicName>
, female, f) 
<taxonomicName box="[990,1167,1655,1673]" class="Insecta" family="Cerambycidae" genus="Elateropsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="dichroma">
<emphasis box="[990,1167,1655,1673]" italics="true" pageId="9" pageNumber="362">Elateropsis dichroma</emphasis>
</taxonomicName>
, female holotype, g) 
<taxonomicName box="[322,495,1679,1697]" class="Insecta" family="Cerambycidae" genus="Elateropsis" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="woodleyi">
<emphasis box="[322,495,1679,1697]" italics="true" pageId="9" pageNumber="362">Elateropsis woodleyi</emphasis>
</taxonomicName>
, male holotype, h) 
<taxonomicName baseAuthorityName="Gahan" baseAuthorityYear="1890" box="[663,877,1679,1697]" class="Insecta" family="Cerambycidae" genus="Solenoptera" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="dominicensis">
<emphasis box="[663,877,1679,1697]" italics="true" pageId="9" pageNumber="362">Solenoptera dominicensis</emphasis>
</taxonomicName>
, male, i) 
<taxonomicName baseAuthorityName="Gahan" baseAuthorityYear="1890" box="[960,1090,1679,1697]" class="Insecta" family="Cerambycidae" genus="Solenoptera" kingdom="Animalia" order="Coleoptera" pageId="9" pageNumber="362" phylum="Arthropoda" rank="species" species="dominicensis">
<emphasis box="[960,1090,1679,1697]" italics="true" pageId="9" pageNumber="362">S. dominicensis</emphasis>
