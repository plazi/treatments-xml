<taxonomicName authorityName="Ris" authorityYear="1916" box="[121,516,1656,1694]" class="Insecta" family="Libellulidae" genus="Lyriothemis" higherTaxonomySource="GBIF" kingdom="Animalia" order="Odonata" pageId="6" pageNumber="63" phylum="Arthropoda" rank="species" species="tricolor">
