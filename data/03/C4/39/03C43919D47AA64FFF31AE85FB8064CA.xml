<subSubSection box="[199,999,837,864]" pageId="1" pageNumber="352" type="etymology">
<paragraph blockId="1.[151,1437,765,1008]" box="[199,999,837,864]" pageId="1" pageNumber="352">
<emphasis bold="true" box="[199,335,838,864]" pageId="1" pageNumber="352">Etymology.</emphasis>
