(
<figureCitation box="[159,294,1447,1471]" captionStart-0="FIGURES 21 – 24" captionStart-1="FIGURES 25 – 27" captionStartId-0="8.[151,264,1908,1931]" captionStartId-1="9.[151,264,1476,1499]" captionTargetBox-0="[216,1381,489,1846]" captionTargetBox-1="[224,1350,435,1422]" captionTargetId-0="figure@8.[188,1398,450,1886]" captionTargetId-1="figure@9.[211,1376,411,1452]" captionTargetPageId-0="8" captionTargetPageId-1="9" captionText-0="FIGURES 21 – 24. Rhipidia (Rhipidia) chenwenyoungi sp. nov. 21: Habitus of male, lateral view; 22 Head of male; 23: Wing; 24: Hypopygium, dorsal view. Scale bar: 21 = 0.5 mm; 22 = 0.25 mm; 23 = 0.5 mm; 24 = 0.1 mm." captionText-1="FIGURES 25 – 27. Rhipidia (Rhipidia) chenwenyoungi sp. nov. 25: Hypopygium, lateral view; 26: Hypopygium, dorsal view; 27: Hypopygium, ventral view. Scale bar: 25 – 27 = 0.1 mm." httpUri-0="https://zenodo.org/record/280542/files/figure.png" httpUri-1="https://zenodo.org/record/280543/files/figure.png" pageId="7" pageNumber="39">
Figs. 
<number box="[223,294,1447,1471]" pageId="7" pageNumber="39" value="24.0" valueMax="27.0" valueMin="21.0">21–27</number>
</figureCitation>
)
</paragraph>
