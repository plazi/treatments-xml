. HW: 
<quantity box="[601,756,1997,2020]" metricMagnitude="-3" metricUnit="m" metricValue="1.27" metricValueMax="1.3" metricValueMin="1.24" pageId="3" pageNumber="240" unit="mm" value="1.27" valueMax="1.3" valueMin="1.24">1.24–1.30 mm</quantity>
