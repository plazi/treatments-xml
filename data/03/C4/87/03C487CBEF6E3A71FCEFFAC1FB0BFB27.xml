<subSubSection pageId="63" pageNumber="64" type="etymology">
<paragraph blockId="63.[810,1452,1397,2070]" pageId="63" pageNumber="64">
<emphasis bold="true" box="[863,992,1636,1662]" pageId="63" pageNumber="64">Etymology</emphasis>
