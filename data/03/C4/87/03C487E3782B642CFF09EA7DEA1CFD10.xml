species 
<taxonomicName authority="Hodgson, 1910" authorityName="Hodgson" authorityYear="1910" class="Malacostraca" family="Paramunnidae" genus="Austrosignum" kingdom="Animalia" order="Isopoda" pageId="4" pageNumber="405" phylum="Arthropoda" rank="species" species="glaciale">
