<subSubSection pageId="652" pageNumber="653" type="etymology">
<paragraph blockId="652.[136,778,1669,2070]" pageId="652" pageNumber="653">
<emphasis bold="true" box="[189,318,1840,1866]" pageId="652" pageNumber="653">Etymology</emphasis>
