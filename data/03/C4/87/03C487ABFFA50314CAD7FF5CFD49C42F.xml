<quantity box="[502,646,1624,1649]" metricMagnitude="3" metricUnit="m" metricValue="1.5925" metricValueMax="2.56" metricValueMin="0.625" pageId="4" pageNumber="558" unit="m" value="1592.5" valueMax="2560.0" valueMin="625.0">625–2560 m</quantity>
