<subSubSection pageId="635" pageNumber="636" type="etymology">
<paragraph blockId="635.[810,1452,785,2070]" pageId="635" pageNumber="636">
<emphasis bold="true" box="[863,992,955,981]" pageId="635" pageNumber="636">Etymology</emphasis>
