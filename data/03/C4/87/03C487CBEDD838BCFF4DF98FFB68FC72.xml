<subSubSection pageId="649" pageNumber="650" type="etymology">
<paragraph blockId="649.[136,778,1703,2070]" pageId="649" pageNumber="650">
<emphasis bold="true" box="[189,318,1908,1934]" pageId="649" pageNumber="650">Etymology</emphasis>
