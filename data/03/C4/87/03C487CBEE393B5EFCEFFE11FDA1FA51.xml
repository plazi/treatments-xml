<subSubSection pageId="360" pageNumber="361" type="etymology">
<paragraph blockId="360.[810,1452,581,2070]" pageId="360" pageNumber="361">
<emphasis bold="true" box="[863,992,955,981]" pageId="360" pageNumber="361">Etymology</emphasis>
