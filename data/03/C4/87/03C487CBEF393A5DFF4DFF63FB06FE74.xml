<taxonomicName authority="(Thorell, 1859)" baseAuthorityName="Thorell" baseAuthorityYear="1859" box="[362,773,1805,1832]" class="Maxillopoda" family="Notodelphyidae" genus="Pachypygus" kingdom="Animalia" order="Cyclopoida" pageId="104" pageNumber="105" phylum="Arthropoda" rank="species" species="gibber">
