<collectionCode LSID="urn:lsid:biocol.org:col:34925" box="[871,966,226,252]" httpUri="http://biocol.org/urn:lsid:biocol.org:col:34925" name="American Museum of Natural History" pageId="9" pageNumber="42">AMNH</collectionCode>
