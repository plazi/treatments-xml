<subSubSection pageId="9" pageNumber="42" type="etymology">
<paragraph blockId="9.[151,1437,151,887]" pageId="9" pageNumber="42">
<emphasis bold="true" box="[197,338,226,252]" pageId="9" pageNumber="42">Etymology.</emphasis>
