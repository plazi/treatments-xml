<taxonomicName authority="Kerschner, 1879" authorityName="Kerschner" authorityYear="1879" box="[361,774,1397,1423]" class="Maxillopoda" family="Notodelphyidae" genus="Doroixys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="459" pageNumber="460" phylum="Arthropoda" rank="species" species="uncinata">
