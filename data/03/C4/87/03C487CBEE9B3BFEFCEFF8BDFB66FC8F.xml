<emphasis bold="true" box="[810,1216,1907,1934]" pageId="458" pageNumber="459">
Genus 
<taxonomicName authority="Kerschner, 1879" authorityName="Kerschner" authorityYear="1879" box="[897,1216,1907,1934]" class="Maxillopoda" family="Notodelphyidae" genus="Doroixys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="458" pageNumber="459" phylum="Arthropoda" rank="genus">
