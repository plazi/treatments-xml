</elevation>
) in sandy and muddy sand bottoms in three localities north (
<typeStatus box="[158,267,1484,1509]" pageId="13" pageNumber="496">Holotype</typeStatus>
, 
<location LSID="urn:lsid:plazi:treatment:03C487E9436EE916FF78F8C5FB29F5BC:8EB260244369E916FEF9FA42FE3EF5BC" box="[278,451,1484,1509]" county="Table" municipality="Vicki's Reef" name="Mermaid Cove" pageId="13" pageNumber="496">Mermaid Cove</location>
), east (
<location LSID="urn:lsid:plazi:treatment:03C487E9436EE916FF78F8C5FB29F5BC:8EB260244369E916FDFEFA42FD53F5BC" box="[529,686,1484,1508]" county="Table" municipality="Vicki's Reef" name="Watson's Bay" pageId="13" pageNumber="496">Watson’s Bay</location>
) and 
<location LSID="urn:lsid:plazi:treatment:03C487E9436EE916FF78F8C5FB29F5BC:8EB260244369E916FD03FA42FCD0F5BD" box="[748,813,1484,1509]" county="Table" municipality="Vicki's Reef" name="South" pageId="13" pageNumber="496">South</location>
(
<collectingMunicipality box="[828,971,1484,1509]" pageId="13" pageNumber="496">Vicki’s Reef</collectingMunicipality>
) of LI (
