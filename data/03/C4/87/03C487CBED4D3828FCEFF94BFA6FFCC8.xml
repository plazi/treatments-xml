<taxonomicName authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[1277,1451,683,709]" class="Maxillopoda" family="Notodelphyidae" genus="Doroixys" higherTaxonomySource="GBIF" kingdom="Animalia" order="Cyclopoida" pageId="541" pageNumber="542" phylum="Arthropoda" rank="species" species="obesa" status="sp. nov.">
<emphasis box="[1277,1451,683,709]" italics="true" pageId="541" pageNumber="542">Doroixys obesa</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[810,903,717,744]" pageId="541" pageNumber="542" rank="species">
<emphasis bold="true" box="[810,896,717,743]" pageId="541" pageNumber="542">sp. nov</emphasis>
.
</taxonomicNameLabel>
and two other species of copepods (
<taxonomicName authorityName="Illg" authorityYear="1958" box="[1327,1451,717,743]" class="Maxillopoda" family="Notodelphyidae" genus="Scolecodes" kingdom="Animalia" order="Cyclopoida" pageId="541" pageNumber="542" phylum="Arthropoda" rank="genus">
<emphasis box="[1327,1451,717,743]" italics="true" pageId="541" pageNumber="542">Scolecodes</emphasis>
</taxonomicName>
sp. and 
<taxonomicName box="[896,1033,751,777]" class="Maxillopoda" family="Ascidicolidae" genus="Haplostoma" kingdom="Animalia" order="Cyclopoida" pageId="541" pageNumber="542" phylum="Arthropoda" rank="genus">
<emphasis box="[896,1033,751,777]" italics="true" pageId="541" pageNumber="542">Haplostoma</emphasis>
