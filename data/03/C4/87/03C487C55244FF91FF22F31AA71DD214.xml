<subSubSection box="[198,1080,599,626]" pageId="4" pageNumber="67" type="etymology">
<paragraph blockId="4.[151,1437,152,812]" box="[198,1080,599,626]" pageId="4" pageNumber="67">
<emphasis bold="true" box="[198,339,599,625]" pageId="4" pageNumber="67">Etymology.</emphasis>
