<number box="[890,903,908,932]" pageId="5" pageNumber="360" value="1.0">1</number>
), mII: 
<number box="[989,1004,908,932]" pageId="5" pageNumber="360" value="2.0">2</number>
- 
<number box="[1012,1027,908,932]" pageId="5" pageNumber="360" value="2.0">2</number>
(prolateral 
<number box="[1164,1177,908,932]" pageId="5" pageNumber="360" value="1.0">1</number>
