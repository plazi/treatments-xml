<subSubSection box="[136,387,1295,1322]" pageId="187" pageNumber="188" type="nomenclature">
<paragraph blockId="187.[136,387,1295,1322]" box="[136,387,1295,1322]" pageId="187" pageNumber="188">
<heading bold="true" box="[136,387,1295,1322]" fontSize="11" level="1" pageId="187" pageNumber="188" reason="1">
<emphasis bold="true" box="[136,387,1295,1322]" pageId="187" pageNumber="188">
<taxonomicName authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[136,270,1295,1322]" class="Maxillopoda" family="Notodelphyidae" genus="Ooishillgia" kingdom="Animalia" order="Cyclopoida" pageId="187" pageNumber="188" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[136,270,1295,1322]" italics="true" pageId="187" pageNumber="188">Ooishillgia</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[277,387,1295,1321]" pageId="187" pageNumber="188" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
</heading>
</paragraph>
