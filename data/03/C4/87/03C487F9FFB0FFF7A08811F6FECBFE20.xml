<geoCoordinate box="[971,1125,225,251]" direction="north" orientation="latitude" pageId="8" pageNumber="51" precision="92" value="58.885">
<number box="[971,1004,225,251]" pageId="8" pageNumber="51" value="58.0">58</number>
° 
<number box="[1024,1084,225,251]" pageId="8" pageNumber="51" value="53.1">53.1</number>
’ N
</geoCoordinate>
; 
<geoCoordinate box="[1139,1288,225,251]" direction="east" orientation="longitude" pageId="8" pageNumber="51" precision="92" value="11.106667">
<number box="[1139,1170,225,251]" pageId="8" pageNumber="51" value="11.0">11</number>
