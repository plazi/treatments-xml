<subSubSection box="[810,1049,1397,1424]" pageId="409" pageNumber="410" type="nomenclature">
<paragraph blockId="409.[810,1049,1397,1424]" box="[810,1049,1397,1424]" pageId="409" pageNumber="410">
<heading bold="true" box="[810,1049,1397,1424]" fontSize="11" level="1" pageId="409" pageNumber="410" reason="1">
<emphasis bold="true" box="[810,1049,1397,1424]" pageId="409" pageNumber="410">
<taxonomicName authority="Kim &amp; Boxshall, 2020" authorityName="Kim &amp; Boxshall" authorityYear="2020" box="[810,932,1397,1423]" class="Maxillopoda" family="Notodelphyidae" genus="Sympygus" kingdom="Animalia" order="Cyclopoida" pageId="409" pageNumber="410" phylum="Arthropoda" rank="genus" status="gen. nov.">
<emphasis bold="true" box="[810,932,1397,1423]" italics="true" pageId="409" pageNumber="410">Sympygus</emphasis>
</taxonomicName>
<taxonomicNameLabel box="[939,1049,1398,1424]" pageId="409" pageNumber="410" rank="genus">gen. nov.</taxonomicNameLabel>
</emphasis>
