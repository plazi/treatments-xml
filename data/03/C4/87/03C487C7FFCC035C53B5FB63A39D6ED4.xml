<subSubSection pageId="7" pageNumber="8" type="etymology">
<paragraph blockId="7.[151,1437,152,962]" pageId="7" pageNumber="8">
<emphasis bold="true" box="[196,347,898,924]" pageId="7" pageNumber="8">Etymology.</emphasis>
