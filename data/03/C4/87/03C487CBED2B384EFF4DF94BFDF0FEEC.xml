<subSubSection pageId="634" pageNumber="635" type="etymology">
<paragraph blockId="634.[136,778,1771,2070]" pageId="634" pageNumber="635">
<emphasis bold="true" box="[189,318,1942,1968]" pageId="634" pageNumber="635">Etymology</emphasis>
