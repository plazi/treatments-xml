<number box="[159,191,2011,2037]" pageId="4" pageNumber="47" value="60.0">60</number>
° 
<number box="[201,274,2011,2037]" pageId="4" pageNumber="47" value="21.27">21.27</number>
’N
</geoCoordinate>
; 
<geoCoordinate box="[314,435,2011,2037]" direction="east" orientation="longitude" pageId="4" pageNumber="47" precision="9" value="6.3481665">
<number box="[314,328,2011,2037]" pageId="4" pageNumber="47" value="6.0">6</number>
° 
<number box="[338,411,2011,2037]" pageId="4" pageNumber="47" value="20.89">20.89</number>
’E
