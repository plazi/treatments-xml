depth (
<geoCoordinate box="[997,1127,1933,1959]" direction="north" orientation="latitude" pageId="4" pageNumber="47" precision="92" value="58.885">
<number box="[997,1030,1933,1959]" pageId="4" pageNumber="47" value="58.0">58</number>
° 
<number box="[1040,1097,1933,1959]" pageId="4" pageNumber="47" value="53.1">53.1</number>
’N
</geoCoordinate>
; 
<geoCoordinate box="[1140,1262,1933,1959]" direction="east" orientation="longitude" pageId="4" pageNumber="47" precision="92" value="11.106667">
<number box="[1140,1167,1933,1959]" pageId="4" pageNumber="47" value="11.0">11</number>
°06.4’E
</geoCoordinate>
