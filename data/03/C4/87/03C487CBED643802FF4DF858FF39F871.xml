<collectingCountry box="[136,226,1874,1900]" name="Georgia" pageId="567" pageNumber="568">Georgia</collectingCountry>
(
<collectingCountry box="[241,528,1874,1901]" name="United States of America" pageId="567" pageNumber="568">United States of America</collectingCountry>
) in the north to 
<collectingCountry box="[709,777,1874,1900]" name="Brazil" pageId="567" pageNumber="568">Brazil</collectingCountry>
