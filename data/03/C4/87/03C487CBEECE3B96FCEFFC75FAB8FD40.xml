<subSubSection pageId="415" pageNumber="416" type="etymology">
<paragraph blockId="415.[810,1452,1057,2070]" pageId="415" pageNumber="416">
<emphasis bold="true" box="[863,992,1295,1321]" pageId="415" pageNumber="416">Etymology</emphasis>
