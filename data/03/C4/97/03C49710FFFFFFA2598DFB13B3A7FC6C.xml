and 
<taxonomicName authority="Carrejo &amp; Obando &amp; Aldrete &amp; Mendivil, 2021" authorityName="Carrejo &amp; Obando &amp; Aldrete &amp; Mendivil" authorityYear="2021" box="[623,756,1483,1509]" class="Insecta" family="Epipsocidae" genus="Goja" kingdom="Animalia" order="Psocodea" pageId="20" pageNumber="521" phylum="Arthropoda" rank="species" species="tamensis" status="n. sp.">
<emphasis box="[623,756,1483,1509]" italics="true" pageId="20" pageNumber="521">G. tamensis</emphasis>
