<taxonomicName authority="Carrejo &amp; Obando &amp; Aldrete &amp; Mendivil, 2021" authorityName="Carrejo &amp; Obando &amp; Aldrete &amp; Mendivil" authorityYear="2021" box="[334,494,1897,1921]" class="Insecta" family="Epipsocidae" genus="Goja" kingdom="Animalia" order="Psocodea" pageId="6" pageNumber="507" phylum="Arthropoda" rank="species" species="camachensis" status="n. sp.">
