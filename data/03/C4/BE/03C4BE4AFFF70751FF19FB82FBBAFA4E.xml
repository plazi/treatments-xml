. 
<taxonomicName authority="Anker, Poddoubtchenko &amp; Jeng, 2006" authorityName="Anker, Poddoubtchenko &amp; Jeng" authorityYear="2006" box="[288,920,1324,1347]" class="Malacostraca" family="Alpheidae" genus="Acanthanas" higherTaxonomySource="GBIF" kingdom="Animalia" order="Decapoda" pageId="3" pageNumber="26" phylum="Arthropoda" rank="species" species="pusillus">
