<location LSID="urn:lsid:plazi:treatment:03C4331ECF1D780CF0A6A3CE2194FE06:8EB2D4D3CF1A780CF189A70F21DAFE93" box="[440,504,260,285]" country="Iceland" county="southern Rockall Trough" latitude="63.133335" longLatPrecision="1015" longitude="-15.666667" municipality="Permanent station" name="Other" pageId="20" pageNumber="321">Other</location>
