<location LSID="urn:lsid:plazi:treatment:03C4331ECF1D780CF0A6A3CE2194FE06:8EB2D4D3CF1A780CF27DA72C22CDFECE" box="[588,751,295,320]" country="Iceland" county="southern Rockall Trough" latitude="63.133335" longLatPrecision="1015" longitude="-15.666667" municipality="Permanent station" name="North Atlantic" pageId="20" pageNumber="321">North Atlantic</location>
